{"textgrid.poem.43996": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nahrung edler Geister,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nahrung edler Geister,", "tokens": ["Nah\u00b7rung", "ed\u00b7ler", "Geis\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Aller Sorgen Meister,", "tokens": ["Al\u00b7ler", "Sor\u00b7gen", "Meis\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NE", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Du mein Element,", "tokens": ["Du", "mein", "E\u00b7le\u00b7ment", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Was man jezo Knaster nennt,", "tokens": ["Was", "man", "je\u00b7zo", "Knas\u00b7ter", "nennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADV", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Komm und las die m\u00fcden Sinnen", "tokens": ["Komm", "und", "las", "die", "m\u00fc\u00b7den", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wieder Ruh gewinnen!", "tokens": ["Wie\u00b7der", "Ruh", "ge\u00b7win\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Auf dem Erdenkreise", "tokens": ["Auf", "dem", "Er\u00b7den\u00b7krei\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Kommet deinem Preise", "tokens": ["Kom\u00b7met", "dei\u00b7nem", "Prei\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Kein Getr\u00e4ncke gleich;", "tokens": ["Kein", "Ge\u00b7tr\u00e4n\u00b7cke", "gleich", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Auch der \u00c4rzte drittes Reich", "tokens": ["Auch", "der", "\u00c4rz\u00b7te", "drit\u00b7tes", "Reich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Flicht dich, deiner Kraft zu Lohne,", "tokens": ["Flicht", "dich", ",", "dei\u00b7ner", "Kraft", "zu", "Loh\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Um Hygaeens Crone.", "tokens": ["Um", "Hy\u00b7gae\u00b7ens", "Cro\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "NE", "NE", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Nach den Lorbeerreisern,", "tokens": ["Nach", "den", "Lor\u00b7beer\u00b7rei\u00b7sern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Die vor allen Kaysern", "tokens": ["Die", "vor", "al\u00b7len", "Kay\u00b7sern"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "PIAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Unsern Carl erh\u00f6hn,", "tokens": ["Un\u00b7sern", "Carl", "er\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NE", "VVINF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Soltu \u00fcber alles gehn,", "tokens": ["Sol\u00b7tu", "\u00fc\u00b7ber", "al\u00b7les", "gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PIS", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Was aus Erd und Wurzel steiget", "tokens": ["Was", "aus", "Erd", "und", "Wur\u00b7zel", "stei\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "NN", "KON", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und den Gipfel neiget.", "tokens": ["Und", "den", "Gip\u00b7fel", "nei\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Deine Kraft und St\u00e4rcke", "tokens": ["Dei\u00b7ne", "Kraft", "und", "St\u00e4r\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Macht durch Wunderwercke", "tokens": ["Macht", "durch", "Wun\u00b7der\u00b7wer\u00b7cke"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Allen Kummer zahm;", "tokens": ["Al\u00b7len", "Kum\u00b7mer", "zahm", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Misgunst, Furcht, Verdru\u00df und Gram", "tokens": ["Mis\u00b7gunst", ",", "Furcht", ",", "Ver\u00b7dru\u00df", "und", "Gram"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Fliehn, so bald ich dich empfinde,", "tokens": ["Fliehn", ",", "so", "bald", "ich", "dich", "emp\u00b7fin\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADV", "PPER", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schneller als die Winde.", "tokens": ["Schnel\u00b7ler", "als", "die", "Win\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Deine Tugend heilet,", "tokens": ["Dei\u00b7ne", "Tu\u00b7gend", "hei\u00b7let", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Deine Macht ertheilet", "tokens": ["Dei\u00b7ne", "Macht", "er\u00b7thei\u00b7let"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und gebiehrt die Ruh;", "tokens": ["Und", "ge\u00b7biehrt", "die", "Ruh", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Will der Schlaf nicht bald herzu,", "tokens": ["Will", "der", "Schlaf", "nicht", "bald", "her\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PTKNEG", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Kan ich ihn mit deinen Wafen", "tokens": ["Kan", "ich", "ihn", "mit", "dei\u00b7nen", "Wa\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bald ins Zimmer schafen.", "tokens": ["Bald", "ins", "Zim\u00b7mer", "scha\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Kommt der lichte Morgen,", "tokens": ["Kommt", "der", "lich\u00b7te", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Bringt der Tag die Sorgen,", "tokens": ["Bringt", "der", "Tag", "die", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Macht der Mittag warm,", "tokens": ["Macht", "der", "Mit\u00b7tag", "warm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "St\u00fcz ich ruhig Kopf und Arm", "tokens": ["St\u00fcz", "ich", "ru\u00b7hig", "Kopf", "und", "Arm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADJD", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und gebrauche deiner Kr\u00e4fte", "tokens": ["Und", "ge\u00b7brau\u00b7che", "dei\u00b7ner", "Kr\u00e4f\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Edle Nectars\u00e4fte.", "tokens": ["Ed\u00b7le", "Nec\u00b7tar\u00b7s\u00e4f\u00b7te", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Die dich nicht vertragen", "tokens": ["Die", "dich", "nicht", "ver\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "PTKNEG", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Und zum Schimpfe sagen,", "tokens": ["Und", "zum", "Schimp\u00b7fe", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Du verderbst die Luft,", "tokens": ["Du", "ver\u00b7derbst", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "M\u00f6gen in des Schinders Gruft,", "tokens": ["M\u00f6\u00b7gen", "in", "des", "Schin\u00b7ders", "Gruft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja zum Teufel selber kriechen", "tokens": ["Ja", "zum", "Teu\u00b7fel", "sel\u00b7ber", "krie\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKANT", "APPRART", "NN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und was Be\u00dfers riechen.", "tokens": ["Und", "was", "Be\u00b7\u00dfers", "rie\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "NN", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.8": {"line.1": {"text": "Kommt ein junges H\u00e4schen", "tokens": ["Kommt", "ein", "jun\u00b7ges", "H\u00e4\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Mit dem wei\u00dfen N\u00e4schen,", "tokens": ["Mit", "dem", "wei\u00b7\u00dfen", "N\u00e4\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Das nach Biesam stinckt,", "tokens": ["Das", "nach", "Bie\u00b7sam", "stinckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NE", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Soll es, wenn es dich verdringt,", "tokens": ["Soll", "es", ",", "wenn", "es", "dich", "ver\u00b7dringt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "In den aufgerollten Haaren", "tokens": ["In", "den", "auf\u00b7ge\u00b7roll\u00b7ten", "Haa\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Glut und Dampf erfahren.", "tokens": ["Glut", "und", "Dampf", "er\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Wer dich gar nicht brauchet", "tokens": ["Wer", "dich", "gar", "nicht", "brau\u00b7chet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADV", "PTKNEG", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Und nicht st\u00fcndlich schmauchet,", "tokens": ["Und", "nicht", "st\u00fcnd\u00b7lich", "schmau\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ist des Mauls nicht werth,", "tokens": ["Ist", "des", "Mauls", "nicht", "werth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Weil er die Natur verkehrt", "tokens": ["Weil", "er", "die", "Na\u00b7tur", "ver\u00b7kehrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und die Gaben, die dich zieren,", "tokens": ["Und", "die", "Ga\u00b7ben", ",", "die", "dich", "zie\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Niemahls will probieren.", "tokens": ["Nie\u00b7mahls", "will", "pro\u00b7bie\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "Las die Canzeln schm\u00e4hlen,", "tokens": ["Las", "die", "Can\u00b7zeln", "schm\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ihre Diener fehlen", "tokens": ["Ih\u00b7re", "Die\u00b7ner", "feh\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und betriegen sich,", "tokens": ["Und", "be\u00b7trie\u00b7gen", "sich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Wenn sie, theurer Knaster, dich,", "tokens": ["Wenn", "sie", ",", "theu\u00b7rer", "Knas\u00b7ter", ",", "dich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADJD", "NN", "$,", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da sie dich nicht brauchen k\u00f6nnen,", "tokens": ["Da", "sie", "dich", "nicht", "brau\u00b7chen", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "VVINF", "VMINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Teufels Abbi\u00df nennen.", "tokens": ["Teu\u00b7fels", "Ab\u00b7bi\u00df", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+--+--", "measure": "dactylic.di.plus"}}, "stanza.11": {"line.1": {"text": "Andre m\u00f6gen sizen", "tokens": ["And\u00b7re", "m\u00f6\u00b7gen", "si\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["PIS", "VMFIN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Und die Lippen spizen,", "tokens": ["Und", "die", "Lip\u00b7pen", "spi\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Bis ihr M\u00e4gdgen will;", "tokens": ["Bis", "ihr", "M\u00e4gd\u00b7gen", "will", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Gelt, du h\u00e4ltst mir immer still", "tokens": ["Gelt", ",", "du", "h\u00e4ltst", "mir", "im\u00b7mer", "still"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und vermehrest meine Plagen", "tokens": ["Und", "ver\u00b7meh\u00b7rest", "mei\u00b7ne", "Pla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Durch kein H\u00f6rnertragen.", "tokens": ["Durch", "kein", "H\u00f6r\u00b7ner\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.12": {"line.1": {"text": "Las den eckeln Frauen", "tokens": ["Las", "den", "ec\u00b7keln", "Frau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Vor dem Dampfe grauen!", "tokens": ["Vor", "dem", "Damp\u00b7fe", "grau\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Die, so kl\u00fcger sind,", "tokens": ["Die", ",", "so", "kl\u00fc\u00b7ger", "sind", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADV", "ADJD", "VAFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Sprechen: Allerliebstes Kind,", "tokens": ["Spre\u00b7chen", ":", "Al\u00b7ler\u00b7liebs\u00b7tes", "Kind", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mich erg\u00f6zet deine Pfeife,", "tokens": ["Mich", "er\u00b7g\u00f6\u00b7zet", "dei\u00b7ne", "Pfei\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die ich selbst ergreife.", "tokens": ["Die", "ich", "selbst", "er\u00b7grei\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.13": {"line.1": {"text": "Rom verbrannte Leichen", "tokens": ["Rom", "ver\u00b7brann\u00b7te", "Lei\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Auf den Zimmetstr\u00e4uchen;", "tokens": ["Auf", "den", "Zim\u00b7mets\u00b7tr\u00e4u\u00b7chen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Mu\u00df ich von der Welt,", "tokens": ["Mu\u00df", "ich", "von", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Hab ich schon voraus bestellt,", "tokens": ["Hab", "ich", "schon", "vo\u00b7raus", "be\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df die Lauge deiner Asche", "tokens": ["Da\u00df", "die", "Lau\u00b7ge", "dei\u00b7ner", "A\u00b7sche"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Meinen C\u00f6rper wasche.", "tokens": ["Mei\u00b7nen", "C\u00f6r\u00b7per", "wa\u00b7sche", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.14": {"line.1": {"text": "Held, vor de\u00dfen Schwerdte", "tokens": ["Held", ",", "vor", "de\u00b7\u00dfen", "Schwerd\u00b7te"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Stambol r\u00fcckw\u00e4rts kehrte,", "tokens": ["Stam\u00b7bol", "r\u00fcck\u00b7w\u00e4rts", "kehr\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ewiger Eugen,", "tokens": ["E\u00b7wi\u00b7ger", "Eu\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Will dein Bliz durch Ungarn gehn,", "tokens": ["Will", "dein", "Bliz", "durch", "Un\u00b7garn", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ey, so las doch nur der Bohnen", "tokens": ["Ey", ",", "so", "las", "doch", "nur", "der", "Boh\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "VVFIN", "ADV", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und des Knasters schonen.", "tokens": ["Und", "des", "Knas\u00b7ters", "scho\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.15": {"line.1": {"text": "Pursche fangen Grillen;", "tokens": ["Pur\u00b7sche", "fan\u00b7gen", "Gril\u00b7len", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Aber wenn sie f\u00fcllen", "tokens": ["A\u00b7ber", "wenn", "sie", "f\u00fcl\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und die Pfeifen gl\u00fchn,", "tokens": ["Und", "die", "Pfei\u00b7fen", "gl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Mu\u00df der Schmerz so weit entfliehn", "tokens": ["Mu\u00df", "der", "Schmerz", "so", "weit", "ent\u00b7fliehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "ADV", "ADJD", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Als die Span'sche Degenklinge", "tokens": ["Als", "die", "Span'\u00b7sche", "De\u00b7gen\u00b7klin\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Vor dem tapfern Binge.", "tokens": ["Vor", "dem", "tap\u00b7fern", "Bin\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.16": {"line.1": {"text": "Rosmarin und Nelcken", "tokens": ["Ros\u00b7ma\u00b7rin", "und", "Nel\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Schwinden, wenn sie welcken,", "tokens": ["Schwin\u00b7den", ",", "wenn", "sie", "wel\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "An Gef\u00e4lligkeit;", "tokens": ["An", "Ge\u00b7f\u00e4l\u00b7lig\u00b7keit", ";"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Du gef\u00e4llst zu jeder Zeit,", "tokens": ["Du", "ge\u00b7f\u00e4llst", "zu", "je\u00b7der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn dein Ruhm ged\u00f6rrter Bl\u00e4tter", "tokens": ["Denn", "dein", "Ruhm", "ge\u00b7d\u00f6rr\u00b7ter", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gr\u00fcnt durch alle Wetter.", "tokens": ["Gr\u00fcnt", "durch", "al\u00b7le", "Wet\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.17": {"line.1": {"text": "Sind uns unsre Wahren", "tokens": ["Sind", "uns", "uns\u00b7re", "Wah\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "An den Fels gefahren", "tokens": ["An", "den", "Fels", "ge\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und ins Meer versenckt,", "tokens": ["Und", "ins", "Meer", "ver\u00b7senckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Br\u00fcder, last euch ungekr\u00e4nckt!", "tokens": ["Br\u00fc\u00b7der", ",", "last", "euch", "un\u00b7ge\u00b7kr\u00e4nckt", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bl\u00e4tter, so die Mohren r\u00f6sten,", "tokens": ["Bl\u00e4t\u00b7ter", ",", "so", "die", "Moh\u00b7ren", "r\u00f6s\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "K\u00f6nnen wieder tr\u00f6sten.", "tokens": ["K\u00f6n\u00b7nen", "wie\u00b7der", "tr\u00f6s\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.18": {"line.1": {"text": "Epheu cr\u00f6nt Poeten;", "tokens": ["E\u00b7pheu", "cr\u00f6nt", "Po\u00b7et\u00b7en", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Doch um meine Fl\u00f6then", "tokens": ["Doch", "um", "mei\u00b7ne", "Fl\u00f6\u00b7then"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Soll Tobackskraut bl\u00fchn.", "tokens": ["Soll", "To\u00b7backs\u00b7kraut", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "VVINF", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Br\u00fcder, macht euch zum Camin", "tokens": ["Br\u00fc\u00b7der", ",", "macht", "euch", "zum", "Ca\u00b7min"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und verjagt mit diesem Pfeile", "tokens": ["Und", "ver\u00b7jagt", "mit", "die\u00b7sem", "Pfei\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eure lange Weile!", "tokens": ["Eu\u00b7re", "lan\u00b7ge", "Wei\u00b7le", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.19": {"line.1": {"text": "H\u00f6rt den Winter rasen,", "tokens": ["H\u00f6rt", "den", "Win\u00b7ter", "ra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "H\u00f6rt den Nordwind blasen,", "tokens": ["H\u00f6rt", "den", "Nord\u00b7wind", "bla\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "H\u00f6rt, er pfeift und f\u00e4hrt!", "tokens": ["H\u00f6rt", ",", "er", "pfeift", "und", "f\u00e4hrt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Kommt, wir wollen um den Herd", "tokens": ["Kommt", ",", "wir", "wol\u00b7len", "um", "den", "Herd"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PPER", "VMFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Seinem kalt- und stolzen W\u00fcten", "tokens": ["Sei\u00b7nem", "kal\u00b7t", "und", "stol\u00b7zen", "W\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "TRUNC", "KON", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Ruhig Troz gebiethen.", "tokens": ["Ru\u00b7hig", "Troz", "ge\u00b7bie\u00b7then", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.20": {"line.1": {"text": "Wollt ihr L\u00e4ndern rathen,", "tokens": ["Wollt", "ihr", "L\u00e4n\u00b7dern", "ra\u00b7then", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "So verpfl\u00fcgt die Saaten,", "tokens": ["So", "ver\u00b7pfl\u00fcgt", "die", "Saa\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Haut die W\u00e4lder aus,", "tokens": ["Haut", "die", "W\u00e4l\u00b7der", "aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Macht uns ein Tobacksfeld draus,", "tokens": ["Macht", "uns", "ein", "To\u00b7back\u00b7sfeld", "draus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "PAV", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Und verz\u00e4unt es mit den Reben,", "tokens": ["Und", "ver\u00b7z\u00e4unt", "es", "mit", "den", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die uns Freude geben!", "tokens": ["Die", "uns", "Freu\u00b7de", "ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.21": {"line.1": {"text": "Top, es leben alle,", "tokens": ["Top", ",", "es", "le\u00b7ben", "al\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "PIS", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Die bey diesem Falle", "tokens": ["Die", "bey", "die\u00b7sem", "Fal\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "PDAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Der Toback erg\u00f6zt!", "tokens": ["Der", "To\u00b7back", "er\u00b7g\u00f6zt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Drum, ihr Br\u00fcder, raucht und nezt,", "tokens": ["Drum", ",", "ihr", "Br\u00fc\u00b7der", ",", "raucht", "und", "nezt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PPOSAT", "NN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bis der Blick vom andern Tage", "tokens": ["Bis", "der", "Blick", "vom", "an\u00b7dern", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Uns zu Bette jage!", "tokens": ["Uns", "zu", "Bet\u00b7te", "ja\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.22": {"line.1": {"text": "Junge, schneide Knaster!", "tokens": ["Jun\u00b7ge", ",", "schnei\u00b7de", "Knas\u00b7ter", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Dieses Lebenspflaster", "tokens": ["Die\u00b7ses", "Le\u00b7bens\u00b7pflas\u00b7ter"], "token_info": ["word", "word"], "pos": ["PDAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ist ein Polychrest.", "tokens": ["Ist", "ein", "Po\u00b7ly\u00b7chrest", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Dem, der uns nicht rauchen l\u00e4st,", "tokens": ["Dem", ",", "der", "uns", "nicht", "rau\u00b7chen", "l\u00e4st", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PPER", "PTKNEG", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Soll an Statt der Nerv- und Flachsen", "tokens": ["Soll", "an", "Statt", "der", "Ner\u00b7v", "und", "Flach\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "NN", "ART", "TRUNC", "KON", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Ein Tobacksstrunck wachsen.",