{"textgrid.poem.54544": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kind/ das der H\u00f6chst uns hat aus lauter Hold gegeben/", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kind/ das der H\u00f6chst uns hat aus lauter Hold gegeben/", "tokens": ["Kind", "/", "das", "der", "H\u00f6chst", "uns", "hat", "aus", "lau\u00b7ter", "Hold", "ge\u00b7ge\u00b7ben", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PRELS", "ART", "NN", "PPER", "VAFIN", "APPR", "PIAT", "NN", "VVPP", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Kind/ das man schon vor Todt eh es gebohren sch\u00e4tzt:", "tokens": ["Kind", "/", "das", "man", "schon", "vor", "Todt", "eh", "es", "ge\u00b7boh\u00b7ren", "sch\u00e4tzt", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PRELS", "PIS", "ADV", "APPR", "NN", "KOUS", "PPER", "VVPP", "VVFIN", "$."], "meter": "+----+-+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "Kind/ das nach langer Angst hat unser Hertz erg\u00f6tzt/", "tokens": ["Kind", "/", "das", "nach", "lan\u00b7ger", "Angst", "hat", "un\u00b7ser", "Hertz", "er\u00b7g\u00f6tzt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PDS", "APPR", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Kind/ das des Lebens Herr auch in dem Todt hie\u00df leben!", "tokens": ["Kind", "/", "das", "des", "Le\u00b7bens", "Herr", "auch", "in", "dem", "Todt", "hie\u00df", "le\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ART", "ART", "NN", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der m\u00fcsse f\u00fcr und f\u00fcr in deinem Hertzen schweben/", "tokens": ["Der", "m\u00fcs\u00b7se", "f\u00fcr", "und", "f\u00fcr", "in", "dei\u00b7nem", "Hert\u00b7zen", "schwe\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPR", "KON", "APPR", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der sich vor dich und uns vom Thron ins Creutz versetzt/", "tokens": ["Der", "sich", "vor", "dich", "und", "uns", "vom", "Thron", "ins", "Creutz", "ver\u00b7setzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PPER", "KON", "PPER", "APPRART", "NN", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Den Liebe nur allein zu dir und uns verletzt;", "tokens": ["Den", "Lie\u00b7be", "nur", "al\u00b7lein", "zu", "dir", "und", "uns", "ver\u00b7letzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "APPR", "PPER", "KON", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Er m\u00fcsse f\u00fcr und f\u00fcr dich sein Geschenck erheben", "tokens": ["Er", "m\u00fcs\u00b7se", "f\u00fcr", "und", "f\u00fcr", "dich", "sein", "Ge\u00b7schenck", "er\u00b7he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "KON", "APPR", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ja Gottes sein Geschenck: Ach danck es ihm allein;", "tokens": ["Ja", "Got\u00b7tes", "sein", "Ge\u00b7schenck", ":", "Ach", "danck", "es", "ihm", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "NN", "PPOSAT", "NN", "$.", "NN", "APPR", "PPER", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da\u00df du ein Mensch/ sein Kind/ und noch kanst lebend seyn.", "tokens": ["Da\u00df", "du", "ein", "Mensch", "/", "sein", "Kind", "/", "und", "noch", "kanst", "le\u00b7bend", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$(", "PPOSAT", "NN", "$(", "KON", "ADV", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Was kanst du Ihm/ als dich hergegen wieder schencken.", "tokens": ["Was", "kanst", "du", "Ihm", "/", "als", "dich", "her\u00b7ge\u00b7gen", "wie\u00b7der", "schen\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PPER", "$(", "KOUS", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Er nehme dich/ von uns/ mein Kind gen\u00e4dig an/", "tokens": ["Er", "neh\u00b7me", "dich", "/", "von", "uns", "/", "mein", "Kind", "ge\u00b7n\u00e4\u00b7dig", "an", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$(", "APPR", "PPER", "$(", "PPOSAT", "NN", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der di\u00df was von Ihm kommt ja nicht verschm\u00e4hen kan/", "tokens": ["Der", "di\u00df", "was", "von", "Ihm", "kommt", "ja", "nicht", "ver\u00b7schm\u00e4\u00b7hen", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDS", "PRELS", "APPR", "PPER", "VVFIN", "ADV", "PTKNEG", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Dann liegt Ihm ob vor dich zu sorgen und zu dencken.", "tokens": ["Dann", "liegt", "Ihm", "ob", "vor", "dich", "zu", "sor\u00b7gen", "und", "zu", "den\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOUS", "APPR", "PPER", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}