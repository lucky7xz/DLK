{"textgrid.poem.65380": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Braut", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine blaue Sch\u00fcrze hast du mir gegeben,", "tokens": ["Ei\u00b7ne", "blau\u00b7e", "Sch\u00fcr\u00b7ze", "hast", "du", "mir", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Mutter, Schad' um's F\u00e4rben, Mutter, Schad um's Weben!", "tokens": ["Mut\u00b7ter", ",", "Schad'", "um's", "F\u00e4r\u00b7ben", ",", "Mut\u00b7ter", ",", "Schad", "um's", "We\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "ADJA", "NN", "$,", "NN", "$,", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Morgen in der Fr\u00fche wird sie bleich erscheinen,", "tokens": ["Mor\u00b7gen", "in", "der", "Fr\u00fc\u00b7he", "wird", "sie", "bleich", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VAFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Will zu Nacht so lange Thr\u00e4nen auf sie weinen.", "tokens": ["Will", "zu", "Nacht", "so", "lan\u00b7ge", "Thr\u00e4\u00b7nen", "auf", "sie", "wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "ADV", "ADJA", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Und wenn meine Thr\u00e4nen es nicht schaffen k\u00f6nnen,", "tokens": ["Und", "wenn", "mei\u00b7ne", "Thr\u00e4\u00b7nen", "es", "nicht", "schaf\u00b7fen", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "PPER", "PTKNEG", "VVINF", "VMINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Wie sie immer str\u00f6men, wie sie immer brennen,", "tokens": ["Wie", "sie", "im\u00b7mer", "str\u00f6\u00b7men", ",", "wie", "sie", "im\u00b7mer", "bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "$,", "PWAV", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Wird mein Liebster kommen und mir Wasser bringen,", "tokens": ["Wird", "mein", "Liebs\u00b7ter", "kom\u00b7men", "und", "mir", "Was\u00b7ser", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVINF", "KON", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Wird sich Meereswasser aus den Locken ringen.", "tokens": ["Wird", "sich", "Mee\u00b7res\u00b7was\u00b7ser", "aus", "den", "Lo\u00b7cken", "rin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.3": {"line.1": {"text": "Denn er liegt da unten in des Meeres Grunde,", "tokens": ["Denn", "er", "liegt", "da", "un\u00b7ten", "in", "des", "Mee\u00b7res", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Und wenn ihm die Wogen rauschen diese Kunde,", "tokens": ["Und", "wenn", "ihm", "die", "Wo\u00b7gen", "rau\u00b7schen", "die\u00b7se", "Kun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da\u00df ich hier soll freien und ihm treulos werden,", "tokens": ["Da\u00df", "ich", "hier", "soll", "frei\u00b7en", "und", "ihm", "treu\u00b7los", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIAT", "ADJA", "KON", "PPER", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Aus der Tiefe steigt er auf zur b\u00f6sen Erden.", "tokens": ["Aus", "der", "Tie\u00b7fe", "steigt", "er", "auf", "zur", "b\u00f6\u00b7sen", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "APPR", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.4": {"line.1": {"text": "In die Kirche soll ich \u2013 nun, ich will ja kommen,", "tokens": ["In", "die", "Kir\u00b7che", "soll", "ich", "\u2013", "nun", ",", "ich", "will", "ja", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "$(", "ADV", "$,", "PPER", "VMFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Will mich fromm gesellen zu den andern Frommen.", "tokens": ["Will", "mich", "fromm", "ge\u00b7sel\u00b7len", "zu", "den", "an\u00b7dern", "From\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "VVPP", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "La\u00dft mich am Altare still vor\u00fcberziehen,", "tokens": ["La\u00dft", "mich", "am", "Al\u00b7ta\u00b7re", "still", "vor\u00b7\u00fc\u00b7ber\u00b7zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Denn dort ist mein Pl\u00e4tzchen, wo die Witwen knieen.", "tokens": ["Denn", "dort", "ist", "mein", "Pl\u00e4tz\u00b7chen", ",", "wo", "die", "Wit\u00b7wen", "kni\u00b7e\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPOSAT", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+--", "measure": "iambic.penta.relaxed"}}}}}