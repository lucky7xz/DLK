{"textgrid.poem.46433": {"metadata": {"author": {"name": "Ewald, Johann Joachim", "birth": "N.A.", "death": "N.A."}, "title": "1L: Iren denkt stark und sch\u00f6n, in ihm ist lauter Licht,", "genre": "verse", "period": "N.A.", "pub_year": 1744, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Iren denkt stark und sch\u00f6n, in ihm ist lauter Licht,", "tokens": ["I\u00b7ren", "denkt", "stark", "und", "sch\u00f6n", ",", "in", "ihm", "ist", "lau\u00b7ter", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "KON", "ADJD", "$,", "APPR", "PPER", "VAFIN", "PIAT", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Er ist beherzt, gerecht, doch liebensw\u00fcrdig nicht,", "tokens": ["Er", "ist", "be\u00b7herzt", ",", "ge\u00b7recht", ",", "doch", "lie\u00b7bens\u00b7w\u00fcr\u00b7dig", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "ADJD", "$,", "ADV", "ADJD", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sein strenger Ernst der ihn mit Runzeln \u00fcberzieht,", "tokens": ["Sein", "stren\u00b7ger", "Ernst", "der", "ihn", "mit", "Run\u00b7zeln", "\u00fc\u00b7berz\u00b7ieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Macht da\u00df ihn jeder ehrt, bewundert, r\u00fchmt und flieht.", "tokens": ["Macht", "da\u00df", "ihn", "je\u00b7der", "ehrt", ",", "be\u00b7wun\u00b7dert", ",", "r\u00fchmt", "und", "flieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct",