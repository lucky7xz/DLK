{"textgrid.poem.40536": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Feldgr\u00fcne, Meeresbl\u00e4ue, Himmelshelle,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Feldgr\u00fcne, Meeresbl\u00e4ue, Himmelshelle,", "tokens": ["Feld\u00b7gr\u00fc\u00b7ne", ",", "Mee\u00b7res\u00b7bl\u00e4u\u00b7e", ",", "Him\u00b7mels\u00b7hel\u00b7le", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Mir sonst so lieb, wie grinst ihr hier mich an!", "tokens": ["Mir", "sonst", "so", "lieb", ",", "wie", "grinst", "ihr", "hier", "mich", "an", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADJD", "$,", "PWAV", "VVFIN", "PPER", "ADV", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Blau ist das Meer, doch tr\u00e4gt die ruh'nde Welle", "tokens": ["Blau", "ist", "das", "Meer", ",", "doch", "tr\u00e4gt", "die", "ruh'n\u00b7de", "Wel\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Kein Segel, keinen Nachen, keinen Schwan.", "tokens": ["Kein", "Se\u00b7gel", ",", "kei\u00b7nen", "Na\u00b7chen", ",", "kei\u00b7nen", "Schwan", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Hell ist die Luft, doch eine Glanzesw\u00fcste,", "tokens": ["Hell", "ist", "die", "Luft", ",", "doch", "ei\u00b7ne", "Glan\u00b7zes\u00b7w\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.2": {"text": "Durch die kein Vogel singt, kein W\u00f6lkchen schwebt;", "tokens": ["Durch", "die", "kein", "Vo\u00b7gel", "singt", ",", "kein", "W\u00f6lk\u00b7chen", "schwebt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "VVFIN", "$,", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gr\u00fcn ist das Feld, doch Moor, bis fern zur K\u00fcste,", "tokens": ["Gr\u00fcn", "ist", "das", "Feld", ",", "doch", "Moor", ",", "bis", "fern", "zur", "K\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "KON", "NN", "$,", "KOUS", "ADJD", "APPRART", "NN", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.4": {"text": "Draus sich kein Haus, kein Baum, kein Strauch erhebt.", "tokens": ["Draus", "sich", "kein", "Haus", ",", "kein", "Baum", ",", "kein", "Strauch", "er\u00b7hebt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "PRF", "PIAT", "NN", "$,", "PIAT", "NN", "$,", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und nur ein Streif von wei\u00dfem Nebelrauche", "tokens": ["Und", "nur", "ein", "Streif", "von", "wei\u00b7\u00dfem", "Ne\u00b7bel\u00b7rau\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kriecht durch die Moores\u00f6de, lang und weit,", "tokens": ["Kriecht", "durch", "die", "Moo\u00b7re\u00b7s\u00f6\u00b7de", ",", "lang", "und", "weit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als w\u00e4lzte fra\u00dfesmatt, tr\u00e4g auf dem Bauche", "tokens": ["Als", "w\u00e4lz\u00b7te", "fra\u00b7\u00dfes\u00b7matt", ",", "tr\u00e4g", "auf", "dem", "Bau\u00b7che"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "PTKVZ", "$,", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dahin die Schlange sich der Ewigkeit.", "tokens": ["Da\u00b7hin", "die", "Schlan\u00b7ge", "sich", "der", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sieh, m\u00e4hlich aus dem schwanken Dunstkolosse", "tokens": ["Sieh", ",", "m\u00e4h\u00b7lich", "aus", "dem", "schwan\u00b7ken", "Dunst\u00b7ko\u00b7los\u00b7se"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Entringt sich Form und Bild im Sonnenstrahl,", "tokens": ["Ent\u00b7ringt", "sich", "Form", "und", "Bild", "im", "Son\u00b7nen\u00b7strahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "NN", "KON", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er wird zum leuchtenden kristallnen Schlosse", "tokens": ["Er", "wird", "zum", "leuch\u00b7ten\u00b7den", "kris\u00b7tall\u00b7nen", "Schlos\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit blankem Silberdach und hohem Saal.", "tokens": ["Mit", "blan\u00b7kem", "Sil\u00b7ber\u00b7dach", "und", "ho\u00b7hem", "Saal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Auf diamant'nem Thron sa\u00df siegestrunken", "tokens": ["Auf", "di\u00b7a\u00b7mant'\u00b7nem", "Thron", "sa\u00df", "sie\u00b7ge\u00b7strun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der K\u00f6nig, \u2013 ach, wie hie\u00df er doch? \u2013 sein Haupt", "tokens": ["Der", "K\u00f6\u00b7nig", ",", "\u2013", "ach", ",", "wie", "hie\u00df", "er", "doch", "?", "\u2013", "sein", "Haupt"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "$(", "XY", "$,", "PWAV", "VVFIN", "PPER", "ADV", "$.", "$(", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "War an die Brust der K\u00f6nigin gesunken,", "tokens": ["War", "an", "die", "Brust", "der", "K\u00f6\u00b7ni\u00b7gin", "ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vom Kranz war's der Unsterblichkeit umlaubt.", "tokens": ["Vom", "Kranz", "wa\u00b7r's", "der", "U\u00b7nsterb\u00b7lich\u00b7keit", "um\u00b7laubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Am Throne links r\u00fchrt' eine goldne Leier", "tokens": ["Am", "Thro\u00b7ne", "links", "r\u00fchrt'", "ei\u00b7ne", "gold\u00b7ne", "Lei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Dichter s\u00fc\u00df, \u2013 wie hie\u00df er doch? \u2013 der sang:", "tokens": ["Ein", "Dich\u00b7ter", "s\u00fc\u00df", ",", "\u2013", "wie", "hie\u00df", "er", "doch", "?", "\u2013", "der", "sang", ":"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "$(", "PWAV", "VVFIN", "PPER", "ADV", "$.", "$(", "ART", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbunsterblich ist dein Lieben! ihm zur Feier,", "tokens": ["\u00bb", "uns\u00b7terb\u00b7lich", "ist", "dein", "Lie\u00b7ben", "!", "ihm", "zur", "Fei\u00b7er", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VAFIN", "PPOSAT", "ADJA", "$.", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fcrst, gibt ja mein unsterblich Lied den Klang!\u00ab", "tokens": ["F\u00fcrst", ",", "gibt", "ja", "mein", "uns\u00b7terb\u00b7lich", "Lied", "den", "Klang", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "VVFIN", "ADV", "PPOSAT", "ADJD", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Am Throne rechts, da sa\u00df ein weiser Seher, \u2013", "tokens": ["Am", "Thro\u00b7ne", "rechts", ",", "da", "sa\u00df", "ein", "wei\u00b7ser", "Se\u00b7her", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "ADV", "$,", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u2013 Wie hie\u00df er doch? \u2013 der schrieb's in Marmor ein:", "tokens": ["\u2013", "Wie", "hie\u00df", "er", "doch", "?", "\u2013", "der", "schrie\u00b7b's", "in", "Mar\u00b7mor", "ein", ":"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VVFIN", "PPER", "ADV", "$.", "$(", "ART", "ADJA", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "\u00bbunsterblich ist dein Sieg! Es m\u00fc\u00dfte eher", "tokens": ["\u00bb", "uns\u00b7terb\u00b7lich", "ist", "dein", "Sieg", "!", "Es", "m\u00fc\u00df\u00b7te", "e\u00b7her"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ADJD", "VAFIN", "PPOSAT", "NN", "$.", "PPER", "VMFIN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ja mein unsterblich Wort verklungen sein!\u00ab", "tokens": ["Ja", "mein", "uns\u00b7terb\u00b7lich", "Wort", "ver\u00b7klun\u00b7gen", "sein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKANT", "PPOSAT", "ADJD", "NN", "VVPP", "VAINF", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Ein Volk, \u2013 wie hie\u00df es doch? \u2013 das pries unsterblich", "tokens": ["Ein", "Volk", ",", "\u2013", "wie", "hie\u00df", "es", "doch", "?", "\u2013", "das", "pries", "uns\u00b7terb\u00b7lich"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "$(", "PWAV", "VVFIN", "PPER", "ADV", "$.", "$(", "PDS", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den S\u00e4nger, Seher und das F\u00fcrstenpaar:", "tokens": ["Den", "S\u00e4n\u00b7ger", ",", "Se\u00b7her", "und", "das", "F\u00fcrs\u00b7ten\u00b7paar", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbein Volk, an Gl\u00fcck und Ehren unverderblich", "tokens": ["\u00bb", "ein", "Volk", ",", "an", "Gl\u00fcck", "und", "Eh\u00b7ren", "un\u00b7ver\u00b7derb\u00b7lich"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "NN", "$,", "APPR", "NN", "KON", "NN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hebt auf dem Schild euch zu den G\u00f6ttern dar!\u00ab", "tokens": ["Hebt", "auf", "dem", "Schild", "euch", "zu", "den", "G\u00f6t\u00b7tern", "dar", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Als so den Trank Unsterblichkeit sie tranken", "tokens": ["Als", "so", "den", "Trank", "U\u00b7nsterb\u00b7lich\u00b7keit", "sie", "tran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In vollem Zug, fa\u00dft Trunkenheit sie all',", "tokens": ["In", "vol\u00b7lem", "Zug", ",", "fa\u00dft", "Trun\u00b7ken\u00b7heit", "sie", "all'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "NN", "PPER", "PIS", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Des K\u00f6nigs Kron', des Dichters Harfe wanken", "tokens": ["Des", "K\u00f6\u00b7nigs", "Kron'", ",", "des", "Dich\u00b7ters", "Har\u00b7fe", "wan\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "$,", "ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Weisen Marmor, Volk und Schlo\u00df und Wall!", "tokens": ["Des", "Wei\u00b7sen", "Mar\u00b7mor", ",", "Volk", "und", "Schlo\u00df", "und", "Wall", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "NN", "KON", "NN", "KON", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Wo flieh' ich hin, da\u00df nicht kristall'ne Thore,", "tokens": ["Wo", "flieh'", "ich", "hin", ",", "da\u00df", "nicht", "kris\u00b7tall'\u00b7ne", "Tho\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "PTKNEG", "ADJA", "NN", "$,"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Demant'ne S\u00e4ulen st\u00fcrzen auf mich ein? \u2013 \u2013", "tokens": ["De\u00b7mant'\u00b7ne", "S\u00e4u\u00b7len", "st\u00fcr\u00b7zen", "auf", "mich", "ein", "?", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$.", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ei, sieh um dich! Im weiten gr\u00fcnen Moore,", "tokens": ["Ei", ",", "sieh", "um", "dich", "!", "Im", "wei\u00b7ten", "gr\u00fc\u00b7nen", "Moo\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "PPER", "$.", "APPRART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Am Strand des Meers, stehst du ja ganz allein!", "tokens": ["Am", "Strand", "des", "Meers", ",", "stehst", "du", "ja", "ganz", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "VVFIN", "PPER", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Und nur ein Streif von wei\u00dfem Nebelrauche", "tokens": ["Und", "nur", "ein", "Streif", "von", "wei\u00b7\u00dfem", "Ne\u00b7bel\u00b7rau\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kriecht durch die Moores\u00f6de, lang und weit,", "tokens": ["Kriecht", "durch", "die", "Moo\u00b7re\u00b7s\u00f6\u00b7de", ",", "lang", "und", "weit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als w\u00e4lzte fra\u00dfesmatt, tr\u00e4g auf dem Bauche", "tokens": ["Als", "w\u00e4lz\u00b7te", "fra\u00b7\u00dfes\u00b7matt", ",", "tr\u00e4g", "auf", "dem", "Bau\u00b7che"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "PTKVZ", "$,", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dahin die Schlange sich der Ewigkeit.", "tokens": ["Da\u00b7hin", "die", "Schlan\u00b7ge", "sich", "der", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}