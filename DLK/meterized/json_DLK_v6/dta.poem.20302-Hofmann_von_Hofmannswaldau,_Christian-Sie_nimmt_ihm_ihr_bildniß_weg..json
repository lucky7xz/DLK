{"dta.poem.20302": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Sie nimmt ihm ihr bildni\u00df weg.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du reist dein bildni\u00df mir gewaltsam aus der hand/", "tokens": ["Du", "reist", "dein", "bild\u00b7ni\u00df", "mir", "ge\u00b7walt\u00b7sam", "aus", "der", "hand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PPER", "ADJD", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wilst mir diesen schatz durchaus nicht l\u00e4nger lassen.", "tokens": ["Und", "wilst", "mir", "die\u00b7sen", "schatz", "durc\u00b7haus", "nicht", "l\u00e4n\u00b7ger", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PDAT", "NN", "ADV", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was aber soll ich wohl hier vor gedancken fassen?", "tokens": ["Was", "a\u00b7ber", "soll", "ich", "wohl", "hier", "vor", "ge\u00b7dan\u00b7cken", "fas\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VMFIN", "PPER", "ADV", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mi\u00dfg\u00f6nnstu etwan mir di\u00df theure liebes-pfand?", "tokens": ["Mi\u00df\u00b7g\u00f6nns\u00b7tu", "et\u00b7wan", "mir", "di\u00df", "theu\u00b7re", "lie\u00b7bes\u00b7pfand", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPER", "PDS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Meynst du vielleicht/ es sey bey mir nicht angewandt?", "tokens": ["Meynst", "du", "viel\u00b7leicht", "/", "es", "sey", "bey", "mir", "nicht", "an\u00b7ge\u00b7wandt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "$(", "PPER", "VAFIN", "APPR", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Gewi\u00df/ das erste macht mich vor der zeit erblassen/", "tokens": ["Ge\u00b7wi\u00df", "/", "das", "ers\u00b7te", "macht", "mich", "vor", "der", "zeit", "er\u00b7blas\u00b7sen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "ART", "ADJA", "VVFIN", "PRF", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dieweil du als ein feind wilt meine neigung hassen/", "tokens": ["Die\u00b7weil", "du", "als", "ein", "feind", "wilt", "mei\u00b7ne", "nei\u00b7gung", "has\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOUS", "ART", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und dieses letztre hat dein mund mir selbst bekandt.", "tokens": ["Und", "die\u00b7ses", "letz\u00b7tre", "hat", "dein", "mund", "mir", "selbst", "be\u00b7kandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "PIS", "VAFIN", "PPOSAT", "NN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Doch du betriegst dich sehr. Wer eiffert mehr um dich?", "tokens": ["Doch", "du", "be\u00b7triegst", "dich", "sehr", ".", "Wer", "eif\u00b7fert", "mehr", "um", "dich", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "$.", "PWS", "VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wer sehnt sich mehr nach dir? wer liebt auch mehr als ich?", "tokens": ["Wer", "sehnt", "sich", "mehr", "nach", "dir", "?", "wer", "liebt", "auch", "mehr", "als", "ich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "ADV", "APPR", "PPER", "$.", "PWS", "VVFIN", "ADV", "ADV", "KOUS", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Nimmst du mir nun dein bild/ so mu\u00df ichs zwar verschmertzen/", "tokens": ["Nimmst", "du", "mir", "nun", "dein", "bild", "/", "so", "mu\u00df", "ichs", "zwar", "ver\u00b7schmert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "PPOSAT", "NN", "$(", "ADV", "VMFIN", "PIS", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Allein/ so stoltz du bist/ so glaube nur dabey/", "tokens": ["Al\u00b7lein", "/", "so", "stoltz", "du", "bist", "/", "so", "glau\u00b7be", "nur", "da\u00b7bey", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "ADV", "ADJD", "PPER", "VAFIN", "$(", "ADV", "VVFIN", "ADV", "PAV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich trotze deinen ha\u00df/ und sag es ohne scheu:", "tokens": ["Ich", "trot\u00b7ze", "dei\u00b7nen", "ha\u00df", "/", "und", "sag", "es", "oh\u00b7ne", "scheu", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$(", "KON", "VVFIN", "PPER", "APPR", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Du reist mirs aus der hand/ nicht aber aus dem hertzen.", "tokens": ["Du", "reist", "mirs", "aus", "der", "hand", "/", "nicht", "a\u00b7ber", "aus", "dem", "hert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "APPR", "ART", "NN", "$(", "PTKNEG", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}