{"dta.poem.8191": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "I .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Wie das Ding sich immer putzt!", "tokens": ["Wie", "das", "Ding", "sich", "im\u00b7mer", "putzt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Fehlt nur, da\u00df mit unser Einem", "tokens": ["Fehlt", "nur", ",", "da\u00df", "mit", "un\u00b7ser", "Ei\u00b7nem"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie sich noch vertraulich duzt.", "tokens": ["Sie", "sich", "noch", "ver\u00b7trau\u00b7lich", "duzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Setzt sich, wo wir auch erscheinen,", "tokens": ["Setzt", "sich", ",", "wo", "wir", "auch", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "PWAV", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie von selber nebenbei;", "tokens": ["Wie", "von", "sel\u00b7ber", "ne\u00b7ben\u00b7bei", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Pr\u00e4sidentens k\u00f6nnten meinen,", "tokens": ["Pr\u00e4\u00b7si\u00b7den\u00b7tens", "k\u00f6nn\u00b7ten", "mei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPOSAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df es hei\u00dfe Freundschaft sei.", "tokens": ["Da\u00df", "es", "hei\u00b7\u00dfe", "Freund\u00b7schaft", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und es will sich doch nicht schicken,", "tokens": ["Und", "es", "will", "sich", "doch", "nicht", "schi\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PRF", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df man so mit Jeder geht,", "tokens": ["Da\u00df", "man", "so", "mit", "Je\u00b7der", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "APPR", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seit Papa im Staatskalender", "tokens": ["Seit", "Pa\u00b7pa", "im", "Staats\u00b7ka\u00b7len\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "APPRART", "NN"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.4": {"text": "In der dritten Classe steht.", "tokens": ["In", "der", "drit\u00b7ten", "Clas\u00b7se", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Hat Mama doch auch den Diensten", "tokens": ["Hat", "Ma\u00b7ma", "doch", "auch", "den", "Diens\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "ADV", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Anbefohlen klar und hell,", "tokens": ["An\u00b7be\u00b7foh\u00b7len", "klar", "und", "hell", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fr\u00e4ulein hie\u00dfen wir jetzunder,", "tokens": ["Fr\u00e4u\u00b7lein", "hie\u00b7\u00dfen", "wir", "je\u00b7tzun\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fr\u00e4ulein, und nicht mehr Mamsell!", "tokens": ["Fr\u00e4u\u00b7lein", ",", "und", "nicht", "mehr", "Mam\u00b7sell", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "PTKNEG", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ach, ein kleines Bischen adlich,", "tokens": ["Ach", ",", "ein", "klei\u00b7nes", "Bi\u00b7schen", "ad\u00b7lich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So ein Bischen \u2014 glaub', wir sind's!", "tokens": ["So", "ein", "Bi\u00b7schen", "glaub'", ",", "wir", "sin\u00b7d's", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$(", "VVFIN", "$,", "PPER", "VAFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Morgen in der goldnen Kutsche", "tokens": ["Mor\u00b7gen", "in", "der", "gold\u00b7nen", "Kut\u00b7sche"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Holt uns ein verw\u00fcnschter Prinz!\u201c", "tokens": ["Holt", "uns", "ein", "ver\u00b7w\u00fcnschter", "Prinz", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}