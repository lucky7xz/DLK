{"textgrid.poem.46560": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[reizender als alle Sprachen]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Reizender als alle Sprachen,", "tokens": ["Rei\u00b7zen\u00b7der", "als", "al\u00b7le", "Spra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die ich jemals lernt' und sprach,", "tokens": ["Die", "ich", "je\u00b7mals", "lernt'", "und", "sprach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "T\u00f6nt, was deine Lippchen brachen,", "tokens": ["T\u00f6nt", ",", "was", "dei\u00b7ne", "Lipp\u00b7chen", "bra\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mir noch jetzt im Traume nach.", "tokens": ["Mir", "noch", "jetzt", "im", "Trau\u00b7me", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn man dir von Gro\u00dfpapachen", "tokens": ["Wenn", "man", "dir", "von", "Gro\u00df\u00b7pa\u00b7pa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und von Gro\u00dfmamachen sprach,", "tokens": ["Und", "von", "Gro\u00df\u00b7ma\u00b7ma\u00b7chen", "sprach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bildeten in deinen Sprachen", "tokens": ["Bil\u00b7de\u00b7ten", "in", "dei\u00b7nen", "Spra\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Neue Formen k\u00fchn sich nach:", "tokens": ["Neu\u00b7e", "For\u00b7men", "k\u00fchn", "sich", "nach", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "PRF", "APPR", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Kleinpapachen, Kleinmamachen,", "tokens": ["Klein\u00b7pa\u00b7pa\u00b7chen", ",", "Klein\u00b7ma\u00b7ma\u00b7chen", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vater, Mutter, nanntest du,", "tokens": ["Va\u00b7ter", ",", "Mut\u00b7ter", ",", "nann\u00b7test", "du", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn sie f\u00fcr dich Blumen brachen,", "tokens": ["Wenn", "sie", "f\u00fcr", "dich", "Blu\u00b7men", "bra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Oder trugen Fr\u00fcchte zu.", "tokens": ["O\u00b7der", "tru\u00b7gen", "Fr\u00fcch\u00b7te", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Kleinpapachen! Kleinpapachen!", "tokens": ["Klein\u00b7pa\u00b7pa\u00b7chen", "!", "Klein\u00b7pa\u00b7pa\u00b7chen", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Riefst du recht als wie zur Schmach,", "tokens": ["Riefst", "du", "recht", "als", "wie", "zur", "Schmach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "KOUS", "KOKOM", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wann dich deine Kitzel stachen,", "tokens": ["Wann", "dich", "dei\u00b7ne", "Kit\u00b7zel", "sta\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deinem gro\u00dfen Vater nach.", "tokens": ["Dei\u00b7nem", "gro\u00b7\u00dfen", "Va\u00b7ter", "nach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Kleinmamachen aber sprachen", "tokens": ["Klein\u00b7ma\u00b7ma\u00b7chen", "a\u00b7ber", "spra\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht die Lippchen halb so gern,", "tokens": ["Nicht", "die", "Lipp\u00b7chen", "halb", "so", "gern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "ADJD", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil die Kleinheit von Mamachen", "tokens": ["Weil", "die", "Klein\u00b7heit", "von", "Ma\u00b7ma\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wirklich stand nicht halb so fern.", "tokens": ["Wirk\u00b7lich", "stand", "nicht", "halb", "so", "fern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PTKNEG", "ADJD", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}