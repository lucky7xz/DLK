{"textgrid.poem.36198": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie der Frommen Lanze bl\u00fchet,", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie der Frommen Lanze bl\u00fchet,", "tokens": ["Wie", "der", "From\u00b7men", "Lan\u00b7ze", "bl\u00fc\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die vollendend ihr Gel\u00fcbde,", "tokens": ["Die", "voll\u00b7en\u00b7dend", "ihr", "Ge\u00b7l\u00fcb\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hier die Schulden abzub\u00fc\u00dfen,", "tokens": ["Hier", "die", "Schul\u00b7den", "ab\u00b7zu\u00b7b\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich in frommen Streit bem\u00fchen,", "tokens": ["Sich", "in", "from\u00b7men", "Streit", "be\u00b7m\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit der Palme sich zu schm\u00fccken,", "tokens": ["Mit", "der", "Pal\u00b7me", "sich", "zu", "schm\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die im Himmel immer gr\u00fcnet,", "tokens": ["Die", "im", "Him\u00b7mel", "im\u00b7mer", "gr\u00fc\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Gern in eignem Blut sich k\u00fchlen;", "tokens": ["Gern", "in", "eig\u00b7nem", "Blut", "sich", "k\u00fch\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie im Maien die Geb\u00fcsche", "tokens": ["Wie", "im", "Mai\u00b7en", "die", "Ge\u00b7b\u00fc\u00b7sche"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "ART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.9": {"text": "In den stillen Talen gr\u00fcnen,", "tokens": ["In", "den", "stil\u00b7len", "Ta\u00b7len", "gr\u00fc\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Bl\u00fctumkr\u00e4nzt die vollen H\u00fcgel,", "tokens": ["Bl\u00fc\u00b7tum\u00b7kr\u00e4nzt", "die", "vol\u00b7len", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Linde liebe Blumen gl\u00fchen,", "tokens": ["Lin\u00b7de", "lie\u00b7be", "Blu\u00b7men", "gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Auf der Erde buntem G\u00fcrtel", "tokens": ["Auf", "der", "Er\u00b7de", "bun\u00b7tem", "G\u00fcr\u00b7tel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Sich erhebt ein Liebesgr\u00fc\u00dfen,", "tokens": ["Sich", "er\u00b7hebt", "ein", "Lie\u00b7bes\u00b7gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Auf Gesanges k\u00fchnem Fl\u00fcgel;", "tokens": ["Auf", "Ge\u00b7san\u00b7ges", "k\u00fch\u00b7nem", "Fl\u00fc\u00b7gel", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Also bl\u00fchet, also gr\u00fcnet,", "tokens": ["Al\u00b7so", "bl\u00fc\u00b7het", ",", "al\u00b7so", "gr\u00fc\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Von jedwedem Mund ger\u00fchmet,", "tokens": ["Von", "jed\u00b7we\u00b7dem", "Mund", "ge\u00b7r\u00fch\u00b7met", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Manches Heldenherz entz\u00fcndend,", "tokens": ["Man\u00b7ches", "Hel\u00b7den\u00b7herz", "ent\u00b7z\u00fcn\u00b7dend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Und in manchem Lied verk\u00fcndet,", "tokens": ["Und", "in", "man\u00b7chem", "Lied", "ver\u00b7k\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Rolands Tod und Heldenk\u00fchne,", "tokens": ["Ro\u00b7lands", "Tod", "und", "Hel\u00b7den\u00b7k\u00fch\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Auch sein adelich Gem\u00fcte,", "tokens": ["Auch", "sein", "a\u00b7de\u00b7lich", "Ge\u00b7m\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Wie er fern von Trug und L\u00fcgen,", "tokens": ["Wie", "er", "fern", "von", "Trug", "und", "L\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Doch vor allem, wie er fr\u00fche", "tokens": ["Doch", "vor", "al\u00b7lem", ",", "wie", "er", "fr\u00fc\u00b7he"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "PIS", "$,", "PWAV", "PPER", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Alle seine Schuld abb\u00fc\u00dfte,", "tokens": ["Al\u00b7le", "sei\u00b7ne", "Schuld", "ab\u00b7b\u00fc\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Mit der M\u00e4rt'rer Kranz sich schm\u00fcckend,", "tokens": ["Mit", "der", "M\u00e4rt'\u00b7rer", "Kranz", "sich", "schm\u00fc\u00b7ckend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "PRF", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Deren Palme immer gr\u00fcnet.", "tokens": ["De\u00b7ren", "Pal\u00b7me", "im\u00b7mer", "gr\u00fc\u00b7net", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Noch in fernen Zeiten gl\u00fchen", "tokens": ["Noch", "in", "fer\u00b7nen", "Zei\u00b7ten", "gl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Helden in dem Schlachtgew\u00fchle", "tokens": ["Hel\u00b7den", "in", "dem", "Schlacht\u00b7ge\u00b7w\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Bei dem Rolandsliede k\u00fchner,", "tokens": ["Bei", "dem", "Ro\u00b7lands\u00b7lie\u00b7de", "k\u00fch\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Wenn der Held also begr\u00fc\u00dfet", "tokens": ["Wenn", "der", "Held", "al\u00b7so", "be\u00b7gr\u00fc\u00b7\u00dfet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Vor der Schlacht die Heldenbr\u00fcder,", "tokens": ["Vor", "der", "Schlacht", "die", "Hel\u00b7den\u00b7br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Ziehend \u00fcber Tal und H\u00fcgel.", "tokens": ["Zie\u00b7hend", "\u00fc\u00b7ber", "Tal", "und", "H\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Lied wird gesungen,", "tokens": ["Lied", "wird", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Kampf dann begunnen,", "tokens": ["Kampf", "dann", "be\u00b7gun\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Wohlauf ihr Gesellen", "tokens": ["Wohl\u00b7auf", "ihr", "Ge\u00b7sel\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Froh in Reih'n zu stellen.", "tokens": ["Froh", "in", "Reih'n", "zu", "stel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Sonne hoch da leuchtet,", "tokens": ["Son\u00b7ne", "hoch", "da", "leuch\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADV", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wies' im Taue feuchtet,", "tokens": ["Wies'", "im", "Tau\u00b7e", "feuch\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Einer l\u00e4\u00dft vor allen", "tokens": ["Ei\u00b7ner", "l\u00e4\u00dft", "vor", "al\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "PIAT"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Seine Stimm' wohl schallen.", "tokens": ["Sei\u00b7ne", "Stimm'", "wohl", "schal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Wie die wei\u00df' und rote", "tokens": ["Wie", "die", "wei\u00df'", "und", "ro\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "KON", "ADJA"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Bl\u00fct' im Sturm zu Boden,", "tokens": ["Bl\u00fct'", "im", "Sturm", "zu", "Bo\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Also blut't der Ritter", "tokens": ["Al\u00b7so", "blut't", "der", "Rit\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "In der Freunde Mitte.", "tokens": ["In", "der", "Freun\u00b7de", "Mit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "So in roten Wunden", "tokens": ["So", "in", "ro\u00b7ten", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Alles Leids gesunder,", "tokens": ["Al\u00b7les", "Leids", "ge\u00b7sun\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "H\u00f6ret wie Roland all", "tokens": ["H\u00f6\u00b7ret", "wie", "Ro\u00b7land", "all"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "KOKOM", "NE", "PIAT"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Fiel dort in Roncisvall.", "tokens": ["Fiel", "dort", "in", "Ron\u00b7cis\u00b7vall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "War er da verraten,", "tokens": ["War", "er", "da", "ver\u00b7ra\u