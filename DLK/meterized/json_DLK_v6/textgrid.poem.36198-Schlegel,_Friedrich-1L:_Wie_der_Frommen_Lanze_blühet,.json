": "Roland blieb noch eine,", "tokens": ["Ro\u00b7land", "blieb", "noch", "ei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ART", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Sah der Mannen keinen,", "tokens": ["Sah", "der", "Man\u00b7nen", "kei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PIAT", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Noch sein Horn erklungen,", "tokens": ["Noch", "sein", "Horn", "er\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Da\u00df es mitten sprunge.", "tokens": ["Da\u00df", "es", "mit\u00b7ten", "sprun\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Lied mu\u00df erklingen,", "tokens": ["Lied", "mu\u00df", "er\u00b7klin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Schlacht dann beginnen,", "tokens": ["Schlacht", "dann", "be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "H\u00f6ret wie Roland all", "tokens": ["H\u00f6\u00b7ret", "wie", "Ro\u00b7land", "all"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "KOKOM", "NE", "PIAT"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Fiel dort in Roncisvall.", "tokens": ["Fiel", "dort", "in", "Ron\u00b7cis\u00b7vall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "