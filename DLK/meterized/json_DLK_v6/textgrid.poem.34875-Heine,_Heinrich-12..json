{"textgrid.poem.34875": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Deutschland trank sich einen Zopf,", "tokens": ["Mein", "Deutschland", "trank", "sich", "ei\u00b7nen", "Zopf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und du, du glaubtest den Toasten!", "tokens": ["Und", "du", ",", "du", "glaub\u00b7test", "den", "Toas\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Du glaubtest jedem Pfeifenkopf", "tokens": ["Du", "glaub\u00b7test", "je\u00b7dem", "Pfei\u00b7fen\u00b7kopf"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und seinen schwarzrotgoldnen Quasten.", "tokens": ["Und", "sei\u00b7nen", "schwarz\u00b7rot\u00b7gold\u00b7nen", "Quas\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch als der holde Rausch entwich,", "tokens": ["Doch", "als", "der", "hol\u00b7de", "Rausch", "ent\u00b7wich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein teurer Freund, du warst betroffen \u2013", "tokens": ["Mein", "teu\u00b7rer", "Freund", ",", "du", "warst", "be\u00b7trof\u00b7fen", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPER", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das Volk wie katzenj\u00e4mmerlich,", "tokens": ["Das", "Volk", "wie", "kat\u00b7zen\u00b7j\u00e4m\u00b7mer\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das eben noch so sch\u00f6n besoffen!", "tokens": ["Das", "e\u00b7ben", "noch", "so", "sch\u00f6n", "be\u00b7sof\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ein schimpfender Bedientenschwarm,", "tokens": ["Ein", "schimp\u00b7fen\u00b7der", "Be\u00b7dien\u00b7ten\u00b7schwarm", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und faule \u00c4pfel statt der Kr\u00e4nze \u2013", "tokens": ["Und", "fau\u00b7le", "\u00c4p\u00b7fel", "statt", "der", "Kr\u00e4n\u00b7ze", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "An jeder Seite ein Gendarm,", "tokens": ["An", "je\u00b7der", "Sei\u00b7te", "ein", "Gen\u00b7darm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Erreichtest endlich du die Grenze.", "tokens": ["Er\u00b7reich\u00b7test", "end\u00b7lich", "du", "die", "Gren\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Dort bleibst du stehn. Wehmut ergreift", "tokens": ["Dort", "bleibst", "du", "stehn", ".", "Weh\u00b7mut", "er\u00b7greift"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVINF", "$.", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dich bei dem Anblick jener Pf\u00e4hle,", "tokens": ["Dich", "bei", "dem", "An\u00b7blick", "je\u00b7ner", "Pf\u00e4h\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "PDAT", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Die wie das Zebra sind gestreift,", "tokens": ["Die", "wie", "das", "Zeb\u00b7ra", "sind", "ge\u00b7streift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Seufzer dringen aus der Seele:", "tokens": ["Und", "Seuf\u00b7zer", "drin\u00b7gen", "aus", "der", "See\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bbaranjuez, in deinem Sand,", "tokens": ["\u00bb", "a\u00b7ran\u00b7juez", ",", "in", "dei\u00b7nem", "Sand", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "NE", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie schnell die sch\u00f6nen Tage schwanden,", "tokens": ["Wie", "schnell", "die", "sch\u00f6\u00b7nen", "Ta\u00b7ge", "schwan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wo ich vor K\u00f6nig Philipp stand", "tokens": ["Wo", "ich", "vor", "K\u00f6\u00b7nig", "Phi\u00b7lipp", "stand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "NN", "NE", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und seinen uckerm\u00e4rk'schen Granden.", "tokens": ["Und", "sei\u00b7nen", "u\u00b7cker\u00b7m\u00e4rk'\u00b7schen", "Gran\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Er hat mir Beifall zugenickt,", "tokens": ["Er", "hat", "mir", "Bei\u00b7fall", "zu\u00b7ge\u00b7nickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als ich gespielt den Marquis Posa;", "tokens": ["Als", "ich", "ge\u00b7spielt", "den", "Mar\u00b7quis", "Po\u00b7sa", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "ART", "NN", "NE", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "In Versen hab ich ihn entz\u00fcckt,", "tokens": ["In", "Ver\u00b7sen", "hab", "ich", "ihn", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch ihm gefiel nicht meine Prosa.\u00ab", "tokens": ["Doch", "ihm", "ge\u00b7fiel", "nicht", "mei\u00b7ne", "Pro\u00b7sa", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}}}}