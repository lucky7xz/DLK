{"dta.poem.11005": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vern\u00fcnftiges  \n Gebeth.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dieweil es nicht in unsrer Macht,", "tokens": ["Die\u00b7weil", "es", "nicht", "in", "uns\u00b7rer", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu denken immer, wie wir sollen,", "tokens": ["Zu", "den\u00b7ken", "im\u00b7mer", ",", "wie", "wir", "sol\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADV", "$,", "PWAV", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zu glauben alles, was wir wollen:", "tokens": ["Zu", "glau\u00b7ben", "al\u00b7les", ",", "was", "wir", "wol\u00b7len", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PIS", "$,", "PRELS", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So nehmet doch die Zeit in Acht,", "tokens": ["So", "neh\u00b7met", "doch", "die", "Zeit", "in", "Acht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "APPR", "CARD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn nicht zu viele Leidenschaften,", "tokens": ["Wenn", "nicht", "zu", "vie\u00b7le", "Lei\u00b7den\u00b7schaf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Durch Umst\u00e4nd, an den Sinnen haften,", "tokens": ["Durch", "Um\u00b7st\u00e4nd", ",", "an", "den", "Sin\u00b7nen", "haf\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Und rufet, mit Vertrauen, dann,", "tokens": ["Und", "ru\u00b7fet", ",", "mit", "Ver\u00b7trau\u00b7en", ",", "dann", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "NN", "$,", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und einem kindlich-br\u00fcnstgen Triebe,", "tokens": ["Und", "ei\u00b7nem", "kind\u00b7lich\u00b7br\u00fcnst\u00b7gen", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Den Sch\u00f6pfer um die Weisheit an:", "tokens": ["Den", "Sch\u00f6p\u00b7fer", "um", "die", "Weis\u00b7heit", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "So wird er, als die ewge Liebe,", "tokens": ["So", "wird", "er", ",", "als", "die", "ew\u00b7ge", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Ein solches wohlgemeyntes Flehn,", "tokens": ["Ein", "sol\u00b7ches", "wohl\u00b7ge\u00b7meyn\u00b7tes", "Flehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Voll v\u00e4terlicher G\u00fcte, sehn,", "tokens": ["Voll", "v\u00e4\u00b7ter\u00b7li\u00b7cher", "G\u00fc\u00b7te", ",", "sehn", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und voller ewgen Z\u00e4rtlichkeiten,", "tokens": ["Und", "vol\u00b7ler", "ew\u00b7gen", "Z\u00e4rt\u00b7lich\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Euch schon auf solche Wege leiten,", "tokens": ["Euch", "schon", "auf", "sol\u00b7che", "We\u00b7ge", "lei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Damit ihr seinen guten Willen,", "tokens": ["Da\u00b7mit", "ihr", "sei\u00b7nen", "gu\u00b7ten", "Wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Bey eurem Hierseyn, m\u00f6gt erf\u00fcllen.", "tokens": ["Bey", "eu\u00b7rem", "Hier\u00b7seyn", ",", "m\u00f6gt", "er\u00b7f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}