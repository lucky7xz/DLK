{"textgrid.poem.67589": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwei Hirsche fl\u00fcstern sich ins Ohr;", "tokens": ["Zwei", "Hir\u00b7sche", "fl\u00fcs\u00b7tern", "sich", "ins", "Ohr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da streckt der Fuchs in allen Ehren", "tokens": ["Da", "streckt", "der", "Fuchs", "in", "al\u00b7len", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NE", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sein neubegierig Haupt hervor:", "tokens": ["Sein", "neu\u00b7be\u00b7gie\u00b7rig", "Haupt", "her\u00b7vor", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbdarf ich auch h\u00f6ren?\u00ab", "tokens": ["\u00bb", "darf", "ich", "auch", "h\u00f6\u00b7ren", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VMFIN", "PPER", "ADV", "VVINF", "$.", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "\u00bbwer will Dir's wehren?", "tokens": ["\u00bb", "wer", "will", "Dir's", "weh\u00b7ren", "?"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Wir haben gar nichts vor.\u00ab", "tokens": ["Wir", "ha\u00b7ben", "gar", "nichts", "vor", ".", "\u00ab"], "token_info": ["word", "word", 