{"textgrid.poem.66305": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Ein Blick auf die Welt", "genre": "verse", "period": "N.A.", "pub_year": 1767, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Welt, die nur irdische Gem\u00fcther", "tokens": ["Welt", ",", "die", "nur", "ir\u00b7di\u00b7sche", "Ge\u00b7m\u00fc\u00b7ther"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "ADV", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Mit ihren Banden fesseln kann,", "tokens": ["Mit", "ih\u00b7ren", "Ban\u00b7den", "fes\u00b7seln", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Christ schaut alle deine G\u00fcter", "tokens": ["Der", "Christ", "schaut", "al\u00b7le", "dei\u00b7ne", "G\u00fc\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PIS", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf einer andern Seite an,", "tokens": ["Auf", "ei\u00b7ner", "an\u00b7dern", "Sei\u00b7te", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dein Gut ist Staub! dein Stolz zerf\u00e4llt!", "tokens": ["Dein", "Gut", "ist", "Staub", "!", "dein", "Stolz", "zer\u00b7f\u00e4llt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$.", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Tand ist dein Gl\u00fcck! o arme Welt!", "tokens": ["Tand", "ist", "dein", "Gl\u00fcck", "!", "o", "ar\u00b7me", "Welt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$.", "FM", "ADJA", "NN", "$."], "meter": "++-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mein Glaube steht auf einem Berge", "tokens": ["Mein", "Glau\u00b7be", "steht", "auf", "ei\u00b7nem", "Ber\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und schaut ins weite Thal hinab;", "tokens": ["Und", "schaut", "ins", "wei\u00b7te", "Thal", "hin\u00b7ab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Riesenkinder werden Zwerge", "tokens": ["Die", "Rie\u00b7sen\u00b7kin\u00b7der", "wer\u00b7den", "Zwer\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und jeder H\u00fcgel wird ein Grab.", "tokens": ["Und", "je\u00b7der", "H\u00fc\u00b7gel", "wird", "ein", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gott, welch ein Zug! erst Gram und Noth,", "tokens": ["Gott", ",", "welch", "ein", "Zug", "!", "erst", "Gram", "und", "Noth", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAT", "ART", "NN", "$.", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Furcht, Hoffnung, und zuletzt der Tod.", "tokens": ["Furcht", ",", "Hoff\u00b7nung", ",", "und", "zu\u00b7letzt", "der", "Tod", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "KON", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O welche Aussicht in die Weite!", "tokens": ["O", "wel\u00b7che", "Aus\u00b7sicht", "in", "die", "Wei\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Unschuld blickt durch einen Flor;", "tokens": ["Die", "Un\u00b7schuld", "blickt", "durch", "ei\u00b7nen", "Flor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Tugend geht im Trauerkleide", "tokens": ["Die", "Tu\u00b7gend", "geht", "im", "Trau\u00b7er\u00b7klei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Klagen wimmern laut empor.", "tokens": ["Und", "Kla\u00b7gen", "wim\u00b7mern", "laut", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Seuchen liegen auf der Luft,", "tokens": ["Die", "Seu\u00b7chen", "lie\u00b7gen", "auf", "der", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und \u00fcberall ist eine Gruft.", "tokens": ["Und", "\u00fc\u00b7be\u00b7rall", "ist", "ei\u00b7ne", "Gruft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Welch ein Get\u00fcmmel von Betr\u00fcbten!", "tokens": ["Welch", "ein", "Ge\u00b7t\u00fcm\u00b7mel", "von", "Be\u00b7tr\u00fcb\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und Gott, wie jammern sie so laut;", "tokens": ["Und", "Gott", ",", "wie", "jam\u00b7mern", "sie", "so", "laut", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PWAV", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dort klagt ein Weib um den Geliebten,", "tokens": ["Dort", "klagt", "ein", "Weib", "um", "den", "Ge\u00b7lieb\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und hier ein J\u00fcngling um die Braut.", "tokens": ["Und", "hier", "ein", "J\u00fcng\u00b7ling", "um", "die", "Braut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von jedem H\u00fcgel jammert Noth", "tokens": ["Von", "je\u00b7dem", "H\u00fc\u00b7gel", "jam\u00b7mert", "Noth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und hier ist Tod und da ist Tod.", "tokens": ["Und", "hier", "ist", "Tod", "und", "da", "ist", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "NN", "KON", "ADV", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich kann nicht mehr; mit schwachen F\u00fc\u00dfen", "tokens": ["Ich", "kann", "nicht", "mehr", ";", "mit", "schwa\u00b7chen", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Eil' ich von meinem Berg hinab.", "tokens": ["Eil'", "ich", "von", "mei\u00b7nem", "Berg", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es klopft mein Herz und Thr\u00e4nen flie\u00dfen;", "tokens": ["Es", "klopft", "mein", "Herz", "und", "Thr\u00e4\u00b7nen", "flie\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wohin sie flie\u00dfen, ist ein Grab.", "tokens": ["Wo\u00b7hin", "sie", "flie\u00b7\u00dfen", ",", "ist", "ein", "Grab", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "$,", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich stehe, wandle, sinke hin,", "tokens": ["Ich", "ste\u00b7he", ",", "wand\u00b7le", ",", "sin\u00b7ke", "hin", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und Grab ist alles, wo ich bin.", "tokens": ["Und", "Grab", "ist", "al\u00b7les", ",", "wo", "ich", "bin", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PIS", "$,", "PWAV", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Tod und Verwesung und Verderben", "tokens": ["Tod", "und", "Ver\u00b7we\u00b7sung", "und", "Ver\u00b7der\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "KON", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Beschlie\u00dfen unsern Lebenslauf.", "tokens": ["Be\u00b7schlie\u00b7\u00dfen", "un\u00b7sern", "Le\u00b7bens\u00b7lauf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn: ", "tokens": ["Denn", ":"], "token_info": ["word", "punct"], "pos": ["KON", "$."], "meter": "+", "measure": "single.up"}, "line.4": {"text": "T\u00f6nt ja von jedem Sarg herauf.", "tokens": ["T\u00f6nt", "ja", "von", "je\u00b7dem", "Sarg", "her\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auch mir schlie\u00dft einst des Todes Ruh'", "tokens": ["Auch", "mir", "schlie\u00dft", "einst", "des", "To\u00b7des", "Ruh'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "ART", "NN", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Die wundgeweinten Augen zu.", "tokens": ["Die", "wund\u00b7ge\u00b7wein\u00b7ten", "Au\u00b7gen", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Nun, ich entfliehe mit den Frommen", "tokens": ["Nun", ",", "ich", "ent\u00b7flie\u00b7he", "mit", "den", "From\u00b7men"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dir, Welt, und aller deiner Pracht.", "tokens": ["Dir", ",", "Welt", ",", "und", "al\u00b7ler", "dei\u00b7ner", "Pracht", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "KON", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr aber seid mir jetzt willkommen,", "tokens": ["Ihr", "a\u00b7ber", "seid", "mir", "jetzt", "will\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Tod, Sarg und Grab und Mitternacht!", "tokens": ["Tod", ",", "Sarg", "und", "Grab", "und", "Mit\u00b7ter\u00b7nacht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es flammt das Wort in meiner Brust:", "tokens": ["Es", "flammt", "das", "Wort", "in", "mei\u00b7ner", "Brust", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}