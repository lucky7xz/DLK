{"textgrid.poem.43357": {"metadata": {"author": {"name": "Leopardi, Giacomo", "birth": "N.A.", "death": "N.A."}, "title": "XxIV. Die Ruhe nach dem Gewitter.", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Wetter ist vergangen.", "tokens": ["Das", "Wet\u00b7ter", "ist", "ver\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die muntern V\u00f6gel fangen an zu singen,", "tokens": ["Die", "mun\u00b7tern", "V\u00f6\u00b7gel", "fan\u00b7gen", "an", "zu", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Henne wagt mit Gackern", "tokens": ["Die", "Hen\u00b7ne", "wagt", "mit", "Ga\u00b7ckern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sich auf die Stra\u00dfe wieder. Sieh, wie pl\u00f6tzlich", "tokens": ["Sich", "auf", "die", "Stra\u00b7\u00dfe", "wie\u00b7der", ".", "Sieh", ",", "wie", "pl\u00f6tz\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PRF", "APPR", "ART", "NN", "ADV", "$.", "NE", "$,", "PWAV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Im West am Berg der Himmel sich erhellt.", "tokens": ["Im", "West", "am", "Berg", "der", "Him\u00b7mel", "sich", "er\u00b7hellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Nun lichtet sich das Feld,", "tokens": ["Nun", "lich\u00b7tet", "sich", "das", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und aus dem Thale gl\u00e4nzt der Flu\u00df herauf.", "tokens": ["Und", "aus", "dem", "Tha\u00b7le", "gl\u00e4nzt", "der", "Flu\u00df", "her\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ein jedes Herz wird froh; all\u00fcberall", "tokens": ["Ein", "je\u00b7des", "Herz", "wird", "froh", ";", "al\u00b7l\u00fc\u00b7be\u00b7rall"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "PIAT", "NN", "VAFIN", "ADJD", "$.", "PIAT"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Beginnt die Arbeit wieder", "tokens": ["Be\u00b7ginnt", "die", "Ar\u00b7beit", "wie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Und regt sich r\u00fcst'ger Schall.", "tokens": ["Und", "regt", "sich", "r\u00fcst'\u00b7ger", "Schall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Der Handwerksmann, sein Werkzeug in der Hand,", "tokens": ["Der", "Hand\u00b7werks\u00b7mann", ",", "sein", "Werk\u00b7zeug", "in", "der", "Hand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Tritt singend, nach dem feuchten Blau zu sp\u00e4hen,", "tokens": ["Tritt", "sin\u00b7gend", ",", "nach", "dem", "feuch\u00b7ten", "Blau", "zu", "sp\u00e4\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Vor seines Hauses Schwelle;", "tokens": ["Vor", "sei\u00b7nes", "Hau\u00b7ses", "Schwel\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Das Weiblein kommt heraus, in ihr Gef\u00e4\u00df", "tokens": ["Das", "Weib\u00b7lein", "kommt", "he\u00b7raus", ",", "in", "ihr", "Ge\u00b7f\u00e4\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Die Regenflut zu fassen.", "tokens": ["Die", "Re\u00b7gen\u00b7flut", "zu", "fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Lautrufend durch die Gassen", "tokens": ["Laut\u00b7ru\u00b7fend", "durch", "die", "Gas\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Zieht mit Gem\u00fcsen wieder", "tokens": ["Zieht", "mit", "Ge\u00b7m\u00fc\u00b7sen", "wie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "ADV"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Der H\u00e4ndler auf und nieder.", "tokens": ["Der", "H\u00e4nd\u00b7ler", "auf", "und", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "O sieh, da kommt die Sonne; wie verkl\u00e4rt", "tokens": ["O", "sieh", ",", "da", "kommt", "die", "Son\u00b7ne", ";", "wie", "ver\u00b7kl\u00e4rt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "PTKVZ", "$,", "ADV", "VVFIN", "ART", "NN", "$.", "PWAV", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Sie H\u00f6h'n und Villen. Die Bewohner \u00f6ffnen", "tokens": ["Sie", "H\u00f6h'n", "und", "Vil\u00b7len", ".", "Die", "Be\u00b7woh\u00b7ner", "\u00f6ff\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "NN", "KON", "NN", "$.", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Terrassen und Balcone. Horch, wie dort", "tokens": ["Ter\u00b7ras\u00b7sen", "und", "Bal\u00b7co\u00b7ne", ".", "Horch", ",", "wie", "dort"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "KON", "NN", "$.", "NE", "$,", "PWAV", "ADV"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.22": {"text": "Vom Fahrweg Schellenl\u00e4uten aus der Ferne", "tokens": ["Vom", "Fahr\u00b7weg", "Schel\u00b7len\u00b7l\u00e4u\u00b7ten", "aus", "der", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Her\u00fcbert\u00f6nt. Des Reisenden Gef\u00e4hrt", "tokens": ["Her\u00b7\u00fc\u00b7ber\u00b7t\u00f6nt", ".", "Des", "Rei\u00b7sen\u00b7den", "Ge\u00b7f\u00e4hrt"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$.", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Knarrt durch den Sand und setzt die Reise fort.", "tokens": ["Knarrt", "durch", "den", "Sand", "und", "setzt", "die", "Rei\u00b7se", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Aufathmet jede Brust.", "tokens": ["Auf\u00b7ath\u00b7met", "je\u00b7de", "Brust", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wann ist das Leben so", "tokens": ["Wann", "ist", "das", "Le\u00b7ben", "so"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie jetzt uns s\u00fc\u00df und froh?", "tokens": ["Wie", "jetzt", "uns", "s\u00fc\u00df", "und", "froh", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wann mag mit solcher Lust", "tokens": ["Wann", "mag", "mit", "sol\u00b7cher", "Lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Man auf sein Tagwerk sinnen,", "tokens": ["Man", "auf", "sein", "Tag\u00b7werk", "sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das alte f\u00f6rdern, neues Thun beginnen?", "tokens": ["Das", "al\u00b7te", "f\u00f6r\u00b7dern", ",", "neu\u00b7es", "Thun", "be\u00b7gin\u00b7nen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVINF", "$,", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wann sind wir minder unsrer Noth gedenk?", "tokens": ["Wann", "sind", "wir", "min\u00b7der", "uns\u00b7rer", "Noth", "ge\u00b7denk", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "O Lust, du Kind des Schmerzes!", "tokens": ["O", "Lust", ",", "du", "Kind", "des", "Schmer\u00b7zes", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "O eitle Freude, Frucht nur", "tokens": ["O", "eit\u00b7le", "Freu\u00b7de", ",", "Frucht", "nur"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "NN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Vergangner Angst, die unser Herz durchbebt,", "tokens": ["Ver\u00b7gang\u00b7ner", "Angst", ",", "die", "un\u00b7ser", "Herz", "durch\u00b7bebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Da\u00df vor dem Tod wir bangen,", "tokens": ["Da\u00df", "vor", "dem", "Tod", "wir", "ban\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Wie bitter auch das Leben,", "tokens": ["Wie", "bit\u00b7ter", "auch", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Da\u00df stumm die armen Thoren,", "tokens": ["Da\u00df", "stumm", "die", "ar\u00b7men", "Tho\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Mit todesbleichen Wangen", "tokens": ["Mit", "to\u00b7des\u00b7blei\u00b7chen", "Wan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Voll Angstschwei\u00df, in des Himmels", "tokens": ["Voll", "A\u00b7ngstschwei\u00df", ",", "in", "des", "Him\u00b7mels"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Gewitterst\u00fcrme blicken,", "tokens": ["Ge\u00b7wit\u00b7ter\u00b7st\u00fcr\u00b7me", "bli\u00b7cken", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Die wider sie verschworen!", "tokens": ["Die", "wi\u00b7der", "sie", "ver\u00b7schwo\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "O g\u00fctige Natur,", "tokens": ["O", "g\u00fc\u00b7ti\u00b7ge", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Das sind die hohen Freuden,", "tokens": ["Das", "sind", "die", "ho\u00b7hen", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Gaben, die du liebreich", "tokens": ["Die", "Ga\u00b7ben", ",", "die", "du", "lieb\u00b7reich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Menschen g\u00f6nnst! Ihm soll es Wonne sein,", "tokens": ["Den", "Men\u00b7schen", "g\u00f6nnst", "!", "Ihm", "soll", "es", "Won\u00b7ne", "sein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VMFIN", "PPER", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wenn von ihm weicht das Leiden.", "tokens": ["Wenn", "von", "ihm", "weicht", "das", "Lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Freigebig theilst du Qualen aus. Der Schmerz", "tokens": ["Frei\u00b7ge\u00b7big", "theilst", "du", "Qua\u00b7len", "aus", ".", "Der", "Schmerz"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "NN", "PTKVZ", "$.", "ART", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "Entspringt von selber, und die karge Lust,", "tokens": ["Ent\u00b7springt", "von", "sel\u00b7ber", ",", "und", "die", "kar\u00b7ge", "Lust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADV", "$,", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Die als ein m\u00e4chtig Wunder hin und wieder", "tokens": ["Die", "als", "ein", "m\u00e4ch\u00b7tig", "Wun\u00b7der", "hin", "und", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "KOUS", "ART", "ADJD", "NN", "PTKVZ", "KON", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Dem Weh entbl\u00fcht, ist schon ein Gl\u00fcck gewesen.", "tokens": ["Dem", "Weh", "ent\u00b7bl\u00fcht", ",", "ist", "schon", "ein", "Gl\u00fcck", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "VAFIN", "ADV", "ART", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "So lieb sind wir den Ew'gen! Gl\u00fccks genug", "tokens": ["So", "lieb", "sind", "wir", "den", "Ew'\u00b7gen", "!", "Gl\u00fccks", "ge\u00b7nug"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "ART", "NN", "$.", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Ein freier Athemzug", "tokens": ["Ein", "frei\u00b7er", "A\u00b7them\u00b7zug"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Nach langem Schmerz, und selig,", "tokens": ["Nach", "lan\u00b7gem", "Schmerz", ",", "und", "se\u00b7lig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Wenn wir im Tod von allem Schmerz genesen.", "tokens": ["Wenn", "wir", "im", "Tod", "von", "al\u00b7lem", "Schmerz", "ge\u00b7ne\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "APPR", "PIS", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}