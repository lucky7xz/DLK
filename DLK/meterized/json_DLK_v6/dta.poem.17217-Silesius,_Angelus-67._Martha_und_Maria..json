{"dta.poem.17217": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "67.  Martha und Maria.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Die Martha laufft und rennt da\u00df sie den HErren speise/", "tokens": ["Die", "Mar\u00b7tha", "laufft", "und", "rennt", "da\u00df", "sie", "den", "Her\u00b7ren", "spei\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "KON", "VVFIN", "KOUS", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Maria sitzet still; und hat doch solcher weise", "tokens": ["Ma\u00b7ria", "sit\u00b7zet", "still", ";", "und", "hat", "doch", "sol\u00b7cher", "wei\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PTKVZ", "$.", "KON", "VAFIN", "ADV", "PIAT", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Das beste theil erw\u00f6hlt: sie speiset jhn allein/", "tokens": ["Das", "bes\u00b7te", "theil", "er\u00b7w\u00f6hlt", ":", "sie", "spei\u00b7set", "jhn", "al\u00b7lein", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$.", "PPER", "VVFIN", "PPER", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die aber findt auch fich von jhm gespeiset seyn.", "tokens": ["Die", "a\u00b7ber", "findt", "auch", "fich", "von", "jhm", "ge\u00b7spei\u00b7set", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "ADV", "ADJD", "APPR", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}