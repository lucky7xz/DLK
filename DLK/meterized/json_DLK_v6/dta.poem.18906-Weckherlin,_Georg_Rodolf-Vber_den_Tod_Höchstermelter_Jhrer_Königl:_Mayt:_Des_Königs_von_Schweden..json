{"dta.poem.18906": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Vber den Tod  \n H\u00f6chstermelter Jhrer K\u00f6nigl: Mayt:  \n Des K\u00f6nigs von Schweden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.99"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Dein aigner muht/ O Held/ weil Gotsforcht/ Ehr", "tokens": ["Dein", "aig\u00b7ner", "muht", "/", "O", "Held", "/", "weil", "Gots\u00b7forcht", "/", "Ehr"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPOSAT", "ADJA", "VVFIN", "$(", "NE", "NN", "$(", "KOUS", "NN", "$(", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "vnd Recht", "tokens": ["vnd", "Recht"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Dein hertz vn\u0303 schwerd allein gest\u00f6rcket vn\u0303 gew\u00f6tzet/", "tokens": ["Dein", "hertz", "v\u00f1", "schwerd", "al\u00b7lein", "ge\u00b7st\u00f6r\u00b7cket", "v\u00f1", "ge\u00b7w\u00f6t\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil auch der erdenkrai\u00df f\u00fcr dich zu eng vnnd", "tokens": ["Weil", "auch", "der", "er\u00b7den\u00b7krai\u00df", "f\u00fcr", "dich", "zu", "eng", "vnnd"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "PPER", "PTKA", "ADJD", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "schlecht/", "tokens": ["schlecht", "/"], "token_info": ["word", "punct"], "pos": ["ADJD", "$("], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Hat in den himmel dich (zu fr\u00fch f\u00fcr vns) vers\u00f6tzet.", "tokens": ["Hat", "in", "den", "him\u00b7mel", "dich", "(", "zu", "fr\u00fch", "f\u00fcr", "vns", ")", "ver\u00b7s\u00f6t\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "PPER", "$(", "PTKA", "ADJD", "APPR", "PPER", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dan gleichwie deine faust der gl\u00e4ubigen geschlecht/", "tokens": ["Dan", "gleich\u00b7wie", "dei\u00b7ne", "faust", "der", "gl\u00e4u\u00b7bi\u00b7gen", "ge\u00b7schlecht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "PPOSAT", "NN", "ART", "ADJA", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als es in h\u00f6chster noht/ err\u00f6ttet vnd erg\u00f6tzet:", "tokens": ["Als", "es", "in", "h\u00f6chs\u00b7ter", "noht", "/", "er\u00b7r\u00f6t\u00b7tet", "vnd", "er\u00b7g\u00f6t\u00b7zet", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "$(", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Also hat durch dein haupt die kugel (layder!) recht", "tokens": ["Al\u00b7so", "hat", "durch", "dein", "haupt", "die", "ku\u00b7gel", "(", "lay\u00b7der", "!", ")", "recht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "punct", "word"], "pos": ["ADV", "VAFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "$(", "PTKVZ", "$.", "$(", "ADJD"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Der Teutschen Freyheit hertz vnd Tugent haupt", "tokens": ["Der", "Teut\u00b7schen", "Frey\u00b7heit", "hertz", "vnd", "Tu\u00b7gent", "haupt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "KON", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "verl\u00f6tzet.", "tokens": ["ver\u00b7l\u00f6t\u00b7zet", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Sigreich vnd seelig zwar hat dich/ weil in der schlacht", "tokens": ["Sig\u00b7reich", "vnd", "see\u00b7lig", "zwar", "hat", "dich", "/", "weil", "in", "der", "schlacht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "KON", "ADJD", "ADV", "VAFIN", "PPER", "$(", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du frey f\u00fcr Gottes wort dein thewres blut ver-", "tokens": ["Du", "frey", "f\u00fcr", "Got\u00b7tes", "wort", "dein", "thew\u00b7res", "blut", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "APPR", "NN", "NN", "PPOSAT", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "gossen", "tokens": ["gos\u00b7sen"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "In die endlose frewd vnd ehr dein end gebracht:", "tokens": ["In", "die", "end\u00b7lo\u00b7se", "frewd", "vnd", "ehr", "dein", "end", "ge\u00b7bracht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Jedoch in layd vnd noht seind deine bunds-genossen/", "tokens": ["Je\u00b7doch", "in", "layd", "vnd", "noht", "seind", "dei\u00b7ne", "bunds\u00b7ge\u00b7nos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "VAFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil deine herrschung du mit sig/ triumff vnd", "tokens": ["Weil", "dei\u00b7ne", "herr\u00b7schung", "du", "mit", "sig", "/", "tri\u00b7umff", "vnd"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "APPR", "ADJD", "$(", "VVFIN", "KON"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "pracht", "tokens": ["pracht"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Dort in dem himmelreich anfangend/ hie be-", "tokens": ["Dort", "in", "dem", "him\u00b7mel\u00b7reich", "an\u00b7fan\u00b7gend", "/", "hie", "be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJD", "VVPP", "$(", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "schlossen.", "tokens": ["schlos\u00b7sen", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}