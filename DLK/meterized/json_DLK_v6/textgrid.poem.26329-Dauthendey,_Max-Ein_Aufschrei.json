{"textgrid.poem.26329": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Ein Aufschrei", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Aufschrei steckt in meiner Brust,", "tokens": ["Ein", "Auf\u00b7schrei", "steckt", "in", "mei\u00b7ner", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es schreit aus mir die Heimwehlust.", "tokens": ["Es", "schreit", "aus", "mir", "die", "Heim\u00b7weh\u00b7lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wie ein Sterbender sich streckt,", "tokens": ["Und", "wie", "ein", "Ster\u00b7ben\u00b7der", "sich", "streckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Geist sich nach der Heimat reckt.", "tokens": ["Mein", "Geist", "sich", "nach", "der", "Hei\u00b7mat", "reckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er will nichts sehn, nichts h\u00f6ren mehr,", "tokens": ["Er", "will", "nichts", "sehn", ",", "nichts", "h\u00f6\u00b7ren", "mehr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "VVINF", "$,", "PIS", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Fremde ist ihm menschenleer.", "tokens": ["Die", "Frem\u00b7de", "ist", "ihm", "men\u00b7schen\u00b7leer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die fremden Worte sind ihm Last,", "tokens": ["Die", "frem\u00b7den", "Wor\u00b7te", "sind", "ihm", "Last", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die fremde Luft mein Atem ha\u00dft.", "tokens": ["Die", "frem\u00b7de", "Luft", "mein", "A\u00b7tem", "ha\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Gefangenschaft macht grau mein Haar.", "tokens": ["Ge\u00b7fan\u00b7gen\u00b7schaft", "macht", "grau", "mein", "Haar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "O Leben, das mich einst gebar,", "tokens": ["O", "Le\u00b7ben", ",", "das", "mich", "einst", "ge\u00b7bar", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "La\u00df mich zur Heimat! H\u00f6r' den Schrei.", "tokens": ["La\u00df", "mich", "zur", "Hei\u00b7mat", "!", "H\u00f6r'", "den", "Schrei", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "$.", "NE", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Allmacht des Lebens, mach' mich frei.", "tokens": ["All\u00b7macht", "des", "Le\u00b7bens", ",", "mach'", "mich", "frei", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}