{"dta.poem.21702": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Klage um Ho\u00ebl.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1802", "urn": "urn:nbn:de:kobv:b4-200905193310", "language": ["de:0.85", "sv:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "H&#228;tt ich nur des Waldstroms Kraft", "tokens": ["H", "&#228;", "tt", "ich", "nur", "des", "Wald\u00b7stroms", "Kraft"], "token_info": ["word", "XML_entity", "word", "word", "word", "word", "word", "word"], "pos": ["XY", "$(", "VVFIN", "PPER", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit grimmiger w\u00fcthiger Leidenschaft", "tokens": ["Mit", "grim\u00b7mi\u00b7ger", "w\u00fct\u00b7hi\u00b7ger", "Lei\u00b7den\u00b7schaft"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zu st\u00fcrzen auf ", "tokens": ["Zu", "st\u00fcr\u00b7zen", "auf"], "token_info": ["word", "word", "word"], "pos": ["PTKZU", "VVINF", "APPR"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Und aus der Welt sie wegzuspotten.", "tokens": ["Und", "aus", "der", "Welt", "sie", "weg\u00b7zus\u00b7pot\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mein Freund, mein ", "tokens": ["Mein", "Freund", ",", "mein"], "token_info": ["word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT"], "meter": "++-", "measure": "unknown.measure.di"}, "line.2": {"text": "Gestemmt auf seinen tapfern Sinn.", "tokens": ["Ge\u00b7stemmt", "auf", "sei\u00b7nen", "tap\u00b7fern", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von ", "tokens": ["Von"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Kein schn\u00f6des Gold er heischt. Geschm\u00fcckt", "tokens": ["Kein", "schn\u00f6\u00b7des", "Gold", "er", "heischt", ".", "Ge\u00b7schm\u00fcckt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PIAT", "ADJA", "NN", "PPER", "VVFIN", "$.", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit seiner Jugend Herrlichkeit", "tokens": ["Mit", "sei\u00b7ner", "Ju\u00b7gend", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Erwarb er sich die sch\u00f6nste Maid.", "tokens": ["Er\u00b7warb", "er", "sich", "die", "sch\u00f6ns\u00b7te", "Maid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "R\u00fcstig zum Kampf, gesch\u00fcrzt zum Siegen", "tokens": ["R\u00fcs\u00b7tig", "zum", "Kampf", ",", "ge\u00b7sch\u00fcrzt", "zum", "Sie\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "$,", "VVPP", "APPRART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Vierhundert wackre Krieger stiegen", "tokens": ["Vier\u00b7hun\u00b7dert", "wack\u00b7re", "Krie\u00b7ger", "stie\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hinab in ", "tokens": ["Hin\u00b7ab", "in"], "token_info": ["word", "word"], "pos": ["ADV", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Wie blitzten die Reihen im Sonnenstrahl!", "tokens": ["Wie", "blitz\u00b7ten", "die", "Rei\u00b7hen", "im", "Son\u00b7nen\u00b7strahl", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Gluthroth brannten der J\u00fcnglinge Backen.", "tokens": ["Gluth\u00b7roth", "brann\u00b7ten", "der", "J\u00fcng\u00b7lin\u00b7ge", "Ba\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+++-+-", "measure": "unknown.measure.hexa"}, "line.6": {"text": "Es schm\u00fcckte jedes Kriegers Nacken", "tokens": ["Es", "schm\u00fcck\u00b7te", "je\u00b7des", "Krie\u00b7gers", "Na\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ein Kettlein sch\u00f6n aus Gold geschlungen.", "tokens": ["Ein", "Ket\u00b7tlein", "sch\u00f6n", "aus", "Gold", "ge\u00b7schlun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zu rasch ach! tranken die braven Jungen", "tokens": ["Zu", "rasch", "ach", "!", "tran\u00b7ken", "die", "bra\u00b7ven", "Jun\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "ITJ", "$.", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Des Nektars, den die Biene braut,", "tokens": ["Des", "Nek\u00b7tars", ",", "den", "die", "Bie\u00b7ne", "braut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Des Rauschsafts, den das Ausland baut.", "tokens": ["Des", "Rauschs\u00b7afts", ",", "den", "das", "Aus\u00b7land", "baut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.11": {"text": "Sie jauchzen, sie taumeln, sie sinken nieder.", "tokens": ["Sie", "jauch\u00b7zen", ",", "sie", "tau\u00b7meln", ",", "sie", "sin\u00b7ken", "nie\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "PPER", "VVINF", "$,", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.12": {"text": "Aus Cattrack's Thal kehrt keiner wieder.", "tokens": ["Aus", "Cat\u00b7track's", "Thal", "kehrt", "kei\u00b7ner", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "PIS", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Keiner als ", "tokens": ["Kei\u00b7ner", "als"], "token_info": ["word", "word"], "pos": ["PIS", "KOKOM"], "meter": "+-+", "measure": "trochaic.di"}, "line.14": {"text": "Hindurch uns hauend w\u00fcthiglich", "tokens": ["Hin\u00b7durch", "uns", "hau\u00b7end", "w\u00fct\u00b7hig\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPER", "ADJD", "ADJD"], "meter": "+--+-+--", "measure": "iambic.tri.invert"}, "line.15": {"text": "Leben wir ", "tokens": ["Le\u00b7ben", "wir"], "token_info": ["word", "word"], "pos": ["NN", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.16": {"text": "Und singen, wie die Helden gefallen.", "tokens": ["Und", "sin\u00b7gen", ",", "wie", "die", "Hel\u00b7den", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}