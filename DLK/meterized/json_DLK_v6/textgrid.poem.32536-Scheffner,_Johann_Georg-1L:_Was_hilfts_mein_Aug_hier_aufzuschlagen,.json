{"textgrid.poem.32536": {"metadata": {"author": {"name": "Scheffner, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was hilfts mein Aug hier aufzuschlagen,", "genre": "verse", "period": "N.A.", "pub_year": 1778, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was hilfts mein Aug hier aufzuschlagen,", "tokens": ["Was", "hilfts", "mein", "Aug", "hier", "auf\u00b7zu\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und meine Reize Schau zu tragen?", "tokens": ["Und", "mei\u00b7ne", "Rei\u00b7ze", "Schau", "zu", "tra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hier bin ich doch nicht sch\u00f6n, nicht K\u00f6nigin der Welt;", "tokens": ["Hier", "bin", "ich", "doch", "nicht", "sch\u00f6n", ",", "nicht", "K\u00f6\u00b7ni\u00b7gin", "der", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PTKNEG", "ADJD", "$,", "PTKNEG", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zwar steht mein Bild hier aufgestellt,", "tokens": ["Zwar", "steht", "mein", "Bild", "hier", "auf\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Allein du darfst dein Herz nur fragen,", "tokens": ["Al\u00b7lein", "du", "darfst", "dein", "Herz", "nur", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So wird dir sein Orakel sagen:", "tokens": ["So", "wird", "dir", "sein", "O\u00b7ra\u00b7kel", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die Staue bleibt hier so lang nur aufgestellt,", "tokens": ["Die", "Stau\u00b7e", "bleibt", "hier", "so", "lang", "nur", "auf\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Bis die, vor der sich tausend Herzen neigen", "tokens": ["Bis", "die", ",", "vor", "der", "sich", "tau\u00b7send", "Her\u00b7zen", "nei\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "$,", "APPR", "PRELS", "PRF", "CARD", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Bis ", "tokens": ["Bis"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}, "line.10": {"text": "In diesem Heiligthum sich selbst zu zeigen.", "tokens": ["In", "die\u00b7sem", "Hei\u00b7lig\u00b7thum", "sich", "selbst", "zu", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PRF", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}