{"textgrid.poem.30216": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "30.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weil Junge denn Alte weit muthiger springen,", "tokens": ["Weil", "Jun\u00b7ge", "denn", "Al\u00b7te", "weit", "mut\u00b7hi\u00b7ger", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "ADJD", "ADJD", "VVINF", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Weil Junge denn Alte weit lustiger singen,", "tokens": ["Weil", "Jun\u00b7ge", "denn", "Al\u00b7te", "weit", "lus\u00b7ti\u00b7ger", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "ADJD", "ADJD", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Weil Junge denn Alte weit r\u00fcstiger j\u00fcngen:", "tokens": ["Weil", "Jun\u00b7ge", "denn", "Al\u00b7te", "weit", "r\u00fcs\u00b7ti\u00b7ger", "j\u00fcn\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+--+-++--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "So pflegt es den Jungen bey solcherley Dingen", "tokens": ["So", "pflegt", "es", "den", "Jun\u00b7gen", "bey", "sol\u00b7cher\u00b7ley", "Din\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "PIAT", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Bey Jungfern und Witwen f\u00fcr Alten gelingen,", "tokens": ["Bey", "Jung\u00b7fern", "und", "Wit\u00b7wen", "f\u00fcr", "Al\u00b7ten", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Da\u00df leichtlich sie niessen und leichtlich erringen,", "tokens": ["Da\u00df", "leicht\u00b7lich", "sie", "nies\u00b7sen", "und", "leicht\u00b7lich", "er\u00b7rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "VVPP", "KON", "ADJD", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Was pfleget zu sch\u00f6nen, was pfleget zu klingen.", "tokens": ["Was", "pfle\u00b7get", "zu", "sch\u00f6\u00b7nen", ",", "was", "pfle\u00b7get", "zu", "klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PTKZU", "VVINF", "$,", "PWS", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}}}}