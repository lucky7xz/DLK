{"textgrid.poem.53896": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Joachim der Erste (genannt Ringel)", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.71", "da:0.28"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weil aber der Morgen kommt,", "tokens": ["Weil", "a\u00b7ber", "der", "Mor\u00b7gen", "kommt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "der alle Tr\u00e4nen trocknet \u2013", "tokens": ["der", "al\u00b7le", "Tr\u00e4\u00b7nen", "trock\u00b7net", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das hei\u00dft bei Storm:", "tokens": ["Das", "hei\u00dft", "bei", "Storm", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Dies aber kann ich nicht ertragen,", "tokens": ["Dies", "a\u00b7ber", "kann", "ich", "nicht", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "da\u00df so wie sonst die Stunden gehn;", "tokens": ["da\u00df", "so", "wie", "sonst", "die", "Stun\u00b7den", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "KOKOM", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "da\u00df so wie sonst die Uhren schlagen \u2013", "tokens": ["da\u00df", "so", "wie", "sonst", "die", "Uh\u00b7ren", "schla\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "KOKOM", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie seine eigene Spucke schmeckt,", "tokens": ["Wie", "sei\u00b7ne", "ei\u00b7ge\u00b7ne", "Spu\u00b7cke", "schmeckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "das wei\u00df man nicht.", "tokens": ["das", "wei\u00df", "man", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "La\u00df du doch das Klavier in Ruhe,", "tokens": ["La\u00df", "du", "doch", "das", "Kla\u00b7vier", "in", "Ru\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "es hat dir nichts getan.", "tokens": ["es", "hat", "dir", "nichts", "ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIS", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nimm lieber deine Gummischuhe", "tokens": ["Nimm", "lie\u00b7ber", "dei\u00b7ne", "Gum\u00b7mi\u00b7schu\u00b7he"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "und bring mich an die Bahn.", "tokens": ["und", "bring", "mich", "an", "die", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und die reine Liebe wird vergossen", "tokens": ["Und", "die", "rei\u00b7ne", "Lie\u00b7be", "wird", "ver\u00b7gos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "im Vorbei.", "tokens": ["im", "Vor\u00b7bei", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}