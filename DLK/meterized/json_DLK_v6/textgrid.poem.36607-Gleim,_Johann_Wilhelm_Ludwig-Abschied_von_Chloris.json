{"textgrid.poem.36607": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Abschied von Chloris", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Sch\u00f6nen zittert gar zu leicht,", "tokens": ["Ihr", "Sch\u00f6\u00b7nen", "zit\u00b7tert", "gar", "zu", "leicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn Amor euch bekriegt;", "tokens": ["Wenn", "A\u00b7mor", "euch", "be\u00b7kriegt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denn, eh euch noch sein Pfeil erreicht,", "tokens": ["Denn", ",", "eh", "euch", "noch", "sein", "Pfeil", "er\u00b7reicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hat er euch schon besiegt.", "tokens": ["Hat", "er", "euch", "schon", "be\u00b7siegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die mich nicht ha\u00dft, eh sie mich liebt,", "tokens": ["Die", "mich", "nicht", "ha\u00dft", ",", "eh", "sie", "mich", "liebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "VVFIN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die mir nicht widersteht,", "tokens": ["Die", "mir", "nicht", "wi\u00b7der\u00b7steht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die sich, wie Leipzig, leicht ergiebt,", "tokens": ["Die", "sich", ",", "wie", "Leip\u00b7zig", ",", "leicht", "er\u00b7giebt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PRF", "$,", "PWAV", "NE", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die wird von mir verschm\u00e4ht.", "tokens": ["Die", "wird", "von", "mir", "ver\u00b7schm\u00e4ht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich fragte Chloris: willst du mich?", "tokens": ["Ich", "frag\u00b7te", "Chlo\u00b7ris", ":", "willst", "du", "mich", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "$.", "VMFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da sprach sie gleich: Ich will!", "tokens": ["Da", "sprach", "sie", "gleich", ":", "Ich", "will", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$.", "PPER", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schnell regten meine Lippen sich,", "tokens": ["Schnell", "reg\u00b7ten", "mei\u00b7ne", "Lip\u00b7pen", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ihre hielten still.", "tokens": ["Und", "ih\u00b7re", "hiel\u00b7ten", "still", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich k\u00fc\u00dfte sie ein hundert mal,", "tokens": ["Ich", "k\u00fc\u00df\u00b7te", "sie", "ein", "hun\u00b7dert", "mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "CARD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da sagte sie: Halt ein!", "tokens": ["Da", "sag\u00b7te", "sie", ":", "Halt", "ein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "VVIMP", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dir mu\u00df noch eine gr\u00f6\u00dfre Zahl", "tokens": ["Dir", "mu\u00df", "noch", "ei\u00b7ne", "gr\u00f6\u00df\u00b7re", "Zahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von mir gegeben seyn.", "tokens": ["Von", "mir", "ge\u00b7ge\u00b7ben", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sie fing mit hundert K\u00fcssen an,", "tokens": ["Sie", "fing", "mit", "hun\u00b7dert", "K\u00fcs\u00b7sen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "CARD", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und hundert folgten drauf.", "tokens": ["Und", "hun\u00b7dert", "folg\u00b7ten", "drauf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie sprach: Mein liebster k\u00fcnftger Mann!", "tokens": ["Sie", "sprach", ":", "Mein", "liebs\u00b7ter", "k\u00fcnft\u00b7ger", "Mann", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPOSAT", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich aber sprach: H\u00f6r auf!", "tokens": ["Ich", "a\u00b7ber", "sprach", ":", "H\u00f6r", "auf", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$.", "NE", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}