{"dta.poem.19853": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Adeline .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1778", "urn": "urn:nbn:de:kobv:b4-20090519672", "language": ["de:0.99"], "booktitle": "B\u00fcrger, Gottfried August: Gedichte. G\u00f6ttingen, 1778."}, "poem": {"stanza.1": {"line.1": {"text": "Seh' ich, bei des Tempels Harmonieen,               ", "tokens": ["Seh'", "ich", ",", "bei", "des", "Tem\u00b7pels", "Har\u00b7mo\u00b7ni\u00b7e\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Ihr Gesicht von Seelenandacht gl\u00fchen,", "tokens": ["Ihr", "Ge\u00b7sicht", "von", "See\u00b7len\u00b7an\u00b7dacht", "gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ach! so w\u00e4hnt mein hochget\u00e4uschter Blik,", "tokens": ["Ach", "!", "so", "w\u00e4hnt", "mein", "hoch\u00b7ge\u00b7t\u00e4uschter", "Blik", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Eine Himmelsbraut in ihr zu schauen.", "tokens": ["Ei\u00b7ne", "Him\u00b7mels\u00b7braut", "in", "ihr", "zu", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Mir entsinket alle mein Vertrauen,", "tokens": ["Mir", "ent\u00b7sin\u00b7ket", "al\u00b7le", "mein", "Ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und die Liebe bebt vor ihr zur\u00fck.", "tokens": ["Und", "die", "Lie\u00b7be", "bebt", "vor", "ihr", "zu\u00b7r\u00fck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Aber seh\u2019 ich, wie im Altagskreise,", "tokens": ["A\u00b7ber", "seh'", "ich", ",", "wie", "im", "Al\u00b7tags\u00b7krei\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PWAV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Frei und fr\u00f6lich, doch nach Sitt\u2019 und Weise,", "tokens": ["Frei", "und", "fr\u00f6\u00b7lich", ",", "doch", "nach", "Sitt'", "und", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$,", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Sie so m\u00e4dchenhaft sich haben kan;", "tokens": ["Sie", "so", "m\u00e4d\u00b7chen\u00b7haft", "sich", "ha\u00b7ben", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "PRF", "VAINF", "VMFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Wie sie Scherz und Ernst so lieblich kleidet,", "tokens": ["Wie", "sie", "Scherz", "und", "Ernst", "so", "lieb\u00b7lich", "klei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "KON", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und um ihre Huld sich alles neidet:", "tokens": ["Und", "um", "ih\u00b7re", "Huld", "sich", "al\u00b7les", "nei\u00b7det", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "PRF", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Dann wagt Liebe wieder sich heran.", "tokens": ["Dann", "wagt", "Lie\u00b7be", "wie\u00b7der", "sich", "he\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "ADV", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Ehrfurcht neigt sich ihr im Engelglanze.", "tokens": ["Ehr\u00b7furcht", "neigt", "sich", "ihr", "im", "En\u00b7gel\u00b7glan\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "PPOSAT", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Lieb\u2019 umschmeichelt sie, im M\u00e4dchenkranze", "tokens": ["Lieb'", "um\u00b7schmei\u00b7chelt", "sie", ",", "im", "M\u00e4d\u00b7chen\u00b7kran\u00b7ze"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "$,", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Sanfter Myrten, ohne Himmelsschein.", "tokens": ["Sanf\u00b7ter", "Myr\u00b7ten", ",", "oh\u00b7ne", "Him\u00b7mels\u00b7schein", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KOUI", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Ach! so himlisch d\u00fcnke sie stets allen!", "tokens": ["Ach", "!", "so", "him\u00b7lisch", "d\u00fcn\u00b7ke", "sie", "stets", "al\u00b7len", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "ADJD", "VVFIN", "PPER", "ADV", "PIAT", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Aber meiner Liebe zu gefallen,", "tokens": ["A\u00b7ber", "mei\u00b7ner", "Lie\u00b7be", "zu", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Hold und magdlich meinem Blik allein!", "tokens": ["Hold", "und", "magd\u00b7lich", "mei\u00b7nem", "Blik", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}