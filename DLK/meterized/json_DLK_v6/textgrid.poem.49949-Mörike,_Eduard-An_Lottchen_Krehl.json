{"textgrid.poem.49949": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "An Lottchen Krehl", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich h\u00e4tte wohl, dein Haar zu zieren,", "tokens": ["Ich", "h\u00e4t\u00b7te", "wohl", ",", "dein", "Haar", "zu", "zie\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Kr\u00e4nzchen, auch ein klein Gedicht;", "tokens": ["Ein", "Kr\u00e4nz\u00b7chen", ",", "auch", "ein", "klein", "Ge\u00b7dicht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie aber? ich will gratulieren,", "tokens": ["Wie", "a\u00b7ber", "?", "ich", "will", "gra\u00b7tu\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$.", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wei\u00df den Tag des Festes nicht!", "tokens": ["Und", "wei\u00df", "den", "Tag", "des", "Fes\u00b7tes", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn ich es gleichwohl nun probierte,", "tokens": ["Wenn", "ich", "es", "gleich\u00b7wohl", "nun", "pro\u00b7bier\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich meint es drum nicht minder treu:", "tokens": ["Ich", "meint", "es", "drum", "nicht", "min\u00b7der", "treu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PAV", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist's nicht der erste, dritte, vierte,", "tokens": ["Ist's", "nicht", "der", "ers\u00b7te", ",", "drit\u00b7te", ",", "vier\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "PTKNEG", "ART", "ADJA", "$,", "ADJA", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So feir ich dir den ganzen Mai.", "tokens": ["So", "feir", "ich", "dir", "den", "gan\u00b7zen", "Mai", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch ach, was ist vom Mai zu singen?", "tokens": ["Doch", "ach", ",", "was", "ist", "vom", "Mai", "zu", "sin\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "PWS", "VAFIN", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Komm, Lottchen, uns den Mai zu bringen,", "tokens": ["Komm", ",", "Lott\u00b7chen", ",", "uns", "den", "Mai", "zu", "brin\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NE", "$,", "PPER", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dann bl\u00fchen Garten, Haus und Feld!", "tokens": ["Dann", "bl\u00fc\u00b7hen", "Gar\u00b7ten", ",", "Haus", "und", "Feld", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}