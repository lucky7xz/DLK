{"textgrid.poem.50998": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hell im Silberlichte flimmernd", "tokens": ["Hell", "im", "Sil\u00b7ber\u00b7lich\u00b7te", "flim\u00b7mernd"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPRART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zieht und singt des Baches Welle,", "tokens": ["Zieht", "und", "singt", "des", "Ba\u00b7ches", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Goldengr\u00fcn und tiefblau schimmernd", "tokens": ["Gol\u00b7den\u00b7gr\u00fcn", "und", "tief\u00b7blau", "schim\u00b7mernd"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00fc\u00dft sie fl\u00fcchtig die Libelle;", "tokens": ["K\u00fc\u00dft", "sie", "fl\u00fcch\u00b7tig", "die", "Li\u00b7bel\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN", "$."], "meter": "+-+--++-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Und ein drittes kommt dazu,", "tokens": ["Und", "ein", "drit\u00b7tes", "kommt", "da\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "VVFIN", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Eine Bl\u00fcte hergeschwommen:", "tokens": ["Ei\u00b7ne", "Bl\u00fc\u00b7te", "her\u00b7ge\u00b7schwom\u00b7men", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Alle haben drauf im Nu", "tokens": ["Al\u00b7le", "ha\u00b7ben", "drauf", "im", "Nu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "PAV", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Heitern Abschied schon genommen.", "tokens": ["Hei\u00b7tern", "Ab\u00b7schied", "schon", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und die Esche beugt sich dr\u00fcber,", "tokens": ["Und", "die", "E\u00b7sche", "beugt", "sich", "dr\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PRF", "PAV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schaut in Ruh das holde Treiben,", "tokens": ["Schaut", "in", "Ruh", "das", "hol\u00b7de", "Trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denkt: Ihr Lieben, zieht vor\u00fcber,", "tokens": ["Denkt", ":", "Ihr", "Lie\u00b7ben", ",", "zieht", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PPOSAT", "ADJA", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich will gr\u00fcnen hier und bleiben!", "tokens": ["Ich", "will", "gr\u00fc\u00b7nen", "hier", "und", "blei\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "ADV", "KON", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und ich unterm Eschenbaum:", "tokens": ["Und", "ich", "un\u00b7term", "E\u00b7schen\u00b7baum", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Was soll denn mit mir geschehen", "tokens": ["Was", "soll", "denn", "mit", "mir", "ge\u00b7sche\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "ADV", "APPR", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "In dem reizend leichten Traum?", "tokens": ["In", "dem", "rei\u00b7zend", "leich\u00b7ten", "Traum", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Soll ich bleiben? Soll ich gehen?", "tokens": ["Soll", "ich", "blei\u00b7ben", "?", "Soll", "ich", "ge\u00b7hen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$.", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}