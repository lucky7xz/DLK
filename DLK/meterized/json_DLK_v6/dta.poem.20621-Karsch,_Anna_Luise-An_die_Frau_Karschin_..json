{"dta.poem.20621": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An die Frau Karschin .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein neues Alles blickt auf uns             ", "tokens": ["Ein", "neu\u00b7es", "Al\u00b7les", "blickt", "auf", "uns"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "PIS", "VVFIN", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Wiederschein aus deiner Seele,", "tokens": ["Im", "Wie\u00b7der\u00b7schein", "aus", "dei\u00b7ner", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du reinrer Spiegel der Natur!", "tokens": ["Du", "rein\u00b7rer", "Spie\u00b7gel", "der", "Na\u00b7tur", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Viel sch\u00f6ner bl\u00fchet dir die Rose,", "tokens": ["Viel", "sch\u00f6\u00b7ner", "bl\u00fc\u00b7het", "dir", "die", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Weit z\u00e4rtlicher weint dir ein Pylades,", "tokens": ["Weit", "z\u00e4rt\u00b7li\u00b7cher", "weint", "dir", "ein", "Py\u00b7la\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und heldenm\u00fcthiger in Wunden", "tokens": ["Und", "hel\u00b7den\u00b7m\u00fct\u00b7hi\u00b7ger", "in", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Stirbt dir ein Held, noch mehr, ein Kleist.", "tokens": ["Stirbt", "dir", "ein", "Held", ",", "noch", "mehr", ",", "ein", "Kleist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "ADV", "ADV", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Weit gr\u00fcner winket dir die Aue,", "tokens": ["Weit", "gr\u00fc\u00b7ner", "win\u00b7ket", "dir", "die", "Au\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und lauter singt die Lerche dir,", "tokens": ["Und", "lau\u00b7ter", "singt", "die", "Ler\u00b7che", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und schmachtender seufzt Philomele,", "tokens": ["Und", "schmach\u00b7ten\u00b7der", "seufzt", "Phi\u00b7lo\u00b7me\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "NE", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.11": {"text": "Wenn sie dir ihr Geheimni\u00df sagt,", "tokens": ["Wenn", "sie", "dir", "ihr", "Ge\u00b7heim\u00b7ni\u00df", "sagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Da\u00df sie um ihren Gatten klagt,", "tokens": ["Da\u00df", "sie", "um", "ih\u00b7ren", "Gat\u00b7ten", "klagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und willst Du Z\u00e4rtlichkeit und Ku\u00df verm\u00e4hlt beschreiben;", "tokens": ["Und", "willst", "Du", "Z\u00e4rt\u00b7lich\u00b7keit", "und", "Ku\u00df", "ver\u00b7m\u00e4hlt", "be\u00b7schrei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "NN", "KON", "NN", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So k\u00fc\u00dft nicht Damon seine Phyllis,", "tokens": ["So", "k\u00fc\u00dft", "nicht", "Da\u00b7mon", "sei\u00b7ne", "Phyl\u00b7lis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "NE", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.15": {"text": "Nein, beide k\u00fcsset die Natur \u2014", "tokens": ["Nein", ",", "bei\u00b7de", "k\u00fcs\u00b7set", "die", "Na\u00b7tur"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PIS", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Die meisten Dichter sind nur Tr\u00e4umer,", "tokens": ["Die", "meis\u00b7ten", "Dich\u00b7ter", "sind", "nur", "Tr\u00e4u\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "F\u00fcr sie liegt die Natur im tiefen Schlaf,", "tokens": ["F\u00fcr", "sie", "liegt", "die", "Na\u00b7tur", "im", "tie\u00b7fen", "Schlaf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.18": {"text": "F\u00fcr dich erwachet sie, wenn du sie gr\u00fc\u00dfest,", "tokens": ["F\u00fcr", "dich", "er\u00b7wa\u00b7chet", "sie", ",", "wenn", "du", "sie", "gr\u00fc\u00b7\u00dfest", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Und reicht dir, w\u00fcrdiger, die Hand,", "tokens": ["Und", "reicht", "dir", ",", "w\u00fcr\u00b7di\u00b7ger", ",", "die", "Hand", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "ADJD", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Und zeigt Dir die geheimen Wunder.", "tokens": ["Und", "zeigt", "Dir", "die", "ge\u00b7hei\u00b7men", "Wun\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "F\u00fcr dich entfaltet sie der Rose Bl\u00e4tter,", "tokens": ["F\u00fcr", "dich", "ent\u00b7fal\u00b7tet", "sie", "der", "Ro\u00b7se", "Bl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "F\u00fcr Dich zieht sie der Wolken Flor vom Himmel,", "tokens": ["F\u00fcr", "Dich", "zieht", "sie", "der", "Wol\u00b7ken", "Flor", "vom", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "ART", "NN", "NN", "APPRART", "NN", "$,"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.23": {"text": "F\u00fcr dich entlarvt sie das geputzte Laster", "tokens": ["F\u00fcr", "dich", "ent\u00b7larvt", "sie", "das", "ge\u00b7putz\u00b7te", "Las\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Und zeigt die Tugend dir im \u00e4chten Glanze,", "tokens": ["Und", "zeigt", "die", "Tu\u00b7gend", "dir", "im", "\u00e4ch\u00b7ten", "Glan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Und endlich l\u00e4\u00dft in Andachtsvollen Stunden", "tokens": ["Und", "end\u00b7lich", "l\u00e4\u00dft", "in", "An\u00b7dachts\u00b7vol\u00b7len", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Die ewge Wahrheit und die ewge Liebe", "tokens": ["Die", "ew\u00b7ge", "Wahr\u00b7heit", "und", "die", "ew\u00b7ge", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "F\u00fcr dich, anbetende ger\u00fchrte Karschin,", "tokens": ["F\u00fcr", "dich", ",", "an\u00b7be\u00b7ten\u00b7de", "ge\u00b7r\u00fchr\u00b7te", "Kar\u00b7schin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+----+--+", "measure": "iambic.tetra.chol"}, "line.28": {"text": "Den Schleyer fallen \u2014 Heilig ist ihr Name! \u2014", "tokens": ["Den", "Schle\u00b7yer", "fal\u00b7len", "Hei\u00b7lig", "ist", "ihr", "Na\u00b7me", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVINF", "$(", "ADJA", "VAFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Entfern dich, Muse, st\u00f6hre sie anbetend nicht!", "tokens": ["Ent\u00b7fern", "dich", ",", "Mu\u00b7se", ",", "st\u00f6h\u00b7re", "sie", "an\u00b7be\u00b7tend", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "NN", "$,", "VVFIN", "PPER", "VVPP", "PTKNEG", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}}}}}