{"dta.poem.20930": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Agnes an Ungenand.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Dein Agnes schreibet hier mit Banden an den", "tokens": ["Dein", "Ag\u00b7nes", "schrei\u00b7bet", "hier", "mit", "Ban\u00b7den", "an", "den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NE", "VVFIN", "ADV", "APPR", "NN", "APPR", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "H\u00e4nden/", "tokens": ["H\u00e4n\u00b7den", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Mit Riegeln wohl verwahrt die mehr als st\u00e4h-", "tokens": ["Mit", "Rie\u00b7geln", "wohl", "ver\u00b7wahrt", "die", "mehr", "als", "st\u00e4h"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "VVFIN", "ART", "PIAT", "KOKOM", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "lern seyn/", "tokens": ["lern", "seyn", "/"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "VAINF", "$("], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Mit Finstern\u00fcs umbstrickt/ verwacht an allen Enden/", "tokens": ["Mit", "Fins\u00b7ter\u00b7n\u00fcs", "umbs\u00b7trickt", "/", "ver\u00b7wacht", "an", "al\u00b7len", "En\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$(", "VVFIN", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wer aber liefert dir di\u00df kleine Schreiben ein?", "tokens": ["Wer", "a\u00b7ber", "lie\u00b7fert", "dir", "di\u00df", "klei\u00b7ne", "Schrei\u00b7ben", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "PPER", "PDS", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich mu\u00df itzund aus Noth dergleichen Leuthen trauen/", "tokens": ["Ich", "mu\u00df", "it\u00b7zund", "aus", "Noth", "derg\u00b7lei\u00b7chen", "Leu\u00b7then", "trau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "NN", "PIS", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da keine H\u00f6fligkeit ie eingewurtzelt hat/", "tokens": ["Da", "kei\u00b7ne", "H\u00f6f\u00b7lig\u00b7keit", "ie", "ein\u00b7ge\u00b7wurt\u00b7zelt", "hat", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ADV", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wird ein verd\u00e4chtig Aug\u2019 auf meine Zeilen schauen/", "tokens": ["Wird", "ein", "ver\u00b7d\u00e4ch\u00b7tig", "Aug'", "auf", "mei\u00b7ne", "Zei\u00b7len", "schau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJD", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So find die gantze Welt hier neue Missethat.", "tokens": ["So", "find", "die", "gant\u00b7ze", "Welt", "hier", "neu\u00b7e", "Mis\u00b7se\u00b7that", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Es zeiget mir itzund das schl\u00fcpffrige Gel\u00fccke/", "tokens": ["Es", "zei\u00b7get", "mir", "it\u00b7zund", "das", "schl\u00fcpf\u00b7fri\u00b7ge", "Ge\u00b7l\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.12": {"text": "Wie seine Schmeicheley die Welt ber\u00fccken kan/", "tokens": ["Wie", "sei\u00b7ne", "Schmei\u00b7che\u00b7ley", "die", "Welt", "be\u00b7r\u00fc\u00b7cken", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ART", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Zuvor erqvickten mich die Strahlen deiner Blicke/", "tokens": ["Zu\u00b7vor", "er\u00b7qvick\u00b7ten", "mich", "die", "Strah\u00b7len", "dei\u00b7ner", "Bli\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Jtzt schaut ein Scherge mich mit schelen Augen an.", "tokens": ["Jtzt", "schaut", "ein", "Scher\u00b7ge", "mich", "mit", "sche\u00b7len", "Au\u00b7gen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ein Hertzog k\u00fcste mir vor diesem Haud und Armen/", "tokens": ["Ein", "Hert\u00b7zog", "k\u00fcs\u00b7te", "mir", "vor", "die\u00b7sem", "Haud", "und", "Ar\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "PPER", "APPR", "PDAT", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Jtzt schleust man meinen Leib in Ketten und in Band/", "tokens": ["Jtzt", "schleust", "man", "mei\u00b7nen", "Leib", "in", "Ket\u00b7ten", "und", "in", "Band", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPOSAT", "NN", "APPR", "NN", "KON", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Vor schaut ich nichts als Neid/ itzt schau ich kein Er-", "tokens": ["Vor", "schaut", "ich", "nichts", "als", "Neid", "/", "itzt", "schau", "ich", "kein", "Er"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "VVFIN", "PPER", "PIS", "KOKOM", "NN", "$(", "ADV", "ADJD", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.18": {"text": "barmen/", "tokens": ["bar\u00b7men", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.19": {"text": "Und bin ein Gauckel Spiel vor dieses gantze Land.", "tokens": ["Und", "bin", "ein", "Gau\u00b7ckel", "Spiel", "vor", "die\u00b7ses", "gant\u00b7ze", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "NN", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Di\u00df macht der Purpur Rock/ damit du mich umgeben/", "tokens": ["Di\u00df", "macht", "der", "Pur\u00b7pur", "Rock", "/", "da\u00b7mit", "du", "mich", "um\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "NN", "$(", "KOUS", "PPER", "PRF", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Di\u00df macht/ dieweil dein Geist dem meinen wolgewolt/", "tokens": ["Di\u00df", "macht", "/", "die\u00b7weil", "dein", "Geist", "dem", "mei\u00b7nen", "wol\u00b7ge\u00b7wolt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$(", "KOUS", "PPOSAT", "NN", "ART", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Der Ku\u00df/ den ich empfing/ der bringt mich umb das Le-", "tokens": ["Der", "Ku\u00df", "/", "den", "ich", "emp\u00b7fing", "/", "der", "bringt", "mich", "umb", "das", "Le"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "VVFIN", "$(", "ART", "VVFIN", "PPER", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "ben/", "tokens": ["ben", "/"], "token_info": ["word", "punct"], "pos": ["VVPP", "$("], "meter": "-", "measure": "single.down"}, "line.24": {"text": "Denn das du mich geliebt ist meine gr\u00f6ste Schuldt.", "tokens": ["Denn", "das", "du", "mich", "ge\u00b7liebt", "ist", "mei\u00b7ne", "gr\u00f6s\u00b7te", "Schuldt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "PRF", "VVPP", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wie leichtlich irren doch die Circkel unsrer Sinnen/", "tokens": ["Wie", "leicht\u00b7lich", "ir\u00b7ren", "doch", "die", "Cir\u00b7ckel", "uns\u00b7rer", "Sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ADV", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Wie macht das Hofnungs Gla\u00df uns alles viel zugro\u00df.", "tokens": ["Wie", "macht", "das", "Hof\u00b7nungs", "Gla\u00df", "uns", "al\u00b7les", "viel", "zu\u00b7gro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "NN", "PPER", "PIS", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ich meint\u2019/ ich w\u00fcrde nun forthin nicht fallen k\u00f6nnen/", "tokens": ["Ich", "meint'", "/", "ich", "w\u00fcr\u00b7de", "nun", "for\u00b7thin", "nicht", "fal\u00b7len", "k\u00f6n\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VAFIN", "ADV", "ADV", "PTKNEG", "VVINF", "VMINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Ich wolte G\u00f6ttin seyn und nicht ein Erdenklo\u00df.", "tokens": ["Ich", "wol\u00b7te", "G\u00f6t\u00b7tin", "seyn", "und", "nicht", "ein", "Er\u00b7den\u00b7klo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "VAINF", "KON", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ich meint\u2019/ ich were nur vor F\u00fcrsten Bluth erkohren/", "tokens": ["Ich", "meint'", "/", "ich", "we\u00b7re", "nur", "vor", "F\u00fcrs\u00b7ten", "Bluth", "er\u00b7koh\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VAFIN", "ADV", "APPR", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Es were nur mein Mund gekr\u00f6nter K\u00fcsse werth/", "tokens": ["Es", "we\u00b7re", "nur", "mein", "Mund", "ge\u00b7kr\u00f6n\u00b7ter", "K\u00fcs\u00b7se", "werth", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN", "ADJA", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ich glaubte nicht/ da\u00df mich ein B\u00fcrgers Weib geboh-", "tokens": ["Ich", "glaub\u00b7te", "nicht", "/", "da\u00df", "mich", "ein", "B\u00fcr\u00b7gers", "Weib", "ge\u00b7boh"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$(", "KOUS", "PPER", "ART", "NN", "NE", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "ren/", "tokens": ["ren", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "-", "measure": "single.down"}, "line.33": {"text": "Wie aber hat die Zeit mir diesen Wahn verkehrt?", "tokens": ["Wie", "a\u00b7ber", "hat", "die", "Zeit", "mir", "die\u00b7sen", "Wahn", "ver\u00b7kehrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "ART", "NN", "PPER", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Dein Vater hat mich recht auch meinen lassen wissen/", "tokens": ["Dein", "Va\u00b7ter", "hat", "mich", "recht", "auch", "mei\u00b7nen", "las\u00b7sen", "wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADV", "ADV", "PPOSAT", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Und gr\u00fcndlich kund gethan/ wo ich entsprossen bin/", "tokens": ["Und", "gr\u00fcnd\u00b7lich", "kund", "ge\u00b7than", "/", "wo", "ich", "ent\u00b7spros\u00b7sen", "bin", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKVZ", "VVPP", "$(", "PWAV", "PPER", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Den Purpur hat er mir vom Leibe weggerissen/", "tokens": ["Den", "Pur\u00b7pur", "hat", "er", "mir", "vom", "Lei\u00b7be", "weg\u00b7ge\u00b7ris\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PPER", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Und jagt mich itzt entbl\u00f6st in ein Gef\u00e4ngn\u00fcs hin;", "tokens": ["Und", "jagt", "mich", "itzt", "ent\u00b7bl\u00f6st", "in", "ein", "Ge\u00b7f\u00e4ng\u00b7n\u00fcs", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Hier mu\u00df ich mich geb\u00fcckt in Ketten lassen legen/", "tokens": ["Hier", "mu\u00df", "ich", "mich", "ge\u00b7b\u00fcckt", "in", "Ket\u00b7ten", "las\u00b7sen", "le\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVPP", "APPR", "NN", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Wie dr\u00fcckt das Eisen doch itzt meine zarte Handt!", "tokens": ["Wie", "dr\u00fcckt", "das", "Ei\u00b7sen", "doch", "itzt", "mei\u00b7ne", "zar\u00b7te", "Handt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Wie mir zu Muthe sey das kanstu leicht erwegen/", "tokens": ["Wie", "mir", "zu", "Mu\u00b7the", "sey", "das", "kans\u00b7tu", "leicht", "er\u00b7we\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "VAFIN", "PDS", "VMFIN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Dann dir ist mein Gem\u00fcth und auch mein Leib bekant.", "tokens": ["Dann", "dir", "ist", "mein", "Ge\u00b7m\u00fcth", "und", "auch", "mein", "Leib", "be\u00b7kant", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "PPOSAT", "NN", "KON", "ADV", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Mein Albrecht scheu dich nicht mein Schreiben zu", "tokens": ["Mein", "Al\u00b7brecht", "scheu", "dich", "nicht", "mein", "Schrei\u00b7ben", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "PTKZU"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "durch lesen/", "tokens": ["durch", "le\u00b7sen", "/"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "VVINF", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.44": {"text": "Es komt von dieser her die du hast hoch gesch\u00e4tzt/", "tokens": ["Es", "komt", "von", "die\u00b7ser", "her", "die", "du", "hast", "hoch", "ge\u00b7sch\u00e4tzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "ADV", "ART", "PPER", "VAFIN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Schau was ich itzund bin/ du weist was ich gewesen/", "tokens": ["Schau", "was", "ich", "it\u00b7zund", "bin", "/", "du", "weist", "was", "ich", "ge\u00b7we\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWS", "PPER", "ADV", "VAFIN", "$(", "PPER", "VVFIN", "PWS", "PPER", "VAPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Und wie manch fe\u00fcchter Ku\u00df hat deinen Mund ergetzt.", "tokens": ["Und", "wie", "manch", "fe\u00fcch\u00b7ter", "Ku\u00df", "hat", "dei\u00b7nen", "Mund", "er\u00b7getzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIAT", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Send\u2019 einen Seuffzer nur auf meine schwere Bande/", "tokens": ["Send'", "ei\u00b7nen", "Seuff\u00b7zer", "nur", "auf", "mei\u00b7ne", "schwe\u00b7re", "Ban\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Dann keine Rettung ist vor mich auf dieser Welt/", "tokens": ["Dann", "kei\u00b7ne", "Ret\u00b7tung", "ist", "vor", "mich", "auf", "die\u00b7ser", "Welt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VAFIN", "APPR", "PRF", "APPR", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Ach were nicht mein Blut von allzuschlechtem Stan-", "tokens": ["Ach", "we\u00b7re", "nicht", "mein", "Blut", "von", "all\u00b7zu\u00b7schlech\u00b7tem", "Stan"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "VAFIN", "PTKNEG", "PPOSAT", "NN", "APPR", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "de/", "tokens": ["de", "/"], "token_info": ["word", "punct"], "pos": ["NE", "$("], "meter": "-", "measure": "single.down"}, "line.51": {"text": "So w\u00fcrd\u2019 ich dir/ und nicht dem Tode zugesellt!", "tokens": ["So", "w\u00fcrd'", "ich", "dir", "/", "und", "nicht", "dem", "To\u00b7de", "zu\u00b7ge\u00b7sellt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "$(", "KON", "PTKNEG", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Ich d\u00fcrffte nicht wie itzt bey Henckers Buben leben/", "tokens": ["Ich", "d\u00fcrff\u00b7te", "nicht", "wie", "itzt", "bey", "Hen\u00b7ckers", "Bu\u00b7ben", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "KOKOM", "ADV", "APPR", "NE", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Man salbte meinen Leib mit frembden Balsam ein/", "tokens": ["Man", "salb\u00b7te", "mei\u00b7nen", "Leib", "mit", "fremb\u00b7den", "Bal\u00b7sam", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Es m\u00fcste Seid und Gold umb meine Lenden schweben/", "tokens": ["Es", "m\u00fcs\u00b7te", "Seid", "und", "Gold", "umb", "mei\u00b7ne", "Len\u00b7den", "schwe\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VAIMP", "KON", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Und Agnes m\u00fcste Braut des jungen Hertzogs seyn.", "tokens": ["Und", "Ag\u00b7nes", "m\u00fcs\u00b7te", "Braut", "des", "jun\u00b7gen", "Hert\u00b7zogs", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ADJA", "NN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Es w\u00fcrde dieses Land Gel\u00fcck und Segen ruffen/", "tokens": ["Es", "w\u00fcr\u00b7de", "die\u00b7ses", "Land", "Ge\u00b7l\u00fcck", "und", "Se\u00b7gen", "ruf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PDAT", "NN", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Man w\u00fcrffe mir erfreut des Fr\u00fchlings Kinder zu/", "tokens": ["Man", "w\u00fcrf\u00b7fe", "mir", "er\u00b7freut", "des", "Fr\u00fch\u00b7lings", "Kin\u00b7der", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "VVFIN", "ART", "NN", "NN", "PTKZU", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Ich h\u00e4tte nichts als Lust/ und nichts als Ruhm zuhoffen/", "tokens": ["Ich", "h\u00e4t\u00b7te", "nichts", "als", "Lust", "/", "und", "nichts", "als", "Ruhm", "zu\u00b7hof\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "KOKOM", "NN", "$(", "KON", "PIS", "KOKOM", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Und meiner Sch\u00e4tze Schatz/ O Hertzog! w\u00e4rest du.", "tokens": ["Und", "mei\u00b7ner", "Sch\u00e4t\u00b7ze", "Schatz", "/", "O", "Hert\u0