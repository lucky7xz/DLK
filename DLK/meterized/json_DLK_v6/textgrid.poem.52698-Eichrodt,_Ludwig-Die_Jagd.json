{"textgrid.poem.52698": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die Jagd", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwohlauf, ihr Herrn, ha wohlauf zur Jagd!", "tokens": ["\u00bb", "wohl\u00b7auf", ",", "ihr", "Herrn", ",", "ha", "wohl\u00b7auf", "zur", "Jagd", "!"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "PPOSAT", "NN", "$,", "ITJ", "ADV", "APPRART", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Rei\u00dft weg die Becher vom Mund!", "tokens": ["Rei\u00dft", "weg", "die", "Be\u00b7cher", "vom", "Mund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Der Himmel wird grau, es windet, es tagt,", "tokens": ["Der", "Him\u00b7mel", "wird", "grau", ",", "es", "win\u00b7det", ",", "es", "tagt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Hahn kr\u00e4ht Morgenstund!\u00ab", "tokens": ["Der", "Hahn", "kr\u00e4ht", "Mor\u00b7gen\u00b7stund", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So ruft der Junker von Hesselhag,", "tokens": ["So", "ruft", "der", "Jun\u00b7ker", "von", "Hes\u00b7sel\u00b7hag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Das leere Glas in der Hand,", "tokens": ["Das", "lee\u00b7re", "Glas", "in", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Ins \u00fcbern\u00e4chtige Zechgelag,", "tokens": ["Ins", "\u00fc\u00b7bern\u00b7\u00e4ch\u00b7ti\u00b7ge", "Zech\u00b7ge\u00b7lag", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und wirft das Glas an die Wand.", "tokens": ["Und", "wirft", "das", "Glas", "an", "die", "Wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Schlaftrunken fahren die G\u00e4ste auf,", "tokens": ["Schlaf\u00b7trun\u00b7ken", "fah\u00b7ren", "die", "G\u00e4s\u00b7te", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie sch\u00fctteln das Lockengeflecht,", "tokens": ["Sie", "sch\u00fct\u00b7teln", "das", "Lo\u00b7cken\u00b7ge\u00b7flecht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und r\u00fctteln im Hin- und Widerlauf", "tokens": ["Und", "r\u00fct\u00b7teln", "im", "Hin", "und", "Wi\u00b7der\u00b7lauf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "TRUNC", "KON", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Koller sich rasch zurecht.", "tokens": ["Die", "Kol\u00b7ler", "sich", "rasch", "zu\u00b7recht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ADJD", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Die Kohle im Schlot, der Wein im Krug", "tokens": ["Die", "Koh\u00b7le", "im", "Schlot", ",", "der", "Wein", "im", "Krug"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "ART", "NN", "APPRART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ist todt, es schauert die Herrn,", "tokens": ["Ist", "todt", ",", "es", "schau\u00b7ert", "die", "Herrn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Sie waschen den Kopf, sie haben genug,", "tokens": ["Sie", "wa\u00b7schen", "den", "Kopf", ",", "sie", "ha\u00b7ben", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VAFIN", "ADV", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Sie h\u00f6ren den Jagdruf gern.", "tokens": ["Sie", "h\u00f6\u00b7ren", "den", "Jag\u00b7druf", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Und Rossegewieher dringt herauf", "tokens": ["Und", "Ros\u00b7se\u00b7ge\u00b7wie\u00b7her", "dringt", "her\u00b7auf"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "PTKVZ"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zum Saal und Fackelschein,", "tokens": ["Zum", "Saal", "und", "Fa\u00b7ckel\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im Schlo\u00dfhof l\u00e4rmt der Hundehauf,", "tokens": ["Im", "Schlo\u00df\u00b7hof", "l\u00e4rmt", "der", "Hun\u00b7de\u00b7hauf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hell klingen die H\u00f6rner darein.", "tokens": ["Hell", "klin\u00b7gen", "die", "H\u00f6r\u00b7ner", "da\u00b7rein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "PAV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Das hat den Junker aufgemannt,", "tokens": ["Das", "hat", "den", "Jun\u00b7ker", "auf\u00b7ge\u00b7mannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er schreitet hinaus zum Saal,", "tokens": ["Er", "schrei\u00b7tet", "hin\u00b7aus", "zum", "Saal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Sieh da, im fliegenden Nachtgewand,", "tokens": ["Sieh", "da", ",", "im", "flie\u00b7gen\u00b7den", "Nacht\u00b7ge\u00b7wand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "---+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Sein blutjung Ehgemahl!", "tokens": ["Sein", "blut\u00b7jung", "Eh\u00b7ge\u00b7mahl", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie blicket ihn an: o lieber Herr,", "tokens": ["Sie", "bli\u00b7cket", "ihn", "an", ":", "o", "lie\u00b7ber", "Herr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$.", "FM", "ADV", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Geht heut nicht auf die Jagd!", "tokens": ["Geht", "heut", "nicht", "auf", "die", "Jagd", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verzeiht, da\u00df ich den Weg Euch sperr,", "tokens": ["Ver\u00b7zeiht", ",", "da\u00df", "ich", "den", "Weg", "Euch", "sperr", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich tr\u00e4umte so b\u00f6s zur Nacht;", "tokens": ["Ich", "tr\u00e4um\u00b7te", "so", "b\u00f6s", "zur", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPRART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Die Rosse, die jetzt ihr wiehern h\u00f6rt,", "tokens": ["Die", "Ros\u00b7se", ",", "die", "jetzt", "ihr", "wie\u00b7hern", "h\u00f6rt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Sie fuhren uns beid hinaus,", "tokens": ["Sie", "fuh\u00b7ren", "uns", "beid", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Zur Gruft nach Sankt Katharinenw\u00f6rth,", "tokens": ["Zur", "Gruft", "nach", "Sankt", "Ka\u00b7tha\u00b7ri\u00b7nen\u00b7w\u00f6rth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "VVFIN", "NE", "$,"], "meter": "-+-+----+", "measure": "unknown.measure.tri"}, "line.8": {"text": "Heut, Herr, bleibt heut zu Haus!", "tokens": ["Heut", ",", "Herr", ",", "bleibt", "heut", "zu", "Haus", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und z\u00fcrnet nicht, so tr\u00e4umt ich, Herr!", "tokens": ["Und", "z\u00fcr\u00b7net", "nicht", ",", "so", "tr\u00e4umt", "ich", ",", "Herr", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "ADV", "VVFIN", "PPER", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbwas?\u00ab donnert der rauhe Mann,", "tokens": ["\u00bb", "was", "?", "\u00ab", "don\u00b7nert", "der", "rau\u00b7he", "Mann", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "$.", "$(", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "\u00bbschon wieder das eckle Weibsgepl\u00e4rr?", "tokens": ["\u00bb", "schon", "wie\u00b7der", "das", "ec\u00b7kle", "Weibs\u00b7ge\u00b7pl\u00e4rr", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ein Schwachkopf h\u00f6r es an!", "tokens": ["Ein", "Schwach\u00b7kopf", "h\u00f6r", "es", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Geh weg!\u00ab Ach Herr! \u00bbVerstehst du deutsch?", "tokens": ["Geh", "weg", "!", "\u00ab", "Ach", "Herr", "!", "\u00bb", "Ver\u00b7stehst", "du", "deutsch", "?"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "$(", "ITJ", "NN", "$.", "$(", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Geh weg!\u00ab Gott nein, ich bleib", "tokens": ["Geh", "weg", "!", "\u00ab", "Gott", "nein", ",", "ich", "bleib"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "PTKVZ", "$.", "$(", "NN", "PTKANT", "$,", "PPER", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "An deinem Halse \u2013 \u00bbdie Hundepeitsch", "tokens": ["An", "dei\u00b7nem", "Hal\u00b7se", "\u2013", "\u00bb", "die", "Hun\u00b7de\u00b7peitsch"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$(", "$(", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "F\u00fcr dich, zudringlich Weib!\u00ab", "tokens": ["F\u00fcr", "dich", ",", "zu\u00b7dring\u00b7lich", "Weib", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["APPR", "PPER", "$,", "ADJD", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Geschlagen ist die holde Frau,", "tokens": ["Ge\u00b7schla\u00b7gen", "ist", "die", "hol\u00b7de", "Frau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da steht sie wie versteint,", "tokens": ["Da", "steht", "sie", "wie", "ver\u00b7steint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOKOM", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr gro\u00dfes Auge himmelblau", "tokens": ["Ihr", "gro\u00b7\u00dfes", "Au\u00b7ge", "him\u00b7mel\u00b7blau"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Thr\u00e4nenglanz erscheint.", "tokens": ["In", "Thr\u00e4\u00b7nen\u00b7glanz", "er\u00b7scheint", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie wankt dahin, sie weint sich aus,", "tokens": ["Sie", "wankt", "da\u00b7hin", ",", "sie", "weint", "sich", "aus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "$,", "PPER", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "L\u00e4\u00dft Alles gehen und stehn,", "tokens": ["L\u00e4\u00dft", "Al\u00b7les", "ge\u00b7hen", "und", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Den ganzen Tag hat Niemand im Haus", "tokens": ["Den", "gan\u00b7zen", "Tag", "hat", "Nie\u00b7mand", "im", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PIS", "APPRART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Die arme Herrin gesehn.", "tokens": ["Die", "ar\u00b7me", "Her\u00b7rin", "ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Indessen sucht in Wald und Feld", "tokens": ["In\u00b7des\u00b7sen", "sucht", "in", "Wald", "und", "Feld"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Junker Waidmannslust \u2013", "tokens": ["Der", "Jun\u00b7ker", "Waid\u00b7manns\u00b7lust", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbei, Bettelmann, willst du kein Geld?", "tokens": ["\u00bb", "ei", ",", "Bet\u00b7tel\u00b7mann", ",", "willst", "du", "kein", "Geld", "?"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "NN", "$,", "VMFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was wirfst dich in die Brust?\u00ab", "tokens": ["Was", "wirfst", "dich", "in", "die", "Brust", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "PRF", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ei, Edelmann, die sch\u00f6ne Au", "tokens": ["Ei", ",", "E\u00b7del\u00b7mann", ",", "die", "sch\u00f6\u00b7ne", "Au"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Verw\u00fcstet l\u00e4nger nicht!", "tokens": ["Ver\u00b7w\u00fcs\u00b7tet", "l\u00e4n\u00b7ger", "nicht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text"