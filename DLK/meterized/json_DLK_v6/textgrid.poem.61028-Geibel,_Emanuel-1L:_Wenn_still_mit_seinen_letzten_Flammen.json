{"textgrid.poem.61028": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn still mit seinen letzten Flammen", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn still mit seinen letzten Flammen", "tokens": ["Wenn", "still", "mit", "sei\u00b7nen", "letz\u00b7ten", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Abend in das Meer versank,", "tokens": ["Der", "A\u00b7bend", "in", "das", "Meer", "ver\u00b7sank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann wandeln traulich wir zusammen", "tokens": ["Dann", "wan\u00b7deln", "trau\u00b7lich", "wir", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "PPER", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Am Waldgestad im Buchengang.", "tokens": ["Am", "Wald\u00b7ge\u00b7stad", "im", "Bu\u00b7chen\u00b7gang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wir sehn den Mond durch Wolken steigen,", "tokens": ["Wir", "sehn", "den", "Mond", "durch", "Wol\u00b7ken", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wir h\u00f6ren fern die Nachtigall,", "tokens": ["Wir", "h\u00f6\u00b7ren", "fern", "die", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir atmen D\u00fcfte, doch wir schweigen -", "tokens": ["Wir", "at\u00b7men", "D\u00fcf\u00b7te", ",", "doch", "wir", "schwei\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "$,", "KON", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was soll der Worte leerer Schall?", "tokens": ["Was", "soll", "der", "Wor\u00b7te", "lee\u00b7rer", "Schall", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Das h\u00f6chste Gl\u00fcck hat keine Lieder,", "tokens": ["Das", "h\u00f6chs\u00b7te", "Gl\u00fcck", "hat", "kei\u00b7ne", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Liebe Lust ist still und mild;", "tokens": ["Der", "Lie\u00b7be", "Lust", "ist", "still", "und", "mild", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Ku\u00df, ein Blicken hin und wieder,", "tokens": ["Ein", "Ku\u00df", ",", "ein", "Bli\u00b7cken", "hin", "und", "wie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "PTKVZ", "KON", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und alle Sehnsucht ist gestillt.", "tokens": ["Und", "al\u00b7le", "Sehn\u00b7sucht", "ist", "ge\u00b7stillt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}