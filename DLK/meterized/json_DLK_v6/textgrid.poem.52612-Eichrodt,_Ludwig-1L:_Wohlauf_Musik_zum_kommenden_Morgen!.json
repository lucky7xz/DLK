{"textgrid.poem.52612": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wohlauf Musik zum kommenden Morgen!", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohlauf Musik zum kommenden Morgen!", "tokens": ["Wohl\u00b7auf", "Mu\u00b7sik", "zum", "kom\u00b7men\u00b7den", "Mor\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nun werf ich hinweg die bleichen Sorgen,", "tokens": ["Nun", "werf", "ich", "hin\u00b7weg", "die", "blei\u00b7chen", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Den eiteln Kummer,", "tokens": ["Den", "ei\u00b7teln", "Kum\u00b7mer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Den Herzensverdummer,", "tokens": ["Den", "Her\u00b7zens\u00b7ver\u00b7dum\u00b7mer", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Und allen Gram und jeden Verdru\u00df \u2013", "tokens": ["Und", "al\u00b7len", "Gram", "und", "je\u00b7den", "Ver\u00b7dru\u00df", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "KON", "PIAT", "NN", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Der Freundschaft geb ich neuen Ku\u00df!", "tokens": ["Der", "Freund\u00b7schaft", "geb", "ich", "neu\u00b7en", "Ku\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nun bringet mir wieder den sch\u00e4umenden Becher,", "tokens": ["Nun", "brin\u00b7get", "mir", "wie\u00b7der", "den", "sch\u00e4u\u00b7men\u00b7den", "Be\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ich werde schon wieder ein l\u00f6blicher Zecher.", "tokens": ["Ich", "wer\u00b7de", "schon", "wie\u00b7der", "ein", "l\u00f6b\u00b7li\u00b7cher", "Ze\u00b7cher", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "In rauschende Lieder", "tokens": ["In", "rau\u00b7schen\u00b7de", "Lie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Schw\u00e4rmender Br\u00fcder", "tokens": ["Schw\u00e4r\u00b7men\u00b7der", "Br\u00fc\u00b7der"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Misch ich fortan meinen Jubel ein \u2013", "tokens": ["Misch", "ich", "for\u00b7tan", "mei\u00b7nen", "Ju\u00b7bel", "ein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PPOSAT", "NN", "ART", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Es lebe die Freude, es lebe der Wein!", "tokens": ["Es", "le\u00b7be", "die", "Freu\u00b7de", ",", "es", "le\u00b7be", "der", "Wein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Das lumpichte Schmachten, das leidige Sehnen", "tokens": ["Das", "lum\u00b7pich\u00b7te", "Schmach\u00b7ten", ",", "das", "lei\u00b7di\u00b7ge", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Es tauget nicht, bringt statt Gedanken nur Thr\u00e4nen,", "tokens": ["Es", "tau\u00b7get", "nicht", ",", "bringt", "statt", "Ge\u00b7dan\u00b7ken", "nur", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "VVFIN", "APPR", "NN", "ADV", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Nur Wanken und Schwanken,", "tokens": ["Nur", "Wan\u00b7ken", "und", "Schwan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Gr\u00fcbeln und Kranken,", "tokens": ["Gr\u00fc\u00b7beln", "und", "Kran\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Ein mi\u00dftrauisch Wesen, Jedwedem Halbfeind,", "tokens": ["Ein", "mi\u00df\u00b7trau\u00b7isch", "We\u00b7sen", ",", "Jed\u00b7we\u00b7dem", "Halb\u00b7feind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PIAT", "NN", "$,"], "meter": "+-+-+--+-++", "measure": "sapphicusminor"}, "line.6": {"text": "Und bringt zur Verzweiflung den besten Freund.", "tokens": ["Und", "bringt", "zur", "Ver\u00b7zwei\u00b7flung", "den", "bes\u00b7ten", "Freund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+---+-+-+", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Begraben der Liebe Lust und Schmerzen!", "tokens": ["Be\u00b7gra\u00b7ben", "der", "Lie\u00b7be", "Lust", "und", "Schmer\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich habe euch wieder, ihr fr\u00f6hlichen Herzen!", "tokens": ["Ich", "ha\u00b7be", "euch", "wie\u00b7der", ",", "ihr", "fr\u00f6h\u00b7li\u00b7chen", "Her\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Die kleinen Leiden", "tokens": ["Die", "klei\u00b7nen", "Lei\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Sterben und scheiden,", "tokens": ["Ster\u00b7ben", "und", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Wir kosten den sch\u00f6nen Augenblick,", "tokens": ["Wir", "kos\u00b7ten", "den", "sch\u00f6\u00b7nen", "Au\u00b7gen\u00b7blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und tr\u00e4umen laut von der Zukunft Gl\u00fcck.", "tokens": ["Und", "tr\u00e4u\u00b7men", "laut", "von", "der", "Zu\u00b7kunft", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Drum sollen sich alle Stimmen l\u00f6sen,", "tokens": ["Drum", "sol\u00b7len", "sich", "al\u00b7le", "Stim\u00b7men", "l\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PRF", "PIAT", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es ist ein t\u00fcchtiges Herz genesen.", "tokens": ["Es", "ist", "ein", "t\u00fcch\u00b7ti\u00b7ges", "Herz", "ge\u00b7ne\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Gesang erschalle,", "tokens": ["Ge\u00b7sang", "er\u00b7schal\u00b7le", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Der B\u00f6ller knalle!", "tokens": ["Der", "B\u00f6l\u00b7ler", "knal\u00b7le", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Schaut vorw\u00e4rts nur und nie zur\u00fcck \u2013", "tokens": ["Schaut", "vor\u00b7w\u00e4rts", "nur", "und", "nie", "zu\u00b7r\u00fcck", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "KON", "ADV", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gesundheit Br\u00fcder! Wohlauf Musik!", "tokens": ["Ge\u00b7sund\u00b7heit", "Br\u00fc\u00b7der", "!", "Wohl\u00b7auf", "Mu\u00b7sik", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "NN", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}}}}