{"dta.poem.3841": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Auf eine liebe Tochter,  \n  C. E. Georgin.  1736.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Wo JEsus ist, da mu\u00df ich leben,", "tokens": ["Wo", "Je\u00b7sus", "ist", ",", "da", "mu\u00df", "ich", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "VAFIN", "$,", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er geht zu seinem Vater ein,", "tokens": ["Er", "geht", "zu", "sei\u00b7nem", "Va\u00b7ter", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Drum will auch ich den Geist aufgeben,", "tokens": ["Drum", "will", "auch", "ich", "den", "Geist", "auf\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "ADV", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo JEsus ist, da mu\u00df ich seyn.", "tokens": ["Wo", "Je\u00b7sus", "ist", ",", "da", "mu\u00df", "ich", "seyn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "VAFIN", "$,", "ADV", "VMFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Fault gleich mein Leib im Schoo\u00df der Erden,", "tokens": ["Fault", "gleich", "mein", "Leib", "im", "Schoo\u00df", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Einst wird er neu belebet werden,", "tokens": ["Einst", "wird", "er", "neu", "be\u00b7le\u00b7bet", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VVFIN", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Er ruhet sanfft in seiner Grufft;", "tokens": ["Er", "ru\u00b7het", "sanfft", "in", "sei\u00b7ner", "Grufft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ob ihn gleich Sand und Bogen decken,", "tokens": ["Ob", "ihn", "gleich", "Sand", "und", "Bo\u00b7gen", "de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Mein Joel wird ihn auferwecken,", "tokens": ["Mein", "Joel", "wird", "ihn", "au\u00b7fer\u00b7we\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "VAFIN", "PPER", "VVIZU", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Wenn er dereinst die Seinen rufft.", "tokens": ["Wenn", "er", "de\u00b7reinst", "die", "Sei\u00b7nen", "rufft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "PPOSS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Darum rechnet meinen Tod euch zu keinem Her-", "tokens": ["Da\u00b7rum", "rech\u00b7net", "mei\u00b7nen", "Tod", "euch", "zu", "kei\u00b7nem", "Her"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "PPER", "APPR", "PIAT", "TRUNC"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.12": {"text": "tzeleide,", "tokens": ["tze\u00b7lei\u00b7de", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "Papa, Schwester, lebet wohl, g\u00f6nnet mir die", "tokens": ["Pa\u00b7pa", ",", "Schwes\u00b7ter", ",", "le\u00b7bet", "wohl", ",", "g\u00f6n\u00b7net", "mir", "die"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "ADV", "$,", "VVFIN", "PPER", "ART"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.14": {"text": "s\u00fcsse Freude.", "tokens": ["s\u00fcs\u00b7se", "Freu\u00b7de", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}