{"dta.poem.13151": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Neuntes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201edie Wahrheit \u201e, sagte sie, mu\u00df meine Neigung kennen,", "tokens": ["\u201e", "die", "Wahr\u00b7heit", "\u201e", ",", "sag\u00b7te", "sie", ",", "mu\u00df", "mei\u00b7ne", "Nei\u00b7gung", "ken\u00b7nen", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$(", "$,", "VVFIN", "PPER", "$,", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201eda\u00df sie Belieben tr\u00e4gt, mein Augen-Ma\u00df zu nennen.", "tokens": ["\u201e", "da\u00df", "sie", "Be\u00b7lie\u00b7ben", "tr\u00e4gt", ",", "mein", "Au\u00b7gen\u00b7Ma\u00df", "zu", "nen\u00b7nen", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "NN", "VVFIN", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201ewahr ists, ich halte viel auf dessen Richtigkeit;", "tokens": ["\u201e", "wahr", "ists", ",", "ich", "hal\u00b7te", "viel", "auf", "des\u00b7sen", "Rich\u00b7tig\u00b7keit", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VAFIN", "$,", "PPER", "VVFIN", "ADV", "APPR", "PRELAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "250\u201dEs zeigt mir jedes Wercks Kunst und Vollkommenheit:", "tokens": ["\"", "Es", "zeigt", "mir", "je\u00b7des", "Wercks", "Kunst", "und", "Voll\u00b7kom\u00b7men\u00b7heit", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "PIAT", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-++-++-+", "measure": "unknown.measure.septa"}, "line.5": {"text": "\u201eich ward auch diese Nacht dadurch erg\u00f6zt, vergn\u00fcget,", "tokens": ["\u201e", "ich", "ward", "auch", "die\u00b7se", "Nacht", "da\u00b7durch", "er\u00b7g\u00f6zt", ",", "ver\u00b7gn\u00fc\u00b7get", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "PDAT", "NN", "PAV", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201ees hat vielleicht daher der Zufall sich gef\u00fcget.", "tokens": ["\u201e", "es", "hat", "viel\u00b7leicht", "da\u00b7her", "der", "Zu\u00b7fall", "sich", "ge\u00b7f\u00fc\u00b7get", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "PAV", "ART", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201ekaum trat\u2019 ich in die Stadt, traff\u2019 ich die Wahrheit an,", "tokens": ["\u201e", "kaum", "trat'", "ich", "in", "die", "Stadt", ",", "traff'", "ich", "die", "Wahr\u00b7heit", "an", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201edie mir gleich Geist und Herz und Sinnen abgewann.", "tokens": ["\u201e", "die", "mir", "gleich", "Geist", "und", "Herz", "und", "Sin\u00b7nen", "ab\u00b7ge\u00b7wann", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PRELS", "PPER", "ADV", "NN", "KON", "NN", "KON", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "255\u201dWir hatten vielerley, der Zeit gem\u00e4\u00df, gesprochen,", "tokens": ["\"", "Wir", "hat\u00b7ten", "vie\u00b7ler\u00b7ley", ",", "der", "Zeit", "ge\u00b7m\u00e4\u00df", ",", "ge\u00b7spro\u00b7chen", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PIAT", "$,", "ART", "NN", "ADJD", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201ebi\u00df unter andern sie mein Reden unterbrochen:", "tokens": ["\u201e", "bi\u00df", "un\u00b7ter", "an\u00b7dern", "sie", "mein", "Re\u00b7den", "un\u00b7ter\u00b7bro\u00b7chen", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "APPR", "PIS", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201esie m\u00f6chte, trug sie vor, ein solches Pracht-Geb\u00e4u,", "tokens": ["\u201e", "sie", "m\u00f6ch\u00b7te", ",", "trug", "sie", "vor", ",", "ein", "sol\u00b7ches", "Pracht\u00b7Ge\u00b7b\u00e4u", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "$,", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201edas f\u00fcr ", "tokens": ["\u201e", "das", "f\u00fcr"], "token_info": ["punct", "word", "word"], "pos": ["$(", "PDS", "APPR"], "meter": "-+", "measure": "iambic.single"}, "line.13": {"text": "\u201edoch sonder aller Kunst gemeiner Richtschnur bauen;", "tokens": ["\u201e", "doch", "son\u00b7der", "al\u00b7ler", "Kunst", "ge\u00b7mei\u00b7ner", "Richt\u00b7schnur", "bau\u00b7en", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "PIAT", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "260\u201dWer k\u00f6nnte, sagte sie, sich es zu f\u00fchren trauen?", "tokens": ["\"", "Wer", "k\u00f6nn\u00b7te", ",", "sag\u00b7te", "sie", ",", "sich", "es", "zu", "f\u00fch\u00b7ren", "trau\u00b7en", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VMFIN", "$,", "VVFIN", "PPER", "$,", "PRF", "PPER", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}