{"textgrid.poem.43093": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Eine lange graue Fl\u00e4che,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine lange graue Fl\u00e4che,", "tokens": ["Ei\u00b7ne", "lan\u00b7ge", "grau\u00b7e", "Fl\u00e4\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mitten drauf ein Schl\u00f6\u00dflein traut;", "tokens": ["Mit\u00b7ten", "drauf", "ein", "Schl\u00f6\u00df\u00b7lein", "traut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wei\u00df und voll im Winde schwanket", "tokens": ["Wei\u00df", "und", "voll", "im", "Win\u00b7de", "schwan\u00b7ket"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "ADJD", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rings umher das Haidekraut.", "tokens": ["Rings", "um\u00b7her", "das", "Hai\u00b7de\u00b7kraut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Bei des Schl\u00f6\u00dfchens Erkerfenster", "tokens": ["Bei", "des", "Schl\u00f6\u00df\u00b7chens", "Er\u00b7ker\u00b7fens\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Steht ein Mann und jubelt laut;", "tokens": ["Steht", "ein", "Mann", "und", "ju\u00b7belt", "laut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn er hat jetzt in der Ferne", "tokens": ["Denn", "er", "hat", "jetzt", "in", "der", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sein geliebtes Weib erschaut.", "tokens": ["Sein", "ge\u00b7lieb\u00b7tes", "Weib", "er\u00b7schaut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Jauchzend springt er ihr entgegen,", "tokens": ["Jauch\u00b7zend", "springt", "er", "ihr", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00fc\u00dft sie hei\u00df auf Mund und Hand,", "tokens": ["K\u00fc\u00dft", "sie", "hei\u00df", "auf", "Mund", "und", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ordnet die zerstreuten Locken", "tokens": ["Ord\u00b7net", "die", "zer\u00b7streu\u00b7ten", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und das flatternde Gewand.", "tokens": ["Und", "das", "flat\u00b7tern\u00b7de", "Ge\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und wie Kinder selig plaudernd", "tokens": ["Und", "wie", "Kin\u00b7der", "se\u00b7lig", "plau\u00b7dernd"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "NN", "ADJD", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gehen sie nun Hand in Hand,", "tokens": ["Ge\u00b7hen", "sie", "nun", "Hand", "in", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und des Weibes Seele segnet", "tokens": ["Und", "des", "Wei\u00b7bes", "See\u00b7le", "seg\u00b7net"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dankbar Mann und Haus und Land.", "tokens": ["Dank\u00b7bar", "Mann", "und", "Haus", "und", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}