{"textgrid.poem.32718": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "Lied einer Nonne", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, du lieber Mond! wie helle", "tokens": ["Ach", ",", "du", "lie\u00b7ber", "Mond", "!", "wie", "hel\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "$,", "PPER", "ADV", "NN", "$.", "PWAV", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Scheinest du in diese Zelle,", "tokens": ["Schei\u00b7nest", "du", "in", "die\u00b7se", "Zel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo, auf ewig eingemaurt,", "tokens": ["Wo", ",", "auf", "e\u00b7wig", "ein\u00b7ge\u00b7maurt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "APPR", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gottes Anverlobte traurt!", "tokens": ["Got\u00b7tes", "An\u00b7ver\u00b7lob\u00b7te", "traurt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aber leiser, meine Klage!", "tokens": ["A\u00b7ber", "lei\u00b7ser", ",", "mei\u00b7ne", "Kla\u00b7ge", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df kein Laut das Gl\u00fcck verjage,", "tokens": ["Da\u00df", "kein", "Laut", "das", "Gl\u00fcck", "ver\u00b7ja\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "So in Tr\u00e4umen mild und s\u00fc\u00df,", "tokens": ["So", "in", "Tr\u00e4u\u00b7men", "mild", "und", "s\u00fc\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Sich zu Schwestern niederlie\u00df.", "tokens": ["Sich", "zu", "Schwes\u00b7tern", "nie\u00b7der\u00b7lie\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Schlummert, o geliebte Seelen!", "tokens": ["Schlum\u00b7mert", ",", "o", "ge\u00b7lieb\u00b7te", "See\u00b7len", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "FM", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich will mich alleine qu\u00e4len;", "tokens": ["Ich", "will", "mich", "al\u00b7lei\u00b7ne", "qu\u00e4\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Will, im stillen, meiner Pein", "tokens": ["Will", ",", "im", "stil\u00b7len", ",", "mei\u00b7ner", "Pein"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "$,", "APPRART", "ADJA", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jammernde Vertraute sein.", "tokens": ["Jam\u00b7mern\u00b7de", "Ver\u00b7trau\u00b7te", "sein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAINF", "$."], "meter": "+---+-+", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Schlummert ihr auch, deren H\u00e4rte", "tokens": ["Schlum\u00b7mert", "ihr", "auch", ",", "de\u00b7ren", "H\u00e4r\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PRELAT", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Mich in diesen Kerker sperrte!", "tokens": ["Mich", "in", "die\u00b7sen", "Ker\u00b7ker", "sperr\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Vater! Mutter! schlummert ein!", "tokens": ["Va\u00b7ter", "!", "Mut\u00b7ter", "!", "schlum\u00b7mert", "ein", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jesus will, ich soll verzeihn.", "tokens": ["Je\u00b7sus", "will", ",", "ich", "soll", "ver\u00b7zeihn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "$,", "PPER", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Aber fromme, sanfte Klagen", "tokens": ["A\u00b7ber", "from\u00b7me", ",", "sanf\u00b7te", "Kla\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kann mir Jesus nicht versagen;", "tokens": ["Kann", "mir", "Je\u00b7sus", "nicht", "ver\u00b7sa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NE", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schuf er meine Seele doch", "tokens": ["Schuf", "er", "mei\u00b7ne", "See\u00b7le", "doch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht f\u00fcr dieses schwere Joch!", "tokens": ["Nicht", "f\u00fcr", "die\u00b7ses", "schwe\u00b7re", "Joch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Jeder Vogel darf im Freien", "tokens": ["Je\u00b7der", "Vo\u00b7gel", "darf", "im", "Frei\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VMFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich mit seinesgleichen freuen;", "tokens": ["Sich", "mit", "sei\u00b7nes\u00b7glei\u00b7chen", "freu\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jedes W\u00fcrmchen, noch so zart,", "tokens": ["Je\u00b7des", "W\u00fcrm\u00b7chen", ",", "noch", "so", "zart", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Spielt mit W\u00fcrmchen seiner Art.", "tokens": ["Spielt", "mit", "W\u00fcrm\u00b7chen", "sei\u00b7ner", "Art", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Noch im sp\u00e4ten Mondenglanze", "tokens": ["Noch", "im", "sp\u00e4\u00b7ten", "Mon\u00b7den\u00b7glan\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Drehen M\u00fccken sich im Tanze;", "tokens": ["Dre\u00b7hen", "M\u00fc\u00b7cken", "sich", "im", "Tan\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "PRF", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Alles freuet inniglich", "tokens": ["Al\u00b7les", "freu\u00b7et", "in\u00b7nig\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["PIS", "VVFIN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dein, o s\u00fc\u00dfe Freiheit, sich!", "tokens": ["Dein", ",", "o", "s\u00fc\u00b7\u00dfe", "Frei\u00b7heit", ",", "sich", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "$,", "FM", "ADJA", "NN", "$,", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Nur uns armen, guten Seelen", "tokens": ["Nur", "uns", "ar\u00b7men", ",", "gu\u00b7ten", "See\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PPER", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Soll dein Gl\u00fcck auf ewig fehlen;", "tokens": ["Soll", "dein", "Gl\u00fcck", "auf", "e\u00b7wig", "feh\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "APPR", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Allen Freuden unbekannt,", "tokens": ["Al\u00b7len", "Freu\u00b7den", "un\u00b7be\u00b7kannt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat uns Wahn hieher gebannt!", "tokens": ["Hat", "uns", "Wahn", "hie\u00b7her", "ge\u00b7bannt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "PAV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "An den heiligen Alt\u00e4ren", "tokens": ["An", "den", "hei\u00b7li\u00b7gen", "Al\u00b7t\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mu\u00dft' ich jeder Lust entschw\u00f6ren;", "tokens": ["Mu\u00dft'", "ich", "je\u00b7der", "Lust", "ent\u00b7schw\u00f6\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mutter Gottes! Ach, ich schwur!", "tokens": ["Mut\u00b7ter", "Got\u00b7tes", "!", "Ach", ",", "ich", "schwur", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "ITJ", "$,", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ich brach, ich brach den Schwur!", "tokens": ["Und", "ich", "brach", ",", "ich", "brach", "den", "Schwur", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Diese Seufzer, diese Blicke", "tokens": ["Die\u00b7se", "Seuf\u00b7zer", ",", "die\u00b7se", "Bli\u00b7cke"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PDAT", "NN", "$,", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schmachten nach der Welt zur\u00fccke;", "tokens": ["Schmach\u00b7ten", "nach", "der", "Welt", "zu\u00b7r\u00fc\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sehnen wiederum von hier,", "tokens": ["Seh\u00b7nen", "wie\u00b7de\u00b7rum", "von", "hier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "O Clarissa, sich zu dir!", "tokens": ["O", "Cla\u00b7ris\u00b7sa", ",", "sich", "zu", "dir", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PRF", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Du, an meiner Brust erzogen,", "tokens": ["Du", ",", "an", "mei\u00b7ner", "Brust", "er\u00b7zo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach, ich bin, ich bin betrogen!", "tokens": ["Ach", ",", "ich", "bin", ",", "ich", "bin", "be\u00b7tro\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "$,", "PPER", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Was man mir so sch\u00f6n gemalt,", "tokens": ["Was", "man", "mir", "so", "sch\u00f6n", "ge\u00b7malt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist des Jammers Aufenthalt.", "tokens": ["Ist", "des", "Jam\u00b7mers", "Auf\u00b7ent\u00b7halt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Weine, Freundin! Ach vergebens", "tokens": ["Wei\u00b7ne", ",", "Freun\u00b7din", "!", "Ach", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "NN", "$.", "ITJ", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Freut' ich mich mit dir des Lebens;", "tokens": ["Freut'", "ich", "mich", "mit", "dir", "des", "Le\u00b7bens", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Welt, die voller Pracht", "tokens": ["Und", "der", "Welt", ",", "die", "vol\u00b7ler", "Pracht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Allen, nur nicht mir, gelacht!", "tokens": ["Al\u00b7len", ",", "nur", "nicht", "mir", ",", "ge\u00b7lacht", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "ADV", "PTKNEG", "PPER", "$,", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Rosen pflanzt' ich; eh' sie bl\u00fchen,", "tokens": ["Ro\u00b7sen", "pflanzt'", "ich", ";", "eh'", "sie", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$.", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Werd' ich diesen Jammer fliehen.", "tokens": ["Werd'", "ich", "die\u00b7sen", "Jam\u00b7mer", "flie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PDAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Pfl\u00fccke sie vom Strauch herab,", "tokens": ["Pfl\u00fc\u00b7cke", "sie", "vom", "Strauch", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und bestreue mir mein Grab!", "tokens": ["Und", "be\u00b7streu\u00b7e", "mir", "mein", "Grab", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Weine Freundin! Diese Blicke", "tokens": ["Wei\u00b7ne", "Freun\u00b7din", "!", "Die\u00b7se", "Bli\u00b7cke"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "NN", "$.", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schmachten nach der Welt zur\u00fccke.", "tokens": ["Schmach\u00b7ten", "nach", "der", "Welt", "zu\u00b7r\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mutter Gottes! ach, ich schwur!", "tokens": ["Mut\u00b7ter", "Got\u00b7tes", "!", "ach", ",", "ich", "schwur", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "XY", "$,", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ich brach, ich brach den Schwur!", "tokens": ["Und", "ich", "brach", ",", "ich", "brach", "den", "Schwur", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}