{"dta.poem.16310": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "46.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ungl\u00fccklich kan ein Mensch vor lauter Gl\u00fcck sich f\u00fchlen;", "tokens": ["Un\u00b7gl\u00fcck\u00b7lich", "kan", "ein", "Mensch", "vor", "lau\u00b7ter", "Gl\u00fcck", "sich", "f\u00fch\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "ART", "NN", "APPR", "PIAT", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein kleines Ungemach kan gro\u00dfen Schaden k\u00fchlen.", "tokens": ["Ein", "klei\u00b7nes", "Un\u00b7ge\u00b7mach", "kan", "gro\u00b7\u00dfen", "Scha\u00b7den", "k\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich denk' an einen Freund, der, weil bei Nacht und Schlaf", "tokens": ["Ich", "denk'", "an", "ei\u00b7nen", "Freund", ",", "der", ",", "weil", "bei", "Nacht", "und", "Schlaf"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "$,", "KOUS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nie eine Feuersbrunst, ein Diebstahl nie ihn traf,", "tokens": ["Nie", "ei\u00b7ne", "Feu\u00b7ers\u00b7brunst", ",", "ein", "Dieb\u00b7stahl", "nie", "ihn", "traf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ART", "NN", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sich muste Nacht f\u00fcr Nacht vom Traume lassen \u00e4ffen,", "tokens": ["Sich", "mus\u00b7te", "Nacht", "f\u00fcr", "Nacht", "vom", "Trau\u00b7me", "las\u00b7sen", "\u00e4f\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJA", "NN", "APPR", "NN", "APPRART", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jetzt endlich sollte, was ihn noch nicht traf, betreffen.", "tokens": ["Jetzt", "end\u00b7lich", "soll\u00b7te", ",", "was", "ihn", "noch", "nicht", "traf", ",", "be\u00b7tref\u00b7fen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "$,", "PRELS", "PPER", "ADV", "PTKNEG", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wie gl\u00fccklich h\u00e4tt' ein Dieb, ein Feuer ihn gemacht!", "tokens": ["Wie", "gl\u00fcck\u00b7lich", "h\u00e4tt'", "ein", "Dieb", ",", "ein", "Feu\u00b7er", "ihn", "ge\u00b7macht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "$,", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Geruhig h\u00e4tt' er dann geschlafen jede Nacht.", "tokens": ["Ge\u00b7ru\u00b7hig", "h\u00e4tt'", "er", "dann", "ge\u00b7schla\u00b7fen", "je\u00b7de", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "VVPP", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}