{"textgrid.poem.59474": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Schatzgr\u00e4ber", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn alle W\u00e4lder schliefen,", "tokens": ["Wenn", "al\u00b7le", "W\u00e4l\u00b7der", "schlie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er an zu graben hub,", "tokens": ["Er", "an", "zu", "gra\u00b7ben", "hub", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Rastlos in Berges Tiefen", "tokens": ["Rast\u00b7los", "in", "Ber\u00b7ges", "Tie\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nach einem Schatz er grub.", "tokens": ["Nach", "ei\u00b7nem", "Schatz", "er", "grub", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Engel Gottes sangen", "tokens": ["Die", "En\u00b7gel", "Got\u00b7tes", "san\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Derweil in stiller Nacht,", "tokens": ["Der\u00b7weil", "in", "stil\u00b7ler", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie rote Augen drangen", "tokens": ["Wie", "ro\u00b7te", "Au\u00b7gen", "dran\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Metalle aus dem Schacht.", "tokens": ["Me\u00b7tal\u00b7le", "aus", "dem", "Schacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u00bbund wirst doch mein!\u00ab und grimmer", "tokens": ["\u00bb", "und", "wirst", "doch", "mein", "!", "\u00ab", "und", "grim\u00b7mer"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["$(", "KON", "VAFIN", "ADV", "PPOSAT", "$.", "$(", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "W\u00fchlt er und w\u00fchlt hinab,", "tokens": ["W\u00fchlt", "er", "und", "w\u00fchlt", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da st\u00fcrzen Steine und Tr\u00fcmmer", "tokens": ["Da", "st\u00fcr\u00b7zen", "Stei\u00b7ne", "und", "Tr\u00fcm\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "\u00dcber dem Narren herab.", "tokens": ["\u00dc\u00b7ber", "dem", "Nar\u00b7ren", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Hohnlachen wild erschallte", "tokens": ["Hohn\u00b7la\u00b7chen", "wild", "er\u00b7schall\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJD", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus der verfallnen Kluft,", "tokens": ["Aus", "der", "ver\u00b7fall\u00b7nen", "Kluft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Engelgesang verhallte", "tokens": ["Der", "En\u00b7gel\u00b7ge\u00b7sang", "ver\u00b7hall\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wehm\u00fctig in der Luft.", "tokens": ["Weh\u00b7m\u00fc\u00b7tig", "in", "der", "Luft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}