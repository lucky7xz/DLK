{"dta.poem.784": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Xx.  \n An Eben denselben: gegen \u00fcbersendete  \n Sonnet.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Der Himmel hat auff mich gew\u00fcttet. ", "tokens": ["Der", "Him\u00b7mel", "hat", "auff", "mich", "ge\u00b7w\u00fct\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Er hat mich mehr denn hoch betr\u00fcbt.", "tokens": ["Er", "hat", "mich", "mehr", "denn", "hoch", "be\u00b7tr\u00fcbt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vnd alles was mein Hertz gelibt/", "tokens": ["Vnd", "al\u00b7les", "was", "mein", "Hertz", "ge\u00b7libt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PWS", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Erden/ in der grufft besch\u00fcttet/", "tokens": ["Mit", "Er\u00b7den", "/", "in", "der", "grufft", "be\u00b7sch\u00fct\u00b7tet", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er hat im Elend mich verh\u00fcttet/", "tokens": ["Er", "hat", "im", "E\u00b7lend", "mich", "ver\u00b7h\u00fct\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er hat durch marter mich ge\u00fcbt", "tokens": ["Er", "hat", "durch", "mar\u00b7ter", "mich", "ge\u00b7\u00fcbt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gleich einem den man \u00fcbergibt/", "tokens": ["Gleich", "ei\u00b7nem", "den", "man", "\u00fc\u00b7berg\u00b7ibt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ART", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der pein/ die Seel vnd Fleisch zerr\u00fcttet.", "tokens": ["Der", "pein", "/", "die", "Seel", "vnd", "Fleisch", "zer\u00b7r\u00fct\u00b7tet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch! da\u00df ich bi\u00dfher nicht verschmachtet/", "tokens": ["Doch", "!", "da\u00df", "ich", "bi\u00df\u00b7her", "nicht", "ver\u00b7schmach\u00b7tet", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$.", "KOUS", "PPER", "ADV", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da\u00df mich die welt noch lebend achtet?", "tokens": ["Da\u00df", "mich", "die", "welt", "noch", "le\u00b7bend", "ach\u00b7tet", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "R\u00fchrt daher/ da\u00df mein Hertz bey Sch\u00f6nborn sich befunden.", "tokens": ["R\u00fchrt", "da\u00b7her", "/", "da\u00df", "mein", "Hertz", "bey", "Sch\u00f6n\u00b7bo\u00b7rn", "sich", "be\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "$(", "KOUS", "PPOSAT", "NN", "APPR", "NN", "PRF", "VVPP", "$."], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}}, "stanza.5": {"line.1": {"text": "Ich f\u00fchle den den rawen schlag", "tokens": ["Ich", "f\u00fch\u00b7le", "den", "den", "ra\u00b7wen", "schlag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als ein entseelter Leib/ die plag/", "tokens": ["Als", "ein", "ent\u00b7seel\u00b7ter", "Leib", "/", "die", "plag", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$(", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ja als ein Fel\u00dft: den Sturm. al\u00df Marmor: eine wunden.", "tokens": ["Ja", "als", "ein", "Fel\u00dft", ":", "den", "Sturm", ".", "al\u00df", "Mar\u00b7mor", ":", "ei\u00b7ne", "wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "KOUS", "ART", "NN", "$.", "ART", "NN", "$.", "KOUS", "NN", "$.", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}