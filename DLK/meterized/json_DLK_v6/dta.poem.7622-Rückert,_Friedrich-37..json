{"dta.poem.7622": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "37.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Sonne kannst du nicht ins Feuerauge schaun,", "tokens": ["Der", "Son\u00b7ne", "kannst", "du", "nicht", "ins", "Feu\u00b7er\u00b7au\u00b7ge", "schaun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PTKNEG", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zum sanften Monde nur hast du ein solch Vertraun.", "tokens": ["Zum", "sanf\u00b7ten", "Mon\u00b7de", "nur", "hast", "du", "ein", "solch", "Ver\u00b7traun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "VAFIN", "PPER", "ART", "PIAT", "NN", "$."], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Blumen aber thun vorm Mond ihr Auge zu,", "tokens": ["Die", "Blu\u00b7men", "a\u00b7ber", "thun", "vorm", "Mond", "ihr", "Au\u00b7ge", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "APPRART", "NN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und auf vorm Sonnenblick, den Blumen gleichst nicht du.", "tokens": ["Und", "auf", "vorm", "Son\u00b7nen\u00b7blick", ",", "den", "Blu\u00b7men", "gleichst", "nicht", "du", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "APPRART", "NN", "$,", "ART", "NN", "ADV", "PTKNEG", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wenn deine Unschuld erst ist Blumen gleich vollendet,", "tokens": ["Wenn", "dei\u00b7ne", "Un\u00b7schuld", "erst", "ist", "Blu\u00b7men", "gleich", "voll\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "VAFIN", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wirst du die Sonne, wie den Mond, sehn ungeblendet.", "tokens": ["Wirst", "du", "die", "Son\u00b7ne", ",", "wie", "den", "Mond", ",", "sehn", "un\u00b7ge\u00b7blen\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "PWAV", "ART", "NN", "$,", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}