{"textgrid.poem.57924": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Keine Bange", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Logenhaus im Hoftheater", "tokens": ["Das", "Lo\u00b7gen\u00b7haus", "im", "Hof\u00b7the\u00b7a\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wird umgebaut, so wie man sagt,", "tokens": ["Wird", "um\u00b7ge\u00b7baut", ",", "so", "wie", "man", "sagt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "ADV", "KOKOM", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Warum denn dieses nun gesch\u00e4he,", "tokens": ["Wa\u00b7rum", "denn", "die\u00b7ses", "nun", "ge\u00b7sch\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PDAT", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der gute Hannovraner fragt.", "tokens": ["Der", "gu\u00b7te", "Han\u00b7no\u00b7vra\u00b7ner", "fragt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "In unserm alten Hoftheater", "tokens": ["In", "un\u00b7serm", "al\u00b7ten", "Hof\u00b7the\u00b7a\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Liebt man sonst keine \u00c4nderung;", "tokens": ["Liebt", "man", "sonst", "kei\u00b7ne", "\u00c4n\u00b7de\u00b7rung", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "PIAT", "NN", "$."], "meter": "+--+-+--", "measure": "iambic.tri.invert"}, "line.3": {"text": "Man sagt dort, was so lange gut war,", "tokens": ["Man", "sagt", "dort", ",", "was", "so", "lan\u00b7ge", "gut", "war", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "$,", "PRELS", "ADV", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Das ist auch heut noch gut genung.", "tokens": ["Das", "ist", "auch", "heut", "noch", "gut", "ge\u00b7nung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "ADV", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So denkt man sich: Aha, Chikago,", "tokens": ["So", "denkt", "man", "sich", ":", "A\u00b7ha", ",", "Chi\u00b7ka\u00b7go", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PRF", "$.", "NN", "$,", "NE", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vorbeugung gegen Brandgefahr,", "tokens": ["Vor\u00b7beu\u00b7gung", "ge\u00b7gen", "Brand\u00b7ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Logenhaus ist alt und dr\u00f6ge,", "tokens": ["Das", "Lo\u00b7gen\u00b7haus", "ist", "alt", "und", "dr\u00f6\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch mehr noch das Repertoire.", "tokens": ["Doch", "mehr", "noch", "das", "Re\u00b7per\u00b7toi\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch darum seid nur ohne Sorge,", "tokens": ["Doch", "da\u00b7rum", "seid", "nur", "oh\u00b7ne", "Sor\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VAFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Ausnahme des \u00bbGro\u00dfen Lichts\u00ab,", "tokens": ["Mit", "Aus\u00b7nah\u00b7me", "des", "\u00bb", "Gro\u00b7\u00dfen", "Lichts", "\u00ab", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "ART", "$(", "ADJA", "NN", "$(", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Da z\u00fcndet in dem Hoftheater", "tokens": ["Da", "z\u00fcn\u00b7det", "in", "dem", "Hof\u00b7the\u00b7a\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Seit Ewigkeit schon lange nichts.", "tokens": ["Seit", "E\u00b7wig\u00b7keit", "schon", "lan\u00b7ge", "nichts", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADV", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}