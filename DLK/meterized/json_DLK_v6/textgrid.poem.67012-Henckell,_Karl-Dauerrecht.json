{"textgrid.poem.67012": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Dauerrecht", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was echte Leidenschaft gebar", "tokens": ["Was", "ech\u00b7te", "Lei\u00b7den\u00b7schaft", "ge\u00b7bar"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Selbst aus dem Irrtum ihrer Zeit,", "tokens": ["Selbst", "aus", "dem", "Irr\u00b7tum", "ih\u00b7rer", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Bleibt mit dem Recht des Herzens wahr", "tokens": ["Bleibt", "mit", "dem", "Recht", "des", "Her\u00b7zens", "wahr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und w\u00e4hrt, vom Kelch der Form geweiht.", "tokens": ["Und", "w\u00e4hrt", ",", "vom", "Kelch", "der", "Form", "ge\u00b7weiht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPRART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}