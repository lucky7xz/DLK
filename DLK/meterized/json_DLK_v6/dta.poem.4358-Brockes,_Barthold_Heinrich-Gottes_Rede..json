{"dta.poem.4358": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Gottes Rede.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Sch\u00f6pfer, la\u00df, in Deinen Werken,", "tokens": ["Mein", "Sch\u00f6p\u00b7fer", ",", "la\u00df", ",", "in", "Dei\u00b7nen", "Wer\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVIMP", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mich Deine Lieb\u2019 und Macht bemerken!", "tokens": ["Mich", "Dei\u00b7ne", "Lieb'", "und", "Macht", "be\u00b7mer\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sey Du darinn mein Zweck allein!", "tokens": ["Sey", "Du", "da\u00b7rinn", "mein", "Zweck", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PAV", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "La\u00df meinen Willen Deinen Willen", "tokens": ["La\u00df", "mei\u00b7nen", "Wil\u00b7len", "Dei\u00b7nen", "Wil\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Auf eine Weise zu erf\u00fcllen,", "tokens": ["Auf", "ei\u00b7ne", "Wei\u00b7se", "zu", "er\u00b7f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Dir gef\u00e4llt, besch\u00e4ftigt seyn!", "tokens": ["Die", "Dir", "ge\u00b7f\u00e4llt", ",", "be\u00b7sch\u00e4f\u00b7tigt", "seyn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "$,", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Gieb, wenn ich h\u00f6re, schmeck\u2019 und sehe,", "tokens": ["Gieb", ",", "wenn", "ich", "h\u00f6\u00b7re", ",", "schmeck'", "und", "se\u00b7he", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "KOUS", "PPER", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df es, nur Dir zum Ruhm, geschehe!", "tokens": ["Da\u00df", "es", ",", "nur", "Dir", "zum", "Ruhm", ",", "ge\u00b7sche\u00b7he", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADV", "PPER", "APPRART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Du zeigst in Deiner Werke Pracht", "tokens": ["Du", "zeigst", "in", "Dei\u00b7ner", "Wer\u00b7ke", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Weit mehr, als v\u00e4terliche Triebe;", "tokens": ["Weit", "mehr", ",", "als", "v\u00e4\u00b7ter\u00b7li\u00b7che", "Trie\u00b7be", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "$,", "KOUS", "ADJA", "NN", "$."], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Du zeigest Weisheit, zeigest Liebe,", "tokens": ["Du", "zei\u00b7gest", "Weis\u00b7heit", ",", "zei\u00b7gest", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und einer wahren Gottheit Macht.", "tokens": ["Und", "ei\u00b7ner", "wah\u00b7ren", "Got\u00b7theit", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du liessest Himmel, Meer und Erden", "tokens": ["Du", "lies\u00b7sest", "Him\u00b7mel", ",", "Meer", "und", "Er\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch Dein allm\u00e4chtig ", "tokens": ["Durch", "Dein", "all\u00b7m\u00e4ch\u00b7tig"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Ist denn nicht hier auch ", "tokens": ["Ist", "denn", "nicht", "hier", "auch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PTKNEG", "ADV", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Die\u00df spricht von Seinem ew\u2019gen Lieben,", "tokens": ["Die\u00df", "spricht", "von", "Sei\u00b7nem", "ew'\u00b7gen", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Es ward in unser Herz geschrieben,", "tokens": ["Es", "ward", "in", "un\u00b7ser", "Herz", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Es spricht dasselbe fort und fort.", "tokens": ["Es", "spricht", "das\u00b7sel\u00b7be", "fort", "und", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Er hat auch Heiden, Jhn zu fassen,", "tokens": ["Er", "hat", "auch", "Hei\u00b7den", ",", "Jhn", "zu", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sich ja nicht unbezeugt gelassen.", "tokens": ["Sich", "ja", "nicht", "un\u00b7be\u00b7zeugt", "ge\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "PTKNEG", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Wodurch? blo\u00df in der Creatur.", "tokens": ["Wo\u00b7durch", "?", "blo\u00df", "in", "der", "Crea\u00b7tur", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Die Bibel selbst spricht: In den Werken", "tokens": ["Die", "Bi\u00b7bel", "selbst", "spricht", ":", "In", "den", "Wer\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "$.", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Kann man der Gottheit Wesen merken,", "tokens": ["Kann", "man", "der", "Got\u00b7theit", "We\u00b7sen", "mer\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Man sieht darinn von GOtt die Spur.", "tokens": ["Man", "sieht", "da\u00b7rinn", "von", "Gott", "die", "Spur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PAV", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie, will man denn die\u00df Wort nicht achten?", "tokens": ["Wie", ",", "will", "man", "denn", "die\u00df", "Wort", "nicht", "ach\u00b7ten", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "VMFIN", "PIS", "ADV", "PDS", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Will man den Inhalt nicht betrachten,", "tokens": ["Will", "man", "den", "In\u00b7halt", "nicht", "be\u00b7trach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Der die selbst\u00e4nd\u2019ge Gottheit ist?", "tokens": ["Der", "die", "selb\u00b7st\u00e4n\u00b7d'\u00b7ge", "Got\u00b7theit", "ist", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Will man von Seinem Wunder-Wesen", "tokens": ["Will", "man", "von", "Sei\u00b7nem", "Wun\u00b7der\u00b7We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die selbstgeschriebne Schrift nicht lesen?", "tokens": ["Die", "selbst\u00b7ge\u00b7schrieb\u00b7ne", "Schrift", "nicht", "le\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie, da\u00df man sich so sehr vergi\u00dft!", "tokens": ["Wie", ",", "da\u00df", "man", "sich", "so", "sehr", "ver\u00b7gi\u00dft", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "KOUS", "PIS", "PRF", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auf das, was GOtt so sch\u00f6n gezieret,", "tokens": ["Auf", "das", ",", "was", "Gott", "so", "sch\u00f6n", "ge\u00b7zie\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PWS", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wird man selbst durch die Schrift gef\u00fchret.", "tokens": ["Wird", "man", "selbst", "durch", "die", "Schrift", "ge\u00b7f\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die Offenbahrung selber spricht,", "tokens": ["Die", "Of\u00b7fen\u00b7bah\u00b7rung", "sel\u00b7ber", "spricht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Jm Werk, von einer Offenbahrung,", "tokens": ["Jm", "Werk", ",", "von", "ei\u00b7ner", "Of\u00b7fen\u00b7bah\u00b7rung", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und leitet uns auf die Erfahrung.", "tokens": ["Und", "lei\u00b7tet", "uns", "auf", "die", "Er\u00b7fah\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Warum gehorcht man beyden nicht?", "tokens": ["Wa\u00b7rum", "ge\u00b7horcht", "man", "bey\u00b7den", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PIS", "PIS", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}