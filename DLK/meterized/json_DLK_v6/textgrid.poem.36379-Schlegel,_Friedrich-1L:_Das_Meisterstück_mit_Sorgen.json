{"textgrid.poem.36379": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das Meisterst\u00fcck mit Sorgen", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Meisterst\u00fcck mit Sorgen", "tokens": ["Das", "Meis\u00b7ter\u00b7st\u00fcck", "mit", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wer nur will schauen an,", "tokens": ["Wer", "nur", "will", "schau\u00b7en", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VMFIN", "VVINF", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihm nimmermehr verborgen", "tokens": ["Ihm", "nim\u00b7mer\u00b7mehr", "ver\u00b7bor\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Meister bleiben kann.", "tokens": ["Der", "Meis\u00b7ter", "blei\u00b7ben", "kann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Von oben mu\u00df uns geben", "tokens": ["Von", "o\u00b7ben", "mu\u00df", "uns", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Licht und golden Schein,", "tokens": ["Das", "Licht", "und", "gol\u00b7den", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In stetem Lauf und Leben", "tokens": ["In", "ste\u00b7tem", "Lauf", "und", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sonn', Mond und Sterne sein.", "tokens": ["Sonn'", ",", "Mond", "und", "Ster\u00b7ne", "sein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Des Tags bis auf den Abend", "tokens": ["Des", "Tags", "bis", "auf", "den", "A\u00b7bend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Sonn' gar freundlich lacht,", "tokens": ["Die", "Sonn'", "gar", "freund\u00b7lich", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu Nacht der Mond erlabend", "tokens": ["Zu", "Nacht", "der", "Mond", "er\u00b7la\u00b7bend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fchrt auf die Sternenwacht.", "tokens": ["F\u00fchrt", "auf", "die", "Ster\u00b7nen\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wer deutet ihn' die Stra\u00dfen,", "tokens": ["Wer", "deu\u00b7tet", "ihn'", "die", "Stra\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wer zeiget ihn' den Weg?", "tokens": ["Wer", "zei\u00b7get", "ihn'", "den", "Weg", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df nie sie unterlassen", "tokens": ["Da\u00df", "nie", "sie", "un\u00b7ter\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu finden ihren Steg?", "tokens": ["Zu", "fin\u00b7den", "ih\u00b7ren", "Steg", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "In lauter gr\u00fcner Seiden", "tokens": ["In", "lau\u00b7ter", "gr\u00fc\u00b7ner", "Sei\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gar zierlich ausgebreit,", "tokens": ["Gar", "zier\u00b7lich", "aus\u00b7ge\u00b7breit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Erdreich sich tut kleiden", "tokens": ["Das", "Er\u00b7dreich", "sich", "tut", "klei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PRF", "ADJD", "VVINF"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Zur werten Sommerzeit.", "tokens": ["Zur", "wer\u00b7ten", "Som\u00b7mer\u00b7zeit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die Pfl\u00e4nzlein in den Felden", "tokens": ["Die", "Pfl\u00e4nz\u00b7lein", "in", "den", "Fel\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sich lieblich schm\u00fccken auf,", "tokens": ["Sich", "lieb\u00b7lich", "schm\u00fc\u00b7cken", "auf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die gr\u00fcne Zweig in W\u00e4lden", "tokens": ["Die", "gr\u00fc\u00b7ne", "Zweig", "in", "W\u00e4l\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auch schlagen aus mit Hauf.", "tokens": ["Auch", "schla\u00b7gen", "aus", "mit", "Hauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "In G\u00e4rten merk ich eben", "tokens": ["In", "G\u00e4r\u00b7ten", "merk", "ich", "e\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die sch\u00f6nen Bl\u00fcmelein,", "tokens": ["Die", "sch\u00f6\u00b7nen", "Bl\u00fc\u00b7me\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie freudig sie da schweben,", "tokens": ["Wie", "freu\u00b7dig", "sie", "da", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wann Wind nur spielt hinein.", "tokens": ["Wann", "Wind", "nur", "spielt", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "O fr\u00f6hlich' Gartenjugend.", "tokens": ["O", "fr\u00f6h\u00b7lich'", "Gar\u00b7ten\u00b7ju\u00b7gend", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O frisch und zartes Blut!", "tokens": ["O", "frisch", "und", "zar\u00b7tes", "Blut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "An Farben reich und Tugend", "tokens": ["An", "Far\u00b7ben", "reich", "und", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJD", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu geben Freudenmut.", "tokens": ["Zu", "ge\u00b7ben", "Freu\u00b7den\u00b7mut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Und wie gemalt dann bl\u00fchet", "tokens": ["Und", "wie", "ge\u00b7malt", "dann", "bl\u00fc\u00b7het"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADJD", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr Bl\u00fcmlein tausendfalt,", "tokens": ["Ihr", "Bl\u00fcm\u00b7lein", "tau\u00b7send\u00b7falt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da alles ihr doch ziehet", "tokens": ["Da", "al\u00b7les", "ihr", "doch", "zie\u00b7het"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Aus schwarzer Erden kalt?", "tokens": ["Aus", "schwar\u00b7zer", "Er\u00b7den", "kalt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "All' Saft und Kraft und Wesen", "tokens": ["All'", "Saft", "und", "Kraft", "und", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nehmt ihr von schlechter Erd',", "tokens": ["Nehmt", "ihr", "von", "schlech\u00b7ter", "Erd'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und doch wer euch geht lesen", "tokens": ["Und", "doch", "wer", "euch", "geht", "le\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PWS", "PPER", "VVFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nichts Zierlichers begehrt.", "tokens": ["Nichts", "Zier\u00b7li\u00b7chers", "be\u00b7gehrt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Die Br\u00fcnnlein sich ergie\u00dfen", "tokens": ["Die", "Br\u00fcnn\u00b7lein", "sich", "er\u00b7gie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und ihre Wasser klar", "tokens": ["Und", "ih\u00b7re", "Was\u00b7ser", "klar"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie Silberstrahlen schie\u00dfen", "tokens": ["Wie", "Sil\u00b7ber\u00b7strah\u00b7len", "schie\u00b7\u00dfen"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vom Felsen offenbar.", "tokens": ["Vom", "Fel\u00b7sen", "of\u00b7fen\u00b7bar", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Die Sonn' es bald erblicket,", "tokens": ["Die", "Sonn'", "es", "bald", "er\u00b7bli\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Drin k\u00fchlet ihren Schein,", "tokens": ["Drin", "k\u00fch\u00b7let", "ih\u00b7ren", "Schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Tier' es auch erquicket,", "tokens": ["Die", "Tier'", "es", "auch", "er\u00b7quic\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So hei\u00df und durstig sein.", "tokens": ["So", "hei\u00df", "und", "durs\u00b7tig", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Frisch hin und her gehn wanken", "tokens": ["Frisch", "hin", "und", "her", "gehn", "wan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "PTKVZ", "KON", "ADV", "VVINF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die klaren B\u00e4chlein krumm;", "tokens": ["Die", "kla\u00b7ren", "B\u00e4ch\u00b7lein", "krumm", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und mit den Steinlein zanken,", "tokens": ["Und", "mit", "den", "Stein\u00b7lein", "zan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn sie sich biegen um.", "tokens": ["Wenn", "sie", "sich", "bie\u00b7gen", "um", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Allweg sie s\u00fc\u00dflich sausen", "tokens": ["All\u00b7weg", "sie", "s\u00fc\u00df\u00b7lich", "sau\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zum Sang und Gang gewohnt,", "tokens": ["Zum", "Sang", "und", "Gang", "ge\u00b7wohnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das ganze Jahr ohn' Pausen", "tokens": ["Das", "gan\u00b7ze", "Jahr", "ohn'", "Pau\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Man h\u00f6ret ihren Ton.", "tokens": ["Man", "h\u00f6\u00b7ret", "ih\u00b7ren", "Ton", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Das wilde Meer nun brauset", "tokens": ["Das", "wil\u00b7de", "Meer", "nun", "brau\u00b7set"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und w\u00fctet ungest\u00fcm;", "tokens": ["Und", "w\u00fc\u00b7tet", "un\u00b7ge\u00b7st\u00fcm", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun still es wieder sauset,", "tokens": ["Nun", "still", "es", "wie\u00b7der", "sau\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Liegt fest in runder Kr\u00fcmm.", "tokens": ["Liegt", "fest", "in", "run\u00b7der", "Kr\u00fcmm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Gar lieblich tut's bestrahlen", "tokens": ["Gar", "lieb\u00b7lich", "tut's", "be\u00b7strah\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "NE", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Sonn' mit sanfter Glut,", "tokens": ["Die", "Sonn'", "mit", "sanf\u00b7ter", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wann sie zu oftermalen", "tokens": ["Wann", "sie", "zu", "of\u00b7ter\u00b7ma\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sich drein erspiegeln tut.", "tokens": ["Sich", "drein", "er\u00b7spie\u00b7geln", "tut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Wer will die B\u00e4um' nun zehlen", "tokens": ["Wer", "will", "die", "B\u00e4um'", "nun", "zeh\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "ART", "NN", "ADV", "VVINF"], "meter": "-+-+-+-", "m