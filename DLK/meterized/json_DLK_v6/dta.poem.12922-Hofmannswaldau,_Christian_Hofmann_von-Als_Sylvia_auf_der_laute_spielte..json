{"dta.poem.12922": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als Sylvia auf der laute spielte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach, Sylvia! h\u00f6r' auf zu spielen,", "tokens": ["Ach", ",", "Syl\u00b7via", "!", "h\u00f6r'", "auf", "zu", "spie\u00b7len", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NE", "$.", "VVFIN", "APPR", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn sonst begehst du einen mord.", "tokens": ["Denn", "sonst", "be\u00b7gehst", "du", "ei\u00b7nen", "mord", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch nein! spiel immer fort,", "tokens": ["Doch", "nein", "!", "spiel", "im\u00b7mer", "fort", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$.", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So kan ich noch mit lust den sto\u00df des todes f\u00fchlen.", "tokens": ["So", "kan", "ich", "noch", "mit", "lust", "den", "sto\u00df", "des", "to\u00b7des", "f\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "VVFIN", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Zwar, kaltes Kind! du lachst;", "tokens": ["Zwar", ",", "kal\u00b7tes", "Kind", "!", "du", "lachst", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJA", "NN", "$.", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Allein hier l\u00e4st es sich nicht schertzen.", "tokens": ["Al\u00b7lein", "hier", "l\u00e4st", "es", "sich", "nicht", "schert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So viel du tremulanten machst,", "tokens": ["So", "viel", "du", "tre\u00b7mu\u00b7lan\u00b7ten", "machst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So viel empfind\u2019 ich stich\u2019 im hertzen.", "tokens": ["So", "viel", "emp\u00b7find'", "ich", "stich'", "im", "hert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}