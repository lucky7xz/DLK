{"textgrid.poem.35057": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Dein Auge", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00fcte dein Auge; bewache es immer,", "tokens": ["H\u00fc\u00b7te", "dein", "Au\u00b7ge", ";", "be\u00b7wa\u00b7che", "es", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$.", "VVFIN", "PPER", "ADV", "$,"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Denn deine Seele, sie zeigt sich darin,", "tokens": ["Denn", "dei\u00b7ne", "See\u00b7le", ",", "sie", "zeigt", "sich", "da\u00b7rin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PRF", "PAV", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sei es in sanftem, erbarmendem Schimmer,", "tokens": ["Sei", "es", "in", "sanf\u00b7tem", ",", "er\u00b7bar\u00b7men\u00b7dem", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Sei es verd\u00fcstert von grollendem Sinn.", "tokens": ["Sei", "es", "ver\u00b7d\u00fcs\u00b7tert", "von", "grol\u00b7len\u00b7dem", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Gutes und B\u00f6ses bereiten die H\u00e4nde;", "tokens": ["Gu\u00b7tes", "und", "B\u00f6\u00b7ses", "be\u00b7rei\u00b7ten", "die", "H\u00e4n\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "Segen und Fluch, sie entquellen dem Mund;", "tokens": ["Se\u00b7gen", "und", "Fluch", ",", "sie", "ent\u00b7quel\u00b7len", "dem", "Mund", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.7": {"text": "Aber durch wundergeheime Verb\u00e4nde", "tokens": ["A\u00b7ber", "durch", "wun\u00b7der\u00b7ge\u00b7hei\u00b7me", "Ver\u00b7b\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.8": {"text": "Thun sie vorher schon im Blicke sich kund.", "tokens": ["Thun", "sie", "vor\u00b7her", "schon", "im", "Bli\u00b7cke", "sich", "kund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "APPRART", "NN", "PRF", "PTKVZ", "$."], "meter": "+---+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "H\u00fcte dein Auge; bewache es immer,", "tokens": ["H\u00fc\u00b7te", "dein", "Au\u00b7ge", ";", "be\u00b7wa\u00b7che", "es", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$.", "VVFIN", "PPER", "ADV", "$,"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Nicht wegen Anderen, sondern f\u00fcr dich.", "tokens": ["Nicht", "we\u00b7gen", "An\u00b7de\u00b7ren", ",", "son\u00b7dern", "f\u00fcr", "dich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ADJA", "$,", "KON", "APPR", "PPER", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "T\u00e4uscht dich sonst Alles, das Auge tr\u00fcgt nimmer,", "tokens": ["T\u00e4uscht", "dich", "sonst", "Al\u00b7les", ",", "das", "Au\u00b7ge", "tr\u00fcgt", "nim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PIS", "$,", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "+-++--+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Denn auch nach innen entschleiert es sich.", "tokens": ["Denn", "auch", "nach", "in\u00b7nen", "ent\u00b7schlei\u00b7ert", "es", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADV", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Pr\u00fcfe dich flei\u00dfig, so wirst du entdecken,", "tokens": ["Pr\u00fc\u00b7fe", "dich", "flei\u00b7\u00dfig", ",", "so", "wirst", "du", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "$,", "ADV", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "Da\u00df jede Regung ins Aug sich verirrt,", "tokens": ["Da\u00df", "je\u00b7de", "Re\u00b7gung", "ins", "Aug", "sich", "ver\u00b7irrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "APPRART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Um dort verr\u00e4th'rische Lichter zu wecken,", "tokens": ["Um", "dort", "ver\u00b7r\u00e4th'\u00b7ri\u00b7sche", "Lich\u00b7ter", "zu", "we\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Ehe zum Worte, zum Werke sie wird.", "tokens": ["E\u00b7he", "zum", "Wor\u00b7te", ",", "zum", "Wer\u00b7ke", "sie", "wird", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "APPRART", "NN", "PPER", "VAFIN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "H\u00fcte dein Auge; bewache es immer,", "tokens": ["H\u00fc\u00b7te", "dein", "Au\u00b7ge", ";", "be\u00b7wa\u00b7che", "es", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$.", "VVFIN", "PPER", "ADV", "$,"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Halte es stetig in sorgender Hut.", "tokens": ["Hal\u00b7te", "es", "ste\u00b7tig", "in", "sor\u00b7gen\u00b7der", "Hut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "F\u00fchlst du im Blick einen gl\u00fchenden Flimmer,", "tokens": ["F\u00fchlst", "du", "im", "Blick", "ei\u00b7nen", "gl\u00fc\u00b7hen\u00b7den", "Flim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Warte und schweig, denn \u2013 \u2013 du bist jetzt nicht gut.", "tokens": ["War\u00b7te", "und", "schweig", ",", "denn", "\u2013", "\u2013", "du", "bist", "jetzt", "nicht", "gut", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$,", "KON", "$(", "$(", "PPER", "VAFIN", "ADV", "PTKNEG", "ADJD", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Warte und schweig, bis ein besseres Regen,", "tokens": ["War\u00b7te", "und", "schweig", ",", "bis", "ein", "bes\u00b7se\u00b7res", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$,", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "+--+-++--+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "Welches die s\u00fcndige Wallung vertrieb,", "tokens": ["Wel\u00b7ches", "die", "s\u00fcn\u00b7di\u00b7ge", "Wal\u00b7lung", "ver\u00b7trieb", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.7": {"text": "Zeit gewann, sich in dein Auge zu legen;", "tokens": ["Zeit", "ge\u00b7wann", ",", "sich", "in", "dein", "Au\u00b7ge", "zu", "le\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PRF", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+--++", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "Dann rede frei, denn \u2013 \u2013 du bist wieder lieb.", "tokens": ["Dann", "re\u00b7de", "frei", ",", "denn", "\u2013", "\u2013", "du", "bist", "wie\u00b7der", "lieb", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "$,", "KON", "$(", "$(", "PPER", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}