{"textgrid.poem.37541": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Silen", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Siehe, da sitzet Silen bei der wohlgebildeten Nymphe.", "tokens": ["Sie\u00b7he", ",", "da", "sit\u00b7zet", "Si\u00b7len", "bei", "der", "wohl\u00b7ge\u00b7bil\u00b7de\u00b7ten", "Nym\u00b7phe", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "ADV", "VVFIN", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Gern entleert er den Krug, was er schon \u00f6fters getan.", "tokens": ["Gern", "ent\u00b7leert", "er", "den", "Krug", ",", "was", "er", "schon", "\u00f6f\u00b7ters", "ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "PWS", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Endlich aber jedoch erklimmt er den n\u00fctzlichen Esel,", "tokens": ["End\u00b7lich", "a\u00b7ber", "je\u00b7doch", "er\u00b7klimmt", "er", "den", "n\u00fctz\u00b7li\u00b7chen", "E\u00b7sel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Wenn auch dieses nicht ganz ohne Beschwerde geschah.", "tokens": ["Wenn", "auch", "die\u00b7ses", "nicht", "ganz", "oh\u00b7ne", "Be\u00b7schwer\u00b7de", "ge\u00b7schah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PDS", "PTKNEG", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Fast vergi\u00dft er den Thyrsus, woran er sein Lebtag gew\u00f6hnt ist;", "tokens": ["Fast", "ver\u00b7gi\u00dft", "er", "den", "Thyr\u00b7sus", ",", "wo\u00b7ran", "er", "sein", "Leb\u00b7tag", "ge\u00b7w\u00f6hnt", "ist", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NE", "$,", "PWAV", "PPER", "PPOSAT", "NN", "VVPP", "VAFIN", "$."], "meter": "+-+---+-+--+--+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "K\u00e4me derselbe ihm weg, w\u00e4r' es ihm schrecklich fatal. \u2013", "tokens": ["K\u00e4\u00b7me", "der\u00b7sel\u00b7be", "ihm", "weg", ",", "w\u00e4r'", "es", "ihm", "schreck\u00b7lich", "fa\u00b7tal", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PDAT", "PPER", "PTKVZ", "$,", "VAFIN", "PPER", "PPER", "ADJD", "ADJD", "$.", "$("], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}}, "stanza.4": {"line.1": {"text": "Also reitet er fort und erhebt auf Kunst keinen Anspruch;", "tokens": ["Al\u00b7so", "rei\u00b7tet", "er", "fort", "und", "er\u00b7hebt", "auf", "Kunst", "kei\u00b7nen", "An\u00b7spruch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "APPR", "NN", "PIAT", "NN", "$."], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Bald mal sitzet er so, bald auch wieder mal so.", "tokens": ["Bald", "mal", "sit\u00b7zet", "er", "so", ",", "bald", "auch", "wie\u00b7der", "mal", "so", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV", "$,", "ADV", "ADV", "ADV", "ADV", "ADV", "$."], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}}, "stanza.5": {"line.1": {"text": "Horch, wer fl\u00f6tet denn da? Nat\u00fcrlich, Amor der Lausbub;", "tokens": ["Horch", ",", "wer", "fl\u00f6\u00b7tet", "denn", "da", "?", "Na\u00b7t\u00fcr\u00b7lich", ",", "A\u00b7mor", "der", "Laus\u00b7bub", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "VVFIN", "ADV", "ADV", "$.", "ADV", "$,", "NE", "ART", "NN", "$."], "meter": "+-+-+--+-+---+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Aber der Esel erhebt \u00e4u\u00dferst bedenklich das Ohr.", "tokens": ["A\u00b7ber", "der", "E\u00b7sel", "er\u00b7hebt", "\u00e4u\u00b7\u00dferst", "be\u00b7denk\u00b7lich", "das", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "ADJD", "ART", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Schlimmer als Fl\u00f6tenget\u00f6n ist das lautlos wirkende Pustrohr;", "tokens": ["Schlim\u00b7mer", "als", "Fl\u00f6\u00b7ten\u00b7ge\u00b7t\u00f6n", "ist", "das", "laut\u00b7los", "wir\u00b7ken\u00b7de", "Pust\u00b7rohr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "NN", "VAFIN", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Pustet man hinten, so fliegt vorne was Spitzes heraus.", "tokens": ["Pus\u00b7tet", "man", "hin\u00b7ten", ",", "so", "fliegt", "vor\u00b7ne", "was", "Spit\u00b7zes", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "$,", "ADV", "VVFIN", "ADV", "PWS", "NN", "PTKVZ", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Ungern empfindet den Schmerz das redlich dienende Lasttier;", "tokens": ["Un\u00b7gern", "emp\u00b7fin\u00b7det", "den", "Schmerz", "das", "red\u00b7lich", "die\u00b7nen\u00b7de", "Last\u00b7tier", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Aber der Reiter hat auch manche Geschichten nicht gern.", "tokens": ["A\u00b7ber", "der", "Rei\u00b7ter", "hat", "auch", "man\u00b7che", "Ge\u00b7schich\u00b7ten", "nicht", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "PIAT", "NN", "PTKNEG", "ADV", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}}, "stanza.8": {"line.1": {"text": "Leicht erwischt man den Vogel durch List und schlaue Beschleichung;", "tokens": ["Leicht", "er\u00b7wischt", "man", "den", "Vo\u00b7gel", "durch", "List", "und", "schlau\u00b7e", "Be\u00b7schlei\u00b7chung", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PIS", "ART", "NN", "APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}}, "stanza.9": {"line.1": {"text": "Wenn er es aber bemerkt, flieget er meistens davon.", "tokens": ["Wenn", "er", "es", "a\u00b7ber", "be\u00b7merkt", ",", "flie\u00b7get", "er", "meis\u00b7tens", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVPP", "$,", "VVFIN", "PPER", "ADV", "PAV", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "Mancher erreichet den Zweck durch t\u00e4uschend ge\u00fcbte Verstellung;", "tokens": ["Man\u00b7cher", "er\u00b7rei\u00b7chet", "den", "Zweck", "durch", "t\u00e4u\u00b7schend", "ge\u00b7\u00fcb\u00b7te", "Ver\u00b7stel\u00b7lung", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "APPR", "ADJD", "ADJA", "NN", "$."], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Scheinbar schlummert der Leib, aber die Seele ist wach.", "tokens": ["Schein\u00b7bar", "schlum\u00b7mert", "der", "Leib", ",", "a\u00b7ber", "die", "See\u00b7le", "ist", "wach", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,", "ADV", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.11": {"line.1": {"text": "Schnupp! Er hat ihn erwischt. Laut kreischt der l\u00e4stige Vogel,", "tokens": ["Schnupp", "!", "Er", "hat", "ihn", "er\u00b7wischt", ".", "Laut", "kreischt", "der", "l\u00e4s\u00b7ti\u00b7ge", "Vo\u00b7gel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VAFIN", "PPER", "VVPP", "$.", "APPR", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--++--+--+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "W\u00e4hrend der handliche Stab t\u00f6nend die Backe ber\u00fchrt.", "tokens": ["W\u00e4h\u00b7rend", "der", "hand\u00b7li\u00b7che", "Stab", "t\u00f6\u00b7nend", "die", "Ba\u00b7cke", "be\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}}, "stanza.12": {"line.1": {"text": "\u00dcbel wird es vermerkt, entrupft man dem Vogel die Feder;", "tokens": ["\u00dc\u00b7bel", "wird", "es", "ver\u00b7merkt", ",", "en\u00b7trupft", "man", "dem", "Vo\u00b7gel", "die", "Fe\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$,", "VVFIN", "PIS", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Erstens scheint sie ihm sch\u00f6n, zweitens gebraucht er sie auch.", "tokens": ["Ers\u00b7tens", "scheint", "sie", "ihm", "sch\u00f6n", ",", "zwei\u00b7tens", "ge\u00b7braucht", "er", "sie", "auch", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADJD", "$,", "ADV", "VVFIN", "PPER", "PPER", "ADV", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}}, "stanza.13": {"line.1": {"text": "Heimw\u00e4rts reitet Silen und spielt auf der lieblichen Fl\u00f6te,", "tokens": ["Heim\u00b7w\u00e4rts", "rei\u00b7tet", "Si\u00b7len", "und", "spielt", "auf", "der", "lieb\u00b7li\u00b7chen", "Fl\u00f6\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Freilich verschiedenerlei, aber doch meistens d\u00fcdell\u00fctt!", "tokens": ["Frei\u00b7lich", "ver\u00b7schie\u00b7de\u00b7ner\u00b7lei", ",", "a\u00b7ber", "doch", "meis\u00b7tens", "d\u00fc\u00b7del\u00b7l\u00fctt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "ADV", "ADV", "ADV", "ADJD", "$."], "meter": "+--+--+---+-+-+", "measure": "dactylic.tri.plus"}}}}}