{"textgrid.poem.59312": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Geniale", "genre": "verse", "period": "N.A.", "pub_year": 1811, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lustig auf den Kopf, mein Liebchen,", "tokens": ["Lus\u00b7tig", "auf", "den", "Kopf", ",", "mein", "Lieb\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stell dich, in die Luft die Bein!", "tokens": ["Stell", "dich", ",", "in", "die", "Luft", "die", "Bein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Heisa! ich will sein dein B\u00fcbchen,", "tokens": ["Hei\u00b7sa", "!", "ich", "will", "sein", "dein", "B\u00fcb\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VMFIN", "VAINF", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Heute nacht soll Hochzeit sein!", "tokens": ["Heu\u00b7te", "nacht", "soll", "Hoch\u00b7zeit", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VMFIN", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn du Shakespeare kannst vertragen,", "tokens": ["Wenn", "du", "Sha\u00b7ke\u00b7spe\u00b7a\u00b7re", "kannst", "ver\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "VMFIN", "VVFIN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "O du liebe Unschuld du!", "tokens": ["O", "du", "lie\u00b7be", "Un\u00b7schuld", "du", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wirst du mich wohl auch ertragen", "tokens": ["Wirst", "du", "mich", "wohl", "auch", "er\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PRF", "ADV", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und noch jedermann dazu. \u2013", "tokens": ["Und", "noch", "je\u00b7der\u00b7mann", "da\u00b7zu", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "PIS", "PAV", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}