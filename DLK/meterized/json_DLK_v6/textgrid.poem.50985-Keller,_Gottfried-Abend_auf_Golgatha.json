{"textgrid.poem.50985": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Abend auf Golgatha", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eben die dornige Krone geneiget, verschied der Erl\u00f6ser,", "tokens": ["E\u00b7ben", "die", "dor\u00b7ni\u00b7ge", "Kro\u00b7ne", "ge\u00b7nei\u00b7get", ",", "ver\u00b7schied", "der", "Er\u00b7l\u00f6\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVPP", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Wei\u00dflich in d\u00e4mmernder Luft gl\u00e4nzte die Schulter des Herrn?", "tokens": ["Wei\u00df\u00b7lich", "in", "d\u00e4m\u00b7mern\u00b7der", "Luft", "gl\u00e4nz\u00b7te", "die", "Schul\u00b7ter", "des", "Herrn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Siehe, da schwebte, vom tauigen Schimmer gelockt, die Phal\u00e4ne", "tokens": ["Sie\u00b7he", ",", "da", "schweb\u00b7te", ",", "vom", "tau\u00b7i\u00b7gen", "Schim\u00b7mer", "ge\u00b7lockt", ",", "die", "Pha\u00b7l\u00e4\u00b7ne"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "$,", "ADV", "VVFIN", "$,", "APPRART", "ADJA", "NN", "VVPP", "$,", "ART", "NN"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Flatternd hernieder zu ruhn dort, wo gelastet das Kreuz.", "tokens": ["Flat\u00b7ternd", "her\u00b7nie\u00b7der", "zu", "ruhn", "dort", ",", "wo", "ge\u00b7las\u00b7tet", "das", "Kreuz", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "PTKZU", "VVINF", "ADV", "$,", "PWAV", "VVPP", "ART", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Langsam schlug sie ein Weilchen die samtenen Fl\u00fcgel zusammen,", "tokens": ["Lang\u00b7sam", "schlug", "sie", "ein", "Weil\u00b7chen", "die", "sam\u00b7te\u00b7nen", "Fl\u00fc\u00b7gel", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Breitet' sie aus und entschwand fern in die sinkende Nacht.", "tokens": ["Brei\u00b7tet'", "sie", "aus", "und", "ent\u00b7schwand", "fern", "in", "die", "sin\u00b7ken\u00b7de", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Nicht ganz blieb verlassen ihr Sch\u00f6pfer: den Pfeiler des Kreuzes", "tokens": ["Nicht", "ganz", "blieb", "ver\u00b7las\u00b7sen", "ihr", "Sch\u00f6p\u00b7fer", ":", "den", "Pfei\u00b7ler", "des", "Kreu\u00b7zes"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "VVFIN", "VVINF", "PPOSAT", "NN", "$.", "ART", "NN", "ART", "NN"], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.8": {"text": "Hielt umfangen das Weib, das er zur Mutter sich schuf.", "tokens": ["Hielt", "um\u00b7fan\u00b7gen", "das", "Weib", ",", "das", "er", "zur", "Mut\u00b7ter", "sich", "schuf", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "PRF", "VVFIN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}}}}}