{"textgrid.poem.54796": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "10. An Herrn Daniel Blanck bey selbter Gelegenheit", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr die der rawe Grim der ungeheuren Schar/", "tokens": ["Ihr", "die", "der", "ra\u00b7we", "Grim", "der", "un\u00b7ge\u00b7heu\u00b7ren", "Schar", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Seuchen hart erschreckt/ die die beschw\u00e4rtzte Bahr/", "tokens": ["Der", "Seu\u00b7chen", "hart", "er\u00b7schreckt", "/", "die", "die", "be\u00b7schw\u00e4rtz\u00b7te", "Bahr", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$(", "PRELS", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Trauerkleid und Grufft/ die der Tyran betr\u00fcbet/", "tokens": ["Die", "Trau\u00b7er\u00b7kleid", "und", "Grufft", "/", "die", "der", "Ty\u00b7ran", "be\u00b7tr\u00fc\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$(", "PRELS", "ART", "NE", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der t\u00e4glich seinen Trotz an Menschen Fleisch ver\u00fcbet:", "tokens": ["Der", "t\u00e4g\u00b7lich", "sei\u00b7nen", "Trotz", "an", "Men\u00b7schen", "Fleisch", "ver\u00b7\u00fc\u00b7bet", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPOSAT", "NN", "APPR", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Stelt heut ewr Trawren ein weil Blanckens weiser Geist/", "tokens": ["Stelt", "heut", "ewr", "Traw\u00b7ren", "ein", "weil", "Blan\u00b7ckens", "wei\u00b7ser", "Geist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJA", "NN", "ART", "KOUS", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was schwartz und unklar ist/ durch Kunst verschwinden heist.", "tokens": ["Was", "schwartz", "und", "un\u00b7klar", "ist", "/", "durch", "Kunst", "ver\u00b7schwin\u00b7den", "heist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "KON", "ADJD", "VAFIN", "$(", "APPR", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}