{"textgrid.poem.57781": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Kurz ist der Mai", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herzblatt am Lindenbaum,", "tokens": ["Herz\u00b7blatt", "am", "Lin\u00b7den\u00b7baum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Du gr\u00fcner Maientraum,", "tokens": ["Du", "gr\u00fc\u00b7ner", "Mai\u00b7ent\u00b7raum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es sang die Nachtigall", "tokens": ["Es", "sang", "die", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ihren s\u00fc\u00dfen Schall;", "tokens": ["Ih\u00b7ren", "s\u00fc\u00b7\u00dfen", "Schall", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Sang Liebe, sang Leide,", "tokens": ["Sang", "Lie\u00b7be", ",", "sang", "Lei\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VVFIN", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Sang Freud und sang Leid,", "tokens": ["Sang", "Freud", "und", "sang", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "ADJD", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Lang ist das Leben,", "tokens": ["Lang", "ist", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Aber kurz die Maienzeit.", "tokens": ["A\u00b7ber", "kurz", "die", "Mai\u00b7en\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sch\u00f6ne Zeit ist l\u00e4ngst vorbei", "tokens": ["Sch\u00f6\u00b7ne", "Zeit", "ist", "l\u00e4ngst", "vor\u00b7bei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VAFIN", "ADV", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Welk ist der gr\u00fcne Mai,", "tokens": ["Welk", "ist", "der", "gr\u00fc\u00b7ne", "Mai", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nachtigall singt nicht mehr,", "tokens": ["Nach\u00b7ti\u00b7gall", "singt", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKNEG", "ADV", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Der Lindenbaum steht leer;", "tokens": ["Der", "Lin\u00b7den\u00b7baum", "steht", "leer", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Aus Liebe ward Leide,", "tokens": ["Aus", "Lie\u00b7be", "ward", "Lei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Aus Liebe ward Leid,", "tokens": ["Aus", "Lie\u00b7be", "ward", "Leid", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Lang ist das Leben,", "tokens": ["Lang", "ist", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Aber kurz die Maienzeit.", "tokens": ["A\u00b7ber", "kurz", "die", "Mai\u00b7en\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Will in den Garten gehn,", "tokens": ["Will", "in", "den", "Gar\u00b7ten", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Wo die letzten Rosen stehn,", "tokens": ["Wo", "die", "letz\u00b7ten", "Ro\u00b7sen", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber o weh, o weh,", "tokens": ["A\u00b7ber", "o", "weh", ",", "o", "weh", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "FM", "PTKVZ", "$,", "FM", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Da liegt der Schnee;", "tokens": ["Da", "liegt", "der", "Schnee", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Schnee der tut wehe,", "tokens": ["Schnee", "der", "tut", "we\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Schnee der bringt Leid,", "tokens": ["Schnee", "der", "bringt", "Leid", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "VVFIN", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Lang ist das Leben,", "tokens": ["Lang", "ist", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Aber kurz die Maienzeit.", "tokens": ["A\u00b7ber", "kurz", "die", "Mai\u00b7en\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}