{"dta.poem.12056": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "76.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Je gr\u00f6\u00dfer einen Kreis du hast zu \u00fcbersehn,", "tokens": ["Je", "gr\u00f6\u00b7\u00dfer", "ei\u00b7nen", "Kreis", "du", "hast", "zu", "\u00fc\u00b7ber\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "PPER", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Je minder kann dein Blick in alles Einzle gehn.", "tokens": ["Je", "min\u00b7der", "kann", "dein", "Blick", "in", "al\u00b7les", "Einz\u00b7le", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPOSAT", "NN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein Menschenk\u00f6nig und die K\u00f6n'gin der Gedanken,", "tokens": ["Ein", "Men\u00b7schen\u00b7k\u00f6\u00b7nig", "und", "die", "K\u00f6n'\u00b7gin", "der", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Sich w\u00e4hnend unumschr\u00e4nkt, erkennen diese Schranken.", "tokens": ["Sich", "w\u00e4h\u00b7nend", "un\u00b7um\u00b7schr\u00e4nkt", ",", "er\u00b7ken\u00b7nen", "die\u00b7se", "Schran\u00b7ken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ADJD", "$,", "VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sie nicht, die unteren Organe sehn das Kleinste,", "tokens": ["Sie", "nicht", ",", "die", "un\u00b7te\u00b7ren", "Or\u00b7ga\u00b7ne", "sehn", "das", "Kleins\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "$,", "ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die mittlern Mittleres, sie erst das Allgemeinste.", "tokens": ["Die", "mitt\u00b7lern", "Mitt\u00b7le\u00b7res", ",", "sie", "erst", "das", "All\u00b7ge\u00b7meins\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "ADV", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und was von untenauf man mittelbar vernimmt,", "tokens": ["Und", "was", "von", "un\u00b7ten\u00b7auf", "man", "mit\u00b7tel\u00b7bar", "ver\u00b7nimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "APPR", "PIS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird auch von obenher nur mittelbar bestimmt.", "tokens": ["Wird", "auch", "von", "o\u00b7ben\u00b7her", "nur", "mit\u00b7tel\u00b7bar", "be\u00b7stimmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Nur Gott ist die Vernunft, die keine Schrank' umzieht,", "tokens": ["Nur", "Gott", "ist", "die", "Ver\u00b7nunft", ",", "die", "kei\u00b7ne", "Schrank'", "um\u00b7zieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "ART", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die selbst unmittelbar ins Einzle Alles sieht.", "tokens": ["Die", "selbst", "un\u00b7mit\u00b7tel\u00b7bar", "ins", "Einz\u00b7le", "Al\u00b7les", "sieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "APPRART", "NN", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}