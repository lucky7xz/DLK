{"textgrid.poem.59662": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Der gr\u00fcne Wald", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O der s\u00fc\u00dfe, gr\u00fcne Wald,", "tokens": ["O", "der", "s\u00fc\u00b7\u00dfe", ",", "gr\u00fc\u00b7ne", "Wald", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo wir einst in Wonne klangen,", "tokens": ["Wo", "wir", "einst", "in", "Won\u00b7ne", "klan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo wir spielten, wo wir sangen,", "tokens": ["Wo", "wir", "spiel\u00b7ten", ",", "wo", "wir", "san\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo wir tanzten Maient\u00e4nze,", "tokens": ["Wo", "wir", "tanz\u00b7ten", "Mai\u00b7en\u00b7t\u00e4n\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo wir pfl\u00fcckten Maienkr\u00e4nze,", "tokens": ["Wo", "wir", "pfl\u00fcck\u00b7ten", "Mai\u00b7en\u00b7kr\u00e4n\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "O der s\u00fc\u00dfe, gr\u00fcne Wald!", "tokens": ["O", "der", "s\u00fc\u00b7\u00dfe", ",", "gr\u00fc\u00b7ne", "Wald", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie er immer widerhallt,", "tokens": ["Wie", "er", "im\u00b7mer", "wi\u00b7der\u00b7hallt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie er schallt:", "tokens": ["Wie", "er", "schallt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Schalle nur, du gr\u00fcner Wald,", "tokens": ["Schal\u00b7le", "nur", ",", "du", "gr\u00fc\u00b7ner", "Wald", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Rufe immer deinem Frommen,", "tokens": ["Ru\u00b7fe", "im\u00b7mer", "dei\u00b7nem", "From\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach! Er kann nicht wiederkommen!", "tokens": ["Ach", "!", "Er", "kann", "nicht", "wie\u00b7der\u00b7kom\u00b7men", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bl\u00fchet, Blumen, fl\u00fcstert, Bl\u00e4tter,", "tokens": ["Bl\u00fc\u00b7het", ",", "Blu\u00b7men", ",", "fl\u00fcs\u00b7tert", ",", "Bl\u00e4t\u00b7ter", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Klinget, V\u00f6glein, das Geschmetter", "tokens": ["Klin\u00b7get", ",", "V\u00f6\u00b7glein", ",", "das", "Ge\u00b7schmet\u00b7ter"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "NN", "$,", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eures Lenzes durch den Wald \u2013", "tokens": ["Eu\u00b7res", "Len\u00b7zes", "durch", "den", "Wald", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Bleich ist eure Lichtgestalt,", "tokens": ["Bleich", "ist", "eu\u00b7re", "Licht\u00b7ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Stumm und kalt \u2013", "tokens": ["Stumm", "und", "kalt", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$("], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "O du s\u00fc\u00dfer, gr\u00fcner Wald!", "tokens": ["O", "du", "s\u00fc\u00b7\u00dfer", ",", "gr\u00fc\u00b7ner", "Wald", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo wir nun in leisen Tr\u00e4nen", "tokens": ["Wo", "wir", "nun", "in", "lei\u00b7sen", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Uns nach unserm Liebling sehnen,", "tokens": ["Uns", "nach", "un\u00b7serm", "Lieb\u00b7ling", "seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nimmermehr im frischen Maien", "tokens": ["Nim\u00b7mer\u00b7mehr", "im", "fri\u00b7schen", "Mai\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit der jungen Lust juchheien \u2013", "tokens": ["Mit", "der", "jun\u00b7gen", "Lust", "juch\u00b7hei\u00b7en", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NE", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Rufe ewig, gr\u00fcner Wald,", "tokens": ["Ru\u00b7fe", "e\u00b7wig", ",", "gr\u00fc\u00b7ner", "Wald", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Mit der Liebe Allgewalt,", "tokens": ["Mit", "der", "Lie\u00b7be", "All\u00b7ge\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df es schallt:", "tokens": ["Da\u00df", "es", "schallt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}