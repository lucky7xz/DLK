{"textgrid.poem.37452": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Entr\u00fcstet", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu gr\u00e4\u00dflich hatt' er mich geneckt.", "tokens": ["Zu", "gr\u00e4\u00df\u00b7lich", "hatt'", "er", "mich", "ge\u00b7neckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VAFIN", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie weh war mir zu Sinn.", "tokens": ["Wie", "weh", "war", "mir", "zu", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und tief gekr\u00e4nkt und aufgeschreckt", "tokens": ["Und", "tief", "ge\u00b7kr\u00e4nkt", "und", "auf\u00b7ge\u00b7schreckt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVPP", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Kirchhof lief ich hin.", "tokens": ["Zum", "Kirch\u00b7hof", "lief", "ich", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich sa\u00df auf einem Leichenstein,", "tokens": ["Ich", "sa\u00df", "auf", "ei\u00b7nem", "Lei\u00b7chen\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Augen weint ich rot.", "tokens": ["Die", "Au\u00b7gen", "weint", "ich", "rot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ach lieber Gott, erbarm dich mein", "tokens": ["Ach", "lie\u00b7ber", "Gott", ",", "er\u00b7barm", "dich", "mein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "ADV", "NN", "$,", "ADJD", "PPER", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und mach mich endlich tot.", "tokens": ["Und", "mach", "mich", "end\u00b7lich", "tot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Sieht er mich dann in meinem Sarg,", "tokens": ["Sieht", "er", "mich", "dann", "in", "mei\u00b7nem", "Sarg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "So wird er lebenssatt", "tokens": ["So", "wird", "er", "le\u00b7bens\u00b7satt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Und stirbt vor Gram, weil er so arg", "tokens": ["Und", "stirbt", "vor", "Gram", ",", "weil", "er", "so", "arg"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "$,", "KOUS", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Mein Herz behandelt hat.", "tokens": ["Mein", "Herz", "be\u00b7han\u00b7delt", "hat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Kaum war's gesagt, so legten sich", "tokens": ["Kaum", "wa\u00b7r's", "ge\u00b7sagt", ",", "so", "leg\u00b7ten", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "VVPP", "$,", "ADV", "VVFIN", "PRF"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Zwei Arme um mich her,", "tokens": ["Zwei", "Ar\u00b7me", "um", "mich", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Und auf der Stelle f\u00fchlte ich,", "tokens": ["Und", "auf", "der", "Stel\u00b7le", "f\u00fchl\u00b7te", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Wer das getan, war er.", "tokens": ["Wer", "das", "ge\u00b7tan", ",", "war", "er", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PDS", "VVPP", "$,", "VAFIN", "PPER", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.17": {"text": "Wir kehrten Arm in Arm zur\u00fcck.", "tokens": ["Wir", "kehr\u00b7ten", "Arm", "in", "Arm", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Ich sah ihn an bei Licht.", "tokens": ["Ich", "sah", "ihn", "an", "bei", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Nein, solchen treuen Liebesblick", "tokens": ["Nein", ",", "sol\u00b7chen", "treu\u00b7en", "Lie\u00b7bes\u00b7blick"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Hat doch kein B\u00f6sewicht.", "tokens": ["Hat", "doch", "kein", "B\u00f6\u00b7se\u00b7wicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}