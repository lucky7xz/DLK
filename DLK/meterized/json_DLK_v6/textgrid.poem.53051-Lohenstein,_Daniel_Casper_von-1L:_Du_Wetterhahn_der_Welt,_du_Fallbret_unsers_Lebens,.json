sal dienen.", "tokens": ["Wo\u00b7f\u00fcr", "ihr", "Ho\u00b7nig", "uns", "doch", "mu\u00df", "zum", "Lab\u00b7sal", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "ADV", "VMFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Der Wind ist nicht so sehr ver\u00e4nderlich im M\u00e4rzen,", "tokens": ["Der", "Wind", "ist", "nicht", "so", "sehr", "ver\u00b7\u00e4n\u00b7der\u00b7lich", "im", "M\u00e4r\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADV", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als sich des Hofes Gunst, des Gl\u00fcckes West verkehrt.", "tokens": ["Als", "sich", "des", "Ho\u00b7fes", "Gunst", ",", "des", "Gl\u00fc\u00b7ckes", "West", "ver\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "NN", "$,", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn er mit Zweigen scheint zu spielen und zu scherzen,", "tokens": ["Wenn", "er", "mit", "Zwei\u00b7gen", "scheint", "zu", "spie\u00b7len", "und", "zu", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sieht man, da\u00df er, wie Blitz', in Cedernst\u00e4mme f\u00e4hrt.", "tokens": ["Sieht", "man", ",", "da\u00df", "er", ",", "wie", "Blitz'", ",", "in", "Ce\u00b7dern\u00b7st\u00e4m\u00b7me", "f\