info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRELAT", "NN", "ADV", "ADV", "APPR", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein H\u00f6fling aber liegt an Seel' und Geist umschr\u00e4nket.", "tokens": ["Ein", "H\u00f6f\u00b7ling", "a\u00b7ber", "liegt", "an", "Seel'", "und", "Geist", "um\u00b7schr\u00e4n\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.16": {"line.1": {"text": "Bei uns ist Jedermann sein Herr, sein F\u00fcrst, sein K\u00f6nig;", "tokens": ["Bei", "uns", "ist", "Je\u00b7der\u00b7mann", "sein", "Herr", ",", "sein", "F\u00fcrst", ",", "sein", "K\u00f6\u00b7nig", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PIS", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Man dringet uns kein Joch, auch wir Niemandem auf;", "tokens": ["Man", "drin\u00b7get", "uns", "kein", "Joch", ",", "auch", "wir", "Nie\u00b7man\u00b7dem", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PIAT", "NN", "$,", "ADV", "PPER", "PIS", "PTKVZ", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Jedweder ist vergn\u00fcgt und Keiner uns zu wenig;", "tokens": ["Jed\u00b7we\u00b7der", "ist", "ver\u00b7gn\u00fcgt", "und", "Kei\u00b7ner", "uns", "zu", "we\u00b7nig", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "VVPP", "KON", "PIS", "PPER", "PTKA", "PIS", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Tugend lassen wir den Preis, der Zeit den Lauf.", "tokens": ["Der", "Tu\u00b7gend", "las\u00b7sen", "wir", "den", "Preis", ",", "der", "Zeit", "den", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wer f\u00fcr's gemeine Heil will Schwei\u00df und Witz verwenden,", "tokens": ["Wer", "f\u00fcr's", "ge\u00b7mei\u00b7ne", "Heil", "will", "Schwei\u00df", "und", "Witz", "ver\u00b7wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "ADJA", "NN", "VMFIN", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dem hilft man selbst an's Bret und tr\u00e4gt ihn auf den H\u00e4nden.", "tokens": ["Dem", "hilft", "man", "selbst", "an's", "Bret", "und", "tr\u00e4gt", "ihn", "auf", "den", "H\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "ADV", "APPRART", "NN", "KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.17": {"line.1": {"text": "Dort tobet Gl\u00fcck und Neid auch auf die Ehrenmale,", "tokens": ["Dort", "to\u00b7bet", "Gl\u00fcck", "und", "Neid", "auch", "auf", "die", "Eh\u00b7ren\u00b7ma\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zermalmt Erz und Porphyr, wirft Bilder in den Schach.", "tokens": ["Zer\u00b7malmt", "Erz", "und", "Por\u00b7phyr", ",", "wirft", "Bil\u00b7der", "in", "den", "Schach", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$,", "VVFIN", "NN", "APPR", "ART", "NN", "$."], "meter": "-++-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der Schutzherr gestern war, der steckt heut' auf dem Pfahle,", "tokens": ["Der", "Schutz\u00b7herr", "ge\u00b7stern", "war", ",", "der", "steckt", "heut'", "auf", "dem", "Pfah\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "$,", "PRELS", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Der ihn j\u00fcngst segnete, ruft jetzt \u00bbdu Schelm!\u00ab ihm nach.", "tokens": ["Der", "ihn", "j\u00fcngst", "seg\u00b7ne\u00b7te", ",", "ruft", "jetzt", "\u00bb", "du", "Schelm", "!", "\u00ab", "ihm", "nach", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "$,", "VVFIN", "ADV", "$(", "PPER", "NN", "$.", "$(", "PPER", "PTKVZ", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Hier wei\u00df man Nichts von Fluch, der Andrer Ruhm versehre;", "tokens": ["Hier", "wei\u00df", "man", "Nichts", "von", "Fluch", ",", "der", "A\u00b7ndrer", "Ruhm", "ver\u00b7seh\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PIS", "APPR", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Andacht klimmt zu Gott, die Tugend strebt nach Ehre.", "tokens": ["Die", "An\u00b7dacht", "klimmt", "zu", "Gott", ",", "die", "Tu\u00b7gend", "strebt", "nach", "Eh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,", "ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}