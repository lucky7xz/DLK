{"textgrid.poem.53051": {"metadata": {"author": {"name": "Lohenstein, Daniel Casper von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du Wetterhahn der Welt, du Fallbret unsers Lebens,", "genre": "verse", "period": "N.A.", "pub_year": 1659, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Wetterhahn der Welt, du Fallbret unsers Lebens,", "tokens": ["Du", "Wet\u00b7ter\u00b7hahn", "der", "Welt", ",", "du", "Fall\u00b7bret", "un\u00b7sers", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du Gaukelspiel der Zeit, o Gl\u00fccke, gute Nacht!", "tokens": ["Du", "Gau\u00b7kel\u00b7spiel", "der", "Zeit", ",", "o", "Gl\u00fc\u00b7cke", ",", "gu\u00b7te", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$,", "FM", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Menschen z\u00fcnden dir den Weihrauch an vergebens;", "tokens": ["Die", "Men\u00b7schen", "z\u00fcn\u00b7den", "dir", "den", "Weih\u00b7rauch", "an", "ver\u00b7ge\u00b7bens", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dein taubes Ohr giebt nie auf Wunsch und Andacht Acht.", "tokens": ["Dein", "tau\u00b7bes", "Ohr", "giebt", "nie", "auf", "Wunsch", "und", "An\u00b7dacht", "Acht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "CARD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wenn du einmal dein Rad, wir eine Hand umdrehen,", "tokens": ["Wenn", "du", "ein\u00b7mal", "dein", "Rad", ",", "wir", "ei\u00b7ne", "Hand", "um\u00b7dre\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "$,", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sehn wir Colossen fall'n und schweres Erz verwehen.", "tokens": ["Sehn", "wir", "Co\u00b7los\u00b7sen", "fall'n", "und", "schwe\u00b7res", "Erz", "ver\u00b7we\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "VVINF", "KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich aber sch\u00e4tze dich weit \u00fcber Ganges Sch\u00e4tze,", "tokens": ["Ich", "a\u00b7ber", "sch\u00e4t\u00b7ze", "dich", "weit", "\u00fc\u00b7ber", "Gan\u00b7ges", "Sch\u00e4t\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du ird'sches Paradies, du Hafen s\u00fc\u00dfer Ruh',", "tokens": ["Du", "ird'\u00b7sches", "Pa\u00b7ra\u00b7dies", ",", "du", "Ha\u00b7fen", "s\u00fc\u00b7\u00dfer", "Ruh'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PPER", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil hier kein W\u00fctherich giebt knechtische Gesetze,", "tokens": ["Weil", "hier", "kein", "W\u00fct\u00b7he\u00b7rich", "giebt", "knech\u00b7ti\u00b7sche", "Ge\u00b7set\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIAT", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil die Natur uns hier l\u00e4\u00dft allen Willen zu,", "tokens": ["Weil", "die", "Na\u00b7tur", "uns", "hier", "l\u00e4\u00dft", "al\u00b7len", "Wil\u00b7len", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "ADV", "VVFIN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wo die Begierde nie aus dem Geschirre schl\u00e4get,", "tokens": ["Wo", "die", "Be\u00b7gier\u00b7de", "nie", "aus", "dem", "Ge\u00b7schir\u00b7re", "schl\u00e4\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vergn\u00fcgung und Vernunft sich in ein Bette leget.", "tokens": ["Ver\u00b7gn\u00fc\u00b7gung", "und", "Ver\u00b7nunft", "sich", "in", "ein", "Bet\u00b7te", "le\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Verdammter Heirathsschlu\u00df! unselige Verm\u00e4hlung!", "tokens": ["Ver\u00b7damm\u00b7ter", "Hei\u00b7raths\u00b7schlu\u00df", "!", "un\u00b7se\u00b7li\u00b7ge", "Ver\u00b7m\u00e4h\u00b7lung", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo Geiz ein g\u00fclden Nichts bebr\u00fctet Tag und Nacht,", "tokens": ["Wo", "Geiz", "ein", "g\u00fcl\u00b7den", "Nichts", "be\u00b7br\u00fc\u00b7tet", "Tag", "und", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "ADJA", "NN", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo der sonst todte Schatz nur lebt zu unsrer Qu\u00e4lung,", "tokens": ["Wo", "der", "sonst", "tod\u00b7te", "Schatz", "nur", "lebt", "zu", "uns\u00b7rer", "Qu\u00e4\u00b7lung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADV", "ADJA", "NN", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wo Ueberflu\u00df uns arm und uners\u00e4ttlich macht,", "tokens": ["Wo", "Ue\u00b7berf\u00b7lu\u00df", "uns", "arm", "und", "un\u00b7er\u00b7s\u00e4tt\u00b7lich", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPER", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wo wir, wie Tantalus, beim Reichthum Hunger leiden,", "tokens": ["Wo", "wir", ",", "wie", "Tan\u00b7ta\u00b7lus", ",", "beim", "Reicht\u00b7hum", "Hun\u00b7ger", "lei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "PWAV", "NE", "$,", "APPRART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Des Nachbars dicke Saat und fettes Euter neiden.", "tokens": ["Des", "Nach\u00b7bars", "di\u00b7cke", "Saat", "und", "fet\u00b7tes", "Eu\u00b7ter", "nei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die g\u00fcldnen Berge sind kein Merkmal g\u00fcldner L\u00e4nder;", "tokens": ["Die", "g\u00fcld\u00b7nen", "Ber\u00b7ge", "sind", "kein", "Merk\u00b7mal", "g\u00fcld\u00b7ner", "L\u00e4n\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PIAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo Gold in Fl\u00fcssen schwimmt, da rinnt auch Ueppigkeit;", "tokens": ["Wo", "Gold", "in", "Fl\u00fcs\u00b7sen", "schwimmt", ",", "da", "rinnt", "auch", "Uep\u00b7pig\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "VVFIN", "$,", "ADV", "VVFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dies Erz heckt aus den Geiz, der Geiz gebiert Verschwender;", "tokens": ["Dies", "Erz", "heckt", "aus", "den", "Geiz", ",", "der", "Geiz", "ge\u00b7biert", "Ver\u00b7schwen\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VVFIN", "APPR", "ART", "NN", "$,", "ART", "NN", "VVPP", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wo man das Gold nicht kennt, da ist die g\u00fcldne Zeit,", "tokens": ["Wo", "man", "das", "Gold", "nicht", "kennt", ",", "da", "ist", "die", "g\u00fcld\u00b7ne", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "PTKNEG", "VVFIN", "$,", "ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und da die eiserne, wo man sch\u00e4rft Stahl und Degen", "tokens": ["Und", "da", "die", "ei\u00b7ser\u00b7ne", ",", "wo", "man", "sch\u00e4rft", "Stahl", "und", "De\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "$,", "PWAV", "PIS", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Und nicht das Eisen schmelzt zu Pflugschaar und zu Eggen.", "tokens": ["Und", "nicht", "das", "Ei\u00b7sen", "schmelzt", "zu", "Pflug\u00b7schaar", "und", "zu", "Eg\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "VVFIN", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wie herrlich lebt man hier, wenn dort der Tisch der F\u00fcrsten", "tokens": ["Wie", "herr\u00b7lich", "lebt", "man", "hier", ",", "wenn", "dort", "der", "Tisch", "der", "F\u00fcrs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "PIS", "ADV", "$,", "KOUS", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das kaum erschwitzte Brot oft halb verschimmelt giebt,", "tokens": ["Das", "kaum", "er\u00b7schwitz\u00b7te", "Brot", "oft", "halb", "ver\u00b7schim\u00b7melt", "giebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJA", "NN", "ADV", "ADJD", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn oft selbst K\u00f6nige bei voller Tafel d\u00fcrsten,", "tokens": ["Wenn", "oft", "selbst", "K\u00f6\u00b7ni\u00b7ge", "bei", "vol\u00b7ler", "Ta\u00b7fel", "d\u00fcrs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "NN", "APPR", "ADJA", "NN", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Aus Sorge, da\u00df Jemand wo ihnen Gift einschiebt,", "tokens": ["Aus", "Sor\u00b7ge", ",", "da\u00df", "Je\u00b7mand", "wo", "ih\u00b7nen", "Gift", "ein\u00b7schiebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUS", "PIS", "PWAV", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wenn man vor eigner Wach' und Sklaven sich ersch\u00fcttert", "tokens": ["Wenn", "man", "vor", "eig\u00b7ner", "Wach'", "und", "Skla\u00b7ven", "sich", "er\u00b7sch\u00fct\u00b7tert"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "ADJA", "NN", "KON", "NN", "PRF", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und als ein Espenlaub vor Schatten bebt und zittert!", "tokens": ["Und", "als", "ein", "E\u00b7spen\u00b7laub", "vor", "Schat\u00b7ten", "bebt", "und", "zit\u00b7tert", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPR", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}}, "stanza.6": {"line.1": {"text": "Hier aber wohnt die Ruh' und sicheres Vergn\u00fcgen;", "tokens": ["Hier", "a\u00b7ber", "wohnt", "die", "Ruh'", "und", "si\u00b7che\u00b7res", "Ver\u00b7gn\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Einfalt wei\u00df von Mord und von Vergiften nicht.", "tokens": ["Die", "Ein\u00b7falt", "wei\u00df", "von", "Mord", "und", "von", "Ver\u00b7gif\u00b7ten", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "APPR", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Tiger sind hier zahm, der Wolf spielt mit den Ziegen,", "tokens": ["Die", "Ti\u00b7ger", "sind", "hier", "zahm", ",", "der", "Wolf", "spielt", "mit", "den", "Zie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,", "ART", "NE", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und keine Natter kann hier bleiben, welche sticht.", "tokens": ["Und", "kei\u00b7ne", "Nat\u00b7ter", "kann", "hier", "blei\u00b7ben", ",", "wel\u00b7che", "sticht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VMFIN", "ADV", "VVINF", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Betrug und Arglist hei\u00dft bei Hof' ein Meisterst\u00fccke;", "tokens": ["Be\u00b7trug", "und", "Arg\u00b7list", "hei\u00dft", "bei", "Hof'", "ein", "Meis\u00b7ter\u00b7st\u00fc\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Hier wei\u00df man Nichts von List, als, wie man's Wild ber\u00fccke.", "tokens": ["Hier", "wei\u00df", "man", "Nichts", "von", "List", ",", "als", ",", "wie", "man's", "Wild", "be\u00b7r\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PIS", "APPR", "NE", "$,", "KOUS", "$,", "PWAV", "PIS", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Bei Hofe wei\u00df ein Greif zur Taube sich zu machen,", "tokens": ["Bei", "Ho\u00b7fe", "wei\u00df", "ein", "Greif", "zur", "Tau\u00b7be", "sich", "zu", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Fuchs ist mit der Haut der L\u00e4mmer angethan,", "tokens": ["Ein", "Fuchs", "ist", "mit", "der", "Haut", "der", "L\u00e4m\u00b7mer", "an\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Panther scheint ein Schaf, die Geier und die Drachen", "tokens": ["Der", "Pan\u00b7ther", "scheint", "ein", "Schaf", ",", "die", "Gei\u00b7er", "und", "die", "Dra\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sieht man f\u00fcr V\u00f6gel oft des Paradieses an;", "tokens": ["Sieht", "man", "f\u00fcr", "V\u00f6\u00b7gel", "oft", "des", "Pa\u00b7ra\u00b7die