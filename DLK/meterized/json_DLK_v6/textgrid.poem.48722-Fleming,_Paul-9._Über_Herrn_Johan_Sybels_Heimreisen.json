{"textgrid.poem.48722": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "9. \u00dcber Herrn Johan Sybels Heimreisen", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Dank ist nun verdient. Gebt zu ihr Pierinnen,", "tokens": ["Der", "Dank", "ist", "nun", "ver\u00b7dient", ".", "Gebt", "zu", "ihr", "Pie\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$.", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Apollo la\u00df es sein, da\u00df dieser euer Sohn", "tokens": ["A\u00b7pol\u00b7lo", "la\u00df", "es", "sein", ",", "da\u00df", "die\u00b7ser", "eu\u00b7er", "Sohn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "VAINF", "$,", "KOUS", "PDAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "den wolerzielten Preis und ausgezahlten Lohn", "tokens": ["den", "wo\u00b7ler\u00b7ziel\u00b7ten", "Preis", "und", "aus\u00b7ge\u00b7zahl\u00b7ten", "Lohn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "mit sich zu Hause f\u00fchr'! Er wird die regen Sinnen,", "tokens": ["mit", "sich", "zu", "Hau\u00b7se", "f\u00fchr'", "!", "Er", "wird", "die", "re\u00b7gen", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "APPR", "NN", "VVFIN", "$.", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "den aufgeweckten Geist so weiter schwingen k\u00f6nnen", "tokens": ["den", "auf\u00b7ge\u00b7weck\u00b7ten", "Geist", "so", "wei\u00b7ter", "schwin\u00b7gen", "k\u00f6n\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "ADV", "VVINF", "VMINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und euren Ruhm mit ihm. Nun wol! Er zeucht darvon,", "tokens": ["und", "eu\u00b7ren", "Ruhm", "mit", "ihm", ".", "Nun", "wol", "!", "Er", "zeucht", "dar\u00b7von", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "PPER", "$.", "ADV", "ADV", "$.", "PPER", "VVFIN", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "mein ungef\u00e4rbter Freund. Und seht, ihr habt ihn schon", "tokens": ["mein", "un\u00b7ge\u00b7f\u00e4rb\u00b7ter", "Freund", ".", "Und", "seht", ",", "ihr", "habt", "ihn", "schon"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "KON", "VVFIN", "$,", "PPER", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "mit eurer Gunst bedacht. Nun wol! Er zeucht von hinnen.", "tokens": ["mit", "eu\u00b7rer", "Gunst", "be\u00b7dacht", ".", "Nun", "wol", "!", "Er", "zeucht", "von", "hin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$.", "ADV", "ADV", "$.", "PPER", "VVFIN", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ihr G\u00f6tter, die ihr sonst die Wege pflegt zu sch\u00fctzen,", "tokens": ["Ihr", "G\u00f6t\u00b7ter", ",", "die", "ihr", "sonst", "die", "We\u00b7ge", "pflegt", "zu", "sch\u00fct\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "ADV", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "verschafft ihm sichern Pa\u00df; ihr Winde, brecht den Lauf,", "tokens": ["ver\u00b7schafft", "ihm", "si\u00b7chern", "Pa\u00df", ";", "ihr", "Win\u00b7de", ",", "brecht", "den", "Lauf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$.", "PPOSAT", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "halt, Himmel, Frost und Schnee und alle Wolken auf!", "tokens": ["halt", ",", "Him\u00b7mel", ",", "Frost", "und", "Schnee", "und", "al\u00b7le", "Wol\u00b7ken", "auf", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "NN", "KON", "NN", "KON", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und du, o Vaterland, der ists, der dir wird n\u00fctzen,", "tokens": ["Und", "du", ",", "o", "Va\u00b7ter\u00b7land", ",", "der", "ists", ",", "der", "dir", "wird", "n\u00fct\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "FM", "NN", "$,", "PRELS", "VAFIN", "$,", "PRELS", "PPER", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "dein liebes Kind, der ists, gieb ihm nun s\u00fc\u00dfe Rast,", "tokens": ["dein", "lie\u00b7bes", "Kind", ",", "der", "ists", ",", "gieb", "ihm", "nun", "s\u00fc\u00b7\u00dfe", "Rast", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PRELS", "VAFIN", "$,", "VVIMP", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "gleich wie du ihm vorhin das Leben geben hast!", "tokens": ["gleich", "wie", "du", "ihm", "vor\u00b7hin", "das", "Le\u00b7ben", "ge\u00b7ben", "hast", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "PPER", "ADV", "ART", "NN", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}