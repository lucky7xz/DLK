{"dta.poem.8477": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Und der silberne Mond hinter dem Walde steht,", "tokens": ["Und", "der", "sil\u00b7ber\u00b7ne", "Mond", "hin\u00b7ter", "dem", "Wal\u00b7de", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+---+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Ist ein irdischer Himmel,", "tokens": ["Ist", "ein", "ir\u00b7di\u00b7scher", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Gleicht den Thalen der Seligen.", "tokens": ["Gleicht", "den", "Tha\u00b7len", "der", "Se\u00b7li\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sch\u00f6ner l\u00e4chelt der Hain, silberner schwebt der Mond,", "tokens": ["Sch\u00f6\u00b7ner", "l\u00e4\u00b7chelt", "der", "Hain", ",", "sil\u00b7ber\u00b7ner", "schwebt", "der", "Mond", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "ADJA", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Und der ganze Olimp fleusst auf die Erd' herab,", "tokens": ["Und", "der", "gan\u00b7ze", "O\u00b7limp", "fleusst", "auf", "die", "Erd'", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "ADV", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Wann die Liebe den J\u00fcngling", "tokens": ["Wann", "die", "Lie\u00b7be", "den", "J\u00fcng\u00b7ling"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Durch die einsamen B\u00fcsche f\u00fchrt.", "tokens": ["Durch", "die", "ein\u00b7sa\u00b7men", "B\u00fc\u00b7sche", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}}, "stanza.3": {"line.1": {"text": "Wann ihr goldener Stab winket, befl\u00fcgelt sich", "tokens": ["Wann", "ihr", "gol\u00b7de\u00b7ner", "Stab", "win\u00b7ket", ",", "be\u00b7fl\u00fc\u00b7gelt", "sich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PPER", "ADJA", "NN", "VVFIN", "$,", "VVFIN", "PRF"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Jede Seele mit Glut, schwingt sich den Sternen zu,", "tokens": ["Je\u00b7de", "See\u00b7le", "mit", "Glut", ",", "schwingt", "sich", "den", "Ster\u00b7nen", "zu", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "$,", "VVFIN", "PRF", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Schwebt durch Engelgefilde,", "tokens": ["Schwebt", "durch", "En\u00b7gel\u00b7ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Trinkt aus B\u00e4chen der Serafim;", "tokens": ["Trinkt", "aus", "B\u00e4\u00b7chen", "der", "Se\u00b7ra\u00b7fim", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Weilt, und trinket, und weilt, schwanket im Labirinth;", "tokens": ["Weilt", ",", "und", "trin\u00b7ket", ",", "und", "weilt", ",", "schwan\u00b7ket", "im", "La\u00b7bi\u00b7rinth", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "VVFIN", "$,", "KON", "VVFIN", "$,", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Eine reinere Luft athmet von Gottes Stul", "tokens": ["Ei\u00b7ne", "rei\u00b7ne\u00b7re", "Luft", "ath\u00b7met", "von", "Got\u00b7tes", "Stul"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "NN"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Ihr entgegen, und weht sie", "tokens": ["Ihr", "ent\u00b7ge\u00b7gen", ",", "und", "weht", "sie"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "PTKVZ", "$,", "KON", "VVFIN", "PPER"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Gleich dem S\u00e4useln Jehovas an.", "tokens": ["Gleich", "dem", "S\u00e4u\u00b7seln", "Je\u00b7ho\u00b7vas", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NE", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "Selten winket ihr Stab, selten enth\u00fcllet sie", "tokens": ["Sel\u00b7ten", "win\u00b7ket", "ihr", "Stab", ",", "sel\u00b7ten", "ent\u00b7h\u00fcl\u00b7let", "sie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "ADJD", "VVFIN", "PPER"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Sich den S\u00f6hnen des Staubs. Ach! sie verkennen dich;", "tokens": ["Sich", "den", "S\u00f6h\u00b7nen", "des", "Staubs", ".", "Ach", "!", "sie", "ver\u00b7ken\u00b7nen", "dich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "ART", "NN", "$.", "ITJ", "$.", "PPER", "VVFIN", "PPER", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Ach! sie h\u00fcllen der Wollust", "tokens": ["Ach", "!", "sie", "h\u00fcl\u00b7len", "der", "Wol\u00b7lust"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Deinen heiligen Schleier um.", "tokens": ["Dei\u00b7nen", "hei\u00b7li\u00b7gen", "Schlei\u00b7er", "um", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.6": {"line.1": {"text": "Mir erschienest du, mir, h\u00f6heres Glanzes voll,", "tokens": ["Mir", "er\u00b7schie\u00b7nest", "du", ",", "mir", ",", "h\u00f6\u00b7he\u00b7res", "Glan\u00b7zes", "voll", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "$,", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Wie dein Sokrates dich, wie dich dein Plato sah;", "tokens": ["Wie", "dein", "Sok\u00b7ra\u00b7tes", "dich", ",", "wie", "dich", "dein", "Pla\u00b7to", "sah", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PPER", "$,", "PWAV", "PPER", "PPOSAT", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie du jenem im Thale", "tokens": ["Wie", "du", "je\u00b7nem", "im", "Tha\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PDAT", "APPRART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Seiner Quelle begegnetest.", "tokens": ["Sei\u00b7ner", "Quel\u00b7le", "be\u00b7ge\u00b7gne\u00b7test", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Erd' und Himmel entflieht sterbenden Heiligen;", "tokens": ["Erd'", "und", "Him\u00b7mel", "ent\u00b7flieht", "ster\u00b7ben\u00b7den", "Hei\u00b7li\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Lebensbl\u00fctengeruch str\u00f6met um sie herum;", "tokens": ["Le\u00b7bens\u00b7bl\u00fc\u00b7ten\u00b7ge\u00b7ruch", "str\u00f6\u00b7met", "um", "sie", "he\u00b7rum", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Engelfittige rauschen,", "tokens": ["En\u00b7gel\u00b7fit\u00b7ti\u00b7ge", "rau\u00b7schen", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Und die goldene Krone winkt.", "tokens": ["Und", "die", "gol\u00b7de\u00b7ne", "Kro\u00b7ne", "winkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.8": {"line.1": {"text": "Erd' und Himmel entfloh, als ich dich, Dafne, sah;", "tokens": ["Erd'", "und", "Him\u00b7mel", "ent\u00b7floh", ",", "als", "ich", "dich", ",", "Daf\u00b7ne", ",", "sah", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "$,", "KOUS", "PPER", "PRF", "$,", "NE", "$,", "VVFIN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Als dein purpurner Mund sch\u00fcchtern mir l\u00e4chelte,", "tokens": ["Als", "dein", "pur\u00b7pur\u00b7ner", "Mund", "sch\u00fcch\u00b7tern", "mir", "l\u00e4\u00b7chel\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "--++-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Als dein athmender Busen", "tokens": ["Als", "dein", "ath\u00b7men\u00b7der", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Meinen Blicken entgegen stieg.", "tokens": ["Mei\u00b7nen", "Bli\u00b7cken", "ent\u00b7ge\u00b7gen", "stieg", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.9": {"line.1": {"text": "Unbekanntes Gef\u00fchl bebte zum erstenmal", "tokens": ["Un\u00b7be\u00b7kann\u00b7tes", "Ge\u00b7f\u00fchl", "beb\u00b7te", "zum", "ers\u00b7ten\u00b7mal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "APPRART", "ADV"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Durch mein jugendlich Herz; froh wie Anakreon,", "tokens": ["Durch", "mein", "ju\u00b7gend\u00b7lich", "Herz", ";", "froh", "wie", "A\u00b7nak\u00b7re\u00b7on", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJD", "NN", "$.", "ADJD", "KOKOM", "NE", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Goss ich Flammen der Seele", "tokens": ["Goss", "ich", "Flam\u00b7men", "der", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "NN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "In mein zitterndes Saitenspiel.", "tokens": ["In", "mein", "zit\u00b7tern\u00b7des", "Sai\u00b7ten\u00b7spiel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.10": {"line.1": {"text": "Eine Nachtigall flog, als ich mein erstes Lied,", "tokens": ["Ei\u00b7ne", "Nach\u00b7ti\u00b7gall", "flog", ",", "als", "ich", "mein", "ers\u00b7tes", "Lied", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "S\u00fcsse Liebe, dir sang, fl\u00f6tend um mich herum;", "tokens": ["S\u00fcs\u00b7se", "Lie\u00b7be", ",", "dir", "sang", ",", "fl\u00f6\u00b7tend", "um", "mich", "he\u00b7rum", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PPER", "VVFIN", "$,", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Und es taumelten Bl\u00fcten", "tokens": ["Und", "es", "tau\u00b7mel\u00b7ten", "Bl\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Auf mein lispelndes Spiel herab.", "tokens": ["Auf", "mein", "lis\u00b7peln\u00b7des", "Spiel", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Seit ich Dafnen erblickt, raucht kein vergossenes", "tokens": ["Seit", "ich", "Daf\u00b7nen", "er\u00b7blickt", ",", "raucht", "kein", "ver\u00b7gos\u00b7se\u00b7nes"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "NE", "VVPP", "$,", "VVFIN", "PIAT", "ADJA"], "meter": "+-+--+-+-+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Blut durch meinen Gesang, spend' ich den K\u00f6nigen", "tokens": ["Blut", "durch", "mei\u00b7nen", "Ge\u00b7sang", ",", "spend'", "ich", "den", "K\u00f6\u00b7ni\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Keinen schmeichelnden Lorber,", "tokens": ["Kei\u00b7nen", "schmei\u00b7cheln\u00b7den", "Lor\u00b7ber", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Sing' ich M\u00e4dchen und M\u00e4dchenkuss.", "tokens": ["Sing'", "ich", "M\u00e4d\u00b7chen", "und", "M\u00e4d\u00b7chen\u00b7kuss", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "KON", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}