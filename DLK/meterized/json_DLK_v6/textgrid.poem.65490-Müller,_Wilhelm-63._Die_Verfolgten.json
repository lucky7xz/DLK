{"textgrid.poem.65490": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "63. Die Verfolgten", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn man jagt den Elephanten um sein wei\u00dfes Elfenbein,", "tokens": ["Wenn", "man", "jagt", "den", "E\u00b7le\u00b7phan\u00b7ten", "um", "sein", "wei\u00b7\u00dfes", "El\u00b7fen\u00b7bein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Wenn man schl\u00e4gt das Dach der Auster um die edlen Perlen ein,", "tokens": ["Wenn", "man", "schl\u00e4gt", "das", "Dach", "der", "Aus\u00b7ter", "um", "die", "ed\u00b7len", "Per\u00b7len", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Sag', wie kann es dich verwundern, da\u00df die Welt dich jagt und schl\u00e4gt,", "tokens": ["Sag'", ",", "wie", "kann", "es", "dich", "ver\u00b7wun\u00b7dern", ",", "da\u00df", "die", "Welt", "dich", "jagt", "und", "schl\u00e4gt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VMFIN", "PPER", "PRF", "VVINF", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Weil sie dir es angesehen, da\u00df dein Busen Sch\u00e4tze hegt?", "tokens": ["Weil", "sie", "dir", "es", "an\u00b7ge\u00b7se\u00b7hen", ",", "da\u00df", "dein", "Bu\u00b7sen", "Sch\u00e4t\u00b7ze", "hegt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PPER", "VVPP", "$,", "KOUS", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}