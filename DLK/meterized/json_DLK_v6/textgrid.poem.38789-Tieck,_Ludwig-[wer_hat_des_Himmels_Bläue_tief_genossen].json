{"textgrid.poem.38789": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "[wer hat des Himmels Bl\u00e4ue tief genossen]", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer hat des Himmels Bl\u00e4ue tief genossen,", "tokens": ["Wer", "hat", "des", "Him\u00b7mels", "Bl\u00e4u\u00b7e", "tief", "ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den inngen Blick aus den azurnen L\u00fcften,", "tokens": ["Den", "inn\u00b7gen", "Blick", "aus", "den", "a\u00b7zur\u00b7nen", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Den Blumenkelch, das Aug' in s\u00fc\u00dfen D\u00fcften,", "tokens": ["Den", "Blu\u00b7men\u00b7kelch", ",", "das", "Aug'", "in", "s\u00fc\u00b7\u00dfen", "D\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den klaren Quell, vom gr\u00fcnen Licht umflossen?", "tokens": ["Den", "kla\u00b7ren", "Quell", ",", "vom", "gr\u00fc\u00b7nen", "Licht", "um\u00b7flos\u00b7sen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Aus allem ist mir oft ein Aug' entsprossen,", "tokens": ["Aus", "al\u00b7lem", "ist", "mir", "oft", "ein", "Aug'", "ent\u00b7spros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So fand ich Sehnsucht, Lieb' in Steineskl\u00fcften,", "tokens": ["So", "fand", "ich", "Sehn\u00b7sucht", ",", "Lieb'", "in", "Stei\u00b7nes\u00b7kl\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein tr\u00e4umend Liebes-Echo selbst in Gr\u00fcften,", "tokens": ["Ein", "tr\u00e4u\u00b7mend", "Lie\u00b7bes\u00b7E\u00b7cho", "selbst", "in", "Gr\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In Wald, Berg, Thal und Flu\u00df meine Genossen.", "tokens": ["In", "Wald", ",", "Berg", ",", "Thal", "und", "Flu\u00df", "mei\u00b7ne", "Ge\u00b7nos\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}}, "stanza.3": {"line.1": {"text": "Wie ward mein Herz im Innersten ersch\u00fcttert,", "tokens": ["Wie", "ward", "mein", "Herz", "im", "In\u00b7ners\u00b7ten", "er\u00b7sch\u00fct\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Als lichte T\u00f6ne flogen wie die Tauben,", "tokens": ["Als", "lich\u00b7te", "T\u00f6\u00b7ne", "flo\u00b7gen", "wie", "die", "Tau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die in der Sonne wie klar Gold erfunkeln:", "tokens": ["Die", "in", "der", "Son\u00b7ne", "wie", "klar", "Gold", "er\u00b7fun\u00b7keln", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "KOKOM", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ein Blick, blau, hold, sich aufthat aus dem Dunkeln.", "tokens": ["Ein", "Blick", ",", "blau", ",", "hold", ",", "sich", "auf\u00b7that", "aus", "dem", "Dun\u00b7keln", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "$,", "ADJD", "$,", "PRF", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nun kann ich erst an Stern und Auge glauben,", "tokens": ["Nun", "kann", "ich", "erst", "an", "Stern", "und", "Au\u00b7ge", "glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Seit mir im Herzen dieses Blicken zittert.", "tokens": ["Seit", "mir", "im", "Her\u00b7zen", "die\u00b7ses", "Bli\u00b7cken", "zit\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "NN", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}