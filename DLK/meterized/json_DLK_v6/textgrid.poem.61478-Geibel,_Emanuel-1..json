{"textgrid.poem.61478": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Solang du wallst auf Erdenbahnen,", "tokens": ["So\u00b7lang", "du", "wallst", "auf", "Er\u00b7den\u00b7bah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Irrtum, Freund, entgehst du nicht;", "tokens": ["Dem", "Irr\u00b7tum", ",", "Freund", ",", "ent\u00b7gehst", "du", "nicht", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch l\u00e4\u00dft dich Irrtum Wahrheit ahnen,", "tokens": ["Doch", "l\u00e4\u00dft", "dich", "Irr\u00b7tum", "Wahr\u00b7heit", "ah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Irrtum ist Farbe, Wahrheit Licht.", "tokens": ["Irr\u00b7tum", "ist", "Far\u00b7be", ",", "Wahr\u00b7heit", "Licht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VAFIN", "NN", "$,", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}