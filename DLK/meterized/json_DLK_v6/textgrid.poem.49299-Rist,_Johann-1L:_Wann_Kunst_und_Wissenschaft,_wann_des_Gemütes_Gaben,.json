{"textgrid.poem.49299": {"metadata": {"author": {"name": "Rist, Johann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wann Kunst und Wissenschaft, wann des Gem\u00fctes Gaben,", "genre": "verse", "period": "N.A.", "pub_year": 1637, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann Kunst und Wissenschaft, wann des Gem\u00fctes Gaben,", "tokens": ["Wann", "Kunst", "und", "Wis\u00b7sen\u00b7schaft", ",", "wann", "des", "Ge\u00b7m\u00fc\u00b7tes", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "$,", "PWAV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Damit f\u00fcr schlechtes Volk die hohen Geister traben,", "tokens": ["Da\u00b7mit", "f\u00fcr", "schlech\u00b7tes", "Volk", "die", "ho\u00b7hen", "Geis\u00b7ter", "tra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wann Scherz, wann kluge List, wann Witz aus Sterbens Not", "tokens": ["Wann", "Scherz", ",", "wann", "klu\u00b7ge", "List", ",", "wann", "Witz", "aus", "Ster\u00b7bens", "Not"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "$,", "PWAV", "ADJA", "NN", "$,", "PWAV", "NN", "APPR", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "K\u00f6nt' helfen, ei, so w\u00e4r mein ", "tokens": ["K\u00f6nt'", "hel\u00b7fen", ",", "ei", ",", "so", "w\u00e4r", "mein"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "VVINF", "$,", "ITJ", "$,", "ADV", "VAFIN", "PPOSAT"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Ihn hatt' Apollo selbst zum Erben auserkoren,", "tokens": ["Ihn", "hatt'", "A\u00b7pol\u00b7lo", "selbst", "zum", "Er\u00b7ben", "au\u00b7ser\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "ADV", "APPRART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Pallas nahm ihn auf, kaum wie er war geboren", "tokens": ["Die", "Pal\u00b7las", "nahm", "ihn", "auf", ",", "kaum", "wie", "er", "war", "ge\u00b7bo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "KOKOM", "PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und in der Wiegen lag; sein Reichtum, Schatz und Zier", "tokens": ["Und", "in", "der", "Wie\u00b7gen", "lag", ";", "sein", "Reich\u00b7tum", ",", "Schatz", "und", "Zier"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$.", "PPOSAT", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "War zwar kein Geld noch Gold, nur B\u00fccher und Papier.", "tokens": ["War", "zwar", "kein", "Geld", "noch", "Gold", ",", "nur", "B\u00fc\u00b7cher", "und", "Pa\u00b7pier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "ADV", "NN", "$,", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Er liebte ja kein Geld, wie mancher, der sein Dichten", "tokens": ["Er", "lieb\u00b7te", "ja", "kein", "Geld", ",", "wie", "man\u00b7cher", ",", "der", "sein", "Dich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "NN", "$,", "PWAV", "PIS", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Auf das, was eitel hei\u00dft, nur einzig pflegt zu richten;", "tokens": ["Auf", "das", ",", "was", "ei\u00b7tel", "hei\u00dft", ",", "nur", "ein\u00b7zig", "pflegt", "zu", "rich\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "ADJD", "VVFIN", "$,", "ADV", "ADJD", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Auch fragt' er nichts nach Gunst, der Heuchler falschem Schein,", "tokens": ["Auch", "fragt'", "er", "nichts", "nach", "Gunst", ",", "der", "Heuch\u00b7ler", "fal\u00b7schem", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "APPR", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Sein Wunsch war, da\u00df er k\u00f6nt' nur allen dienstlich sein.", "tokens": ["Sein", "Wunsch", "war", ",", "da\u00df", "er", "k\u00f6nt'", "nur", "al\u00b7len", "dienst\u00b7lich", "sein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "$,", "KOUS", "PPER", "VMFIN", "ADV", "PIAT", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Von seiner Kindheit an, auch durch sein ganzes Leben", "tokens": ["Von", "sei\u00b7ner", "Kind\u00b7heit", "an", ",", "auch", "durch", "sein", "gan\u00b7zes", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$,", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Hat er sein Herz' und Sinn der Wissenschaft ergeben,", "tokens": ["Hat", "er", "sein", "Her\u00b7z'", "und", "Sinn", "der", "Wis\u00b7sen\u00b7schaft", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "KON", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.15": {"text": "Und das mit rechtem ", "tokens": ["Und", "das", "mit", "rech\u00b7tem"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDS", "APPR", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.16": {"text": "Jedoch sein kluger Geist macht' ihn der Welt bekant.", "tokens": ["Je\u00b7doch", "sein", "klu\u00b7ger", "Geist", "macht'", "ihn", "der", "Welt", "be\u00b7kant", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die weltber\u00fchmte Schul der theuren G\u00fclpher Helden,", "tokens": ["Die", "welt\u00b7be\u00b7r\u00fchm\u00b7te", "Schul", "der", "theu\u00b7ren", "G\u00fcl\u00b7pher", "Hel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die sol sein wertes Lob von langer Zeit vermelden,", "tokens": ["Die", "sol", "sein", "wer\u00b7tes", "Lob", "von", "lan\u00b7ger", "Zeit", "ver\u00b7mel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Dich mein' ich, Helmenst\u00e4tt; du hast ihn oft geh\u00f6rt,", "tokens": ["Dich", "mein'", "ich", ",", "Hel\u00b7men\u00b7st\u00e4tt", ";", "du", "hast", "ihn", "oft", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "NN", "$.", "PPER", "VAFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wann er dein wertes Volk mit Reden hat verehrt,", "tokens": ["Wann", "er", "dein", "wer\u00b7tes", "Volk", "mit", "Re\u00b7den", "hat", "ver\u00b7ehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "ADJA", "NN", "APPR", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Mit Reden, da er k\u00f6nt' ein hartes Herz durch beugen.", "tokens": ["Mit", "Re\u00b7den", ",", "da", "er", "k\u00f6nt'", "ein", "har\u00b7tes", "Herz", "durch", "beu\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUS", "PPER", "VMFIN", "ART", "ADJA", "NN", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Ihr hohen St\u00fchl', ihr Tisch', euch ruf' ich all' zu Zeugen,", "tokens": ["Ihr", "ho\u00b7hen", "St\u00fchl'", ",", "ihr", "Tisch'", ",", "euch", "ruf'", "ich", "all'", "zu", "Zeu\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER", "PIS", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ihr Seulen, die ihr in den sch\u00f6nen Zimmern steht,", "tokens": ["Ihr", "Seu\u00b7len", ",", "die", "ihr", "in", "den", "sch\u00f6\u00b7nen", "Zim\u00b7mern", "steht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Wo Ph\u00f6bus und sein Volk oft auf und nieder geht;", "tokens": ["Wo", "Ph\u00f6\u00b7bus", "und", "sein", "Volk", "oft", "auf", "und", "nie\u00b7der", "geht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "KON", "PPOSAT", "NN", "ADV", "PTKVZ", "KON", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ihr wisset, wie er pflag mit Versen das zu preisen,", "tokens": ["Ihr", "wis\u00b7set", ",", "wie", "er", "pflag", "mit", "Ver\u00b7sen", "das", "zu", "prei\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "APPR", "NN", "ART", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Was recht zu preisen war; er that euch allen weisen,", "tokens": ["Was", "recht", "zu", "prei\u00b7sen", "war", ";", "er", "that", "euch", "al\u00b7len", "wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "PTKZU", "VVINF", "VAFIN", "$.", "PPER", "VVFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Da\u00df zwar ein Hofemann gemachet werd', allein", "tokens": ["Da\u00df", "zwar", "ein", "Ho\u00b7fe\u00b7mann", "ge\u00b7ma\u00b7chet", "werd'", ",", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "VVPP", "VAFIN", "$,", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Ein Singer und Poet m\u00fc\u00df' erst ", "tokens": ["Ein", "Sin\u00b7ger", "und", "Po\u00b7et", "m\u00fc\u00df'", "erst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "VMFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Ich wil hier, was ich sonst noch r\u00fchmlich k\u00f6nt' erz\u00e4hlen", "tokens": ["Ich", "wil", "hier", ",", "was", "ich", "sonst", "noch", "r\u00fchm\u00b7lich", "k\u00f6nt'", "er\u00b7z\u00e4h\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "$,", "PWS", "PPER", "ADV", "ADV", "ADJD", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Von seinem gro\u00dfen Flei\u00df in Gottes Schrift, verhehlen.", "tokens": ["Von", "sei\u00b7nem", "gro\u00b7\u00dfen", "Flei\u00df", "in", "Got\u00b7tes", "Schrift", ",", "ver\u00b7heh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "APPR", "NN", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Sein erstes Thun war Furcht des Herren Zebaoth,", "tokens": ["Sein", "ers\u00b7tes", "Thun", "war", "Furcht", "des", "Her\u00b7ren", "Ze\u00b7bao\u00b7th", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "NN", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Sein andres Ehr' und Zucht, das letzte Freud' in Gott.", "tokens": ["Sein", "and\u00b7res", "Ehr'", "und", "Zucht", ",", "das", "letz\u00b7te", "Freud'", "in", "Gott", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "NN", "$,", "ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Mehr schreib ich nichts hievon. Was \u00fcber die\u00df die Tugend", "tokens": ["Mehr", "schreib", "ich", "nichts", "hie\u00b7von", ".", "Was", "\u00fc\u00b7ber", "die\u00df", "die", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "PIS", "PAV", "$.", "PWS", "APPR", "PDS", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Der Lieb' und Treu' betrifft, auch wie er in der Jugend", "tokens": ["Der", "Lieb'", "und", "Treu'", "be\u00b7tr\u00b7ifft", ",", "auch", "wie", "er", "in", "der", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$,", "ADV", "KOKOM", "PPER", "APPR", "ART", "NN"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.35": {"text": "Sein Leben und sein Thun auf Redlichkeit gewandt,", "tokens": ["Sein", "Le\u00b7ben", "und", "sein", "Thun", "auf", "Red\u00b7lich\u00b7keit", "ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Das, mein' ich, ist mir auch zum guten Theil bekant.", "tokens": ["Das", ",", "mein'", "ich", ",", "ist", "mir", "auch", "zum", "gu\u00b7ten", "Theil", "be\u00b7kant", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "VVFIN", "PPER", "$,", "VAFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Sein Wissen war nicht schlecht, sein Lernen und sein Lehren", "tokens": ["Sein", "Wis\u00b7sen", "war", "nicht", "schlecht", ",", "sein", "Ler\u00b7nen", "und", "sein", "Leh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "ADJD", "$,", "PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Ist vielen wol bewust; die\u00df kan sein Lob vermehren,", "tokens": ["Ist", "vie\u00b7len", "wol", "be\u00b7wust", ";", "die\u00df", "kan", "sein", "Lob", "ver\u00b7meh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ADV", "VVFIN", "$.", "PDS", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Da\u00df Rostock, weil es ihn herzgr\u00fcndlich lieben that,", "tokens": ["Da\u00df", "Ros\u00b7tock", ",", "weil", "es", "ihn", "herz\u00b7gr\u00fcnd\u00b7lich", "lie\u00b7ben", "that", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "$,", "KOUS", "PPER", "PPER", "ADJD", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Ein Zeugnis seiner Kunst ihm gern ertheilet hat.", "tokens": ["Ein", "Zeug\u00b7nis", "sei\u00b7ner", "Kunst", "ihm", "gern", "er\u00b7thei\u00b7let", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PPER", "ADV", "VVFIN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Was soll ich von der Lust, die er stets pflag zu tragen", "tokens": ["Was", "soll", "ich", "von", "der", "Lust", ",", "die", "er", "stets", "pflag", "zu", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Zu mancherlei Gedicht' und Freudenspielen sagen?", "tokens": ["Zu", "man\u00b7cher\u00b7lei", "Ge\u00b7dicht'", "und", "Freu\u00b7den\u00b7spie\u00b7len", "sa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Da war er Meister inn'; er hats dahin gebracht,", "tokens": ["Da", "war", "er", "Meis\u00b7ter", "inn'", ";", "er", "hats", "da\u00b7hin", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "PTKVZ", "$.", "PPER", "VAFIN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Da\u00df nun so mancher Geist auf Schauspiel ist bedacht.", "tokens": ["Da\u00df", "nun", "so", "man\u00b7cher", "Geist", "auf", "Schau\u00b7spiel", "ist", "be\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "PIAT", "NN", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Ist Varro gleich ber\u00fchmt, ist er gleich Prinz gewesen,", "tokens": ["Ist", "Var\u00b7ro", "gleich", "be\u00b7r\u00fchmt", ",", "ist", "er", "gleich", "Prinz", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "ADV", "ADJD", "$,", "VAFIN", "PPER", "ADV", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Der Dichter seiner Zeit, solt' er die Spiele lesen,", "tokens": ["Der", "Dich\u00b7ter", "sei\u00b7ner", "Zeit", ",", "solt'", "er", "die", "Spie\u00b7le", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Die Stapel auf dem Platz hat ehmals vorgestellt,", "tokens": ["Die", "Sta\u00b7pel", "auf", "dem", "Platz", "hat", "eh\u00b7mals", "vor\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Er w\u00fcrde sich vor Scham hinschwingen aus der Welt", "tokens": ["Er", "w\u00fcr\u00b7de", "sich", "vor", "Scham", "hin\u00b7schwin\u00b7gen", "aus", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PRF", "APPR", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Nach Proserpinen zu. Was? Plautus mu\u00df ihm weichen,", "tokens": ["Nach", "Pro\u00b7ser\u00b7pi\u00b7nen", "zu", ".", "Was", "?", "Plau\u00b7tus", "mu\u00df", "ihm", "wei\u00b7chen", ","], "token_