{"dta.poem.13370": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Abhandlung  \n von der Schreibart in Miltons  \n verlohrnen Paradiese.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1742", "urn": "urn:nbn:de:kobv:b4-200905198519", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Indem er drauf, ", "tokens": ["In\u00b7dem", "er", "drauf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Den W\u00fcrden nach vertheilet, stellt, und z\u00e4hlt,", "tokens": ["Den", "W\u00fcr\u00b7den", "nach", "ver\u00b7thei\u00b7let", ",", "stellt", ",", "und", "z\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "VVFIN", "$,", "VVFIN", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bezeichnet er die ihm recht artig scheinen.", "tokens": ["Be\u00b7zeich\u00b7net", "er", "die", "ihm", "recht", "ar\u00b7tig", "schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}}}}}