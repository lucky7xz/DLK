{"dta.poem.9623": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Er giebt sich der liebe gefangen.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.57", "af:0.42"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Njmm dann die waffen hin/", "tokens": ["Njmm", "dann", "die", "waf\u00b7fen", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "PTKVZ", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Cupido/ gott der liebe/", "tokens": ["Cu\u00b7pi\u00b7do", "/", "gott", "der", "lie\u00b7be", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$(", "NN", "ART", "VVFIN", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Ich folge dir und deinem heissen triebe/", "tokens": ["Ich", "fol\u00b7ge", "dir", "und", "dei\u00b7nem", "heis\u00b7sen", "trie\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KON", "PPOSAT", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und mag des eitlen ruhms der stoltzen freyheit nicht.", "tokens": ["Und", "mag", "des", "eit\u00b7len", "ruhms", "der", "stolt\u00b7zen", "frey\u00b7heit", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich opffre dir das hertz/ den leichten sinn/", "tokens": ["Ich", "opff\u00b7re", "dir", "das", "hertz", "/", "den", "leich\u00b7ten", "sinn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Den unbestand/ und was ich bin/", "tokens": ["Den", "un\u00b7be\u00b7stand", "/", "und", "was", "ich", "bin", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "KON", "PWS", "PPER", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ja selber mein vergn\u00fcgen.", "tokens": ["Ja", "sel\u00b7ber", "mein", "ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKANT", "KON", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dein strahl; doch nein! Aurorens angesicht", "tokens": ["Dein", "strahl", ";", "doch", "nein", "!", "Au\u00b7ro\u00b7rens", "an\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "ADV", "PTKANT", "$.", "NE", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Hat seinen blitz auff meine brust gericht/", "tokens": ["Hat", "sei\u00b7nen", "blitz", "auff", "mei\u00b7ne", "brust", "ge\u00b7richt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und sucht mich armen v\u00f6llig zu besiegen/", "tokens": ["Und", "sucht", "mich", "ar\u00b7men", "v\u00f6l\u00b7lig", "zu", "be\u00b7sie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Sie tr\u00e4gt den sieg davon/ und dir bleibt der gewinn;", "tokens": ["Sie", "tr\u00e4gt", "den", "sieg", "da\u00b7von", "/", "und", "dir", "bleibt", "der", "ge\u00b7winn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PAV", "$(", "KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Nimm dann die waffen hin.", "tokens": ["Nimm", "dann", "die", "waf\u00b7fen", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}