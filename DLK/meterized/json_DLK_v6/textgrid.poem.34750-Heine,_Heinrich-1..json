{"textgrid.poem.34750": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wandl' ich in dem Wald des Abends,", "tokens": ["Wandl'", "ich", "in", "dem", "Wald", "des", "A\u00b7bends", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem tr\u00e4umerischen Wald,", "tokens": ["In", "dem", "tr\u00e4u\u00b7me\u00b7ri\u00b7schen", "Wald", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Immer wandelt mir zur Seite", "tokens": ["Im\u00b7mer", "wan\u00b7delt", "mir", "zur", "Sei\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deine z\u00e4rtliche Gestalt.", "tokens": ["Dei\u00b7ne", "z\u00e4rt\u00b7li\u00b7che", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ist es nicht dein wei\u00dfer Schleier?", "tokens": ["Ist", "es", "nicht", "dein", "wei\u00b7\u00dfer", "Schlei\u00b7er", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht dein sanftes Angesicht?", "tokens": ["Nicht", "dein", "sanf\u00b7tes", "An\u00b7ge\u00b7sicht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Oder ist es nur der Mondschein,", "tokens": ["O\u00b7der", "ist", "es", "nur", "der", "Mond\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der durch Tannendunkel bricht?", "tokens": ["Der", "durch", "Tan\u00b7nen\u00b7dun\u00b7kel", "bricht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sind es meine eignen Tr\u00e4nen,", "tokens": ["Sind", "es", "mei\u00b7ne", "eig\u00b7nen", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die ich leise rinnen h\u00f6r?", "tokens": ["Die", "ich", "lei\u00b7se", "rin\u00b7nen", "h\u00f6r", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Oder gehst du, Liebste, wirklich", "tokens": ["O\u00b7der", "gehst", "du", ",", "Liebs\u00b7te", ",", "wirk\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "NN", "$,", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weinend neben mir einher?", "tokens": ["Wei\u00b7nend", "ne\u00b7ben", "mir", "ein\u00b7her", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}