{"dta.poem.13042": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Siebendes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ewelch unverzagtes Volck? erweget nicht die Zahl!", "tokens": ["\u201e", "welch", "un\u00b7ver\u00b7zag\u00b7tes", "Volck", "?", "er\u00b7we\u00b7get", "nicht", "die", "Zahl", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAT", "ADJA", "NN", "$.", "VVFIN", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201enur dessen Streitbarkeit, Herz, Antliz, Muth und Wahl!", "tokens": ["\u201e", "nur", "des\u00b7sen", "Streit\u00b7bar\u00b7keit", ",", "Herz", ",", "Ant\u00b7liz", ",", "Muth", "und", "Wahl", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PDS", "NN", "$,", "NN", "$,", "NE", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201eist es nicht eine Reih von unbesiegten Rittern,", "tokens": ["\u201e", "ist", "es", "nicht", "ei\u00b7ne", "Reih", "von", "un\u00b7be\u00b7sieg\u00b7ten", "Rit\u00b7tern", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "PTKNEG", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.4": {"text": "\u201edie nur, wann sich der Feind entfernt, f\u00fcr Rache zittern?", "tokens": ["\u201e", "die", "nur", ",", "wann", "sich", "der", "Feind", "ent\u00b7fernt", ",", "f\u00fcr", "Ra\u00b7che", "zit\u00b7tern", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PRELS", "ADV", "$,", "PWAV", "PRF", "ART", "NN", "VVPP", "$,", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "325\u201dHabt ihr nicht jenes Barts, des dick bewachsnen Kinns,", "tokens": ["\"", "Habt", "ihr", "nicht", "je\u00b7nes", "Barts", ",", "des", "dick", "be\u00b7wachs\u00b7nen", "Kinns", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "PTKNEG", "PDAT", "NN", "$,", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201edes scheelen Augenblicks, des starren Krieger-Sinns", "tokens": ["\u201e", "des", "schee\u00b7len", "Au\u00b7gen\u00b7blicks", ",", "des", "star\u00b7ren", "Krie\u00b7ger\u00b7Sinns"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201elebendiges Ger\u00fcst, das Waffen-Haus gesehen?", "tokens": ["\u201e", "le\u00b7ben\u00b7di\u00b7ges", "Ge\u00b7r\u00fcst", ",", "das", "Waf\u00b7fen\u00b7Haus", "ge\u00b7se\u00b7hen", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201emit was vor Muth und Lust es pflegt zum Kampf zu gehen?", "tokens": ["\u201e", "mit", "was", "vor", "Muth", "und", "Lust", "es", "pflegt", "zum", "Kampf", "zu", "ge\u00b7hen", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PRELS", "APPR", "NN", "KON", "NN", "PPER", "VVFIN", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201ees schmiegt, verbirget sich, steckt in dem Wald bereit;", "tokens": ["\u201e", "es", "schmiegt", ",", "ver\u00b7bir\u00b7get", "sich", ",", "steckt", "in", "dem", "Wald", "be\u00b7reit", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "VVFIN", "PRF", "$,", "VVFIN", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "330\u201dEh sich der Feind versieht, wirfft es sich in den Streit,", "tokens": ["\"", "Eh", "sich", "der", "Feind", "ver\u00b7sieht", ",", "wirfft", "es", "sich", "in", "den", "Streit", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "PRF", "ART", "NN", "VVFIN", "$,", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}, "line.11": {"text": "\u201eschie\u00dft, hauet, schl\u00e4gt und sticht, st\u00fcrmt, hagelt, st\u00fcrzt und wittert,", "tokens": ["\u201e", "schie\u00dft", ",", "hau\u00b7et", ",", "schl\u00e4gt", "und", "sticht", ",", "st\u00fcrmt", ",", "ha\u00b7gelt", ",", "st\u00fcrzt", "und", "wit\u00b7tert", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201eda\u00df f\u00fcr der Gr\u00e4\u00dflichkeit sich Erd und Luft ersch\u00fcttert.", "tokens": ["\u201e", "da\u00df", "f\u00fcr", "der", "Gr\u00e4\u00df\u00b7lich\u00b7keit", "sich", "Erd", "und", "Luft", "er\u00b7sch\u00fct\u00b7tert", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "APPR", "ART", "NN", "PRF", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "\u201ewie viele des Geschlechts bewaffnen nicht die Faust,", "tokens": ["\u201e", "wie", "vie\u00b7le", "des", "Ge\u00b7schlechts", "be\u00b7waff\u00b7nen", "nicht", "die", "Faust", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PIS", "ART", "NN", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "\u201eda\u00df es den Helden selbst f\u00fcr solchen Helden grau\u00dft?", "tokens": ["\u201e", "da\u00df", "es", "den", "Hel\u00b7den", "selbst", "f\u00fcr", "sol\u00b7chen", "Hel\u00b7den", "grau\u00dft", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ART", "NN", "ADV", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "335\u201dSie lassen Heerd und Pflug auf ihren Feldern stehen,", "tokens": ["\"", "Sie", "las\u00b7sen", "Heerd", "und", "Pflug", "auf", "ih\u00b7ren", "Fel\u00b7dern", "ste\u00b7hen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "\u201ef\u00fcr ihre K\u00f6niginn ins Krieges-Feld zu gehen.", "tokens": ["\u201e", "f\u00fcr", "ih\u00b7re", "K\u00f6\u00b7ni\u00b7ginn", "ins", "Krie\u00b7ge\u00b7sFeld", "zu", "ge\u00b7hen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPOSAT", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "\u201eist Eisen oder Stahl zum Ackerbau geschmidt,", "tokens": ["\u201e", "ist", "Ei\u00b7sen", "o\u00b7der", "Stahl", "zum", "A\u00b7cker\u00b7bau", "ge\u00b7schmidt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "NN", "KON", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "\u201eso wird es hingerafft, es mu\u00df zum K\u00e4mpfen mit.", "tokens": ["\u201e", "so", "wird", "es", "hin\u00b7ge\u00b7rafft", ",", "es", "mu\u00df", "zum", "K\u00e4mp\u00b7fen", "mit", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "VVFIN", "$,", "PPER", "VMFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "\u201eda denckt man nimmermehr den Bauren-Hof zu h\u00fctten;", "tokens": ["\u201e", "da", "denckt", "man", "nim\u00b7mer\u00b7mehr", "den", "Bau\u00b7ren\u00b7Hof", "zu", "h\u00fct\u00b7ten", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PIS", "ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "340\u201dNein: sondern K\u00f6nigen in Schlachten zu gebieten.", "tokens": ["\"", "Nein", ":", "son\u00b7dern", "K\u00f6\u00b7ni\u00b7gen", "in", "Schlach\u00b7ten", "zu", "ge\u00b7bie\u00b7ten", "."], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$.", "KON", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}