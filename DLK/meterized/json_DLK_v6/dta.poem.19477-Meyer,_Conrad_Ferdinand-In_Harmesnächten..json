{"dta.poem.19477": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "In Harmesn\u00e4chten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Die Rechte streckt' ich schmerzlich oft", "tokens": ["Die", "Rech\u00b7te", "streckt'", "ich", "schmerz\u00b7lich", "oft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Harmesn\u00e4chten", "tokens": ["In", "Har\u00b7me\u00b7sn\u00e4ch\u00b7ten"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und f\u00fchlt' gedr\u00fcckt sie unverhofft", "tokens": ["Und", "f\u00fchlt'", "ge\u00b7dr\u00fcckt", "sie", "un\u00b7ver\u00b7hofft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "VVPP", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von einer Rechten \u2014", "tokens": ["Von", "ei\u00b7ner", "Rech\u00b7ten"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Was Gott ist, wird in Ewigkeit", "tokens": ["Was", "Gott", "ist", ",", "wird", "in", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "NN", "VAFIN", "$,", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kein Mensch ergr\u00fcnden,", "tokens": ["Kein", "Mensch", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Doch will er treu sich allezeit", "tokens": ["Doch", "will", "er", "treu", "sich", "al\u00b7le\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ADJD", "PRF", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit uns verb\u00fcnden.", "tokens": ["Mit", "uns", "ver\u00b7b\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVINF", "$."], "meter": "++-+-", "measure": "iambic.di"}}}}}