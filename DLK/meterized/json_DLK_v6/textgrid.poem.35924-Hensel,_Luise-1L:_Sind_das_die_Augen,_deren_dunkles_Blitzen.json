{"textgrid.poem.35924": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sind das die Augen, deren dunkles Blitzen", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sind das die Augen, deren dunkles Blitzen", "tokens": ["Sind", "das", "die", "Au\u00b7gen", ",", "de\u00b7ren", "dunk\u00b7les", "Blit\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PDS", "ART", "NN", "$,", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Einst in das Herz mir drang so tief und klar,", "tokens": ["Einst", "in", "das", "Herz", "mir", "drang", "so", "tief", "und", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PPER", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df ich Maria bat, es zu besch\u00fctzen,", "tokens": ["Da\u00df", "ich", "Ma\u00b7ria", "bat", ",", "es", "zu", "be\u00b7sch\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "VVFIN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Weil's ihrem Kinde schon geopfert war?", "tokens": ["Weil's", "ih\u00b7rem", "Kin\u00b7de", "schon", "ge\u00b7o\u00b7pfert", "war", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wohin die reiche Lockenpracht gestoben,", "tokens": ["Wo\u00b7hin", "die", "rei\u00b7che", "Lo\u00b7cken\u00b7pracht", "ge\u00b7sto\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die fast umstrickte mir den freien Sinn,", "tokens": ["Die", "fast", "um\u00b7strick\u00b7te", "mir", "den", "frei\u00b7en", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als sie die reine, edle Stirn umwoben?", "tokens": ["Als", "sie", "die", "rei\u00b7ne", ",", "ed\u00b7le", "Stirn", "um\u00b7wo\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "$,", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dahin, mit aller Jugendzier dahin! \u2013", "tokens": ["Da\u00b7hin", ",", "mit", "al\u00b7ler", "Ju\u00b7gend\u00b7zier", "da\u00b7hin", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PAV", "$,", "APPR", "PIAT", "NN", "PAV", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}