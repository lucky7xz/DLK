{"textgrid.poem.47603": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[ich gebe dir ein gut Gesetz]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich gebe dir ein gut Gesetz,", "tokens": ["Ich", "ge\u00b7be", "dir", "ein", "gut", "Ge\u00b7setz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein redliches und reines hie:", "tokens": ["Ein", "red\u00b7li\u00b7ches", "und", "rei\u00b7nes", "hie", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ADJA", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Flasche trug ich unter'm Arm,", "tokens": ["Die", "Fla\u00b7sche", "trug", "ich", "un\u00b7ter'm", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da meinte man, es sei ein Buch", "tokens": ["Da", "mein\u00b7te", "man", ",", "es", "sei", "ein", "Buch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "$,", "PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und irrte nicht; ich lerne draus", "tokens": ["Und", "irr\u00b7te", "nicht", ";", "ich", "ler\u00b7ne", "draus"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "$.", "PPER", "VVFIN", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Rhetorik und Philosophie. \u2013", "tokens": ["Rhe\u00b7to\u00b7rik", "und", "Phi\u00b7lo\u00b7so\u00b7phie", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NE", "KON", "NN", "$.", "$("], "meter": "+-+-++-+", "measure": "unknown.measure.penta"}}, "stanza.3": {"line.1": {"text": "Lang an der Schale klebet' ich", "tokens": ["Lang", "an", "der", "Scha\u00b7le", "kle\u00b7bet'", "ich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da zog der Liebe starke Hand", "tokens": ["Da", "zog", "der", "Lie\u00b7be", "star\u00b7ke", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In's flammenhei\u00dfe Centrum mich", "tokens": ["In's", "flam\u00b7men\u00b7hei\u00b7\u00dfe", "Cen\u00b7trum", "mich"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus frostiger Peripherie. \u2013", "tokens": ["Aus", "fros\u00b7ti\u00b7ger", "Pe\u00b7ri\u00b7phe\u00b7rie", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wohin geh\u00f6rst du, strenge Maid?", "tokens": ["Wo\u00b7hin", "ge\u00b7h\u00f6rst", "du", ",", "stren\u00b7ge", "Maid", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In's unbelebte Pflanzenreich.", "tokens": ["In's", "un\u00b7be\u00b7leb\u00b7te", "Pflan\u00b7zen\u00b7reich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Meine, weil sie lebt, verlacht", "tokens": ["Die", "Mei\u00b7ne", ",", "weil", "sie", "lebt", ",", "ver\u00b7lacht"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Sitte dumpfe Despotie. \u2013", "tokens": ["Der", "Sit\u00b7te", "dump\u00b7fe", "Des\u00b7po\u00b7tie", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Des Schelteworts Beleidigung", "tokens": ["Des", "Schel\u00b7te\u00b7worts", "Be\u00b7lei\u00b7di\u00b7gung"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verzeihet dir kein N\u00fcchterner;", "tokens": ["Ver\u00b7zei\u00b7het", "dir", "kein", "N\u00fcch\u00b7ter\u00b7ner", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Mich mag sie treffen ohne Scheu,", "tokens": ["Mich", "mag", "sie", "tref\u00b7fen", "oh\u00b7ne", "Scheu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich Trunkner \u00fcberh\u00f6re sie. \u2013", "tokens": ["Ich", "Trunk\u00b7ner", "\u00fc\u00b7ber\u00b7h\u00f6\u00b7re", "sie", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "NN", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Sch\u00f6n mag es in der H\u00f6he sein,", "tokens": ["Sch\u00f6n", "mag", "es", "in", "der", "H\u00f6\u00b7he", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "APPR", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch auch hier unten ist es sch\u00f6n;", "tokens": ["Doch", "auch", "hier", "un\u00b7ten", "ist", "es", "sch\u00f6n", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Lenz, Liebe, Becher, Lautenklang \u2013", "tokens": ["Lenz", ",", "Lie\u00b7be", ",", "Be\u00b7cher", ",", "Lau\u00b7ten\u00b7klang", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was willst du, da\u00df ich ferne zieh'? \u2013", "tokens": ["Was", "willst", "du", ",", "da\u00df", "ich", "fer\u00b7ne", "zieh'", "?", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VMFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Nicht sprich, Hafis, da\u00df ungerecht", "tokens": ["Nicht", "sprich", ",", "Ha\u00b7fis", ",", "da\u00df", "un\u00b7ge\u00b7recht"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PTKNEG", "ADJD", "$,", "NE", "$,", "KOUS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Welt getheilt! Du hast genug;", "tokens": ["Die", "Welt", "ge\u00b7theilt", "!", "Du", "hast", "ge\u00b7nug", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$.", "PPER", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hast deinen ewig heitern Geist", "tokens": ["Hast", "dei\u00b7nen", "e\u00b7wig", "hei\u00b7tern", "Geist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und deiner Reime Melodie.", "tokens": ["Und", "dei\u00b7ner", "Rei\u00b7me", "Me\u00b7lo\u00b7die", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}