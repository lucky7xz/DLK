{"textgrid.poem.52652": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: J\u00fcngst schaute von Tages Lasten", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "J\u00fcngst schaute von Tages Lasten", "tokens": ["J\u00fcngst", "schau\u00b7te", "von", "Ta\u00b7ges", "Las\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ich m\u00fcd in den Abend hinein,", "tokens": ["Ich", "m\u00fcd", "in", "den", "A\u00b7bend", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Die gl\u00fchenden Strahlen verbla\u00dften", "tokens": ["Die", "gl\u00fc\u00b7hen\u00b7den", "Strah\u00b7len", "ver\u00b7bla\u00df\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "In milden Mondenschein;", "tokens": ["In", "mil\u00b7den", "Mon\u00b7den\u00b7schein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und hoch am Himmelsbogen,", "tokens": ["Und", "hoch", "am", "Him\u00b7mels\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Aus D\u00e4mmrung tauchend empor,", "tokens": ["Aus", "D\u00e4mm\u00b7rung", "tau\u00b7chend", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Kam friedlich angezogen", "tokens": ["Kam", "fried\u00b7lich", "an\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Sterne goldner Chor.", "tokens": ["Der", "Ster\u00b7ne", "gold\u00b7ner", "Chor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mit sittigem Schlummerhaupte", "tokens": ["Mit", "sit\u00b7ti\u00b7gem", "Schlum\u00b7mer\u00b7haup\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Schlie\u00dft sich die Blume zu,", "tokens": ["Schlie\u00dft", "sich", "die", "Blu\u00b7me", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "PTKVZ", "$,"], "meter": "++-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Baum der duftig belaubte", "tokens": ["Der", "Baum", "der", "duf\u00b7tig", "be\u00b7laub\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJD", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die Bl\u00e4tter senkt zur Ruh;", "tokens": ["Die", "Bl\u00e4t\u00b7ter", "senkt", "zur", "Ruh", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Des Abends liebliche Stimmen", "tokens": ["Des", "A\u00b7bends", "lieb\u00b7li\u00b7che", "Stim\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Umspielen der Haine Gr\u00fcn,", "tokens": ["Um\u00b7spie\u00b7len", "der", "Hai\u00b7ne", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Die fernen Berge verschwimmen", "tokens": ["Die", "fer\u00b7nen", "Ber\u00b7ge", "ver\u00b7schwim\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "In sanftes Dunkel hin.", "tokens": ["In", "sanf\u00b7tes", "Dun\u00b7kel", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Des Sommers Herrlichkeiten", "tokens": ["Des", "Som\u00b7mers", "Herr\u00b7lich\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Noch spr\u00fcheten in die Natur,", "tokens": ["Noch", "spr\u00fc\u00b7he\u00b7ten", "in", "die", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Des Lebens Sterne bestreuten", "tokens": ["Des", "Le\u00b7bens", "Ster\u00b7ne", "be\u00b7streu\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mit Segen die stille Flur \u2013", "tokens": ["Mit", "Se\u00b7gen", "die", "stil\u00b7le", "Flur", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und jetzt, da ich trete wieder", "tokens": ["Und", "jetzt", ",", "da", "ich", "tre\u00b7te", "wie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "KOUS", "PPER", "VVFIN", "ADV"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Ans traute Fenster her,", "tokens": ["Ans", "trau\u00b7te", "Fens\u00b7ter", "her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und schaue zur Erde nieder,", "tokens": ["Und", "schau\u00b7e", "zur", "Er\u00b7de", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Liegt sie verwaist und leer.", "tokens": ["Liegt", "sie", "ver\u00b7waist", "und", "leer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Des Herbstes G\u00fcter prangen", "tokens": ["Des", "Herbs\u00b7tes", "G\u00fc\u00b7ter", "pran\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nicht mehr am reichen Ast;", "tokens": ["Nicht", "mehr", "am", "rei\u00b7chen", "Ast", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Festklammernd, kahle Stangen", "tokens": ["Fest\u00b7klam\u00b7mernd", ",", "kah\u00b7le", "Stan\u00b7gen"], "token_info": ["word", "punct", "word", "word"], "pos": ["VVPP", "$,", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vorm Sturme die Reb umfa\u00dft \u2013", "tokens": ["Vorm", "Stur\u00b7me", "die", "Reb", "um\u00b7fa\u00dft", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "So will der Mensch sich halten", "tokens": ["So", "will", "der", "Mensch", "sich", "hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Am armen Menschenkind,", "tokens": ["Am", "ar\u00b7men", "Men\u00b7schen\u00b7kind", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wenn wilde Geschicke schalten", "tokens": ["Wenn", "wil\u00b7de", "Ge\u00b7schi\u00b7cke", "schal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Und nicht zu wenden sind.", "tokens": ["Und", "nicht", "zu", "wen\u00b7den", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der Tod ist kalt und eisig,", "tokens": ["Der", "Tod", "ist", "kalt", "und", "ei\u00b7sig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er kommt in des Winters Kleid \u2013", "tokens": ["Er", "kommt", "in", "des", "Win\u00b7ters", "Kleid", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der Lorbeer wird zum Reisig,", "tokens": ["Der", "Lor\u00b7beer", "wird", "zum", "Rei\u00b7sig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der f\u00fcr die Unsterblichkeit.", "tokens": ["Der", "f\u00fcr", "die", "U\u00b7nsterb\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Die Kr\u00e4nze f\u00fcr dein Streiten", "tokens": ["Die", "Kr\u00e4n\u00b7ze", "f\u00fcr", "dein", "Strei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie schm\u00fcckten sie dich sch\u00f6n!", "tokens": ["Wie", "schm\u00fcck\u00b7ten", "sie", "dich", "sch\u00f6n", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "In ewge Vergessenheiten", "tokens": ["In", "ew\u00b7ge", "Ver\u00b7ges\u00b7sen\u00b7hei\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Entbl\u00e4ttert im Tod sie gehn.", "tokens": ["Ent\u00b7bl\u00e4t\u00b7tert", "im", "Tod", "sie", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Was willst du m\u00fcde Seele", "tokens": ["Was", "willst", "du", "m\u00fc\u00b7de", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und ringst nach Lieb und Licht?", "tokens": ["Und", "ringst", "nach", "Lieb", "und", "Licht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O Menschenseele qu\u00e4le", "tokens": ["O", "Men\u00b7schen\u00b7see\u00b7le", "qu\u00e4\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["NE", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dich um Verlornes nicht!", "tokens": ["Dich", "um", "Ver\u00b7lor\u00b7nes", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du greifst darnach mit H\u00e4nden,", "tokens": ["Du", "greifst", "dar\u00b7nach", "mit", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Freust dich am Lebensroth \u2013", "tokens": ["Freust", "dich", "am", "Le\u00b7bens\u00b7roth", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Bald wird es dich verblenden", "tokens": ["Bald", "wird", "es", "dich", "ver\u00b7blen\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und du bist blind und todt.", "tokens": ["Und", "du", "bist", "blind", "und", "todt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}