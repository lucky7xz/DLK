{"textgrid.poem.48670": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "33. Sehnsucht nach Elsgen", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erbarme du dich meiner Qualen,", "tokens": ["Er\u00b7bar\u00b7me", "du", "dich", "mei\u00b7ner", "Qua\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "du dicker w\u00fcster Hain,", "tokens": ["du", "di\u00b7cker", "w\u00fcs\u00b7ter", "Hain", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "dem Titans allerhellste Strahlen", "tokens": ["dem", "Ti\u00b7tans", "al\u00b7ler\u00b7hells\u00b7te", "Strah\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "doch geben keinen Schein.", "tokens": ["doch", "ge\u00b7ben", "kei\u00b7nen", "Schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie dunkel hier ist deine schwarze H\u00f6le,", "tokens": ["Wie", "dun\u00b7kel", "hier", "ist", "dei\u00b7ne", "schwar\u00b7ze", "H\u00f6\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "so finster auch ist meine kranke Seele.", "tokens": ["so", "fins\u00b7ter", "auch", "ist", "mei\u00b7ne", "kran\u00b7ke", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "La\u00df unter deinem stillen Schatten", "tokens": ["La\u00df", "un\u00b7ter", "dei\u00b7nem", "stil\u00b7len", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "mein Klagen sicher gehn", "tokens": ["mein", "Kla\u00b7gen", "si\u00b7cher", "gehn"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJD", "VVINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "und h\u00f6re meinen Sinn, den matten,", "tokens": ["und", "h\u00f6\u00b7re", "mei\u00b7nen", "Sinn", ",", "den", "mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "sein Leidlied recht erh\u00f6hn,", "tokens": ["sein", "Leid\u00b7lied", "recht", "er\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "den armen Sinn, der seinen Ha\u00df auch liebet,", "tokens": ["den", "ar\u00b7men", "Sinn", ",", "der", "sei\u00b7nen", "Ha\u00df", "auch", "lie\u00b7bet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "den nichts erfreut, als da\u00df er ist betr\u00fcbet.", "tokens": ["den", "nichts", "er\u00b7freut", ",", "als", "da\u00df", "er", "ist", "be\u00b7tr\u00fc\u00b7bet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,", "KOKOM", "KOUS", "PPER", "VAFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So mu\u00df ich Toter dennoch leben?", "tokens": ["So", "mu\u00df", "ich", "To\u00b7ter", "den\u00b7noch", "le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ach! kan di\u00df m\u00fcglich sein?", "tokens": ["Ach", "!", "kan", "di\u00df", "m\u00fcg\u00b7lich", "sein", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VMFIN", "PDS", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was meiner Seelen Trost soll geben,", "tokens": ["Was", "mei\u00b7ner", "See\u00b7len", "Trost", "soll", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "das selbst ist ihre Pein.", "tokens": ["das", "selbst", "ist", "ih\u00b7re", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ach mir! was ists vor ein verkehrtes Wesen,", "tokens": ["Ach", "mir", "!", "was", "ists", "vor", "ein", "ver\u00b7kehr\u00b7tes", "We\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "$.", "PWS", "VAFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "das mich bringt um, von dem will ich genesen.", "tokens": ["das", "mich", "bringt", "um", ",", "von", "dem", "will", "ich", "ge\u00b7ne\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "PTKVZ", "$,", "APPR", "PDS", "VMFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+----", "measure": "unknown.measure.tetra"}}, "stanza.4": {"line.1": {"text": "Glaubts, wo ihrs anders k\u00f6nnt verstehen,", "tokens": ["Glaubts", ",", "wo", "ihrs", "an\u00b7ders", "k\u00f6nnt", "ver\u00b7ste\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "ADV", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "ihr Bl\u00e4tter ingesammt,", "tokens": ["ihr", "Bl\u00e4t\u00b7ter", "in\u00b7ge\u00b7sammt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "der Pein mu\u00df eure Zahl nachgehen,", "tokens": ["der", "Pein", "mu\u00df", "eu\u00b7re", "Zahl", "nach\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "in die ich bin verdammt.", "tokens": ["in", "die", "ich", "bin", "ver\u00b7dammt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.6": {"text": "als \u00c4ngste mir an meine Seele klopfen.", "tokens": ["als", "\u00c4ngs\u00b7te", "mir", "an", "mei\u00b7ne", "See\u00b7le", "klop\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Es m\u00f6chte m\u00fcglich sein zu messen", "tokens": ["Es", "m\u00f6ch\u00b7te", "m\u00fcg\u00b7lich", "sein", "zu", "mes\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADJD", "VAINF", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "die Flut der ", "tokens": ["die", "Flut", "der"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "zu z\u00e4hlen wie viel Bienen essen", "tokens": ["zu", "z\u00e4h\u00b7len", "wie", "viel", "Bie\u00b7nen", "es\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "KOKOM", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "von Hyblens s\u00fc\u00dfem Klee,", "tokens": ["von", "Hyb\u00b7lens", "s\u00fc\u00b7\u00dfem", "Klee", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "nur meine Pein, ein Ding auf aller Erden", "tokens": ["nur", "mei\u00b7ne", "Pein", ",", "ein", "Ding", "auf", "al\u00b7ler", "Er\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "$,", "ART", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "kan nicht gez\u00e4hlt, kan nicht gemessen werden.", "tokens": ["kan", "nicht", "ge\u00b7z\u00e4hlt", ",", "kan", "nicht", "ge\u00b7mes\u00b7sen", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "VVPP", "$,", "VMFIN", "PTKNEG", "VVPP", "VAINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.6": {"line.1": {"text": "Naturlich ists, da\u00df stetigs Klagen", "tokens": ["Na\u00b7tur\u00b7lich", "ists", ",", "da\u00df", "ste\u00b7tigs", "Kla\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "$,", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "uns endlich alle macht.", "tokens": ["uns", "end\u00b7lich", "al\u00b7le", "macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PIS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich werd' erquickt durch ewigs Plagen", "tokens": ["Ich", "werd'", "er\u00b7quickt", "durch", "e\u00b7wigs", "Pla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "und will sein umgebracht.", "tokens": ["und", "will", "sein", "um\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "La\u00df seh'n, ob ich durch Freude denn kan sterben,", "tokens": ["La\u00df", "seh'n", ",", "ob", "ich", "durch", "Freu\u00b7de", "denn", "kan", "ster\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "$,", "KOUS", "PPER", "APPR", "NN", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "dieweil kein Leid mich doch nicht kan verderben.", "tokens": ["die\u00b7weil", "kein", "Leid", "mich", "doch", "nicht", "kan", "ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "PPER", "ADV", "PTKNEG", "VMFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}