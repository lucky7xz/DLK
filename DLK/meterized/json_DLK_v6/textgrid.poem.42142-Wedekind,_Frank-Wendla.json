{"textgrid.poem.42142": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Wendla", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieh die taufrische Maid,", "tokens": ["Sieh", "die", "tauf\u00b7ri\u00b7sche", "Maid", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Erst eben erbl\u00fcht;", "tokens": ["Erst", "e\u00b7ben", "er\u00b7bl\u00fcht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Durch ihr knappkurzes Kleid", "tokens": ["Durch", "ihr", "knapp\u00b7kur\u00b7zes", "Kleid"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Morgenwind zieht.", "tokens": ["Der", "Mor\u00b7gen\u00b7wind", "zieht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Wie schreitet sie r\u00fcstig,", "tokens": ["Wie", "schrei\u00b7tet", "sie", "r\u00fcs\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Jubiliert und frohlockt,", "tokens": ["Ju\u00b7bi\u00b7liert", "und", "froh\u00b7lockt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und ahnt nicht, wer listig", "tokens": ["Und", "ahnt", "nicht", ",", "wer", "lis\u00b7tig"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "PWS", "ADJD"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Unterm Taxusbusch hockt.", "tokens": ["Un\u00b7term", "Ta\u00b7xus\u00b7busch", "hockt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Der allerfrechste Weidmann", "tokens": ["Der", "al\u00b7ler\u00b7frechs\u00b7te", "Weid\u00b7mann"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im ganzen Revier,", "tokens": ["Im", "gan\u00b7zen", "Re\u00b7vier", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Er tut ihr ein Leid an", "tokens": ["Er", "tut", "ihr", "ein", "Leid", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In frevler Jagdbegier.", "tokens": ["In", "frev\u00b7ler", "Jagd\u00b7be\u00b7gier", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "In einem langen Kleide", "tokens": ["In", "ei\u00b7nem", "lan\u00b7gen", "Klei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Geht sie nun bald einher,", "tokens": ["Geht", "sie", "nun", "bald", "ein\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sinnt vergangener Zeiten", "tokens": ["Sinnt", "ver\u00b7gan\u00b7ge\u00b7ner", "Zei\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Und jubelt nicht mehr.", "tokens": ["Und", "ju\u00b7belt", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}