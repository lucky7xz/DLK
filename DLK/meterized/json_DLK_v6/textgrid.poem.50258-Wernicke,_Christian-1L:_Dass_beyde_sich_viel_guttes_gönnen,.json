{"textgrid.poem.50258": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dass beyde sich viel guttes g\u00f6nnen,", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dass beyde sich viel guttes g\u00f6nnen,", "tokens": ["Dass", "bey\u00b7de", "sich", "viel", "gut\u00b7tes", "g\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "PIAT", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das geben sie, so viel sie k\u00f6nnen,", "tokens": ["Das", "ge\u00b7ben", "sie", ",", "so", "viel", "sie", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$,", "ADV", "ADV", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sich durch die ", "tokens": ["Sich", "durch", "die"], "token_info": ["word", "word", "word"], "pos": ["PRF", "APPR", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Die sieht man hin und wieder gehn,", "tokens": ["Die", "sieht", "man", "hin", "und", "wie\u00b7der", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "PTKVZ", "KON", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch sprechen sie kein eintzig Wort,", "tokens": ["Doch", "spre\u00b7chen", "sie", "kein", "eint\u00b7zig", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn sie sich sehn in ", "tokens": ["Wenn", "sie", "sich", "sehn", "in"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "APPR"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Und sie ", "tokens": ["Und", "sie"], "token_info": ["word", "word"], "pos": ["KON", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "So hab ich manchesmahl geh\u00f6rt,", "tokens": ["So", "hab", "ich", "man\u00b7ches\u00b7mahl", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}