, "PTKVZ", "$,", "FM", "NE", "$,", "ADV", "PPER", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Dich huben sie hervor, und du stehest nun da!", "tokens": ["Dich", "hu\u00b7ben", "sie", "her\u00b7vor", ",", "und", "du", "ste\u00b7hest", "nun", "da", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "PPER", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Gleichest dich dem Griechen! trotzest ihm!", "tokens": ["Glei\u00b7chest", "dich", "dem", "Grie\u00b7chen", "!", "trot\u00b7zest", "ihm", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$.", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und fragst, ob wie du er entflamme den Gesang?", "tokens": ["Und", "fragst", ",", "ob", "wie", "du", "er", "ent\u00b7flam\u00b7me", "den", "Ge\u00b7sang", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "KOKOM", "PPER", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.15": {"line.1": {"text": "Voll Gedanken auf der Stirne h\u00f6ret' ihn Apoll,", "tokens": ["Voll", "Ge\u00b7dan\u00b7ken", "auf", "der", "Stir\u00b7ne", "h\u00f6\u00b7ret'", "ihn", "A\u00b7poll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "ART", "NN", "VVFIN", "PPER", "NE", "$,"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Und sprach nicht! und gelehnt auf die Harfe Walhalls", "tokens": ["Und", "sprach", "nicht", "!", "und", "ge\u00b7lehnt", "auf", "die", "Har\u00b7fe", "Wal\u00b7halls"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "$.", "KON", "VVPP", "APPR", "ART", "NN", "NE"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Stellt sich vor Apollo Bragor hin,", "tokens": ["Stellt", "sich", "vor", "A\u00b7pol\u00b7lo", "Bra\u00b7gor", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NE", "NE", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und l\u00e4chelt, und schweiget, und z\u00fcrnet nicht auf ihn.", "tokens": ["Und", "l\u00e4\u00b7chelt", ",", "und", "schwei\u00b7get", ",", "und", "z\u00fcr\u00b7net", "nicht", "auf", "ihn", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KON", "VVFIN", "$,", "KON", "VVFIN", "PTKNEG", "APPR", "PPER", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}}}}