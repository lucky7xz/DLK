{"textgrid.poem.57292": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: An der H\u00f6he, wo der Quell der Barden in das Thal", "genre": "verse", "period": "N.A.", "pub_year": 1767, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An der H\u00f6he, wo der Quell der Barden in das Thal", "tokens": ["An", "der", "H\u00f6\u00b7he", ",", "wo", "der", "Quell", "der", "Bar\u00b7den", "in", "das", "Thal"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "ART", "NN", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Sein fliegendes Get\u00f6ne, mit Silber bew\u00f6lkt,", "tokens": ["Sein", "flie\u00b7gen\u00b7des", "Ge\u00b7t\u00f6\u00b7ne", ",", "mit", "Sil\u00b7ber", "be\u00b7w\u00f6lkt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "St\u00fcrzet, da erblickt' ich, zeug' es, Hain!", "tokens": ["St\u00fcr\u00b7zet", ",", "da", "er\u00b7blickt'", "ich", ",", "zeug'", "es", ",", "Hain", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Die G\u00f6ttin! sie kam zu dem Sterblichen herab!", "tokens": ["Die", "G\u00f6t\u00b7tin", "!", "sie", "kam", "zu", "dem", "Sterb\u00b7li\u00b7chen", "her\u00b7ab", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PPER", "VVFIN", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Und mit Hoheit in der Mine stand sie! und ich sah", "tokens": ["Und", "mit", "Ho\u00b7heit", "in", "der", "Mi\u00b7ne", "stand", "sie", "!", "und", "ich", "sah"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "APPR", "ART", "NN", "VVFIN", "PPER", "$.", "KON", "PPER", "VVFIN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Die Geister um sie her, die den Liedern entlockt", "tokens": ["Die", "Geis\u00b7ter", "um", "sie", "her", ",", "die", "den", "Lie\u00b7dern", "ent\u00b7lockt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPER", "PTKVZ", "$,", "PRELS", "ART", "NN", "VVPP"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "T\u00e4uschen, ihr Gebild. Die Wurdi's Dolch", "tokens": ["T\u00e4u\u00b7schen", ",", "ihr", "Ge\u00b7bild", ".", "Die", "Wur\u00b7di's", "Dolch"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "NN", "$.", "ART", "NN", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Unschuldige traf, die begleiteten sie fern,", "tokens": ["Un\u00b7schul\u00b7di\u00b7ge", "traf", ",", "die", "be\u00b7glei\u00b7te\u00b7ten", "sie", "fern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PRELS", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Wie in D\u00e4mrung; und die Skulda's m\u00e4chtigerer Stab", "tokens": ["Wie", "in", "D\u00e4m\u00b7rung", ";", "und", "die", "Skulda's", "m\u00e4ch\u00b7ti\u00b7ge\u00b7rer", "Stab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "$.", "KON", "ART", "NN", "ADJA", "NN"], "meter": "--+-+-++-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Errettete, die schwebten umher in Triumph,", "tokens": ["Er\u00b7ret\u00b7te\u00b7te", ",", "die", "schweb\u00b7ten", "um\u00b7her", "in", "Tri\u00b7umph", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "VVFIN", "PTKVZ", "APPR", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Schimmernd, um die G\u00f6ttin, hatten stolz", "tokens": ["Schim\u00b7mernd", ",", "um", "die", "G\u00f6t\u00b7tin", ",", "hat\u00b7ten", "stolz"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVPP", "$,", "KOUI", "ART", "NN", "$,", "VAFIN", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Mit Laube der Eiche die Schl\u00e4fe sich bekr\u00e4nzt!", "tokens": ["Mit", "Lau\u00b7be", "der", "Ei\u00b7che", "die", "Schl\u00e4\u00b7fe", "sich", "be\u00b7kr\u00e4nzt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "ART", "NN", "PRF", "VVPP", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Den Gedanken, die Empfindung, treffend, und mit Kraft,", "tokens": ["Den", "Ge\u00b7dan\u00b7ken", ",", "die", "Emp\u00b7fin\u00b7dung", ",", "tref\u00b7fend", ",", "und", "mit", "Kraft", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ADJD", "$,", "KON", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Mit Wendungen der K\u00fchnheit, zu sagen! das ist,", "tokens": ["Mit", "Wen\u00b7dun\u00b7gen", "der", "K\u00fchn\u00b7heit", ",", "zu", "sa\u00b7gen", "!", "das", "ist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,", "PTKZU", "VVINF", "$.", "PDS", "VAFIN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "Sprache des Thuiskon, G\u00f6ttin, dir,", "tokens": ["Spra\u00b7che", "des", "Thu\u00b7is\u00b7kon", ",", "G\u00f6t\u00b7tin", ",", "dir", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "NN", "$,", "PPER", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Wie unseren Helden Eroberung, ein Spiel!", "tokens": ["Wie", "un\u00b7se\u00b7ren", "Hel\u00b7den", "Er\u00b7o\u00b7be\u00b7rung", ",", "ein", "Spiel", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "NN", "$,", "ART", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.5": {"line.1": {"text": "O Begeistrung! Sie erhebt sich! Feurigeres Blicks", "tokens": ["O", "Be\u00b7geis\u00b7trung", "!", "Sie", "er\u00b7hebt", "sich", "!", "Feu\u00b7ri\u00b7ge\u00b7res", "Blicks"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$.", "PPER", "VVFIN", "PRF", "$.", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Ergiesset sich ihr Auge, die Seel' in der Glut!", "tokens": ["Er\u00b7gies\u00b7set", "sich", "ihr", "Au\u00b7ge", ",", "die", "Seel'", "in", "der", "Glut", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "$,", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Str\u00f6me! denn du schonest dess umsonst,", "tokens": ["Str\u00f6\u00b7me", "!", "denn", "du", "scho\u00b7nest", "dess", "um\u00b7sonst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KON", "PPER", "VVFIN", "PDS", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Der, leer des Gef\u00fchls, den Gedanken nicht erreicht!", "tokens": ["Der", ",", "leer", "des", "Ge\u00b7f\u00fchls", ",", "den", "Ge\u00b7dan\u00b7ken", "nicht", "er\u00b7reicht", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADJD", "ART", "NN", "$,", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.6": {"line.1": {"text": "Wie sie herschwebt an des Quells Fall! M\u00e4chtiges Get\u00f6n,", "tokens": ["Wie", "sie", "her\u00b7schwebt", "an", "des", "Quells", "Fall", "!", "M\u00e4ch\u00b7ti\u00b7ges", "Ge\u00b7t\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$.", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Wie Rauschen im Beginne des Walds ist ihr Schwung!", "tokens": ["Wie", "Rau\u00b7schen", "im", "Be\u00b7gin\u00b7ne", "des", "Walds", "ist", "ihr", "Schwung", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPRART", "NN", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Draussen um die Felsen braust der Sturm!", "tokens": ["Draus\u00b7sen", "um", "die", "Fel\u00b7sen", "braust", "der", "Sturm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Gern h\u00f6ret der Wandrer das Rauschen in dem Wald!", "tokens": ["Gern", "h\u00f6\u00b7ret", "der", "Wand\u00b7rer", "das", "Rau\u00b7schen", "in", "dem", "Wald", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.7": {"line.1": {"text": "Wie sie schwebet an der Quelle! Sanfteres Get\u00f6n,", "tokens": ["Wie", "sie", "schwe\u00b7bet", "an", "der", "Quel\u00b7le", "!", "Sanf\u00b7te\u00b7res", "Ge\u00b7t\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "APPR", "ART", "NN", "$.", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Wie Wehen in dem tieferen Wald' ist ihr Schwung.", "tokens": ["Wie", "We\u00b7hen", "in", "dem", "tie\u00b7fe\u00b7ren", "Wald'", "ist", "ihr", "Schwung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Draussen um die Felsen braust der Sturm!", "tokens": ["Draus\u00b7sen", "um", "die", "Fel\u00b7sen", "braust", "der", "Sturm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Gern h\u00f6ret im Walde der Wanderer das Wehn.", "tokens": ["Gern", "h\u00f6\u00b7ret", "im", "Wal\u00b7de", "der", "Wan\u00b7de\u00b7rer", "das", "Wehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.8": {"line.1": {"text": "Die der Fremdling nicht entweiht, (Teutonien erlag", "tokens": ["Die", "der", "Fremd\u00b7ling", "nicht", "ent\u00b7weiht", ",", "(", "Teu\u00b7to\u00b7ni\u00b7en", "er\u00b7lag"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["ART", "ART", "NN", "PTKNEG", "VVFIN", "$,", "$(", "NE", "VVFIN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Nur Siegen, unerobert!) o freyere, dich", "tokens": ["Nur", "Sie\u00b7gen", ",", "un\u00b7er\u00b7o\u00b7bert", "!", ")", "o", "frey\u00b7e\u00b7re", ",", "dich"], "token_info": ["word", "word", "punct", "word", "punct", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "NN", "$,", "ADJD", "$.", "$(", "FM", "ADJA", "$,", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wagte der geschreckten Fessel nicht", "tokens": ["Wag\u00b7te", "der", "ge\u00b7schreck\u00b7ten", "Fes\u00b7sel", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKNEG"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Zu fesseln! Die Adler entflogen, und du bliebst,", "tokens": ["Zu", "fes\u00b7seln", "!", "Die", "Ad\u00b7ler", "ent\u00b7flo\u00b7gen", ",", "und", "du", "bliebst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$.", "ART", "NN", "VVPP", "$,", "KON", "PPER", "VVFIN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.9": {"line.1": {"text": "Die du warest! An dem Rhodan klirret sie noch laut", "tokens": ["Die", "du", "wa\u00b7rest", "!", "An", "dem", "Rho\u00b7dan", "klir\u00b7ret", "sie", "noch", "laut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VAFIN", "$.", "APPR", "ART", "NN", "VVFIN", "PPER", "ADV", "ADJD"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Die Kette des Eroberers! laut am Ibeer!", "tokens": ["Die", "Ket\u00b7te", "des", "Er\u00b7o\u00b7be\u00b7rers", "!", "laut", "am", "I\u00b7beer", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$.", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Also, o Britanne, schalt dir noch", "tokens": ["Al\u00b7so", ",", "o", "Bri\u00b7tan\u00b7ne", ",", "schalt", "dir", "noch"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "FM", "NN", "$,", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Der Angel und Sachse mit herschendem Geklirr!", "tokens": ["Der", "An\u00b7gel", "und", "Sach\u00b7se", "mit", "her\u00b7schen\u00b7dem", "Ge\u00b7klirr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.10": {"line.1": {"text": "So bezwang nicht an des Rheins Strom Romulus Geschlecht!", "tokens": ["So", "be\u00b7zwang", "nicht", "an", "des", "Rheins", "Strom", "Ro\u00b7mu\u00b7lus", "Ge\u00b7schlecht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "APPR", "ART", "NE", "NE", "NE", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Entscheidungen, Vergeltungen sprachen wir aus,", "tokens": ["Ent\u00b7schei\u00b7dun\u00b7gen", ",", "Ver\u00b7gel\u00b7tun\u00b7gen", "spra\u00b7chen", "wir", "aus", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "Rache, mit des Deutschen Schwert, und Wort!", "tokens": ["Ra\u00b7che", ",", "mit", "des", "Deut\u00b7schen", "Schwert", ",", "und", "Wort", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "ADJA", "NN", "$,", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Die Kette verstumte mit Varus in dem Blut!", "tokens": ["Die", "Ket\u00b7te", "ver\u00b7stum\u00b7te", "mit", "Va\u00b7rus", "in", "dem", "Blut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NE", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.11": {"line.1": {"text": "Die dich damals mit erhielten, Sprache, da im Forst", "tokens": ["Die", "dich", "da\u00b7mals", "mit", "er\u00b7hiel\u00b7ten", ",", "Spra\u00b7che", ",", "da", "im", "Forst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "APPR", "VVFIN", "$,", "NN", "$,", "KOUS", "APPRART", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Der Weser die Erobererkette versank,", "tokens": ["Der", "We\u00b7ser", "die", "Er\u00b7o\u00b7be\u00b7rer\u00b7ket\u00b7te", "ver\u00b7sank", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Schweigend in der Legionen Blut", "tokens": ["Schwei\u00b7gend", "in", "der", "Le\u00b7gi\u00b7o\u00b7nen", "Blut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Versank, sie umh\u00fcllt die Vergessenheit mit Nacht!", "tokens": ["Ver\u00b7sank", ",", "sie", "um\u00b7h\u00fcllt", "die", "Ver\u00b7ges\u00b7sen\u00b7heit", "mit", "Nacht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.12": {"line.1": {"text": "Ah die Geister der Bardiete, welche sie zur Schlacht", "tokens": ["Ah", "die", "Geis\u00b7ter", "der", "Bar\u00b7die\u00b7te", ",", "wel\u00b7che", "sie", "zur", "Schlacht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ART", "NN", "$,", "PRELS", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Ert\u00f6neten dem z\u00fcrnenden Vaterlandsheer,", "tokens": ["Er\u00b7t\u00f6\u00b7ne\u00b7ten", "dem", "z\u00fcr\u00b7nen\u00b7den", "Va\u00b7ter\u00b7lands\u00b7heer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Folgen mit der Todeswunde dir!", "tokens": ["Fol\u00b7gen", "mit", "der", "To\u00b7des\u00b7wun\u00b7de", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Ha Norne, dein Dolch! Wirst auch diesen, so sie klagt", "tokens": ["Ha", "Nor\u00b7ne", ",", "dein", "Dolch", "!", "Wirst", "auch", "die\u00b7sen", ",", "so", "sie", "klagt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "PPOSAT", "NN", "$.", "VAFIN", "ADV", "PDAT", "$,", "ADV", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"tex