{"textgrid.poem.55957": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Selbstbildnis aus dem Jahre 1906", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des alten lange adligen Geschlechtes", "tokens": ["Des", "al\u00b7ten", "lan\u00b7ge", "ad\u00b7li\u00b7gen", "Ge\u00b7schlech\u00b7tes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Feststehendes im Augenbogenbau.", "tokens": ["Fest\u00b7ste\u00b7hen\u00b7des", "im", "Au\u00b7gen\u00b7bo\u00b7gen\u00b7bau", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Im Blicke noch der Kindheit Angst und Blau", "tokens": ["Im", "Bli\u00b7cke", "noch", "der", "Kind\u00b7heit", "Angst", "und", "Blau"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "und Demut da und dort, nicht eines Knechtes", "tokens": ["und", "De\u00b7mut", "da", "und", "dort", ",", "nicht", "ei\u00b7nes", "Knech\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "ADV", "KON", "ADV", "$,", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "doch eines Dienenden und einer Frau.", "tokens": ["doch", "ei\u00b7nes", "Die\u00b7nen\u00b7den", "und", "ei\u00b7ner", "Frau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der Mund als Mund gemacht, gro\u00df und genau,", "tokens": ["Der", "Mund", "als", "Mund", "ge\u00b7macht", ",", "gro\u00df", "und", "ge\u00b7nau", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOUS", "NN", "VVPP", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "nicht \u00fcberredend, aber ein Gerechtes", "tokens": ["nicht", "\u00fc\u00b7berr\u00b7e\u00b7dend", ",", "a\u00b7ber", "ein", "Ge\u00b7rech\u00b7tes"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "VVPP", "$,", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Aussagendes. Die Stirne ohne Schlechtes", "tokens": ["Aus\u00b7sa\u00b7gen\u00b7des", ".", "Die", "Stir\u00b7ne", "oh\u00b7ne", "Schlech\u00b7tes"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "und gern im Schatten stiller Niederschau.", "tokens": ["und", "gern", "im", "Schat\u00b7ten", "stil\u00b7ler", "Nie\u00b7der\u00b7schau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das, als Zusammenhang, erst nur geahnt;", "tokens": ["Das", ",", "als", "Zu\u00b7sam\u00b7men\u00b7hang", ",", "erst", "nur", "ge\u00b7ahnt", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "KOUS", "NN", "$,", "ADV", "ADV", "VVPP", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "noch nie im Leiden oder im Gelingen", "tokens": ["noch", "nie", "im", "Lei\u00b7den", "o\u00b7der", "im", "Ge\u00b7lin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPRART", "NN", "KON", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "zusammgefa\u00dft zu dauerndem Durchdringen,", "tokens": ["zu\u00b7samm\u00b7ge\u00b7fa\u00dft", "zu", "dau\u00b7ern\u00b7dem", "Durch\u00b7drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "doch so, als w\u00e4re mit zerstreuten Dingen", "tokens": ["doch", "so", ",", "als", "w\u00e4\u00b7re", "mit", "zer\u00b7streu\u00b7ten", "Din\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "KOKOM", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "von fern ein Ernstes, Wirkliches geplant.", "tokens": ["von", "fern", "ein", "Erns\u00b7tes", ",", "Wirk\u00b7li\u00b7ches", "ge\u00b7plant", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ART", "NN", "$,", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}