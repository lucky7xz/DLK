{"textgrid.poem.50590": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Die leblose Sch\u00f6nheit", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So schl\u00e4frig, doch so sch\u00f6n! Kann man den Augen trauen?", "tokens": ["So", "schl\u00e4f\u00b7rig", ",", "doch", "so", "sch\u00f6n", "!", "Kann", "man", "den", "Au\u00b7gen", "trau\u00b7en", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADV", "ADJD", "$.", "VMFIN", "PIS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist Chloris ein Gesch\u00f6pf? Wie? Oder ist sie nur", "tokens": ["Ist", "Chlo\u00b7ris", "ein", "Ge\u00b7sch\u00f6pf", "?", "Wie", "?", "O\u00b7der", "ist", "sie", "nur"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "NE", "ART", "NN", "$.", "PWAV", "$.", "NE", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein ungemeines Bild? Denn was wir an ihr schauen,", "tokens": ["Ein", "un\u00b7ge\u00b7mei\u00b7nes", "Bild", "?", "Denn", "was", "wir", "an", "ihr", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "KON", "PWS", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ist f\u00fcr die Kunst zu gro\u00df, zu schwach f\u00fcr die Natur.", "tokens": ["Ist", "f\u00fcr", "die", "Kunst", "zu", "gro\u00df", ",", "zu", "schwach", "f\u00fcr", "die", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "PTKA", "ADJD", "$,", "PTKA", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wollt' etwa die Natur des Malers Hand nachaffen?", "tokens": ["Wollt'", "et\u00b7wa", "die", "Na\u00b7tur", "des", "Ma\u00b7lers", "Hand", "nach\u00b7af\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Hat sie mit gleicher M\u00fcnz' ein K\u00fcnstler hier bezahlt?", "tokens": ["Hat", "sie", "mit", "glei\u00b7cher", "M\u00fcnz'", "ein", "K\u00fcnst\u00b7ler", "hier", "be\u00b7zahlt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So hat sie die ", "tokens": ["So", "hat", "sie", "die"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "So hat ein ", "tokens": ["So", "hat", "ein"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VAFIN", "ART"], "meter": "+--", "measure": "dactylic.init"}}}}}