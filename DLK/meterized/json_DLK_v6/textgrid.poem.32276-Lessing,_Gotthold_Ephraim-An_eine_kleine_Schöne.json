{"textgrid.poem.32276": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "An eine kleine Sch\u00f6ne", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kleine Sch\u00f6ne, k\u00fcsse mich.", "tokens": ["Klei\u00b7ne", "Sch\u00f6\u00b7ne", ",", "k\u00fcs\u00b7se", "mich", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kleine Sch\u00f6ne, sch\u00e4mst du dich?", "tokens": ["Klei\u00b7ne", "Sch\u00f6\u00b7ne", ",", "sch\u00e4mst", "du", "dich", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00fcsse geben, K\u00fcsse nehmen,", "tokens": ["K\u00fcs\u00b7se", "ge\u00b7ben", ",", "K\u00fcs\u00b7se", "neh\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Darf dich jetzo nicht besch\u00e4men.", "tokens": ["Darf", "dich", "jet\u00b7zo", "nicht", "be\u00b7sch\u00e4\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "K\u00fcsse mich noch hundertmal!", "tokens": ["K\u00fcs\u00b7se", "mich", "noch", "hun\u00b7dert\u00b7mal", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "K\u00fc\u00df' und merk' der K\u00fcsse Zahl.", "tokens": ["K\u00fc\u00df'", "und", "merk'", "der", "K\u00fcs\u00b7se", "Zahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "KON", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ich will dir, bei meinem Leben!", "tokens": ["Ich", "will", "dir", ",", "bei", "mei\u00b7nem", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Alle zehnfach wiedergeben,", "tokens": ["Al\u00b7le", "zehn\u00b7fach", "wie\u00b7der\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wenn der Ku\u00df kein Scherz mehr ist,", "tokens": ["Wenn", "der", "Ku\u00df", "kein", "Scherz", "mehr", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PIAT", "NN", "ADV", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Und du zehn Jahr \u00e4lter bist.", "tokens": ["Und", "du", "zehn", "Jahr", "\u00e4l\u00b7ter", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "CARD", "NN", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}