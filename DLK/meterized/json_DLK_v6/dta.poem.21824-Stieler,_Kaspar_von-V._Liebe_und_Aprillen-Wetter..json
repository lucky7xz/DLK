{"dta.poem.21824": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "V.  \n  Liebe und Aprillen-Wetter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Sjsyfus Gebirg' erreichen/", "tokens": ["Sjsy\u00b7fus", "Ge\u00b7bir\u00b7g'", "er\u00b7rei\u00b7chen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVINF", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Tantals Weger-trank erstehn/", "tokens": ["Tan\u00b7tals", "We\u00b7ger\u00b7trank", "er\u00b7stehn", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "auff dem Schlangen-rad\u2019 erbleichen", "tokens": ["auff", "dem", "Schlan\u00b7gen\u00b7rad'", "er\u00b7blei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "tausend Martern vor sich sehn:", "tokens": ["tau\u00b7send", "Mar\u00b7tern", "vor", "sich", "sehn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "PRF", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "ist Amors grimme Dienstbarkeit", "tokens": ["ist", "A\u00b7mors", "grim\u00b7me", "Dienst\u00b7bar\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "NE", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "die Kette der bejungten Zeit.", "tokens": ["die", "Ket\u00b7te", "der", "be\u00b7jung\u00b7ten", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ach/ da\u00df ich in Fr\u00fchlings-Jahren/", "tokens": ["Ach", "/", "da\u00df", "ich", "in", "Fr\u00fch\u00b7lings\u00b7Jah\u00b7ren", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$(", "KOUS", "PPER", "APPR", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "mu\u00df solchen Zwang erfahren.", "tokens": ["mu\u00df", "sol\u00b7chen", "Zwang", "er\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Weint nu mit mir/ die Jhr sahet/", "tokens": ["Weint", "nu", "mit", "mir", "/", "die", "Ihr", "sa\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPER", "$(", "PRELS", "PPER", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "wie ich vor begl\u00fckket stund\u2019", "tokens": ["wie", "ich", "vor", "be\u00b7gl\u00fck\u00b7ket", "stund'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "VVFIN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "als mein Kind sich zu mir nahet\u2019", "tokens": ["als", "mein", "Kind", "sich", "zu", "mir", "na\u00b7het'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "APPR", "PPER", "VVFIN"], "meter": "+-+-++-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "und mir boht den Rosen-mund:", "tokens": ["und", "mir", "boht", "den", "Ro\u00b7sen\u00b7mund", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jezt heget sie den Wankel-sinn/", "tokens": ["Jezt", "he\u00b7get", "sie", "den", "Wan\u00b7kel\u00b7sinn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Meiner Liebe treues Feuer", "tokens": ["Mei\u00b7ner", "Lie\u00b7be", "treu\u00b7es", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "k\u00f6mmt mir nu schlecht zu steuer.", "tokens": ["k\u00f6mmt", "mir", "nu", "schlecht", "zu", "steu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "PTKA", "ADJD", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Selbst der Neid hat sich verwundert", "tokens": ["Selbst", "der", "Neid", "hat", "sich", "ver\u00b7wun\u00b7dert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VAFIN", "PRF", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "wenn der Liebe Funken-gluht", "tokens": ["wenn", "der", "Lie\u00b7be", "Fun\u00b7ken\u00b7gluht"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "in der Liebsten Herzen zundert\u2019", "tokens": ["in", "der", "Liebs\u00b7ten", "Her\u00b7zen", "zun\u00b7dert'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "und ihr hei\u00df-entbrannter Muht", "tokens": ["und", "ihr", "hei\u00df\u00b7ent\u00b7brann\u00b7ter", "Muht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "auff meinen Schwefel h\u00e4uffig stie\u00df", "tokens": ["auff", "mei\u00b7nen", "Schwe\u00b7fel", "h\u00e4uf\u00b7fig", "stie\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und Gegen-flammen in mich blie\u00df:", "tokens": ["und", "Ge\u00b7gen\u00b7flam\u00b7men", "in", "mich", "blie\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nun ihr Feuer au\u00dfgegangen", "tokens": ["Nun", "ihr", "Feu\u00b7er", "au\u00df\u00b7ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "vergeh ich f\u00fcr Verlangen.", "tokens": ["ver\u00b7geh", "ich", "f\u00fcr", "Ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ach! kein Elend ist zu sch\u00e4zzen", "tokens": ["Ach", "!", "kein", "E\u00b7lend", "ist", "zu", "sch\u00e4z\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PIAT", "NN", "VAFIN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "gegen Liebe/ die verbleicht/", "tokens": ["ge\u00b7gen", "Lie\u00b7be", "/", "die", "ver\u00b7bleicht", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "kein Beschwern\u00fc\u00df und Verlezzen", "tokens": ["kein", "Be\u00b7schwer\u00b7n\u00fc\u00df", "und", "Ver\u00b7lez\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "ist/ das diesem Schmerzen gleicht/", "tokens": ["ist", "/", "das", "die\u00b7sem", "Schmer\u00b7zen", "gleicht", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$(", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "wenn sich die Gunst verkehrt in Ha\u00df/", "tokens": ["wenn", "sich", "die", "Gunst", "ver\u00b7kehrt", "in", "Ha\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "VVPP", "APPR", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "wenn uns der Spott macht sehnend-bla\u00df/", "tokens": ["wenn", "uns", "der", "Spott", "macht", "seh\u00b7nen\u00b7dbla\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "NE", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "wenn die Lust uns wird verschlossen", "tokens": ["wenn", "die", "Lust", "uns", "wird", "ver\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPER", "VAFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "der wir so offt genossen.", "tokens": ["der", "wir", "so", "offt", "ge\u00b7nos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Jezt wird mir nicht einst erleubet", "tokens": ["Jezt", "wird", "mir", "nicht", "einst", "er\u00b7leu\u00b7bet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "ADV", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "um die dunkle T\u00fchr zu stehn/", "tokens": ["um", "die", "dunk\u00b7le", "T\u00fchr", "zu", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lunen/ so die Sternen treibet", "tokens": ["Lu\u00b7nen", "/", "so", "die", "Ster\u00b7nen", "trei\u00b7bet"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$(", "ADV", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "darff ich nicht vergn\u00fcget sehn:", "tokens": ["darff", "ich", "nicht", "ver\u00b7gn\u00fc\u00b7get", "sehn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVFIN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Um den sie mich nunmehr verlacht", "tokens": ["Um", "den", "sie", "mich", "nun\u00b7mehr", "ver\u00b7lacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "PPER", "PRF", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "der Neben-buhler steht zur Wacht/", "tokens": ["der", "Ne\u00b7ben\u00b7buh\u00b7ler", "steht", "zur", "Wacht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "um ihn h\u00e4lt der Neides-drache/", "tokens": ["um", "ihn", "h\u00e4lt", "der", "Nei\u00b7des\u00b7dra\u00b7che", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mord/ Eyfer/ Zorn und Rache.", "tokens": ["Mord", "/", "Ey\u00b7fer", "/", "Zorn", "und", "Ra\u00b7che", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Dennoch wil ich ab-nicht-lassen/", "tokens": ["Den\u00b7noch", "wil", "ich", "ab\u00b7nicht\u00b7las\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "dennoch lieb\u2019 ich wie vorhin/", "tokens": ["den\u00b7noch", "lieb'", "ich", "wie", "vor\u00b7hin", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOKOM", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Solt\u2019 ich sie/ mein Leben/ hassen", "tokens": ["Solt'", "ich", "sie", "/", "mein", "Le\u00b7ben", "/", "has\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VMFIN", "PPER", "PPER", "$(", "PPOSAT", "NN", "$(", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "ohne die ich Seel-lo\u00df bin?", "tokens": ["oh\u00b7ne", "die", "ich", "Seel\u00b7lo\u00df", "bin", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach! eher m\u00fcst\u2019 ein kalter Stahl/", "tokens": ["Ach", "!", "e\u00b7her", "m\u00fcst'", "ein", "kal\u00b7ter", "Stahl", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VMFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "verhindern solcher Falschheit Wahl.", "tokens": ["ver\u00b7hin\u00b7dern", "sol\u00b7cher", "Falschheit", "Wahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Endlich wird sie mich den Treuen", "tokens": ["End\u00b7lich", "wird", "sie", "mich", "den", "Treu\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "mit Tr\u00e4hnen noch bereuen.", "tokens": ["mit", "Tr\u00e4h\u00b7nen", "noch", "be\u00b7reu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}