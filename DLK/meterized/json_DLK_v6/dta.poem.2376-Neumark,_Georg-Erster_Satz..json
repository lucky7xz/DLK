{"dta.poem.2376": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Erster Satz.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Der ist Selig welcher bald auf Heyraht d\u00e4nket/", "tokens": ["Der", "ist", "Se\u00b7lig", "wel\u00b7cher", "bald", "auf", "Hey\u00b7raht", "d\u00e4n\u00b7ket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NE", "PRELS", "ADV", "APPR", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Und auf dessen s\u00fcsses Thun die Sinnen lenket/", "tokens": ["Und", "auf", "des\u00b7sen", "s\u00fcs\u00b7ses", "Thun", "die", "Sin\u00b7nen", "len\u00b7ket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PRELAT", "ADJA", "NN", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Selig der ihm vorgenommen/", "tokens": ["Se\u00b7lig", "der", "ihm", "vor\u00b7ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "PPER", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bey noch frischer Zeit/", "tokens": ["Bey", "noch", "fri\u00b7scher", "Zeit", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "In den s\u00fcssen Stand zu kommen/", "tokens": ["In", "den", "s\u00fcs\u00b7sen", "Stand", "zu", "kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Aller Liebligkeit.", "tokens": ["Al\u00b7ler", "Lieb\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Er kan da in stoltzer Ruh die Sinnen weiden/", "tokens": ["Er", "kan", "da", "in", "stolt\u00b7zer", "Ruh", "die", "Sin\u00b7nen", "wei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "ADJA", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und sein Leid verzukkeren mit s\u00fcssen Freuden/", "tokens": ["Und", "sein", "Leid", "ver\u00b7zuk\u00b7ke\u00b7ren", "mit", "s\u00fcs\u00b7sen", "Freu\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Er kan sich mit Lust erwarmen/", "tokens": ["Er", "kan", "sich", "mit", "Lust", "er\u00b7war\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "NN", "VVINF", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Achtet keine Pein/", "tokens": ["Ach\u00b7tet", "kei\u00b7ne", "Pein", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Schleust sich in der Liebsten Armen/", "tokens": ["Schleust", "sich", "in", "der", "Liebs\u00b7ten", "Ar\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und schl\u00e4fft sicher ein.", "tokens": ["Und", "schl\u00e4fft", "si\u00b7cher", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Wenn die Weiderwertigkeit auf ihn wil st\u00fcrmen/", "tokens": ["Wenn", "die", "Wei\u00b7der\u00b7wer\u00b7tig\u00b7keit", "auf", "ihn", "wil", "st\u00fcr\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PPER", "VMFIN", "VVINF", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Steht sein treuer Mitgeno\u00df/ und hilfft beschirmen/", "tokens": ["Steht", "sein", "treu\u00b7er", "Mit\u00b7ge\u00b7no\u00df", "/", "und", "hilfft", "be\u00b7schir\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$(", "KON", "VVFIN", "VVINF", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Sie kan durch ein freundlichs Lachen/", "tokens": ["Sie", "kan", "durch", "ein", "freund\u00b7lichs", "La\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sein betr\u00fcbtes Hertz", "tokens": ["Sein", "be\u00b7tr\u00fcb\u00b7tes", "Hertz"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Und sein\u2019 Arbeit s\u00fcsse machen/", "tokens": ["Und", "sein'", "Ar\u00b7beit", "s\u00fcs\u00b7se", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Durch verliebten Schertz.", "tokens": ["Durch", "ver\u00b7lieb\u00b7ten", "Schertz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Lieb und Leid kan Er allzeit mit ihr vertheilen.", "tokens": ["Lieb", "und", "Leid", "kan", "Er", "all\u00b7zeit", "mit", "ihr", "ver\u00b7thei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VMFIN", "PPER", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+---+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Denn die Last liegt halb so schwer auf zweyen Seulen/", "tokens": ["Denn", "die", "Last", "liegt", "halb", "so", "schwer", "auf", "zwe\u00b7yen", "Seu\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "ADV", "ADJD", "APPR", "VVFIN", "NN", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "M\u00e4nnlich hilffet Sie ihm tragen/", "tokens": ["M\u00e4nn\u00b7lich", "hilf\u00b7fet", "Sie", "ihm", "tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles Eheleid/", "tokens": ["Al\u00b7les", "E\u00b7he\u00b7leid", "/"], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Sie ist in den Freudentagen/", "tokens": ["Sie", "ist", "in", "den", "Freu\u00b7den\u00b7ta\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Sein\u2019 Ergetzligkeit.", "tokens": ["Sein'", "Er\u00b7getz\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}