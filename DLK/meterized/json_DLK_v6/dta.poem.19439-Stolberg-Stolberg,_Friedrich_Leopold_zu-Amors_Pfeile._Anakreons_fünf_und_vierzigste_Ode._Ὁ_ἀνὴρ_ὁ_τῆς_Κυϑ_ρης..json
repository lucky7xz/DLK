{"dta.poem.19439": {"metadata": {"author": {"name": "Stolberg-Stolberg, Friedrich Leopold zu", "birth": "N.A.", "death": "N.A."}, "title": "Amors Pfeile.  \n Anakreons f\u00fcnf und vierzigste Ode.  \n \u1f49 \u1f00\u03bd\u1f74\u03c1 \u1f41 \u03c4\u1fc6\u03c2 \u039a\u03c5\u03d1 \u03c1\u03b7\u03c2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1779", "urn": "urn:nbn:de:kobv:b4-20589-5", "language": ["de:0.57", "nl:0.42"], "booktitle": "Stolberg-Stolberg, Christian zu; Stolberg-Stolberg, Friedrich Leopold zu: Gedichte. Leipzig, 1779."}, "poem": {"stanza.1": {"line.1": {"text": "Der Gatte Cythereens", "tokens": ["Der", "Gat\u00b7te", "Cy\u00b7ther\u00b7eens"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Nahm Stal in Lemnos Esse,", "tokens": ["Nahm", "Stal", "in", "Lem\u00b7nos", "Es\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und schmiedet\u2019 Amors Pfeile.", "tokens": ["Und", "schmie\u00b7det'", "A\u00b7mors", "Pfei\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Spizen tauchte Cypris", "tokens": ["Die", "Spi\u00b7zen", "tauch\u00b7te", "Cyp\u00b7ris"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "In Honigseim; doch Amor", "tokens": ["In", "Ho\u00b7ni\u00b7gseim", ";", "doch", "A\u00b7mor"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NE", "$.", "ADV", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "That in den Honig Galle.", "tokens": ["That", "in", "den", "Ho\u00b7nig", "Gal\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "J\u00fcngst kehrte Mars vom Treffen,", "tokens": ["J\u00fcngst", "kehr\u00b7te", "Mars", "vom", "Tref\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Schwang seine hohe Lanze,", "tokens": ["Schwang", "sei\u00b7ne", "ho\u00b7he", "Lan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Und spottel\u2019 Amors Pfeile.", "tokens": ["Und", "spot\u00b7tel'", "A\u00b7mors", "Pfei\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Sieh, der ist schwer! sprach Amor;", "tokens": ["Sieh", ",", "der", "ist", "schwer", "!", "sprach", "A\u00b7mor", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "VAFIN", "ADJD", "$.", "VVFIN", "NE", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.11": {"text": "Du kanst ihn selbst versuchen!", "tokens": ["Du", "kanst", "ihn", "selbst", "ver\u00b7su\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Mars nimt das kleine Pfeilchen", "tokens": ["Mars", "nimt", "das", "klei\u00b7ne", "Pfeil\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Und lose l\u00e4chelt Cypris:", "tokens": ["Und", "lo\u00b7se", "l\u00e4\u00b7chelt", "Cyp\u00b7ris", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Doch keuchend rief der Kriegsgott:", "tokens": ["Doch", "keu\u00b7chend", "rief", "der", "Kriegs\u00b7gott", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Schwer ist er! Nim ihn wieder!", "tokens": ["Schwer", "ist", "er", "!", "Nim", "ihn", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "$.", "NE", "PPER", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Doch Amor sprach: Behalt ihn!", "tokens": ["Doch", "A\u00b7mor", "sprach", ":", "Be\u00b7halt", "ihn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "$.", "VVFIN", "PPER", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}