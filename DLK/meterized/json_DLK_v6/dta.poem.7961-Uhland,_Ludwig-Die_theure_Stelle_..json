{"dta.poem.7961": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die theure Stelle .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Die Stelle, wo ich auf verschlungnen Wegen", "tokens": ["Die", "Stel\u00b7le", ",", "wo", "ich", "auf", "ver\u00b7schlung\u00b7nen", "We\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Begegnete dem wundersch\u00f6nen Kinde,", "tokens": ["Be\u00b7ge\u00b7gne\u00b7te", "dem", "wun\u00b7der\u00b7sch\u00f6\u00b7nen", "Kin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Das, leicht vor\u00fcbereilend mit dem Winde,", "tokens": ["Das", ",", "leicht", "vor\u00b7\u00fc\u00b7be\u00b7rei\u00b7lend", "mit", "dem", "Win\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "ADJD", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mir spendete des holden Blickes Segen:", "tokens": ["Mir", "spen\u00b7de\u00b7te", "des", "hol\u00b7den", "Bli\u00b7ckes", "Se\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wohl m\u00f6cht\u2019 ich jene Stelle liebend hegen,", "tokens": ["Wohl", "m\u00f6cht'", "ich", "je\u00b7ne", "Stel\u00b7le", "lie\u00b7bend", "he\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PDAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dort Zeichen graben in des Baumes Rinde,", "tokens": ["Dort", "Zei\u00b7chen", "gra\u00b7ben", "in", "des", "Bau\u00b7mes", "Rin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mich schm\u00fccken mit der Blumen Angebinde,", "tokens": ["Mich", "schm\u00fc\u00b7cken", "mit", "der", "Blu\u00b7men", "An\u00b7ge\u00b7bin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu Tr\u00e4umen mich in k\u00fchle Schatten legen.", "tokens": ["Zu", "Tr\u00e4u\u00b7men", "mich", "in", "k\u00fch\u00b7le", "Schat\u00b7ten", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch so verwirrte mich des Blickes Helle,", "tokens": ["Doch", "so", "ver\u00b7wirr\u00b7te", "mich", "des", "Bli\u00b7ckes", "Hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PRF", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und so geblendet blieb ich von dem Bilde,", "tokens": ["Und", "so", "ge\u00b7blen\u00b7det", "blieb", "ich", "von", "dem", "Bil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df lang ich wie ein Trunkner mu\u00dfte wanken;", "tokens": ["Da\u00df", "lang", "ich", "wie", "ein", "Trunk\u00b7ner", "mu\u00df\u00b7te", "wan\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "KOKOM", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und nun mit allem Streben der Gedanken,", "tokens": ["Und", "nun", "mit", "al\u00b7lem", "Stre\u00b7ben", "der", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PIS", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So wie mit allem Suchen im Gefilde,", "tokens": ["So", "wie", "mit", "al\u00b7lem", "Su\u00b7chen", "im", "Ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "APPR", "PIS", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht mehr erforschen kann die theure Stelle.", "tokens": ["Nicht", "mehr", "er\u00b7for\u00b7schen", "kann", "die", "theu\u00b7re", "Stel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVINF", "VMFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}