{"textgrid.poem.64446": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "68. Als ein Frauenzimmer Vorstehendes \u00fcbel aufnahm", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ich von G\u00e4nsen hier geschrieben,", "tokens": ["Was", "ich", "von", "G\u00e4n\u00b7sen", "hier", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Trifft Sie, Madam, gewi\u00dflich nicht.", "tokens": ["Trifft", "Sie", ",", "Ma\u00b7dam", ",", "ge\u00b7wi\u00df\u00b7lich", "nicht", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "ADV", "PTKNEG", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "In G\u00e4nse, so wie die, von den die Mutter spricht,", "tokens": ["In", "G\u00e4n\u00b7se", ",", "so", "wie", "die", ",", "von", "den", "die", "Mut\u00b7ter", "spricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADV", "KOKOM", "ART", "$,", "APPR", "ART", "ART", "NN", "VVFIN", "$,"], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.4": {"text": "Kann man sich ja verlieben.", "tokens": ["Kann", "man", "sich", "ja", "ver\u00b7lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}