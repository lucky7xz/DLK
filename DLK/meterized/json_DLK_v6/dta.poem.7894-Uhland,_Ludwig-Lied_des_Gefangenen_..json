{"dta.poem.7894": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Lied des Gefangenen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Wie lieblicher Klang!", "tokens": ["Wie", "lieb\u00b7li\u00b7cher", "Klang", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "O Lerche! dein Sang,", "tokens": ["O", "Ler\u00b7che", "!", "dein", "Sang", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PPOSAT", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Er hebt sich, er schwingt sich in Wonne.", "tokens": ["Er", "hebt", "sich", ",", "er", "schwingt", "sich", "in", "Won\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "PPER", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Du nimmst mich von hier,", "tokens": ["Du", "nimmst", "mich", "von", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADV", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Ich singe mit dir,", "tokens": ["Ich", "sin\u00b7ge", "mit", "dir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Wir steigen durch Wolken zur Sonne.", "tokens": ["Wir", "stei\u00b7gen", "durch", "Wol\u00b7ken", "zur", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "O Lerche! du neigst", "tokens": ["O", "Ler\u00b7che", "!", "du", "neigst"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$.", "PPER", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Dich nieder, du schweigst,", "tokens": ["Dich", "nie\u00b7der", ",", "du", "schweigst", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "PPER", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Du sinkst in die bl\u00fchenden Auen.", "tokens": ["Du", "sinkst", "in", "die", "bl\u00fc\u00b7hen\u00b7den", "Au\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Ich schweige zumal", "tokens": ["Ich", "schwei\u00b7ge", "zu\u00b7mal"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Und sinke zuthal,", "tokens": ["Und", "sin\u00b7ke", "zut\u00b7hal", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Ach! tief in Moder und Grauen.", "tokens": ["Ach", "!", "tief", "in", "Mo\u00b7der", "und", "Grau\u00b7en", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}