{"textgrid.poem.36958": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: 's war einmal Bruder und Schwester:", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "'s war einmal Bruder und Schwester:", "tokens": ["'s", "war", "ein\u00b7mal", "Bru\u00b7der", "und", "Schwes\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Der Reichthum und die Noth;", "tokens": ["Der", "Reicht\u00b7hum", "und", "die", "Noth", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er schwelgte in tausend Gen\u00fc\u00dfen,", "tokens": ["Er", "schwelg\u00b7te", "in", "tau\u00b7send", "Ge\u00b7n\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "CARD", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Sie hatte kaum trocken Brot.", "tokens": ["Sie", "hat\u00b7te", "kaum", "tro\u00b7cken", "Brot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Schwester diente beim Bruder", "tokens": ["Die", "Schwes\u00b7ter", "dien\u00b7te", "beim", "Bru\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Viel Hundert Jahre lang;", "tokens": ["Viel", "Hun\u00b7dert", "Jah\u00b7re", "lang", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihn r\u00fchrt es nicht, wenn sie weinte,", "tokens": ["Ihn", "r\u00fchrt", "es", "nicht", ",", "wenn", "sie", "wein\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Noch wenn sie ihr Leiden besang.", "tokens": ["Noch", "wenn", "sie", "ihr", "Lei\u00b7den", "be\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Er fluchte und trat sie mit F\u00fc\u00dfen;", "tokens": ["Er", "fluch\u00b7te", "und", "trat", "sie", "mit", "F\u00fc\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Er schlug ihr in's sanfte Gesicht;", "tokens": ["Er", "schlug", "ihr", "in's", "sanf\u00b7te", "Ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Sie fiel auf die Erde und flehte:", "tokens": ["Sie", "fiel", "auf", "die", "Er\u00b7de", "und", "fleh\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "KON", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Hilfst du, o Gott, mir nicht?", "tokens": ["Hilfst", "du", ",", "o", "Gott", ",", "mir", "nicht", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "FM", "NN", "$,", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wie wird das Lied wohl enden?", "tokens": ["Wie", "wird", "das", "Lied", "wohl", "en\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das ist ein traurig Lied!", "tokens": ["Das", "ist", "ein", "trau\u00b7rig", "Lied", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich will's nicht weiter h\u00f6ren,", "tokens": ["Ich", "will's", "nicht", "wei\u00b7ter", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn Nichts f\u00fcr die Schwester geschieht!", "tokens": ["Wenn", "Nichts", "f\u00fcr", "die", "Schwes\u00b7ter", "ge\u00b7schieht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Das ist das Ende vom Liede,", "tokens": ["Das", "ist", "das", "En\u00b7de", "vom", "Lie\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Vom Reichthum und der Noth:", "tokens": ["Vom", "Reicht\u00b7hum", "und", "der", "Noth", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "An einem sch\u00f6nen Morgen", "tokens": ["An", "ei\u00b7nem", "sch\u00f6\u00b7nen", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schlug sie ihren Bruder todt.", "tokens": ["Schlug", "sie", "ih\u00b7ren", "Bru\u00b7der", "todt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}