{"textgrid.poem.66851": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Spiele noch einmal das St\u00fcck,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Spiele noch einmal das St\u00fcck,", "tokens": ["Spie\u00b7le", "noch", "ein\u00b7mal", "das", "St\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Spiel die Weise deines Lebens,", "tokens": ["Spiel", "die", "Wei\u00b7se", "dei\u00b7nes", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist zersprungen auch dein Gl\u00fcck", "tokens": ["Ist", "zer\u00b7sprun\u00b7gen", "auch", "dein", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "VVPP", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Traum der Kraft vergebens!", "tokens": ["Und", "der", "Traum", "der", "Kraft", "ver\u00b7ge\u00b7bens", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Tr\u00e4ume, tr\u00e4ume dich hinein,", "tokens": ["Tr\u00e4u\u00b7me", ",", "tr\u00e4u\u00b7me", "dich", "hin\u00b7ein", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sei der junge Quell der Lieder", "tokens": ["Sei", "der", "jun\u00b7ge", "Quell", "der", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und vergi\u00df die starre Pein,", "tokens": ["Und", "ver\u00b7gi\u00df", "die", "star\u00b7re", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Die dich steinern dr\u00fcckt danieder!", "tokens": ["Die", "dich", "stei\u00b7nern", "dr\u00fcckt", "da\u00b7nie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "VVFIN", "PAV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "H\u00f6rst du, wie die Freiheit weckt", "tokens": ["H\u00f6rst", "du", ",", "wie", "die", "Frei\u00b7heit", "weckt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Durch die n\u00e4chtlich \u00f6den Gassen,", "tokens": ["Durch", "die", "n\u00e4cht\u00b7lich", "\u00f6\u00b7den", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Siehst du, wie vom Klang erstreckt,", "tokens": ["Siehst", "du", ",", "wie", "vom", "Klang", "er\u00b7streckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Wesen, die die Freiheit hassen,", "tokens": ["We\u00b7sen", ",", "die", "die", "Frei\u00b7heit", "has\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "\u00c4ngstlich, winselnd, scheu sich drehn", "tokens": ["\u00c4ngst\u00b7lich", ",", "win\u00b7selnd", ",", "scheu", "sich", "drehn"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "VVPP", "$,", "ADJD", "PRF", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Und in alle Winkel stieben,", "tokens": ["Und", "in", "al\u00b7le", "Win\u00b7kel", "stie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Wie sie bl\u00f6den Blickes sehn,", "tokens": ["Wie", "sie", "bl\u00f6\u00b7den", "Bli\u00b7ckes", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Was die neuen Menschen lieben?", "tokens": ["Was", "die", "neu\u00b7en", "Men\u00b7schen", "lie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Spiele noch einmal das St\u00fcck,", "tokens": ["Spie\u00b7le", "noch", "ein\u00b7mal", "das", "St\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Spiel das k\u00fchne Lied der Fr\u00fche,", "tokens": ["Spiel", "das", "k\u00fch\u00b7ne", "Lied", "der", "Fr\u00fc\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Spielmann, spiel dich selbst zur\u00fcck,", "tokens": ["Spiel\u00b7mann", ",", "spiel", "dich", "selbst", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "Gl\u00fch empor im Traum, ergl\u00fche!", "tokens": ["Gl\u00fch", "em\u00b7por", "im", "Traum", ",", "er\u00b7gl\u00fc\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "PTKVZ", "APPRART", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Bist du tot? O nein, du lebst,", "tokens": ["Bist", "du", "tot", "?", "O", "nein", ",", "du", "lebst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$.", "NE", "PTKANT", "$,", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Rufen alle deine Br\u00fcder,", "tokens": ["Ru\u00b7fen", "al\u00b7le", "dei\u00b7ne", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Und du zitterst und du bebst:", "tokens": ["Und", "du", "zit\u00b7terst", "und", "du", "bebst", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Wache auf, du Lebensm\u00fcder!", "tokens": ["Wa\u00b7che", "auf", ",", "du", "Le\u00b7bens\u00b7m\u00fc\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPER", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Wache auf! Die Freiheit ruft:", "tokens": ["Wa\u00b7che", "auf", "!", "Die", "Frei\u00b7heit", "ruft", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.26": {"text": "Willst du wirklich mich verlassen?", "tokens": ["Willst", "du", "wirk\u00b7lich", "mich", "ver\u00b7las\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Musen, die ihr ihn erschuft,", "tokens": ["Mu\u00b7sen", ",", "die", "ihr", "ihn", "er\u00b7schuft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "Helft, o helft ihn stark umfassen!", "tokens": ["Helft", ",", "o", "helft", "ihn", "stark", "um\u00b7fas\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "FM", "VVFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Schach dem Schicksal! Rei\u00df das Buch", "tokens": ["Schach", "dem", "Schick\u00b7sal", "!", "Rei\u00df", "das", "Buch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ART", "NN", "$.", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.30": {"text": "Deines Mi\u00dfgeschicks zunichte,", "tokens": ["Dei\u00b7nes", "Mi\u00df\u00b7ge\u00b7schicks", "zu\u00b7nich\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "\u00dcberwinde deinen Fluch", "tokens": ["\u00dc\u00b7berw\u00b7in\u00b7de", "dei\u00b7nen", "Fluch"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.32": {"text": "Und am Lied der Zukunft dichte!", "tokens": ["Und", "am", "Lied", "der", "Zu\u00b7kunft", "dich\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Greif die Weise noch einmal,", "tokens": ["Greif", "die", "Wei\u00b7se", "noch", "ein\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.34": {"text": "Durch die Grundakkorde w\u00fchle,", "tokens": ["Durch", "die", "Grun\u00b7dak\u00b7kor\u00b7de", "w\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Deiner dumpfen Eigenqual", "tokens": ["Dei\u00b7ner", "dum\u00b7pfen", "Ei\u00b7gen\u00b7qual"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.36": {"text": "Schreckensbann von hinnen sp\u00fcle!", "tokens": ["Schre\u00b7ckens\u00b7bann", "von", "hin\u00b7nen", "sp\u00fc\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Bist du tot? So wachs empor", "tokens": ["Bist", "du", "tot", "?", "So", "wachs", "em\u00b7por"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADJD", "$.", "ADV", "VVFIN", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.38": {"text": "Aus des schwarzen Sarges W\u00e4nden,", "tokens": ["Aus", "des", "schwar\u00b7zen", "Sar\u00b7ges", "W\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Um dein Haupt der Schwermut Flor,", "tokens": ["Um", "dein", "Haupt", "der", "Schwer\u00b7mut", "Flor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.40": {"text": "Doch das Schwert noch in den H\u00e4nden \u2013", "tokens": ["Doch", "das", "Schwert", "noch", "in", "den", "H\u00e4n\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Vorw\u00e4rts, vorw\u00e4rts, nie zur\u00fcck,", "tokens": ["Vor\u00b7w\u00e4rts", ",", "vor\u00b7w\u00e4rts", ",", "nie", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "$,", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.42": {"text": "S\u00fc\u00dfer Marschtakt neuen Lebens ...", "tokens": ["S\u00fc\u00b7\u00dfer", "Marschtakt", "neu\u00b7en", "Le\u00b7bens", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.43": {"text": "Ist zersprungen auch dein Gl\u00fcck,", "tokens": ["Ist", "zer\u00b7sprun\u00b7gen", "auch", "dein", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.44": {"text": "Und der Traum der Kraft vergebens.", "tokens": ["Und", "der", "Traum", "der", "Kraft", "ver\u00b7ge\u00b7bens", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}