{"textgrid.poem.65716": {"metadata": {"author": {"name": "Drollinger, Carl Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zween Br\u00fcder zankten sich zu Prag", "genre": "verse", "period": "N.A.", "pub_year": 1715, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zween Br\u00fcder zankten sich zu Prag", "tokens": ["Zween", "Br\u00fc\u00b7der", "zank\u00b7ten", "sich", "zu", "Prag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "VVFIN", "PRF", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Einst um den Rang beym Staupenschlag.", "tokens": ["Einst", "um", "den", "Rang", "beym", "Stau\u00b7pen\u00b7schlag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es war ein schw\u00e4res Streiten.", "tokens": ["Es", "war", "ein", "schw\u00e4\u00b7res", "Strei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der B\u00fcttel schlichtet ihren Zwist,", "tokens": ["Der", "B\u00fct\u00b7tel", "schlich\u00b7tet", "ih\u00b7ren", "Zwist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und stellte sie mit kluger List", "tokens": ["Und", "stell\u00b7te", "sie", "mit", "klu\u00b7ger", "List"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zur recht- und linken Seiten.", "tokens": ["Zur", "recht", "und", "lin\u00b7ken", "Sei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "TRUNC", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Doch diesen, der zur Linken stund,", "tokens": ["Doch", "die\u00b7sen", ",", "der", "zur", "Lin\u00b7ken", "stund", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den strich er mit dem Ruhtenbund", "tokens": ["Den", "strich", "er", "mit", "dem", "Ruh\u00b7ten\u00b7bund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Am ersten um die Lenden.", "tokens": ["Am", "ers\u00b7ten", "um", "die", "Len\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Des triumfirt das edle Paar,", "tokens": ["Des", "tri\u00b7um\u00b7firt", "das", "ed\u00b7le", "Paar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und jeder dacht: Nun ist f\u00fcrwahr", "tokens": ["Und", "je\u00b7der", "dacht", ":", "Nun", "ist", "f\u00fcr\u00b7wahr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "$.", "ADV", "VAFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Der Sieg in meinen H\u00e4nden.", "tokens": ["Der", "Sieg", "in", "mei\u00b7nen", "H\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Der Eine rief: Victoria!", "tokens": ["Der", "Ei\u00b7ne", "rief", ":", "Vic\u00b7to\u00b7ria", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$.", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "Seht! mu\u00df sich doch der Flegel da", "tokens": ["Seht", "!", "mu\u00df", "sich", "doch", "der", "Fle\u00b7gel", "da"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "VMFIN", "PRF", "ADV", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Zu meiner Linken b\u00fccken.", "tokens": ["Zu", "mei\u00b7ner", "Lin\u00b7ken", "b\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Bald schry der Andre: Jubilo!", "tokens": ["Bald", "schry", "der", "And\u00b7re", ":", "Ju\u00b7bi\u00b7lo", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "PIS", "$.", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Der erste Streich, seyd alle froh,", "tokens": ["Der", "ers\u00b7te", "Streich", ",", "seyd", "al\u00b7le", "froh", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VAFIN", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Der fiel auf meinen R\u00fccken.", "tokens": ["Der", "fiel", "auf", "mei\u00b7nen", "R\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}