{"dta.poem.13318": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z w\u00f6lfftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eden Tempel? der geb\u00fchrt dem, welcher euch belohnt,", "tokens": ["\u201e", "den", "Tem\u00b7pel", "?", "der", "ge\u00b7b\u00fchrt", "dem", ",", "wel\u00b7cher", "euch", "be\u00b7lohnt", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$.", "ART", "VVFIN", "ART", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "430\u201dUnd in der ganzen Welt durch sein Verm\u00f6gen wohnt;", "tokens": ["\"", "Und", "in", "der", "gan\u00b7zen", "Welt", "durch", "sein", "Ver\u00b7m\u00f6\u00b7gen", "wohnt", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "APPR", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201eder Ehgemahl wird ihn an jenem Ort erbauen,", "tokens": ["\u201e", "der", "Eh\u00b7ge\u00b7mahl", "wird", "ihn", "an", "je\u00b7nem", "Ort", "er\u00b7bau\u00b7en", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "PPER", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201ewo man zum ersten Mahl ihn wird als K\u00f6nig schauen.", "tokens": ["\u201e", "wo", "man", "zum", "ers\u00b7ten", "Mahl", "ihn", "wird", "als", "K\u00f6\u00b7nig", "schau\u00b7en", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PIS", "APPRART", "ADJA", "NN", "PPER", "VAFIN", "KOKOM", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201eer widmet solchen Bau der h\u00f6chsten Majest\u00e4t,", "tokens": ["\u201e", "er", "wid\u00b7met", "sol\u00b7chen", "Bau", "der", "h\u00f6chs\u00b7ten", "Ma\u00b7jes\u00b7t\u00e4t", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PIAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201edurch welche Gl\u00fcck und Heil und Kron und Thron besteht.", "tokens": ["\u201e", "durch", "wel\u00b7che", "Gl\u00fcck", "und", "Heil", "und", "Kron", "und", "Thron", "be\u00b7steht", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PWAT", "NN", "KON", "NN", "KON", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "435\u201dEr kennet gar zu wohl, woher der Ausgang r\u00fchre;", "tokens": ["\"", "Er", "ken\u00b7net", "gar", "zu", "wohl", ",", "wo\u00b7her", "der", "Aus\u00b7gang", "r\u00fch\u00b7re", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "APPR", "ADV", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201ewer dieser Zeiten Lauf zu solchem Ende f\u00fchre.", "tokens": ["\u201e", "wer", "die\u00b7ser", "Zei\u00b7ten", "Lauf", "zu", "sol\u00b7chem", "En\u00b7de", "f\u00fch\u00b7re", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "PDAT", "NN", "NN", "APPR", "PIAT", 