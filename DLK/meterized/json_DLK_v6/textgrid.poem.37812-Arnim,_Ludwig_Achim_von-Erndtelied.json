{"textgrid.poem.37812": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Erndtelied", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist ein Schnitter, der hei\u00dft Tod,", "tokens": ["Es", "ist", "ein", "Schnit\u00b7ter", ",", "der", "hei\u00dft", "Tod", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat Gewalt vom h\u00f6chsten Gott,", "tokens": ["Hat", "Ge\u00b7walt", "vom", "h\u00f6chs\u00b7ten", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Heut wezt er das Messer,", "tokens": ["Heut", "wezt", "er", "das", "Mes\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Es schneidt schon viel besser,", "tokens": ["Es", "schneidt", "schon", "viel", "bes\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Bald wird er drein schneiden,", "tokens": ["Bald", "wird", "er", "drein", "schnei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Wir m\u00fcssens nur leiden.", "tokens": ["Wir", "m\u00fcs\u00b7sens", "nur", "lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "H\u00fcte dich sch\u00f6ns Bl\u00fcmelein!", "tokens": ["H\u00fc\u00b7te", "dich", "sch\u00f6ns", "Bl\u00fc\u00b7me\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Was heut noch gr\u00fcn und frisch da steht,", "tokens": ["Was", "heut", "noch", "gr\u00fcn", "und", "frisch", "da", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "ADJD", "KON", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird morgen schon hinweggem\u00e4ht:", "tokens": ["Wird", "mor\u00b7gen", "schon", "hin\u00b7weg\u00b7ge\u00b7m\u00e4ht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die edlen Narcissen,", "tokens": ["Die", "ed\u00b7len", "Nar\u00b7cis\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die Zierden der Wiesen,", "tokens": ["Die", "Zier\u00b7den", "der", "Wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Die sch\u00f6n' Hiazinten,", "tokens": ["Die", "sch\u00f6n'", "Hi\u00b7a\u00b7zin\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Die t\u00fcrkischen Binden.", "tokens": ["Die", "t\u00fcr\u00b7ki\u00b7schen", "Bin\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "H\u00fcte dich sch\u00f6ns Bl\u00fcmelein!", "tokens": ["H\u00fc\u00b7te", "dich", "sch\u00f6ns", "Bl\u00fc\u00b7me\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Viel hundert tausend ungez\u00e4hlt,", "tokens": ["Viel", "hun\u00b7dert", "tau\u00b7send", "un\u00b7ge\u00b7z\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "CARD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was nur unter die Sichel f\u00e4llt,", "tokens": ["Was", "nur", "un\u00b7ter", "die", "Si\u00b7chel", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Ihr Rosen, ihr Liljen,", "tokens": ["Ihr", "Ro\u00b7sen", ",", "ihr", "Lil\u00b7jen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Euch wird er austilgen,", "tokens": ["Euch", "wird", "er", "aus\u00b7til\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Auch die Kaiser-Kronen,", "tokens": ["Auch", "die", "Kai\u00b7ser\u00b7Kro\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Wird er nicht verschonen.", "tokens": ["Wird", "er", "nicht", "ver\u00b7scho\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "H\u00fcte dich sch\u00f6ns Bl\u00fcmelein!", "tokens": ["H\u00fc\u00b7te", "dich", "sch\u00f6ns", "Bl\u00fc\u00b7me\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Das himmelfarbe Ehrenprei\u00df,", "tokens": ["Das", "him\u00b7mel\u00b7far\u00b7be", "Eh\u00b7ren\u00b7prei\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Tulipanen gelb und wei\u00df,", "tokens": ["Die", "Tu\u00b7li\u00b7pa\u00b7nen", "gelb", "und", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die silbernen Glocken,", "tokens": ["Die", "sil\u00b7ber\u00b7nen", "Glo\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die goldenen Flocken,", "tokens": ["Die", "gol\u00b7de\u00b7nen", "Flo\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Senkt alles zur Erden,", "tokens": ["Senkt", "al\u00b7les", "zur", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Was wird daraus werden?", "tokens": ["Was", "wird", "da\u00b7raus", "wer\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PAV", "VAINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "H\u00fcte dich sch\u00f6ns Bl\u00fcmelein!", "tokens": ["H\u00fc\u00b7te", "dich", "sch\u00f6ns", "Bl\u00fc\u00b7me\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ihr h\u00fcbsch Lavendel, Ro\u00dfmarein,", "tokens": ["Ihr", "h\u00fcbsch", "La\u00b7ven\u00b7del", ",", "Ro\u00df\u00b7ma\u00b7rein", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADJD", "NE", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr vielf\u00e4rbige R\u00f6selein.", "tokens": ["Ihr", "viel\u00b7f\u00e4r\u00b7bi\u00b7ge", "R\u00f6\u00b7se\u00b7lein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr stolze Schwerdliljen,", "tokens": ["Ihr", "stol\u00b7ze", "Schwerd\u00b7lil\u00b7jen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ihr krause Basiljen,", "tokens": ["Ihr", "krau\u00b7se", "Ba\u00b7sil\u00b7jen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ihr zarte Violen,", "tokens": ["Ihr", "zar\u00b7te", "Vi\u00b7o\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Man wird euch bald holen.", "tokens": ["Man", "wird", "euch", "bald", "ho\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "H\u00fcte dich sch\u00f6ns Bl\u00fcmelein!", "tokens": ["H\u00fc\u00b7te", "dich", "sch\u00f6ns", "Bl\u00fc\u00b7me\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Trotz! Tod, komm her, ich f\u00fcrcht dich nicht,", "tokens": ["Trotz", "!", "Tod", ",", "komm", "her", ",", "ich", "f\u00fcrcht", "dich", "nicht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$,", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Trotz, eil daher in einem Schnitt.", "tokens": ["Trotz", ",", "eil", "da\u00b7her", "in", "ei\u00b7nem", "Schnitt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PAV", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Werd ich nur verletzet,", "tokens": ["Werd", "ich", "nur", "ver\u00b7let\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "So werd ich versetzet", "tokens": ["So", "werd", "ich", "ver\u00b7set\u00b7zet"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "In den himmlischen Garten,", "tokens": ["In", "den", "himm\u00b7li\u00b7schen", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Auf den alle wir warten.", "tokens": ["Auf", "den", "al\u00b7le", "wir", "war\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIS", "PPER", "VVINF", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Freu' dich du sch\u00f6ns Bl\u00fcmelein.", "tokens": ["Freu'", "dich", "du", "sch\u00f6ns", "Bl\u00fc\u00b7me\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}