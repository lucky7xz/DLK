{"textgrid.poem.52508": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "28. Abschied an David He\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freund! der bei des Busches Eichen", "tokens": ["Freund", "!", "der", "bei", "des", "Bu\u00b7sches", "Ei\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "ART", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lieber denkt, vom Mond erhellt,", "tokens": ["Lie\u00b7ber", "denkt", ",", "vom", "Mond", "er\u00b7hellt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als sich zu den flitterreichen", "tokens": ["Als", "sich", "zu", "den", "flit\u00b7ter\u00b7rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eiteln H\u00f6flingspuppen stellt;", "tokens": ["Ei\u00b7teln", "H\u00f6f\u00b7lings\u00b7pup\u00b7pen", "stellt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der das Bild geharnschter, braver", "tokens": ["Der", "das", "Bild", "ge\u00b7harnschter", ",", "bra\u00b7ver"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ART", "NN", "ADJA", "$,", "ADJA"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Schweizerhelden h\u00f6her h\u00e4lt,", "tokens": ["Schwei\u00b7zer\u00b7hel\u00b7den", "h\u00f6\u00b7her", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Als der heutigen Bataver", "tokens": ["Als", "der", "heu\u00b7ti\u00b7gen", "Ba\u00b7ta\u00b7ver"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Panzerhelden auf dem Geld.", "tokens": ["Pan\u00b7zer\u00b7hel\u00b7den", "auf", "dem", "Geld", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Stunden, deiner w\u00fcrdig, warten", "tokens": ["Stun\u00b7den", ",", "dei\u00b7ner", "w\u00fcr\u00b7dig", ",", "war\u00b7ten"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "$,", "PPOSAT", "ADJD", "$,", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dein, auf Z\u00fcrichs heitrer Flur;", "tokens": ["Dein", ",", "auf", "Z\u00fc\u00b7richs", "hei\u00b7trer", "Flur", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "$,", "APPR", "NE", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihre Auen sind ein Garten", "tokens": ["Ih\u00b7re", "Au\u00b7en", "sind", "ein", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcr den Liebling der Natur.", "tokens": ["F\u00fcr", "den", "Lieb\u00b7ling", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und das bist du! \u2013 Hochgef\u00fchle", "tokens": ["Und", "das", "bist", "du", "!", "\u2013", "Hoch\u00b7ge\u00b7f\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word"], "pos": ["KON", "PDS", "VAFIN", "PPER", "$.", "$(", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gab sie dir und Dichtungskraft;", "tokens": ["Gab", "sie", "dir", "und", "Dich\u00b7tungs\u00b7kraft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Lehrte dich beim Saitenspiele", "tokens": ["Lehr\u00b7te", "dich", "beim", "Sai\u00b7ten\u00b7spie\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "T\u00f6ne sanfter Leidenschaft.", "tokens": ["T\u00f6\u00b7ne", "sanf\u00b7ter", "Lei\u00b7den\u00b7schaft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Aber sieh! Begeist'rung waltet!", "tokens": ["A\u00b7ber", "sieh", "!", "Be\u00b7geist'\u00b7rung", "wal\u00b7tet", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$.", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Malt mir neue Bilder vor.", "tokens": ["Malt", "mir", "neu\u00b7e", "Bil\u00b7der", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieh! Ein M\u00e4dchen, schlank gestaltet,", "tokens": ["Sieh", "!", "Ein", "M\u00e4d\u00b7chen", ",", "schlank", "ge\u00b7stal\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "ART", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schimmert durch des Schleiers Flor,", "tokens": ["Schim\u00b7mert", "durch", "des", "Schlei\u00b7ers", "Flor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Eilet sanft, mit holder Scheue,", "tokens": ["Ei\u00b7let", "sanft", ",", "mit", "hol\u00b7der", "Scheu\u00b7e", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf den besten J\u00fcngling zu,", "tokens": ["Auf", "den", "bes\u00b7ten", "J\u00fcng\u00b7ling", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Lohnt ihm Tugenden durch Treue;", "tokens": ["Lohnt", "ihm", "Tu\u00b7gen\u00b7den", "durch", "Treu\u00b7e", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und der J\u00fcngling \u2013 Freund! bist du.", "tokens": ["Und", "der", "J\u00fcng\u00b7ling", "\u2013", "Freund", "!", "bist", "du", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$(", "NN", "$.", "VAFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "O! was wirst du dann empfinden,", "tokens": ["O", "!", "was", "wirst", "du", "dann", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWS", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "T\u00f6nt bei Nacht, im Schattengang,", "tokens": ["T\u00f6nt", "bei", "Nacht", ",", "im", "Schat\u00b7ten\u00b7gang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In den hohen Limmatlinden", "tokens": ["In", "den", "ho\u00b7hen", "Lim\u00b7mat\u00b7lin\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einer Nachtigall Gesang.", "tokens": ["Ei\u00b7ner", "Nach\u00b7ti\u00b7gall", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Liebe, die den Winterwiesen", "tokens": ["Lie\u00b7be", ",", "die", "den", "Win\u00b7ter\u00b7wie\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und der Heide Blumen leiht,", "tokens": ["Und", "der", "Hei\u00b7de", "Blu\u00b7men", "leiht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Leiht auf Erden Paradiesen", "tokens": ["Leiht", "auf", "Er\u00b7den", "Pa\u00b7ra\u00b7die\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Schon des Himmels Seligkeit.", "tokens": ["Schon", "des", "Him\u00b7mels", "Se\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wonne wird dein Herz erheben,", "tokens": ["Won\u00b7ne", "wird", "dein", "Herz", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wandelst du im Erlenthal,", "tokens": ["Wan\u00b7delst", "du", "im", "Er\u00b7len\u00b7thal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Oder bei des H\u00fcgels Reben", "tokens": ["O\u00b7der", "bei", "des", "H\u00fc\u00b7gels", "Re\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In der Sonne Scheidestrahl. \u2013", "tokens": ["In", "der", "Son\u00b7ne", "Schei\u00b7des\u00b7trahl", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wann auf Schneegebirgen milder", "tokens": ["Wann", "auf", "Schnee\u00b7ge\u00b7bir\u00b7gen", "mil\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Rosenfarbner Schimmer ruht;", "tokens": ["Ro\u00b7sen\u00b7farb\u00b7ner", "Schim\u00b7mer", "ruht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dunkler, purpurn, ihre Bilder", "tokens": ["Dunk\u00b7ler", ",", "pur\u00b7purn", ",", "ih\u00b7re", "Bil\u00b7der"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "$,", "NE", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Strahlen in des Sees Flut.", "tokens": ["Strah\u00b7len", "in", "des", "Sees", "Flut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Wo des Nebels matter Fl\u00fcgel", "tokens": ["Wo", "des", "Ne\u00b7bels", "mat\u00b7ter", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht auf flache S\u00fcmpfe sinkt,", "tokens": ["Nicht", "auf", "fla\u00b7che", "S\u00fcmp\u00b7fe", "sinkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und am gr\u00fcnen Tannenh\u00fcgel", "tokens": ["Und", "am", "gr\u00fc\u00b7nen", "Tan\u00b7nen\u00b7h\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Klarer Quellen F\u00fcll' entspringt,", "tokens": ["Kla\u00b7rer", "Quel\u00b7len", "F\u00fcll'", "ent\u00b7springt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo in deines Gartens Linden", "tokens": ["Wo", "in", "dei\u00b7nes", "Gar\u00b7tens", "Lin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PPOSAT", "NN", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Reine, heitre L\u00fcfte wehn,", "tokens": ["Rei\u00b7ne", ",", "heit\u00b7re", "L\u00fcf\u00b7te", "wehn", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Werd' ich, Bester! einst dich finden:", "tokens": ["Werd'", "ich", ",", "Bes\u00b7ter", "!", "einst", "dich", "fin\u00b7den", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "NN", "$.", "ADV", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Lebe wohl! \u2013 Auf Wiedersehn!", "tokens": ["Le\u00b7be", "wohl", "!", "\u2013", "Auf", "Wie\u00b7der\u00b7sehn", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$.", "$(", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}