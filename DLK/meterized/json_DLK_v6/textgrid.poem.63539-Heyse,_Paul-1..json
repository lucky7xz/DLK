{"textgrid.poem.63539": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und schau' ich fremd und seltsam drein,", "tokens": ["Und", "schau'", "ich", "fremd", "und", "selt\u00b7sam", "drein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mu\u00dft doch mein treuer Liebster sein.", "tokens": ["Mu\u00dft", "doch", "mein", "treu\u00b7er", "Liebs\u00b7ter", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist nicht meine Schuld, ist nur mein Schmerz,", "tokens": ["Ist", "nicht", "mei\u00b7ne", "Schuld", ",", "ist", "nur", "mein", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "PPOSAT", "NN", "$,", "VAFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Deine Lieb allein schafft mir ein Herz.", "tokens": ["Dei\u00b7ne", "Lieb", "al\u00b7lein", "schafft", "mir", "ein", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Bist du bei mir, deine Hand mich kost,", "tokens": ["Bist", "du", "bei", "mir", ",", "dei\u00b7ne", "Hand", "mich", "kost", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "$,", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Kommt's \u00fcber mich wie Himmelstrost.", "tokens": ["Kommt's", "\u00fc\u00b7ber", "mich", "wie", "Him\u00b7mel\u00b7strost", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "KOKOM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur manchmal bricht's durch Gl\u00fcck und Ruh:", "tokens": ["Nur", "manch\u00b7mal", "bricht's", "durch", "Gl\u00fcck", "und", "Ruh", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Melusine herzest du!", "tokens": ["Die", "Me\u00b7lu\u00b7si\u00b7ne", "her\u00b7zest", "du", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Doch wenn du gingst \u2013 mir friert der Leib,", "tokens": ["Doch", "wenn", "du", "gingst", "\u2013", "mir", "friert", "der", "Leib", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$(", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bin wieder ein elend Nixenweib.", "tokens": ["Bin", "wie\u00b7der", "ein", "e\u00b7lend", "Ni\u00b7xen\u00b7weib", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJD", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Mir sagen die blutigen Tr\u00e4nen nur,", "tokens": ["Mir", "sa\u00b7gen", "die", "blu\u00b7ti\u00b7gen", "Tr\u00e4\u00b7nen", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Da\u00df ich von Einem Liebes erfuhr!", "tokens": ["Da\u00df", "ich", "von", "Ei\u00b7nem", "Lie\u00b7bes", "er\u00b7fuhr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "\u2013 Ich bin ja dein, ich bin ja jung,", "tokens": ["\u2013", "Ich", "bin", "ja", "dein", ",", "ich", "bin", "ja", "jung", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "PPOSAT", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu jedem Wagen k\u00fchn genung.", "tokens": ["Zu", "je\u00b7dem", "Wa\u00b7gen", "k\u00fchn", "ge\u00b7nung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O glaub an mich! O sag mir an,", "tokens": ["O", "glaub", "an", "mich", "!", "O", "sag", "mir", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPER", "$.", "NE", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie ich den Zauber brechen kann! \u2013", "tokens": ["Wie", "ich", "den", "Zau\u00b7ber", "bre\u00b7chen", "kann", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVINF", "VMFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "\u2013 Ist's nicht genug der Himmelslust,", "tokens": ["\u2013", "Ist's", "nicht", "ge\u00b7nug", "der", "Him\u00b7mels\u00b7lust", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "PTKNEG", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Weib zu sein an deiner Brust?", "tokens": ["Ein", "Weib", "zu", "sein", "an", "dei\u00b7ner", "Brust", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VAINF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Zauber bricht der Tod allein.", "tokens": ["Den", "Zau\u00b7ber", "bricht", "der", "Tod", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Komm! la\u00df uns lachen und selig sein!", "tokens": ["Komm", "!", "la\u00df", "uns", "la\u00b7chen", "und", "se\u00b7lig", "sein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "VVIMP", "PPER", "VVINF", "KON", "ADJD", "VAINF", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}