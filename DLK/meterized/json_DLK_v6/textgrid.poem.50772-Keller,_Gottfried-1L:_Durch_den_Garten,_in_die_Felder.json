{"textgrid.poem.50772": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Durch den Garten, in die Felder", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch den Garten, in die Felder", "tokens": ["Durch", "den", "Gar\u00b7ten", ",", "in", "die", "Fel\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Irre ich mit dunkeln Augen,", "tokens": ["Ir\u00b7re", "ich", "mit", "dun\u00b7keln", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Achte nicht, wie tausend Kelche", "tokens": ["Ach\u00b7te", "nicht", ",", "wie", "tau\u00b7send", "Kel\u00b7che"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PTKNEG", "$,", "PWAV", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Licht und \u00c4ther um mich saugen.", "tokens": ["Licht", "und", "\u00c4\u00b7ther", "um", "mich", "sau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mu\u00df der Mai mit holdem Lachen", "tokens": ["Mu\u00df", "der", "Mai", "mit", "hol\u00b7dem", "La\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mir denn eine Leiche geben,", "tokens": ["Mir", "denn", "ei\u00b7ne", "Lei\u00b7che", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "W\u00e4hrend meine Freunde haschen", "tokens": ["W\u00e4h\u00b7rend", "mei\u00b7ne", "Freun\u00b7de", "ha\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Neue Liebe, warmes Leben?", "tokens": ["Neu\u00b7e", "Lie\u00b7be", ",", "war\u00b7mes", "Le\u00b7ben", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aber sagt, wie kommt es mir denn,", "tokens": ["A\u00b7ber", "sagt", ",", "wie", "kommt", "es", "mir", "denn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "VVFIN", "PPER", "PPER", "ADV", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Da\u00df durch meines Grames Schatten", "tokens": ["Da\u00df", "durch", "mei\u00b7nes", "Gra\u00b7mes", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch die Sonnenstrahlen dringen", "tokens": ["Doch", "die", "Son\u00b7nen\u00b7strah\u00b7len", "drin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sich mit den Schmerzen gatten?", "tokens": ["Und", "sich", "mit", "den", "Schmer\u00b7zen", "gat\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df der Lenz mit seinen Reizen", "tokens": ["Da\u00df", "der", "Lenz", "mit", "sei\u00b7nen", "Rei\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mir noch zehnmal \u00fcpp'ger scheinet", "tokens": ["Mir", "noch", "zehn\u00b7mal", "\u00fcpp'\u00b7ger", "schei\u00b7net"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADV", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und mit seinem alten Schmucke", "tokens": ["Und", "mit", "sei\u00b7nem", "al\u00b7ten", "Schmu\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Eine neue Sch\u00f6nheit einet?", "tokens": ["Ei\u00b7ne", "neu\u00b7e", "Sch\u00f6n\u00b7heit", "ei\u00b7net", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ja, die todeskranke Liebe", "tokens": ["Ja", ",", "die", "to\u00b7des\u00b7kran\u00b7ke", "Lie\u00b7be"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einen Geisterabglanz gie\u00dfet", "tokens": ["Ei\u00b7nen", "Geis\u00b7ter\u00b7ab\u00b7glanz", "gie\u00b7\u00dfet"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00dcber all die Lenzesf\u00fclle,", "tokens": ["\u00dc\u00b7ber", "all", "die", "Len\u00b7zes\u00b7f\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die da dr\u00e4ngt und bl\u00fcht und sprie\u00dfet!", "tokens": ["Die", "da", "dr\u00e4ngt", "und", "bl\u00fcht", "und", "sprie\u00b7\u00dfet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hunderttausend Blumen wollen", "tokens": ["Hun\u00b7dert\u00b7tau\u00b7send", "Blu\u00b7men", "wol\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["CARD", "NN", "VMFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und noch viel mehr Knospen eilen,", "tokens": ["Und", "noch", "viel", "mehr", "Knos\u00b7pen", "ei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Solche Feier zu erleben.", "tokens": ["Sol\u00b7che", "Fei\u00b7er", "zu", "er\u00b7le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sehet da, die wei\u00dfen Lilien", "tokens": ["Se\u00b7het", "da", ",", "die", "wei\u00b7\u00dfen", "Li\u00b7li\u00b7en"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Sind vor ihrer Zeit gekommen,", "tokens": ["Sind", "vor", "ih\u00b7rer", "Zeit", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Als sie von der Blumentrauer", "tokens": ["Als", "sie", "von", "der", "Blu\u00b7men\u00b7trau\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rings im weiten Land vernommen;", "tokens": ["Rings", "im", "wei\u00b7ten", "Land", "ver\u00b7nom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ihre Schwester zu begleiten,", "tokens": ["Ih\u00b7re", "Schwes\u00b7ter", "zu", "be\u00b7glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bl\u00fchen sie in langen Reihen,", "tokens": ["Bl\u00fc\u00b7hen", "sie", "in", "lan\u00b7gen", "Rei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "W\u00e4hrend sie aus ihren Kelchen", "tokens": ["W\u00e4h\u00b7rend", "sie", "aus", "ih\u00b7ren", "Kel\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Weihrauch in die L\u00fcfte streuen.", "tokens": ["Weih\u00b7rauch", "in", "die", "L\u00fcf\u00b7te", "streu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und die Abendr\u00f6te schlingt sich", "tokens": ["Und", "die", "A\u00b7bend\u00b7r\u00f6\u00b7te", "schlingt", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PRF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6n in rosigen Girlanden", "tokens": ["Sch\u00f6n", "in", "ro\u00b7si\u00b7gen", "Gir\u00b7lan\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Um die hohen Silberberge,", "tokens": ["Um", "die", "ho\u00b7hen", "Sil\u00b7ber\u00b7ber\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+---", "measure": "unknown.measure.tri"}, "line.4": {"text": "Die noch eben sonnig standen;", "tokens": ["Die", "noch", "e\u00b7ben", "son\u00b7nig", "stan\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und der Hesperus dort funkelt", "tokens": ["Und", "der", "Hes\u00b7pe\u00b7rus", "dort", "fun\u00b7kelt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NE", "ADV", "VVFIN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Als der Zeremonienmeister,", "tokens": ["Als", "der", "Ze\u00b7re\u00b7mo\u00b7ni\u00b7en\u00b7meis\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,"], "meter": "+-+----+-", "measure": "unknown.measure.tri"}, "line.7": {"text": "Rufend in die weiten Sph\u00e4ren", "tokens": ["Ru\u00b7fend", "in", "die", "wei\u00b7ten", "Sph\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Alle guten Sternengeister!", "tokens": ["Al\u00b7le", "gu\u00b7ten", "Ster\u00b7nen\u00b7geis\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Alle Silberbronnen klingen,", "tokens": ["Al\u00b7le", "Sil\u00b7ber\u00b7bron\u00b7nen", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alle Nachtigallen schlagen \u2013", "tokens": ["Al\u00b7le", "Nach\u00b7ti\u00b7gal\u00b7len", "schla\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jetzt seh ich die Blumenleiche", "tokens": ["Jetzt", "seh", "ich", "die", "Blu\u00b7men\u00b7lei\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Schwankend \u00fcber die Auen tragen;", "tokens": ["Schwan\u00b7kend", "\u00fc\u00b7ber", "die", "Au\u00b7en", "tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Morgenr\u00f6ten, Abendr\u00f6ten,", "tokens": ["Mor\u00b7gen\u00b7r\u00f6\u00b7ten", ",", "A\u00b7bend\u00b7r\u00f6\u00b7ten", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wetterleuchten, Regenbogen,", "tokens": ["Wet\u00b7ter\u00b7leuch\u00b7ten", ",", "Re\u00b7gen\u00b7bo\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Alles Sch\u00f6ne kommt der Bahre", "tokens": ["Al\u00b7les", "Sch\u00f6\u00b7ne", "kommt", "der", "Bah\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Trauerfunkelnd nachgezogen!", "tokens": ["Trau\u00b7er\u00b7fun\u00b7kelnd", "nach\u00b7ge\u00b7zo\u00b7gen", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Sagt, wann wird der T\u00e4uschung Schleier", "tokens": ["Sagt", ",", "wann", "wird", "der", "T\u00e4u\u00b7schung", "Schlei\u00b7er"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "VAFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Endlich mir vom Aug gehoben?", "tokens": ["End\u00b7lich", "mir", "vom", "Aug", "ge\u00b7ho\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Unverw\u00fcstlich sind die Dichter,", "tokens": ["Un\u00b7ver\u00b7w\u00fcst\u00b7lich", "sind", "die", "Dich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles wird zum Traum verwoben;", "tokens": ["Al\u00b7les", "wird", "zum", "Traum", "ver\u00b7wo\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Selbst der nahe Tod wird spielend", "tokens": ["Selbst", "der", "na\u00b7he", "Tod", "wird", "spie\u00b7lend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "VAFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Noch mit Schein und Tand umschlungen \u2013", "tokens": ["Noch", "mit", "Schein", "und", "Tand", "um\u00b7schlun\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Oh, ich glaube, er ist eben", "tokens": ["Oh", ",", "ich", "glau\u00b7be", ",", "er", "ist", "e\u00b7ben"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Eisig in ein Herz gedrungen!", "tokens": ["Ei\u00b7sig", "in", "ein", "Herz", "ge\u00b7drun\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}