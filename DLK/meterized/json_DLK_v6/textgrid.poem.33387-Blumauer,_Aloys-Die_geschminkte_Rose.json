{"textgrid.poem.33387": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Die geschminkte Rose", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An seinem Fenster pinselte", "tokens": ["An", "sei\u00b7nem", "Fens\u00b7ter", "pin\u00b7sel\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ein Maler eine Rose je,", "tokens": ["Ein", "Ma\u00b7ler", "ei\u00b7ne", "Ro\u00b7se", "je", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und weil sie nicht bestellet war,", "tokens": ["Und", "weil", "sie", "nicht", "be\u00b7stel\u00b7let", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PTKNEG", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gelang die Ros' ihm wunderbar:", "tokens": ["Ge\u00b7lang", "die", "Ros'", "ihm", "wun\u00b7der\u00b7bar", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nun war er fertig, nahm den Hut,", "tokens": ["Nun", "war", "er", "fer\u00b7tig", ",", "nahm", "den", "Hut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ging seines Weg's, und dachte: \u2013 gut!", "tokens": ["Ging", "sei\u00b7nes", "Weg's", ",", "und", "dach\u00b7te", ":", "\u2013", "gut", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "KON", "VVFIN", "$.", "$(", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und eine weisse Rose, die", "tokens": ["Und", "ei\u00b7ne", "weis\u00b7se", "Ro\u00b7se", ",", "die"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "PRELS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Am Fenster bl\u00fchte, sah es, wie,", "tokens": ["Am", "Fens\u00b7ter", "bl\u00fch\u00b7te", ",", "sah", "es", ",", "wie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "VVFIN", "PPER", "$,", "PWAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "So sch\u00f6n das Roth der Schwester fand:", "tokens": ["So", "sch\u00f6n", "das", "Roth", "der", "Schwes\u00b7ter", "fand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Den Vorzug sie gar tief empfand.", "tokens": ["Den", "Vor\u00b7zug", "sie", "gar", "tief", "emp\u00b7fand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Sie fa\u00dfte Neid, schalt ihren Topf,", "tokens": ["Sie", "fa\u00df\u00b7te", "Neid", ",", "schalt", "ih\u00b7ren", "Topf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Zergr\u00e4mte sich und hing den Kopf.", "tokens": ["Zer\u00b7gr\u00e4m\u00b7te", "sich", "und", "hing", "den", "Kopf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Als nun der Maler wieder kam,", "tokens": ["Als", "nun", "der", "Ma\u00b7ler", "wie\u00b7der", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Und wahr der Rose Trauern nahm,", "tokens": ["Und", "wahr", "der", "Ro\u00b7se", "Trau\u00b7ern", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Da trat er mit dem Spritzkrug hin,", "tokens": ["Da", "trat", "er", "mit", "dem", "Spritz\u00b7krug", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Zu H\u00fclf' der armen Kr\u00e4nklerin;", "tokens": ["Zu", "H\u00fcl\u00b7f'", "der", "ar\u00b7men", "Kr\u00e4nk\u00b7le\u00b7rin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Allein die Rose str\u00e4ubte sich,", "tokens": ["Al\u00b7lein", "die", "Ro\u00b7se", "str\u00e4ub\u00b7te", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Und klagte bitter: Lasset mich!", "tokens": ["Und", "klag\u00b7te", "bit\u00b7ter", ":", "Las\u00b7set", "mich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$.", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Was hilft mir euer Wasserkrug?", "tokens": ["Was", "hilft", "mir", "eu\u00b7er", "Was\u00b7ser\u00b7krug", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Ich bleibe doch wie Leichentuch!", "tokens": ["Ich", "blei\u00b7be", "doch", "wie", "Lei\u00b7chen\u00b7tuch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KOKOM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "So roth, wie die dort, werd' ich nie,", "tokens": ["So", "roth", ",", "wie", "die", "dort", ",", "werd'", "ich", "nie", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWAV", "ART", "ADV", "$,", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Und bin doch auch so gut, wie sie.", "tokens": ["Und", "bin", "doch", "auch", "so", "gut", ",", "wie", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "ADV", "ADJD", "$,", "PWAV", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Das Wasser bleicht mich nur noch mehr:", "tokens": ["Das", "Was\u00b7ser", "bleicht", "mich", "nur", "noch", "mehr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Nehmt lieber euern Pinsel her,", "tokens": ["Nehmt", "lie\u00b7ber", "eu\u00b7ern", "Pin\u00b7sel", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Und gebt mir auch so sch\u00f6nes Roth;", "tokens": ["Und", "gebt", "mir", "auch", "so", "sch\u00f6\u00b7nes", "Roth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Ich bin ja sonst so bla\u00df, wie Tod.", "tokens": ["Ich", "bin", "ja", "sonst", "so", "bla\u00df", ",", "wie", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADV", "ADJD", "$,", "PWAV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Der Maler dacht' in seinem Sinn,", "tokens": ["Der", "Ma\u00b7ler", "dacht'", "in", "sei\u00b7nem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Du eitle N\u00e4rrin! nahm Karmin,", "tokens": ["Du", "eit\u00b7le", "N\u00e4r\u00b7rin", "!", "nahm", "Kar\u00b7min", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Und strich ihr roth die Bl\u00e4tter all',", "tokens": ["Und", "strich", "ihr", "roth", "die", "Bl\u00e4t\u00b7ter", "all'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ART", "NN", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Das dankte sie ihm tausendmal;", "tokens": ["Das", "dank\u00b7te", "sie", "ihm", "tau\u00b7send\u00b7mal", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Allein kaum war die Farbe d'ran,", "tokens": ["Al\u00b7lein", "kaum", "war", "die", "Far\u00b7be", "d'\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.32": {"text": "So fieng sie auch zu welken an.", "tokens": ["So", "fi\u00b7eng", "sie", "auch", "zu", "wel\u00b7ken", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.33": {"text": "Das Roth verdarb den Lebenssaft,", "tokens": ["Das", "Roth", "ver\u00b7darb", "den", "Le\u00b7bens\u00b7saft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Zerfra\u00df der zarten Fibern Kraft,", "tokens": ["Zer\u00b7fra\u00df", "der", "zar\u00b7ten", "Fi\u00b7bern", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Gelbrothe Flecken zeigten sich,", "tokens": ["Gel\u00b7bro\u00b7the", "Fle\u00b7cken", "zeig\u00b7ten", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Zusammenschrumpften j\u00e4mmerlich", "tokens": ["Zu\u00b7sam\u00b7men\u00b7schrumpf\u00b7ten", "j\u00e4m\u00b7mer\u00b7lich"], "token_info": ["word", "word"], "pos": ["NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Die Bl\u00e4tter alle, und ihr Duft", "tokens": ["Die", "Bl\u00e4t\u00b7ter", "al\u00b7le", ",", "und", "ihr", "Duft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PIS", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "War Odem einer Leichengruft.", "tokens": ["War", "O\u00b7dem", "ei\u00b7ner", "Lei\u00b7chen\u00b7gruft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Der Maler kam, und sah, und roch:", "tokens": ["Der", "Ma\u00b7ler", "kam", ",", "und", "sah", ",", "und", "roch", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "VVFIN", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Gott, rief er, das die Rose noch,", "tokens": ["Gott", ",", "rief", "er", ",", "das", "die", "Ro\u00b7se", "noch", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "PRELS", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Die gestern so den Text mir las,", "tokens": ["Die", "ge\u00b7stern", "so", "den", "Text", "mir", "las", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.42": {"text": "Heut' stinkend, wie ein faules Aas? \u2013", "tokens": ["Heut'", "stin\u00b7kend", ",", "wie", "ein", "fau\u00b7les", "Aas", "?", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "$,", "PWAV", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Er ri\u00df die Rose von dem Stock,", "tokens": ["Er", "ri\u00df", "die", "Ro\u00b7se", "von", "dem", "Stock", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Im Hut sie \u00fcber's Fenster flog", "tokens": ["Im", "Hut", "sie", "\u00fc\u00b7ber's", "Fens\u00b7ter", "flog"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPER", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "H\u00f6rt, M\u00e4dchen, was die Fabel spricht,", "tokens": ["H\u00f6rt", ",", "M\u00e4d\u00b7chen", ",", "was", "die", "Fa\u00b7bel", "spricht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und malt die weisse Rose nicht.", "tokens": ["Und", "malt", "die", "weis\u00b7se", "Ro\u00b7se", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}