{"textgrid.poem.25553": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Meine Thr\u00e4nen sind geweint!", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meine Thr\u00e4nen sind geweint!", "tokens": ["Mei\u00b7ne", "Thr\u00e4\u00b7nen", "sind", "ge\u00b7weint", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine Seufzer sind verflogen!", "tokens": ["Mei\u00b7ne", "Seuf\u00b7zer", "sind", "ver\u00b7flo\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ruhig bin ich, keinem feind,", "tokens": ["Ru\u00b7hig", "bin", "ich", ",", "kei\u00b7nem", "feind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Selbst nicht der, die mich betrogen,", "tokens": ["Selbst", "nicht", "der", ",", "die", "mich", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ART", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Zwar wie liegt die M\u00fcdigkeit", "tokens": ["Zwar", "wie", "liegt", "die", "M\u00fc\u00b7dig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Schwer auf meinem ganzen Wesen!", "tokens": ["Schwer", "auf", "mei\u00b7nem", "gan\u00b7zen", "We\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Aber nur noch kurze Zeit,", "tokens": ["A\u00b7ber", "nur", "noch", "kur\u00b7ze", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Kranker! und du bist genesen!", "tokens": ["Kran\u00b7ker", "!", "und", "du", "bist", "ge\u00b7ne\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KON", "PPER", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "O! dem Ekel sey es Dank,", "tokens": ["O", "!", "dem", "E\u00b7kel", "sey", "es", "Dank", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ART", "NN", "VAFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Da\u00df er gern den Gram begleitet,", "tokens": ["Da\u00df", "er", "gern", "den", "Gram", "be\u00b7glei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Da\u00df er g\u00fctig Speis' und Trank", "tokens": ["Da\u00df", "er", "g\u00fc\u00b7tig", "Speis'", "und", "Trank"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Mir mit Wermuth zubereitet;", "tokens": ["Mir", "mit", "Wer\u00b7muth", "zu\u00b7be\u00b7rei\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "VVPP", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.13": {"text": "Denn in jedem Bissen Brod", "tokens": ["Denn", "in", "je\u00b7dem", "Bis\u00b7sen", "Brod"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Und in jedem Tropfen Weine,", "tokens": ["Und", "in", "je\u00b7dem", "Trop\u00b7fen", "Wei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "N\u00e4hm' und tr\u00e4nk' ich sp\u00e4tern Tod", "tokens": ["N\u00e4hm'", "und", "tr\u00e4nk'", "ich", "sp\u00e4\u00b7tern", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "VVFIN", "PPER", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "In die schmachtenden Gebeine.", "tokens": ["In", "die", "schmach\u00b7ten\u00b7den", "Ge\u00b7bei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Ha! zum allerersten mal", "tokens": ["Ha", "!", "zum", "al\u00b7le\u00b7rers\u00b7ten", "mal"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Seh' ich mich vergn\u00fcgt im Spiegel!", "tokens": ["Seh'", "ich", "mich", "ver\u00b7gn\u00fcgt", "im", "Spie\u00b7gel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "VVPP", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Welch ein d\u00fcrres wei\u00dfes Thal", "tokens": ["Welch", "ein", "d\u00fcr\u00b7res", "wei\u00b7\u00dfes", "Thal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "Sind itzt diese Rosenh\u00fcgel", "tokens": ["Sind", "itzt", "die\u00b7se", "Ro\u00b7sen\u00b7h\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Meiner Wangen? wie so klein,", "tokens": ["Mei\u00b7ner", "Wan\u00b7gen", "?", "wie", "so", "klein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PWAV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Wie so d\u00fcster diese Sonnen?", "tokens": ["Wie", "so", "d\u00fcs\u00b7ter", "die\u00b7se", "Son\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Suada, Scherz und Schmeicheleyn,", "tokens": ["Sua\u00b7da", ",", "Scherz", "und", "Schmei\u00b7che\u00b7leyn", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Sind von meinem Mund' entronnen.", "tokens": ["Sind", "von", "mei\u00b7nem", "Mund'", "ent\u00b7ron\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Nur noch wenig, wenig Fluth", "tokens": ["Nur", "noch", "we\u00b7nig", ",", "we\u00b7nig", "Fluth"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "PIS", "$,", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.26": {"text": "Treibt des Herzens tr\u00e4ge M\u00fchle;", "tokens": ["Treibt", "des", "Her\u00b7zens", "tr\u00e4\u00b7ge", "M\u00fch\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Bald ihr m\u00fcden F\u00fc\u00dfe ruht,", "tokens": ["Bald", "ihr", "m\u00fc\u00b7den", "F\u00fc\u00b7\u00dfe", "ruht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "Ruht euch aus am nahen Ziele!", "tokens": ["Ruht", "euch", "aus", "am", "na\u00b7hen", "Zie\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Ach! Gehirn! dein Feuer macht", "tokens": ["Ach", "!", "Ge\u00b7hirn", "!", "dein", "Feu\u00b7er", "macht"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "NN", "$.", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.30": {"text": "Meines Lebens Abend schw\u00fcle.", "tokens": ["Mei\u00b7nes", "Le\u00b7bens", "A\u00b7bend", "schw\u00fc\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Aber sieh! da kommt die Nacht!", "tokens": ["A\u00b7ber", "sieh", "!", "da", "kommt", "die", "Nacht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$.", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.32": {"text": "Diese bringet mich ins K\u00fchle.", "tokens": ["Die\u00b7se", "brin\u00b7get", "mich", "ins", "K\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Todesnacht! sollt' ich in dir,", "tokens": ["To\u00b7des\u00b7nacht", "!", "sollt'", "ich", "in", "dir", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VMFIN", "PPER", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.34": {"text": "Ungewi\u00df, wie lange? schlafen,", "tokens": ["Un\u00b7ge\u00b7wi\u00df", ",", "wie", "lan\u00b7ge", "?", "schla\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ADV", "$.", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "O! wie k\u00f6nnte schon mich hier", "tokens": ["O", "!", "wie", "k\u00f6nn\u00b7te", "schon", "mich", "hier"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "PWAV", "VMFIN", "ADV", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.36": {"text": "Die Natur wohl h\u00e4rter strafen?", "tokens": ["Die", "Na\u00b7tur", "wohl", "h\u00e4r\u00b7ter", "stra\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Schlafen, oder nicht mehr seyn,", "tokens": ["Schla\u00b7fen", ",", "o\u00b7der", "nicht", "mehr", "seyn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "PTKNEG", "ADV", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.38": {"text": "Das ist Eins, eh' er's erf\u00e4hret;", "tokens": ["Das", "ist", "Eins", ",", "eh'", "er's", "er\u00b7f\u00e4h\u00b7ret", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "$,", "KOUS", "PIS", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.39": {"text": "Ruhe werde dem Gebein',", "tokens": ["Ru\u00b7he", "wer\u00b7de", "dem", "Ge\u00b7bein'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.40": {"text": "Und Gef\u00fchl dem Geist' gew\u00e4hret.", "tokens": ["Und", "Ge\u00b7f\u00fchl", "dem", "Geist'", "ge\u00b7w\u00e4h\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Wieder wachen wirst du Geist!", "tokens": ["Wie\u00b7der", "wa\u00b7chen", "wirst", "du", "Geist", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VAFIN", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.42": {"text": "Zwar wie liegt die trockne H\u00fclle,", "tokens": ["Zwar", "wie", "liegt", "die", "trock\u00b7ne", "H\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.43": {"text": "Die der Schmetterling zerrei\u00dft,", "tokens": ["Die", "der", "Schmet\u00b7ter\u00b7ling", "zer\u00b7rei\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.44": {"text": "Gleich als schlief er noch, so stille?", "tokens": ["Gleich", "als", "schlief", "er", "noch", ",", "so", "stil\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "VVFIN", "PPER", "ADV", "$,", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.45": {"text": "Aber sieh! dort fliegt er schon", "tokens": ["A\u00b7ber", "sieh", "!", "dort", "fliegt", "er", "schon"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "$.", "ADV", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.46": {"text": "Auf die blaue Veilchen-Aue,", "tokens": ["Auf", "die", "blau\u00b7e", "Veil\u00b7chen\u00b7Aue", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.47": {"text": "Sauget Honig aus dem Mohn,", "tokens": ["Sau\u00b7get", "Ho\u00b7nig", "aus", "dem", "Mohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.48": {"text": "Oder trinkt vom Rosenthaue.", "tokens": ["O\u00b7der", "trinkt", "vom", "Ro\u00b7sent\u00b7hau\u00b7e", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.49": {"text": "Doch, o Seele! sey auch wach:", "tokens": ["Doch", ",", "o", "See\u00b7le", "!", "sey", "auch", "wach", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "FM", "NN", "$.", "VAFIN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.50": {"text": "Wirst du diese Welt nicht missen?", "tokens": ["Wirst", "du", "die\u00b7se", "Welt", "nicht", "mis\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PDAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.51": {"text": "Wirst du noch von ", "tokens": ["Wirst", "du", "noch", "von"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "APPR"], "meter": "-+-+", "measure": "iambic.di"}, "line.52": {"text": "Dort gewi\u00df ", "tokens": ["Dort", "ge\u00b7wi\u00df"], "token_info": ["word", "word"], "pos": ["ADV", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.53": {"text": "Wirst du, oder wirst du nicht? \u2013", "tokens": ["Wirst", "du", ",", "o\u00b7der", "wirst", "du", "nicht", "?", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "$,", "KON", "VAFIN", "PPER", "PTKNEG", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.54": {"text": "Nicht? \u2013 Entsetzen! Tod! Erbarmen!", "tokens": ["Nicht", "?", "\u2013", "Ent\u00b7set\u00b7zen", "!", "Tod", "!", "Er\u00b7bar\u00b7men", "!"], "token_info": ["word", "punct", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "$.", "$(", "NN", "$.", "NN", "$.", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.55": {"text": "Schone! sieh! mein Herz zerbricht!", "tokens": ["Scho\u00b7ne", "!", "sieh", "!", "mein", "Herz", "zer\u00b7bricht", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVIMP", "$.", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.56": {"text": "M\u00f6rder! fort aus meinen Armen!", "tokens": ["M\u00f6r\u00b7der", "!", "fort", "aus", "mei\u00b7nen", "Ar\u00b7men", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PTKVZ", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.57": {"text": "Ahnung? Traum? was ist es? wie?", "tokens": ["Ah\u00b7nung", "?", "Traum", "?", "was", "ist", "es", "?", "wie", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "PWS", "VAFIN", "PPER", "$.", "PWAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.58": {"text": "Bleibt mein ", "tokens": ["Bleibt", "mein"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPOSAT"], "meter": "-+", "measure": "iambic.single"}, "line.59": {"text": "Bleib' ich hier? und werd' ich sie", "tokens": ["Bleib'", "ich", "hier", "?", "und", "werd'", "ich", "sie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$.", "KON", "VAFIN", "PPER", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.60": {"text": "Wie die dichte Luft umgeben?", "tokens": ["Wie", "die", "dich\u00b7te", "Luft", "um\u00b7ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.61": {"text": "Wann die Reu' in ihr erwacht,", "tokens": ["Wann", "die", "Reu'", "in", "ihr", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.62": {"text": "Werd' ich Tr\u00f6ster seyn, nicht R\u00e4cher?", "tokens": ["Werd'", "ich", "Tr\u00f6s\u00b7ter", "seyn", ",", "nicht", "R\u00e4\u00b7cher", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "VAINF", "$,", "PTKNEG", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.63": {"text": "Werd' ich? \u2013 Leben! gute Nacht!", "tokens": ["Werd'", "ich", "?", "\u2013", "Le\u00b7ben", "!", "gu\u00b7te", "Nacht", "!"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$.", "$(", "NN", "$.", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.64": {"text": "Gib mir, Tod! den Schlummerbecher!", "tokens": ["Gib", "mir", ",", "Tod", "!", "den", "Schlum\u00b7mer\u00b7be\u00b7cher", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", 