{"textgrid.poem.42713": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es war eine Schnupftabaksdose,", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war eine Schnupftabaksdose,", "tokens": ["Es", "war", "ei\u00b7ne", "Schnupf\u00b7ta\u00b7baks\u00b7do\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die hatte Friedrich der Gro\u00dfe", "tokens": ["Die", "hat\u00b7te", "Fried\u00b7rich", "der", "Gro\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "NE", "ART", "ADJA"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sich selbst geschnitzelt aus Nu\u00dfbaumholz.", "tokens": ["Sich", "selbst", "ge\u00b7schnit\u00b7zelt", "aus", "Nu\u00df\u00b7baum\u00b7holz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVPP", "APPR", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und darauf war sie nat\u00fcrlich stolz.", "tokens": ["Und", "da\u00b7rauf", "war", "sie", "na\u00b7t\u00fcr\u00b7lich", "stolz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Da kam ein Holzwurm gekrochen.", "tokens": ["Da", "kam", "ein", "Holz\u00b7wurm", "ge\u00b7kro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der hatte Nu\u00dfbaum gerochen.", "tokens": ["Der", "hat\u00b7te", "Nu\u00df\u00b7baum", "ge\u00b7ro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Dose erz\u00e4hlte ihm lang und breit", "tokens": ["Die", "Do\u00b7se", "er\u00b7z\u00e4hl\u00b7te", "ihm", "lang", "und", "breit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "KON", "ADJD"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Von Friedrich dem Gro\u00dfen und seiner Zeit.", "tokens": ["Von", "Fried\u00b7rich", "dem", "Gro\u00b7\u00dfen", "und", "sei\u00b7ner", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ART", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Sie nannte den alten Fritz gener\u00f6s.", "tokens": ["Sie", "nann\u00b7te", "den", "al\u00b7ten", "Fritz", "ge\u00b7ne\u00b7r\u00f6s", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NE", "NE", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da aber wurde der Holzwurm nerv\u00f6s", "tokens": ["Da", "a\u00b7ber", "wur\u00b7de", "der", "Holz\u00b7wurm", "ner\u00b7v\u00f6s"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "NE"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und sagte, indem er zu bohren begann:", "tokens": ["Und", "sag\u00b7te", ",", "in\u00b7dem", "er", "zu", "boh\u00b7ren", "be\u00b7gann", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "\u00bbwas geht mich Friedrich der Gro\u00dfe an!\u00ab", "tokens": ["\u00bb", "was", "geht", "mich", "Fried\u00b7rich", "der", "Gro\u00b7\u00dfe", "an", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "VVFIN", "PRF", "NE", "ART", "ADJA", "PTKVZ", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}