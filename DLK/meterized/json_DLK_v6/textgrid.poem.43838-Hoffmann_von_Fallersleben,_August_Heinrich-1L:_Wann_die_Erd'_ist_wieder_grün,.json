{"textgrid.poem.43838": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wann die Erd' ist wieder gr\u00fcn,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann die Erd' ist wieder gr\u00fcn,", "tokens": ["Wann", "die", "Erd'", "ist", "wie\u00b7der", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wann die B\u00e4um' und Blumen bl\u00fch'n,", "tokens": ["Wann", "die", "B\u00e4um'", "und", "Blu\u00b7men", "bl\u00fch'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00f6cht' ich gern ein Vogel sein,", "tokens": ["M\u00f6cht'", "ich", "gern", "ein", "Vo\u00b7gel", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Fliegen in die Welt hinein.", "tokens": ["Flie\u00b7gen", "in", "die", "Welt", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Fliegen m\u00f6cht' ich kreuz und quer", "tokens": ["Flie\u00b7gen", "m\u00f6cht'", "ich", "kreuz", "und", "quer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueber Berge, Fl\u00fcss' und Meer,", "tokens": ["Ue\u00b7ber", "Ber\u00b7ge", ",", "Fl\u00fcss'", "und", "Meer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ueber jeden Busch und Horst,", "tokens": ["Ue\u00b7ber", "je\u00b7den", "Busch", "und", "Horst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ueber jeden Wald und Forst.", "tokens": ["Ue\u00b7ber", "je\u00b7den", "Wald", "und", "Forst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wann die Erd' ist wieder gr\u00fcn,", "tokens": ["Wann", "die", "Erd'", "ist", "wie\u00b7der", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wann die B\u00e4um' und Blumen bl\u00fch'n \u2013", "tokens": ["Wann", "die", "B\u00e4um'", "und", "Blu\u00b7men", "bl\u00fch'n", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "KON", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nein, ich mag kein Vogel sein,", "tokens": ["Nein", ",", "ich", "mag", "kein", "Vo\u00b7gel", "sein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VMFIN", "PIAT", "NN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn die Welt ist dennoch mein.", "tokens": ["Denn", "die", "Welt", "ist", "den\u00b7noch", "mein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wo kein Vogel fliegt und singt,", "tokens": ["Wo", "kein", "Vo\u00b7gel", "fliegt", "und", "singt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wohin mein Gedanke dringt,", "tokens": ["Wo\u00b7hin", "mein", "Ge\u00b7dan\u00b7ke", "dringt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Naht mir aus dem Winterfeld", "tokens": ["Naht", "mir", "aus", "dem", "Win\u00b7ter\u00b7feld"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Eine bunte Fr\u00fchlingswelt.", "tokens": ["Ei\u00b7ne", "bun\u00b7te", "Fr\u00fch\u00b7lings\u00b7welt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}