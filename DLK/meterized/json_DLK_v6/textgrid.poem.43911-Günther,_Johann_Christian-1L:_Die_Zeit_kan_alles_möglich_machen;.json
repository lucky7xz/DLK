{"textgrid.poem.43911": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Zeit kan alles m\u00f6glich machen;", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Zeit kan alles m\u00f6glich machen;", "tokens": ["Die", "Zeit", "kan", "al\u00b7les", "m\u00f6g\u00b7lich", "ma\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PIS", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Drum fa\u00dfe dich nur mit Gedult!", "tokens": ["Drum", "fa\u00b7\u00dfe", "dich", "nur", "mit", "Ge\u00b7dult", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Himmel blizt, die Sp\u00f6tter lachen;", "tokens": ["Der", "Him\u00b7mel", "blizt", ",", "die", "Sp\u00f6t\u00b7ter", "la\u00b7chen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Getrost, es ist nicht unsre Schuld.", "tokens": ["Ge\u00b7trost", ",", "es", "ist", "nicht", "uns\u00b7re", "Schuld", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Anfang unsrer reinen Liebe", "tokens": ["Der", "An\u00b7fang", "uns\u00b7rer", "rei\u00b7nen", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist Unruh, Ungl\u00fcck, Hohn und Pein,", "tokens": ["Ist", "Un\u00b7ruh", ",", "Un\u00b7gl\u00fcck", ",", "Hohn", "und", "Pein", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Mittel ist nicht minder tr\u00fcbe,", "tokens": ["Das", "Mit\u00b7tel", "ist", "nicht", "min\u00b7der", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch soll das Ende lustig seyn.", "tokens": ["Doch", "soll", "das", "En\u00b7de", "lus\u00b7tig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So lebe wohl, du edles Herze,", "tokens": ["So", "le\u00b7be", "wohl", ",", "du", "ed\u00b7les", "Her\u00b7ze", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "PPER", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und denck an deinen Philimen!", "tokens": ["Und", "denck", "an", "dei\u00b7nen", "Phi\u00b7li\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+----", "measure": "unknown.measure.di"}, "line.3": {"text": "Er wandert jezt mit herbem Schmerze", "tokens": ["Er", "wan\u00b7dert", "jezt", "mit", "her\u00b7bem", "Schmer\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und m\u00f6chte fast vor Angst vergehn.", "tokens": ["Und", "m\u00f6ch\u00b7te", "fast", "vor", "Angst", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Dies, was ihn st\u00e4rckt, das sind die K\u00fc\u00dfe,", "tokens": ["Dies", ",", "was", "ihn", "st\u00e4rckt", ",", "das", "sind", "die", "K\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PPER", "VVFIN", "$,", "PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Womit du ihn vorher versehn.", "tokens": ["Wo\u00b7mit", "du", "ihn", "vor\u00b7her", "ver\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Post ist da, mein Kind, ich schlie\u00dfe:", "tokens": ["Die", "Post", "ist", "da", ",", "mein", "Kind", ",", "ich", "schlie\u00b7\u00dfe", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gott la\u00dfe deinen Wuntsch geschehn!", "tokens": ["Gott", "la\u00b7\u00dfe", "dei\u00b7nen", "Wunt\u00b7sch", "ge\u00b7schehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}