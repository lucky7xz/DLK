{"textgrid.poem.58577": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn man wider Willen noch in sich selbst gefangen bleibet", "tokens": ["Wenn", "man", "wi\u00b7der", "Wil\u00b7len", "noch", "in", "sich", "selbst", "ge\u00b7fan\u00b7gen", "blei\u00b7bet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "NN", "ADV", "APPR", "PRF", "ADV", "ADJD", "VVFIN"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Und bald die, bald jene Lust in uns herrschet und uns treibet,", "tokens": ["Und", "bald", "die", ",", "bald", "je\u00b7ne", "Lust", "in", "uns", "herr\u00b7schet", "und", "uns", "trei\u00b7bet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "$,", "ADV", "PDAT", "NN", "APPR", "PPER", "VVFIN", "KON", "PPER", "VVFIN", "$,"], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Das ist wohl ein harter Dienst, voller Unruh, M\u00fch und Schmerz,", "tokens": ["Das", "ist", "wohl", "ein", "har\u00b7ter", "Dienst", ",", "vol\u00b7ler", "Un\u00b7ruh", ",", "M\u00fch", "und", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,", "ADJA", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Oh, wie klagt und jammert man, oh, wie \u00e4chzet da das Herz!", "tokens": ["Oh", ",", "wie", "klagt", "und", "jam\u00b7mert", "man", ",", "oh", ",", "wie", "\u00e4ch\u00b7zet", "da", "das", "Herz", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VVFIN", "KON", "VVFIN", "PIS", "$,", "FM", "$,", "PWAV", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Sei getrost, bedr\u00fcckte Seel'! so sollst du nicht immer leben;", "tokens": ["Sei", "ge\u00b7trost", ",", "be\u00b7dr\u00fcck\u00b7te", "Seel'", "!", "so", "sollst", "du", "nicht", "im\u00b7mer", "le\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "ADJA", "NN", "$.", "ADV", "VMFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+--+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Gott wird dir zu seiner Zeit wahre Seelenruhe geben.", "tokens": ["Gott", "wird", "dir", "zu", "sei\u00b7ner", "Zeit", "wah\u00b7re", "See\u00b7len\u00b7ru\u00b7he", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "Ei, der Herr kommt selbst in dich, dann verlachst du deine Feind',", "tokens": ["Ei", ",", "der", "Herr", "kommt", "selbst", "in", "dich", ",", "dann", "ver\u00b7lachst", "du", "dei\u00b7ne", "Feind'", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "VVFIN", "ADV", "APPR", "PPER", "$,", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Treiber, Welt und S\u00fcndenlust dann in dir gebunden seind.", "tokens": ["Trei\u00b7ber", ",", "Welt", "und", "S\u00fcn\u00b7den\u00b7lust", "dann", "in", "dir", "ge\u00b7bun\u00b7den", "seind", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "ADV", "APPR", "PPER", "VVPP", "VAFIN", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}}}}}