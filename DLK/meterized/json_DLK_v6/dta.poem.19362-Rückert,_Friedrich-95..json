{"dta.poem.19362": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "95.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der erste K\u00f6nig ist es durch Gewalt geworden,", "tokens": ["Der", "ers\u00b7te", "K\u00f6\u00b7nig", "ist", "es", "durch", "Ge\u00b7walt", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "APPR", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und um zu siegen fehlt' ihm nicht der Muth zu morden.", "tokens": ["Und", "um", "zu", "sie\u00b7gen", "fehlt'", "ihm", "nicht", "der", "Muth", "zu", "mor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUI", "PTKZU", "VVINF", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Auf Blut gegr\u00fcndet, lie\u00df er sterbend seinem Blut", "tokens": ["Auf", "Blut", "ge\u00b7gr\u00fcn\u00b7det", ",", "lie\u00df", "er", "ster\u00b7bend", "sei\u00b7nem", "Blut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVPP", "$,", "VVFIN", "PPER", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Herrschaft und die Lust dazu, doch nicht den Muth.", "tokens": ["Die", "Herr\u00b7schaft", "und", "die", "Lust", "da\u00b7zu", ",", "doch", "nicht", "den", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "PAV", "$,", "ADV", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Zuletzt ist sie herab gelangt an einen weichen,", "tokens": ["Zu\u00b7letzt", "ist", "sie", "her\u00b7ab", "ge\u00b7langt", "an", "ei\u00b7nen", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der sehn kein rothes Blut kann ohne zu erbleichen.", "tokens": ["Der", "sehn", "kein", "ro\u00b7thes", "Blut", "kann", "oh\u00b7ne", "zu", "er\u00b7blei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "ADJA", "NN", "VMFIN", "KOUI", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Er scheut sich in der Hand ein blo\u00dfes Schwert zu tragen,", "tokens": ["Er", "scheut", "sich", "in", "der", "Hand", "ein", "blo\u00b7\u00dfes", "Schwert", "zu", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus Furcht deswegen kann er keine Ritter schlagen.", "tokens": ["Aus", "Furcht", "des\u00b7we\u00b7gen", "kann", "er", "kei\u00b7ne", "Rit\u00b7ter", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PAV", "VMFIN", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Sie aber schlagen sich f\u00fcr ihn nicht minder k\u00fchn,", "tokens": ["Sie", "a\u00b7ber", "schla\u00b7gen", "sich", "f\u00fcr", "ihn", "nicht", "min\u00b7der", "k\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PRF", "APPR", "PPER", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bereit ihr Herzblut f\u00fcr den Blutscheu'n zu verspr\u00fchn.", "tokens": ["Be\u00b7reit", "ihr", "Herz\u00b7blut", "f\u00fcr", "den", "Blut\u00b7scheu'n", "zu", "ver\u00b7spr\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "So ob der Wirklichkeit ist siegreich der Gedanken,", "tokens": ["So", "ob", "der", "Wirk\u00b7lich\u00b7keit", "ist", "sieg\u00b7reich", "der", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "VAFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Unumschr\u00e4nkte setzt dem Schrankenlosen Schranken.", "tokens": ["Der", "Un\u00b7um\u00b7schr\u00e4nk\u00b7te", "setzt", "dem", "Schran\u00b7ken\u00b7lo\u00b7sen", "Schran\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}