{"textgrid.poem.57548": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach, wenn es doch ein Traum gewesen w\u00e4re,", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, wenn es doch ein Traum gewesen w\u00e4re,", "tokens": ["Ach", ",", "wenn", "es", "doch", "ein", "Traum", "ge\u00b7we\u00b7sen", "w\u00e4\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "VAPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "H\u00e4tt ich ", "tokens": ["H\u00e4tt", "ich"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Ich f\u00fchlte nicht so tief des Herzens Leere", "tokens": ["Ich", "f\u00fchl\u00b7te", "nicht", "so", "tief", "des", "Her\u00b7zens", "Lee\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "ADJD", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und lauschte nicht verschollenem Gesang;", "tokens": ["Und", "lauschte", "nicht", "ver\u00b7schol\u00b7le\u00b7nem", "Ge\u00b7sang", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ein sch\u00f6ner Traum ist bald vergessen \u2013", "tokens": ["Ein", "sch\u00f6\u00b7ner", "Traum", "ist", "bald", "ver\u00b7ges\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Man denkt nur selten noch an ihn zur\u00fcck,", "tokens": ["Man", "denkt", "nur", "sel\u00b7ten", "noch", "an", "ihn", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ADJD", "ADV", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und das verwaiste Herz zerfressen", "tokens": ["Und", "das", "ver\u00b7wais\u00b7te", "Herz", "zer\u00b7fres\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Nicht stumme Seufzer um begrabnes Gl\u00fcck,", "tokens": ["Nicht", "stum\u00b7me", "Seuf\u00b7zer", "um", "be\u00b7grab\u00b7nes", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Das Bild verbla\u00dft, zerstiebt wie Meeresschaum,", "tokens": ["Das", "Bild", "ver\u00b7bla\u00dft", ",", "zer\u00b7stiebt", "wie", "Mee\u00b7res\u00b7schaum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "VVFIN", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "O, warum tr\u00e4umt ich auch nicht diesen Traum.", "tokens": ["O", ",", "wa\u00b7rum", "tr\u00e4umt", "ich", "auch", "nicht", "die\u00b7sen", "Traum", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "VVFIN", "PPER", "ADV", "PTKNEG", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Sonne sinkt, vergoldend R\u00fcgens H\u00f6hen,", "tokens": ["Die", "Son\u00b7ne", "sinkt", ",", "ver\u00b7gol\u00b7dend", "R\u00fc\u00b7gens", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VVPP", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie Purpur gl\u00e4nzt des Boddens d\u00fcstre Bucht,", "tokens": ["Wie", "Pur\u00b7pur", "gl\u00e4nzt", "des", "Bod\u00b7dens", "d\u00fcst\u00b7re", "Bucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Im leisen Windhauch zu uns nieder wehen", "tokens": ["Im", "lei\u00b7sen", "Wind\u00b7hauch", "zu", "uns", "nie\u00b7der", "we\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "APPR", "PPER", "PTKVZ", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der M\u00f6we Laute, die ihr Sandbett sucht,", "tokens": ["Der", "M\u00f6\u00b7we", "Lau\u00b7te", ",", "die", "ihr", "Sand\u00b7bett", "sucht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Weither des Leuchtschiffs Lampe flimmert,", "tokens": ["Weit\u00b7her", "des", "Leucht\u00b7schiffs", "Lam\u00b7pe", "flim\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Noch einmal pfeift der Dampfer grell und schrill,", "tokens": ["Noch", "ein\u00b7mal", "pfeift", "der", "Damp\u00b7fer", "grell", "und", "schrill", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "In dem zerfallnen Kloster wimmert", "tokens": ["In", "dem", "zer\u00b7fall\u00b7nen", "Klos\u00b7ter", "wim\u00b7mert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das K\u00e4uzchen \u2013 dann ist alles stumm und still,", "tokens": ["Das", "K\u00e4uz\u00b7chen", "\u2013", "dann", "ist", "al\u00b7les", "stumm", "und", "still", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "VAFIN", "PIS", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Da sa\u00dfen wir, dicht an des Meeres Saum,", "tokens": ["Da", "sa\u00b7\u00dfen", "wir", ",", "dicht", "an", "des", "Mee\u00b7res", "Saum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "So ruhig, stillzufrieden, wie im Traum.", "tokens": ["So", "ru\u00b7hig", ",", "still\u00b7zu\u00b7frie\u00b7den", ",", "wie", "im", "Traum", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "VVFIN", "$,", "PWAV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und sp\u00e4ter, als die Stunde l\u00e4ngst entflogen,", "tokens": ["Und", "sp\u00e4\u00b7ter", ",", "als", "die", "Stun\u00b7de", "l\u00e4ngst", "ent\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "KOUS", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da stand ich wieder dort, es scholl der Schrei", "tokens": ["Da", "stand", "ich", "wie\u00b7der", "dort", ",", "es", "scholl", "der", "Schrei"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "$,", "PPER", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der M\u00f6we gellend \u00fcber schwarze Wogen,", "tokens": ["Der", "M\u00f6\u00b7we", "gel\u00b7lend", "\u00fc\u00b7ber", "schwar\u00b7ze", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Nordwind fauchte grimme Melodei;", "tokens": ["Der", "Nord\u00b7wind", "fauch\u00b7te", "grim\u00b7me", "Me\u00b7lo\u00b7dei", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mit trocknem, starrem Auge schaute", "tokens": ["Mit", "trock\u00b7nem", ",", "star\u00b7rem", "Au\u00b7ge", "schau\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ich trostlos auf das krampfdurchzuckte Meer,", "tokens": ["Ich", "trost\u00b7los", "auf", "das", "krampf\u00b7durch\u00b7zuck\u00b7te", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Kein warmer Hoffnungsschimmer graute", "tokens": ["Kein", "war\u00b7mer", "Hoff\u00b7nungs\u00b7schim\u00b7mer", "grau\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "In dem vereisten, \u00f6den Herzen mehr,", "tokens": ["In", "dem", "ver\u00b7eis\u00b7ten", ",", "\u00f6\u00b7den", "Her\u00b7zen", "mehr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und um den Ort, wo ich getr\u00e4umt den Traum,", "tokens": ["Und", "um", "den", "Ort", ",", "wo", "ich", "ge\u00b7tr\u00e4umt", "den", "Traum", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PWAV", "PPER", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Da spritzte geifriggelber Wellenschaum.", "tokens": ["Da", "spritz\u00b7te", "geif\u00b7rig\u00b7gel\u00b7ber", "Wel\u00b7len\u00b7schaum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wie oft hab ich gew\u00fcnscht in fr\u00fchren Tagen:", "tokens": ["Wie", "oft", "hab", "ich", "ge\u00b7w\u00fcnscht", "in", "fr\u00fch\u00b7ren", "Ta\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was ich getr\u00e4umt, o w\u00e4re es doch wahr!", "tokens": ["Was", "ich", "ge\u00b7tr\u00e4umt", ",", "o", "w\u00e4\u00b7re", "es", "doch", "wahr", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "FM", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich wollte mir das volle Gl\u00fcck erjagen,", "tokens": ["Ich", "woll\u00b7te", "mir", "das", "vol\u00b7le", "Gl\u00fcck", "er\u00b7ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie es im Traum mir wurde offenbar;", "tokens": ["Wie", "es", "im", "Traum", "mir", "wur\u00b7de", "of\u00b7fen\u00b7bar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "NN", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und jetzt, \u2013 o h\u00e4tt ich nie empfunden", "tokens": ["Und", "jetzt", ",", "\u2013", "o", "h\u00e4tt", "ich", "nie", "emp\u00b7fun\u00b7den"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "$(", "FM", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dies schnellzerplatzte, traumgleich kurze Gl\u00fcck,", "tokens": ["Dies", "schnell\u00b7zer\u00b7platz\u00b7te", ",", "traum\u00b7gleich", "kur\u00b7ze", "Gl\u00fcck", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich d\u00e4chte nicht in d\u00fcstern Stunden", "tokens": ["Ich", "d\u00e4ch\u00b7te", "nicht", "in", "d\u00fcs\u00b7tern", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So oft an jenen Abend noch zur\u00fcck,", "tokens": ["So", "oft", "an", "je\u00b7nen", "A\u00b7bend", "noch", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PDAT", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Ich g\u00e4be jetzt den Klagen keinen Raum", "tokens": ["Ich", "g\u00e4\u00b7be", "jetzt", "den", "Kla\u00b7gen", "kei\u00b7nen", "Raum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Um eine Stunde Gl\u00fcck, um einen Traum.", "tokens": ["Um", "ei\u00b7ne", "Stun\u00b7de", "Gl\u00fcck", ",", "um", "ei\u00b7nen", "Traum", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "NN", "$,", "KOUI", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}