{"dta.poem.11725": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du brauchst dein eignes Volk deswegen nicht zu schelten,", "tokens": ["Du", "brauchst", "dein", "eig\u00b7nes", "Volk", "des\u00b7we\u00b7gen", "nicht", "zu", "schel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "PAV", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn du nach ihrem Werth auch andre l\u00e4ssest gelten.", "tokens": ["Wenn", "du", "nach", "ih\u00b7rem", "Werth", "auch", "and\u00b7re", "l\u00e4s\u00b7sest", "gel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "ADV", "PIS", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So, wer in Ehren h\u00e4lt die Formen fremder G\u00f6tter,", "tokens": ["So", ",", "wer", "in", "Eh\u00b7ren", "h\u00e4lt", "die", "For\u00b7men", "frem\u00b7der", "G\u00f6t\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWS", "APPR", "NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist noch deswegen nicht der eignen Laren Sp\u00f6tter.", "tokens": ["Ist", "noch", "des\u00b7we\u00b7gen", "nicht", "der", "eig\u00b7nen", "La\u00b7ren", "Sp\u00f6t\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PAV", "PTKNEG", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Dein eigen Gut und Haus und Volk und Land und Leben,", "tokens": ["Dein", "ei\u00b7gen", "Gut", "und", "Haus", "und", "Volk", "und", "Land", "und", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "NN", "KON", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das ist dein eigner Gott, und drum nicht aufzugeben.", "tokens": ["Das", "ist", "dein", "eig\u00b7ner", "Gott", ",", "und", "drum", "nicht", "auf\u00b7zu\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$,", "KON", "PAV", "PTKNEG", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch wie jetzt Reisende von einem Stamm zum andern,", "tokens": ["Doch", "wie", "jetzt", "Rei\u00b7sen\u00b7de", "von", "ei\u00b7nem", "Stamm", "zum", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "NN", "APPR", "ART", "NN", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zeit ists, da\u00df endlich auch die Gottideen wandern.", "tokens": ["Zeit", "ists", ",", "da\u00df", "end\u00b7lich", "auch", "die", "Got\u00b7ti\u00b7de\u00b7en", "wan\u00b7dern", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "KOUS", "ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Da\u00df sich verst\u00e4ndige die menschliche Gemeine,", "tokens": ["Da\u00df", "sich", "ver\u00b7st\u00e4n\u00b7di\u00b7ge", "die", "menschli\u00b7che", "Ge\u00b7mei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Alles sei Allen gleich, und Jedem sein das Seine.", "tokens": ["Al\u00b7les", "sei", "Al\u00b7len", "gleich", ",", "und", "Je\u00b7dem", "sein", "das", "Sei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "NE", "ADV", "$,", "KON", "PIAT", "PPOSAT", "ART", "NE", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}}}}