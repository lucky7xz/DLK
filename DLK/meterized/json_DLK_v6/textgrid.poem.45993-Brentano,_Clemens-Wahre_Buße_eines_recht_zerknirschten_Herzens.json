{"textgrid.poem.45993": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Wahre Bu\u00dfe eines recht zerknirschten Herzens", "genre": "verse", "period": "N.A.", "pub_year": 1816, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann abends uns die braune Nacht", "tokens": ["Wann", "a\u00b7bends", "uns", "die", "brau\u00b7ne", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Schatten schwarz verkleidet,", "tokens": ["Im", "Schat\u00b7ten", "schwarz", "ver\u00b7klei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und ich dann meine S\u00fcnd' betracht,", "tokens": ["Und", "ich", "dann", "mei\u00b7ne", "S\u00fcnd'", "be\u00b7tracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gro\u00df Not mein Herz erleidet.", "tokens": ["Gro\u00df", "Not", "mein", "Herz", "er\u00b7lei\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Von lauter Leid, von Traurigkeit,", "tokens": ["Von", "lau\u00b7ter", "Leid", ",", "von", "Trau\u00b7rig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mein Augen mir fast rinnen,", "tokens": ["Mein", "Au\u00b7gen", "mir", "fast", "rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Zu'n Sternen auf, so sind im Lauf,", "tokens": ["Zu'n", "Ster\u00b7nen", "auf", ",", "so", "sind", "im", "Lauf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$,", "ADV", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ich schau' mit tr\u00fcben Sinnen.", "tokens": ["Ich", "schau'", "mit", "tr\u00fc\u00b7ben", "Sin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Halt, halt, ihr scheinend Perlen klar,", "tokens": ["Halt", ",", "halt", ",", "ihr", "schei\u00b7nend", "Per\u00b7len", "klar", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "PPER", "ADJD", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr tausend Licht und Fackel,", "tokens": ["Ihr", "tau\u00b7send", "Licht", "und", "Fa\u00b7ckel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "CARD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Halt, halt, ihr wohlgez\u00fcndte Schar,", "tokens": ["Halt", ",", "halt", ",", "ihr", "wohl\u00b7ge\u00b7z\u00fcnd\u00b7te", "Schar", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Feur und Flamm' ohn' Makel,", "tokens": ["Ihr", "Feur", "und", "Flamm'", "ohn'", "Ma\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "O sch\u00f6ne Stern, nicht laufet fern,", "tokens": ["O", "sch\u00f6\u00b7ne", "Stern", ",", "nicht", "lau\u00b7fet", "fern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PTKNEG", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "H\u00f6rt an, was euch will klagen,", "tokens": ["H\u00f6rt", "an", ",", "was", "euch", "will", "kla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "PRELS", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Du sch\u00f6ner Mon auch bleibe stohn,", "tokens": ["Du", "sch\u00f6\u00b7ner", "Mon", "auch", "blei\u00b7be", "stohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "H\u00f6r' an mein Leid und Zagen!", "tokens": ["H\u00f6r'", "an", "mein", "Leid", "und", "Za\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ach, ach! was Angst und Herzenleid!", "tokens": ["Ach", ",", "ach", "!", "was", "Angst", "und", "Her\u00b7zen\u00b7leid", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ITJ", "$.", "PWS", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bin gar mit S\u00fcnd' befangen,", "tokens": ["Bin", "gar", "mit", "S\u00fcnd'", "be\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Auf, auf! ihr hei\u00dfe Br\u00fcnnlein beid,", "tokens": ["Auf", ",", "auf", "!", "ihr", "hei\u00b7\u00dfe", "Br\u00fcnn\u00b7lein", "beid", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "PTKVZ", "$.", "PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nun rauschet mir von Wangen.", "tokens": ["Nun", "rau\u00b7schet", "mir", "von", "Wan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ach sch\u00f6ne Stern, wollt' ich so gern,", "tokens": ["Ach", "sch\u00f6\u00b7ne", "Stern", ",", "wollt'", "ich", "so", "gern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "$,", "VMFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "W\u00e4r' nie von Gott gewichen!", "tokens": ["W\u00e4r'", "nie", "von", "Gott", "ge\u00b7wi\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ach sch\u00f6ner Mon, was hab' ich ton?", "tokens": ["Ach", "sch\u00f6\u00b7ner", "Mon", ",", "was", "hab'", "ich", "ton", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "$,", "PWS", "VAFIN", "PPER", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mein Seel' ist Tods verblichen.", "tokens": ["Mein", "Seel'", "ist", "Tods", "ver\u00b7bli\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Flie\u00df ab, flie\u00df ab, du Tr\u00e4nenbad,", "tokens": ["Flie\u00df", "ab", ",", "flie\u00df", "ab", ",", "du", "Tr\u00e4\u00b7nen\u00b7bad", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr Leid kann dich nit halten.", "tokens": ["F\u00fcr", "Leid", "kann", "dich", "nit", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wasch ab all S\u00fcnd' und Missetat,", "tokens": ["Wasch", "ab", "all", "S\u00fcnd'", "und", "Mis\u00b7se\u00b7tat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Herz ist schon gespalten.", "tokens": ["Das", "Herz", "ist", "schon", "ge\u00b7spal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "O treuer Gott, hab' dein Gebot", "tokens": ["O", "treu\u00b7er", "Gott", ",", "hab'", "dein", "Ge\u00b7bot"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In Wind und Luft geschlagen,", "tokens": ["In", "Wind", "und", "Luft", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "O frommer Herr, von dir so fer'", "tokens": ["O", "from\u00b7mer", "Herr", ",", "von", "dir", "so", "fer'"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "APPR", "PPER", "ADV", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die S\u00fcnd' mich hat getragen!", "tokens": ["Die", "S\u00fcnd'", "mich", "hat", "ge\u00b7tra\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ei wie nun will ich's greifen an?", "tokens": ["Ei", "wie", "nun", "will", "ich's", "grei\u00b7fen", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ADV", "VMFIN", "PIS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Recht mag's nie besch\u00f6nen;", "tokens": ["Mit", "Recht", "mag's", "nie", "be\u00b7sch\u00f6\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NE", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ei wie will ich vor dir bestahn,", "tokens": ["Ei", "wie", "will", "ich", "vor", "dir", "be\u00b7stahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "VMFIN", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Angesicht vers\u00f6hnen?", "tokens": ["Dein", "An\u00b7ge\u00b7sicht", "ver\u00b7s\u00f6h\u00b7nen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "O Sch\u00f6pfer mein, ich's nit vernein',", "tokens": ["O", "Sch\u00f6p\u00b7fer", "mein", ",", "ich's", "nit", "ver\u00b7nein'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PPOSAT", "$,", "PIS", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vor dir ich mu\u00df erstummen,", "tokens": ["Vor", "dir", "ich", "mu\u00df", "er\u00b7stum\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Bin's freilich wert, mich Feur und Schwert", "tokens": ["Bin's", "frei\u00b7lich", "wert", ",", "mich", "Feur", "und", "Schwert"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADJD", "$,", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Reib auf in gleicher Summen.", "tokens": ["Reib", "auf", "in", "glei\u00b7cher", "Sum\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Doch nit, wann brinnst in Eifermut,", "tokens": ["Doch", "nit", ",", "wann", "brinnst", "in", "Ei\u00b7fer\u00b7mut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "$,", "PWAV", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dir stell' mein S\u00fcnd zugegen,", "tokens": ["Dir", "stell'", "mein", "S\u00fcnd", "zu\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O nit, wann bist in voller Glut,", "tokens": ["O", "nit", ",", "wann", "bist", "in", "vol\u00b7ler", "Glut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "$,", "PWAV", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich la\u00df mit Straf belegen!", "tokens": ["Mich", "la\u00df", "mit", "Straf", "be\u00b7le\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Bedeck mit Gnad' all meine Tat,", "tokens": ["Be\u00b7deck", "mit", "Gnad'", "all", "mei\u00b7ne", "Tat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nit mehr der S\u00fcnd' gedenke,", "tokens": ["Nit", "mehr", "der", "S\u00fcnd'", "ge\u00b7den\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ach nur ins Meer, nur weit und fer'", "tokens": ["Ach", "nur", "ins", "Meer", ",", "nur", "weit", "und", "fer'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "APPRART", "NN", "$,", "ADV", "ADJD", "KON", "XY"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sie tief in Grund versenke!", "tokens": ["Sie", "tief", "in", "Grund", "ver\u00b7sen\u00b7ke", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Schaff Herr, da\u00df ich mit Z\u00e4hren hei\u00df", "tokens": ["Schaff", "Herr", ",", "da\u00df", "ich", "mit", "Z\u00e4h\u00b7ren", "hei\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "$,", "KOUS", "PPER", "APPR", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Grimmen dein verg\u00fcte;", "tokens": ["Den", "Grim\u00b7men", "dein", "ver\u00b7g\u00fc\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mich mach recht schnee- und schwanenwei\u00df", "tokens": ["Mich", "mach", "recht", "schnee", "und", "schwa\u00b7nen\u00b7wei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "TRUNC", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wasch ab das alt Gebl\u00fcte.", "tokens": ["Wasch", "ab", "das", "alt", "Ge\u00b7bl\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ach 's ist geschehn! kann's nicht umgehn.", "tokens": ["Ach", "'s", "ist", "ge\u00b7schehn", "!", "kann's", "nicht", "um\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "VAFIN", "VVPP", "$.", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Nun kr\u00e4nket's mich von Herzen,", "tokens": ["Nun", "kr\u00e4n\u00b7ket's", "mich", "von", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und ich von Leid fast jederzeit", "tokens": ["Und", "ich", "von", "Leid", "fast", "je\u00b7der\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "NN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.