{"dta.poem.5465": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Zur   Flos admirabilis.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gehab dich wol, geliebte Blume, des Gartens kurtze\nZier und Ehr',", "tokens": ["Ge\u00b7hab", "dich", "wol", ",", "ge\u00b7lieb\u00b7te", "Blu\u00b7me", ",", "des", "Gar\u00b7tens", "kurt\u00b7ze", "Zier", "und", "Ehr'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "$,", "ADJA", "NN", "$,", "ART", "NN", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Mit deiner Farben Glantz und Schein!", "tokens": ["Mit", "dei\u00b7ner", "Far\u00b7ben", "Glantz", "und", "Schein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich seh\u2019 dich nun und nimmermehr.", "tokens": ["Ich", "seh'", "dich", "nun", "und", "nim\u00b7mer\u00b7mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "KON", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Stunde, da du must vergehn, bricht bald heran, sie", "tokens": ["Die", "Stun\u00b7de", ",", "da", "du", "must", "ver\u00b7gehn", ",", "bricht", "bald", "he\u00b7ran", ",", "sie"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "VMFIN", "VVINF", "$,", "VVFIN", "ADV", "PTKVZ", "$,", "PPER"], "meter": "-+-+-+-+-+---", "measure": "unknown.measure.penta"}, "line.5": {"text": "Und eben, da ich mit dir rede, bricht sie herein, sie ist", "tokens": ["Und", "e\u00b7ben", ",", "da", "ich", "mit", "dir", "re\u00b7de", ",", "bricht", "sie", "her\u00b7ein", ",", "sie", "ist"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "$,", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "VAFIN"], "meter": "-+--+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Du wickelst dich in dich zusammen, verschrumpfst, ver-", "tokens": ["Du", "wi\u00b7ckelst", "dich", "in", "dich", "zu\u00b7sam\u00b7men", ",", "ver\u00b7schrumpfst", ",", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPER", "PTKVZ", "$,", "VVFIN", "$,", "TRUNC"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Verwelckst, verk\u00f6mmst, verdirbest gantz,", "tokens": ["Ver\u00b7welckst", ",", "ver\u00b7k\u00f6mmst", ",", "ver\u00b7dir\u00b7best", "gantz", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und zwar so schleunig und so schnell, da\u00df jedermann,", "tokens": ["Und", "zwar", "so", "schleu\u00b7nig", "und", "so", "schnell", ",", "da\u00df", "je\u00b7der\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJD", "KON", "ADV", "ADJD", "$,", "KOUS", "PIS", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die grosse Fl\u00fcchtigkeit nicht gnug bewundern kann.", "tokens": ["Die", "gros\u00b7se", "Fl\u00fcch\u00b7tig\u00b7keit", "nicht", "gnug", "be\u00b7wun\u00b7dern", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nun scheint zwar deine kurtze Dauer und dein so", "tokens": ["Nun", "scheint", "zwar", "dei\u00b7ne", "kurt\u00b7ze", "Dau\u00b7er", "und", "dein", "so"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "KON", "PPOSAT", "ADV"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Bedaurens-mehr als Wunderns-wehrt; allein, wenn man", "tokens": ["Be\u00b7dau\u00b7rens\u00b7mehr", "als", "Wun\u00b7derns\u00b7wehrt", ";", "al\u00b7lein", ",", "wenn", "man"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "KOUS", "NN", "$.", "ADV", "$,", "KOUS", "PIS"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df dein Vergehen andren Blumen nur gleichsam Platz", "tokens": ["Da\u00df", "dein", "Ver\u00b7ge\u00b7hen", "an\u00b7dren", "Blu\u00b7men", "nur", "gleich\u00b7sam", "Platz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADJA", "NN", "ADV", "ADJD", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "So f\u00fchlet und empfindet man nicht nur, da\u00df ihr verge-", "tokens": ["So", "f\u00fch\u00b7let", "und", "emp\u00b7fin\u00b7det", "man", "nicht", "nur", ",", "da\u00df", "ihr", "ver\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PIS", "PTKNEG", "ADV", "$,", "KOUS", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Weil immer andre wieder da, die euer kaum vermercktes", "tokens": ["Weil", "im\u00b7mer", "and\u00b7re", "wie\u00b7der", "da", ",", "die", "eu\u00b7er", "kaum", "ver\u00b7merck\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PIS", "ADV", "ADV", "$,", "PRELS", "PPOSAT", "ADV", "ADJA"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Ersetzen, und, so wie es auch bey uns nicht weniger", "tokens": ["Er\u00b7set\u00b7zen", ",", "und", ",", "so", "wie", "es", "auch", "bey", "uns", "nicht", "we\u00b7ni\u00b7ger"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KON", "$,", "ADV", "KOKOM", "PPER", "ADV", "APPR", "PPER", "PTKNEG", "ADV"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Die Stelle wiederum bekleiden;", "tokens": ["Die", "Stel\u00b7le", "wie\u00b7de\u00b7rum", "be\u00b7klei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Es zeigt vielmehr, geliebte Blumen, da ihr so kurtze Zeit", "tokens": ["Es", "zeigt", "viel\u00b7mehr", ",", "ge\u00b7lieb\u00b7te", "Blu\u00b7men", ",", "da", "ihr", "so", "kurt\u00b7ze", "Zeit"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ADJA", "NN", "$,", "KOUS", "PPER", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.9": {"text": "Und gleichsam, mit nie stillen Schritten, nur andern aus", "tokens": ["Und", "gleich\u00b7sam", ",", "mit", "nie", "stil\u00b7len", "Schrit\u00b7ten", ",", "nur", "an\u00b7dern", "aus"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "APPR", "ADV", "ADJA", "NN", "$,", "ADV", "PIS", "APPR"], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}}, "stanza.3": {"line.1": {"text": "Uns von des grossen Sch\u00f6pfers Wercken und von dem", "tokens": ["Uns", "von", "des", "gros\u00b7sen", "Sch\u00f6p\u00b7fers", "Wer\u00b7cken", "und", "von", "dem"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "NN", "KON", "APPR", "ART"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wie er so unersch\u00f6pflich sey, uns eine neu und wahre", "tokens": ["Wie", "er", "so", "un\u00b7er\u00b7sch\u00f6pf\u00b7lich", "sey", ",", "uns", "ei\u00b7ne", "neu", "und", "wah\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "VAFIN", "$,", "PPER", "ART", "ADJD", "KON", "ADJA"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Es kostet ihr sehr wenig M\u00fch, viel Millionen zu formiren.", "tokens": ["Es", "kos\u00b7tet", "ihr", "sehr", "we\u00b7nig", "M\u00fch", ",", "viel", "Mil\u00b7lion\u00b7en", "zu", "for\u00b7mi\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PIAT", "NN", "$,", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+---+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Man kann demnach und mu\u00df, mit Recht, so wol beym", "tokens": ["Man", "kann", "dem\u00b7nach", "und", "mu\u00df", ",", "mit", "Recht", ",", "so", "wol", "beym"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VMFIN", "PAV", "KON", "VMFIN", "$,", "APPR", "NN", "$,", "ADV", "ADV", "APPRART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Als bey der Zeugung, liebste Blume, den Sch\u00f6pfer der", "tokens": ["Als", "bey", "der", "Zeu\u00b7gung", ",", "liebs\u00b7te", "Blu\u00b7me", ",", "den", "Sch\u00f6p\u00b7fer", "der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "$,", "ADJA", "NN", "$,", "ART", "NN", "ART"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}}}}}