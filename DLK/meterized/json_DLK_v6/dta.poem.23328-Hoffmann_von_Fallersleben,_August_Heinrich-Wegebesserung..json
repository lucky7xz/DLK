{"dta.poem.23328": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Wegebesserung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "La\u00dft uns Gottes G\u00fcte preisen,", "tokens": ["La\u00dft", "uns", "Got\u00b7tes", "G\u00fc\u00b7te", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die uns gab den F\u00fcrstenstand:", "tokens": ["Die", "uns", "gab", "den", "F\u00fcrs\u00b7ten\u00b7stand", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur wenn unsre F\u00fcrsten reisen,", "tokens": ["Nur", "wenn", "uns\u00b7re", "F\u00fcrs\u00b7ten", "rei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bessert sich der Weg durch's Land.", "tokens": ["Bes\u00b7sert", "sich", "der", "Weg", "durch's", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Sind auch solche Reisen theuer,", "tokens": ["Sind", "auch", "sol\u00b7che", "Rei\u00b7sen", "theu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind sie uns doch lieb und werth;", "tokens": ["Sind", "sie", "uns", "doch", "lieb", "und", "werth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gern bezahlt man jede Steuer,", "tokens": ["Gern", "be\u00b7zahlt", "man", "je\u00b7de", "Steu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn man noch ertr\u00e4glich f\u00e4hrt.", "tokens": ["Wenn", "man", "noch", "er\u00b7tr\u00e4g\u00b7lich", "f\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}