{"textgrid.poem.54115": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Sein spannendster Roman", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Mann, der ich nicht bin:", "tokens": ["Der", "Mann", ",", "der", "ich", "nicht", "bin", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und in der Nacht, da sah ich klar,", "tokens": ["Und", "in", "der", "Nacht", ",", "da", "sah", "ich", "klar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich fing zu z\u00e4hlen an:", "tokens": ["Ich", "fing", "zu", "z\u00e4h\u00b7len", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKZU", "VVINF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Er hat so vieles gut gemacht,", "tokens": ["Er", "hat", "so", "vie\u00b7les", "gut", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIS", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich habe nichts getan.", "tokens": ["Ich", "ha\u00b7be", "nichts", "ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Soll ich, sein schlechtes Ebenbild,", "tokens": ["Soll", "ich", ",", "sein", "schlech\u00b7tes", "E\u00b7ben\u00b7bild", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Beweinen, was schon hin?", "tokens": ["Be\u00b7wei\u00b7nen", ",", "was", "schon", "hin", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Er wei\u00df, wie billig Tr\u00e4nen sind,", "tokens": ["Er", "wei\u00df", ",", "wie", "bil\u00b7lig", "Tr\u00e4\u00b7nen", "sind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ADJD", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der Mann, der ich nicht bin.", "tokens": ["Der", "Mann", ",", "der", "ich", "nicht", "bin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}