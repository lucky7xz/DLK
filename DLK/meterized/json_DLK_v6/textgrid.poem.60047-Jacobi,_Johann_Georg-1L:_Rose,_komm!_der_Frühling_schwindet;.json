{"textgrid.poem.60047": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Rose, komm! der Fr\u00fchling schwindet;", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rose, komm! der Fr\u00fchling schwindet;", "tokens": ["Ro\u00b7se", ",", "komm", "!", "der", "Fr\u00fch\u00b7ling", "schwin\u00b7det", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$.", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Veilchen haben dich verk\u00fcndet,", "tokens": ["Veil\u00b7chen", "ha\u00b7ben", "dich", "ver\u00b7k\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mayenblumen starben hin:", "tokens": ["Ma\u00b7yen\u00b7blu\u00b7men", "star\u00b7ben", "hin", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Oeffne dich beym Lust-Get\u00f6ne", "tokens": ["Oeff\u00b7ne", "dich", "beym", "Lust\u00b7Ge\u00b7t\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dieser Fluren; komm, o sch\u00f6ne,", "tokens": ["Die\u00b7ser", "Flu\u00b7ren", ";", "komm", ",", "o", "sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$.", "VVFIN", "$,", "FM", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Holde Blumen-K\u00f6niginn!", "tokens": ["Hol\u00b7de", "Blu\u00b7men\u00b7K\u00f6\u00b7ni\u00b7ginn", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Als du kamst im ersten Lenze,", "tokens": ["Als", "du", "kamst", "im", "ers\u00b7ten", "Len\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hingen tausendfache Kr\u00e4nze", "tokens": ["Hin\u00b7gen", "tau\u00b7send\u00b7fa\u00b7che", "Kr\u00e4n\u00b7ze"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schon um Anger, Berg und Thal;", "tokens": ["Schon", "um", "An\u00b7ger", ",", "Berg", "und", "Thal", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ufer lockten, W\u00e4lder bl\u00fchten,", "tokens": ["U\u00b7fer", "lock\u00b7ten", ",", "W\u00e4l\u00b7der", "bl\u00fch\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Pomeranzen-Haine gl\u00fchten", "tokens": ["Po\u00b7me\u00b7ran\u00b7zen\u00b7Hai\u00b7ne", "gl\u00fch\u00b7ten"], "token_info": ["word", "word"], "pos": ["NE", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Weit umher im Sonnenstrahl.", "tokens": ["Weit", "um\u00b7her", "im", "Son\u00b7nen\u00b7strahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Hoben ihre Cedern-Wipfel", "tokens": ["Ho\u00b7ben", "ih\u00b7re", "Ce\u00b7dern\u00b7Wip\u00b7fel"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Duftend in den Morgenschein;", "tokens": ["Duf\u00b7tend", "in", "den", "Mor\u00b7gen\u00b7schein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch auf dehmuthsvollem Throne", "tokens": ["Doch", "auf", "deh\u00b7muths\u00b7vol\u00b7lem", "Thro\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Solltest du der Sch\u00f6pfung Krone,", "tokens": ["Soll\u00b7test", "du", "der", "Sch\u00f6p\u00b7fung", "Kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Der Geschaffnen Wonne seyn.", "tokens": ["Der", "Ge\u00b7schaff\u00b7nen", "Won\u00b7ne", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und du gingst mit leisem Beben", "tokens": ["Und", "du", "gingst", "mit", "lei\u00b7sem", "Be\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus der zarten Knosp' ins Leben;", "tokens": ["Aus", "der", "zar\u00b7ten", "Knosp'", "ins", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Erd' und Himmel neigten sich;", "tokens": ["Erd'", "und", "Him\u00b7mel", "neig\u00b7ten", "sich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und es huldigten die Wiesen;", "tokens": ["Und", "es", "hul\u00b7dig\u00b7ten", "die", "Wie\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nachtigallen-Ch\u00f6re priesen,", "tokens": ["Nach\u00b7ti\u00b7gal\u00b7len\u00b7Ch\u00f6\u00b7re", "prie\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Alle Nymphen liebten dich.", "tokens": ["Al\u00b7le", "Nym\u00b7phen", "lieb\u00b7ten", "dich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Goldne Schmetterlinge schlugen", "tokens": ["Gold\u00b7ne", "Schmet\u00b7ter\u00b7lin\u00b7ge", "schlu\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Froh die Fl\u00fcgel; Winde trugen,", "tokens": ["Froh", "die", "Fl\u00fc\u00b7gel", ";", "Win\u00b7de", "tru\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$.", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo die Luft in Jubel war,", "tokens": ["Wo", "die", "Luft", "in", "Ju\u00b7bel", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Deinen Balsam; Herzen pochten", "tokens": ["Dei\u00b7nen", "Bal\u00b7sam", ";", "Her\u00b7zen", "poch\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dir entgegen; M\u00e4dchen flochten", "tokens": ["Dir", "ent\u00b7ge\u00b7gen", ";", "M\u00e4d\u00b7chen", "floch\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "PTKVZ", "$.", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Unter Perlen dich ins Haar.", "tokens": ["Un\u00b7ter", "Per\u00b7len", "dich", "ins", "Haar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Die von Weiber-Armuth sangen,", "tokens": ["Die", "von", "Wei\u00b7ber\u00b7Ar\u00b7muth", "san\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mahlten sie mit Rosen-Wangen;", "tokens": ["Mahl\u00b7ten", "sie", "mit", "Ro\u00b7sen\u00b7Wan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jede Seele, gut und mild,", "tokens": ["Je\u00b7de", "See\u00b7le", ",", "gut", "und", "mild", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Arglos, unschuldvoll, bescheiden,", "tokens": ["Arg\u00b7los", ",", "un\u00b7schuld\u00b7voll", ",", "be\u00b7schei\u00b7den", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "War in ihren h\u00f6chsten Freuden", "tokens": ["War", "in", "ih\u00b7ren", "h\u00f6chs\u00b7ten", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dein getreues Ebenbild.", "tokens": ["Dein", "ge\u00b7treu\u00b7es", "E\u00b7ben\u00b7bild", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und der Sch\u00f6nheit und der Jugend", "tokens": ["Und", "der", "Sch\u00f6n\u00b7heit", "und", "der", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00e4chterinnen, Schaam und Tugend,", "tokens": ["W\u00e4ch\u00b7te\u00b7rin\u00b7nen", ",", "Schaam", "und", "Tu\u00b7gend", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zu den Knospen hingeb\u00fcckt,", "tokens": ["Zu", "den", "Knos\u00b7pen", "hin\u00b7ge\u00b7b\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00fcllten unter deinem Nahmen", "tokens": ["H\u00fcll\u00b7ten", "un\u00b7ter", "dei\u00b7nem", "Nah\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ihr Geheimni\u00df; Br\u00e4ute kamen", "tokens": ["Ihr", "Ge\u00b7heim\u00b7ni\u00df", ";", "Br\u00e4u\u00b7te", "ka\u00b7men"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nicht umsonst mit dir geschm\u00fcckt.", "tokens": ["Nicht", "um\u00b7sonst", "mit", "dir", "ge\u00b7schm\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Da begann der rohe Zecher,", "tokens": ["Da", "be\u00b7gann", "der", "ro\u00b7he", "Ze\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Den von dir umbl\u00fcmten Becher", "tokens": ["Den", "von", "dir", "um\u00b7bl\u00fcm\u00b7ten", "Be\u00b7cher"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPER", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Keuschen Grazien zu weihn.", "tokens": ["Keu\u00b7schen", "Gra\u00b7zi\u00b7en", "zu", "weihn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Allen Helden, allen G\u00f6ttern", "tokens": ["Al\u00b7len", "Hel\u00b7den", ",", "al\u00b7len", "G\u00f6t\u00b7tern"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ging das Volk mit deinen Bl\u00e4ttern", "tokens": ["Ging", "das", "Volk", "mit", "dei\u00b7nen", "Bl\u00e4t\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Weg und Tempel zu bestreun.", "tokens": ["Weg", "und", "Tem\u00b7pel", "zu", "be\u00b7streun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Mit verj\u00fcngtem Herzen schlichen", "tokens": ["Mit", "ver\u00b7j\u00fcng\u00b7tem", "Her\u00b7zen", "schli\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Greise zu den Wohlger\u00fcchen", "tokens": ["Grei\u00b7se", "zu", "den", "Wohl\u00b7ge\u00b7r\u00fc\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Deines vollen Kelchs herbey;", "tokens": ["Dei\u00b7nes", "vol\u00b7len", "Kelchs", "her\u00b7bey", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lehrten segnend ihre S\u00f6hne:", "tokens": ["Lehr\u00b7ten", "seg\u00b7nend", "ih\u00b7re", "S\u00f6h\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df hienieden alles Sch\u00f6ne,", "tokens": ["Da\u00df", "hien\u00b7ie\u00b7den", "al\u00b7les", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Selbst die Rose sterblich sey.", "tokens": ["Selbst", "die", "Ro\u00b7se", "sterb\u00b7lich", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "An des Freundes heil'gem Grabe", "tokens": ["An", "des", "Freun\u00b7des", "heil'\u00b7gem", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wurdest du zur letzten Gabe", "tokens": ["Wur\u00b7dest", "du", "zur", "letz\u00b7ten", "Ga\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seinem Schatten dargebracht;", "tokens": ["Sei\u00b7nem", "Schat\u00b7ten", "dar\u00b7ge\u00b7bracht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Solltest ihm den Pfad umschlingen,", "tokens": ["Soll\u00b7test", "ihm", "den", "Pfad", "um\u00b7schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Thr\u00e4nen ihm und K\u00fcsse bringen", "tokens": ["Thr\u00e4\u00b7nen", "ihm", "und", "K\u00fcs\u00b7se", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "KON", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In die leere Todes-Nacht.", "tokens": ["In", "die", "lee\u00b7re", "To\u00b7des\u00b7Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Fromme singen an zu loben,", "tokens": ["From\u00b7me", "sin\u00b7gen", "an", "zu", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sahn gen Himmel, lie\u00dfen droben,", "tokens": ["Sahn", "gen", "Him\u0