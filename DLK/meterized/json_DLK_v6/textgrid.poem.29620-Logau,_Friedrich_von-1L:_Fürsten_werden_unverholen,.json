{"textgrid.poem.29620": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: F\u00fcrsten werden unverholen,", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcrsten werden unverholen,", "tokens": ["F\u00fcrs\u00b7ten", "wer\u00b7den", "un\u00b7ver\u00b7ho\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als die Niedren, mehr bestohlen.", "tokens": ["Als", "die", "Nied\u00b7ren", ",", "mehr", "be\u00b7stoh\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Grosses Brot gibt grosse Bissen,", "tokens": ["Gros\u00b7ses", "Brot", "gibt", "gros\u00b7se", "Bis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und von viel ist viel zu nissen;", "tokens": ["Und", "von", "viel", "ist", "viel", "zu", "nis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADV", "VAFIN", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Grosses Holtz gibt grosse Sp\u00e4ne;", "tokens": ["Gros\u00b7ses", "Holtz", "gibt", "gros\u00b7se", "Sp\u00e4\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ochs, als Schaf, wetzt mehr die Z\u00e4hne.", "tokens": ["Ochs", ",", "als", "Schaf", ",", "wetzt", "mehr", "die", "Z\u00e4h\u00b7ne", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "NN", "$,", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}