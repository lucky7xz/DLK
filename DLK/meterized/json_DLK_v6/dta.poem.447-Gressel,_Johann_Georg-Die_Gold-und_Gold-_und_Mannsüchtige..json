{"dta.poem.447": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Die  Gold-und Gold- und  Manns\u00fcchtige.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.57", "ro:0.28", "ca:0.14"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Corisca glaub' es mir nur zu/", "tokens": ["Co\u00b7ris\u00b7ca", "glaub'", "es", "mir", "nur", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PPER", "ADV", "PTKZU", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Schmertz ist mir bewust/", "tokens": ["Dein", "Schmertz", "ist", "mir", "be\u00b7wust", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich weis/ was deine Ruh", "tokens": ["Ich", "weis", "/", "was", "dei\u00b7ne", "Ruh"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "PTKVZ", "$(", "PWS", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Verst\u00f6hrt; was fehlet deiner Brust?", "tokens": ["Ver\u00b7st\u00f6hrt", ";", "was", "feh\u00b7let", "dei\u00b7ner", "Brust", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$.", "PWS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du hast;", "tokens": ["Du", "hast", ";"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VAFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Soll es heraus?", "tokens": ["Soll", "es", "he\u00b7raus", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Nicht wenig Uberlast", "tokens": ["Nicht", "we\u00b7nig", "U\u00b7ber\u00b7last"], "token_info": ["word", "word", "word"], "pos": ["PTKNEG", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Von ", "tokens": ["Von"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}}}}}