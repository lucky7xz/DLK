{"dta.poem.23519": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Stand und St\u00e4nde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.57", "fr:0.42"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ha! eure Mauern, eure W\u00e4nde,", "tokens": ["Ha", "!", "eu\u00b7re", "Mau\u00b7ern", ",", "eu\u00b7re", "W\u00e4n\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hat sie nicht l\u00e4ngst die Zeit zerst\u00f6rt?", "tokens": ["Hat", "sie", "nicht", "l\u00e4ngst", "die", "Zeit", "zer\u00b7st\u00f6rt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo blieb der Unterschied der St\u00e4nde?", "tokens": ["Wo", "blieb", "der", "Un\u00b7ter\u00b7schied", "der", "St\u00e4n\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hat jeder Stand nicht aufgeh\u00f6rt?", "tokens": ["Hat", "je\u00b7der", "Stand", "nicht", "auf\u00b7ge\u00b7h\u00f6rt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wir haben keine Zeit zum Stehen,", "tokens": ["Wir", "ha\u00b7ben", "kei\u00b7ne", "Zeit", "zum", "Ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nichts hat noch seinen alten Stand;", "tokens": ["Nichts", "hat", "noch", "sei\u00b7nen", "al\u00b7ten", "Stand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jetzt will die ganze Welt nur gehen,", "tokens": ["Jetzt", "will", "die", "gan\u00b7ze", "Welt", "nur", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie kann da stehen noch ein Land!", "tokens": ["Wie", "kann", "da", "ste\u00b7hen", "noch", "ein", "Land", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ADV", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Was soll der Stand? was sollen St\u00e4nde?", "tokens": ["Was", "soll", "der", "Stand", "?", "was", "sol\u00b7len", "St\u00e4n\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "$.", "PWS", "VMFIN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie hemmen nicht der Zeiten Lauf.", "tokens": ["Sie", "hem\u00b7men", "nicht", "der", "Zei\u00b7ten", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O, reicht euch alle gern die H\u00e4nde!", "tokens": ["O", ",", "reicht", "euch", "al\u00b7le", "gern", "die", "H\u00e4n\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "PIS", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Euch alle nimmt Ein Haus nur auf.", "tokens": ["Euch", "al\u00b7le", "nimmt", "Ein", "Haus", "nur", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}