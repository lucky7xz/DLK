{"textgrid.poem.63948": {"metadata": {"author": {"name": "Zachari\u00e4, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dem Spinnenvolke fiel es ein,", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dem Spinnenvolke fiel es ein,", "tokens": ["Dem", "Spin\u00b7nen\u00b7vol\u00b7ke", "fiel", "es", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Zukunft sicherer zu seyn,", "tokens": ["In", "Zu\u00b7kunft", "si\u00b7che\u00b7rer", "zu", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und nicht Jedwedem zu verg\u00f6nnen,", "tokens": ["Und", "nicht", "Jed\u00b7we\u00b7dem", "zu", "ver\u00b7g\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In ihrem Schlo\u00df herum zu rennen,", "tokens": ["In", "ih\u00b7rem", "Schlo\u00df", "he\u00b7rum", "zu", "ren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APZR", "PTKZU", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie wohnten eben dazumal", "tokens": ["Sie", "wohn\u00b7ten", "e\u00b7ben", "da\u00b7zu\u00b7mal"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In einem gro\u00dfen w\u00fcsten Saal,", "tokens": ["In", "ei\u00b7nem", "gro\u00b7\u00dfen", "w\u00fcs\u00b7ten", "Saal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Durch dessen offne Fensterbogen", "tokens": ["Durch", "des\u00b7sen", "off\u00b7ne", "Fens\u00b7ter\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Stets M\u00fccke, Schwalb' und Sperling flogen.", "tokens": ["Stets", "M\u00fc\u00b7cke", ",", "Schwalb'", "und", "Sper\u00b7ling", "flo\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Wir wollen (murreten die Spinnen)", "tokens": ["Wir", "wol\u00b7len", "(", "mur\u00b7re\u00b7ten", "die", "Spin\u00b7nen", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$(", "VVFIN", "ART", "NN", "$("], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.10": {"text": "Den Vortheil euch wohl abgewinnen;", "tokens": ["Den", "Vor\u00b7theil", "euch", "wohl", "ab\u00b7ge\u00b7win\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und zogen in die L\u00e4ng' und Quer'", "tokens": ["Und", "zo\u00b7gen", "in", "die", "L\u00e4ng'", "und", "Quer'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Viel F\u00e4den vor den Fenstern her.", "tokens": ["Viel", "F\u00e4\u00b7den", "vor", "den", "Fens\u00b7tern", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Doch Schwalb' und Sperling kamen bald", "tokens": ["Doch", "Schwalb'", "und", "Sper\u00b7ling", "ka\u00b7men", "bald"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Und fuhren dreist und mit Gewalt", "tokens": ["Und", "fuh\u00b7ren", "dreist", "und", "mit", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "KON", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Durch diese leichten Spinnenweben,", "tokens": ["Durch", "die\u00b7se", "leich\u00b7ten", "Spin\u00b7nen\u00b7we\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Und nur die M\u00fccken blieben kleben.", "tokens": ["Und", "nur", "die", "M\u00fc\u00b7cken", "blie\u00b7ben", "kle\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ganz so, wie diese Spinnennetze,", "tokens": ["Ganz", "so", ",", "wie", "die\u00b7se", "Spin\u00b7nen\u00b7net\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PWAV", "PDAT", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Sind oft im Staate die Gesetze.", "tokens": ["Sind", "oft", "im", "Staa\u00b7te", "die", "Ge\u00b7set\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Kein M\u00e4cht'ger wird darin gefangen,", "tokens": ["Kein", "M\u00e4cht'\u00b7ger", "wird", "da\u00b7rin", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PAV", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur blos der Schwache bleibt d'rin hangen.", "tokens": ["Nur", "blos", "der", "Schwa\u00b7che", "bleibt", "d'\u00b7rin", "han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "ADV", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}}}}