{"dta.poem.1106": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auff die allzugeschwinde Erblassung der  \n Wohlseligen   Baronessen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Seele schicke dich nunmehr zu der lezten Todes-Reise/ ", "tokens": ["See\u00b7le", "schi\u00b7cke", "dich", "nun\u00b7mehr", "zu", "der", "lez\u00b7ten", "To\u00b7des\u00b7Rei\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Die betr\u00fcbte Zeit bricht an/ so dir mit dem Tode", "tokens": ["Die", "be\u00b7tr\u00fcb\u00b7te", "Zeit", "bricht", "an", "/", "so", "dir", "mit", "dem", "To\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$(", "ADV", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.3": {"text": "Sieh/ wie dich der Schmertz bef\u00e4llt auff gar unterschiedne", "tokens": ["Sieh", "/", "wie", "dich", "der", "Schmertz", "be\u00b7f\u00e4llt", "auff", "gar", "un\u00b7ter\u00b7schied\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$(", "PWAV", "PPER", "ART", "NN", "VVFIN", "APPR", "ADV", "ADJA"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Weise/ ", "tokens": ["Wei\u00b7se", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Deine Kr\u00e4ffte nehmen ab: Darum mache dich bereit/", "tokens": ["Dei\u00b7ne", "Kr\u00e4ff\u00b7te", "neh\u00b7men", "ab", ":", "Da\u00b7rum", "ma\u00b7che", "dich", "be\u00b7reit", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$.", "PAV", "VVFIN", "PPER", "ADJD", "$("], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Seele zieh dich von der Welt/ la\u00df ihr falsch geschminck-", "tokens": ["See\u00b7le", "zieh", "dich", "von", "der", "Welt", "/", "la\u00df", "ihr", "falsch", "ge\u00b7schmin\u00b7ck"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$(", "VVIMP", "PPER", "ADJD", "TRUNC"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "tes Wesen/ ", "tokens": ["tes", "We\u00b7sen", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Denn du bist vor diesesmahl zu dem Sterben auser-", "tokens": ["Denn", "du", "bist", "vor", "die\u00b7ses\u00b7mahl", "zu", "dem", "Ster\u00b7ben", "aus\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "APPR", "ADV", "APPR", "ART", "NN", "TRUNC"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Was ist di\u00df vor eine Post/ da\u00df man mir vom Sterben saget/", "tokens": ["Was", "ist", "di\u00df", "vor", "ei\u00b7ne", "Post", "/", "da\u00df", "man", "mir", "vom", "Ster\u00b7ben", "sa\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PDS", "APPR", "ART", "NN", "$(", "KOUS", "PIS", "PPER", "APPRART", "NN", "VVFIN", "$("], "meter": "----+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da ich zu dem Grabe doch noch nicht grau und zeitig", "tokens": ["Da", "ich", "zu", "dem", "Gra\u00b7be", "doch", "noch", "nicht", "grau", "und", "zei\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "ADV", "ADV", "PTKNEG", "ADJD", "KON", "ADJD"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "bin/ ", "tokens": ["bin", "/"], "token_info": ["word", "punct"], "pos": ["VAFIN", "$("], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Dieses ist ja eine Post/ so die meisten Menschen naget/", "tokens": ["Die\u00b7ses", "ist", "ja", "ei\u00b7ne", "Post", "/", "so", "die", "meis\u00b7ten", "Men\u00b7schen", "na\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ART", "NN", "$(", "ADV", "ART", "PIAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Die von GOTT gesegnet sind/ und in Ehr und Gl\u00fccke", "tokens": ["Die", "von", "GoTT", "ge\u00b7seg\u00b7net", "sind", "/", "und", "in", "Ehr", "und", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NE", "VVPP", "VAFIN", "$(", "KON", "APPR", "NN", "KON", "NN"], "meter": "--+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "bl\u00fchn/ ", "tokens": ["bl\u00fchn", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Soll es schon gestorben seyn/ ach so mu\u00df ich zitternd spre-", "tokens": ["Soll", "es", "schon", "ge\u00b7stor\u00b7ben", "seyn", "/", "ach", "so", "mu\u00df", "ich", "zit\u00b7ternd", "spre"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "VVPP", "VAINF", "$(", "ITJ", "ADV", "VMFIN", "PPER", "VVPP", "TRUNC"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Was den schwachen Leib betr\u00fcbt/ Sterben ist nicht Ro-", "tokens": ["Was", "den", "schwa\u00b7chen", "Leib", "be\u00b7tr\u00fcbt", "/", "Ster\u00b7ben", "ist", "nicht", "Ro"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "ART", "ADJA", "NN", "VVPP", "$(", "NN", "VAFIN", "PTKNEG", "TRUNC"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Freylich geb\u2019 ich dieses zu/ Sterben ist nicht Rosenbrechen/", "tokens": ["Frey\u00b7lich", "geb'", "ich", "die\u00b7ses", "zu", "/", "Ster\u00b7ben", "ist", "nicht", "Ro\u00b7sen\u00b7bre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDS", "PTKZU", "$(", "NN", "VAFIN", "PTKNEG", "NN", "$("], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Sterben macht uns allen Angst/ was den Mensch und Leib", "tokens": ["Ster\u00b7ben", "macht", "uns", "al\u00b7len", "Angst", "/", "was", "den", "Mensch", "und", "Leib"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PIAT", "NN", "$(", "PWS", "ART", "NN", "KON", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Aber wenn in Todes-Noth dich die S\u00fcnden-D\u00f6rner ste-", "tokens": ["A\u00b7ber", "wenn", "in", "To\u00b7des\u00b7Noth", "dich", "die", "S\u00fcn\u00b7den\u00b7D\u00f6r\u00b7ner", "ste"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "NN", "PPER", "ART", "NN", "TRUNC"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Zeigt dir der erstandne Held eine solche Uberschrifft:", "tokens": ["Zeigt", "dir", "der", "er\u00b7stand\u00b7ne", "Held", "ei\u00b7ne", "sol\u00b7che", "U\u00b7ber\u00b7schrifft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "ART", "PIAT", "NN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Ich war todt und lebe wieder/ wirst du freudig \u00fcber-", "tokens": ["Ich", "war", "todt", "und", "le\u00b7be", "wie\u00b7der", "/", "wirst", "du", "freu\u00b7dig", "\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "VVFIN", "ADV", "$(", "VAFIN", "PPER", "ADJD", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.6": {"text": "Alsdenn wirstu auch im Sterben rechte Lebens-Rosen", "tokens": ["Als\u00b7denn", "wirs\u00b7tu", "auch", "im", "Ster\u00b7ben", "rech\u00b7te", "Le\u00b7bens\u00b7Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "APPRART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}, "stanza.4": {"line.1": {"text": "Nun so la\u00df ich Welt und Geld/ Gl\u00fcck und G\u00fcter dieser Er-", "tokens": ["Nun", "so", "la\u00df", "ich", "Welt", "und", "Geld", "/", "Gl\u00fcck", "und", "G\u00fc\u00b7ter", "die\u00b7ser", "Er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVIMP", "PPER", "NN", "KON", "NN", "$(", "NN", "KON", "NN", "PDAT", "NN"], "meter": "+-+-+-++-+-+-+", "measure": "unknown.measure.octa.plus"}, "line.2": {"text": "Jesu zeige mir die Bahn/ wo die rechten Rosen stehn;", "tokens": ["Je\u00b7su", "zei\u00b7ge", "mir", "die", "Bahn", "/", "wo", "die", "rech\u00b7ten", "Ro\u00b7sen", "stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ART", "NN", "$(", "PWAV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Gieb da\u00df ich durch dein Verdienst mag gerecht und selig", "tokens": ["Gieb", "da\u00df", "ich", "durch", "dein", "Ver\u00b7dienst", "mag", "ge\u00b7recht", "und", "se\u00b7lig"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "KOUS", "PPER", "APPR", "PPOSAT", "NN", "VMFIN", "ADJD", "KON", "ADJD"], "meter": "+---+-+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "La\u00df mich freudig aus der Welt in das ewge Leben gehn/", "tokens": ["La\u00df", "mich", "freu\u00b7dig", "aus", "der", "Welt", "in", "das", "ew\u00b7ge", "Le\u00b7ben", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "APPR", "ART", "NN", "APPR", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Herr ich sterbe/ nimm mich an/ weil ich kan mit Freu-", "tokens": ["Herr", "ich", "ster\u00b7be", "/", "nimm", "mich", "an", "/", "weil", "ich", "kan", "mit", "Freu"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "VVFIN", "$(", "VVIMP", "PPER", "PTKVZ", "$(", "KOUS", "PPER", "VMFIN", "APPR", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Da ich S\u00fcnd und Tod besiegt/ sterbend mu\u00df man Rosen", "tokens": ["Da", "ich", "S\u00fcnd", "und", "Tod", "be\u00b7siegt", "/", "ster\u00b7bend", "mu\u00df", "man", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "VVPP", "$(", "ADJD", "VMFIN", "PIS", "NN"], "meter": "--+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Nun die Dornen sind vorbey/ und die Rosen abgepfl\u00fcckt/", "tokens": ["Nun", "die", "Dor\u00b7nen", "sind", "vor\u00b7bey", "/", "und", "die", "Ro\u00b7sen", "ab\u00b7ge\u00b7pfl\u00fcckt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ADV", "$(", "KON", "ART", "NN", "VVPP", "$("], "meter": "--+---+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Noth und Sterben sind besiegt/ du hast selig \u00fcberwun-", "tokens": ["Noth", "und", "Ster\u00b7ben", "sind", "be\u00b7siegt", "/", "du", "hast", "se\u00b7lig", "\u00fc\u00b7ber\u00b7wun"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VAFIN", "VVPP", "$(", "PPER", "VAFIN", "ADJD", "TRUNC"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Selige/ du bist nunmehr von der Welt dahin ger\u00fcckt/", "tokens": ["Se\u00b7li\u00b7ge", "/", "du", "bist", "nun\u00b7mehr", "von", "der", "Welt", "da\u00b7hin", "ge\u00b7r\u00fcckt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PPER", "VAFIN", "ADV", "APPR", "ART", "NN", "PAV", "VVPP", "$("], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.4": {"text": "Und hast in des Herren Hand/ Cron und Sieges-Palmen", "tokens": ["Und", "hast", "in", "des", "Her\u00b7ren", "Hand", "/", "Cron", "und", "Sie\u00b7ges\u00b7Pal\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "ART", "NN", "NN", "$(", "NN", "KON", "NN"], "meter": "-+--+-++-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Wer sich in die Welt verliebt/ nichts vom Tode h\u00f6ren", "tokens": ["Wer", "sich", "in", "die", "Welt", "ver\u00b7liebt", "/", "nichts", "vom", "To\u00b7de", "h\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PRF", "APPR", "ART", "NN", "VVPP", "$(", "PIS", "APPRART", "NN", "VVINF"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Dieser trete nur herzu und seh dieses Sterben an.", "tokens": ["Die\u00b7ser", "tre\u00b7te", "nur", "her\u00b7zu", "und", "seh", "die\u00b7ses", "Ster\u00b7ben", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "KON", "VVFIN", "PDAT", "NN", "PTKVZ", "$."], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}}, "stanza.6": {"line.1": {"text": "Selige/ es ist vollbracht/ dein so kurtzer Wittwen-Stand/", "tokens": ["Se\u00b7li\u00b7ge", "/", "es", "ist", "voll\u00b7bracht", "/", "dein", "so", "kurt\u00b7zer", "Witt\u00b7wen\u00b7Stand", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PPER", "VAFIN", "VVPP", "$(", "PPOSAT", "ADV", "ADJA", "NN", "$("], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.2": {"text": "Den dir nur der H\u00f6chste wie\u00df/ hat sich bald in Lust ver-", "tokens": ["Den", "dir", "nur", "der", "H\u00f6chs\u00b7te", "wie\u00df", "/", "hat", "sich", "bald", "in", "Lust", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "ART", "NN", "VVFIN", "$(", "VAFIN", "PRF", "ADV", "APPR", "NN", "TRUNC"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Euch vereinigt wiederum dort erfreut des HErren Hand/", "tokens": ["Euch", "ver\u00b7ei\u00b7nigt", "wie\u00b7de\u00b7rum", "dort", "er\u00b7freut", "des", "Her\u00b7ren", "Hand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "VVFIN", "ART", "NN", "NN", "$("], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Und die Ehe bleibt vergn\u00fcgt/ weil euch kein Verdru\u00df be-", "tokens": ["Und", "die", "E\u00b7he", "bleibt", "ver\u00b7gn\u00fcgt", "/", "weil", "euch", "kein", "Ver\u00b7dru\u00df", "be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "VVPP", "$(", "KOUS", "PPER", "PIAT", "NN", "TRUNC"], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Selige/ es ist vollbracht/ was du w\u00fcnschest ist ge-", "tokens": ["Se\u00b7li\u00b7ge", "/", "es", "ist", "voll\u00b7bracht", "/", "was", "du", "w\u00fcn\u00b7schest", "ist", "ge"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$(", "PPER", "VAFIN", "VVPP", "$(", "PWS", "PPER", "VVFIN", "VAFIN", "TRUNC"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Da\u00df du gl\u00fccklich hast gelebt/ kan man auch im Ster-", "tokens": ["Da\u00df", "du", "gl\u00fcck\u00b7lich", "hast", "ge\u00b7lebt", "/", "kan", "man", "auch", "im", "Ster"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "VVPP", "$(", "VMFIN", "PIS", "ADV", "APPRART", "TRUNC"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}}}}}