{"textgrid.poem.48394": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "8. Das Gebet Manasse, des K\u00f6nigs Juda, da er zu Babel gefangen war", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Herr, du starker Gott, du Vater unsrer V\u00e4ter,", "tokens": ["O", "Herr", ",", "du", "star\u00b7ker", "Gott", ",", "du", "Va\u00b7ter", "uns\u00b7rer", "V\u00e4\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "ADJA", "NN", "$,", "PPER", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und ihres Samens auch, der ein gerechter T\u00e4ter", "tokens": ["und", "ih\u00b7res", "Sa\u00b7mens", "auch", ",", "der", "ein", "ge\u00b7rech\u00b7ter", "T\u00e4\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADV", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "in deinen Satzen ist, der du das Firmament,", "tokens": ["in", "dei\u00b7nen", "Sat\u00b7zen", "ist", ",", "der", "du", "das", "Fir\u00b7ma\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "$,", "PRELS", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "der tiefen Erde Scho\u00df und was sich drinnen wendt,", "tokens": ["der", "tie\u00b7fen", "Er\u00b7de", "Scho\u00df", "und", "was", "sich", "drin\u00b7nen", "wendt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "KON", "PWS", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "auch was steht unverwandt, aus Nichts doch hast erfunden!", "tokens": ["auch", "was", "steht", "un\u00b7ver\u00b7wandt", ",", "aus", "Nichts", "doch", "hast", "er\u00b7fun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VVFIN", "ADJD", "$,", "APPR", "PIS", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Du hast das hohe Meer durch dein Gebot erfunden,", "tokens": ["Du", "hast", "das", "ho\u00b7he", "Meer", "durch", "dein", "Ge\u00b7bot", "er\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "du, Gott, verschleu\u00dfst die See, als siegeltst du sie zu,", "tokens": ["du", ",", "Gott", ",", "ver\u00b7schleu\u00df\u00b7st", "die", "See", ",", "als", "sie\u00b7geltst", "du", "sie", "zu", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VVFIN", "ART", "NN", "$,", "KOUS", "VVFIN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "sie bricht nicht dein Pitschier. Du Herr, allein Herr du", "tokens": ["sie", "bricht", "nicht", "dein", "Pit\u00b7schier", ".", "Du", "Herr", ",", "al\u00b7lein", "Herr", "du"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$.", "PPER", "NN", "$,", "ADV", "NN", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "bist schrecklich und doch gut. Dein herrlich Loh zu mehren", "tokens": ["bist", "schreck\u00b7lich", "und", "doch", "gut", ".", "Dein", "herr\u00b7lich", "Loh", "zu", "meh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "KON", "ADV", "ADJD", "$.", "PPOSAT", "ADJD", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "erschufst du diesen Bau. Dich, dich mu\u00df alles ehren.", "tokens": ["er\u00b7schufst", "du", "die\u00b7sen", "Bau", ".", "Dich", ",", "dich", "mu\u00df", "al\u00b7les", "eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "NN", "$.", "PPER", "$,", "PPER", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Di\u00df Ganz' erschrickt f\u00fcr dir. Wir f\u00fcrchten uns erbla\u00dft", "tokens": ["Di\u00df", "Ganz'", "er\u00b7schrickt", "f\u00fcr", "dir", ".", "Wir", "f\u00fcrch\u00b7ten", "uns", "er\u00b7bla\u00dft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "VVFIN", "APPR", "PPER", "$.", "PPER", "VVFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "f\u00fcr deiner gro\u00dfen Macht, die du dir geben hast.", "tokens": ["f\u00fcr", "dei\u00b7ner", "gro\u00b7\u00dfen", "Macht", ",", "die", "du", "dir", "ge\u00b7ben", "hast", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "PRELS", "PPER", "PPER", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Untr\u00e4glich ist dein Zorn, den du den S\u00fcndern dr\u00e4uest.", "tokens": ["Un\u00b7tr\u00e4g\u00b7lich", "ist", "dein", "Zorn", ",", "den", "du", "den", "S\u00fcn\u00b7dern", "dr\u00e4u\u00b7est", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Doch deine Mildigkeit, die du hierbei verleihest,", "tokens": ["Doch", "dei\u00b7ne", "Mil\u00b7dig\u00b7keit", ",", "die", "du", "hier\u00b7bei", "ver\u00b7lei\u00b7hest", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "ist m\u00e4\u00dfig ohne Ma\u00df, und zu erforschen nicht,", "tokens": ["ist", "m\u00e4\u00b7\u00dfig", "oh\u00b7ne", "Ma\u00df", ",", "und", "zu", "er\u00b7for\u00b7schen", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "NN", "$,", "KON", "PTKZU", "VVINF", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "wie deinen grimmen Ernst die linde Gnade bricht.", "tokens": ["wie", "dei\u00b7nen", "grim\u00b7men", "Ernst", "die", "lin\u00b7de", "Gna\u00b7de", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Der Allerh\u00f6heste bist du allein zu nennen,", "tokens": ["Der", "Al\u00b7ler\u00b7h\u00f6\u00b7hes\u00b7te", "bist", "du", "al\u00b7lein", "zu", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "so weit die Sonne kan mit ihren G\u00e4ulen rennen", "tokens": ["so", "weit", "die", "Son\u00b7ne", "kan", "mit", "ih\u00b7ren", "G\u00e4u\u00b7len", "ren\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "NN", "VMFIN", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "umb die gecirkte Welt! Jedoch, wie gro\u00df du bist,", "tokens": ["umb", "die", "ge\u00b7cirk\u00b7te", "Welt", "!", "Je\u00b7doch", ",", "wie", "gro\u00df", "du", "bist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "ADV", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "so lind' und gn\u00e4dig auch dein Herz, o Vater, ist.", "tokens": ["so", "lind'", "und", "gn\u00e4\u00b7dig", "auch", "dein", "Herz", ",", "o", "Va\u00b7ter", ",", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "ADV", "PPOSAT", "NN", "$,", "FM", "NN", "$,", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die Strafe tr\u00fcbt dich selbst, mit der du uns belegen,", "tokens": ["Die", "Stra\u00b7fe", "tr\u00fcbt", "dich", "selbst", ",", "mit", "der", "du", "uns", "be\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,", "APPR", "PRELS", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "uns harte S\u00fcnder, must. Drumb hastu auch hingegen", "tokens": ["uns", "har\u00b7te", "S\u00fcn\u00b7der", ",", "must", ".", "Drumb", "has\u00b7tu", "auch", "hin\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "VMFIN", "$.", "PAV", "VAFIN", "ADV", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "ein Vorteil auserdacht, wie du der offnen Schuld", "tokens": ["ein", "Vor\u00b7teil", "au\u00b7ser\u00b7dacht", ",", "wie", "du", "der", "off\u00b7nen", "Schuld"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "PWAV", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "kanst einen Durchstrich tun und wieder werden huld.", "tokens": ["kanst", "ei\u00b7nen", "Durch\u00b7strich", "tun", "und", "wie\u00b7der", "wer\u00b7den", "huld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VVINF", "KON", "ADV", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Das ist die ernste Bu\u00df', in der du uns quittirest", "tokens": ["Das", "ist", "die", "erns\u00b7te", "Bu\u00df'", ",", "in", "der", "du", "uns", "quit\u00b7ti\u00b7rest"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,", "APPR", "PRELS", "PPER", "PPER", "VVFIN"], "meter": "-+-+-+--+-+--", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "von aller Missetat. Weil aber du, Herr, f\u00fchrest", "tokens": ["von", "al\u00b7ler", "Mis\u00b7se\u00b7tat", ".", "Weil", "a\u00b7ber", "du", ",", "Herr", ",", "f\u00fch\u00b7rest"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "PIAT", "NN", "$.", "KOUS", "ADV", "PPER", "$,", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "den Namen, da\u00df du bist der Frommen Gott allein,", "tokens": ["den", "Na\u00b7men", ",", "da\u00df", "du", "bist", "der", "From\u00b7men", "Gott", "al\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "VAFIN", "ART", "NN", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "so kan die Bu\u00dfe nicht den Frommen geben sein,", "tokens": ["so", "kan", "die", "Bu\u00b7\u00dfe", "nicht", "den", "From\u00b7men", "ge\u00b7ben", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PTKNEG", "ART", "NN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "wie Abram, Isaak und Jacob f\u00fcr dir waren,", "tokens": ["wie", "Ab\u00b7ram", ",", "I\u00b7saak", "und", "Ja\u00b7cob", "f\u00fcr", "dir", "wa\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "$,", "NE", "KON", "NE", "APPR", "PPER", "VAFIN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "als denen wider dich kein Feil nie widerfahren.", "tokens": ["als", "de\u00b7nen", "wi\u00b7der", "dich", "kein", "Feil", "nie", "wi\u00b7der\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRELS", "APPR", "PPER", "PIAT", "NN", "ADV", "VVPP", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.31": {"text": "Ich aber habe, Herr, vor dir ges\u00fcndigt sehr,", "tokens": ["Ich", "a\u00b7ber", "ha\u00b7be", ",", "Herr", ",", "vor", "dir", "ge\u00b7s\u00fcn\u00b7digt", "sehr", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "$,", "NN", "$,", "APPR", "PPER", "VVPP", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "mein Unrecht \u00fcberwiegt den kleinen Sand am Meer.", "tokens": ["mein", "Un\u00b7recht", "\u00fc\u00b7berw\u00b7iegt", "den", "klei\u00b7nen", "Sand", "am", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ich mu\u00df gekr\u00fcmmet gehn in schweren eisern Banden", "tokens": ["Ich", "mu\u00df", "ge\u00b7kr\u00fcm\u00b7met", "gehn", "in", "schwe\u00b7ren", "ei\u00b7sern", "Ban\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "VVPP", "VVFIN", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "und habe keine Ruh, weil ich mit meinen Schanden", "tokens": ["und", "ha\u00b7be", "kei\u00b7ne", "Ruh", ",", "weil", "ich", "mit", "mei\u00b7nen", "Schan\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PIAT", "NN", "$,", "KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "erwecket deinen Zorn, da ich vor dir getan", "tokens": ["er\u00b7we\u00b7cket", "dei\u00b7nen", "Zorn", ",", "da", "ich", "vor", "dir", "ge\u00b7tan"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "KOUS", "PPER", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "gro\u00df' \u00fcbermachte Schuld, indem ich b\u00f6ser Mann,", "tokens": ["gro\u00df'", "\u00fc\u00b7ber\u00b7mach\u00b7te", "Schuld", ",", "in\u00b7dem", "ich", "b\u00f6\u00b7ser", "Mann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,", "KOUS", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "viel schweres Ergern\u00fc\u00df und solche Greuels\u00fcnden", "tokens": ["viel", "schwe\u00b7res", "Er\u00b7ger\u00b7n\u00fc\u00df", "und", "sol\u00b7che", "Greu\u00b7el\u00b7s\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "KON", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "vorhin hab' ausge\u00fcbt. Doch so noch Heil zu finden,", "tokens": ["vor\u00b7hin", "hab'", "aus\u00b7ge\u00b7\u00fcbt", ".", "Doch", "so", "noch", "Heil", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "$.", "KON", "ADV", "ADV", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "so sieh, ich beuge, Herr, die Knie des Gem\u00fcts,", "tokens": ["so", "sieh", ",", "ich", "beu\u00b7ge", ",", "Herr", ",", "die", "Knie", "des", "Ge\u00b7m\u00fcts", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "PPER", "VVFIN", "$,", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.40": {"text": "mein Herze neigt sich dir. Erteil mich des Beschieds,", "tokens": ["mein", "Her\u00b7ze", "neigt", "sich", "dir", ".", 