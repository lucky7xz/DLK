{"textgrid.poem.47238": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "3. Verschlie\u00dfung", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es r\u00fchrt mich an der Fr\u00fchlingsl\u00fcfte Schauern,", "tokens": ["Es", "r\u00fchrt", "mich", "an", "der", "Fr\u00fch\u00b7lings\u00b7l\u00fcf\u00b7te", "Schau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie kommen sanft ans Herz herangeglitten", "tokens": ["Sie", "kom\u00b7men", "sanft", "ans", "Herz", "her\u00b7an\u00b7ge\u00b7glit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wollen draus vertreiben dumpfes Trauern,", "tokens": ["Und", "wol\u00b7len", "draus", "ver\u00b7trei\u00b7ben", "dum\u00b7pfes", "Trau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PAV", "VVINF", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das drinnen wohnet wie in Winters Mitten.", "tokens": ["Das", "drin\u00b7nen", "woh\u00b7net", "wie", "in", "Win\u00b7ters", "Mit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "KOKOM", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ihr L\u00fcftchen, eure M\u00fch' mu\u00df ich bedauern,", "tokens": ["Ihr", "L\u00fcft\u00b7chen", ",", "eu\u00b7re", "M\u00fch'", "mu\u00df", "ich", "be\u00b7dau\u00b7ern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sie besser anzuwenden, la\u00dft euch bitten.", "tokens": ["Sie", "bes\u00b7ser", "an\u00b7zu\u00b7wen\u00b7den", ",", "la\u00dft", "euch", "bit\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVIZU", "$,", "VVIMP", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wo stille Knospen an den Zweigen lauern,", "tokens": ["Wo", "stil\u00b7le", "Knos\u00b7pen", "an", "den", "Zwei\u00b7gen", "lau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Da kehrt euch hin, da seid ihr wohlgelitten;", "tokens": ["Da", "kehrt", "euch", "hin", ",", "da", "seid", "ihr", "wohl\u00b7ge\u00b7lit\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Da weckt mit euerm Odem junge Rosen,", "tokens": ["Da", "weckt", "mit", "eu\u00b7erm", "O\u00b7dem", "jun\u00b7ge", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Die gern mit euch von Lieb' und Hoffnung kosen,", "tokens": ["Die", "gern", "mit", "euch", "von", "Lieb'", "und", "Hoff\u00b7nung", "ko\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPER", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Und la\u00dft unanger\u00fchrt mich Hoffnungslosen.", "tokens": ["Und", "la\u00dft", "un\u00b7an\u00b7ge\u00b7r\u00fchrt", "mich", "Hoff\u00b7nungs\u00b7lo\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}