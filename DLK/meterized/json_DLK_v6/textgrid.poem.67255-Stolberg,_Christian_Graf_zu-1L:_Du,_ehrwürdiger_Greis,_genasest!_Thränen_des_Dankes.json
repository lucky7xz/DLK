{"textgrid.poem.67255": {"metadata": {"author": {"name": "Stolberg, Christian Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du, ehrw\u00fcrdiger Greis, genasest! Thr\u00e4nen des Dankes", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du, ehrw\u00fcrdiger Greis, genasest! Thr\u00e4nen des Dankes", "tokens": ["Du", ",", "ehr\u00b7w\u00fcr\u00b7di\u00b7ger", "Greis", ",", "ge\u00b7na\u00b7sest", "!", "Thr\u00e4\u00b7nen", "des", "Dan\u00b7kes"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "ADJA", "NN", "$,", "VVPP", "$.", "NN", "ART", "NN"], "meter": "-++--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Flie\u00dfen, es wallet empor frommer Gel\u00fcbde Gew\u00f6lk.", "tokens": ["Flie\u00b7\u00dfen", ",", "es", "wal\u00b7let", "em\u00b7por", "from\u00b7mer", "Ge\u00b7l\u00fcb\u00b7de", "Ge\u00b7w\u00f6lk", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PTKVZ", "ADJA", "NN", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Zahllos sind sie, die Perlen des Auges, die Perlen des Weihrauchs,", "tokens": ["Zahl\u00b7los", "sind", "sie", ",", "die", "Per\u00b7len", "des", "Au\u00b7ges", ",", "die", "Per\u00b7len", "des", "Weih\u00b7rauchs", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "ART", "NN", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Deines J\u00fcngers Hand n\u00e4hret die Glut des Altars.", "tokens": ["Dei\u00b7nes", "J\u00fcn\u00b7gers", "Hand", "n\u00e4h\u00b7ret", "die", "Glut", "des", "Al\u00b7tars", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}}, "stanza.2": {"line.1": {"text": "Bleib', und walte du ferner, ihr Herold der richtenden Wage,", "tokens": ["Bleib'", ",", "und", "wal\u00b7te", "du", "fer\u00b7ner", ",", "ihr", "He\u00b7rold", "der", "rich\u00b7ten\u00b7den", "Wa\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "VVFIN", "PPER", "ADV", "$,", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--++-+--+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Deren Urbild schwebt hoch an des Ewigen Thron!", "tokens": ["De\u00b7ren", "Ur\u00b7bild", "schwebt", "hoch", "an", "des", "E\u00b7wi\u00b7gen", "Thron", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.3": {"text": "Wohl bedarf es hienieden des hellen Geistes, des reinen", "tokens": ["Wohl", "be\u00b7darf", "es", "hien\u00b7ie\u00b7den", "des", "hel\u00b7len", "Geis\u00b7tes", ",", "des", "rei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,", "ART", "ADJA"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Herzens, und beides, o wem ward es \u2013 ach bleibe! \u2013 wie Dir!", "tokens": ["Her\u00b7zens", ",", "und", "bei\u00b7des", ",", "o", "wem", "ward", "es", "\u2013", "ach", "blei\u00b7be", "!", "\u2013", "wie", "Dir", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "PIS", "$,", "FM", "PWS", "VAFIN", "PPER", "$(", "XY", "VVFIN", "$.", "$(", "PWAV", "PPER", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}}}}}