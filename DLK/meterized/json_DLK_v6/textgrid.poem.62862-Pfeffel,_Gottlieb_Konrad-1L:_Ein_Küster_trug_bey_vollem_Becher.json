{"textgrid.poem.62862": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein K\u00fcster trug bey vollem Becher", "genre": "verse", "period": "N.A.", "pub_year": 1779, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein K\u00fcster trug bey vollem Becher", "tokens": ["Ein", "K\u00fcs\u00b7ter", "trug", "bey", "vol\u00b7lem", "Be\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Trotz einem Baccalaureus", "tokens": ["Trotz", "ei\u00b7nem", "Bac\u00b7ca\u00b7lau\u00b7reus"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Den Weltbau nach Copernicus", "tokens": ["Den", "Welt\u00b7bau", "nach", "Co\u00b7per\u00b7ni\u00b7cus"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NE"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.4": {"text": "Im Krug den Bauern vor. Ein grauer Zecher", "tokens": ["Im", "Krug", "den", "Bau\u00b7ern", "vor", ".", "Ein", "grau\u00b7er", "Ze\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "PTKVZ", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Schlug knirschend auf den Tisch: ey, Herr, was schwatzt er da?", "tokens": ["Schlug", "knir\u00b7schend", "auf", "den", "Tisch", ":", "ey", ",", "Herr", ",", "was", "schwatzt", "er", "da", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "$.", "NE", "$,", "NN", "$,", "PWS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Erde soll sich um die Sonne drehen?", "tokens": ["Die", "Er\u00b7de", "soll", "sich", "um", "die", "Son\u00b7ne", "dre\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Les er die Schrift: hie\u00df nicht einst Josua", "tokens": ["Les", "er", "die", "Schrift", ":", "hie\u00df", "nicht", "einst", "Jo\u00b7sua"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PPER", "ART", "NN", "$.", "VVFIN", "PTKNEG", "ADV", "NE"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.8": {"text": "In ihrem Lauf die Sonne stille stehen?", "tokens": ["In", "ih\u00b7rem", "Lauf", "die", "Son\u00b7ne", "stil\u00b7le", "ste\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Das ists ja, was ich sagen will;", "tokens": ["Das", "ists", "ja", ",", "was", "ich", "sa\u00b7gen", "will", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "$,", "PWS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Seit jenem Tage steht sie still,", "tokens": ["Seit", "je\u00b7nem", "Ta\u00b7ge", "steht", "sie", "still", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Versetzte Doctor Kunz; den Pfarrer m\u00f6cht ich sehen,", "tokens": ["Ver\u00b7setz\u00b7te", "Doc\u00b7tor", "Kunz", ";", "den", "Pfar\u00b7rer", "m\u00f6cht", "ich", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NE", "$.", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der aus der Bibel je bewies,", "tokens": ["Der", "aus", "der", "Bi\u00b7bel", "je", "be\u00b7wies", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Da\u00df er sie wieder laufen hie\u00df.", "tokens": ["Da\u00df", "er", "sie", "wie\u00b7der", "lau\u00b7fen", "hie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}