{"dta.poem.8605": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Abschied von Galizien.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Lebt wohl, lebt wohl, ihr trauten Lindenb\u00e4ume,", "tokens": ["Lebt", "wohl", ",", "lebt", "wohl", ",", "ihr", "trau\u00b7ten", "Lin\u00b7den\u00b7b\u00e4u\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "VVFIN", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die ihr an's stille Vaterhaus euch schmiegt!", "tokens": ["Die", "ihr", "an's", "stil\u00b7le", "Va\u00b7ter\u00b7haus", "euch", "schmiegt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihr seyd die Zeugen meiner Jugendtr\u00e4ume,", "tokens": ["Ihr", "seyd", "die", "Zeu\u00b7gen", "mei\u00b7ner", "Ju\u00b7gend\u00b7tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In die mich euer Fl\u00fcstern oft gewiegt.", "tokens": ["In", "die", "mich", "eu\u00b7er", "Fl\u00fcs\u00b7tern", "oft", "ge\u00b7wiegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nahm auch dem Knaben einst auf Augenblicke", "tokens": ["Nahm", "auch", "dem", "Kna\u00b7ben", "einst", "auf", "Au\u00b7gen\u00b7bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "NN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein eingebildet Ungl\u00fcck seine Ruh',", "tokens": ["Ein", "ein\u00b7ge\u00b7bil\u00b7det", "Un\u00b7gl\u00fcck", "sei\u00b7ne", "Ruh'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVPP", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und kam er trostlos dann zu euch zur\u00fccke,", "tokens": ["Und", "kam", "er", "trost\u00b7los", "dann", "zu", "euch", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ADV", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So rauschtet ihr ihm Trost und Freude zu.", "tokens": ["So", "rauschtet", "ihr", "ihm", "Trost", "und", "Freu\u00b7de", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Von meinen frohen Spielen seyd ihr Zeugen,", "tokens": ["Von", "mei\u00b7nen", "fro\u00b7hen", "Spie\u00b7len", "seyd", "ihr", "Zeu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von meinem raschen, leichten Jugendsinn,", "tokens": ["Von", "mei\u00b7nem", "ra\u00b7schen", ",", "leich\u00b7ten", "Ju\u00b7gend\u00b7sinn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nun s\u00e4uselt Wehmuth mir aus euren Zweigen,", "tokens": ["Nun", "s\u00e4u\u00b7selt", "Weh\u00b7muth", "mir", "aus", "eu\u00b7ren", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Tage meiner Jugend sind dahin!", "tokens": ["Die", "Ta\u00b7ge", "mei\u00b7ner", "Ju\u00b7gend", "sind", "da\u00b7hin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VAFIN", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sie sind dahin! \u2014 Ein Knabe noch vor Jahren,", "tokens": ["Sie", "sind", "da\u00b7hin", "!", "Ein", "Kna\u00b7be", "noch", "vor", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "$.", "$(", "ART", "NN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Nehm' Abschied heute ich als Mann von euch;", "tokens": ["Nehm'", "Ab\u00b7schied", "heu\u00b7te", "ich", "als", "Mann", "von", "euch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADV", "PPER", "KOUS", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich ziehe fort zu Thaten und Gefahren,", "tokens": ["Ich", "zie\u00b7he", "fort", "zu", "Tha\u00b7ten", "und", "Ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es gilt der Tyrannei den Todesstreich.", "tokens": ["Es", "gilt", "der", "Ty\u00b7ran\u00b7nei", "den", "To\u00b7des\u00b7streich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "So lebet wohl! \u2014 Du Werkzeug meiner Spiele,", "tokens": ["So", "le\u00b7bet", "wohl", "!", "Du", "Werk\u00b7zeug", "mei\u00b7ner", "Spie\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$.", "$(", "PPER", "NN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das einst ich trug, du kleines Schwert von Holz!", "tokens": ["Das", "einst", "ich", "trug", ",", "du", "klei\u00b7nes", "Schwert", "von", "Holz", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "VVFIN", "$,", "PPER", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sey nun ein Blitz in der Gewitterschw\u00fcle,", "tokens": ["Sey", "nun", "ein", "Blitz", "in", "der", "Ge\u00b7wit\u00b7ter\u00b7schw\u00fc\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du Ritterschwert, sey des Sarmaten Stolz!", "tokens": ["Du", "Rit\u00b7ter\u00b7schwert", ",", "sey", "des", "Sar\u00b7ma\u00b7ten", "Stolz", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "$,", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Lebt wohl, Geschwister! m\u00f6g' euch Gott bewahren!", "tokens": ["Lebt", "wohl", ",", "Ge\u00b7schwis\u00b7ter", "!", "m\u00f6g'", "euch", "Gott", "be\u00b7wah\u00b7ren", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "NN", "$.", "VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich bin ein Pole bis zum letzten Hauch!", "tokens": ["Ich", "bin", "ein", "Po\u00b7le", "bis", "zum", "letz\u00b7ten", "Hauch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hurrah! ihr vaterl\u00e4nd'schen Heldenschaaren!", "tokens": ["Hur\u00b7rah", "!", "ihr", "va\u00b7ter\u00b7l\u00e4n\u00b7d'\u00b7schen", "Hel\u00b7den\u00b7schaa\u00b7ren", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+-++-+-+-", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Leb' wohl, du mein geliebtes M\u00e4dchen auch! \u2014", "tokens": ["Leb'", "wohl", ",", "du", "mein", "ge\u00b7lieb\u00b7tes", "M\u00e4d\u00b7chen", "auch", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "ADV", "$,", "PPER", "PPOSAT", "ADJA", "NN", "ADV", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Schmach, J\u00fcngling, dir! h\u00e4lt dich der Glanz von Thr\u00e4nen", "tokens": ["Schmach", ",", "J\u00fcng\u00b7ling", ",", "dir", "!", "h\u00e4lt", "dich", "der", "Glanz", "von", "Thr\u00e4\u00b7nen"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "PPER", "$.", "VVFIN", "PPER", "ART", "NN", "APPR", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Zur\u00fcck vom ewig hellen Waffenglanz!", "tokens": ["Zu\u00b7r\u00fcck", "vom", "e\u00b7wig", "hel\u00b7len", "Waf\u00b7fen\u00b7glanz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dir, Jungfrau, Schmach! die du, bei Polens Sehnen", "tokens": ["Dir", ",", "Jung\u00b7frau", ",", "Schmach", "!", "die", "du", ",", "bei", "Po\u00b7lens", "Seh\u00b7nen"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "NN", "$,", "NN", "$.", "PRELS", "PPER", "$,", "APPR", "NE", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nach Freiheit, nun empf\u00e4ngst den Myrtenkranz!", "tokens": ["Nach", "Frei\u00b7heit", ",", "nun", "emp\u00b7f\u00e4ngst", "den", "Myr\u00b7ten\u00b7kranz", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Das Herz des Polenm\u00e4dchens darf nur schlagen", "tokens": ["Das", "Herz", "des", "Po\u00b7len\u00b7m\u00e4d\u00b7chens", "darf", "nur", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VMFIN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem Edlen, dem vor Schlachten nimmer graut,", "tokens": ["Dem", "Ed\u00b7len", ",", "dem", "vor", "Schlach\u00b7ten", "nim\u00b7mer", "graut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der gerne will die Todeswunde tragen,", "tokens": ["Der", "ger\u00b7ne", "will", "die", "To\u00b7des\u00b7wun\u00b7de", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn nur sein Schwert das Sklavenjoch zerhaut!", "tokens": ["Wenn", "nur", "sein", "Schwert", "das", "Skla\u00b7ven\u00b7joch", "zer\u00b7haut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Schmach, Mutter, dir! den du zur Schmach geboren,", "tokens": ["Schmach", ",", "Mut\u00b7ter", ",", "dir", "!", "den", "du", "zur", "Schmach", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PPER", "$.", "ART", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Umklammre deinen Sohn! entlass' ihn nicht!", "tokens": ["Um\u00b7klamm\u00b7re", "dei\u00b7nen", "Sohn", "!", "ent\u00b7lass'", "ihn", "nicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$.", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Freiheit Ruf schlug nicht an seine Ohren,", "tokens": ["Der", "Frei\u00b7heit", "Ruf", "schlug", "nicht", "an", "sei\u00b7ne", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er f\u00fchlt f\u00fcr Polen keine Kindespflicht!", "tokens": ["Er", "f\u00fchlt", "f\u00fcr", "Po\u00b7len", "kei\u00b7ne", "Kin\u00b7des\u00b7pflicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Dem Vater Schmach! \u2014 \u2014 doch dort, mit Silberhaaren,", "tokens": ["Dem", "Va\u00b7ter", "Schmach", "!", "doch", "dort", ",", "mit", "Sil\u00b7ber\u00b7haa\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$.", "$(", "$(", "ADV", "ADV", "$,", "APPR", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Wer ist der schwache Greis in Kriegertracht?", "tokens": ["Wer", "ist", "der", "schwa\u00b7che", "Greis", "in", "Krie\u00b7ger\u00b7tracht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du Alter, l\u00e4\u00dft du Weib und Kinder fahren?", "tokens": ["Du", "Al\u00b7ter", ",", "l\u00e4\u00dft", "du", "Weib", "und", "Kin\u00b7der", "fah\u00b7ren", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "VVFIN", "PPER", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kehrst du vom Grabe um, und wankst zur Schlacht?", "tokens": ["Kehrst", "du", "vom", "Gra\u00b7be", "um", ",", "und", "wankst", "zur", "Schlacht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$,", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "\u201eich habe Weib und Kinder Gott befohlen!", "tokens": ["\u201e", "ich", "ha\u00b7be", "Weib", "und", "Kin\u00b7der", "Gott", "be\u00b7foh\u00b7len", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "NN", "KON", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u201emein Haupt ist wei\u00df, es zittert meine Hand;", "tokens": ["\u201e", "mein", "Haupt", "ist", "wei\u00df", ",", "es", "zit\u00b7tert", "mei\u00b7ne", "Hand", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "VVFIN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u201edoch k\u00e4mpf' ich mit den heil'gen Kampf der Polen;", "tokens": ["\u201e", "doch", "k\u00e4mpf'", "ich", "mit", "den", "heil'\u00b7gen", "Kampf", "der", "Po\u00b7len", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u201ewohl mir! ich folge meinem Vaterland!\u201c", "tokens": ["\u201e", "wohl", "mir", "!", "ich", "fol\u00b7ge", "mei\u00b7nem", "Va\u00b7ter\u00b7land", "!", "\u201c"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "PPER", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "\u201eund m\u00f6ge nicht mein Vaterland verschm\u00e4hen", "tokens": ["\u201e", "und", "m\u00f6\u00b7ge", "nicht", "mein", "Va\u00b7ter\u00b7land", "ver\u00b7schm\u00e4\u00b7hen"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "KON", "VMFIN", "PTKNEG",