{"textgrid.poem.46907": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[zu leiden hast du selbst genug]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu leiden hast du selbst genug,", "tokens": ["Zu", "lei\u00b7den", "hast", "du", "selbst", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VAFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wovon dich kann kein Andrer scheiden;", "tokens": ["Wo\u00b7von", "dich", "kann", "kein", "A\u00b7ndrer", "schei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VMFIN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn alles Leid, das Andre schlug,", "tokens": ["Wenn", "al\u00b7les", "Leid", ",", "das", "And\u00b7re", "schlug", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du an dir selbst noch mit willst leiden,", "tokens": ["Du", "an", "dir", "selbst", "noch", "mit", "willst", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "ADV", "ADV", "APPR", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wo bliebe Zeit und Kraft dir nun,", "tokens": ["Wo", "blie\u00b7be", "Zeit", "und", "Kraft", "dir", "nun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "NN", "KON", "NN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auch etwas auf der Welt zu thun?", "tokens": ["Auch", "et\u00b7was", "auf", "der", "Welt", "zu", "thun", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Bedenk, du sollst vor deinem Scheiden", "tokens": ["Be\u00b7denk", ",", "du", "sollst", "vor", "dei\u00b7nem", "Schei\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auch etwas thun;", "tokens": ["Auch", "et\u00b7was", "thun", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Bedenke das, und la\u00df das Leiden", "tokens": ["Be\u00b7den\u00b7ke", "das", ",", "und", "la\u00df", "das", "Lei\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PDS", "$,", "KON", "VVIMP", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf sich beruhn.", "tokens": ["Auf", "sich", "be\u00b7ruhn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PRF", "VVINF", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}