{"textgrid.poem.60297": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Der Schiffsjunge", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das wilde, sch\u00e4umende Ro\u00df,", "tokens": ["Das", "wil\u00b7de", ",", "sch\u00e4u\u00b7men\u00b7de", "Ro\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Gejagt von der Sporen scharfem Sto\u00df,", "tokens": ["Ge\u00b7jagt", "von", "der", "Spo\u00b7ren", "schar\u00b7fem", "Sto\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Auf krumm gewundener Reiterbahn", "tokens": ["Auf", "krumm", "ge\u00b7wun\u00b7de\u00b7ner", "Rei\u00b7ter\u00b7bahn"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit seitw\u00e4rts geneigtem Leibe st\u00fcrmt:", "tokens": ["Mit", "seit\u00b7w\u00e4rts", "ge\u00b7neig\u00b7tem", "Lei\u00b7be", "st\u00fcrmt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "So fliegt, wie die Flut sich senkt und t\u00fcrmt,", "tokens": ["So", "fliegt", ",", "wie", "die", "Flut", "sich", "senkt", "und", "t\u00fcrmt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWAV", "ART", "NN", "PRF", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Das Schiff die Wellen hinab, hinan,", "tokens": ["Das", "Schiff", "die", "Wel\u00b7len", "hin\u00b7ab", ",", "hi\u00b7nan", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKVZ", "$,", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Vom m\u00e4chtigen Seitenwinde gefa\u00dft,", "tokens": ["Vom", "m\u00e4ch\u00b7ti\u00b7gen", "Sei\u00b7ten\u00b7win\u00b7de", "ge\u00b7fa\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Mit tief bord\u00fcber geneigtem Mast.", "tokens": ["Mit", "tief", "bord\u00b7\u00fc\u00b7ber", "ge\u00b7neig\u00b7tem", "Mast", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Es braust das Meer, es kracht und st\u00f6hnt", "tokens": ["Es", "braust", "das", "Meer", ",", "es", "kracht", "und", "st\u00f6hnt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des beladnen Fahrzeugs schwere Wucht", "tokens": ["Des", "be\u00b7lad\u00b7nen", "Fahr\u00b7zeugs", "schwe\u00b7re", "Wucht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Auf seiner rastlos eiligen Flucht;", "tokens": ["Auf", "sei\u00b7ner", "rast\u00b7los", "ei\u00b7li\u00b7gen", "Flucht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Der Matrosen freudiges Hurra! t\u00f6nt.", "tokens": ["Der", "Mat\u00b7ro\u00b7sen", "freu\u00b7di\u00b7ges", "Hur\u00b7ra", "!", "t\u00f6nt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$.", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Der Steuermann am Ruder steht,", "tokens": ["Der", "Steu\u00b7er\u00b7mann", "am", "Ru\u00b7der", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Rad mit gewaltigen Armen dreht,", "tokens": ["Das", "Rad", "mit", "ge\u00b7wal\u00b7ti\u00b7gen", "Ar\u00b7men", "dreht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Stets blickend scharf aufs zitternde Schwanken", "tokens": ["Stets", "bli\u00b7ckend", "scharf", "aufs", "zit\u00b7tern\u00b7de", "Schwan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Der Bussole mit mancherlei frohen Gedanken:", "tokens": ["Der", "Bus\u00b7so\u00b7le", "mit", "man\u00b7cher\u00b7lei", "fro\u00b7hen", "Ge\u00b7dan\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Er \u00fcberz\u00e4hlt sein Geldchen im stillen;", "tokens": ["Er", "\u00fc\u00b7berz\u00b7\u00e4hlt", "sein", "Geld\u00b7chen", "im", "stil\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPRART", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Schon h\u00f6rt er am Strande die Fiedel klingen,", "tokens": ["Schon", "h\u00f6rt", "er", "am", "Stran\u00b7de", "die", "Fie\u00b7del", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.11": {"text": "Wo bl\u00fchende, lustige Dirnen springen,", "tokens": ["Wo", "bl\u00fc\u00b7hen\u00b7de", ",", "lus\u00b7ti\u00b7ge", "Dir\u00b7nen", "sprin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "$,", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.12": {"text": "Die gerne dem Seemann sind zu Willen.", "tokens": ["Die", "ger\u00b7ne", "dem", "See\u00b7mann", "sind", "zu", "Wil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VAFIN", "APPR", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Vergn\u00fcgt, die Heimat wiederzusehn,", "tokens": ["Ver\u00b7gn\u00fcgt", ",", "die", "Hei\u00b7mat", "wie\u00b7der\u00b7zu\u00b7sehn", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.14": {"text": "Am Verdeck frisch auf und nieder geht", "tokens": ["Am", "Ver\u00b7deck", "frisch", "auf", "und", "nie\u00b7der", "geht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJD", "PTKVZ", "KON", "PTKVZ", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.15": {"text": "Waghaltenden Schritts der Kapit\u00e4n", "tokens": ["Wag\u00b7hal\u00b7ten\u00b7den", "Schritts", "der", "Ka\u00b7pi\u00b7t\u00e4n"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Und l\u00e4chelnd empor in die Segel sp\u00e4ht,", "tokens": ["Und", "l\u00e4\u00b7chelnd", "em\u00b7por", "in", "die", "Se\u00b7gel", "sp\u00e4ht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKVZ", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.17": {"text": "Die voll ihm schwellen zur Augenlabe", "tokens": ["Die", "voll", "ihm", "schwel\u00b7len", "zur", "Au\u00b7gen\u00b7la\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Von des Windes k\u00f6stlicher, fl\u00fcchtiger Habe.", "tokens": ["Von", "des", "Win\u00b7des", "k\u00f6st\u00b7li\u00b7cher", ",", "fl\u00fcch\u00b7ti\u00b7ger", "Ha\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,", "ADJA", "NN", "$."], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Dort klettert ein Junge gar flink und heiter", "tokens": ["Dort", "klet\u00b7tert", "ein", "Jun\u00b7ge", "gar", "flink", "und", "hei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die Sprossen hinauf der schwankenden Leiter;", "tokens": ["Die", "Spros\u00b7sen", "hin\u00b7auf", "der", "schwan\u00b7ken\u00b7den", "Lei\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+----+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Schon hat er erreicht in munterer Hast", "tokens": ["Schon", "hat", "er", "er\u00b7reicht", "in", "mun\u00b7te\u00b7rer", "Hast"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "APPR", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die h\u00f6chsten Segel am stolzen Mast;", "tokens": ["Die", "h\u00f6chs\u00b7ten", "Se\u00b7gel", "am", "stol\u00b7zen", "Mast", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Den L\u00fcftef\u00e4nger, den Wolkenraser,", "tokens": ["Den", "L\u00fcf\u00b7te\u00b7f\u00e4n\u00b7ger", ",", "den", "Wol\u00b7ken\u00b7ra\u00b7ser", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Den Mondespfl\u00fccker, den Sternengraser;", "tokens": ["Den", "Mon\u00b7des\u00b7pfl\u00fc\u00b7cker", ",", "den", "Ster\u00b7nen\u00b7gra\u00b7ser", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Da bricht das morsche Tau entzwei,", "tokens": ["Da", "bricht", "das", "mor\u00b7sche", "Tau", "ent\u00b7zwei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Woran er geschwebt, \u2013 ein banger Schrei \u2013", "tokens": ["Wo\u00b7ran", "er", "ge\u00b7schwebt", ",", "\u2013", "ein", "ban\u00b7ger", "Schrei", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "$,", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Er st\u00fcrzt hinunter ins Meer,", "tokens": ["Er", "st\u00fcrzt", "hin\u00b7un\u00b7ter", "ins", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "Und \u00fcber ihn st\u00fcrzen die Wellen her.", "tokens": ["Und", "\u00fc\u00b7ber", "ihn", "st\u00fcr\u00b7zen", "die", "Wel\u00b7len", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Umsonst, Matrosen, ist euer Bem\u00fchn,", "tokens": ["Um\u00b7sonst", ",", "Mat\u00b7ro\u00b7sen", ",", "ist", "eu\u00b7er", "Be\u00b7m\u00fchn", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Den J\u00fcngling zu retten, er ist dahin!", "tokens": ["Den", "J\u00fcng\u00b7ling", "zu", "ret\u00b7ten", ",", "er", "ist", "da\u00b7hin", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,", "PPER", "VAFIN", "PAV", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Wie hungernde Bestien st\u00fcrzen die Wellen", "tokens": ["Wie", "hun\u00b7gern\u00b7de", "Be\u00b7sti\u00b7en", "st\u00fcr\u00b7zen", "die", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Dem Opfer entgegen, sie schnauben und bellen;", "tokens": ["Dem", "Op\u00b7fer", "ent\u00b7ge\u00b7gen", ",", "sie", "schnau\u00b7ben", "und", "bel\u00b7len", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "PPER", "VVINF", "KON", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Schon hat ihn die eine w\u00fctend verschlungen,", "tokens": ["Schon", "hat", "ihn", "die", "ei\u00b7ne", "w\u00fc\u00b7tend", "ver\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ART", "ADJD", "VVPP", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und \u00fcber sie kommen die andern gesprungen,", "tokens": ["Und", "\u00fc\u00b7ber", "sie", "kom\u00b7men", "die", "an\u00b7dern", "ge\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ART", "ADJA", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Die um die Gierige neidisch schw\u00e4rmen", "tokens": ["Die", "um", "die", "Gie\u00b7ri\u00b7ge", "nei\u00b7disch", "schw\u00e4r\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ADJD", "VVINF"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Mit sch\u00e4umendem Rachen und wildem L\u00e4rmen.", "tokens": ["Mit", "sch\u00e4u\u00b7men\u00b7dem", "Ra\u00b7chen", "und", "wil\u00b7dem", "L\u00e4r\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.5": {"line.1": {"text": "Die Sonne wiederum zu Himmel steigt,", "tokens": ["Die", "Son\u00b7ne", "wie\u00b7de\u00b7rum", "zu", "Him\u00b7mel", "steigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da ruhn die Winde, jede Welle schweigt,", "tokens": ["Da", "ruhn", "die", "Win\u00b7de", ",", "je\u00b7de", "Wel\u00b7le", "schweigt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und traurig steht der feiernde Matrose,", "tokens": ["Und", "trau\u00b7rig", "steht", "der", "fei\u00b7ern\u00b7de", "Mat\u00b7ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--++-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Nachdenkend seinem wandelbaren Lose.", "tokens": ["Nach\u00b7den\u00b7kend", "sei\u00b7nem", "wan\u00b7del\u00b7ba\u00b7ren", "Lo\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Klar blickt der alte M\u00f6rder Ozean", "tokens": ["Klar", "blickt", "der", "al\u00b7te", "M\u00f6r\u00b7der", "O\u00b7ze\u00b7an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Dem Himmel zu, als h\u00e4tt er nichts getan.", "tokens": ["Dem", "Him\u00b7mel", "zu", ",", "als", "h\u00e4tt", "er", "nichts", "ge\u00b7tan", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "KOKOM", "VAFIN", "PPER", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}