{"dta.poem.8569": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1899", "urn": "urn:nbn:de:kobv:b4-200905192690", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dicke, gelbe Butterblumen!", "tokens": ["Di\u00b7cke", ",", "gel\u00b7be", "But\u00b7ter\u00b7blu\u00b7men", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Rasen blinkt, die G\u00f6tter gl\u00e4nzen.", "tokens": ["Der", "Ra\u00b7sen", "blinkt", ",", "die", "G\u00f6t\u00b7ter", "gl\u00e4n\u00b7zen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Eine nackte Venus untersucht ihr Knie, ein steinerner Hercules schl\u00e4gt die", "tokens": ["Ei\u00b7ne", "nack\u00b7te", "Ve\u00b7nus", "un\u00b7ter\u00b7sucht", "ihr", "Knie", ",", "ein", "stei\u00b7ner\u00b7ner", "Her\u00b7cu\u00b7les", "schl\u00e4gt", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "ART"], "meter": "+-+-+-+-+-+-+--++-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "Leyer.", "tokens": ["Le\u00b7yer", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.4": {"line.1": {"text": "Die Wasser st\u00fcrzen, die Wolken eilen,", "tokens": ["Die", "Was\u00b7ser", "st\u00fcr\u00b7zen", ",", "die", "Wol\u00b7ken", "ei\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "die Welt voll Sonne.", "tokens": ["die", "Welt", "voll", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Fr\u00fchling!", "tokens": ["Fr\u00fch\u00b7ling", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.6": {"line.1": {"text": "In meinem Herzen", "tokens": ["In", "mei\u00b7nem", "Her\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "tr\u00e4umt das Bild eines kleinen M\u00e4dchens", "tokens": ["tr\u00e4umt", "das", "Bild", "ei\u00b7nes", "klei\u00b7nen", "M\u00e4d\u00b7chens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "mit ge\u00f6ffneten Lippen und lachenden Augen.", "tokens": ["mit", "ge\u00b7\u00f6ff\u00b7ne\u00b7ten", "Lip\u00b7pen", "und", "la\u00b7chen\u00b7den", "Au\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}}}}}