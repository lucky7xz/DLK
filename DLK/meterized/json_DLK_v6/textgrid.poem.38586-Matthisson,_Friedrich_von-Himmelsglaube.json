{"textgrid.poem.38586": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Himmelsglaube", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es mag der Trennung Arm, im Vollgenu\u00df der Freuden", "tokens": ["Es", "mag", "der", "Tren\u00b7nung", "Arm", ",", "im", "Voll\u00b7ge\u00b7nu\u00df", "der", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "NN", "$,", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erhabner Sympathie, den Freund vom Freunde scheiden,", "tokens": ["Er\u00b7hab\u00b7ner", "Sym\u00b7pa\u00b7thie", ",", "den", "Freund", "vom", "Freun\u00b7de", "schei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der sanft und fest und treu, am Rande der Gefahr,", "tokens": ["Der", "sanft", "und", "fest", "und", "treu", ",", "am", "Ran\u00b7de", "der", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "KON", "ADJD", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie auf der Bahn des Gl\u00fccks, ihm Alles, Alles war:", "tokens": ["Wie", "auf", "der", "Bahn", "des", "Gl\u00fccks", ",", "ihm", "Al\u00b7les", ",", "Al\u00b7les", "war", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN", "$,", "PPER", "PIS", "$,", "PIS", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wo Himmelsglaube wohnt, Verla\u00dfner! da erhellt", "tokens": ["Wo", "Him\u00b7mels\u00b7glau\u00b7be", "wohnt", ",", "Ver\u00b7la\u00df\u00b7ner", "!", "da", "er\u00b7hellt"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PWAV", "NN", "VVFIN", "$,", "NN", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Zukunft Mitternacht ein Stern der h\u00f6hern Welt,", "tokens": ["Der", "Zu\u00b7kunft", "Mit\u00b7ter\u00b7nacht", "ein", "Stern", "der", "h\u00f6\u00b7hern", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und aus der Ferne winkt voll Glanz", "tokens": ["Und", "aus", "der", "Fer\u00b7ne", "winkt", "voll", "Glanz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Hoffnung mit dem Siegeskranz!", "tokens": ["Die", "Hoff\u00b7nung", "mit", "dem", "Sie\u00b7ges\u00b7kranz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es mag, wenn ringsumher die Rosen sich entf\u00e4rben,", "tokens": ["Es", "mag", ",", "wenn", "rings\u00b7um\u00b7her", "die", "Ro\u00b7sen", "sich", "ent\u00b7f\u00e4r\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "ADV", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des J\u00fcnglings Scherze fliehn, des Mannes Freuden sterben,", "tokens": ["Des", "J\u00fcng\u00b7lings", "Scher\u00b7ze", "fliehn", ",", "des", "Man\u00b7nes", "Freu\u00b7den", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der letzte s\u00fc\u00dfe Ton der Liebe selbst verwehn,", "tokens": ["Der", "letz\u00b7te", "s\u00fc\u00b7\u00dfe", "Ton", "der", "Lie\u00b7be", "selbst", "ver\u00b7wehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und jedes goldne Bild der T\u00e4uschung untergehn:", "tokens": ["Und", "je\u00b7des", "gold\u00b7ne", "Bild", "der", "T\u00e4u\u00b7schung", "un\u00b7ter\u00b7gehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wo Himmelsglaube wohnt, beut ihren Labetrunk", "tokens": ["Wo", "Him\u00b7mels\u00b7glau\u00b7be", "wohnt", ",", "beut", "ih\u00b7ren", "La\u00b7be\u00b7trunk"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "NN", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dem Allverge\u00dfnen mild noch die Erinnerung,", "tokens": ["Dem", "All\u00b7ver\u00b7ge\u00df\u00b7nen", "mild", "noch", "die", "E\u00b7rin\u00b7ne\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wenn ihm die Wange, schwer und kalt,", "tokens": ["Wenn", "ihm", "die", "Wan\u00b7ge", ",", "schwer", "und", "kalt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Des Todes Odem schon umwallt.", "tokens": ["Des", "To\u00b7des", "O\u00b7dem", "schon", "um\u00b7wallt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Kein Stundenschlag ert\u00f6nt, kein Tropfen Zeit entfluthet,", "tokens": ["Kein", "Stun\u00b7den\u00b7schlag", "er\u00b7t\u00f6nt", ",", "kein", "Trop\u00b7fen", "Zeit", "ent\u00b7flut\u00b7het", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,", "PIAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df nicht ein edles Herz um edle Herzen blutet;", "tokens": ["Da\u00df", "nicht", "ein", "ed\u00b7les", "Herz", "um", "ed\u00b7le", "Her\u00b7zen", "blu\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kein Abendstern erscheint, kein Morgenroth ergl\u00e4nzt,", "tokens": ["Kein", "A\u00b7bends\u00b7tern", "er\u00b7scheint", ",", "kein", "Mor\u00b7gen\u00b7roth", "er\u00b7gl\u00e4nzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,", "PIAT", "NN", "VVPP", "$,"], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.4": {"text": "Da\u00df fromme Liebe nicht ein fr\u00fches Grab umkr\u00e4nzt:", "tokens": ["Da\u00df", "from\u00b7me", "Lie\u00b7be", "nicht", "ein", "fr\u00fc\u00b7hes", "Grab", "um\u00b7kr\u00e4nzt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PTKNEG", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wo Himmelsglaube wohnt, schwingt \u00fcber Gruft und Zeit", "tokens": ["Wo", "Him\u00b7mels\u00b7glau\u00b7be", "wohnt", ",", "schwingt", "\u00fc\u00b7ber", "Gruft", "und", "Zeit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "VVFIN", "$,", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und Trennung, im Gef\u00fchl der Unverg\u00e4nglichkeit,", "tokens": ["Und", "Tren\u00b7nung", ",", "im", "Ge\u00b7f\u00fchl", "der", "Un\u00b7ver\u00b7g\u00e4ng\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sich zu verwandter Engel Chor", "tokens": ["Sich", "zu", "ver\u00b7wand\u00b7ter", "En\u00b7gel", "Chor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Des Ueberwinders Geist empor!", "tokens": ["Des", "Ue\u00b7ber\u00b7win\u00b7ders", "Geist", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}