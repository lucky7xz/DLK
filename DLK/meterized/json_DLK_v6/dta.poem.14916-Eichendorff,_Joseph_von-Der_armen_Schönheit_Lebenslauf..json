{"dta.poem.14916": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der armen Sch\u00f6nheit Lebenslauf.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Da zieh'n manch' redliche Gesellen,", "tokens": ["Da", "zieh'n", "man\u00b7ch'", "red\u00b7li\u00b7che", "Ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die sagen: Hast nicht Geld, noch Haus,", "tokens": ["Die", "sa\u00b7gen", ":", "Hast", "nicht", "Geld", ",", "noch", "Haus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "VVINF", "$.", "VAFIN", "PTKNEG", "NN", "$,", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir f\u00fcrchten Deine Augen helle,", "tokens": ["Wir", "f\u00fcrch\u00b7ten", "Dei\u00b7ne", "Au\u00b7gen", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wir haben nichts zum Hochzeitschmaus.", "tokens": ["Wir", "ha\u00b7ben", "nichts", "zum", "Hoch\u00b7zeit\u00b7schmaus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}