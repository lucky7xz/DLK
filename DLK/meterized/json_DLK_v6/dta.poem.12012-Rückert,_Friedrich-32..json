{"dta.poem.12012": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "32.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht \u00e4rgern sollst du dich an Fratzen, die der Glaube", "tokens": ["Nicht", "\u00e4r\u00b7gern", "sollst", "du", "dich", "an", "Frat\u00b7zen", ",", "die", "der", "Glau\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "VVINF", "VMFIN", "PPER", "PRF", "APPR", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Geschaffen hat, da\u00df er die Macht der Sch\u00f6nheit raube.", "tokens": ["Ge\u00b7schaf\u00b7fen", "hat", ",", "da\u00df", "er", "die", "Macht", "der", "Sch\u00f6n\u00b7heit", "rau\u00b7be", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "KOUS", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So schaffet Fratzen auch die ewige Natur;", "tokens": ["So", "schaf\u00b7fet", "Frat\u00b7zen", "auch", "die", "e\u00b7wi\u00b7ge", "Na\u00b7tur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sieh du von ihnen weg, und auf ihr Sch\u00f6nes nur!", "tokens": ["Sieh", "du", "von", "ih\u00b7nen", "weg", ",", "und", "auf", "ihr", "Sch\u00f6\u00b7nes", "nur", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "PPER", "PTKVZ", "$,", "KON", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und Leben, Welt und Staat ist reich an Fratzenbildern,", "tokens": ["Und", "Le\u00b7ben", ",", "Welt", "und", "Staat", "ist", "reich", "an", "Frat\u00b7zen\u00b7bil\u00b7dern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "NN", "KON", "NN", "VAFIN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Daher die Pfuscher auch am liebsten Fratzen schildern.", "tokens": ["Da\u00b7her", "die", "Pfu\u00b7scher", "auch", "am", "liebs\u00b7ten", "Frat\u00b7zen", "schil\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ADV", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nur vom Gebiet der Kunst hinweg, ihr Fratzen, geht!", "tokens": ["Nur", "vom", "Ge\u00b7biet", "der", "Kunst", "hin\u00b7weg", ",", "ihr", "Frat\u00b7zen", ",", "geht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Kunst, die \u00fcber Welt, Natur und Glauben steht.", "tokens": ["Der", "Kunst", ",", "die", "\u00fc\u00b7ber", "Welt", ",", "Na\u00b7tur", "und", "Glau\u00b7ben", "steht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "$,", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "So wenn sie jetzt nicht steht, hat sie doch einst gestanden;", "tokens": ["So", "wenn", "sie", "jetzt", "nicht", "steht", ",", "hat", "sie", "doch", "einst", "ge\u00b7stan\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADV", "PTKNEG", "VVFIN", "$,", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und bis sie's wieder thut, ehr ist sie nicht vorhanden.", "tokens": ["Und", "bis", "sie's", "wie\u00b7der", "thut", ",", "ehr", "ist", "sie", "nicht", "vor\u00b7han\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "ADV", "VVFIN", "$,", "NN", "VAFIN", "PPER", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}