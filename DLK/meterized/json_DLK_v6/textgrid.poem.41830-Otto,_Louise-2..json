{"textgrid.poem.41830": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wehm\u00fctig hat sie dann das Haupt gesenket", "tokens": ["Weh\u00b7m\u00fc\u00b7tig", "hat", "sie", "dann", "das", "Haupt", "ge\u00b7sen\u00b7ket"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und still gebetet: \u00bbGott mag dich bewahren!", "tokens": ["Und", "still", "ge\u00b7be\u00b7tet", ":", "\u00bb", "Gott", "mag", "dich", "be\u00b7wah\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "$.", "$(", "NN", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er sende dir aus seinen Engelscharen", "tokens": ["Er", "sen\u00b7de", "dir", "aus", "sei\u00b7nen", "En\u00b7gel\u00b7scha\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den reinen Schutzgeist, der dich f\u00fchrt und lenket.", "tokens": ["Den", "rei\u00b7nen", "Schutz\u00b7geist", ",", "der", "dich", "f\u00fchrt", "und", "len\u00b7ket", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Viel wirst geliebt du werden, viel gekr\u00e4nket,", "tokens": ["Viel", "wirst", "ge\u00b7liebt", "du", "wer\u00b7den", ",", "viel", "ge\u00b7kr\u00e4n\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "PPER", "VAINF", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Viel T\u00e4uschung und viel Leid wirst du erfahren;", "tokens": ["Viel", "T\u00e4u\u00b7schung", "und", "viel", "Leid", "wirst", "du", "er\u00b7fah\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Viel Thr\u00e4nen tr\u00fcben dir den Blick, den klaren,", "tokens": ["Viel", "Thr\u00e4\u00b7nen", "tr\u00fc\u00b7ben", "dir", "den", "Blick", ",", "den", "kla\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn sich dein Sehnen nicht noch selbst beschr\u00e4nket.\u00ab", "tokens": ["Wenn", "sich", "dein", "Seh\u00b7nen", "nicht", "noch", "selbst", "be\u00b7schr\u00e4n\u00b7ket", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "PTKNEG", "ADV", "ADV", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So gab sie sterbend mir den letzten Segen.", "tokens": ["So", "gab", "sie", "ster\u00b7bend", "mir", "den", "letz\u00b7ten", "Se\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich sank entsetzt vor der Entseelten nieder \u2013", "tokens": ["Ich", "sank", "ent\u00b7setzt", "vor", "der", "Ent\u00b7seel\u00b7ten", "nie\u00b7der", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Lang f\u00fchlt' ich nichts als meines Jammers Regen.", "tokens": ["Lang", "f\u00fchlt'", "ich", "nichts", "als", "mei\u00b7nes", "Jam\u00b7mers", "Re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PIS", "KOKOM", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Da rieselten mir Schauer durch die Glieder:", "tokens": ["Da", "rie\u00b7sel\u00b7ten", "mir", "Schau\u00b7er", "durch", "die", "Glie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NE", "APPR", "ART", "NN", "$."], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Soll ich nicht mit in deinen Sarg mich legen,", "tokens": ["Soll", "ich", "nicht", "mit", "in", "dei\u00b7nen", "Sarg", "mich", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "APPR", "APPR", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So la\u00df mir meine Tr\u00e4ume, meine Lieder.", "tokens": ["So", "la\u00df", "mir", "mei\u00b7ne", "Tr\u00e4u\u00b7me", ",", "mei\u00b7ne", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}