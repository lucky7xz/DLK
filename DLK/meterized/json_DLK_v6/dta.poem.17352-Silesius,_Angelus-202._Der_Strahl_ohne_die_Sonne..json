{"dta.poem.17352": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "202.  Der Strahl ohne die Sonne.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Der Strahl ist nichts wen\u0304 er sich von der Sonn abbricht;", "tokens": ["Der", "Strahl", "ist", "nichts", "wen\u0304", "er", "sich", "von", "der", "Sonn", "ab\u00b7bricht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIS", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Du gleichfalls/ l\u00e4stu GOtt dein wesentliches licht.", "tokens": ["Du", "gleich\u00b7falls", "/", "l\u00e4s\u00b7tu", "Gott", "dein", "we\u00b7sent\u00b7li\u00b7ches", "licht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$(", "VVFIN", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}