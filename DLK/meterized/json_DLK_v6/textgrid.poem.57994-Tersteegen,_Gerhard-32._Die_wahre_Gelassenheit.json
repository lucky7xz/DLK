{"textgrid.poem.57994": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "32. Die wahre Gelassenheit", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bald folgt man Gott nicht nach, bald l\u00e4uft man vor zu heftig,", "tokens": ["Bald", "folgt", "man", "Gott", "nicht", "nach", ",", "bald", "l\u00e4uft", "man", "vor", "zu", "hef\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "NN", "PTKNEG", "PTKVZ", "$,", "ADV", "VVFIN", "PIS", "APPR", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der eine ist zu tr\u00e4g, der andre zu gesch\u00e4ftig;", "tokens": ["Der", "ei\u00b7ne", "ist", "zu", "tr\u00e4g", ",", "der", "and\u00b7re", "zu", "ge\u00b7sch\u00e4f\u00b7tig", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "PTKA", "ADJD", "$,", "PRELS", "PIS", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Herr, w\u00e4r' ich dir nur so, wie mir ist meine Hand,", "tokens": ["Herr", ",", "w\u00e4r'", "ich", "dir", "nur", "so", ",", "wie", "mir", "ist", "mei\u00b7ne", "Hand", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "PPER", "ADV", "ADV", "$,", "PWAV", "PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.4": {"text": "So, d\u00fcnkt mich, h\u00e4tt' ich wohl den rechten Mittelstand.", "tokens": ["So", ",", "d\u00fcnkt", "mich", ",", "h\u00e4tt'", "ich", "wohl", "den", "rech\u00b7ten", "Mit\u00b7tel\u00b7stand", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PPER", "$,", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}