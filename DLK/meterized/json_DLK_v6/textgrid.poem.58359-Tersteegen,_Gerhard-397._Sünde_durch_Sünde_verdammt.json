{"textgrid.poem.58359": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "397. S\u00fcnde durch S\u00fcnde verdammt", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich tat vorhin die S\u00fcnd' mit Willen und mit Freuden,", "tokens": ["Ich", "tat", "vor\u00b7hin", "die", "S\u00fcnd'", "mit", "Wil\u00b7len", "und", "mit", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nun mu\u00df ich S\u00fcnd' mit Schmerz und Widerwillen leiden;", "tokens": ["Nun", "mu\u00df", "ich", "S\u00fcnd'", "mit", "Schmerz", "und", "Wi\u00b7der\u00b7wil\u00b7len", "lei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dies Leiden bessert zwar, doch, o der bittern Pein,", "tokens": ["Dies", "Lei\u00b7den", "bes\u00b7sert", "zwar", ",", "doch", ",", "o", "der", "bit\u00b7tern", "Pein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VVFIN", "ADV", "$,", "ADV", "$,", "FM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn S\u00fcnd' durch S\u00fcnde soll in uns verdammet sein!", "tokens": ["Wenn", "S\u00fcnd'", "durch", "S\u00fcn\u00b7de", "soll", "in", "uns", "ver\u00b7dam\u00b7met", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "NN", "VMFIN", "APPR", "PPER", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}