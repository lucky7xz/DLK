{"textgrid.poem.26058": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Noch ist die Zeit der blauen B\u00e4ume", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch ist die Zeit der blauen B\u00e4ume,", "tokens": ["Noch", "ist", "die", "Zeit", "der", "blau\u00b7en", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie schauen mit kahlem Ge\u00e4st", "tokens": ["Sie", "schau\u00b7en", "mit", "kah\u00b7lem", "Ge\u00b7\u00e4st"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Weit in die funkelnde Ewigkeit", "tokens": ["Weit", "in", "die", "fun\u00b7keln\u00b7de", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und halten sich kahl am Himmelsblau fest.", "tokens": ["Und", "hal\u00b7ten", "sich", "kahl", "am", "Him\u00b7mels\u00b7blau", "fest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und nur die Wolken, wei\u00df und breit,", "tokens": ["Und", "nur", "die", "Wol\u00b7ken", ",", "wei\u00df", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bauen im blauen Baum ihr Nest.", "tokens": ["Bau\u00b7en", "im", "blau\u00b7en", "Baum", "ihr", "Nest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Die Winde fegten fort verj\u00e4hrten Bl\u00e4tterrest,", "tokens": ["Die", "Win\u00b7de", "feg\u00b7ten", "fort", "ver\u00b7j\u00e4hr\u00b7ten", "Bl\u00e4t\u00b7ter\u00b7rest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und dein Auge im Baum weiten Raum hat", "tokens": ["Und", "dein", "Au\u00b7ge", "im", "Baum", "wei\u00b7ten", "Raum", "hat"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "APPRART", "NN", "ADJA", "NN", "VAFIN"], "meter": "--+--++-+-", "measure": "anapaest.di.plus"}, "line.9": {"text": "F\u00fcr der verliebten Gedanken luftige Lagerstatt.", "tokens": ["F\u00fcr", "der", "ver\u00b7lieb\u00b7ten", "Ge\u00b7dan\u00b7ken", "luf\u00b7ti\u00b7ge", "La\u00b7ger\u00b7statt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "+--+--+-+--+-+", "measure": "dactylic.di.plus"}}}}}