{"textgrid.poem.38824": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "[ich bin ein Engel, Menschenkind, das wisse]", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin ein Engel, Menschenkind, das wisse,", "tokens": ["Ich", "bin", "ein", "En\u00b7gel", ",", "Men\u00b7schen\u00b7kind", ",", "das", "wis\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "NN", "$,", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein Fl\u00fcgelpaar klingt in dem Morgenlichte,", "tokens": ["Mein", "Fl\u00fc\u00b7gel\u00b7paar", "klingt", "in", "dem", "Mor\u00b7gen\u00b7lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den gr\u00fcnen Wald erfreut mein Angesichte,", "tokens": ["Den", "gr\u00fc\u00b7nen", "Wald", "er\u00b7freut", "mein", "An\u00b7ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Nachtigallen-Chor giebt seine Gr\u00fcsse.", "tokens": ["Das", "Nach\u00b7ti\u00b7gal\u00b7len\u00b7Chor", "giebt", "sei\u00b7ne", "Gr\u00fcs\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wem ich der Sterblichen die Lippe k\u00fcsse,", "tokens": ["Wem", "ich", "der", "Sterb\u00b7li\u00b7chen", "die", "Lip\u00b7pe", "k\u00fcs\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem t\u00f6nt die Welt ein g\u00f6ttliches Gedichte,", "tokens": ["Dem", "t\u00f6nt", "die", "Welt", "ein", "g\u00f6tt\u00b7li\u00b7ches", "Ge\u00b7dich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wald, Wasser, Feld und Luft spricht ihm Geschichte,", "tokens": ["Wald", ",", "Was\u00b7ser", ",", "Feld", "und", "Luft", "spricht", "ihm", "Ge\u00b7schich\u00b7te", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.4": {"text": "Im Herzen rinnen Paradieses-Fl\u00fcsse.", "tokens": ["Im", "Her\u00b7zen", "rin\u00b7nen", "Pa\u00b7ra\u00b7die\u00b7ses\u00b7F\u00b7l\u00fcs\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die ewge Liebe, welche nie vergangen,", "tokens": ["Die", "ew\u00b7ge", "Lie\u00b7be", ",", "wel\u00b7che", "nie", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erscheint ihm im Triumph auf allen Wogen,", "tokens": ["Er\u00b7scheint", "ihm", "im", "Tri\u00b7umph", "auf", "al\u00b7len", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Er nimmt den T\u00f6nen ihre dunkle H\u00fclle,", "tokens": ["Er", "nimmt", "den", "T\u00f6\u00b7nen", "ih\u00b7re", "dunk\u00b7le", "H\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Da regt sich, schl\u00e4gt in Jubel auf die Stille,", "tokens": ["Da", "regt", "sich", ",", "schl\u00e4gt", "in", "Ju\u00b7bel", "auf", "die", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "$,", "VVFIN", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zur spiel'nden Glorie wird der Himmelsbogen,", "tokens": ["Zur", "spiel'n\u00b7den", "Glo\u00b7rie", "wird", "der", "Him\u00b7mels\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Trunkne h\u00f6rt, was alle Engel sangen.", "tokens": ["Der", "Trunk\u00b7ne", "h\u00f6rt", ",", "was", "al\u00b7le", "En\u00b7gel", "san\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}