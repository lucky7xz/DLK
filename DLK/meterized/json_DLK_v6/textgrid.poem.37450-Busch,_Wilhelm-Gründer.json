{"textgrid.poem.37450": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Gr\u00fcnder", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gesch\u00e4ftig sind die Menschenkinder,", "tokens": ["Ge\u00b7sch\u00e4f\u00b7tig", "sind", "die", "Men\u00b7schen\u00b7kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die gro\u00dfe Zunft von kleinen Meistern,", "tokens": ["Die", "gro\u00b7\u00dfe", "Zunft", "von", "klei\u00b7nen", "Meis\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Als Mitbegr\u00fcnder, Miterfinder", "tokens": ["Als", "Mit\u00b7be\u00b7gr\u00fcn\u00b7der", ",", "Mi\u00b7ter\u00b7fin\u00b7der"], "token_info": ["word", "word", "punct", "word"], "pos": ["KOUS", "NN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sich diese Welt zurechtzukleistern.", "tokens": ["Sich", "die\u00b7se", "Welt", "zu\u00b7recht\u00b7zu\u00b7kleis\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nur leider kann man sich nicht einen,", "tokens": ["Nur", "lei\u00b7der", "kann", "man", "sich", "nicht", "ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PIS", "PRF", "PTKNEG", "ART", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie man das Ding am besten mache.", "tokens": ["Wie", "man", "das", "Ding", "am", "bes\u00b7ten", "ma\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "PTKA", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das Bauen mit belebten Steinen", "tokens": ["Das", "Bau\u00b7en", "mit", "be\u00b7leb\u00b7ten", "Stei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist eine h\u00f6chst verzwickte Sache.", "tokens": ["Ist", "ei\u00b7ne", "h\u00f6chst", "ver\u00b7zwick\u00b7te", "Sa\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Welch ein Gedr\u00e4ngel und Getriebe", "tokens": ["Welch", "ein", "Ge\u00b7dr\u00e4n\u00b7gel", "und", "Ge\u00b7trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von Lieb und Ha\u00df bei Nacht und Tage,", "tokens": ["Von", "Lieb", "und", "Ha\u00df", "bei", "Nacht", "und", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und unaufh\u00f6rlich setzt es Hiebe,", "tokens": ["Und", "un\u00b7auf\u00b7h\u00f6r\u00b7lich", "setzt", "es", "Hie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und unaufh\u00f6rlich t\u00f6nt die Klage.", "tokens": ["Und", "un\u00b7auf\u00b7h\u00f6r\u00b7lich", "t\u00f6nt", "die", "Kla\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Gottlob, es gibt auch stille Leute,", "tokens": ["Gott\u00b7lob", ",", "es", "gibt", "auch", "stil\u00b7le", "Leu\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die meiden dies Gew\u00fchl und hassen's", "tokens": ["Die", "mei\u00b7den", "dies", "Ge\u00b7w\u00fchl", "und", "has\u00b7sen's"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "PDS", "NN", "KON", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und bauen auf der andern Seite", "tokens": ["Und", "bau\u00b7en", "auf", "der", "an\u00b7dern", "Sei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sich eine Welt des Unterlassens.", "tokens": ["Sich", "ei\u00b7ne", "Welt", "des", "Un\u00b7ter\u00b7las\u00b7sens", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}