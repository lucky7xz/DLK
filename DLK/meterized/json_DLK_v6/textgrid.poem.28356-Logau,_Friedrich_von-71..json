{"textgrid.poem.28356": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "71.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hat nicht der das halbe Brot, der ein sch\u00f6nes Weiblein hat?", "tokens": ["Hat", "nicht", "der", "das", "hal\u00b7be", "Brot", ",", "der", "ein", "sch\u00f6\u00b7nes", "Weib\u00b7lein", "hat", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "ART", "ADJA", "NN", "$,", "PRELS", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Freylich; wer im Magen nicht, nur an Augen wil seyn sat;", "tokens": ["Frey\u00b7lich", ";", "wer", "im", "Ma\u00b7gen", "nicht", ",", "nur", "an", "Au\u00b7gen", "wil", "seyn", "sat", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PWS", "APPRART", "NN", "PTKNEG", "$,", "ADV", "APPR", "NN", "VMFIN", "VAINF", "VVFIN", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Sch\u00f6nes Weib ist Fleisch, nicht Brot, da\u00df die Sinnen speist mit Lust,", "tokens": ["Sch\u00f6\u00b7nes", "Weib", "ist", "Fleisch", ",", "nicht", "Brot", ",", "da\u00df", "die", "Sin\u00b7nen", "speist", "mit", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "NN", "$,", "PTKNEG", "NN", "$,", "KOUS", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Darff Brot selbst und darff auch Fleisch, weil es viel zu m\u00e4sten kost.", "tokens": ["Darff", "Brot", "selbst", "und", "darff", "auch", "Fleisch", ",", "weil", "es", "viel", "zu", "m\u00e4s\u00b7ten", "kost", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "ADV", "KON", "VMFIN", "ADV", "NN", "$,", "KOUS", "PPER", "ADV", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}