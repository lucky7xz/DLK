{"textgrid.poem.33576": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sinkend schwebt der Mond in Schleiern", "tokens": ["Sin\u00b7kend", "schwebt", "der", "Mond", "in", "Schlei\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tr\u00fcber Wolken durch die Luft,", "tokens": ["Tr\u00fc\u00b7ber", "Wol\u00b7ken", "durch", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rosen und Jasminbl\u00fct' feiern", "tokens": ["Ro\u00b7sen", "und", "Jas\u00b7min\u00b7bl\u00fct'", "fei\u00b7ern"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVINF"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Seinen Glanz mit s\u00fc\u00dfem Duft.", "tokens": ["Sei\u00b7nen", "Glanz", "mit", "s\u00fc\u00b7\u00dfem", "Duft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Unbegrenzte W\u00fcnsche dehnen", "tokens": ["Un\u00b7be\u00b7grenz\u00b7te", "W\u00fcn\u00b7sche", "deh\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine Brust und regen, ach!", "tokens": ["Mei\u00b7ne", "Brust", "und", "re\u00b7gen", ",", "ach", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "ADJA", "$,", "ITJ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gl\u00fchender ein hei\u00dfes Sehnen", "tokens": ["Gl\u00fc\u00b7hen\u00b7der", "ein", "hei\u00b7\u00dfes", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unbestimmter W\u00fcnsche wach.", "tokens": ["Un\u00b7be\u00b7stimm\u00b7ter", "W\u00fcn\u00b7sche", "wach", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "K\u00f6rperlos, ein Geisterleben,", "tokens": ["K\u00f6r\u00b7per\u00b7los", ",", "ein", "Geis\u00b7ter\u00b7le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Frei jetzt m\u00f6cht' ich und allein", "tokens": ["Frei", "jetzt", "m\u00f6cht'", "ich", "und", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VMFIN", "PPER", "KON", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00dcber Berg' und Meere schweben,", "tokens": ["\u00dc\u00b7ber", "Ber\u00b7g'", "und", "Mee\u00b7re", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "VVINF", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Cherub oder D\u00e4mon sein.", "tokens": ["Che\u00b7rub", "o\u00b7der", "D\u00e4\u00b7mon", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VAINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Mit dem Sturz des Wasserfalles", "tokens": ["Mit", "dem", "Sturz", "des", "Was\u00b7ser\u00b7fal\u00b7les"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jauchzt' ich Nacht und Abgrund zu:", "tokens": ["Jauchzt'", "ich", "Nacht", "und", "Ab\u00b7grund", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbeine lieb' ich \u00fcber alles,", "tokens": ["\u00bb", "ei\u00b7ne", "lieb'", "ich", "\u00fc\u00b7ber", "al\u00b7les", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "VVFIN", "PPER", "APPR", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Eine, die bist du!\u00ab", "tokens": ["Und", "die", "Ei\u00b7ne", ",", "die", "bist", "du", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "VAFIN", "PPER", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "W\u00e4rst du da, Geliebte, k\u00fchltest", "tokens": ["W\u00e4rst", "du", "da", ",", "Ge\u00b7lieb\u00b7te", ",", "k\u00fchl\u00b7test"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["VAFIN", "PPER", "ADV", "$,", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine hei\u00dfe Stirne sacht", "tokens": ["Mei\u00b7ne", "hei\u00b7\u00dfe", "Stir\u00b7ne", "sacht"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit der zarten Hand und f\u00fchltest", "tokens": ["Mit", "der", "zar\u00b7ten", "Hand", "und", "f\u00fchl\u00b7test"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit mir diese sch\u00f6ne Nacht!", "tokens": ["Mit", "mir", "die\u00b7se", "sch\u00f6\u00b7ne", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "O des Mondes Licht erschiene", "tokens": ["O", "des", "Mon\u00b7des", "Licht", "er\u00b7schie\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "NN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht so tr\u00fcb dort im Vergl\u00fchn,", "tokens": ["Nicht", "so", "tr\u00fcb", "dort", "im", "Ver\u00b7gl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJD", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn die Rosen und Jasmine", "tokens": ["Denn", "die", "Ro\u00b7sen", "und", "Jas\u00b7mi\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00fcrden f\u00fcr uns beide bl\u00fchn.", "tokens": ["W\u00fcr\u00b7den", "f\u00fcr", "uns", "bei\u00b7de", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "PIS", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Winke dir im Sternenscheine", "tokens": ["Win\u00b7ke", "dir", "im", "Ster\u00b7nen\u00b7schei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine Seele Frieden zu;", "tokens": ["Mei\u00b7ne", "See\u00b7le", "Frie\u00b7den", "zu", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00dcber alles lieb' ich Eine,", "tokens": ["\u00dc\u00b7ber", "al\u00b7les", "lieb'", "ich", "Ei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "PPER", "ART", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Eine, die bist du!", "tokens": ["Und", "die", "Ei\u00b7ne", ",", "die", "bist", "du", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "VAFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}