{"textgrid.poem.67633": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "54.", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwas f\u00e4ngst Du an,", "tokens": ["\u00bb", "was", "f\u00e4ngst", "Du", "an", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Du Thor? s\u00e4ufst einen Ocean,", "tokens": ["Du", "Thor", "?", "s\u00e4ufst", "ei\u00b7nen", "O\u00b7cean", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$.", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zu jenem Stein zu kommen?", "tokens": ["Zu", "je\u00b7nem", "Stein", "zu", "kom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und schl\u00fcrfest Du ihn denn auf einmal ein,", "tokens": ["Und", "schl\u00fcr\u00b7fest", "Du", "ihn", "denn", "auf", "ein\u00b7mal", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "ADV", "APPR", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es ist ein Stein.\u00ab", "tokens": ["Es", "ist", "ein", "Stein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Jedoch in Mitte seiner Bahn,", "tokens": ["Je\u00b7doch", "in", "Mit\u00b7te", "sei\u00b7ner", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dem Schl\u00fcrfen und dem Ocean,", "tokens": ["Dem", "Schl\u00fcr\u00b7fen", "und", "dem", "O\u00b7cean", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Barst er zu seinem Frommen.", "tokens": ["Barst", "er", "zu", "sei\u00b7nem", "From\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein fr\u00fches Ungl\u00fcck,", "tokens": ["Ein", "fr\u00fc\u00b7hes", "Un\u00b7gl\u00fcck", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Allzeit ein Gl\u00fcck.", "tokens": ["All\u00b7zeit", "ein", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Es spart Dir sp\u00e4te Reu'", "tokens": ["Es", "spart", "Dir", "sp\u00e4\u00b7te", "Reu'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und Schmach dabei.", "tokens": ["Und", "Schmach", "da\u00b7bei", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "PAV", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}