{"textgrid.poem.53043": {"metadata": {"author": {"name": "Lohenstein, Daniel Casper von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich bin die Blumenk\u00f6niginn,", "genre": "verse", "period": "N.A.", "pub_year": 1659, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin die Blumenk\u00f6niginn,", "tokens": ["Ich", "bin", "die", "Blu\u00b7men\u00b7k\u00f6\u00b7ni\u00b7ginn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Welt- und Himmelsg\u00e4rtnerinn;", "tokens": ["Die", "Welt", "und", "Him\u00b7mels\u00b7g\u00e4rt\u00b7ne\u00b7rinn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn Berg und Thal, Gebirg' und Wiesen s\u00e4ugen", "tokens": ["Denn", "Berg", "und", "Thal", ",", "Ge\u00b7bir\u00b7g'", "und", "Wie\u00b7sen", "s\u00e4u\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "$,", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die edeln Blumen nicht allein:", "tokens": ["Die", "e\u00b7deln", "Blu\u00b7men", "nicht", "al\u00b7lein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie wachsen in Krystall und Stein,", "tokens": ["Sie", "wach\u00b7sen", "in", "Krys\u00b7tall", "und", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Sie lassen sich in Erz und Muscheln zeugen.", "tokens": ["Sie", "las\u00b7sen", "sich", "in", "Erz", "und", "Mu\u00b7scheln", "zeu\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Fl\u00fcsse, Seen und das Meer", "tokens": ["Die", "Fl\u00fcs\u00b7se", ",", "Seen", "und", "das", "Meer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "NN", "KON", "ART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Sind nicht von Klee und Veilchen leer;", "tokens": ["Sind", "nicht", "von", "Klee", "und", "Veil\u00b7chen", "leer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "APPR", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ja, Vorwitz hat sogar die Pracht", "tokens": ["Ja", ",", "Vor\u00b7witz", "hat", "so\u00b7gar", "die", "Pracht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "NN", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Agtsteinenen Gebl\u00fcms und Rosen aus Krystallen,", "tokens": ["Agt\u00b7stei\u00b7ne\u00b7nen", "Ge\u00b7bl\u00fcms", "und", "Ro\u00b7sen", "aus", "Krys\u00b7tal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Als die sich in der Luft versteinernden Korallen,", "tokens": ["Als", "die", "sich", "in", "der", "Luft", "ver\u00b7stei\u00b7nern\u00b7den", "Ko\u00b7ral\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PRF", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Aus Thetis Schoo\u00df' an's Licht gebracht.", "tokens": ["Aus", "The\u00b7tis", "Schoo\u00df'", "an's", "Licht", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So ist's auch nur ein Alpbild im Gehirne,", "tokens": ["So", "ist's", "auch", "nur", "ein", "Alp\u00b7bild", "im", "Ge\u00b7hir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df ein Gestirn ein B\u00e4r sei oder Stier.", "tokens": ["Da\u00df", "ein", "Ge\u00b7stirn", "ein", "B\u00e4r", "sei", "o\u00b7der", "Stier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VAFIN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Erdball stellt ja einen Garten f\u00fcr", "tokens": ["Der", "Erd\u00b7ball", "stellt", "ja", "ei\u00b7nen", "Gar\u00b7ten", "f\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Durch meiner Blumen irdische Gestirne;", "tokens": ["Durch", "mei\u00b7ner", "Blu\u00b7men", "ir\u00b7di\u00b7sche", "Ge\u00b7stir\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der Himmel aber ", "tokens": ["Der", "Him\u00b7mel", "a\u00b7ber"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Sind Blumen; der neun hellen Sterne Glanz", "tokens": ["Sind", "Blu\u00b7men", ";", "der", "neun", "hel\u00b7len", "Ster\u00b7ne", "Glanz"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "$.", "ART", "CARD", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "War vor der Zeit der Ariadne Kranz.", "tokens": ["War", "vor", "der", "Zeit", "der", "A\u00b7riad\u00b7ne", "Kranz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+----+", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "So m\u00f6gt ihr euch vor mir schamr\u00f6thig nun entfernen,", "tokens": ["So", "m\u00f6gt", "ihr", "euch", "vor", "mir", "scham\u00b7r\u00f6\u00b7thig", "nun", "ent\u00b7fer\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "APPR", "PPER", "ADJD", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr G\u00f6ttinnen der andern Jahreszeit,", "tokens": ["Ihr", "G\u00f6t\u00b7tin\u00b7nen", "der", "an\u00b7dern", "Jah\u00b7res\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Weil Ceres nur allein im Sommer Klee abmeiht,", "tokens": ["Weil", "Ce\u00b7res", "nur", "al\u00b7lein", "im", "Som\u00b7mer", "Klee", "ab\u00b7meiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADV", "ADV", "APPRART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Pomone nur den Herbst ausziert mit Obstgerichten,", "tokens": ["Po\u00b7mo\u00b7ne", "nur", "den", "Herbst", "aus\u00b7ziert", "mit", "Obst\u00b7ge\u00b7rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Himmel auch nur prangt mit Blumen, nicht mit Fr\u00fcchten.", "tokens": ["Der", "Him\u00b7mel", "auch", "nur", "prangt", "mit", "Blu\u00b7men", ",", "nicht", "mit", "Fr\u00fcch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "VVFIN", "APPR", "NN", "$,", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Hingegen ist mein Schmuck des ganzen Jahres Kleid,", "tokens": ["Hin\u00b7ge\u00b7gen", "ist", "mein", "Schmuck", "des", "gan\u00b7zen", "Jah\u00b7res", "Kleid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Den nicht der Reif des Herbstes kann entf\u00e4rben,", "tokens": ["Den", "nicht", "der", "Reif", "des", "Herbs\u00b7tes", "kann", "ent\u00b7f\u00e4r\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ART", "NN", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der Sommer nicht versengen und verderben,", "tokens": ["Der", "Som\u00b7mer", "nicht", "ver\u00b7sen\u00b7gen", "und", "ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Des Winters Frost nicht tilgt, der Alles sonst verschneit.", "tokens": ["Des", "Win\u00b7ters", "Frost", "nicht", "tilgt", ",", "der", "Al\u00b7les", "sonst", "ver\u00b7schneit", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKNEG", "VVFIN", "$,", "PRELS", "PIS", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Kein Kraut, kein Baum bringt seine Frucht herf\u00fcr,", "tokens": ["Kein", "Kraut", ",", "kein", "Baum", "bringt", "sei\u00b7ne", "Frucht", "her\u00b7f\u00fcr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die nicht vorher mit Bl\u00fcth' und Blumen prahlen.", "tokens": ["Die", "nicht", "vor\u00b7her", "mit", "Bl\u00fcth'", "und", "Blu\u00b7men", "prah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ADV", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Pomeranzen purpurreiche Schalen", "tokens": ["Der", "Po\u00b7me\u00b7ran\u00b7zen", "pur\u00b7pur\u00b7rei\u00b7che", "Scha\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sind doch besch\u00e4mt durch ihrer Bl\u00fcthe Zier;", "tokens": ["Sind", "doch", "be\u00b7sch\u00e4mt", "durch", "ih\u00b7rer", "Bl\u00fc\u00b7the", "Zier", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Nu\u00df giebt nach der Blume der Muskaten,", "tokens": ["Die", "Nu\u00df", "giebt", "nach", "der", "Blu\u00b7me", "der", "Mus\u00b7ka\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und der Geschmack der Aepfel von Granaten", "tokens": ["Und", "der", "Ge\u00b7schmack", "der", "A\u00b7e\u00b7pfel", "von", "Gra\u00b7na\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN", "APPR", "NN"], "meter": "+--+-++--++-", "measure": "iambic.hexa.invert"}, "line.7": {"text": "Weicht ihrer Bl\u00fcth' an Farben und Geruch;", "tokens": ["Weicht", "ih\u00b7rer", "Bl\u00fcth'", "an", "Far\u00b7ben", "und", "Ge\u00b7ruch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Das fette Feld ist ein smaragden Tuch,", "tokens": ["Das", "fet\u00b7te", "Feld", "ist", "ein", "sma\u00b7rag\u00b7den", "Tuch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Eh' als man kann einernten falsche Saaten.", "tokens": ["Eh'", "als", "man", "kann", "ei\u00b7nern\u00b7ten", "fal\u00b7sche", "Saa\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PIS", "VMFIN", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Mein Blumwerk hegt sogar, wie Trauben, Wein und Most,", "tokens": ["Mein", "Blum\u00b7werk", "hegt", "so\u00b7gar", ",", "wie", "Trau\u00b7ben", ",", "Wein", "und", "Most", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$,", "PWAV", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Dient Menschen zur Arznei und Bienen zu der Kost.", "tokens": ["Dient", "Men\u00b7schen", "zur", "Arz\u00b7nei", "und", "Bie\u00b7nen", "zu", "der", "Kost", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPRART", "NN", "KON", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--++-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Ja, meiner Blumen Purpur giebt", "tokens": ["Ja", ",", "mei\u00b7ner", "Blu\u00b7men", "Pur\u00b7pur", "giebt"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Lieb' ein Wohnhaus ab, der Wollust eine Wiege;", "tokens": ["Der", "Lieb'", "ein", "Wohn\u00b7haus", "ab", ",", "der", "Wol\u00b7lust", "ei\u00b7ne", "Wie\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKVZ", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jedweder Stengel ist ein Merkmal ihrer Siege;", "tokens": ["Jed\u00b7we\u00b7der", "Sten\u00b7gel", "ist", "ein", "Merk\u00b7mal", "ih\u00b7rer", "Sie\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Denn alle Blumen sind verliebt,", "tokens": ["Denn", "al\u00b7le", "Blu\u00b7men", "sind", "ver\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr Wohlgeruch ist ihrer Seele Sehnen,", "tokens": ["Ihr", "Wohl\u00b7ge\u00b7ruch", "ist", "ih\u00b7rer", "See\u00b7le", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Farb' ihr Brand, der Thau die Liebesthr\u00e4nen.", "tokens": ["Die", "Fa\u00b7rb'", "ihr", "Brand", ",", "der", "Thau", "die", "Lie\u00b7best\u00b7hr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}}}}