{"textgrid.poem.48441": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "12. Noch ein Anders", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist noch eben Zeit. Wir h\u00e4tten bald vers\u00e4umet", "tokens": ["Es", "ist", "noch", "e\u00b7ben", "Zeit", ".", "Wir", "h\u00e4t\u00b7ten", "bald", "ver\u00b7s\u00e4u\u00b7met"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "NN", "$.", "PPER", "VAFIN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "den lieben sch\u00f6nen Tag, weil uns zu s\u00fc\u00dfe tr\u00e4umet,", "tokens": ["den", "lie\u00b7ben", "sch\u00f6\u00b7nen", "Tag", ",", "weil", "uns", "zu", "s\u00fc\u00b7\u00dfe", "tr\u00e4u\u00b7met", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,", "KOUS", "PPER", "APPR", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und wir was spat erwacht; es ist noch eben Zeit.", "tokens": ["und", "wir", "was", "spat", "er\u00b7wacht", ";", "es", "ist", "noch", "e\u00b7ben", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PIS", "VVFIN", "VVPP", "$.", "PPER", "VAFIN", "ADV", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gebt euch, geliebter Freund, gefangen, wie ihr seid!", "tokens": ["Gebt", "euch", ",", "ge\u00b7lieb\u00b7ter", "Freund", ",", "ge\u00b7fan\u00b7gen", ",", "wie", "ihr", "seid", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "ADJA", "NN", "$,", "VVPP", "$,", "PWAV", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das Binden steht bei uns, gleichwie bei euch das L\u00f6sen.", "tokens": ["Das", "Bin\u00b7den", "steht", "bei", "uns", ",", "gleich\u00b7wie", "bei", "euch", "das", "L\u00f6\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "$,", "KON", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Und gl\u00e4ubt: di\u00df, was wir tun, geschicht aus keinem B\u00f6sen.", "tokens": ["Und", "gl\u00e4ubt", ":", "di\u00df", ",", "was", "wir", "tun", ",", "ge\u00b7schicht", "aus", "kei\u00b7nem", "B\u00f6\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PDS", "$,", "PRELS", "PPER", "VVINF", "$,", "VVPP", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Viel kosten darf es nicht, nur da\u00df man sagt noch heut':", "tokens": ["Viel", "kos\u00b7ten", "darf", "es", "nicht", ",", "nur", "da\u00df", "man", "sagt", "noch", "heut'", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "PPER", "PTKNEG", "$,", "ADV", "KOUS", "PIS", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "ihr habt euch so gel\u00f6st, wie ihr gebunden seid.", "tokens": ["ihr", "habt", "euch", "so", "ge\u00b7l\u00f6st", ",", "wie", "ihr", "ge\u00b7bun\u00b7den", "seid", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,", "PWAV", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}