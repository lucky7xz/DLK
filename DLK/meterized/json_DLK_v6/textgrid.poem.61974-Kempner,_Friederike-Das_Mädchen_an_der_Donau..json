{"textgrid.poem.61974": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "Das M\u00e4dchen an der Donau.", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frischer strahlt im Morgenglanze", "tokens": ["Fri\u00b7scher", "strahlt", "im", "Mor\u00b7gen\u00b7glan\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Uns're junge Erde noch,", "tokens": ["Un\u00b7s'\u00b7re", "jun\u00b7ge", "Er\u00b7de", "noch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und das M\u00e4dchen pfl\u00fcckt zum Kranze,", "tokens": ["Und", "das", "M\u00e4d\u00b7chen", "pfl\u00fcckt", "zum", "Kran\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Klettert auf der Berge Hoch.", "tokens": ["Klet\u00b7tert", "auf", "der", "Ber\u00b7ge", "Hoch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sch\u00f6n ist's auf der Berge R\u00fccken,", "tokens": ["Sch\u00f6n", "ist's", "auf", "der", "Ber\u00b7ge", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6n im schatt'gen Talesgrund,", "tokens": ["Sch\u00f6n", "im", "schatt'\u00b7gen", "Ta\u00b7les\u00b7grund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es l\u00e4chelt voll Entz\u00fccken", "tokens": ["Und", "es", "l\u00e4\u00b7chelt", "voll", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Still des M\u00e4dchens kleiner Mund.", "tokens": ["Still", "des", "M\u00e4d\u00b7chens", "klei\u00b7ner", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Auf der H\u00f6he steh'n noch Reben,", "tokens": ["Auf", "der", "H\u00f6\u00b7he", "steh'n", "noch", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von der Trauben Zahl geb\u00fcckt,", "tokens": ["Von", "der", "Trau\u00b7ben", "Zahl", "ge\u00b7b\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein K\u00f6rbchen dicht daneben,", "tokens": ["Und", "ein", "K\u00f6rb\u00b7chen", "dicht", "da\u00b7ne\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "PAV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dem das M\u00e4dchen n\u00e4her r\u00fcckt.", "tokens": ["Dem", "das", "M\u00e4d\u00b7chen", "n\u00e4\u00b7her", "r\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Schnell sie's fa\u00dft, und stecket denkend", "tokens": ["Schnell", "sie's", "fa\u00dft", ",", "und", "ste\u00b7cket", "den\u00b7kend"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "PIS", "VVFIN", "$,", "KON", "VVFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von den Beeren in den Mund,", "tokens": ["Von", "den", "Bee\u00b7ren", "in", "den", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und das kleine K\u00f6pfchen senkend,", "tokens": ["Und", "das", "klei\u00b7ne", "K\u00f6pf\u00b7chen", "sen\u00b7kend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Blickt sie abw\u00e4rts in den Grund.", "tokens": ["Blickt", "sie", "ab\u00b7w\u00e4rts", "in", "den", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Bricht noch saft'ge Trauben viele,", "tokens": ["Bricht", "noch", "saft'\u00b7ge", "Trau\u00b7ben", "vie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJA", "NN", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Voll gepfl\u00fcckt schon's K\u00f6rbchen steht,", "tokens": ["Voll", "ge\u00b7pfl\u00fcckt", "schon's", "K\u00f6rb\u00b7chen", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch sie ist noch nicht am Ziele,", "tokens": ["Doch", "sie", "ist", "noch", "nicht", "am", "Zie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "PTKNEG", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Still und rasch sie weiter geht.", "tokens": ["Still", "und", "rasch", "sie", "wei\u00b7ter", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Zu dem Strome, der hinunter", "tokens": ["Zu", "dem", "Stro\u00b7me", ",", "der", "hin\u00b7un\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In die weite Eb'ne eilt,", "tokens": ["In", "die", "wei\u00b7te", "Eb'\u00b7ne", "eilt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unser M\u00e4dchen, rasch und munter,", "tokens": ["Un\u00b7ser", "M\u00e4d\u00b7chen", ",", "rasch", "und", "mun\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gern beim wilden Strom verweilt.", "tokens": ["Gern", "beim", "wil\u00b7den", "Strom", "ver\u00b7weilt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Heller strahlen ihre Blicke,", "tokens": ["Hel\u00b7ler", "strah\u00b7len", "ih\u00b7re", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fr\u00f6hlich f\u00e4rbt die Wange sich,", "tokens": ["Fr\u00f6h\u00b7lich", "f\u00e4rbt", "die", "Wan\u00b7ge", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PRF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und auf ein'ge Augenblicke", "tokens": ["Und", "auf", "ein'\u00b7ge", "Au\u00b7gen\u00b7bli\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Setzt das wilde M\u00e4dchen sich.", "tokens": ["Setzt", "das", "wil\u00b7de", "M\u00e4d\u00b7chen", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Stiert hinunter in die Welle,", "tokens": ["Stiert", "hin\u00b7un\u00b7ter", "in", "die", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stiert hinunter in die Flut,", "tokens": ["Stiert", "hin\u00b7un\u00b7ter", "in", "die", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In den Augen spiegelhelle,", "tokens": ["In", "den", "Au\u00b7gen", "spie\u00b7gel\u00b7hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eine sch\u00f6ne Tr\u00e4ne ruht.", "tokens": ["Ei\u00b7ne", "sch\u00f6\u00b7ne", "Tr\u00e4\u00b7ne", "ruht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}