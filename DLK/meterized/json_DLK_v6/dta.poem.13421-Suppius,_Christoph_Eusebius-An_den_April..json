{"dta.poem.13421": {"metadata": {"author": {"name": "Suppius, Christoph Eusebius", "birth": "N.A.", "death": "N.A."}, "title": "An den April.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1749", "urn": "urn:nbn:de:kobv:b4-20594-7", "language": ["de:0.99"], "booktitle": "Suppius, Christoph Eusebius: Oden und Lieder. Gotha, 1749."}, "poem": {"stanza.1": {"line.1": {"text": "April! du Blumen-Wiederbringer!", "tokens": ["Ap\u00b7ril", "!", "du", "Blu\u00b7men\u00b7Wie\u00b7der\u00b7brin\u00b7ger", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du Stifter unsrer Fr\u00f6hlichkeit!", "tokens": ["Du", "Stif\u00b7ter", "uns\u00b7rer", "Fr\u00f6h\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie gern w\u00e4r ich dein Lob-Besinger,", "tokens": ["Wie", "gern", "w\u00e4r", "ich", "dein", "Lob\u00b7Be\u00b7sin\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gern h\u00e4tt ich dir ein Lied geweiht;", "tokens": ["Gern", "h\u00e4tt", "ich", "dir", "ein", "Lied", "ge\u00b7weiht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Jedoch dein wandelbar Gesichte", "tokens": ["Je\u00b7doch", "dein", "wan\u00b7del\u00b7bar", "Ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ist diesesmahl so runzelnvoll,", "tokens": ["Ist", "die\u00b7ses\u00b7mahl", "so", "run\u00b7zeln\u00b7voll", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df dich mein feuriges Gedichte", "tokens": ["Da\u00df", "dich", "mein", "feu\u00b7ri\u00b7ges", "Ge\u00b7dich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Jm Ernst nicht mehr erhitzen soll.", "tokens": ["Jm", "Ernst", "nicht", "mehr", "er\u00b7hit\u00b7zen", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKNEG", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie? haben wir f\u00fcr dein Geschenke", "tokens": ["Wie", "?", "ha\u00b7ben", "wir", "f\u00fcr", "dein", "Ge\u00b7schen\u00b7ke"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$.", "VAFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dir nicht vielleicht genug gedankt?", "tokens": ["Dir", "nicht", "viel\u00b7leicht", "ge\u00b7nug", "ge\u00b7dankt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da doch, so viel ich r\u00fcckw\u00e4rts denke,", "tokens": ["Da", "doch", ",", "so", "viel", "ich", "r\u00fcck\u00b7w\u00e4rts", "den\u00b7ke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADV", "ADV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Volkes Herze nie gewankt;", "tokens": ["Des", "Vol\u00b7kes", "Her\u00b7ze", "nie", "ge\u00b7wankt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Herze, das bey ieden Schl\u00e4gen", "tokens": ["Ein", "Her\u00b7ze", ",", "das", "bey", "ie\u00b7den", "Schl\u00e4\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "VVFIN", "$,", "PRELS", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Sinnlichkeit entz\u00fcckend lehrt;", "tokens": ["Der", "Sinn\u00b7lich\u00b7keit", "ent\u00b7z\u00fc\u00b7ckend", "lehrt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Was f\u00fcr ein ungemeiner Segen", "tokens": ["Was", "f\u00fcr", "ein", "un\u00b7ge\u00b7mei\u00b7ner", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mit dir best\u00e4ndig wiederkehrt.", "tokens": ["Mit", "dir", "be\u00b7st\u00e4n\u00b7dig", "wie\u00b7der\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O! Hoffnungsvoller Sohn vom Lenzen,", "tokens": ["O", "!", "Hoff\u00b7nungs\u00b7vol\u00b7ler", "Sohn", "vom", "Len\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verehrungsw\u00fcrdiger April!", "tokens": ["Ver\u00b7eh\u00b7rungs\u00b7w\u00fcr\u00b7di\u00b7ger", "Ap\u00b7ril", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00df doch dein freundlich Antlitz gl\u00e4nzen!", "tokens": ["La\u00df", "doch", "dein", "freund\u00b7lich", "Ant\u00b7litz", "gl\u00e4n\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PPOSAT", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du siehest ja, wer opfern will,", "tokens": ["Du", "sie\u00b7hest", "ja", ",", "wer", "op\u00b7fern", "will", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da stehet Altenburg und Gothe", "tokens": ["Da", "ste\u00b7het", "Al\u00b7ten\u00b7burg", "und", "Go\u00b7the"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In bunter Feyerkleider-Tracht!", "tokens": ["In", "bun\u00b7ter", "Feyer\u00b7klei\u00b7der\u00b7Tracht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Und jede winkt dem Abendrothe,", "tokens": ["Und", "je\u00b7de", "winkt", "dem", "A\u00b7ben\u00b7dro\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Damit es sie noch sch\u00f6ner macht.", "tokens": ["Da\u00b7mit", "es", "sie", "noch", "sch\u00f6\u00b7ner", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "In ihres Herzogs Gnadenblicken", "tokens": ["In", "ih\u00b7res", "Her\u00b7zogs", "Gna\u00b7den\u00b7bli\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da lacht Zufriedenheit f\u00fcr sie,", "tokens": ["Da", "lacht", "Zu\u00b7frie\u00b7den\u00b7heit", "f\u00fcr", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vor diesem wollen sie sich b\u00fccken,", "tokens": ["Vor", "die\u00b7sem", "wol\u00b7len", "sie", "sich", "b\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VMFIN", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vor ihm nur beuget sich ihr Knie;", "tokens": ["Vor", "ihm", "nur", "beu\u00b7get", "sich", "ihr", "Knie", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VVFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie! sollte denn wohl dein Get\u00f6se", "tokens": ["Wie", "!", "soll\u00b7te", "denn", "wohl", "dein", "Ge\u00b7t\u00f6\u00b7se"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$.", "VMFIN", "ADV", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So wahrer Ehrfurcht Einhalt thun?", "tokens": ["So", "wah\u00b7rer", "Ehr\u00b7furcht", "Ein\u00b7halt", "thun", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Drum sey doch heute nur nicht b\u00f6se,", "tokens": ["Drum", "sey", "doch", "heu\u00b7te", "nur", "nicht", "b\u00f6\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ADV", "ADV", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "La\u00df die verstockten Winde ruhn.", "tokens": ["La\u00df", "die", "ver\u00b7stock\u00b7ten", "Win\u00b7de", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Sprich: Eolus! du Gott der Winde", "tokens": ["Sprich", ":", "E\u00b7o\u00b7lus", "!", "du", "Gott", "der", "Win\u00b7de"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "$.", "NE", "$.", "PPER", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Aus der Poeten bester Welt!", "tokens": ["Aus", "der", "Po\u00b7et\u00b7en", "bes\u00b7ter", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dieweil ich heut vor gut befinde,", "tokens": ["Die\u00b7weil", "ich", "heut", "vor", "gut", "be\u00b7fin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da man ein Lustfest angestellt,", "tokens": ["Da", "man", "ein", "Lust\u00b7fest", "an\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df der dir untergebne Osten", "tokens": ["Da\u00df", "der", "dir", "un\u00b7ter\u00b7geb\u00b7ne", "Os\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sich die\u00dfmahl ruhig halten mag,", "tokens": ["Sich", "die\u00df\u00b7mahl", "ru\u00b7hig", "hal\u00b7ten", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So schlage du an seine Pfosten,", "tokens": ["So", "schla\u00b7ge", "du", "an", "sei\u00b7ne", "Pfos\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und gieb ihm einen Feyertag.", "tokens": ["Und", "gieb", "ihm", "ei\u00b7nen", "Fe\u00b7yer\u00b7tag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ist er nicht mehr zur\u00fcck zu bringen,", "tokens": ["Ist", "er", "nicht", "mehr", "zu\u00b7r\u00fcck", "zu", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und allbereits zu weit entfernt,", "tokens": ["Und", "all\u00b7be\u00b7reits", "zu", "weit", "ent\u00b7fernt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKA", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So pfeif! befiehl! du mu\u00dft ihn zwingen,", "tokens": ["So", "pfeif", "!", "be\u00b7fiehl", "!", "du", "mu\u00dft", "ihn", "zwin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$.", "ADJD", "$.", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df er einmahl gehorchen lernt.", "tokens": ["Da\u00df", "er", "ein\u00b7mahl", "ge\u00b7hor\u00b7chen", "lernt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Beherrscher so begl\u00fcckter Tage!", "tokens": ["Be\u00b7herr\u00b7scher", "so", "be\u00b7gl\u00fcck\u00b7ter", "Ta\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "April! sey diesesmahl geneigt,", "tokens": ["Ap\u00b7ril", "!", "sey", "die\u00b7ses\u00b7mahl", "ge\u00b7neigt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df keine mi\u00dfvergn\u00fcgte Klage", "tokens": ["Da\u00df", "kei\u00b7ne", "mi\u00df\u00b7ver\u00b7gn\u00fcg\u00b7te", "Kla\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sich in den frohen Herzen zeigt.", "tokens": ["Sich", "in", "den", "fro\u00b7hen", "Her\u00b7zen", "zeigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Der Inselsberg mit allen H\u00fcgeln ", "tokens": ["Der", "In\u00b7sels\u00b7berg", "mit", "al\u00b7len", "H\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PIAT", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Die als geharnischt um ihn stehn,", "tokens": ["Die", "als", "ge\u00b7har\u00b7nischt", "um", "ihn", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "VVFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "W\u00fcnscht heute selbst sich zu bespiegeln,", "tokens": ["W\u00fcnscht", "heu\u00b7te", "selbst", "sich", "zu", "be\u00b7spie\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und bey der Nacht einmahl zu sehn,", "tokens": ["Und", "bey", "der", "Nacht", "ein\u00b7mahl", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Erz\u00fcrne nicht den grossen Riesen,", "tokens": ["Er\u00b7z\u00fcr\u00b7ne", "nicht", "den", "gros\u00b7sen", "Rie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Als der vor keiner Wuth erschrickt;", "tokens": ["Als", "der", "vor", "kei\u00b7ner", "Wuth", "er\u00b7schrickt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich wei\u00df wohl eh, da\u00df er vor diesen", "tokens": ["Ich", "wei\u00df", "wohl", "eh", ",", "da\u00df", "er", "vor", "die\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "KOUS", "$,", "KOUS", "PPER", "APPR", "PDAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Schon deiner Wolken 