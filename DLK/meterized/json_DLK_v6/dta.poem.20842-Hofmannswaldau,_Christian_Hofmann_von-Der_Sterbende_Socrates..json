{"dta.poem.20842": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n Sterbende  \n  Socrates.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Der Weise l\u00e4st mit Lust das unbequeme Band/", "tokens": ["Der", "Wei\u00b7se", "l\u00e4st", "mit", "Lust", "das", "un\u00b7be\u00b7que\u00b7me", "Band", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ist fast nichts best\u00fcrtzt zu lassen Leut und Land/", "tokens": ["Und", "ist", "fast", "nichts", "be\u00b7st\u00fcrtzt", "zu", "las\u00b7sen", "Leut", "und", "Land", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PIS", "VVPP", "PTKZU", "VVINF", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Darnach der Geitzhal\u00df trachtet.", "tokens": ["Dar\u00b7nach", "der", "Geitz\u00b7hal\u00df", "trach\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Denn wer das Leben schleust mit Unmuth und Verdru\u00df/", "tokens": ["Denn", "wer", "das", "Le\u00b7ben", "schleust", "mit", "Un\u00b7muth", "und", "Ver\u00b7dru\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": 