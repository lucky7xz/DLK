{"textgrid.poem.65021": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbwas stehst du so d\u00fcster und von mir gewandt?", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwas stehst du so d\u00fcster und von mir gewandt?", "tokens": ["\u00bb", "was", "stehst", "du", "so", "d\u00fcs\u00b7ter", "und", "von", "mir", "ge\u00b7wandt", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "ADV", "ADJD", "KON", "APPR", "PPER", "VVPP", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Was seh ich verh\u00fcllend die zitternde Hand", "tokens": ["Was", "seh", "ich", "ver\u00b7h\u00fcl\u00b7lend", "die", "zit\u00b7tern\u00b7de", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "VVPP", "ART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "An's str\u00f6mende Auge dich pressen?", "tokens": ["An's", "str\u00f6\u00b7men\u00b7de", "Au\u00b7ge", "dich", "pres\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "O la\u00df uns, Geliebte! den peinlichen Streit,", "tokens": ["O", "la\u00df", "uns", ",", "Ge\u00b7lieb\u00b7te", "!", "den", "pein\u00b7li\u00b7chen", "Streit", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "NN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Der unsre Gem\u00fcther f\u00fcr Stunden entzweit,", "tokens": ["Der", "uns\u00b7re", "Ge\u00b7m\u00fc\u00b7ther", "f\u00fcr", "Stun\u00b7den", "ent\u00b7zweit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "In s\u00fc\u00dfer Vers\u00f6hnung vergessen!\u00ab", "tokens": ["In", "s\u00fc\u00b7\u00dfer", "Ver\u00b7s\u00f6h\u00b7nung", "ver\u00b7ges\u00b7sen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Und hab ich verletzt dich mit th\u00f6richtem Wort,", "tokens": ["Und", "hab", "ich", "ver\u00b7letzt", "dich", "mit", "th\u00f6\u00b7rich\u00b7tem", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "So m\u00f6gen die eilenden Winde es fort", "tokens": ["So", "m\u00f6\u00b7gen", "die", "ei\u00b7len\u00b7den", "Win\u00b7de", "es", "fort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "PPER", "PTKVZ"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Wie Nebel des Morgens verjagen!", "tokens": ["Wie", "Ne\u00b7bel", "des", "Mor\u00b7gens", "ver\u00b7ja\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "ADV", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Oft kr\u00e4nket die Liebe so tief wie der Ha\u00df \u2013", "tokens": ["Oft", "kr\u00e4n\u00b7ket", "die", "Lie\u00b7be", "so", "tief", "wie", "der", "Ha\u00df", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADJD", "KOKOM", "ART", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Was irrend an dir sie verbrochen, o la\u00df'", "tokens": ["Was", "ir\u00b7rend", "an", "dir", "sie", "ver\u00b7bro\u00b7chen", ",", "o", "la\u00df'"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "ADJD", "APPR", "PPER", "PPER", "VVPP", "$,", "FM", "FM"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Nicht Wurzeln im Herzen es schlagen!\u00ab", "tokens": ["Nicht", "Wur\u00b7zeln", "im", "Her\u00b7zen", "es", "schla\u00b7gen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "NN", "APPRART", "NN", "PPER", "VVINF", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Wohl mag's der Liebe auch begegnen", "tokens": ["Wohl", "mag's", "der", "Lie\u00b7be", "auch", "be\u00b7geg\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NE", "ART", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df Kr\u00e4nze sie von Dornen flicht,", "tokens": ["Da\u00df", "Kr\u00e4n\u00b7ze", "sie", "von", "Dor\u00b7nen", "flicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch selbst ihr Z\u00fcrnen ist ein Segnen:", "tokens": ["Doch", "selbst", "ihr", "Z\u00fcr\u00b7nen", "ist", "ein", "Seg\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie t\u00f6dtet, doch erniedrigt nicht.", "tokens": ["Sie", "t\u00f6d\u00b7tet", ",", "doch", "er\u00b7nied\u00b7rigt", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr Dolch macht breite Wunden klaffen,", "tokens": ["Ihr", "Dolch", "macht", "brei\u00b7te", "Wun\u00b7den", "klaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn er sich in die Seele taucht,", "tokens": ["Wenn", "er", "sich", "in", "die", "See\u00b7le", "taucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch stolz verschm\u00e4ht sie solche Waffen", "tokens": ["Doch", "stolz", "ver\u00b7schm\u00e4ht", "sie", "sol\u00b7che", "Waf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wie du sie gegen mich gebraucht.", "tokens": ["Wie", "du", "sie", "ge\u00b7gen", "mich", "ge\u00b7braucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "In ihres Zornes wildem Grauen", "tokens": ["In", "ih\u00b7res", "Zor\u00b7nes", "wil\u00b7dem", "Grau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist sie ein Blitz, der z\u00fcndend trifft,", "tokens": ["Ist", "sie", "ein", "Blitz", ",", "der", "z\u00fcn\u00b7dend", "trifft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch saugt sie nicht aus dem Vertrauen,", "tokens": ["Doch", "saugt", "sie", "nicht", "aus", "dem", "Ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das ihr geworden, heimlich Gift!", "tokens": ["Das", "ihr", "ge\u00b7wor\u00b7den", ",", "heim\u00b7lich", "Gift", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "PPER", "VAPP", "$,", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie dr\u00e4ngt sich nicht in eine Seele,", "tokens": ["Sie", "dr\u00e4ngt", "sich", "nicht", "in", "ei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein falscher, lauernder Spion,", "tokens": ["Ein", "fal\u00b7scher", ",", "lau\u00b7ern\u00b7der", "Spi\u00b7on", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Ins Antlitz ihr beweinte Fehle", "tokens": ["Ins", "Ant\u00b7litz", "ihr", "be\u00b7wein\u00b7te", "Feh\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zu schleudern einst mit frechem Hohn. \u2013", "tokens": ["Zu", "schleu\u00b7dern", "einst", "mit", "fre\u00b7chem", "Hohn", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKZU", "VVINF", "ADV", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ein See mit sanftbewegten Wogen", "tokens": ["Ein", "See", "mit", "sanft\u00b7be\u00b7weg\u00b7ten", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schien mir dein tr\u00fcgerisch Gem\u00fcth,", "tokens": ["Schien", "mir", "dein", "tr\u00fc\u00b7ge\u00b7risch", "Ge\u00b7m\u00fcth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Licht \u00fcberw\u00f6lbt vom Himmelsbogen,", "tokens": ["Licht", "\u00fc\u00b7berw\u00b7\u00f6lbt", "vom", "Him\u00b7mels\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPRART", "NN", "$,"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von duft'gen Ranken \u00fcberbl\u00fcht;", "tokens": ["Von", "duft'\u00b7gen", "Ran\u00b7ken", "\u00fc\u00b7berb\u00b7l\u00fcht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Allein die ersten St\u00fcrme riefen", "tokens": ["Al\u00b7lein", "die", "ers\u00b7ten", "St\u00fcr\u00b7me", "rie\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Empor an den wahrhaft'gen Tag", "tokens": ["Em\u00b7por", "an", "den", "wahr\u00b7haft'\u00b7gen", "Tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Was, lang bedeckt, in seinen Tiefen", "tokens": ["Was", ",", "lang", "be\u00b7deckt", ",", "in", "sei\u00b7nen", "Tie\u00b7fen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "$,", "ADJD", "VVPP", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "An ungeahnten Gr\u00e4ueln lag.", "tokens": ["An", "un\u00b7ge\u00b7ahn\u00b7ten", "Gr\u00e4u\u00b7eln", "lag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Zwar hat des Sturmes Nachtgefieder", "tokens": ["Zwar", "hat", "des", "Stur\u00b7mes", "Nacht\u00b7ge\u00b7fie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zur Ruhe sich nunmehr gelegt,", "tokens": ["Zur", "Ru\u00b7he", "sich", "nun\u00b7mehr", "ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mich aber t\u00e4uscht der See nicht wieder \u2013", "tokens": ["Mich", "a\u00b7ber", "t\u00e4uscht", "der", "See", "nicht", "wie\u00b7der", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN", "PTKNEG", "ADV", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich wei\u00df, was seine Tiefe hegt!", "tokens": ["Ich", "wei\u00df", ",", "was", "sei\u00b7ne", "Tie\u00b7fe", "hegt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Entfremdet bist du meinem Herzen,", "tokens": ["Ent\u00b7frem\u00b7det", "bist", "du", "mei\u00b7nem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zerrissen jedes Liebesband!", "tokens": ["Zer\u00b7ris\u00b7sen", "je\u00b7des", "Lie\u00b7bes\u00b7band", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie m\u00f6chte mit der Natter scherzen,", "tokens": ["Wie", "m\u00f6ch\u00b7te", "mit", "der", "Nat\u00b7ter", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wer ihres Stiches Qual empfand!", "tokens": ["Wer", "ih\u00b7res", "Sti\u00b7ches", "Qual", "emp\u00b7fand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}