{"textgrid.poem.34928": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Ungl\u00e4ubige", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du wirst in meinen Armen ruhn!", "tokens": ["Du", "wirst", "in", "mei\u00b7nen", "Ar\u00b7men", "ruhn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Wonnen sonder Schranken", "tokens": ["Von", "Won\u00b7nen", "son\u00b7der", "Schran\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Erbebt und schwillt mein ganzes Herz", "tokens": ["Er\u00b7bebt", "und", "schwillt", "mein", "gan\u00b7zes", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bei diesem Zaubergedanken.", "tokens": ["Bei", "die\u00b7sem", "Zau\u00b7ber\u00b7ge\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Du wirst in meinen Armen ruhn!", "tokens": ["Du", "wirst", "in", "mei\u00b7nen", "Ar\u00b7men", "ruhn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich spiele mit den sch\u00f6nen", "tokens": ["Ich", "spie\u00b7le", "mit", "den", "sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Goldlocken! Dein holdes K\u00f6pfchen wird", "tokens": ["Gold\u00b7lo\u00b7cken", "!", "Dein", "hol\u00b7des", "K\u00f6pf\u00b7chen", "wird"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "PPOSAT", "ADJA", "NN", "VAFIN"], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.4": {"text": "An meine Schulter lehnen.", "tokens": ["An", "mei\u00b7ne", "Schul\u00b7ter", "leh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Du wirst in meinen Armen ruhn!", "tokens": ["Du", "wirst", "in", "mei\u00b7nen", "Ar\u00b7men", "ruhn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Traum will Wahrheit werden,", "tokens": ["Der", "Traum", "will", "Wahr\u00b7heit", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "NN", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich soll des Himmels h\u00f6chste Lust", "tokens": ["Ich", "soll", "des", "Him\u00b7mels", "h\u00f6chs\u00b7te", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hier schon genie\u00dfen auf Erden.", "tokens": ["Hier", "schon", "ge\u00b7nie\u00b7\u00dfen", "auf", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "APPR", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Oh, heil'ger Thomas! Ich glaub es kaum!", "tokens": ["Oh", ",", "heil'\u00b7ger", "Tho\u00b7mas", "!", "Ich", "glaub", "es", "kaum", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJA", "NE", "$.", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich zweifle bis zur Stunde,", "tokens": ["Ich", "zweif\u00b7le", "bis", "zur", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo ich den Finger legen kann", "tokens": ["Wo", "ich", "den", "Fin\u00b7ger", "le\u00b7gen", "kann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN", "VVINF", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In meines Gl\u00fcckes Wunde.", "tokens": ["In", "mei\u00b7nes", "Gl\u00fc\u00b7ckes", "Wun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}