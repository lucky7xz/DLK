{"textgrid.poem.44490": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Literarische Zust\u00e4nde", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der \u00c4rger ist ein schlimmer Gast,", "tokens": ["Der", "\u00c4r\u00b7ger", "ist", "ein", "schlim\u00b7mer", "Gast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch n\u00fctzt er ab und zu;", "tokens": ["Doch", "n\u00fctzt", "er", "ab", "und", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zum mindsten l\u00e4\u00dft er keine Rast,", "tokens": ["Zum", "minds\u00b7ten", "l\u00e4\u00dft", "er", "kei\u00b7ne", "Rast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er straft in Versen, was er ha\u00dft,", "tokens": ["Er", "straft", "in", "Ver\u00b7sen", ",", "was", "er", "ha\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ist t\u00e4tig ohne Ruh.", "tokens": ["Ist", "t\u00e4\u00b7tig", "oh\u00b7ne", "Ruh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Doch w\u00e4hrt der Streit nun allzulang,", "tokens": ["Doch", "w\u00e4hrt", "der", "Streit", "nun", "all\u00b7zu\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Stellt sich der Mi\u00dfmut ein.", "tokens": ["Stellt", "sich", "der", "Mi\u00df\u00b7mut", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der zieht zur\u00fcck sich aus dem Drang,", "tokens": ["Der", "zieht", "zu\u00b7r\u00fcck", "sich", "aus", "dem", "Drang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKVZ", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zuckt nur, wo sonst ein Schwerthieb klang,", "tokens": ["Zuckt", "nur", ",", "wo", "sonst", "ein", "Schwert\u00b7hieb", "klang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PWAV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und sieht unwillig drein.", "tokens": ["Und", "sieht", "un\u00b7wil\u00b7lig", "drein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Den Reihen schlie\u00dft ein Jammersohn,", "tokens": ["Den", "Rei\u00b7hen", "schlie\u00dft", "ein", "Jam\u00b7mer\u00b7sohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Unheilsb\u00fcchse Deckel;", "tokens": ["Der", "Un\u00b7heils\u00b7b\u00fcch\u00b7se", "De\u00b7ckel", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er hei\u00dft \u2013 man spricht nicht gern davon,", "tokens": ["Er", "hei\u00dft", "\u2013", "man", "spricht", "nicht", "gern", "da\u00b7von", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PIS", "VVFIN", "PTKNEG", "ADV", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Wort hat einen herben Ton,", "tokens": ["Das", "Wort", "hat", "ei\u00b7nen", "her\u00b7ben", "Ton", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es reimt auf Reckel, S\u00e4ckel,", "tokens": ["Es", "reimt", "auf", "Re\u00b7ckel", ",", "S\u00e4\u00b7ckel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und hei\u00dft \u2013 je nun! \u2013 der Eckel.", "tokens": ["Und", "hei\u00dft", "\u2013", "je", "nun", "!", "\u2013", "der", "E\u00b7ckel", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "ADV", "ADV", "$.", "$(", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}