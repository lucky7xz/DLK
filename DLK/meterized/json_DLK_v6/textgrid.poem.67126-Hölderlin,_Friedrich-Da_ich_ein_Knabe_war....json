{"textgrid.poem.67126": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Da ich ein Knabe war...", "genre": "verse", "period": "N.A.", "pub_year": 1798, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da ich ein Knabe war,", "tokens": ["Da", "ich", "ein", "Kna\u00b7be", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Rettet' ein Gott mich oft", "tokens": ["Ret\u00b7tet'", "ein", "Gott", "mich", "oft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vom Geschrei und der Rute der Menschen,", "tokens": ["Vom", "Ge\u00b7schrei", "und", "der", "Ru\u00b7te", "der", "Men\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Da spielt ich sicher und gut", "tokens": ["Da", "spielt", "ich", "si\u00b7cher", "und", "gut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Mit den Blumen des Hains,", "tokens": ["Mit", "den", "Blu\u00b7men", "des", "Hains", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Und die L\u00fcftchen des Himmels", "tokens": ["Und", "die", "L\u00fcft\u00b7chen", "des", "Him\u00b7mels"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Spielten mit mir.", "tokens": ["Spiel\u00b7ten", "mit", "mir", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Und wie du das Herz", "tokens": ["Und", "wie", "du", "das", "Herz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Der Pflanzen erfreust,", "tokens": ["Der", "Pflan\u00b7zen", "er\u00b7freust", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Wenn sie entgegen dir", "tokens": ["Wenn", "sie", "ent\u00b7ge\u00b7gen", "dir"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die zarten Arme strecken,", "tokens": ["Die", "zar\u00b7ten", "Ar\u00b7me", "stre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "So hast du mein Herz erfreut,", "tokens": ["So", "hast", "du", "mein", "Herz", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Vater Helios! und, wie Endymion,", "tokens": ["Va\u00b7ter", "He\u00b7li\u00b7os", "!", "und", ",", "wie", "En\u00b7dy\u00b7mi\u00b7on", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "KON", "$,", "PWAV", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "War ich dein Liebling,", "tokens": ["War", "ich", "dein", "Lieb\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Heilige Luna!", "tokens": ["Hei\u00b7li\u00b7ge", "Lu\u00b7na", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "O all ihr treuen", "tokens": ["O", "all", "ihr", "treu\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "PIAT", "PPOSAT", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Freundlichen G\u00f6tter!", "tokens": ["Freund\u00b7li\u00b7chen", "G\u00f6t\u00b7ter", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Da\u00df ihr w\u00fc\u00dftet,", "tokens": ["Da\u00df", "ihr", "w\u00fc\u00df\u00b7tet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Wie euch meine Seele geliebt!", "tokens": ["Wie", "euch", "mei\u00b7ne", "See\u00b7le", "ge\u00b7liebt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "Zwar damals rief ich noch nicht", "tokens": ["Zwar", "da\u00b7mals", "rief", "ich", "noch", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV", "PTKNEG"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Euch mit Namen, auch ihr", "tokens": ["Euch", "mit", "Na\u00b7men", ",", "auch", "ihr"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "APPR", "NN", "$,", "ADV", "PPOSAT"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Nanntet mich nie, wie die Menschen sich nennen,", "tokens": ["Nann\u00b7tet", "mich", "nie", ",", "wie", "die", "Men\u00b7schen", "sich", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PWAV", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "+--+--+----", "measure": "dactylic.tri.plus"}, "line.4": {"text": "Als kennten sie sich.", "tokens": ["Als", "kenn\u00b7ten", "sie", "sich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "PRF", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Doch kannt ich euch besser,", "tokens": ["Doch", "kannt", "ich", "euch", "bes\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "ADJD", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Als ich je die Menschen gekannt,", "tokens": ["Als", "ich", "je", "die", "Men\u00b7schen", "ge\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Ich verstand die Stille des Aethers,", "tokens": ["Ich", "ver\u00b7stand", "die", "Stil\u00b7le", "des", "A\u00b7e\u00b7thers", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Der Menschen Worte verstand ich nie.", "tokens": ["Der", "Men\u00b7schen", "Wor\u00b7te", "ver\u00b7stand", "ich", "nie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Mich erzog der Wohllaut", "tokens": ["Mich", "er\u00b7zog", "der", "Wohl\u00b7laut"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Des s\u00e4uselnden Hains", "tokens": ["Des", "s\u00e4u\u00b7seln\u00b7den", "Hains"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und lieben lernt ich", "tokens": ["Und", "lie\u00b7ben", "lernt", "ich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVINF", "VVFIN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Unter den Blumen.", "tokens": ["Un\u00b7ter", "den", "Blu\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.8": {"line.1": {"text": "Im Arme der G\u00f6tter wuchs ich gro\u00df.", "tokens": ["Im", "Ar\u00b7me", "der", "G\u00f6t\u00b7ter", "wuchs", "ich", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}