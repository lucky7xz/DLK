{"dta.poem.15728": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "61.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Kind! eine T\u00fcchtigkeit, zu einem Zweck gewandt,", "tokens": ["Kind", "!", "ei\u00b7ne", "T\u00fcch\u00b7tig\u00b7keit", ",", "zu", "ei\u00b7nem", "Zweck", "ge\u00b7wandt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Das ists, ein Weiser lehrts, was Tugend wird genannt.", "tokens": ["Das", "ists", ",", "ein", "Wei\u00b7ser", "lehrts", ",", "was", "Tu\u00b7gend", "wird", "ge\u00b7nannt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "ART", "NN", "ADV", "$,", "PWS", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was immer t\u00fcchtig ist und taugend, das ist Tugend,", "tokens": ["Was", "im\u00b7mer", "t\u00fcch\u00b7tig", "ist", "und", "tau\u00b7gend", ",", "das", "ist", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "VAFIN", "KON", "ADJD", "$,", "PDS", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn ihm ein Zweck nicht fehlt, das pfleg' in deiner Jugend.", "tokens": ["Wenn", "ihm", "ein", "Zweck", "nicht", "fehlt", ",", "das", "pfleg'", "in", "dei\u00b7ner", "Ju\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VVFIN", "$,", "PDS", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Richtung auf h\u00f6chsten Zweck mu\u00df h\u00f6chste Tugend seyn;", "tokens": ["Rich\u00b7tung", "auf", "h\u00f6chs\u00b7ten", "Zweck", "mu\u00df", "h\u00f6chs\u00b7te", "Tu\u00b7gend", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "VMFIN", "ADJA", "NN", "VAINF", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Was ist der h\u00f6chste Zweck des Menschen? Gottverein.", "tokens": ["Was", "ist", "der", "h\u00f6chs\u00b7te", "Zweck", "des", "Men\u00b7schen", "?", "Gott\u00b7ver\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN", "ART", "NN", "$.", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}