{"textgrid.poem.53155": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: La\u00dft uns meiden", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00dft uns meiden", "tokens": ["La\u00dft", "uns", "mei\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "PPER", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Was nur leiden", "tokens": ["Was", "nur", "lei\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["PWS", "ADV", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Einem schaffen kan,", "tokens": ["Ei\u00b7nem", "schaf\u00b7fen", "kan", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VVINF", "VMFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Au\u00dferwehltste Freuden", "tokens": ["Au\u00b7\u00dfer\u00b7wehlts\u00b7te", "Freu\u00b7den"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Gebt euch bey uns an,", "tokens": ["Gebt", "euch", "bey", "uns", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Liebste Sachen,", "tokens": ["Liebs\u00b7te", "Sa\u00b7chen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Spiel und Lachen,", "tokens": ["Spiel", "und", "La\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Kompt gesampt zu hauff,", "tokens": ["Kompt", "ge\u00b7sampt", "zu", "hauff", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Steck uns Kertzen", "tokens": ["Steck", "uns", "Kert\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["NE", "PPER", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "In den Hertzen,", "tokens": ["In", "den", "Hert\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "S\u00fcsser Amor, auff!", "tokens": ["S\u00fcs\u00b7ser", "A\u00b7mor", ",", "auff", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NE", "NE", "$,", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Der mein Leben", "tokens": ["Der", "mein", "Le\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ART", "PPOSAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Sich ergeben,", "tokens": ["Sich", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["PRF", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Die mich meiner Pein", "tokens": ["Die", "mich", "mei\u00b7ner", "Pein"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Gn\u00fcglich kan entheben,", "tokens": ["Gn\u00fcg\u00b7lich", "kan", "ent\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Wird nun g\u00e4ntzlich mein.", "tokens": ["Wird", "nun", "g\u00e4ntz\u00b7lich", "mein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "PPOSAT", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Ihre Wangen,", "tokens": ["Ih\u00b7re", "Wan\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Mein Verlangen,", "tokens": ["Mein", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Ihrer Vnschuld Ruhm,", "tokens": ["Ih\u00b7rer", "Vn\u00b7schuld", "Ruhm", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Ihre Jugend,", "tokens": ["Ih\u00b7re", "Ju\u00b7gend", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Zucht und Tugend", "tokens": ["Zucht", "und", "Tu\u00b7gend"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Sind mein Eigenthum.", "tokens": ["Sind", "mein", "Ei\u00b7gen\u00b7thum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "La\u00dft mir weichen", "tokens": ["La\u00dft", "mir", "wei\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "PPER", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Alle Reichen,", "tokens": ["Al\u00b7le", "Rei\u00b7chen", ","], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Alles Gut und Geld,", "tokens": ["Al\u00b7les", "Gut", "und", "Geld", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "KON", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Nichts ist Ihr zu gleichen,", "tokens": ["Nichts", "ist", "Ihr", "zu", "glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Sie ist meine Welt.", "tokens": ["Sie", "ist", "mei\u00b7ne", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "----+", "measure": "unknown.measure.single"}, "line.6": {"text": "Gl\u00e4ntzt, jhr Sterne,", "tokens": ["Gl\u00e4ntzt", ",", "jhr", "Ster\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Sch\u00f6n von ferne,", "tokens": ["Sch\u00f6n", "von", "fer\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Die mein Hertz mir brennt,", "tokens": ["Die", "mein", "Hertz", "mir", "brennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Meine Wonne", "tokens": ["Mei\u00b7ne", "Won\u00b7ne"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Ist mir Sonne,", "tokens": ["Ist", "mir", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Mond und Firmament.", "tokens": ["Mond", "und", "Fir\u00b7ma\u00b7ment", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Seyd selbs Richter,", "tokens": ["Seyd", "selbs", "Rich\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Himmels-Liechter,", "tokens": ["Him\u00b7mels\u00b7Liech\u00b7ter", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Weil jhr auch geliebt,", "tokens": ["Weil", "jhr", "auch", "ge\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Wie die Schaar der Tichter", "tokens": ["Wie", "die", "Schaar", "der", "Tich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Von euch Nachricht giebt,", "tokens": ["Von", "euch", "Nach\u00b7richt", "giebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Sagt zusammen,", "tokens": ["Sagt", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Wolcken-Flammen,", "tokens": ["Wol\u00b7cken\u00b7Flam\u00b7men", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Ob was Liebers mir", "tokens": ["Ob", "was", "Lie\u00b7bers", "mir"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIS", "NN", "PPER"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Hie auff Erden", "tokens": ["Hie", "auff", "Er\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPR", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "K\u00f6nne werden", "tokens": ["K\u00f6n\u00b7ne", "wer\u00b7den"], "token_info": ["word", "word"], "pos": ["NN", "VAINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Weder jhre Zier?", "tokens": ["We\u00b7der", "jhre", "Zier", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Ihrentwegen", "tokens": ["Ih\u00b7rent\u00b7we\u00b7gen"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Halt ich Regen", "tokens": ["Halt", "ich", "Re\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "PPER", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Vnd Gefahr zur See", "tokens": ["Vnd", "Ge\u00b7fahr", "zur", "See"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "APPRART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Niemals mir entgegen,", "tokens": ["Nie\u00b7mals", "mir", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Liebe Frost und Schnee,", "tokens": ["Lie\u00b7be", "Frost", "und", "Schnee", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Sch\u00e4tz' erkohren", "tokens": ["Sch\u00e4tz'", "er\u00b7koh\u00b7ren"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Selbs die Mohren", "tokens": ["Selbs", "die", "Moh\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Vnd den Nilus-Strand,", "tokens": ["Vnd", "den", "Ni\u00b7lus\u00b7Strand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Geht f\u00fcr allen", "tokens": ["Geht", "f\u00fcr", "al\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "PIAT"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Mein Gefallen,", "tokens": ["Mein", "Ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Sie, mir nur zuhand.", "tokens": ["Sie", ",", "mir", "nur", "zu\u00b7hand", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Himmels-G\u00fcte,", "tokens": ["Him\u00b7mels\u00b7G\u00fc\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Halt in Bl\u00fcte", "tokens": ["Halt", "in", "Bl\u00fc\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "APPR", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Vnsrer Liebe Saat,", "tokens": ["Vns\u00b7rer", "Lie\u00b7be", "Saat", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Gr\u00fcnd uns das Gem\u00fcthe", "tokens": ["Gr\u00fcnd", "uns", "das", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Stets auff Gott und Raht!", "tokens": ["Stets", "auff", "Gott", "und", "Raht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Nur ein Wille,", "tokens": ["Nur", "ein", "Wil\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Demuth, Stille,", "tokens": ["De\u00b7muth", ",", "Stil\u00b7le", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "--+-", "measure": "anapaest.init"}, "line.8": {"text": "Kr\u00f6n' uns iederzeit,", "tokens": ["Kr\u00f6n'", "uns", "ie\u00b7der\u00b7zeit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "La\u00df uns fahren", "tokens": ["La\u00df", "uns", "fah\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "PPER", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Alt an Jahren", "tokens": ["Alt", "an", "Jah\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "APPR", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "In dein' Ewigheit!", "tokens": ["In", "dein'", "E\u00b7wig\u00b7heit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}