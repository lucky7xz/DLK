{"textgrid.poem.43768": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die G\u00e4nse mit dem G\u00e4nserich,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die G\u00e4nse mit dem G\u00e4nserich,", "tokens": ["Die", "G\u00e4n\u00b7se", "mit", "dem", "G\u00e4n\u00b7se\u00b7rich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die machen gro\u00df Geschrei:", "tokens": ["Die", "ma\u00b7chen", "gro\u00df", "Ge\u00b7schrei", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die beste Zeit f\u00fcr sie verstrich,", "tokens": ["Die", "bes\u00b7te", "Zeit", "f\u00fcr", "sie", "ver\u00b7strich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Sommer ist vorbei.", "tokens": ["Der", "Som\u00b7mer", "ist", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gigack, gigack, gigack.", "tokens": ["Gi\u00b7gack", ",", "gi\u00b7gack", ",", "gi\u00b7gack", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Tag ist kurz, lang ist die Nacht.", "tokens": ["Der", "Tag", "ist", "kurz", ",", "lang", "ist", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Weid' ist ohne Gras,", "tokens": ["Die", "Weid'", "ist", "oh\u00b7ne", "Gras", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Gang zum Born und Weiher macht", "tokens": ["Der", "Gang", "zum", "Born", "und", "Wei\u00b7her", "macht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Winter keinen Spa\u00df.", "tokens": ["Im", "Win\u00b7ter", "kei\u00b7nen", "Spa\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gigack, gigack, gigack.", "tokens": ["Gi\u00b7gack", ",", "gi\u00b7gack", ",", "gi\u00b7gack", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ihr d\u00fcrft im Schnee und auf dem Eis", "tokens": ["Ihr", "d\u00fcrft", "im", "Schnee", "und", "auf", "dem", "Eis"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht lang mehr barfu\u00df geh'n,", "tokens": ["Nicht", "lang", "mehr", "bar\u00b7fu\u00df", "geh'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "La\u00dft euer Schrei'n, es ist, wer wei\u00df", "tokens": ["La\u00dft", "eu\u00b7er", "Schrei'n", ",", "es", "ist", ",", "wer", "wei\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "$,", "PWS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gar bald um euch gescheh'n.", "tokens": ["Gar", "bald", "um", "euch", "ge\u00b7scheh'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "ADV", "APPR", "PPER", "VVFIN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gigack, gigack, gigack.", "tokens": ["Gi\u00b7gack", ",", "gi\u00b7gack", ",", "gi\u00b7gack", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Am Martinsabend wandert ihr", "tokens": ["Am", "Mar\u00b7tin\u00b7sa\u00b7bend", "wan\u00b7dert", "ihr"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gebraten auf den Tisch,", "tokens": ["Ge\u00b7bra\u00b7ten", "auf", "den", "Tisch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und nichts von euch behalten wir", "tokens": ["Und", "nichts", "von", "euch", "be\u00b7hal\u00b7ten", "wir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "APPR", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als nur den Flederwisch.", "tokens": ["Als", "nur", "den", "Fle\u00b7der\u00b7wisch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gigack, gigack, gigack.", "tokens": ["Gi\u00b7gack", ",", "gi\u00b7gack", ",", "gi\u00b7gack", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "So ist's und ist nun immerso,", "tokens": ["So", "ist's", "und", "ist", "nun", "im\u00b7mer\u00b7so", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "KON", "VAFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr uns nur lebtet ihr,", "tokens": ["F\u00fcr", "uns", "nur", "leb\u00b7tet", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir sind des G\u00e4nsebratens froh,", "tokens": ["Wir", "sind", "des", "G\u00e4n\u00b7se\u00b7bra\u00b7tens", "froh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr schweigt, doch singen wir", "tokens": ["Ihr", "schweigt", ",", "doch", "sin\u00b7gen", "wir"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gigack, gigack, gigack.", "tokens": ["Gi\u00b7gack", ",", "gi\u00b7gack", ",", "gi\u00b7gack", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}