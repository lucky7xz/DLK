{"textgrid.poem.38687": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Die Gnomen", "genre": "verse", "period": "N.A.", "pub_year": 1792, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Tagscheins Blendung dr\u00fcckt,", "tokens": ["Des", "Tag\u00b7scheins", "Blen\u00b7dung", "dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Nur Finsterni\u00df begl\u00fcckt:", "tokens": ["Nur", "Fins\u00b7ter\u00b7ni\u00df", "be\u00b7gl\u00fcckt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Drum hausen wir so gern", "tokens": ["Drum", "hau\u00b7sen", "wir", "so", "gern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Tief in des Erdballs Kern.", "tokens": ["Tief", "in", "des", "Erd\u00b7balls", "Kern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "++-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dort oben, wo der Aether flammt,", "tokens": ["Dort", "o\u00b7ben", ",", "wo", "der", "A\u00b7e\u00b7ther", "flammt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Ward alles was von Adam stammt", "tokens": ["Ward", "al\u00b7les", "was", "von", "A\u00b7dam", "stammt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PIS", "PWS", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zu Licht und Glut mit Recht verdammt.", "tokens": ["Zu", "Licht", "und", "Glut", "mit", "Recht", "ver\u00b7dammt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wir schm\u00e4hn was Menschenlob", "tokens": ["Wir", "schm\u00e4hn", "was", "Men\u00b7schen\u00b7lob"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Zum Sternenplan erhob;", "tokens": ["Zum", "Ster\u00b7nen\u00b7plan", "er\u00b7hob", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des Nordpols B\u00e4renstrand", "tokens": ["Des", "Nord\u00b7pols", "B\u00e4\u00b7ren\u00b7strand"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "D\u00fcnkt uns ein Zauberland,", "tokens": ["D\u00fcnkt", "uns", "ein", "Zau\u00b7ber\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Blumen Schmelz, die Nachtigall,", "tokens": ["Der", "Blu\u00b7men", "Schmelz", ",", "die", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nur Augengift und Ohrenquaal", "tokens": ["Nur", "Au\u00b7gen\u00b7gift", "und", "Oh\u00b7ren\u00b7qua\u00b7al"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "NN", "KON", "NN"], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}, "line.7": {"text": "Und Sieben eine grade Zahl.", "tokens": ["Und", "Sie\u00b7ben", "ei\u00b7ne", "gra\u00b7de", "Zahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Balg des Maulwurfs war", "tokens": ["Der", "Balg", "des", "Maul\u00b7wurfs", "war"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Lang' unser Prunktalar;", "tokens": ["Lang'", "un\u00b7ser", "Prunk\u00b7ta\u00b7lar", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Jetzt bl\u00e4hn wir uns beim Fest", "tokens": ["Jetzt", "bl\u00e4hn", "wir", "uns", "beim", "Fest"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Im Leibrok von Asbest,", "tokens": ["Im", "Lei\u00b7brok", "von", "As\u00b7best", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NE", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Den Pux, der muntre Nachtkumpan,", "tokens": ["Den", "Pux", ",", "der", "mun\u00b7tre", "Nacht\u00b7kum\u00b7pan", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dem Schoo\u00df der Steinkluft abgewann", "tokens": ["Dem", "Schoo\u00df", "der", "Stein\u00b7kluft", "ab\u00b7ge\u00b7wann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und Erl die Wassernixe spann.", "tokens": ["Und", "Erl", "die", "Was\u00b7ser\u00b7ni\u00b7xe", "spann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wann sich dem Gnomenstaat", "tokens": ["Wann", "sich", "dem", "Gno\u00b7mens\u00b7taat"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PRF", "ART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Die Habsucht schaufelnd naht,", "tokens": ["Die", "Hab\u00b7sucht", "schau\u00b7felnd", "naht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Am Goldget\u00e4fel pickt", "tokens": ["Am", "Gold\u00b7ge\u00b7t\u00e4\u00b7fel", "pickt"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So Dom und W\u00e4nde schm\u00fcckt:", "tokens": ["So", "Dom", "und", "W\u00e4n\u00b7de", "schm\u00fcckt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dann l\u00f6schen wir des Bergmanns Licht,", "tokens": ["Dann", "l\u00f6\u00b7schen", "wir", "des", "Berg\u00b7manns", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Spr\u00fchn Schwefeldampf ihm ins Gesicht", "tokens": ["Spr\u00fchn", "Schwe\u00b7fel\u00b7dampf", "ihm", "ins", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und kneipen braun und blau den Wicht.", "tokens": ["Und", "knei\u00b7pen", "braun", "und", "blau", "den", "Wicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "KON", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wir blinzen scharf und klar,", "tokens": ["Wir", "blin\u00b7zen", "scharf", "und", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wie Kobolt, Elf' und Mahr,", "tokens": ["Wie", "Ko\u00b7bolt", ",", "El\u00b7f'", "und", "Mahr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "$,", "NE", "KON", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Mit Augen von Smaragd", "tokens": ["Mit", "Au\u00b7gen", "von", "Sma\u00b7ragd"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Durch schwarzer Gr\u00fcfte Nacht,", "tokens": ["Durch", "schwar\u00b7zer", "Gr\u00fcf\u00b7te", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wo man des Berg\u00f6ls Nektar trinkt", "tokens": ["Wo", "man", "des", "Berg\u00b7\u00f6ls", "Nek\u00b7tar", "trinkt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und, grell mit Kupferglut geschminkt,", "tokens": ["Und", ",", "grell", "mit", "Kup\u00b7fer\u00b7glut", "ge\u00b7schminkt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJD", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Auf Erdschwammpolster niedersinkt.", "tokens": ["Auf", "Erd\u00b7schwamm\u00b7pols\u00b7ter", "nie\u00b7der\u00b7sinkt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wild saust, aus tiefem Schacht", "tokens": ["Wild", "saust", ",", "aus", "tie\u00b7fem", "Schacht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "$,", "APPR", "ADJA", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Vom hagern Greif bewacht,", "tokens": ["Vom", "ha\u00b7gern", "Greif", "be\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im Sturm der Gnomen Trupp", "tokens": ["Im", "Sturm", "der", "Gno\u00b7men", "Trupp"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Hervor zum Hexenklubb,", "tokens": ["Her\u00b7vor", "zum", "He\u00b7xen\u00b7klubb", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Inde\u00df, wie Satans Heerhorn t\u00f6nt,", "tokens": ["In\u00b7de\u00df", ",", "wie", "Sa\u00b7tans", "Heer\u00b7horn", "t\u00f6nt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des Bloksbergs Kuppe furchtbar dr\u00f6nt", "tokens": ["Des", "Bloks\u00b7bergs", "Kup\u00b7pe", "furcht\u00b7bar", "dr\u00f6nt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und sich mit Geisterschaaren kr\u00f6nt.", "tokens": ["Und", "sich", "mit", "Geis\u00b7ter\u00b7schaa\u00b7ren", "kr\u00f6nt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Uns z\u00fcgelt kein Gesetz,", "tokens": ["Uns", "z\u00fc\u00b7gelt", "kein", "Ge\u00b7setz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Plagt weder Pflug noch Netz;", "tokens": ["Plagt", "we\u00b7der", "Pflug", "noch", "Netz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Menschen Lehr' und Kunst", "tokens": ["Der", "Men\u00b7schen", "Lehr'", "und", "Kunst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Bleibt ewig Irrwischdunst!", "tokens": ["Bleibt", "e\u00b7wig", "Irr\u00b7wischdunst", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Kaum reitzt uns noch das Chorgequik", "tokens": ["Kaum", "reitzt", "uns", "noch", "das", "Chor\u00b7ge\u00b7quik"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von Belzebubs Vokalmusik.", "tokens": ["Von", "Bel\u00b7ze\u00b7bubs", "Vo\u00b7kal\u00b7mu\u00b7sik", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$."], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "So treibts die Gnomenrepublik.", "tokens": ["So", "treibts", "die", "Gno\u00b7men\u00b7re\u00b7pub\u00b7lik", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}