{"dta.poem.13154": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Neuntes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201evernehmet also nur, wie breit, wie hoch, wie lang", "tokens": ["\u201e", "ver\u00b7neh\u00b7met", "al\u00b7so", "nur", ",", "wie", "breit", ",", "wie", "hoch", ",", "wie", "lang"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "VVFIN", "ADV", "ADV", "$,", "PWAV", "ADJD", "$,", "PWAV", "ADJD", "$,", "PWAV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "270\u201dDer Bau sey; welchen Raum der Welt er in sich fang.", "tokens": ["\"", "Der", "Bau", "sey", ";", "wel\u00b7chen", "Raum", "der", "Welt", "er", "in", "sich", "fang", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "$.", "PWAT", "NN", "ART", "NN", "PPER", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201edas Erd-Rund ist zu klein den Tempel einzuschliessen;", "tokens": ["\u201e", "das", "Erd\u00b7Rund", "ist", "zu", "klein", "den", "Tem\u00b7pel", "ein\u00b7zu\u00b7schlies\u00b7sen", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "PTKA", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201eder Grund und sein Bezirck ist weiter aufgerissen,", "tokens": ["\u201e", "der", "Grund", "und", "sein", "Be\u00b7zirck", "ist", "wei\u00b7ter", "auf\u00b7ge\u00b7ris\u00b7sen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "KON", "PPOSAT", "NN", "VAFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201enicht von der K\u00fcnste Wiz und Richtschnur aufgef\u00fchrt;", "tokens": ["\u201e", "nicht", "von", "der", "K\u00fcns\u00b7te", "Wiz", "und", "Richt\u00b7schnur", "auf\u00b7ge\u00b7f\u00fchrt", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKNEG", "APPR", "ART", "ADJA", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201ejedoch von Sturz und Fall auf immer unber\u00fchrt.", "tokens": ["\u201e", "je\u00b7doch", "von", "Sturz", "und", "Fall", "auf", "im\u00b7mer", "un\u00b7be\u00b7r\u00fchrt", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "APPR", "NN", "KON", "NN", "APPR", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "275\u201dNichts aus der Erde Scho\u00df, nichts von derselben Sch\u00e4zen,", "tokens": ["\"", "Nichts", "aus", "der", "Er\u00b7de", "Scho\u00df", ",", "nichts", "von", "der\u00b7sel\u00b7ben", "Sch\u00e4\u00b7zen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "APPR", "ART", "NN", "NN", "$,", "PIS", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201enichts was den Reichen pflegt dem F\u00fcrsten beyzusezen,", "tokens": ["\u201e", "nichts", "was", "den", "Rei\u00b7chen", "pflegt", "dem", "F\u00fcrs\u00b7ten", "bey\u00b7zu\u00b7se\u00b7zen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "PWS", "ART", "NN", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201eziert diesen Ehren-Bau; kein Marmel oder Erz,", "tokens": ["\u201e", "ziert", "die\u00b7sen", "Eh\u00b7ren\u00b7Bau", ";", "kein", "Mar\u00b7mel", "o\u00b7der", "Erz", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PDAT", "NN", "$.", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201ekein Silber oder Gold, das man sonst anderw\u00e4rts", "tokens": ["\u201e", "kein", "Sil\u00b7ber", "o\u00b7der", "Gold", ",", "das", "man", "sonst", "an\u00b7der\u00b7w\u00e4rts"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PIAT", "NN", "KON", "NN", "$,", "PRELS", "PIS", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201ein solche Ruhm-Pal\u00e4st\u2019 in Berg\u2019 und B\u00f6gen h\u00e4ufet,", "tokens": ["\u201e", "in", "sol\u00b7che", "Ruhm\u00b7Pa\u00b7l\u00e4st'", "in", "Ber\u00b7g'", "und", "B\u00f6\u00b7gen", "h\u00e4u\u00b7fet", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PIAT", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "280\u201dIst des Geb\u00e4udes Schmuck, in dem die Pracht sich steiffet.", "tokens": ["\"", "Ist", "des", "Ge\u00b7b\u00e4u\u00b7des", "Schmuck", ",", "in", "dem", "die", "Pracht", "sich", "steif\u00b7fet", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ART", "NN", "NN", "$,", "APPR", "PRELS", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}