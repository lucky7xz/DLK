{"textgrid.poem.44624": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Auf eine B\u00fcste des Sokrates", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Warst du kein Mensch, dem dieser Stein geweiht,", "tokens": ["Warst", "du", "kein", "Mensch", ",", "dem", "die\u00b7ser", "Stein", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "NN", "$,", "PRELS", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "da\u00df dich nicht trifft der Gottheit Richterspruch?", "tokens": ["da\u00df", "dich", "nicht", "trifft", "der", "Got\u00b7theit", "Rich\u00b7ter\u00b7spruch", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Seid sterblich! Donnerte nicht so ihr Fluch?", "tokens": ["Seid", "sterb\u00b7lich", "!", "Don\u00b7ner\u00b7te", "nicht", "so", "ihr", "Fluch", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$.", "NN", "PTKNEG", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "und dir reicht selbst der Tod Unsterblichkeit!", "tokens": ["und", "dir", "reicht", "selbst", "der", "Tod", "U\u00b7nsterb\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "--+--++--+", "measure": "anapaest.di.plus"}}}}}