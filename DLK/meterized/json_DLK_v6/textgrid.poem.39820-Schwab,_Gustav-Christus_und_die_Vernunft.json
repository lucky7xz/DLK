{"textgrid.poem.39820": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Christus und die Vernunft", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was Tag und Nacht mein Herz bewegt,", "tokens": ["Was", "Tag", "und", "Nacht", "mein", "Herz", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Bilde sei's euch dargelegt:", "tokens": ["Im", "Bil\u00b7de", "sei's", "euch", "dar\u00b7ge\u00b7legt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Zween Freunde wohnten br\u00fcderlich", "tokens": ["Zween", "Freun\u00b7de", "wohn\u00b7ten", "br\u00fc\u00b7der\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Beisammen, liebten herzlich sich,", "tokens": ["Bei\u00b7sam\u00b7men", ",", "lieb\u00b7ten", "herz\u00b7lich", "sich", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADJD", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der ein' ein orthodoxer Christ,", "tokens": ["Der", "ein'", "ein", "or\u00b7tho\u00b7do\u00b7xer", "Christ", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der andre Rationalist.", "tokens": ["Der", "and\u00b7re", "Ra\u00b7ti\u00b7o\u00b7na\u00b7list", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er macht' es, wie die ganze Zunft:", "tokens": ["Er", "macht'", "es", ",", "wie", "die", "gan\u00b7ze", "Zunft", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was jener treu und eifrig glaubt,", "tokens": ["Was", "je\u00b7ner", "treu", "und", "eif\u00b7rig", "glaubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "H\u00e4tt' er ihm gar zu gern geraubt,", "tokens": ["H\u00e4tt'", "er", "ihm", "gar", "zu", "gern", "ge\u00b7raubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "PTKA", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Er h\u00e4lt es gegen Ehr' und Pflicht,", "tokens": ["Er", "h\u00e4lt", "es", "ge\u00b7gen", "Ehr'", "und", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wenn er nicht ewig widerspricht.", "tokens": ["Wenn", "er", "nicht", "e\u00b7wig", "wi\u00b7der\u00b7spricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Der andre, sonst ein sanfter Mann,", "tokens": ["Der", "and\u00b7re", ",", "sonst", "ein", "sanf\u00b7ter", "Mann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Droht jenem doch den ew'gen Bann;", "tokens": ["Droht", "je\u00b7nem", "doch", "den", "ew'\u00b7gen", "Bann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Ihn fa\u00dft beim Wort Vernunft ein Grau'n,", "tokens": ["Ihn", "fa\u00dft", "beim", "Wort", "Ver\u00b7nunft", "ein", "Grau'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Er kann sich glaubig nur erbau'n,", "tokens": ["Er", "kann", "sich", "glau\u00b7big", "nur", "er\u00b7bau'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADJD", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Wenn er ins Unbegriffne sich", "tokens": ["Wenn", "er", "ins", "Un\u00b7be\u00b7griff\u00b7ne", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Versenket hei\u00df und inniglich.", "tokens": ["Ver\u00b7sen\u00b7ket", "hei\u00df", "und", "in\u00b7nig\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch nur aus Liebe streiten sie,", "tokens": ["Doch", "nur", "aus", "Lie\u00b7be", "strei\u00b7ten", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und scheiden unvers\u00f6hnet nie,", "tokens": ["Und", "schei\u00b7den", "un\u00b7ver\u00b7s\u00f6h\u00b7net", "nie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wenn sie von einander sind,", "tokens": ["Und", "wenn", "sie", "von", "ein\u00b7an\u00b7der", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "PRF", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Thut jeder einen Seufzer lind:", "tokens": ["Thut", "je\u00b7der", "ei\u00b7nen", "Seuf\u00b7zer", "lind", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u00bbw\u00e4r' er vom Aberglauben los!\u00ab \u2013", "tokens": ["\u00bb", "w\u00e4r'", "er", "vom", "A\u00b7berg\u00b7lau\u00b7ben", "los", "!", "\u00ab", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "VAFIN", "PPER", "APPRART", "NN", "PTKVZ", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u00bbach, w\u00e4r' er aus der H\u00f6lle Schoos!\u00ab", "tokens": ["\u00bb", "ach", ",", "w\u00e4r'", "er", "aus", "der", "H\u00f6l\u00b7le", "Schoos", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ITJ", "$,", "VAFIN", "PPER", "APPR", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Im \u00fcbrigen lebt schlecht und recht", "tokens": ["Im", "\u00fcb\u00b7ri\u00b7gen", "lebt", "schlecht", "und", "recht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "VVFIN", "ADJD", "KON", "NN"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.8": {"text": "Der eine des Gesetzes Knecht,", "tokens": ["Der", "ei\u00b7ne", "des", "Ge\u00b7set\u00b7zes", "Knecht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der andere des Glaubens Sohn;", "tokens": ["Der", "an\u00b7de\u00b7re", "des", "Glau\u00b7bens", "Sohn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Sie handeln redlich, nicht um Lohn,", "tokens": ["Sie", "han\u00b7deln", "red\u00b7lich", ",", "nicht", "um", "Lohn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Sie thun dem N\u00e4chsten Guts und Liebs,", "tokens": ["Sie", "thun", "dem", "N\u00e4chs\u00b7ten", "Guts", "und", "Liebs", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und Feinde sind's des Weltgetriebs;", "tokens": ["Und", "Fein\u00b7de", "sin\u00b7d's", "des", "Welt\u00b7ge\u00b7triebs", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Und manchmal wundern beide sich;", "tokens": ["Und", "manch\u00b7mal", "wun\u00b7dern", "bei\u00b7de", "sich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVINF", "PIS", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Der Weise spricht: \u00bbWie freut es mich,", "tokens": ["Der", "Wei\u00b7se", "spricht", ":", "\u00bb", "Wie", "freut", "es", "mich", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "PWAV", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Du handelst alles Irrthums bar,", "tokens": ["Du", "han\u00b7delst", "al\u00b7les", "Irr\u00b7thums", "bar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Vern\u00fcnftig bist du ganz und gar!\u00ab", "tokens": ["Ver\u00b7n\u00fcnf\u00b7tig", "bist", "du", "ganz", "und", "gar", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "KON", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Und Jener ihm erwiedert dann:", "tokens": ["Und", "Je\u00b7ner", "ihm", "er\u00b7wie\u00b7dert", "dann", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "\u00bbseh' ich dein Thun und Treiben an,", "tokens": ["\u00bb", "seh'", "ich", "dein", "Thun", "und", "Trei\u00b7ben", "an", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "PPOSAT", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.19": {"text": "Ach, w\u00fc\u00dft' ich nicht, woher es stammt,", "tokens": ["Ach", ",", "w\u00fc\u00dft'", "ich", "nicht", ",", "wo\u00b7her", "es", "stammt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "PTKNEG", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Ich spr\u00e4ch': In dir der Glaube flammt!\u00ab", "tokens": ["Ich", "spr\u00e4ch'", ":", "In", "dir", "der", "Glau\u00b7be", "flammt", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$.", "APPR", "PPER", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Und haben sie gesprochen so,", "tokens": ["Und", "ha\u00b7ben", "sie", "ge\u00b7spro\u00b7chen", "so", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVPP", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Die H\u00e4nde reichen sie sich froh:", "tokens": ["Die", "H\u00e4n\u00b7de", "rei\u00b7chen", "sie", "sich", "froh", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PRF", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Vielleicht, vielleicht \u2013 jetzt ist's an dem \u2013", "tokens": ["Viel\u00b7leicht", ",", "viel\u00b7leicht", "\u2013", "jetzt", "ist's", "an", "dem", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "$(", "ADV", "VAFIN", "APPR", "ART", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.24": {"text": "Doch nein, es sieget ihr System!", "tokens": ["Doch", "nein", ",", "es", "sie\u00b7get", "ihr", "Sys\u00b7tem", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Da kam's, da\u00df an sein Sterbebett", "tokens": ["Da", "kam's", ",", "da\u00df", "an", "sein", "Ster\u00b7be\u00b7bett"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die zween ein Freund beschieden h\u00e4tt'.", "tokens": ["Die", "zween", "ein", "Freund", "be\u00b7schie\u00b7den", "h\u00e4tt'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er legt' in ihre treue Hand,", "tokens": ["Er", "legt'", "in", "ih\u00b7re", "treu\u00b7e", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den einz'gen Sohn, ein theures Pfand;", "tokens": ["Den", "einz'\u00b7gen", "Sohn", ",", "ein", "theu\u00b7res", "Pfand", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er sprach: \u00bbDie\u00df liebe Kind erzieht", "tokens": ["Er", "sprach", ":", "\u00bb", "Die\u00df", "lie\u00b7be", "Kind", "er\u00b7zieht"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "$(", "PDS", "VVFIN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da sehen sie in's Angesicht", "tokens": ["Da", "se\u00b7hen", "sie", "in's", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dem toten Freund und hadern nicht;", "tokens": ["Dem", "to\u00b7ten", "Freund", "und", "ha\u00b7dern", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sie gehn zu Hause Hand in Hand:", "tokens": ["Sie", "gehn", "zu", "Hau\u00b7se", "Hand", "in", "Hand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "\u00bbdas Rechte lehrt uns der Verstand!\u00ab \u2013", "tokens": ["\u00bb", "das", "Rech\u00b7te", "lehrt", "uns", "der", "Ver\u00b7stand", "!", "\u00ab", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "\u00bbdas Rechte zeigt der Herr uns an!\u00ab", "tokens": ["\u00bb", "das", "Rech\u00b7te", "zeigt", "der", "Herr", "uns", "an", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Kopfsch\u00fcttelnd scheidet Mann von Mann.", "tokens": ["Kopf\u00b7sch\u00fct\u00b7telnd", "schei\u00b7det", "Mann", "von", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und in dem einsamen Gemach", "tokens": ["Und", "in", "dem", "ein\u00b7sa\u00b7men", "Ge\u00b7mach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.13": {"text": "Denkt jeder seiner Sorge nach.", "tokens": ["Denkt", "je\u0