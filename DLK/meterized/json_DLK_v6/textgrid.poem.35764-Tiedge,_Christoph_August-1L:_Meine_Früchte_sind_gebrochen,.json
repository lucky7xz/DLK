{"textgrid.poem.35764": {"metadata": {"author": {"name": "Tiedge, Christoph August", "birth": "N.A.", "death": "N.A."}, "title": "1L: Meine Fr\u00fcchte sind gebrochen,", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meine Fr\u00fcchte sind gebrochen,", "tokens": ["Mei\u00b7ne", "Fr\u00fcch\u00b7te", "sind", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine Rosen sind gepfl\u00fcckt,", "tokens": ["Mei\u00b7ne", "Ro\u00b7sen", "sind", "ge\u00b7pfl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und das letzte, frohe Pochen", "tokens": ["Und", "das", "letz\u00b7te", ",", "fro\u00b7he", "Po\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieses Herzens ist erstickt;", "tokens": ["Die\u00b7ses", "Her\u00b7zens", "ist", "er\u00b7stickt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dieses Herzens, das so innig", "tokens": ["Die\u00b7ses", "Her\u00b7zens", ",", "das", "so", "in\u00b7nig"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDAT", "NN", "$,", "PRELS", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Seine Lieb' um alles schlang,", "tokens": ["Sei\u00b7ne", "Lieb'", "um", "al\u00b7les", "schlang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Seinen Ha\u00df so gern versang,", "tokens": ["Sei\u00b7nen", "Ha\u00df", "so", "gern", "ver\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Nur vielleicht zu eigensinnig", "tokens": ["Nur", "viel\u00b7leicht", "zu", "ei\u00b7gen\u00b7sin\u00b7nig"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "PTKA", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Gegen Sturm und Fluten rang.", "tokens": ["Ge\u00b7gen", "Sturm", "und", "Flu\u00b7ten", "rang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Was, o Herz, hast du errungen?", "tokens": ["Was", ",", "o", "Herz", ",", "hast", "du", "er\u00b7run\u00b7gen", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "FM", "NN", "$,", "VAFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo ist dein gelobtes Land?", "tokens": ["Wo", "ist", "dein", "ge\u00b7lob\u00b7tes", "Land", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deine sch\u00f6nsten Huldigungen", "tokens": ["Dei\u00b7ne", "sch\u00f6ns\u00b7ten", "Hul\u00b7di\u00b7gun\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nahm die Hoffnung an \u2013 und schwand.", "tokens": ["Nahm", "die", "Hoff\u00b7nung", "an", "\u2013", "und", "schwand", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$(", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nun ist dieser Mut geschieden,", "tokens": ["Nun", "ist", "die\u00b7ser", "Mut", "ge\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PDAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der so stolz die Fl\u00fcgel schlug,", "tokens": ["Der", "so", "stolz", "die", "Fl\u00fc\u00b7gel", "schlug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und auf seinem Adlerflug", "tokens": ["Und", "auf", "sei\u00b7nem", "Ad\u00b7ler\u00b7flug"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Meine Seel' und ihren Frieden", "tokens": ["Mei\u00b7ne", "Seel'", "und", "ih\u00b7ren", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Mitten durch die St\u00fcrme trug.", "tokens": ["Mit\u00b7ten", "durch", "die", "St\u00fcr\u00b7me", "trug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Dich nur kenn' ich noch, o Freude,", "tokens": ["Dich", "nur", "kenn'", "ich", "noch", ",", "o", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "ADV", "$,", "FM", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die du dem Ger\u00e4usch entweichst,", "tokens": ["Die", "du", "dem", "Ge\u00b7r\u00e4usch", "ent\u00b7weichst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und zur dunkeln Thr\u00e4nenweide", "tokens": ["Und", "zur", "dun\u00b7keln", "Thr\u00e4\u00b7nen\u00b7wei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gern mit deiner Wehmut schleichst.", "tokens": ["Gern", "mit", "dei\u00b7ner", "Weh\u00b7mut", "schleichst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dort umwankt mich noch ein Schimmer,", "tokens": ["Dort", "um\u00b7wankt", "mich", "noch", "ein", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie ein Geist aus toter Welt,", "tokens": ["Wie", "ein", "Geist", "aus", "to\u00b7ter", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Der sich still zu mir gesellt,", "tokens": ["Der", "sich", "still", "zu", "mir", "ge\u00b7sellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADJD", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und im Dunkellicht die Tr\u00fcmmer", "tokens": ["Und", "im", "Dun\u00b7kel\u00b7licht", "die", "Tr\u00fcm\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Der Vergangenheit erhellt.", "tokens": ["Der", "Ver\u00b7gan\u00b7gen\u00b7heit", "er\u00b7hellt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Alles ist vor\u00fcberfliehend.", "tokens": ["Al\u00b7les", "ist", "vor\u00b7\u00fc\u00b7berf\u00b7lie\u00b7hend", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weinend rei\u00dft sich aus dem Scho\u00df", "tokens": ["Wei\u00b7nend", "rei\u00dft", "sich", "aus", "dem", "Scho\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eines Lebens, das so bl\u00fchend", "tokens": ["Ei\u00b7nes", "Le\u00b7bens", ",", "das", "so", "bl\u00fc\u00b7hend"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie umfing, die Seele los.", "tokens": ["Sie", "um\u00b7fing", ",", "die", "See\u00b7le", "los", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Unter frommen Nachtigallen", "tokens": ["Un\u00b7ter", "from\u00b7men", "Nach\u00b7ti\u00b7gal\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist mein sch\u00f6nster Traum verhallt;", "tokens": ["Ist", "mein", "sch\u00f6ns\u00b7ter", "Traum", "ver\u00b7hallt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wachend seh' ich jetzt: der Wald", "tokens": ["Wa\u00b7chend", "seh'", "ich", "jetzt", ":", "der", "Wald"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "$.", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wird, wenn seine Bl\u00e4tter fallen,", "tokens": ["Wird", ",", "wenn", "sei\u00b7ne", "Bl\u00e4t\u00b7ter", "fal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KOUS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Heller wird er, aber kalt.", "tokens": ["Hel\u00b7ler", "wird", "er", ",", "a\u00b7ber", "kalt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "$,", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00dcber Gegendruck und M\u00e4ngel", "tokens": ["\u00dc\u00b7ber", "Ge\u00b7gen\u00b7druck", "und", "M\u00e4n\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Flog ich hin, mit Lust und Scherz;", "tokens": ["Flog", "ich", "hin", ",", "mit", "Lust", "und", "Scherz", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle Menschen waren Engel,", "tokens": ["Al\u00b7le", "Men\u00b7schen", "wa\u00b7ren", "En\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alle lud ich in mein Herz.", "tokens": ["Al\u00b7le", "lud", "ich", "in", "mein", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alles, alles f\u00fchlt' ich leiser,", "tokens": ["Al\u00b7les", ",", "al\u00b7les", "f\u00fchlt'", "ich", "lei\u00b7ser", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PIS", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Was das Leben niederdr\u00fcckt,", "tokens": ["Was", "das", "Le\u00b7ben", "nie\u00b7der\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Leicht befriedigt, leicht entz\u00fcckt:", "tokens": ["Leicht", "be\u00b7frie\u00b7digt", ",", "leicht", "ent\u00b7z\u00fcckt", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Jetzt bin ich ein wenig weiser", "tokens": ["Jetzt", "bin", "ich", "ein", "we\u00b7nig", "wei\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART", "PIAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und viel weniger begl\u00fcckt.", "tokens": ["Und", "viel", "we\u00b7ni\u00b7ger", "be\u00b7gl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Junge, heitre W\u00fcnsche traten", "tokens": ["Jun\u00b7ge", ",", "heit\u00b7re", "W\u00fcn\u00b7sche", "tra\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hin vor meine Phantasie,", "tokens": ["Hin", "vor", "mei\u00b7ne", "Phan\u00b7ta\u00b7sie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die f\u00fcr alles, was sie baten,", "tokens": ["Die", "f\u00fcr", "al\u00b7les", ",", "was", "sie", "ba\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIS", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihnen Zuversicht verlieh;", "tokens": ["Ih\u00b7nen", "Zu\u00b7ver\u00b7sicht", "ver\u00b7lieh", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Furchtlos, irgendwo zu stranden,", "tokens": ["Furcht\u00b7los", ",", "ir\u00b7gend\u00b7wo", "zu", "stran\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schifften sie den Strom der Zeit,", "tokens": ["Schiff\u00b7ten", "sie", "den", "Strom", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Unter scherzendem Geleit,", "tokens": ["Un\u00b7ter", "scher\u00b7zen\u00b7dem", "Ge\u00b7leit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Rasch und fr\u00f6hlich hin, und fanden", "tokens": ["Rasch", "und", "fr\u00f6h\u00b7lich", "hin", ",", "und", "fan\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "PTKVZ", "$,", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Nicht das Land der Seligkeit.", "tokens": ["Nicht", "das", "Land", "der", "Se\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Doch war sch\u00f6n die Zeit der Bl\u00fcte,", "tokens": ["Doch", "war", "sch\u00f6n", "die", "Zeit", "der", "Bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "ART", "NN", "ART", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Sch\u00f6n die Thyrsusschwingerin;", "tokens": ["Sch\u00f6n", "die", "Thyr\u00b7sus\u00b7schwin\u00b7ge\u00b7rin", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hold, wie lauter Lieb' und G\u00fcte,", "tokens": ["Hold", ",", "wie", "lau\u00b7ter", "Lieb'", "und", "G\u00fc\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PIAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Froh, wie lauter Lebenssinn,", "tokens": ["Froh", ",", "wie", "lau\u00b7ter", "Le\u00b7bens\u00b7sinn", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Warf sie freundlich auf den Reigen", "tokens": ["Warf", "sie", "freund\u00b7lich", "auf", "den", "Rei\u00b7gen"], "token_i