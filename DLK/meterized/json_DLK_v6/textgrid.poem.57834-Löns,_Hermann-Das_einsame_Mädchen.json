{"textgrid.poem.57834": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Das einsame M\u00e4dchen", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich stehe auf der Heide", "tokens": ["Ich", "ste\u00b7he", "auf", "der", "Hei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich bin so ganz allein;", "tokens": ["Ich", "bin", "so", "ganz", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Sch\u00e4tzchen m\u00f6cht' ich haben,", "tokens": ["Ein", "Sch\u00e4tz\u00b7chen", "m\u00f6cht'", "ich", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und mich der Liebe freun.", "tokens": ["Und", "mich", "der", "Lie\u00b7be", "freun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Jedweder kleine Vogel", "tokens": ["Jed\u00b7we\u00b7der", "klei\u00b7ne", "Vo\u00b7gel"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der liebt so viel er mag;", "tokens": ["Der", "liebt", "so", "viel", "er", "mag", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "PPER", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch ich mu\u00df einsam bleiben", "tokens": ["Doch", "ich", "mu\u00df", "ein\u00b7sam", "blei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bei Nacht und auch bei Tag.", "tokens": ["Bei", "Nacht", "und", "auch", "bei", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ein Reitersmann zu Pferde", "tokens": ["Ein", "Rei\u00b7ters\u00b7mann", "zu", "Pfer\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das soll mein Liebster sein;", "tokens": ["Das", "soll", "mein", "Liebs\u00b7ter", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er soll den Schl\u00fcssel haben", "tokens": ["Er", "soll", "den", "Schl\u00fcs\u00b7sel", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu meinem G\u00e4rtelein.", "tokens": ["Zu", "mei\u00b7nem", "G\u00e4r\u00b7tel\u00b7ein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Rosen soll er brechen,", "tokens": ["Die", "Ro\u00b7sen", "soll", "er", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So viel und viel er mag;", "tokens": ["So", "viel", "und", "viel", "er", "mag", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "ADV", "PPER", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es wachsen wieder neue", "tokens": ["Es", "wach\u00b7sen", "wie\u00b7der", "neu\u00b7e"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In meinem Gartenhag.", "tokens": ["In", "mei\u00b7nem", "Gar\u00b7ten\u00b7hag", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}