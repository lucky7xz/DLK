{"dta.poem.19881": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Seufzer  \n eines Ungeliebten .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1778", "urn": "urn:nbn:de:kobv:b4-20090519672", "language": ["de:0.99"], "booktitle": "B\u00fcrger, Gottfried August: Gedichte. G\u00f6ttingen, 1778."}, "poem": {"stanza.1": {"line.1": {"text": "Hast du nicht Liebe zu gemessen               ", "tokens": ["Hast", "du", "nicht", "Lie\u00b7be", "zu", "ge\u00b7mes\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PTKNEG", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Leben jeder Kreatur?", "tokens": ["Dem", "Le\u00b7ben", "je\u00b7der", "Kre\u00b7a\u00b7tur", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Warum bin ich allein vergessen,", "tokens": ["Wa\u00b7rum", "bin", "ich", "al\u00b7lein", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auch meine Mutter du! Natur?", "tokens": ["Auch", "mei\u00b7ne", "Mut\u00b7ter", "du", "!", "Na\u00b7tur", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PPER", "$.", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wo lebte wol in Forst und H\u00fcrde,", "tokens": ["Wo", "leb\u00b7te", "wol", "in", "Forst", "und", "H\u00fcr\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und wo in Luft und Meer ein Thier,", "tokens": ["Und", "wo", "in", "Luft", "und", "Meer", "ein", "Thier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPR", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das nimmermehr geliebet w\u00fcrde? \u2014", "tokens": ["Das", "nim\u00b7mer\u00b7mehr", "ge\u00b7lie\u00b7bet", "w\u00fcr\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "ADV", "VVPP", "VAFIN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Geliebt wird alles ausser mir!", "tokens": ["Ge\u00b7liebt", "wird", "al\u00b7les", "aus\u00b7ser", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PIS", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn gleich in Hain und Wiesenmatten", "tokens": ["Wenn", "gleich", "in", "Hain", "und", "Wie\u00b7sen\u00b7mat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich Baum und Staude, Moos und Kraut,", "tokens": ["Sich", "Baum", "und", "Stau\u00b7de", ",", "Moos", "und", "Kraut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "NE", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch Lieb\u2019 und Gegenliebe gatten;", "tokens": ["Durch", "Lieb'", "und", "Ge\u00b7gen\u00b7lie\u00b7be", "gat\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verm\u00e4lt sich mir doch keine Braut.", "tokens": ["Ver\u00b7m\u00e4lt", "sich", "mir", "doch", "kei\u00b7ne", "Braut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPER", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Mir w\u00e4chst vom s\u00fcssesten der Triebe", "tokens": ["Mir", "w\u00e4chst", "vom", "s\u00fcs\u00b7ses\u00b7ten", "der", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "VVFIN", "ART", "NN"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Nie Honigfrucht zur Lust heran.", "tokens": ["Nie", "Ho\u00b7nig\u00b7frucht", "zur", "Lust", "he\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn ach! mir mangelt Gegenliebe,", "tokens": ["Denn", "ach", "!", "mir", "man\u00b7gelt", "Ge\u00b7gen\u00b7lie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Eine nur gew\u00e4ren kan.", "tokens": ["Die", "Ei\u00b7ne", "nur", "ge\u00b7w\u00e4\u00b7ren", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}