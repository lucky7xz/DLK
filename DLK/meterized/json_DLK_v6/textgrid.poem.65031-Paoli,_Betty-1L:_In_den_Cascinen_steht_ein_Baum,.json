{"textgrid.poem.65031": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: In den Cascinen steht ein Baum,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In den Cascinen steht ein Baum,", "tokens": ["In", "den", "Cas\u00b7ci\u00b7nen", "steht", "ein", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den meinen Freund ich nannte.", "tokens": ["Den", "mei\u00b7nen", "Freund", "ich", "nann\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dort ruht' ich oft in wachem Traum", "tokens": ["Dort", "ruht'", "ich", "oft", "in", "wa\u00b7chem", "Traum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn hei\u00df die Sonne brannte.", "tokens": ["Wenn", "hei\u00df", "die", "Son\u00b7ne", "brann\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und wenn dann auf die m\u00fcde Welt", "tokens": ["Und", "wenn", "dann", "auf", "die", "m\u00fc\u00b7de", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Herabsank m\u00e4cht'ges D\u00fcster,", "tokens": ["Her\u00b7ab\u00b7sank", "m\u00e4cht'\u00b7ges", "D\u00fcs\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da drang aus seinem Laubgezelt", "tokens": ["Da", "drang", "aus", "sei\u00b7nem", "Laub\u00b7ge\u00b7zelt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein geisterhaft Gefl\u00fcster.", "tokens": ["Ein", "geis\u00b7ter\u00b7haft", "Ge\u00b7fl\u00fcs\u00b7ter", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er lie\u00df die Bl\u00fcthen duftig wei\u00df", "tokens": ["Er", "lie\u00df", "die", "Bl\u00fc\u00b7then", "duf\u00b7tig", "wei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf mich hernieder regnen,", "tokens": ["Auf", "mich", "her\u00b7nie\u00b7der", "reg\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als wollte er, ein Priestergreis,", "tokens": ["Als", "woll\u00b7te", "er", ",", "ein", "Pries\u00b7ter\u00b7greis", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Haupt, das m\u00fcde, segnen! \u2013", "tokens": ["Mein", "Haupt", ",", "das", "m\u00fc\u00b7de", ",", "seg\u00b7nen", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADJD", "$,", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Jetzt irr' ich mit umflortem Blick", "tokens": ["Jetzt", "irr'", "ich", "mit", "um\u00b7flor\u00b7tem", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf d\u00fcrr versengten Matten", "tokens": ["Auf", "d\u00fcrr", "ver\u00b7seng\u00b7ten", "Mat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und trauernd sehn' ich mich zur\u00fcck", "tokens": ["Und", "trau\u00b7ernd", "sehn'", "ich", "mich", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PRF", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In seinen linden Schatten.", "tokens": ["In", "sei\u00b7nen", "lin\u00b7den", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und soll er einst vom Todesstreich", "tokens": ["Und", "soll", "er", "einst", "vom", "To\u00b7des\u00b7streich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der scharfen Axt erbeben,", "tokens": ["Der", "schar\u00b7fen", "Axt", "er\u00b7be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dann f\u00e4llt mit diesem Baum zugleich", "tokens": ["Dann", "f\u00e4llt", "mit", "die\u00b7sem", "Baum", "zu\u00b7gleich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PDAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Theil von meinem Leben!", "tokens": ["Ein", "Theil", "von", "mei\u00b7nem", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}