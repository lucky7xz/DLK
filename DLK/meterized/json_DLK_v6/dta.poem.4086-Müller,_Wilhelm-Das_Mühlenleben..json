{"dta.poem.4086": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Das M\u00fchlenleben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn sie Fliegennetze strickt,", "tokens": ["Wenn", "sie", "Flie\u00b7gen\u00b7net\u00b7ze", "strickt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Oder Sonntags f\u00fcr die Fenster", "tokens": ["O\u00b7der", "Sonn\u00b7tags", "f\u00fcr", "die", "Fens\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Frische Wiesenblumen pfl\u00fcckt;", "tokens": ["Fri\u00b7sche", "Wie\u00b7sen\u00b7blu\u00b7men", "pfl\u00fcckt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Seh' ich sie zum Garten wandeln,", "tokens": ["Seh'", "ich", "sie", "zum", "Gar\u00b7ten", "wan\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit dem K\u00f6rbchen in der Hand,", "tokens": ["Mit", "dem", "K\u00f6rb\u00b7chen", "in", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nach den ersten Beeren sp\u00e4hen", "tokens": ["Nach", "den", "ers\u00b7ten", "Bee\u00b7ren", "sp\u00e4\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An der gr\u00fcnen Dornenwand:", "tokens": ["An", "der", "gr\u00fc\u00b7nen", "Dor\u00b7nen\u00b7wand", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Dann wird mir die M\u00fchle enge,", "tokens": ["Dann", "wird", "mir", "die", "M\u00fch\u00b7le", "en\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "ADJA", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Alle Mauern ziehn sich ein,", "tokens": ["Al\u00b7le", "Mau\u00b7ern", "ziehn", "sich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich m\u00f6chte flugs ein Fischer,", "tokens": ["Und", "ich", "m\u00f6ch\u00b7te", "flugs", "ein", "Fi\u00b7scher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "J\u00e4ger oder G\u00e4rtner sein.", "tokens": ["J\u00e4\u00b7ger", "o\u00b7der", "G\u00e4rt\u00b7ner", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und der Steine lustig Pfeifen,", "tokens": ["Und", "der", "Stei\u00b7ne", "lus\u00b7tig", "Pfei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und des Wasserrad's Gebraus,", "tokens": ["Und", "des", "Was\u00b7ser\u00b7ra\u00b7d's", "Ge\u00b7braus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und der Werke emsig Klappern,", "tokens": ["Und", "der", "Wer\u00b7ke", "em\u00b7sig", "Klap\u00b7pern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "'s jagt mich schier zum Thor hinaus.", "tokens": ["'s", "jagt", "mich", "schier", "zum", "Thor", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Aber wenn in guter Stunde", "tokens": ["A\u00b7ber", "wenn", "in", "gu\u00b7ter", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Plaudernd sie zum Burschen trit,", "tokens": ["Plau\u00b7dernd", "sie", "zum", "Bur\u00b7schen", "trit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und als kluges Kind des Hauses", "tokens": ["Und", "als", "klu\u00b7ges", "Kind", "des", "Hau\u00b7ses"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seitw\u00e4rts nach dem Rechten sieht;", "tokens": ["Seit\u00b7w\u00e4rts", "nach", "dem", "Rech\u00b7ten", "sieht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und verst\u00e4ndig lobt den Einen,", "tokens": ["Und", "ver\u00b7st\u00e4n\u00b7dig", "lobt", "den", "Ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df der Andre merken mag,", "tokens": ["Da\u00df", "der", "And\u00b7re", "mer\u00b7ken", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PIS", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie er's besser treiben solle,", "tokens": ["Wie", "er's", "bes\u00b7ser", "trei\u00b7ben", "sol\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Geht er ihrem Danke nach \u2014", "tokens": ["Geht", "er", "ih\u00b7rem", "Dan\u00b7ke", "nach"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "APPR", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Keiner f\u00fchlt sich recht getroffen,", "tokens": ["Kei\u00b7ner", "f\u00fchlt", "sich", "recht", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und doch schie\u00dft sie nimmer fehl,", "tokens": ["Und", "doch", "schie\u00dft", "sie", "nim\u00b7mer", "fehl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jeder mu\u00df von Schonung sagen,", "tokens": ["Je\u00b7der", "mu\u00df", "von", "Scho\u00b7nung", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und doch hat sie keinen Hehl.", "tokens": ["Und", "doch", "hat", "sie", "kei\u00b7nen", "Hehl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Keiner w\u00fcnscht, sie m\u00f6chte gehen", "tokens": ["Kei\u00b7ner", "w\u00fcnscht", ",", "sie", "m\u00f6ch\u00b7te", "ge\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "PPER", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Steht sie auch als Herrin da,", "tokens": ["Steht", "sie", "auch", "als", "Her\u00b7rin", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "KOUS", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und fast wie das Auge Gottes", "tokens": ["Und", "fast", "wie", "das", "Au\u00b7ge", "Got\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "KOKOM", "ART", "NN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ist ihr Bild uns immer nah.", "tokens": ["Ist", "ihr", "Bild", "uns", "im\u00b7mer", "nah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PPER", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Und wo wer zum Fallen strauchelt,", "tokens": ["Und", "wo", "wer", "zum", "Fal\u00b7len", "strau\u00b7chelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PWS", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00e4lt es ihn im Sinken schier,", "tokens": ["H\u00e4lt", "es", "ihn", "im", "Sin\u00b7ken", "schier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPRART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wo ich die H\u00e4nde falte,", "tokens": ["Und", "wo", "ich", "die", "H\u00e4n\u00b7de", "fal\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Kniet es still zur Seite mir \u2014", "tokens": ["Kniet", "es", "still", "zur", "Sei\u00b7te", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPRART", "NN", "PPER", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Ei, da mag das M\u00fchlenleben", "tokens": ["Ei", ",", "da", "mag", "das", "M\u00fch\u00b7len\u00b7le\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "VMFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wohl des Liedes w\u00fcrdig sein,", "tokens": ["Wohl", "des", "Lie\u00b7des", "w\u00fcr\u00b7dig", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die R\u00e4der, Stein' und Stampfen", "tokens": ["Und", "die", "R\u00e4\u00b7der", ",", "Stein'", "und", "Stamp\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stimmen als Begleitung ein.", "tokens": ["Stim\u00b7men", "als", "Be\u00b7glei\u00b7tung", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Alles geht in sch\u00f6nem Tanze", "tokens": ["Al\u00b7les", "geht", "in", "sch\u00f6\u00b7nem", "Tan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf und ab, und ein und aus:", "tokens": ["Auf", "und", "ab", ",", "und", "ein", "und", "aus", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KON", "PTKVZ", "$,", "KON", "ART", "KON", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gott gesegne mir das Handwerk", "tokens": ["Gott", "ge\u00b7seg\u00b7ne", "mir", "das", "Hand\u00b7werk"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und des guten Meisters Haus!", "tokens": ["Und", "des", "gu\u00b7ten", "Meis\u00b7ters", "Haus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}