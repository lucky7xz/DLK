{"textgrid.poem.43674": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Zum Honigschmaus", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun la\u00dft uns geh'n aufs Dorf hinaus", "tokens": ["Nun", "la\u00dft", "uns", "geh'n", "aufs", "Dorf", "hin\u00b7aus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "APPRART", "NN", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Honigtrank und Honigschmaus!", "tokens": ["Zum", "Ho\u00b7nig\u00b7trank", "und", "Ho\u00b7nig\u00b7schmaus", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das ist der Bienenv\u00e4ter Brauch,", "tokens": ["Das", "ist", "der", "Bie\u00b7nen\u00b7v\u00e4\u00b7ter", "Brauch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie halten ihre Kirme\u00df auch.", "tokens": ["Sie", "hal\u00b7ten", "ih\u00b7re", "Kir\u00b7me\u00df", "auch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sobald der Honig das Jahr ger\u00e4th,", "tokens": ["So\u00b7bald", "der", "Ho\u00b7nig", "das", "Jahr", "ge\u00b7r\u00e4\u00b7th", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+---", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Verkaufen sie Honigseim und Meth.", "tokens": ["Ver\u00b7kau\u00b7fen", "sie", "Ho\u00b7ni\u00b7gseim", "und", "Me\u00b7th", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NE", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Wie sind wir froh und wohlgemuth!", "tokens": ["Wie", "sind", "wir", "froh", "und", "wohl\u00b7ge\u00b7muth", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie schmeckt's uns Allen doch so gut!", "tokens": ["Wie", "schmeckt's", "uns", "Al\u00b7len", "doch", "so", "gut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PIS", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und wenn wir mit Schmausen fertig sind,", "tokens": ["Und", "wenn", "wir", "mit", "Schmau\u00b7sen", "fer\u00b7tig", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und wi\u00dft ihr, was man dann beginnt?", "tokens": ["Und", "wi\u00dft", "ihr", ",", "was", "man", "dann", "be\u00b7ginnt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PRELS", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Dann wird sofort Musik gemacht,", "tokens": ["Dann", "wird", "so\u00b7fort", "Mu\u00b7sik", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und t\u00fcchtig getanzt, gescherzt und gelacht.", "tokens": ["Und", "t\u00fcch\u00b7tig", "ge\u00b7tanzt", ",", "ge\u00b7scherzt", "und", "ge\u00b7lacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "$,", "VVPP", "KON", "VVPP", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Und singend gehen wir endlich heim", "tokens": ["Und", "sin\u00b7gend", "ge\u00b7hen", "wir", "end\u00b7lich", "heim"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADV", "PTKVZ"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit einem Topf voll Honigseim.", "tokens": ["Mit", "ei\u00b7nem", "Topf", "voll", "Ho\u00b7ni\u00b7gseim", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}