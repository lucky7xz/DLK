{"textgrid.poem.62633": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Herr \u2013 \u2013 \u2013 da dein Auge heut das Neue Jahr erblickt, wird dir von bekannten H\u00e4nden dieses Bl\u00e4tgen zugeschickt", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie sich der Tag und Jahre-Zahl,", "tokens": ["Wie", "sich", "der", "Tag", "und", "Jah\u00b7re\u00b7Zahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In nahen und entfernten L\u00e4ndern,", "tokens": ["In", "na\u00b7hen", "und", "ent\u00b7fern\u00b7ten", "L\u00e4n\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Auf den gew\u00f6lbten Erden-Saal", "tokens": ["Auf", "den", "ge\u00b7w\u00f6lb\u00b7ten", "Er\u00b7den\u00b7Saal"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verkehren und gar bald ver\u00e4ndern;", "tokens": ["Ver\u00b7keh\u00b7ren", "und", "gar", "bald", "ver\u00b7\u00e4n\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So \u00e4ndert sich der Menschen Gl\u00fcck;", "tokens": ["So", "\u00e4n\u00b7dert", "sich", "der", "Men\u00b7schen", "Gl\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bald r\u00fcckt es fort, bald gehts zur\u00fcck;", "tokens": ["Bald", "r\u00fcckt", "es", "fort", ",", "bald", "gehts", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Licht, Fruchtbarkeit und Finsternissen,", "tokens": ["Licht", ",", "Frucht\u00b7bar\u00b7keit", "und", "Fins\u00b7ter\u00b7nis\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Krieg, Theurung, Ungl\u00fcck, Friedens-Zeit,", "tokens": ["Krieg", ",", "Theu\u00b7rung", ",", "Un\u00b7gl\u00fcck", ",", "Frie\u00b7dens\u00b7Zeit", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und was erschrecket und erfreut,", "tokens": ["Und", "was", "er\u00b7schre\u00b7cket", "und", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wird meinen Satz best\u00e4rken m\u00fcssen.", "tokens": ["Wird", "mei\u00b7nen", "Satz", "be\u00b7st\u00e4r\u00b7ken", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Aurora kommt zwar oftermahl,", "tokens": ["Au\u00b7ro\u00b7ra", "kommt", "zwar", "of\u00b7ter\u00b7mahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und beut uns einen g\u00fcldnen Morgen;", "tokens": ["Und", "beut", "uns", "ei\u00b7nen", "g\u00fcld\u00b7nen", "Mor\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Allein auch mit des Seigers-Zahl", "tokens": ["Al\u00b7lein", "auch", "mit", "des", "Sei\u00b7ger\u00b7sZahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vermehren sich gar oft die Sorgen.", "tokens": ["Ver\u00b7meh\u00b7ren", "sich", "gar", "oft", "die", "Sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Eh wir die Sonne recht erblickt;", "tokens": ["Eh", "wir", "die", "Son\u00b7ne", "recht", "er\u00b7blickt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "So wird der Nord-Wind abgeschickt,", "tokens": ["So", "wird", "der", "Nord\u00b7Wind", "ab\u00b7ge\u00b7schickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der macht den Hoffnungs-Klee zunichte.", "tokens": ["Der", "macht", "den", "Hoff\u00b7nungs\u00b7Klee", "zu\u00b7nich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Geht schon ein Freuden-Sterngen auf;", "tokens": ["Geht", "schon", "ein", "Freu\u00b7den\u00b7S\u00b7tern\u00b7gen", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "So folgen zehn Cometen drauf,", "tokens": ["So", "fol\u00b7gen", "zehn", "Co\u00b7me\u00b7ten", "drauf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "CARD", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und die verfinstern das Gesichte.", "tokens": ["Und", "die", "ver\u00b7fins\u00b7tern", "das", "Ge\u00b7sich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie fl\u00fcchtig und ver\u00e4nderlich", "tokens": ["Wie", "fl\u00fcch\u00b7tig", "und", "ver\u00b7\u00e4n\u00b7der\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sind doch die menschlichen Gedancken?", "tokens": ["Sind", "doch", "die", "menschli\u00b7chen", "Ge\u00b7dan\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Kein Halm, (wer wiederleget mich?)", "tokens": ["Kein", "Halm", ",", "(", "wer", "wie\u00b7der\u00b7le\u00b7get", "mich", "?", ")"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "NN", "$,", "$(", "PWS", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kan so geschwind und leichte wanken.", "tokens": ["Kan", "so", "ge\u00b7schwind", "und", "leich\u00b7te", "wan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADJD", "KON", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Zwar spricht ein Gro\u00dfmuthsvolles Blut:", "tokens": ["Zwar", "spricht", "ein", "Gro\u00df\u00b7muths\u00b7vol\u00b7les", "Blut", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kein Ungl\u00fcck nimmt mir meinen Muth,", "tokens": ["Kein", "Un\u00b7gl\u00fcck", "nimmt", "mir", "mei\u00b7nen", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich will bey Gl\u00fcck- und Ungl\u00fccks-Wellen;", "tokens": ["Ich", "will", "bey", "Gl\u00fc\u00b7ck", "und", "Un\u00b7gl\u00fccks\u00b7Wel\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "TRUNC", "KON", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Bey Sturm und Sto\u00df; bey Sonnenschein", "tokens": ["Bey", "Sturm", "und", "Sto\u00df", ";", "bey", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$.", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Best\u00e4ndig, unbeweglich seyn,", "tokens": ["Be\u00b7st\u00e4n\u00b7dig", ",", "un\u00b7be\u00b7weg\u00b7lich", "seyn", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und nie mein Angesicht verstellen.", "tokens": ["Und", "nie", "mein", "An\u00b7ge\u00b7sicht", "ver\u00b7stel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "O wankelm\u00fcthger Geist und Sinn!", "tokens": ["O", "wan\u00b7kel\u00b7m\u00fcth\u00b7ger", "Geist", "und", "Sinn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo bleiben deine Helden-Kr\u00e4fte?", "tokens": ["Wo", "blei\u00b7ben", "dei\u00b7ne", "Hel\u00b7den\u00b7Kr\u00e4f\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der erste Anfal wirft dich hin,", "tokens": ["Der", "ers\u00b7te", "An\u00b7fal", "wirft", "dich", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Furcht vertrocknet deine S\u00e4fte.", "tokens": ["Die", "Furcht", "ver\u00b7trock\u00b7net", "dei\u00b7ne", "S\u00e4f\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Kaum will ein kleines L\u00fcftgen wehn;", "tokens": ["Kaum", "will", "ein", "klei\u00b7nes", "L\u00fcft\u00b7gen", "wehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So wilst du schon verzweifelt stehn.", "tokens": ["So", "wilst", "du", "schon", "ver\u00b7zwei\u00b7felt", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie? l\u00e4ssest Du bey leichtem Knallen,", "tokens": ["Wie", "?", "l\u00e4s\u00b7sest", "Du", "bey", "leich\u00b7tem", "Knal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Bey einem kurzen Sturm und Sto\u00df,", "tokens": ["Bey", "ei\u00b7nem", "kur\u00b7zen", "Sturm", "und", "Sto\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Von deinen Muth so pl\u00f6tzlich lo\u00df?", "tokens": ["Von", "dei\u00b7nen", "Muth", "so", "pl\u00f6tz\u00b7lich", "lo\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wie? ist dein Vorsatz umgefallen?", "tokens": ["Wie", "?", "ist", "dein", "Vor\u00b7satz", "um\u00b7ge\u00b7fal\u00b7len", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VAFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Du Gro\u00dfmuths-voller Sagarit,", "tokens": ["Du", "Gro\u00df\u00b7muths\u00b7vol\u00b7ler", "Sa\u00b7ga\u00b7rit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer will, wie du, den Muth behalten?", "tokens": ["Wer", "will", ",", "wie", "du", ",", "den", "Muth", "be\u00b7hal\u00b7ten", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "$,", "PWAV", "PPER", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Darius, wer folgt deinem Tritt?", "tokens": ["Da\u00b7ri\u00b7us", ",", "wer", "folgt", "dei\u00b7nem", "Tritt", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wer kan wohl so beherzt erkalten?", "tokens": ["Wer", "kan", "wohl", "so", "be\u00b7herzt", "er\u00b7kal\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Pompejus h\u00e4lt den Ungl\u00fccks-Strau\u00df", "tokens": ["Pom\u00b7pe\u00b7jus", "h\u00e4lt", "den", "Un\u00b7gl\u00fccks\u00b7Strau\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Standhaftig auf dem Nachen aus:", "tokens": ["Stand\u00b7haf\u00b7tig", "auf", "dem", "Na\u00b7chen", "aus", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und Agag geht getrost zum Sterben.", "tokens": ["Und", "A\u00b7gag", "geht", "ge\u00b7trost", "zum", "Ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Kein Ungl\u00fccks-Tag, kein Schmerzens-Jahr,", "tokens": ["Kein", "Un\u00b7gl\u00fccks\u00b7Tag", ",", "kein", "Schmer\u00b7zens\u00b7Jahr", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Macht eure Kleinmuth offenbar.", "tokens": ["Macht", "eu\u00b7re", "Klein\u00b7muth", "of\u00b7fen\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wer wird doch eure Tugend erben?", "tokens": ["Wer", "wird", "doch", "eu\u00b7re", "Tu\u00b7gend", "er\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Man spricht, da\u00df diese Jahres-Zeit", "tokens": ["Man", "spricht", ",", "da\u00df", "die\u00b7se", "Jah\u00b7res\u00b7Zeit"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sehr viel Ver\u00e4ndrung mit sich bringe.", "tokens": ["Sehr", "viel", "Ver\u00b7\u00e4n\u00b7drung", "mit", "sich", "brin\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht nur den Krieg, der uns schon dr\u00e4ut.", "tokens": ["Nicht", "nur", "den", "Krieg", ",", "der", "uns", "schon", "dr\u00e4ut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nein, noch viel andre harte Dinge.", "tokens": ["Nein", ",", "noch", "viel", "and\u00b7re", "har\u00b7te", "Din\u00b7ge", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "PIAT", "ADJA", "ADJA", "NN", "$."], "meter": "