{"textgrid.poem.40088": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Oh wie sie ", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oh wie sie ", "tokens": ["Oh", "wie", "sie"], "token_info": ["word", "word", "word"], "pos": ["XY", "PWAV", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "da\u00df nicht zuviel der liebe N\u00e4chste nehme!", "tokens": ["da\u00df", "nicht", "zu\u00b7viel", "der", "lie\u00b7be", "N\u00e4chs\u00b7te", "neh\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "wie sie gewissenhaft den Handel lenken,", "tokens": ["wie", "sie", "ge\u00b7wis\u00b7sen\u00b7haft", "den", "Han\u00b7del", "len\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "da\u00df man nur recht der Menge sich bequeme!", "tokens": ["da\u00df", "man", "nur", "recht", "der", "Men\u00b7ge", "sich", "be\u00b7que\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADV", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O wie sie schelten, wenn mit tausend H\u00e4nden", "tokens": ["O", "wie", "sie", "schel\u00b7ten", ",", "wenn", "mit", "tau\u00b7send", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PWAV", "PPER", "VVFIN", "$,", "KOUS", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "aus meiner Brust mich selbst ich m\u00f6chte streuen,", "tokens": ["aus", "mei\u00b7ner", "Brust", "mich", "selbst", "ich", "m\u00f6ch\u00b7te", "streu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "ADV", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "um tausendfach mein Lieben auszuspenden,", "tokens": ["um", "tau\u00b7send\u00b7fach", "mein", "Lie\u00b7ben", "aus\u00b7zu\u00b7spen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "PPOSAT", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "um tausendfach mein Lieben zu erneuen!", "tokens": ["um", "tau\u00b7send\u00b7fach", "mein", "Lie\u00b7ben", "zu", "er\u00b7neu\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "PPOSAT", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nein, nein! ich ", "tokens": ["Nein", ",", "nein", "!", "ich"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["PTKANT", "$,", "PTKANT", "$.", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "ich ", "tokens": ["ich"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "eh' wollt' ich, meiner Liebe Fluten schl\u00fcgen", "tokens": ["eh'", "wollt'", "ich", ",", "mei\u00b7ner", "Lie\u00b7be", "Flu\u00b7ten", "schl\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "VMFIN", "PPER", "$,", "PPOSAT", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "emp\u00f6rt in St\u00fccken eure Bettelh\u00fctten!", "tokens": ["em\u00b7p\u00f6rt", "in", "St\u00fc\u00b7cken", "eu\u00b7re", "Bet\u00b7tel\u00b7h\u00fct\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}