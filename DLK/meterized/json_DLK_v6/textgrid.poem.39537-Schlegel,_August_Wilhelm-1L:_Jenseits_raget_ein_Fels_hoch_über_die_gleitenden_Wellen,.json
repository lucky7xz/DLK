 "tokens": ["S\u00fc\u00b7\u00dfe", "Ge\u00b7no\u00b7\u00dfin\u00b7nen", "mir", "!", "Ei\u00b7let", ",", "ihr", "al\u00b7le", ",", "her\u00b7bei", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "$.", "NN", "$,", "PPER", "PIS", "$,", "PTKVZ", "$."], "meter": "+---+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.39": {"text": "F\u00fchret den festlichen Reihn, webt Ehrengezelte von Laubwerk,", "tokens": ["F\u00fch\u00b7ret", "den", "fest\u00b7li\u00b7chen", "Reihn", ",", "webt", "Eh\u00b7ren\u00b7ge\u00b7zel\u00b7te", "von", "Laub\u00b7werk", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "VVFIN", "NN", "APPR", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.40": {"text": "Kn\u00fcpfet zu Tauen dem Schiff rankiges Rebengeflecht.", "tokens": ["Kn\u00fcp\u00b7fet", "zu", "Tau\u00b7en", "dem", "Schiff", "ran\u00b7ki\u00b7ges", "Re\u00b7ben\u00b7ge\u00b7flecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.41": {"text": "Auf! Trankopfer gespendet dem Genius unsers Gebieters!", "tokens": ["Auf", "!", "Tran\u00b7kop\u00b7fer", "ge\u00b7spen\u00b7det", "dem", "Ge\u00b7nius", "un\u00b7sers", "Ge\u00b7bie\u00b7ters", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "NN", "VVPP", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.42": {"text": "Auf! mit dem edelsten Na\u00df goldne Pokale gef\u00fcllt!", "tokens": ["Auf", "!", "mit", "dem", "e\u00b7dels\u00b7ten", "Na\u00df", "gold\u00b7ne", "Po\u00b7ka\u00b7le", "ge\u00b7f\u00fcllt", "!"], "token_info": ["word", "punct",