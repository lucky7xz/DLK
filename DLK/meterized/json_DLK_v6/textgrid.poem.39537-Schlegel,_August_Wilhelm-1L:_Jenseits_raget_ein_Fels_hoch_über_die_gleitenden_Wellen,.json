{"textgrid.poem.39537": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Jenseits raget ein Fels hoch \u00fcber die gleitenden Wellen,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jenseits raget ein Fels hoch \u00fcber die gleitenden Wellen,", "tokens": ["Jen\u00b7seits", "ra\u00b7get", "ein", "Fels", "hoch", "\u00fc\u00b7ber", "die", "glei\u00b7ten\u00b7den", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+---+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Burgenumth\u00fcrmt, wo einst grimmig ein Drache gehaust.", "tokens": ["Bur\u00b7gen\u00b7umt\u00b7h\u00fcrmt", ",", "wo", "einst", "grim\u00b7mig", "ein", "Dra\u00b7che", "ge\u00b7haust", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ADV", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.3": {"text": "Da liegt heimlich, am Ufer, des Rheins willkommenste Ruhstatt;", "tokens": ["Da", "liegt", "heim\u00b7lich", ",", "am", "U\u00b7fer", ",", "des", "Rheins", "will\u00b7kom\u00b7mens\u00b7te", "Ruh\u00b7statt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "$,", "APPRART", "NN", "$,", "ART", "NE", "ADJA", "NN", "$."], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Aus leichtbr\u00fcchigem Tuff w\u00f6lbet die Grotte sich ihm.", "tokens": ["Aus", "leicht\u00b7br\u00fc\u00b7chi\u00b7gem", "Tuff", "w\u00f6l\u00b7bet", "die", "Grot\u00b7te", "sich", "ihm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "PRF", "PPER", "$."], "meter": "--+--++--+-++", "measure": "anapaest.di.plus"}, "line.5": {"text": "Da pflegt gern in der K\u00fchle der Greis mitt\u00e4glichen Schlummers,", "tokens": ["Da", "pflegt", "gern", "in", "der", "K\u00fch\u00b7le", "der", "Greis", "mit\u00b7t\u00e4g\u00b7li\u00b7chen", "Schlum\u00b7mers", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "ART", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "W\u00e4hrend die schw\u00fclere Luft Traubengel\u00e4nder umweht.", "tokens": ["W\u00e4h\u00b7rend", "die", "schw\u00fc\u00b7le\u00b7re", "Luft", "Trau\u00b7ben\u00b7ge\u00b7l\u00e4n\u00b7der", "um\u00b7weht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "NN", "VVFIN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Aber best\u00fcrzt hebt pl\u00f6tzlich das Haupt von dem Lager der Flu\u00dfgott,", "tokens": ["A\u00b7ber", "be\u00b7st\u00fcrzt", "hebt", "pl\u00f6tz\u00b7lich", "das", "Haupt", "von", "dem", "La\u00b7ger", "der", "Flu\u00df\u00b7gott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ADJD", "ART", "NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Ruft, vorschauend, sodann aus dem umschattenden Schilf:", "tokens": ["Ruft", ",", "vor\u00b7schau\u00b7end", ",", "so\u00b7dann", "aus", "dem", "um\u00b7schat\u00b7ten\u00b7den", "Schilf", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVPP", "$,", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Welch ein Ger\u00e4usch, fernher sich verk\u00fcndigend? Will der gesalznen", "tokens": ["Welch", "ein", "Ge\u00b7r\u00e4usch", ",", "fern\u00b7her", "sich", "ver\u00b7k\u00fcn\u00b7di\u00b7gend", "?", "Will", "der", "ge\u00b7salz\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "ART", "NN", "$,", "ADJA", "PRF", "VVPP", "$.", "VMFIN", "ART", "ADJA"], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Tiefe Tyrann mein Reich fordern in stolzen Besitz!", "tokens": ["Tie\u00b7fe", "Ty\u00b7rann", "mein", "Reich", "for\u00b7dern", "in", "stol\u00b7zen", "Be\u00b7sitz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Siehe! der Strom schwoll auf von dem Schwung' umrollender R\u00e4der;", "tokens": ["Sie\u00b7he", "!", "der", "Strom", "schwoll", "auf", "von", "dem", "Schwung'", "um\u00b7rol\u00b7len\u00b7der", "R\u00e4\u00b7der", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "ART", "NN", "ADJD", "APPR", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Weithin zeichnet ein Streif sch\u00e4umender Wirbel die Bahn,", "tokens": ["Weit\u00b7hin", "zeich\u00b7net", "ein", "Streif", "sch\u00e4u\u00b7men\u00b7der", "Wir\u00b7bel", "die", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.13": {"text": "Ob an der Meerg\u00f6ttin Thronwagen gespannt die Delphine", "tokens": ["Ob", "an", "der", "Meer\u00b7g\u00f6t\u00b7tin", "Thron\u00b7wa\u00b7gen", "ge\u00b7spannt", "die", "Del\u00b7phi\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "NN", "VVPP", "ART", "NN"], "meter": "-+-+--+---+--+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Bald eintauchen, und bald b\u00e4umen den Nacken empor?", "tokens": ["Bald", "ein\u00b7tau\u00b7chen", ",", "und", "bald", "b\u00e4u\u00b7men", "den", "Na\u00b7cken", "em\u00b7por", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIZU", "$,", "KON", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Oder Neptuns Dreizack unb\u00e4ndige Rosse herantreibt,", "tokens": ["O\u00b7der", "Nep\u00b7tuns", "Drei\u00b7zack", "un\u00b7b\u00e4n\u00b7di\u00b7ge", "Ros\u00b7se", "her\u00b7ant\u00b7reibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.16": {"text": "Welche das bl\u00e4uliche Na\u00df stampfen mit Flo\u00dfen am Huf?", "tokens": ["Wel\u00b7che", "das", "bl\u00e4u\u00b7li\u00b7che", "Na\u00df", "stamp\u00b7fen", "mit", "Flo\u00b7\u00dfen", "am", "Huf", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "APPRART", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.17": {"text": "Sah ich es? t\u00e4uschet der Blick? Nicht spielendes Wild der Gew\u00e4\u00dfer,", "tokens": ["Sah", "ich", "es", "?", "t\u00e4u\u00b7schet", "der", "Blick", "?", "Nicht", "spie\u00b7len\u00b7des", "Wild", "der", "Ge\u00b7w\u00e4\u00b7\u00dfer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$.", "VVFIN", "ART", "NN", "$.", "PTKNEG", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.18": {"text": "Nicht vierf\u00fc\u00dfige Kraft ziehet das fremde Geschirr;", "tokens": ["Nicht", "vier\u00b7f\u00fc\u00b7\u00dfi\u00b7ge", "Kraft", "zie\u00b7het", "das", "frem\u00b7de", "Ge\u00b7schirr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.19": {"text": "Noch stellt Masten es auf, noch breitet es Segel den Winden;", "tokens": ["Noch", "stellt", "Mas\u00b7ten", "es", "auf", ",", "noch", "brei\u00b7tet", "es", "Se\u00b7gel", "den", "Win\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "PPER", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "NN", "ART", "NN", "$."], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.20": {"text": "W\u00fchlet die Flut nicht um, unter der Ruderer Schlag.", "tokens": ["W\u00fch\u00b7let", "die", "Flut", "nicht", "um", ",", "un\u00b7ter", "der", "Ru\u00b7de\u00b7rer", "Schlag", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "PTKVZ", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.21": {"text": "Sondern es eilt freiwillig dahin mit beweglichen Krei\u00dfen,", "tokens": ["Son\u00b7dern", "es", "eilt", "frei\u00b7wil\u00b7lig", "da\u00b7hin", "mit", "be\u00b7weg\u00b7li\u00b7chen", "Krei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "PAV", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+--+-", "measure": "iambic.septa.invert"}, "line.22": {"text": "Gleichwie d\u00e4dalische Kunst lebende Bilder geformt.", "tokens": ["Gleich\u00b7wie", "d\u00e4\u00b7da\u00b7li\u00b7sche", "Kunst", "le\u00b7ben\u00b7de", "Bil\u00b7der", "ge\u00b7formt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.23": {"text": "Aber ich seh' auch Wolken des Rauchs, hoch spr\u00fchende Funken:", "tokens": ["A\u00b7ber", "ich", "seh'", "auch", "Wol\u00b7ken", "des", "Rauchs", ",", "hoch", "spr\u00fc\u00b7hen\u00b7de", "Fun\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "NN", "ART", "NN", "$,", "ADJD", "ADJA", "NN", "$."], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.24": {"text": "Dr\u00e4nget der wilde Vulcan etwa die Wellen zur\u00fcck?", "tokens": ["Dr\u00e4n\u00b7get", "der", "wil\u00b7de", "Vul\u00b7can", "et\u00b7wa", "die", "Wel\u00b7len", "zu\u00b7r\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NE", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+--+----+-+--+", "measure": "dactylic.di.plus"}, "line.25": {"text": "Welch ein Heroen-Geschlecht, dem ", "tokens": ["Welch", "ein", "He\u00b7ro\u00b7en\u00b7Ge\u00b7schlecht", ",", "dem"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PIAT", "ART", "NN", "$,", "PRELS"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.26": {"text": "Welchem der Schiffahrt Lauf ordnete g\u00f6ttlicher Wink?", "tokens": ["Wel\u00b7chem", "der", "Schif\u00b7fahrt", "Lauf", "ord\u00b7ne\u00b7te", "g\u00f6tt\u00b7li\u00b7cher", "Wink", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "+--+-+---+--+", "measure": "iambic.penta.invert"}, "line.27": {"text": "Jetzo erkenn' ich und ehre die Huld in des Nahenden Antlitz.", "tokens": ["Jet\u00b7zo", "er\u00b7kenn'", "ich", "und", "eh\u00b7re", "die", "Huld", "in", "des", "Na\u00b7hen\u00b7den", "Ant\u00b7litz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "++-+--+--+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.28": {"text": "Heil Dir, K\u00f6nig! Noch nie trug ich so herrlichen Gast.", "tokens": ["Heil", "Dir", ",", "K\u00f6\u00b7nig", "!", "Noch", "nie", "trug", "ich", "so", "herr\u00b7li\u00b7chen", "Gast", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "NN", "$.", "ADV", "ADV", "VVFIN", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.29": {"text": "Um den Erhabenen rings steh'n hold aufbl\u00fchende Spro\u00dfen:", "tokens": ["Um", "den", "Er\u00b7ha\u00b7be\u00b7nen", "rings", "steh'n", "hold", "auf\u00b7bl\u00fc\u00b7hen\u00b7de", "Spro\u00b7\u00dfen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ADV", "VVFIN", "ADJD", "ADJA", "NN", "$."], "meter": "---+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "Wahrlich! es tr\u00e4gt die\u00df Schiff F\u00fclle der Wonne, des Ruhms.", "tokens": ["Wahr\u00b7lich", "!", "es", "tr\u00e4gt", "die\u00df", "Schiff", "F\u00fcl\u00b7le", "der", "Won\u00b7ne", ",", "des", "Ruhms", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VVFIN", "PDS", "NN", "NN", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.31": {"text": "Da\u00df ich ein gallischer Strom nicht blieb, Du hast es erk\u00e4mpfet;", "tokens": ["Da\u00df", "ich", "ein", "gal\u00b7li\u00b7scher", "Strom", "nicht", "blieb", ",", "Du", "hast", "es", "er\u00b7k\u00e4mp\u00b7fet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "PTKNEG", "VVFIN", "$,", "PPER", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.32": {"text": "Wenn Du, Held, mich beschirmst, w\u00e4lz' ich germanische Flut.", "tokens": ["Wenn", "Du", ",", "Held", ",", "mich", "be\u00b7schirmst", ",", "w\u00e4lz'", "ich", "ger\u00b7ma\u00b7ni\u00b7sche", "Flut", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "NN", "$,", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.33": {"text": "Gegen der R\u00f6mer Triumph', ich, vormals hemmende Schranke,", "tokens": ["Ge\u00b7gen", "der", "R\u00f6\u00b7mer", "Tri\u00b7um\u00b7ph'", ",", "ich", ",", "vor\u00b7mals", "hem\u00b7men\u00b7de", "Schran\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PPER", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.34": {"text": "Ach! was duldet' ich nicht j\u00fcngst in den Tagen der Schmach!", "tokens": ["Ach", "!", "was", "dul\u00b7det'", "ich", "nicht", "j\u00fcngst", "in", "den", "Ta\u00b