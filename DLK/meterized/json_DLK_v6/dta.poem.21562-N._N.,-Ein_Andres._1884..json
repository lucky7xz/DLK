{"dta.poem.21562": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Ein Andres.  \n 1884.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcnf wurmzernagte Stiegen geht\u2019s hinauf", "tokens": ["F\u00fcnf", "wurm\u00b7zer\u00b7nag\u00b7te", "Stie\u00b7gen", "geht's", "hin\u00b7auf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "ADJA", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ins letzte Stockwerk einer Miethskaserne;", "tokens": ["Ins", "letz\u00b7te", "Stock\u00b7werk", "ei\u00b7ner", "Mieths\u00b7ka\u00b7ser\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hier h\u00e4lt der Nordwind sich am liebsten auf,", "tokens": ["Hier", "h\u00e4lt", "der", "Nord\u00b7wind", "sich", "am", "liebs\u00b7ten", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "PTKA", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und durch das Dachwerk schaun des Himmels Sterne.", "tokens": ["Und", "durch", "das", "Dach\u00b7werk", "schaun", "des", "Him\u00b7mels", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Was sie ersp\u00e4hn, o, es ist grad genug,", "tokens": ["Was", "sie", "er\u00b7sp\u00e4hn", ",", "o", ",", "es", "ist", "grad", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVINF", "$,", "FM", "$,", "PPER", "VAFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Um mit dem Elend br\u00fcderlich zu weinen:", "tokens": ["Um", "mit", "dem", "E\u00b7lend", "br\u00fc\u00b7der\u00b7lich", "zu", "wei\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ein St\u00fcckchen Schwarzbrod und ein Wasserkrug,", "tokens": ["Ein", "St\u00fcck\u00b7chen", "Schwarz\u00b7brod", "und", "ein", "Was\u00b7ser\u00b7krug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ein Werktisch und ein Schemel mit drei Beinen!", "tokens": ["Ein", "Werk\u00b7tisch", "und", "ein", "Sche\u00b7mel", "mit", "drei", "Bei\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}