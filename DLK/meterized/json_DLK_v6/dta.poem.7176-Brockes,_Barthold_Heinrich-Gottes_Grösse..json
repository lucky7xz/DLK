{"dta.poem.7176": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Gottes Gr\u00f6sse.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hat ein Geist so ferne Schrancken;", "tokens": ["Hat", "ein", "Geist", "so", "fer\u00b7ne", "Schran\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist ein menschlichs Hertz so k\u00fchn;", "tokens": ["Ist", "ein", "menschlichs", "Hertz", "so", "k\u00fchn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Durch den Raum, nur in Gedancken,", "tokens": ["Durch", "den", "Raum", ",", "nur", "in", "Ge\u00b7dan\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eine Linie zu ziehn,", "tokens": ["Ei\u00b7ne", "Li\u00b7nie", "zu", "ziehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Und ihm bliebe dennoch dr\u00fcber", "tokens": ["Und", "ihm", "blie\u00b7be", "den\u00b7noch", "dr\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PAV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eine Krafft zu dencken \u00fcber;", "tokens": ["Ei\u00b7ne", "Krafft", "zu", "den\u00b7cken", "\u00fc\u00b7ber", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "APPR", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Der erwege, GOTT zum Preis,", "tokens": ["Der", "er\u00b7we\u00b7ge", ",", "GoTT", "zum", "Preis", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "NE", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Einmahl dieses Durchschnitts Creis.", "tokens": ["Ein\u00b7mahl", "die\u00b7ses", "Durch\u00b7schnitts", "Creis", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}