{"textgrid.poem.43231": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Unheimlich-fremd ist mir das letzte Blatt,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unheimlich-fremd ist mir das letzte Blatt,", "tokens": ["Un\u00b7heim\u00b7lich\u00b7fremd", "ist", "mir", "das", "letz\u00b7te", "Blatt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das ich im Fieberwahne schrieb vor Wochen,", "tokens": ["Das", "ich", "im", "Fie\u00b7ber\u00b7wah\u00b7ne", "schrieb", "vor", "Wo\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPRART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie schnell war jener tolle Muth gebrochen,", "tokens": ["Wie", "schnell", "war", "je\u00b7ner", "tol\u00b7le", "Muth", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PDAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "War Leib und Seele wieder krank und matt.", "tokens": ["War", "Leib", "und", "See\u00b7le", "wie\u00b7der", "krank", "und", "matt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch Du bist ", "tokens": ["Doch", "Du", "bist"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPER", "VAFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Mit leisen T\u00f6nen schluchz' ich es, mit herben:", "tokens": ["Mit", "lei\u00b7sen", "T\u00f6\u00b7nen", "schluch\u00b7z'", "ich", "es", ",", "mit", "her\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PPER", "$,", "APPR", "VVINF", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Du bist nun ", "tokens": ["Du", "bist", "nun"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Ich tr\u00e4ume nicht, ich bin gesund und wach. \u2013", "tokens": ["Ich", "tr\u00e4u\u00b7me", "nicht", ",", "ich", "bin", "ge\u00b7sund", "und", "wach", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PPER", "VAFIN", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und also t\u00f6dte ich den letzten Keim", "tokens": ["Und", "al\u00b7so", "t\u00f6d\u00b7te", "ich", "den", "letz\u00b7ten", "Keim"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Des Trostes, den ich hegt' seit langen Jahren,", "tokens": ["Des", "Tros\u00b7tes", ",", "den", "ich", "hegt'", "seit", "lan\u00b7gen", "Jah\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Darf nicht zur\u00fcck \u2013 kehrt nimmer wieder heim.", "tokens": ["Darf", "nicht", "zu\u00b7r\u00fcck", "\u2013", "kehrt", "nim\u00b7mer", "wie\u00b7der", "heim", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "PTKVZ", "$(", "VVFIN", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}