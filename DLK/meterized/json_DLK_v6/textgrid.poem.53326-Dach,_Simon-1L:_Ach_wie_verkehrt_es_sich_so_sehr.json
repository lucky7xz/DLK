{"textgrid.poem.53326": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach wie verkehrt es sich so sehr", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach wie verkehrt es sich so sehr", "tokens": ["Ach", "wie", "ver\u00b7kehrt", "es", "sich", "so", "sehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "VVFIN", "PPER", "PRF", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Himmel, Lufft vnd Erden?", "tokens": ["Mit", "Him\u00b7mel", ",", "Lufft", "vnd", "Er\u00b7den", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was ursach l\u00e4sst es jetzt nicht mehr,", "tokens": ["Was", "ur\u00b7sach", "l\u00e4sst", "es", "jetzt", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie vormahls, Winter werden:", "tokens": ["Wie", "vor\u00b7mahls", ",", "Win\u00b7ter", "wer\u00b7den", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "NN", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Hat Gott wo, sein Gesch\u00f6pff, die Welt", "tokens": ["Hat", "Gott", "wo", ",", "sein", "Ge\u00b7sch\u00f6pff", ",", "die", "Welt"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "NN", "PWAV", "$,", "PPOSAT", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Au\u00df seiner Auffsicht Hut gestellt,", "tokens": ["Au\u00df", "sei\u00b7ner", "Auff\u00b7sicht", "Hut", "ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Vnd sich f\u00fcr vns verborgen?", "tokens": ["Vnd", "sich", "f\u00fcr", "vns", "ver\u00b7bor\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Geht unbek\u00fcmmert ohn gefehr", "tokens": ["Geht", "un\u00b7be\u00b7k\u00fcm\u00b7mert", "ohn", "ge\u00b7fehr"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wo umb des Himmels umbgang her,", "tokens": ["Wo", "umb", "des", "Him\u00b7mels", "umb\u00b7gang", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Vnd wil f\u00fcr nichts mehr sorgen:", "tokens": ["Vnd", "wil", "f\u00fcr", "nichts", "mehr", "sor\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "PIS", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ist der Natur gehei\u00df vnd zwangk", "tokens": ["Ist", "der", "Na\u00b7tur", "ge\u00b7hei\u00df", "vnd", "zwangk"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ADJD", "KON", "XY"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Mehr nichts als ein Geschw\u00e4tze?", "tokens": ["Mehr", "nichts", "als", "ein", "Ge\u00b7schw\u00e4t\u00b7ze", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PIS", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vnd hat der Mond- vnd Sonnengangk", "tokens": ["Vnd", "hat", "der", "Mon\u00b7d", "vnd", "Son\u00b7nen\u00b7gangk"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "TRUNC", "KON", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nicht Z\u00fcgel noch Gesetze?", "tokens": ["Nicht", "Z\u00fc\u00b7gel", "noch", "Ge\u00b7set\u00b7ze", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wirstu mit deinem Bund, O Gott,", "tokens": ["Wirs\u00b7tu", "mit", "dei\u00b7nem", "Bund", ",", "O", "Gott", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "$,", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vnd deinem Worte nun zu spott?", "tokens": ["Vnd", "dei\u00b7nem", "Wor\u00b7te", "nun", "zu", "spott", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So lang die Welt wird stehen,", "tokens": ["So", "lang", "die", "Welt", "wird", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Soll, sagtest du: des Sommers pracht,", "tokens": ["Soll", ",", "sag\u00b7test", "du", ":", "des", "Som\u00b7mers", "pracht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "VVFIN", "PPER", "$.", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Des Winters-Frost, sampt Tag vnd Nacht", "tokens": ["Des", "Win\u00b7ter\u00b7sFrost", ",", "sampt", "Tag", "vnd", "Nacht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Stets Wechsel-richtig gehen.", "tokens": ["Stets", "Wech\u00b7sel\u00b7rich\u00b7tig", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nein, deine Satzung bleibt getrew,", "tokens": ["Nein", ",", "dei\u00b7ne", "Sat\u00b7zung", "bleibt", "ge\u00b7trew", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Recht aber zu ergr\u00fcnden,", "tokens": ["Recht", "a\u00b7ber", "zu", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Woher di\u00df weiche Wetter sey,", "tokens": ["Wo\u00b7her", "di\u00df", "wei\u00b7che", "Wet\u00b7ter", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es r\u00fchrt von unsern S\u00fcnden:", "tokens": ["Es", "r\u00fchrt", "von", "un\u00b7sern", "S\u00fcn\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Du richtest dich nach unserm Thun", "tokens": ["Du", "rich\u00b7test", "dich", "nach", "un\u00b7serm", "Thun"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vnd hast, wie allzeit so auch nun,", "tokens": ["Vnd", "hast", ",", "wie", "all\u00b7zeit", "so", "auch", "nun", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "PWAV", "ADV", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Vns gleichsam zu Gef\u00e4hrten.", "tokens": ["Vns", "gleich\u00b7sam", "zu", "Ge\u00b7f\u00e4hr\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Denn bey den Reinen pflegst du rein", "tokens": ["Denn", "bey", "den", "Rei\u00b7nen", "pflegst", "du", "rein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Vnd bey den Frommen fromm zu seyn,", "tokens": ["Vnd", "bey", "den", "From\u00b7men", "fromm", "zu", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Verkehrt bey den Verkehrten.", "tokens": ["Ver\u00b7kehrt", "bey", "den", "Ver\u00b7kehr\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "$."], "meter": "-+---+-", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Wir alle gehen unsern Pfadt", "tokens": ["Wir", "al\u00b7le", "ge\u00b7hen", "un\u00b7sern", "Pfadt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "PIS", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vnd auff verderbten Wegen,", "tokens": ["Vnd", "auff", "ver\u00b7derb\u00b7ten", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir folgen unsers Frevels Rath,", "tokens": ["Wir", "fol\u00b7gen", "un\u00b7sers", "Fre\u00b7vels", "Rath", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vnd wandeln dir entgegen:", "tokens": ["Vnd", "wan\u00b7deln", "dir", "ent\u00b7ge\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Predigt dr\u00e4wt uns Fluch vnd Bann,", "tokens": ["Die", "Pre\u00b7digt", "dr\u00e4wt", "uns", "Fluch", "vnd", "Bann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wir kehren uns nicht gro\u00df daran:", "tokens": ["Wir", "keh\u00b7ren", "uns", "nicht", "gro\u00df", "da\u00b7ran", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ADJD", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das Recht ist Vnrecht worden,", "tokens": ["Das", "Recht", "ist", "Vn\u00b7recht", "wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "VAPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Glaub hat weder Mund noch Hand,", "tokens": ["Der", "Glaub", "hat", "we\u00b7der", "Mund", "noch", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "KON", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die Lieb ist in Betrug gewandt", "tokens": ["Die", "Lieb", "ist", "in", "Be\u00b7trug", "ge\u00b7wandt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Bey jedem Stand vnd Orden.", "tokens": ["Bey", "je\u00b7dem", "Stand", "vnd", "Or\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die beste Kunst ist \u00fcberall", "tokens": ["Die", "bes\u00b7te", "Kunst", "ist", "\u00fc\u00b7be\u00b7rall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Nechsten k\u00f6nnen schertzen,", "tokens": ["Den", "Nechs\u00b7ten", "k\u00f6n\u00b7nen", "schert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Im Munde wohnt vns Honig, Gall", "tokens": ["Im", "Mun\u00b7de", "wohnt", "vns", "Ho\u00b7nig", ",", "Gall"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "NN", "$,", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Heuchlerischen Hertzen,", "tokens": ["Im", "Heuch\u00b7le\u00b7ri\u00b7schen", "Hert\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Geitz frist wie ein Krebs umb sich,", "tokens": ["Der", "Geitz", "frist", "wie", "ein", "Krebs", "umb", "sich", ","], "token_info": ["word", "word", "w