{"textgrid.poem.60366": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Zueignung", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von allen, die den S\u00e4nger lieben,", "tokens": ["Von", "al\u00b7len", ",", "die", "den", "S\u00e4n\u00b7ger", "lie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die, was ich f\u00fchlte, nachempfanden,", "tokens": ["Die", ",", "was", "ich", "f\u00fchl\u00b7te", ",", "na\u00b7ch\u00b7emp\u00b7fan\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "$,", "PWS", "PPER", "VVFIN", "$,", "VVIZU", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die es besprochen und beschrieben,", "tokens": ["Die", "es", "be\u00b7spro\u00b7chen", "und", "be\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hat niemand mich wie du verstanden.", "tokens": ["Hat", "nie\u00b7mand", "mich", "wie", "du", "ver\u00b7stan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PRF", "KOKOM", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Des Herzens Klagen hei\u00df und innig,", "tokens": ["Des", "Her\u00b7zens", "Kla\u00b7gen", "hei\u00df", "und", "in\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die liedgeworden ihm entklangen,", "tokens": ["Die", "lied\u00b7ge\u00b7wor\u00b7den", "ihm", "ent\u00b7klan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hat deine Seele, tief und sinnig,", "tokens": ["Hat", "dei\u00b7ne", "See\u00b7le", ",", "tief", "und", "sin\u00b7nig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Getreuer als mein Lied empfangen.", "tokens": ["Ge\u00b7treu\u00b7er", "als", "mein", "Lied", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Schauer, die mein Herz durchwehten,", "tokens": ["Die", "Schau\u00b7er", ",", "die", "mein", "Herz", "durch\u00b7weh\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die unerfa\u00dflich meinem S\u00e4nge,", "tokens": ["Die", "un\u00b7er\u00b7fa\u00df\u00b7lich", "mei\u00b7nem", "S\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie sprachen, tr\u00f6stende Propheten,", "tokens": ["Sie", "spra\u00b7chen", ",", "tr\u00f6s\u00b7ten\u00b7de", "Pro\u00b7phe\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In deines Wortes s\u00fc\u00dfem Klange.", "tokens": ["In", "dei\u00b7nes", "Wor\u00b7tes", "s\u00fc\u00b7\u00dfem", "Klan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und d\u00fcrft ich ahnend in den Bronnen", "tokens": ["Und", "d\u00fcrft", "ich", "ah\u00b7nend", "in", "den", "Bron\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der g\u00f6ttlichen Gedanken sinken,", "tokens": ["Der", "g\u00f6tt\u00b7li\u00b7chen", "Ge\u00b7dan\u00b7ken", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So sah ich klar die dunklen Wonnen", "tokens": ["So", "sah", "ich", "klar", "die", "dunk\u00b7len", "Won\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In deinem sch\u00f6nen Auge blinken.", "tokens": ["In", "dei\u00b7nem", "sch\u00f6\u00b7nen", "Au\u00b7ge", "blin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der Himmel taut in finstern Hainen", "tokens": ["Der", "Him\u00b7mel", "taut", "in", "fins\u00b7tern", "Hai\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Lied der Nachtigallen nieder,", "tokens": ["Zum", "Lied", "der", "Nach\u00b7ti\u00b7gal\u00b7len", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und deine Augen sah ich weinen", "tokens": ["Und", "dei\u00b7ne", "Au\u00b7gen", "sah", "ich", "wei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Herab auf meine bangen Lieder.", "tokens": ["Her\u00b7ab", "auf", "mei\u00b7ne", "ban\u00b7gen", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Seh ich der Augen Zauberkreise", "tokens": ["Seh", "ich", "der", "Au\u00b7gen", "Zau\u00b7ber\u00b7krei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gesenkt, geschwellt, in trauter N\u00e4he,", "tokens": ["Ge\u00b7senkt", ",", "ge\u00b7schwellt", ",", "in", "trau\u00b7ter", "N\u00e4\u00b7he", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ists, ob ich deine Seele leise", "tokens": ["Ists", ",", "ob", "ich", "dei\u00b7ne", "See\u00b7le", "lei\u00b7se"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "PPER", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Luft der Tugend atmen sehe.", "tokens": ["Die", "Luft", "der", "Tu\u00b7gend", "at\u00b7men", "se\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Dein ist mein Herz, mein Schmerz dein eigen,", "tokens": ["Dein", "ist", "mein", "Herz", ",", "mein", "Schmerz", "dein", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "PPOSAT", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und alle Freuden, die es sprengen,", "tokens": ["Und", "al\u00b7le", "Freu\u00b7den", ",", "die", "es", "spren\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dein ist der Wald mit allen Zweigen,", "tokens": ["Dein", "ist", "der", "Wald", "mit", "al\u00b7len", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit allen Bl\u00fcten und Ges\u00e4ngen.", "tokens": ["Mit", "al\u00b7len", "Bl\u00fc\u00b7ten", "und", "Ge\u00b7s\u00e4n\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Das Liebste, was ich mag erbeuten", "tokens": ["Das", "Liebs\u00b7te", ",", "was", "ich", "mag", "er\u00b7beu\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWS", "PPER", "VMFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Liedern, die mein Herz entf\u00fchrten,", "tokens": ["Mit", "Lie\u00b7dern", ",", "die", "mein", "Herz", "ent\u00b7f\u00fchr\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ist mir ein Wort, da\u00df sie dich freuten,", "tokens": ["Ist", "mir", "ein", "Wort", ",", "da\u00df", "sie", "dich", "freu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein stummer Blick, da\u00df sie dich r\u00fchrten.", "tokens": ["Ein", "stum\u00b7mer", "Blick", ",", "da\u00df", "sie", "dich", "r\u00fchr\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Und sollt ich nach dem hellen Ruhme", "tokens": ["Und", "sollt", "ich", "nach", "dem", "hel\u00b7len", "Ruh\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mich manchmal auch am Wege b\u00fccken,", "tokens": ["Mich", "manch\u00b7mal", "auch", "am", "We\u00b7ge", "b\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So will ich mit der sch\u00f6nen Blume", "tokens": ["So", "will", "ich", "mit", "der", "sch\u00f6\u00b7nen", "Blu\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur, Freundin, dir den Busen schm\u00fccken.", "tokens": ["Nur", ",", "Freun\u00b7din", ",", "dir", "den", "Bu\u00b7sen", "schm\u00fc\u00b7cken", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}