{"textgrid.poem.54655": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "69. Auff meine Geburt zu Gott", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du lissest mich/ da ich nichts war/ geboren werden:", "tokens": ["Du", "lis\u00b7sest", "mich", "/", "da", "ich", "nichts", "war", "/", "ge\u00b7bo\u00b7ren", "wer\u00b7den", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$(", "KOUS", "PPER", "PIS", "VAFIN", "$(", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ach la\u00df mich nicht in nichts nun ich was bin vergehn!", "tokens": ["Ach", "la\u00df", "mich", "nicht", "in", "nichts", "nun", "ich", "was", "bin", "ver\u00b7gehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVIMP", "PPER", "PTKNEG", "APPR", "PIS", "ADV", "PPER", "PIS", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du ruftest mich aus nichts mein Sch\u00f6pfer auf die Erden:", "tokens": ["Du", "ruf\u00b7test", "mich", "aus", "nichts", "mein", "Sch\u00f6p\u00b7fer", "auf", "die", "Er\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PIS", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "La\u00df/ nun ich etwas mir den Himmel offen stehn.", "tokens": ["La\u00df", "/", "nun", "ich", "et\u00b7was", "mir", "den", "Him\u00b7mel", "of\u00b7fen", "stehn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$(", "ADV", "PPER", "ADV", "PPER", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}