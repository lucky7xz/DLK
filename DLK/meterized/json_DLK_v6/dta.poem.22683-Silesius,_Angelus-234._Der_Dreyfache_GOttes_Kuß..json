{"dta.poem.22683": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "234. Der Dreyfache GOttes Ku\u00df.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Drey St\u00e4nde k\u00fcssen GOtt: die M\u00e4gde falln zun", "tokens": ["Drey", "St\u00e4n\u00b7de", "k\u00fcs\u00b7sen", "Gott", ":", "die", "M\u00e4g\u00b7de", "falln", "zun"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "NN", "$.", "ART", "NN", "VVINF", "VVINF"], "meter": "-+-+-+-+---", "measure": "unknown.measure.tetra"}, "line.2": {"text": "F\u00fcssen/", "tokens": ["F\u00fcs\u00b7sen", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Die Jungfern nahen sich die milde Hand zuk\u00fcssen.", "tokens": ["Die", "Jung\u00b7fern", "na\u00b7hen", "sich", "die", "mil\u00b7de", "Hand", "zu\u00b7k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "PRF", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Braut so gantz und gar von seiner Lieb ist Wund", "tokens": ["Die", "Braut", "so", "gantz", "und", "gar", "von", "sei\u00b7ner", "Lieb", "ist", "Wund"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ADV", "KON", "ADV", "APPR", "PPOSAT", "NN", "VAFIN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die liegt an seiner Brust und k\u00fcst den H\u00f6nig Mund.", "tokens": ["Die", "liegt", "an", "sei\u00b7ner", "Brust", "und", "k\u00fcst", "den", "H\u00f6\u00b7nig", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "NN", "KON", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}