{"textgrid.poem.39483": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Redner buhlte mit Sophistenschlingen", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Redner buhlte mit Sophistenschlingen", "tokens": ["Ein", "Red\u00b7ner", "buhl\u00b7te", "mit", "So\u00b7phis\u00b7ten\u00b7schlin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Wortgespinnst, die Poesie zu fangen.", "tokens": ["Und", "Wort\u00b7ge\u00b7spinnst", ",", "die", "Poe\u00b7sie", "zu", "fan\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ihm galten nur die Worte, was sie klangen,", "tokens": ["Ihm", "gal\u00b7ten", "nur", "die", "Wor\u00b7te", ",", "was", "sie", "klan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und eitel lie\u00df er Wortget\u00e4ndel klingen.", "tokens": ["Und", "ei\u00b7tel", "lie\u00df", "er", "Wort\u00b7ge\u00b7t\u00e4n\u00b7del", "klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da wandte sie vor seinem schlauen Dringen", "tokens": ["Da", "wand\u00b7te", "sie", "vor", "sei\u00b7nem", "schlau\u00b7en", "Drin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich zu des Rhythmus m\u00e4nnlichem Verlangen;", "tokens": ["Sich", "zu", "des", "Rhyth\u00b7mus", "m\u00e4nn\u00b7li\u00b7chem", "Ver\u00b7lan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit reiner Inbrunst hielt er sie umfangen,", "tokens": ["Mit", "rei\u00b7ner", "In\u00b7brunst", "hielt", "er", "sie", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und beider Lust ward ein harmonisch Singen.", "tokens": ["Und", "bei\u00b7der", "Lust", "ward", "ein", "har\u00b7mo\u00b7nisch", "Sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der Redespieler h\u00e4rmte sich verla\u00dfen,", "tokens": ["Der", "Re\u00b7de\u00b7spie\u00b7ler", "h\u00e4rm\u00b7te", "sich", "ver\u00b7la\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ward blo\u00dfe Stimme, doch nicht leer und nichtig:", "tokens": ["Ward", "blo\u00b7\u00dfe", "Stim\u00b7me", ",", "doch", "nicht", "leer", "und", "nich\u00b7tig", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$,", "ADV", "PTKNEG", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es schien, da\u00df Lieb' in jedem Laut noch keime.", "tokens": ["Es", "schien", ",", "da\u00df", "Lieb'", "in", "je\u00b7dem", "Laut", "noch", "kei\u00b7me", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "NN", "APPR", "PIAT", "APPR", "ADV", "PIAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nun wandelt Poesie in Mild' ihr Ha\u00dfen,", "tokens": ["Nun", "wan\u00b7delt", "Poe\u00b7sie", "in", "Mild'", "ihr", "Ha\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "APPR", "NE", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und sie vernimmt, ruft sie ihn nun sehns\u00fcchtig,", "tokens": ["Und", "sie", "ver\u00b7nimmt", ",", "ruft", "sie", "ihn", "nun", "sehn\u00b7s\u00fcch\u00b7tig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+--++-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Echo des Sinnes, zarte s\u00fc\u00dfe Reime.", "tokens": ["E\u00b7cho", "des", "Sin\u00b7nes", ",", "zar\u00b7te", "s\u00fc\u00b7\u00dfe", "Rei\u00b7me", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADJA", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}}}}