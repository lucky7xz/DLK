{"textgrid.poem.39698": {"metadata": {"author": {"name": "G\u00fcnderode, Karoline von", "birth": "N.A.", "death": "N.A."}, "title": "\u00dcberall Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kann ich im Herzen hei\u00dfe W\u00fcnsche tragen?", "tokens": ["Kann", "ich", "im", "Her\u00b7zen", "hei\u00b7\u00dfe", "W\u00fcn\u00b7sche", "tra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dabei des Lebens Bl\u00fcthenkr\u00e4nze sehn,", "tokens": ["Da\u00b7bei", "des", "Le\u00b7bens", "Bl\u00fc\u00b7then\u00b7kr\u00e4n\u00b7ze", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und unbekr\u00e4nzt daran vor\u00fcber gehn", "tokens": ["Und", "un\u00b7be\u00b7kr\u00e4nzt", "da\u00b7ran", "vor\u00b7\u00fc\u00b7ber", "gehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "PAV", "ADV", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und mu\u00df ich traurend nicht in mir verzagen?", "tokens": ["Und", "mu\u00df", "ich", "trau\u00b7rend", "nicht", "in", "mir", "ver\u00b7za\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADJD", "PTKNEG", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Soll frevelnd ich dem liebsten Wunsch entsagen?", "tokens": ["Soll", "fre\u00b7velnd", "ich", "dem", "liebs\u00b7ten", "Wunsch", "ent\u00b7sa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVPP", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Soll muthig ich zum Schattenreiche gehn?", "tokens": ["Soll", "mut\u00b7hig", "ich", "zum", "Schat\u00b7ten\u00b7rei\u00b7che", "gehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Um andre Freuden andre G\u00f6tter flehn,", "tokens": ["Um", "and\u00b7re", "Freu\u00b7den", "and\u00b7re", "G\u00f6t\u00b7ter", "flehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nach neuen Wonnen bei den Todten fragen?", "tokens": ["Nach", "neu\u00b7en", "Won\u00b7nen", "bei", "den", "Tod\u00b7ten", "fra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich stieg hinab, doch auch in Plutons Reichen,", "tokens": ["Ich", "stieg", "hin\u00b7ab", ",", "doch", "auch", "in", "Plu\u00b7tons", "Rei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "ADV", "ADV", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Schoo\u00df der N\u00e4chte, brennt der Liebe Glut", "tokens": ["Im", "Schoo\u00df", "der", "N\u00e4ch\u00b7te", ",", "brennt", "der", "Lie\u00b7be", "Glut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df sehnend Schatten sich zu Schatten neigen.", "tokens": ["Da\u00df", "seh\u00b7nend", "Schat\u00b7ten", "sich", "zu", "Schat\u00b7ten", "nei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "NN", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Verlohren ist wen Liebe nicht begl\u00fccket,", "tokens": ["Ver\u00b7loh\u00b7ren", "ist", "wen", "Lie\u00b7be", "nicht", "be\u00b7gl\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIAT", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und stieg er auch hinab zur styg'schen Flut,", "tokens": ["Und", "stieg", "er", "auch", "hin\u00b7ab", "zur", "sty\u00b7g'\u00b7schen", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Im Glanz der Himmel blieb er unentz\u00fccket.", "tokens": ["Im", "Glanz", "der", "Him\u00b7mel", "blieb", "er", "un\u00b7ent\u00b7z\u00fc\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}