{"textgrid.poem.64343": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn des Abends Rosenfl\u00fcgel", "genre": "verse", "period": "N.A.", "pub_year": 1779, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn des Abends Rosenfl\u00fcgel", "tokens": ["Wenn", "des", "A\u00b7bends", "Ro\u00b7sen\u00b7fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00fchlend \u00fcber Thal und H\u00fcgel,", "tokens": ["K\u00fch\u00b7lend", "\u00fc\u00b7ber", "Thal", "und", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00dcber Wald und Wiese schwebt,", "tokens": ["\u00dc\u00b7ber", "Wald", "und", "Wie\u00b7se", "schwebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn der Tau die B\u00e4ume tr\u00e4nket,", "tokens": ["Wenn", "der", "Tau", "die", "B\u00e4u\u00b7me", "tr\u00e4n\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sich in bunte Blumen senket,", "tokens": ["Sich", "in", "bun\u00b7te", "Blu\u00b7men", "sen\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und an jungen \u00c4hren bebt;", "tokens": ["Und", "an", "jun\u00b7gen", "\u00c4h\u00b7ren", "bebt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Wenn im Schalle heller Glocken", "tokens": ["Wenn", "im", "Schal\u00b7le", "hel\u00b7ler", "Glo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heimw\u00e4rts sich die Schafe locken,", "tokens": ["Heim\u00b7w\u00e4rts", "sich", "die", "Scha\u00b7fe", "lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und im Gehn das L\u00e4mmchen saugt,", "tokens": ["Und", "im", "Gehn", "das", "L\u00e4mm\u00b7chen", "saugt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Wenn das Gei\u00dfblatt s\u00fc\u00dfe D\u00fcfte", "tokens": ["Wenn", "das", "Gei\u00df\u00b7blatt", "s\u00fc\u00b7\u00dfe", "D\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In dem Wehen leiser L\u00fcfte", "tokens": ["In", "dem", "We\u00b7hen", "lei\u00b7ser", "L\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Labend mir entgegen haucht;", "tokens": ["La\u00b7bend", "mir", "ent\u00b7ge\u00b7gen", "haucht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKVZ", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn die schweren K\u00fche br\u00fcllen,", "tokens": ["Wenn", "die", "schwe\u00b7ren", "K\u00fc\u00b7he", "br\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gern die blanken Eimer f\u00fcllen,", "tokens": ["Gern", "die", "blan\u00b7ken", "Ei\u00b7mer", "f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Dirne melkend singt,", "tokens": ["Und", "die", "Dir\u00b7ne", "mel\u00b7kend", "singt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dann auf ihrem bunten Kranze,", "tokens": ["Dann", "auf", "ih\u00b7rem", "bun\u00b7ten", "Kran\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Leicht, als schwebte sie im Tanze,", "tokens": ["Leicht", ",", "als", "schweb\u00b7te", "sie", "im", "Tan\u00b7ze", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "S\u00fc\u00dfe Milch nach Hause bringt;", "tokens": ["S\u00fc\u00b7\u00dfe", "Milch", "nach", "Hau\u00b7se", "bringt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wenn die Erlen duftend s\u00e4useln,", "tokens": ["Wenn", "die", "Er\u00b7len", "duf\u00b7tend", "s\u00e4u\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn die M\u00fccken Teiche kr\u00e4useln,", "tokens": ["Wenn", "die", "M\u00fc\u00b7cken", "Tei\u00b7che", "kr\u00e4u\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn der Frosch sich qu\u00e4kend bl\u00e4ht,", "tokens": ["Wenn", "der", "Frosch", "sich", "qu\u00e4\u00b7kend", "bl\u00e4ht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn der Fisch im Wasser h\u00fcpfet,", "tokens": ["Wenn", "der", "Fisch", "im", "Was\u00b7ser", "h\u00fcp\u00b7fet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aus der kalten Tiefe schl\u00fcpfet,", "tokens": ["Aus", "der", "kal\u00b7ten", "Tie\u00b7fe", "schl\u00fcp\u00b7fet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Aus der Schwan zu Neste geht;", "tokens": ["Aus", "der", "Schwan", "zu", "Nes\u00b7te", "geht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NE", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wenn im Nachtigallenthale", "tokens": ["Wenn", "im", "Nach\u00b7ti\u00b7gal\u00b7len\u00b7tha\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hesper mit verliebtem Strahle", "tokens": ["Hes\u00b7per", "mit", "ver\u00b7lieb\u00b7tem", "Strah\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Heimlich meine Quelle k\u00fc\u00dft,", "tokens": ["Heim\u00b7lich", "mei\u00b7ne", "Quel\u00b7le", "k\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn, wie eine Braut err\u00f6tend,", "tokens": ["Wenn", ",", "wie", "ei\u00b7ne", "Braut", "er\u00b7r\u00f6\u00b7tend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWAV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Luna freundlich k\u00f6mmt, und fl\u00f6tend", "tokens": ["Lu\u00b7na", "freund\u00b7lich", "k\u00f6mmt", ",", "und", "fl\u00f6\u00b7tend"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "ADJD", "VVFIN", "$,", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Philomele sie begr\u00fc\u00dft:", "tokens": ["Phi\u00b7lo\u00b7me\u00b7le", "sie", "be\u00b7gr\u00fc\u00dft", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Dann umschweben s\u00fc\u00dfe Freuden,", "tokens": ["Dann", "um\u00b7schwe\u00b7ben", "s\u00fc\u00b7\u00dfe", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hand in Hand mit stillem Leiden,", "tokens": ["Hand", "in", "Hand", "mit", "stil\u00b7lem", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Meinen Geist, mein Auge weint;", "tokens": ["Mei\u00b7nen", "Geist", ",", "mein", "Au\u00b7ge", "weint", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn die Thr\u00e4n' in Lunas Schimmer", "tokens": ["Wenn", "die", "Thr\u00e4n'", "in", "Lu\u00b7nas", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "NE", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bebet, wei\u00df ich selbst nicht immer,", "tokens": ["Be\u00b7bet", ",", "wei\u00df", "ich", "selbst", "nicht", "im\u00b7mer", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "$,"], "meter": "+-+-++-+", "measure": "unknown.measure.penta"}, "line.6": {"text": "Was die stille Thr\u00e4ne meint?", "tokens": ["Was", "die", "stil\u00b7le", "Thr\u00e4\u00b7ne", "meint", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Manche nannt' ich Freudethr\u00e4nen,", "tokens": ["Man\u00b7che", "nannt'", "ich", "Freu\u00b7det\u00b7hr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die vielleicht geheimes Sehnen", "tokens": ["Die", "viel\u00b7leicht", "ge\u00b7hei\u00b7mes", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dem get\u00e4uschten Auge stahl,", "tokens": ["Dem", "ge\u00b7t\u00e4uschten", "Au\u00b7ge", "stahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Mancher leise Wunsch entbebte", "tokens": ["Man\u00b7cher", "lei\u00b7se", "Wunsch", "ent\u00b7beb\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Seufzend meiner Brust, und schwebte", "tokens": ["Seuf\u00b7zend", "mei\u00b7ner", "Brust", ",", "und", "schweb\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "$,", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ungesehn im Mondenstrahl.", "tokens": ["Un\u00b7ge\u00b7sehn", "im", "Mon\u00b7dens\u00b7trahl", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Ich beschw\u00f6r' euch, s\u00fc\u00dfe D\u00fcfte,", "tokens": ["Ich", "be\u00b7schw\u00f6r'", "euch", ",", "s\u00fc\u00b7\u00dfe", "D\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich beschw\u00f6r' euch, k\u00fchle L\u00fcfte,", "tokens": ["Ich", "be\u00b7schw\u00f6r'", "euch", ",", "k\u00fch\u00b7le", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hesper, Luna, Nachtigall,", "tokens": ["Hes\u00b7per", ",", "Lu\u00b7na", ",", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sagt, beschleichet dieses Sehnen", "tokens": ["Sagt", ",", "be\u00b7schlei\u00b7chet", "die\u00b7ses", "Seh\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mich allein mit solchen Thr\u00e4nen", "tokens": ["Mich", "al\u00b7lein", "mit", "sol\u00b7chen", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Im geheimen Mondenstrahl? \u2013", "tokens": ["Im", "ge\u00b7hei\u00b7men", "Mon\u00b7dens\u00b7trahl", "?", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPRART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "M\u00e4dchen, frage nicht die L\u00fcfte;", "tokens": ["M\u00e4d\u00b7chen", ",", "fra\u00b7ge", "nicht", "die", "L\u00fcf\u00b7te", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PTKNEG", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00e4dchen, nicht die Abendd\u00fcfte,", "tokens": ["M\u00e4d\u00b7chen", ",", "nicht", "die", "A\u00b7bend\u00b7d\u00fcf\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hesper, Luna, Nachtigall", "tokens": ["Hes\u00b7per", ",", "Lu\u00b7na", ",", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NE", "$,", "NE", "$,", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fchlen deiner Seele Sehnen,", "tokens": ["F\u00fch\u00b7len", "dei\u00b7ner", "See\u00b7le", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "K\u00f6nnen deuten deine Thr\u00e4nen", "tokens": ["K\u00f6n\u00b7nen", "deu\u00b7ten", "dei\u00b7ne", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Im geheimen Mondenstrahl.", "tokens": ["Im", "ge\u00b7hei\u00b7men", "Mon\u00b7dens\u00b7trahl", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Ich nur kann's! ich kann's, du S\u00fc\u00dfe!", "tokens": ["Ich", "nur", "kann's", "!", "ich", "kann's", ",", "du", "S\u00fc\u00b7\u00dfe", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "$.", "PPER", "VMFIN", "$,", "PPER", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00e4dchen, eil' in meine K\u00fcsse!", "tokens": ["M\u00e4d\u00b7chen", ",", "eil'", "in", "mei\u00b7ne", "K\u00fcs\u00b7se", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sauge Lieb' um Liebe ein!", "tokens": ["Sau\u00b7ge", "Lieb'", "um", "Lie\u00b7be", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wer da einsam will genie\u00dfen,", "tokens": ["We