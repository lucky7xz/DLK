{"textgrid.poem.62552": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Am Gr\u00fcndonnerstage", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sollt' ich klagen, wenn die Meinen", "tokens": ["Sollt'", "ich", "kla\u00b7gen", ",", "wenn", "die", "Mei\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine Liebe nicht verstehn?", "tokens": ["Mei\u00b7ne", "Lie\u00b7be", "nicht", "ver\u00b7stehn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn sie mich zu hassen scheinen,", "tokens": ["Wenn", "sie", "mich", "zu", "has\u00b7sen", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Oder kalt vor\u00fcbergehn?", "tokens": ["O\u00b7der", "kalt", "vor\u00b7\u00fc\u00b7ber\u00b7gehn", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mu\u00dftest du doch unter deinen", "tokens": ["Mu\u00df\u00b7test", "du", "doch", "un\u00b7ter", "dei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "PPOSAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auserw\u00e4hlten, reinen Seelen,", "tokens": ["Au\u00b7ser\u00b7w\u00e4hl\u00b7ten", ",", "rei\u00b7nen", "See\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Heiland, den Verr\u00e4ther z\u00e4hlen.", "tokens": ["Hei\u00b7land", ",", "den", "Ver\u00b7r\u00e4\u00b7ther", "z\u00e4h\u00b7len", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach, und was hast du begangen,", "tokens": ["Ach", ",", "und", "was", "hast", "du", "be\u00b7gan\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KON", "PWS", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Der die ganze Welt geliebt,", "tokens": ["Der", "die", "gan\u00b7ze", "Welt", "ge\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ein Mensch, den du umfangen,", "tokens": ["Da\u00df", "ein", "Mensch", ",", "den", "du", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Solche That an dir ver\u00fcbt?", "tokens": ["Sol\u00b7che", "That", "an", "dir", "ver\u00b7\u00fcbt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Kann ich mehr als du verlangen,", "tokens": ["Kann", "ich", "mehr", "als", "du", "ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ich, der schlechteste im Orden,", "tokens": ["Ich", ",", "der", "schlech\u00b7tes\u00b7te", "im", "Or\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "ADJA", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Was dem Meister nicht geworden?", "tokens": ["Was", "dem", "Meis\u00b7ter", "nicht", "ge\u00b7wor\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "PTKNEG", "VAPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}