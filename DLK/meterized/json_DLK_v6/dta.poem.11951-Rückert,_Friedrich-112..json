{"dta.poem.11951": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "112.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn dein Gem\u00fct ist frisch vom Thau der Nacht befeuchtet,", "tokens": ["Wenn", "dein", "Ge\u00b7m\u00fct", "ist", "frisch", "vom", "Thau", "der", "Nacht", "be\u00b7feuch\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VAFIN", "ADJD", "APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und deine Seele klar vom Morgenglanz durchleuchtet;", "tokens": ["Und", "dei\u00b7ne", "See\u00b7le", "klar", "vom", "Mor\u00b7gen\u00b7glanz", "durch\u00b7leuch\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So schwinge mit Vertraun in Andacht dich empor,", "tokens": ["So", "schwin\u00b7ge", "mit", "Ver\u00b7traun", "in", "An\u00b7dacht", "dich", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "APPR", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und trage dein Gebet dem Herrn der Sch\u00f6pfung vor!", "tokens": ["Und", "tra\u00b7ge", "dein", "Ge\u00b7bet", "dem", "Herrn", "der", "Sch\u00f6p\u00b7fung", "vor", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ein Vaterauge schaut, es h\u00f6rt ein Vaterohr;", "tokens": ["Ein", "Va\u00b7te\u00b7rau\u00b7ge", "schaut", ",", "es", "h\u00f6rt", "ein", "Va\u00b7te\u00b7rohr", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihm trage dein Gebet mit aller Sch\u00f6pfung vor!", "tokens": ["Ihm", "tra\u00b7ge", "dein", "Ge\u00b7bet", "mit", "al\u00b7ler", "Sch\u00f6p\u00b7fung", "vor", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Zum Himmel aufwerts blickt und ruft der Wesen Chor;", "tokens": ["Zum", "Him\u00b7mel", "auf\u00b7werts", "blickt", "und", "ruft", "der", "We\u00b7sen", "Chor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "KON", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nun trage dein Gebet mit Blick und Worten vor!", "tokens": ["Nun", "tra\u00b7ge", "dein", "Ge\u00b7bet", "mit", "Blick", "und", "Wor\u00b7ten", "vor", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Den W\u00fcnschen aufgethan ist der Erh\u00f6rung Thor;", "tokens": ["Den", "W\u00fcn\u00b7schen", "auf\u00b7ge\u00b7than", "ist", "der", "Er\u00b7h\u00f6\u00b7rung", "Thor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O trage dein Gebet in frommen W\u00fcnschen vor!", "tokens": ["O", "tra\u00b7ge", "dein", "Ge\u00b7bet", "in", "from\u00b7men", "W\u00fcn\u00b7schen", "vor", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}