{"textgrid.poem.48320": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "3. Adlig Begr\u00e4bnis", "genre": "verse", "period": "N.A.", "pub_year": 1888, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Zugwind ging durch die Stuben,", "tokens": ["Ein", "Zug\u00b7wind", "ging", "durch", "die", "Stu\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Auf standen Hall' und Tor,", "tokens": ["Auf", "stan\u00b7den", "Hall'", "und", "Tor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als die Mittelm\u00e4rk'schen begruben", "tokens": ["Als", "die", "Mit\u00b7tel\u00b7m\u00e4rk'\u00b7schen", "be\u00b7gru\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVINF"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Ihren alten Otto von Rohr.", "tokens": ["Ih\u00b7ren", "al\u00b7ten", "Ot\u00b7to", "von", "Rohr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NE", "APPR", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Sechs Rohrsche Vettern ihn tragen,", "tokens": ["Sechs", "Rohr\u00b7sche", "Vet\u00b7tern", "ihn", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sechs andre nebenher,", "tokens": ["Sechs", "and\u00b7re", "ne\u00b7ben\u00b7her", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "PIS", "ADV", "$,"], "meter": "+-+---", "measure": "unknown.measure.di"}, "line.3": {"text": "Dann folgen drei von der Hagen", "tokens": ["Dann", "fol\u00b7gen", "drei", "von", "der", "Ha\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "CARD", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und drei von H\u00e4seler.", "tokens": ["Und", "drei", "von", "H\u00e4\u00b7se\u00b7ler", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ein Ribbeck, ein Stechow, ein Zieten,", "tokens": ["Ein", "Rib\u00b7beck", ",", "ein", "Stec\u00b7how", ",", "ein", "Zie\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NE", "$,", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ein Rathenow, ein Quast,", "tokens": ["Ein", "Ra\u00b7the\u00b7now", ",", "ein", "Quast", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vor\u00fcber an Scheunen und Mieten", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "an", "Scheu\u00b7nen", "und", "Mie\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Auf den Schultern schwankt die Last.", "tokens": ["Auf", "den", "Schul\u00b7tern", "schwankt", "die", "Last", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Um den Kirchhof her ein Blitzen", "tokens": ["Um", "den", "Kirch\u00b7hof", "her", "ein", "Blit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "APZR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von Herbstessonnenschein,", "tokens": ["Von", "Herbs\u00b7tes\u00b7son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die roten Berberitzen", "tokens": ["Die", "ro\u00b7ten", "Ber\u00b7be\u00b7rit\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "H\u00e4ngen \u00fcber Mauer und Stein.", "tokens": ["H\u00e4n\u00b7gen", "\u00fc\u00b7ber", "Mau\u00b7er", "und", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "Eine dreizehner Landwehrfahne", "tokens": ["Ei\u00b7ne", "drei\u00b7zeh\u00b7ner", "Land\u00b7wehr\u00b7fah\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Der alte von Bredow trug,", "tokens": ["Der", "al\u00b7te", "von", "Bre\u00b7dow", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "NE", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und Hans Rochow von Rekahne", "tokens": ["Und", "Hans", "Ro\u00b7chow", "von", "Re\u00b7kah\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NE", "NE", "APPR", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Schlo\u00df ab den Trauerzug.", "tokens": ["Schlo\u00df", "ab", "den", "Trau\u00b7er\u00b7zug", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}