{"textgrid.poem.37832": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Die Juden in Passau", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit Gott der allen Dingen,", "tokens": ["Mit", "Gott", "der", "al\u00b7len", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Anfang geben hat,", "tokens": ["Ein", "An\u00b7fang", "ge\u00b7ben", "hat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So heben wir an zu singen,", "tokens": ["So", "he\u00b7ben", "wir", "an", "zu", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ein wunderliche That.", "tokens": ["Ein", "wun\u00b7der\u00b7li\u00b7che", "That", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Christoph Ei\u00dfenhammer", "tokens": ["Der", "Chris\u00b7toph", "Ei\u00b7\u00df\u00b7en\u00b7ham\u00b7mer"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch sein gro\u00df Missethat", "tokens": ["Durch", "sein", "gro\u00df", "Mis\u00b7se\u00b7that"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJD", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Fing an ein gro\u00dfen Jammer", "tokens": ["Fing", "an", "ein", "gro\u00b7\u00dfen", "Jam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu Passau in der Stadt.", "tokens": ["Zu", "Pas\u00b7sau", "in", "der", "Stadt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Zun Juden th\u00e4t er laufen,", "tokens": ["Zun", "Ju\u00b7den", "th\u00e4t", "er", "lau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und fragen sie behend:", "tokens": ["Und", "fra\u00b7gen", "sie", "be\u00b7hend", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "\u00bbob sie nit wollten kaufen,", "tokens": ["\u00bb", "ob", "sie", "nit", "woll\u00b7ten", "kau\u00b7fen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das heilig Sakrament?\u00ab", "tokens": ["Das", "hei\u00b7lig", "Sak\u00b7ra\u00b7ment", "?", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Alsbald sie Antwort gaben:", "tokens": ["Als\u00b7bald", "sie", "Ant\u00b7wort", "ga\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u00bber solls ihnen bringen nun,", "tokens": ["\u00bb", "er", "solls", "ih\u00b7nen", "brin\u00b7gen", "nun", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "PPER", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie wollten ihm mit Gaben,", "tokens": ["Sie", "woll\u00b7ten", "ihm", "mit", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein v\u00f6llig Gn\u00fcge thun.\u00ab", "tokens": ["Ein", "v\u00f6l\u00b7lig", "Gn\u00fc\u00b7ge", "thun", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJD", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "In st\u00fcrmischer Nacht, im Finstern.", "tokens": ["In", "st\u00fcr\u00b7mi\u00b7scher", "Nacht", ",", "im", "Fins\u00b7tern", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Brach er die Th\u00fcre auf,", "tokens": ["Brach", "er", "die", "Th\u00fc\u00b7re", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von unser Frauen M\u00fcnster,", "tokens": ["Von", "un\u00b7ser", "Frau\u00b7en", "M\u00fcns\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nahm acht Partikel raus.", "tokens": ["Nahm", "acht", "Par\u00b7ti\u00b7kel", "raus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "NN", "PTKVZ", "$."], "meter": "+----+", "measure": "dactylic.init"}}, "stanza.6": {"line.1": {"text": "Um einen Gulden merk eben,", "tokens": ["Um", "ei\u00b7nen", "Gul\u00b7den", "merk", "e\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Er sie alle acht verkauft,", "tokens": ["Er", "sie", "al\u00b7le", "acht", "ver\u00b7kauft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "PIAT", "CARD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df einer, wie zu sehen,", "tokens": ["Da\u00df", "ei\u00b7ner", ",", "wie", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "PWAV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf drey\u00dfig Pfennig lauft.", "tokens": ["Auf", "drey\u00b7\u00dfig", "Pfen\u00b7nig", "lauft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Die Juden lie\u00dfens zum Tempel,", "tokens": ["Die", "Ju\u00b7den", "lie\u00b7\u00dfens", "zum", "Tem\u00b7pel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Bald tragen auf den Altar,", "tokens": ["Bald", "tra\u00b7gen", "auf", "den", "Al\u00b7tar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ein Messer sie auszogen,", "tokens": ["Ein", "Mes\u00b7ser", "sie", "aus\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und stachen grimmig drein.", "tokens": ["Und", "sta\u00b7chen", "grim\u00b7mig", "drein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Bald sahen sie herausflie\u00dfen,", "tokens": ["Bald", "sa\u00b7hen", "sie", "her\u00b7aus\u00b7flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Das Blut ganz mild und reich,", "tokens": ["Das", "Blut", "ganz", "mild", "und", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gestalt sich sehen lie\u00dfe,", "tokens": ["Ge\u00b7stalt", "sich", "se\u00b7hen", "lie\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Eim jungen Kindlein gleich.", "tokens": ["Eim", "jun\u00b7gen", "Kin\u00b7dlein", "gleich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Das brachte gro\u00dfen Schrecken,", "tokens": ["Das", "brach\u00b7te", "gro\u00b7\u00dfen", "Schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie gingen bald zu Rath:", "tokens": ["Sie", "gin\u00b7gen", "bald", "zu", "Rath", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zwo Hostien zu schicken,", "tokens": ["Zwo", "Hos\u00b7ti\u00b7en", "zu", "schi\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gen Salzburg in die Stadt.", "tokens": ["Gen", "Salz\u00b7burg", "in", "die", "Stadt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "In die Neustadt auch zwo senden,", "tokens": ["In", "die", "Neu\u00b7stadt", "auch", "zwo", "sen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "CARD", "ADJA", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Zwo schickten sie gen Prag,", "tokens": ["Zwo", "schick\u00b7ten", "sie", "gen", "Prag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zwo hielten sie bei H\u00e4nden,", "tokens": ["Zwo", "hiel\u00b7ten", "sie", "bei", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "H\u00e4tten dar\u00fcber Frag.", "tokens": ["H\u00e4t\u00b7ten", "da\u00b7r\u00fc\u00b7ber", "Frag", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.11": {"line.1": {"text": "Sie meinten und verhofften,", "tokens": ["Sie", "mein\u00b7ten", "und", "ver\u00b7hoff\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Christum auszutilgen gar,", "tokens": ["Chris\u00b7tum", "aus\u00b7zu\u00b7til\u00b7gen", "gar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Drum heizten sie ein Ofen,", "tokens": ["Drum", "heiz\u00b7ten", "sie", "ein", "O\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Worin die Hostien warn.", "tokens": ["Wo\u00b7rin", "die", "Hos\u00b7ti\u00b7en", "warn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.12": {"line.1": {"text": "Doch seht vor ihren Augen", "tokens": ["Doch", "seht", "vor", "ih\u00b7ren", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Flogen zwey Engel raus,", "tokens": ["Flo\u00b7gen", "zwey", "En\u00b7gel", "raus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "NN", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Dazu zwo sch\u00f6ne Tauben,", "tokens": ["Da\u00b7zu", "zwo", "sch\u00f6\u00b7ne", "Tau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das machte Furcht und Grau\u00df.", "tokens": ["Das", "mach\u00b7te", "Furcht", "und", "Grau\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Christoph, der Uebelth\u00e4ter,", "tokens": ["Chris\u00b7toph", ",", "der", "Ue\u00b7belt\u00b7h\u00e4\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In S\u00fcnden hart verblendt", "tokens": ["In", "S\u00fcn\u00b7den", "hart", "ver\u00b7blendt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJD", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie Judas der Verr\u00e4ther,", "tokens": ["Wie", "Ju\u00b7das", "der", "Ver\u00b7r\u00e4\u00b7ther", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Stiehlt weiter was er findt.", "tokens": ["Stiehlt", "wei\u00b7ter", "was", "er", "findt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Als er zu Germansbergen", "tokens": ["Als", "er", "zu", "Ger\u00b7mans\u00b7ber\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Angriff den Kirchenstock,", "tokens": ["An\u00b7griff", "den", "Kir\u00b7chen\u00b7stock", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ergriffen ihn die Schergen,", "tokens": ["Er\u00b7grif\u00b7fen", "ihn", "die", "Scher\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie schlugen ihn in Stock.", "tokens": ["Sie", "schlu\u00b7gen", "ihn", "in", "Stock", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Da er nun lag gefangen,", "tokens": ["Da", "er", "nun", "lag", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu Passau im Oberhaus,", "tokens": ["Zu", "Pas\u00b7sau", "im", "O\u00b7ber\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPRART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Was er je h\u00e4tt begangen,", "tokens": ["Was", "er", "je", "h\u00e4tt", "be\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Bekennt er frey heraus.", "tokens": ["Be\u00b7kennt", "er", "frey", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Da wurden die Unthaten", "tokens": ["Da", "wur\u00b7den", "die", "Un\u00b7tha\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Juden auch vermehrt,", "tokens": ["Der", "Ju\u00b7den", "auch", "ver\u00b7mehrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie sie gerathen hatten,", "tokens": ["Wie", "sie", "ge\u00b7ra\u00b7then", "hat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Sakrament entehrt.", "tokens": ["Das", "Sak\u00b7ra\u00b7ment", "ent\u00b7ehrt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Dem Bischof ging zu Herzen", "tokens": ["Dem", "Bi\u00b7schof", "ging", "zu", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Solch l\u00e4sterliche That,", "tokens": ["Solch", "l\u00e4s\u00b7ter\u00b7li\u00b7che", "That", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Darauf ohn alles Scherzen,", "tokens": ["Da\u00b7rauf", "ohn", "al\u00b7les", "Scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er nach ihnen greifen l\u00e4\u00dft.", "tokens": ["Er", "nach", "ih\u00b7nen", "grei\u00b7fen", "l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "Da haben sie bekennet,", "tokens": ["Da", "ha\u00b7ben", "sie", "be\u00b7ken\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df sie das Sakrament,", "tokens": ["Da\u00df", "sie", "das", "Sak\u00b7ra\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gestochen und gebrennet,", "tokens": ["Ge\u00b7sto\u00b7chen", "und", "ge\u00b7bren\u00b7net", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und in drey St\u00e4dt gesendt.", "tokens": ["Und", "in", "drey", "St\u00e4dt", "ge\u00b7sendt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "CARD", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.19": {"line.1": {"text": "Zwar vier aus den Gefangnen,", "tokens": ["Zwar", "vier", "aus", "den", "Ge\u00b7fang\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Haben sich weisen lahn,", "tokens": ["Ha\u00b7ben", "sich", "wei\u00b7sen", "lahn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "VVFIN", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Die Seeligkeit zu erlangen,", "tokens": ["Die", "See\u00b7lig\u00b7keit", "zu", "er\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Den Glauben genommen an.", "tokens": ["Den", "Glau\u00b7ben", "ge\u00b7nom\u00b7men", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.20": {"line.1": {"text": "Die andern sind verbrennet:", "tokens": ["Die", "an\u00b7dern", "sind", "ver\u00b7bren\u00b7net", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die vier, so sich bekehrt,", "tokens": ["Die", "vier", ",", "so", "sich", "be\u00b7kehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "$,", "ADV", "PRF", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Christen sich genennet,", "tokens": ["Die", "Chris\u00b7ten", "sich", "ge\u00b7nen\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die gab man zu dem Schwerdt.", "tokens": ["Die", "gab", "man", "zu", "dem", "Schwerdt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.21": {"line.1": {"text": "Christoph ders angefangen,", "tokens": ["Chris\u00b7toph", "ders", "an\u00b7ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Sakrament verkauft,", "tokens": ["Das", "Sak\u00b7ra\u00b7ment", "ver\u00b7kauft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wurd auch mit heissen Zangen,", "tokens": ["Wurd", "auch", "mit", "heis\u00b7sen", "Zan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nach etlich Wochen gestraft.", "tokens": ["Nach", "et\u00b7lich", "Wo\u00b7chen", "ge\u00b7straft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}