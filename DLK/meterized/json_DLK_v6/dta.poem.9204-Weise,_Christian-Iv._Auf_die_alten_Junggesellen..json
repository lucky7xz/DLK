{"dta.poem.9204": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Iv.  \n Auf die alten Junggesellen.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Wem das gl\u00fccke wiederf\u00e4hret/", "tokens": ["Wem", "das", "gl\u00fc\u00b7cke", "wie\u00b7der\u00b7f\u00e4h\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df er in der jungen zeit", "tokens": ["Da\u00df", "er", "in", "der", "jun\u00b7gen", "zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auch ein junges m\u00e4dgen freyt/", "tokens": ["Auch", "ein", "jun\u00b7ges", "m\u00e4d\u00b7gen", "freyt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dem ist doch ein schatz bescheret/", "tokens": ["Dem", "ist", "doch", "ein", "schatz", "be\u00b7sche\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Welchen alles gold und geld", "tokens": ["Wel\u00b7chen", "al\u00b7les", "gold", "und", "geld"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PIAT", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Lange nicht die wage h\u00e4lt.", "tokens": ["Lan\u00b7ge", "nicht", "die", "wa\u00b7ge", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "2. Er gebraucht sich seiner jugend/", "tokens": ["Er", "ge\u00b7braucht", "sich", "sei\u00b7ner", "ju\u00b7gend", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und vexiert mit h\u00f6chster lust", "tokens": ["Und", "ve\u00b7xiert", "mit", "h\u00f6chs\u00b7ter", "lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Aus der allerliebsten brust", "tokens": ["Aus", "der", "al\u00b7ler\u00b7liebs\u00b7ten", "brust"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die verborgne liebes- tugend", "tokens": ["Die", "ver\u00b7borg\u00b7ne", "lie\u00b7bes", "tu\u00b7gend"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "TRUNC", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und vertauscht zum uberflu\u00df", "tokens": ["Und", "ver\u00b7tauscht", "zum", "u\u00b7ber\u00b7flu\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Lieb um liebe/ ku\u00df um ku\u00df.", "tokens": ["Lieb", "um", "lie\u00b7be", "/", "ku\u00df", "um", "ku\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "VVFIN", "$(", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "3. Er verjungt sich alle morgen/", "tokens": ["Er", "ver\u00b7jungt", "sich", "al\u00b7le", "mor\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PIS", "ADV", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und wie unsre lebens-frist", "tokens": ["Und", "wie", "uns\u00b7re", "le\u00b7bens\u00b7frist"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPOSAT", "NN"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.3": {"text": "Niemahls ohne sorgen ist/", "tokens": ["Nie\u00b7mahls", "oh\u00b7ne", "sor\u00b7gen", "ist", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VAFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Also schenckt er seine sorgen/", "tokens": ["Al\u00b7so", "schenckt", "er", "sei\u00b7ne", "sor\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Zwischen liebe/ lust und pein/", "tokens": ["Zwi\u00b7schen", "lie\u00b7be", "/", "lust", "und", "pein", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$(", "VVFIN", "KON", "NE", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "In der liebsten mund hinein.", "tokens": ["In", "der", "liebs\u00b7ten", "mund", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "4. Ach die atlen junggesellen", "tokens": ["Ach", "die", "at\u00b7len", "jung\u00b7ge\u00b7sel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ITJ", "ART", "ADJA", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wissen von der freude nicht/", "tokens": ["Wis\u00b7sen", "von", "der", "freu\u00b7de", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKNEG", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wann sie gleich ihr angesicht", "tokens": ["Wann", "sie", "gleich", "ihr", "an\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Unterweilen fr\u00f6lich stellen/", "tokens": ["Un\u00b7ter\u00b7wei\u00b7len", "fr\u00f6\u00b7lich", "stel\u00b7len", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach so k\u00f6mmt es ungef\u00e4hr", "tokens": ["Ach", "so", "k\u00f6mmt", "es", "un\u00b7ge\u00b7f\u00e4hr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "VVFIN", "PPER", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Von der blossen hoffnung her.", "tokens": ["Von", "der", "blos\u00b7sen", "hoff\u00b7nung", "her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "5. Sie belecken nur die schalen", "tokens": ["Sie", "be\u00b7le\u00b7cken", "nur", "die", "scha\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit vergebner angst und m\u00fch/", "tokens": ["Mit", "ver\u00b7geb\u00b7ner", "angst", "und", "m\u00fch", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn der kern ist nicht vor sie/", "tokens": ["Denn", "der", "kern", "ist", "nicht", "vor", "sie", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PTKNEG", "APPR", "PPER", "$("], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Und wenn sie mit liebsten praalen/", "tokens": ["Und", "wenn", "sie", "mit", "liebs\u00b7ten", "praa\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ADJA", "NN", "$("], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.5": {"text": "Ist der angemaste glantz", "tokens": ["Ist", "der", "an\u00b7ge\u00b7mas\u00b7te", "glantz"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Warlich weder halb noch gantz.", "tokens": ["War\u00b7lich", "we\u00b7der", "halb", "noch", "gantz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADJD", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "6. Auf den abend in dem bette", "tokens": ["Auf", "den", "a\u00b7bend", "in", "dem", "bet\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liegen sie als wie ein bild/", "tokens": ["Lie\u00b7gen", "sie", "als", "wie", "ein", "bild", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOUS", "KOKOM", "ART", "NN", "$("], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Halb erfroren eingeh\u00fcllt/", "tokens": ["Halb", "er\u00b7fro\u00b7ren", "ein\u00b7ge\u00b7h\u00fcllt", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf den morgen ist es wette/", "tokens": ["Auf", "den", "mor\u00b7gen", "ist", "es", "wet\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "VAFIN", "PPER", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn da brennt das bette-stroh", "tokens": ["Denn", "da", "brennt", "das", "bet\u00b7te\u00b7stroh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Vor der liebe liechter loh.", "tokens": ["Vor", "der", "lie\u00b7be", "liech\u00b7ter", "loh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "7. Wann sie sich am kl\u00fcgsten d\u00fcncken/", "tokens": ["Wann", "sie", "sich", "am", "kl\u00fcgs\u00b7ten", "d\u00fcn\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "APPRART", "ADJA", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wissen sie wohl selber nicht.", "tokens": ["Wis\u00b7sen", "sie", "wohl", "sel\u00b7ber", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was sie in der seite sticht/", "tokens": ["Was", "sie", "in", "der", "sei\u00b7te", "sticht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bi\u00df sie einst das leid vertrincken/", "tokens": ["Bi\u00df", "sie", "einst", "das", "leid", "ver\u00b7trin\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "ADJD", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da vergessen sie der zeit", "tokens": ["Da", "ver\u00b7ges\u00b7sen", "sie", "der", "zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und der tr\u00fcben einsamkeit.", "tokens": ["Und", "der", "tr\u00fc\u00b7ben", "ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "8. Ach wohl dem der sich versorget/", "tokens": ["Ach", "wohl", "dem", "der", "sich", "ver\u00b7sor\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "ART", "PRELS", "PRF", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Welcher hier ein bi\u00dfgen isst/", "tokens": ["Wel\u00b7cher", "hier", "ein", "bi\u00df\u00b7gen", "isst", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dort ein weilgen wieder k\u00fcst/", "tokens": ["Dort", "ein", "weil\u00b7gen", "wie\u00b7der", "k\u00fcst", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "ADV", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und an allen ecken borget/", "tokens": ["Und", "an", "al\u00b7len", "ec\u00b7ken", "bor\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ist f\u00fcrwahr ein armer mann/", "tokens": ["Ist", "f\u00fcr\u00b7wahr", "ein", "ar\u00b7mer", "mann", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Der offt will/ und selten kan.", "tokens": ["Der", "offt", "will", "/", "und", "sel\u00b7ten", "kan", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VMFIN", "$(", "KON", "ADJD", "VMFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}