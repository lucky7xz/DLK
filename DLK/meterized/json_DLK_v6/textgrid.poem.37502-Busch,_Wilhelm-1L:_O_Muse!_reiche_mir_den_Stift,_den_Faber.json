{"textgrid.poem.37502": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Muse! reiche mir den Stift, den Faber", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Muse! reiche mir den Stift, den Faber", "tokens": ["O", "Mu\u00b7se", "!", "rei\u00b7che", "mir", "den", "Stift", ",", "den", "Fa\u00b7ber"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$.", "VVFIN", "PPER", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In N\u00fcrnberg fabrizieren mu\u00df!", "tokens": ["In", "N\u00fcrn\u00b7berg", "fab\u00b7ri\u00b7zie\u00b7ren", "mu\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch einmal sattle mir den harten Traber,", "tokens": ["Noch", "ein\u00b7mal", "satt\u00b7le", "mir", "den", "har\u00b7ten", "Tra\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den alten Stecken-Pegasus!", "tokens": ["Den", "al\u00b7ten", "Ste\u00b7cken\u00b7Pe\u00b7ga\u00b7sus", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nu j\u00fch! \u2013 So reiten wir zu Imker Drallen", "tokens": ["Nu", "j\u00fch", "!", "\u2013", "So", "rei\u00b7ten", "wir", "zu", "Im\u00b7ker", "Dral\u00b7len"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$.", "$(", "ADV", "VVFIN", "PPER", "APPR", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und zu Christinen, welche sch\u00f6n,", "tokens": ["Und", "zu", "Chris\u00b7ti\u00b7nen", ",", "wel\u00b7che", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "$,", "PRELS", "ADJD", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Und zu Herrn Kn\u00f6rrje, dem sie sehr gefallen,", "tokens": ["Und", "zu", "Herrn", "Kn\u00f6rr\u00b7je", ",", "dem", "sie", "sehr", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "NE", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "--+----+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Und dessen Neffen, dem Eugen!", "tokens": ["Und", "des\u00b7sen", "Nef\u00b7fen", ",", "dem", "Eu\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PRELAT", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}