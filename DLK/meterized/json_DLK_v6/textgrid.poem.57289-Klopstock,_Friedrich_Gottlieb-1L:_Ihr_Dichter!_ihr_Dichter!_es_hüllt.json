{"textgrid.poem.57289": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr Dichter! ihr Dichter! es h\u00fcllt", "genre": "verse", "period": "N.A.", "pub_year": 1767, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Dichter! ihr Dichter! es h\u00fcllt", "tokens": ["Ihr", "Dich\u00b7ter", "!", "ihr", "Dich\u00b7ter", "!", "es", "h\u00fcllt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "PPOSAT", "NN", "$.", "PPER", "VVFIN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Nacht die Telyn der Barden ein!", "tokens": ["Nacht", "die", "Te\u00b7lyn", "der", "Bar\u00b7den", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Der am Quell Mimer oft Braga's Saite schwieg,", "tokens": ["Der", "am", "Quell", "Mi\u00b7mer", "oft", "Bra\u00b7ga's", "Sai\u00b7te", "schwieg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "NN", "ADV", "NE", "NN", "VVFIN", "$,"], "meter": "--+---+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Wenn die Erfindung, im West schlummernd, gebar", "tokens": ["Wenn", "die", "Er\u00b7fin\u00b7dung", ",", "im", "West", "schlum\u00b7mernd", ",", "ge\u00b7bar"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "ART", "NN", "$,", "APPRART", "NN", "VVPP", "$,", "ADJD"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Erhabneren Geist, und Gestalt", "tokens": ["Er\u00b7hab\u00b7ne\u00b7ren", "Geist", ",", "und", "Ge\u00b7stalt"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "KON", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Sch\u00f6n wie Knaben im Kriegestanz,", "tokens": ["Sch\u00f6n", "wie", "Kna\u00b7ben", "im", "Krie\u00b7ge\u00b7stanz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "NN", "APPRART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Dass entz\u00fcckt, wenn sie sah, wer geboren war,", "tokens": ["Dass", "ent\u00b7z\u00fcckt", ",", "wenn", "sie", "sah", ",", "wer", "ge\u00b7bo\u00b7ren", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,", "PWS", "VVPP", "VAFIN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Ihr des beseelteren Blicks Trunkenheit schwamm.", "tokens": ["Ihr", "des", "be\u00b7seel\u00b7te\u00b7ren", "Blicks", "Trun\u00b7ken\u00b7heit", "schwamm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.3": {"line.1": {"text": "Lieicht springt er, ein Genius, auf,", "tokens": ["Lie\u00b7icht", "springt", "er", ",", "ein", "Ge\u00b7nius", ",", "auf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "ART", "NN", "$,", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spielt am Sprosse des Eichenhains!", "tokens": ["Spielt", "am", "Spros\u00b7se", "des", "Ei\u00b7chen\u00b7hains", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Den Allhend geht sein Gang! seiner Tritte Ton", "tokens": ["Den", "All\u00b7hend", "geht", "sein", "Gang", "!", "sei\u00b7ner", "Trit\u00b7te", "Ton"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$.", "PPOSAT", "NN", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Rieselt daher, wie der Bach, rauscht wie der Strom", "tokens": ["Rie\u00b7selt", "da\u00b7her", ",", "wie", "der", "Bach", ",", "rauscht", "wie", "der", "Strom"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PAV", "$,", "PWAV", "ART", "NN", "$,", "VVFIN", "KOKOM", "ART", "NN"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Ihr Dichter! ihr Dichter! wo sank", "tokens": ["Ihr", "Dich\u00b7ter", "!", "ihr", "Dich\u00b7ter", "!", "wo", "sank"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "PPOSAT", "NN", "$.", "PWAV", "VVFIN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Unsrer Filea Telyn hin?", "tokens": ["Uns\u00b7rer", "Fi\u00b7lea", "Te\u00b7lyn", "hin", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ah es tr\u00fcbt, sinn' ich nach, was die Tr\u00fcmmer deckt,", "tokens": ["Ah", "es", "tr\u00fcbt", ",", "sinn'", "ich", "nach", ",", "was", "die", "Tr\u00fcm\u00b7mer", "deckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PTKVZ", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Mir den beweinenden Blick w\u00fcnschender Schmerz!", "tokens": ["Mir", "den", "be\u00b7wei\u00b7nen\u00b7den", "Blick", "w\u00fcn\u00b7schen\u00b7der", "Schmerz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}}}}}