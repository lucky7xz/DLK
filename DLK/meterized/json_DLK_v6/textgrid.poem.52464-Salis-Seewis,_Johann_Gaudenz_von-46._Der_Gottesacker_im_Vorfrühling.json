{"textgrid.poem.52464": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "46. Der Gottesacker im Vorfr\u00fchling", "genre": "verse", "period": "N.A.", "pub_year": 1798, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bl\u00e4tter treibt des Kirchhofs Flieder,", "tokens": ["Bl\u00e4t\u00b7ter", "treibt", "des", "Kirch\u00b7hofs", "Flie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Neigt auf Gr\u00fcfte junges Laub;", "tokens": ["Neigt", "auf", "Gr\u00fcf\u00b7te", "jun\u00b7ges", "Laub", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kirschenbl\u00fcte gaukelt nieder", "tokens": ["Kir\u00b7schen\u00b7bl\u00fc\u00b7te", "gau\u00b7kelt", "nie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVFIN", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf der Abgeschiednen Staub.", "tokens": ["Auf", "der", "Ab\u00b7ge\u00b7schied\u00b7nen", "Staub", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bleicher Primeln Keime l\u00fcpfen", "tokens": ["Blei\u00b7cher", "Pri\u00b7meln", "Kei\u00b7me", "l\u00fcp\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sanft das Moos, das sie umgab;", "tokens": ["Sanft", "das", "Moos", ",", "das", "sie", "um\u00b7gab", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und des Dorfes Kinder h\u00fcpfen", "tokens": ["Und", "des", "Dor\u00b7fes", "Kin\u00b7der", "h\u00fcp\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Achtlos auf der M\u00fctter Grab.", "tokens": ["Acht\u00b7los", "auf", "der", "M\u00fct\u00b7ter", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Junges Sinngr\u00fcn dr\u00e4ngt sich dichter", "tokens": ["Jun\u00b7ges", "Sinn\u00b7gr\u00fcn", "dr\u00e4ngt", "sich", "dich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PRF", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An des J\u00fcnglings flachen Stein,", "tokens": ["An", "des", "J\u00fcng\u00b7lings", "fla\u00b7chen", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00d6ffnet blauer Blumen Trichter,", "tokens": ["\u00d6ff\u00b7net", "blau\u00b7er", "Blu\u00b7men", "Trich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Saugt zerflo\u00dfnen Reifen ein.", "tokens": ["Saugt", "zer\u00b7flo\u00df\u00b7nen", "Rei\u00b7fen", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schlaff gedr\u00fcckte Halme richten", "tokens": ["Schlaff", "ge\u00b7dr\u00fcck\u00b7te", "Hal\u00b7me", "rich\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sich vom Winterschlaf empor,", "tokens": ["Sich", "vom", "Win\u00b7ter\u00b7schlaf", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und in naher Waldung Fichten", "tokens": ["Und", "in", "na\u00b7her", "Wal\u00b7dung", "Fich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Fl\u00f6tet laut ein Drosselchor.", "tokens": ["Fl\u00f6\u00b7tet", "laut", "ein", "Dros\u00b7selc\u00b7hor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Drosseln, singt in leisen Ch\u00f6ren!", "tokens": ["Dros\u00b7seln", ",", "singt", "in", "lei\u00b7sen", "Ch\u00f6\u00b7ren", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Amsel, fl\u00f6t im Trauerhain!", "tokens": ["Am\u00b7sel", ",", "fl\u00f6t", "im", "Trau\u00b7er\u00b7hain", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Nur wir Hinterbliebnen h\u00f6ren", "tokens": ["Nur", "wir", "Hin\u00b7ter\u00b7blieb\u00b7nen", "h\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPER", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eure Fr\u00fchlingsmelode'n.", "tokens": ["Eu\u00b7re", "Fr\u00fch\u00b7lings\u00b7me\u00b7lo\u00b7de'", "n."], "token_info": ["word", "word", "abbreviation"], "pos": ["PPOSAT", "NN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach! ihr mahnt an die Genossen,", "tokens": ["Ach", "!", "ihr", "mahnt", "an", "die", "Ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die ein fr\u00fcher Tod verkl\u00e4rt;", "tokens": ["Die", "ein", "fr\u00fc\u00b7her", "Tod", "ver\u00b7kl\u00e4rt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "An die Lenze, die verflossen,", "tokens": ["An", "die", "Len\u00b7ze", ",", "die", "ver\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "An die Zeit, die nimmer kehrt!", "tokens": ["An", "die", "Zeit", ",", "die", "nim\u00b7mer", "kehrt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Fl\u00f6tet nur gela\u00dfne Klage,", "tokens": ["Fl\u00f6\u00b7tet", "nur", "ge\u00b7la\u00df\u00b7ne", "Kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hemmt der Trauert\u00f6ne Lauf;", "tokens": ["Hemmt", "der", "Trau\u00b7er\u00b7t\u00f6\u00b7ne", "Lauf", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn sie nahm von dunkler Tage", "tokens": ["Denn", "sie", "nahm", "von", "dunk\u00b7ler", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Letzter Stuf' ihr Engel auf.", "tokens": ["Letz\u00b7ter", "Stuf'", "ihr", "En\u00b7gel", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Kies und dunkle Schollen warfen", "tokens": ["Kies", "und", "dunk\u00b7le", "Schol\u00b7len", "war\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KON", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wir auf den versenkten Sarg,", "tokens": ["Wir", "auf", "den", "ver\u00b7senk\u00b7ten", "Sarg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Als, begr\u00fc\u00dft von Himmelsharfen,", "tokens": ["Als", ",", "be\u00b7gr\u00fc\u00dft", "von", "Him\u00b7mels\u00b7har\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "VVPP", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sich ihr Geist in Licht uns barg.", "tokens": ["Sich", "ihr", "Geist", "in", "Licht", "uns", "barg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "NN", "APPR", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "In des Geisterreiches Stille", "tokens": ["In", "des", "Geis\u00b7ter\u00b7rei\u00b7ches", "Stil\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tobt kein Sturm der Leidenschaft,", "tokens": ["Tobt", "kein", "Sturm", "der", "Lei\u00b7den\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und des Guten reiner Wille", "tokens": ["Und", "des", "Gu\u00b7ten", "rei\u00b7ner", "Wil\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lohnt sich durch erh\u00f6hte Kraft;", "tokens": ["Lohnt", "sich", "durch", "er\u00b7h\u00f6h\u00b7te", "Kraft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Seelen, fremd im \u00f6den Thale", "tokens": ["See\u00b7len", ",", "fremd", "im", "\u00f6\u00b7den", "Tha\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADJD", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der umschr\u00e4nkten Wirklichkeit,", "tokens": ["Der", "um\u00b7schr\u00e4nk\u00b7ten", "Wirk\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Fanden froh die Ideale", "tokens": ["Fan\u00b7den", "froh", "die", "I\u00b7dea\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Seliger Vollkommenheit.", "tokens": ["Se\u00b7li\u00b7ger", "Voll\u00b7kom\u00b7men\u00b7heit", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ihre Schw\u00e4chen sind vergessen,", "tokens": ["Ih\u00b7re", "Schw\u00e4\u00b7chen", "sind", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Groll und Zwietracht sind vers\u00f6hnt,", "tokens": ["Groll", "und", "Zwiet\u00b7racht", "sind", "ver\u00b7s\u00f6hnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo die Reue mit Cypressen", "tokens": ["Wo", "die", "Reu\u00b7e", "mit", "Cyp\u00b7res\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "NN"], "meter": "--+--+--", "measure": "anapaest.di.plus"}, "line.4": {"text": "Der Gekr\u00f6nten St\u00e4tte kr\u00f6nt.", "tokens": ["Der", "Ge\u00b7kr\u00f6n\u00b7ten", "St\u00e4t\u00b7te", "kr\u00f6nt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aus des niedern Neides Schranke", "tokens": ["Aus", "des", "nie\u00b7dern", "Nei\u00b7des", "Schran\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zu des Friedens H\u00f6h' entr\u00fcckt,", "tokens": ["Zu", "des", "Frie\u00b7dens", "H\u00f6h'", "ent\u00b7r\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ritzt sie nie der