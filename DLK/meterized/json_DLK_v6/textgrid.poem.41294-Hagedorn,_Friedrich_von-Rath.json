{"textgrid.poem.41294": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Rath", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr, die ihr wagt, und stets gesch\u00e4ftig seid,", "tokens": ["Ihr", ",", "die", "ihr", "wagt", ",", "und", "stets", "ge\u00b7sch\u00e4f\u00b7tig", "seid", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "VVFIN", "$,", "KON", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Durch Vortheil reich, durch Knechtschaft gro\u00df zu werden,", "tokens": ["Durch", "Vor\u00b7theil", "reich", ",", "durch", "Knecht\u00b7schaft", "gro\u00df", "zu", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$,", "APPR", "NN", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Begebt euch ja des Vorzugs voll Beschwerden,", "tokens": ["Be\u00b7gebt", "euch", "ja", "des", "Vor\u00b7zugs", "voll", "Be\u00b7schwer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Geist, Geschmack und Wissenschaft verleiht.", "tokens": ["Den", "Geist", ",", "Ge\u00b7schmack", "und", "Wis\u00b7sen\u00b7schaft", "ver\u00b7leiht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Erhebet euch! doch nie in Witz und Wissen:", "tokens": ["Er\u00b7he\u00b7bet", "euch", "!", "doch", "nie", "in", "Witz", "und", "Wis\u00b7sen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "ADV", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Witz bringt Gefahr, und Zweifel geben Qual.", "tokens": ["Witz", "bringt", "Ge\u00b7fahr", ",", "und", "Zwei\u00b7fel", "ge\u00b7ben", "Qual", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "$,", "KON", "NN", "VVINF", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "O kenntet ihr die Sorgfalt edler Wahl;", "tokens": ["O", "kenn\u00b7tet", "ihr", "die", "Sorg\u00b7falt", "ed\u00b7ler", "Wahl", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Was w\u00fcrd' euch nicht ver\u00e4chtlich werden m\u00fcssen?", "tokens": ["Was", "w\u00fcrd'", "euch", "nicht", "ver\u00b7\u00e4cht\u00b7lich", "wer\u00b7den", "m\u00fcs\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PTKNEG", "ADJD", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}