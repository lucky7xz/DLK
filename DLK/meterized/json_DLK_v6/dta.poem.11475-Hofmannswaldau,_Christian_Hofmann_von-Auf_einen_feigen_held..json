{"dta.poem.11475": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen feigen held.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Um Schlendors zagheit hat es warlich schlechte noth:", "tokens": ["Um", "Schlen\u00b7dors", "zag\u00b7heit", "hat", "es", "war\u00b7lich", "schlech\u00b7te", "noth", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "NN", "VAFIN", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er l\u00e4st ihm keinen nicht leicht auf die haube kommen.", "tokens": ["Er", "l\u00e4st", "ihm", "kei\u00b7nen", "nicht", "leicht", "auf", "die", "hau\u00b7be", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "PTKNEG", "ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn als sich eine laus die freyheit ausgenommen;", "tokens": ["Denn", "als", "sich", "ei\u00b7ne", "laus", "die", "frey\u00b7heit", "aus\u00b7ge\u00b7nom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PRF", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So kriegt er sie beym kopff, und schlug sie mause-todt.", "tokens": ["So", "kriegt", "er", "sie", "beym", "kopff", ",", "und", "schlug", "sie", "mau\u00b7se\u00b7todt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$,", "KON", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}