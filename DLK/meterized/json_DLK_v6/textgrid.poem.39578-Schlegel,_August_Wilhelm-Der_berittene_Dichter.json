{"textgrid.poem.39578": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der berittene Dichter", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf Zeltern reiten Cardinal und Pabst;", "tokens": ["Auf", "Zel\u00b7tern", "rei\u00b7ten", "Car\u00b7di\u00b7nal", "und", "Pabst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "'s ist billig, da\u00df du deinen Zelter hab'st.", "tokens": ["'", "s", "ist", "bil\u00b7lig", ",", "da\u00df", "du", "dei\u00b7nen", "Zel\u00b7ter", "hab", "'", "st."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "abbreviation"], "pos": ["$(", "PPER", "VAFIN", "ADJD", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VAFIN", "$(", "XY"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Mit Troddeln werd' ihm M\u00e4hn' und Schweif beh\u00e4ngt,", "tokens": ["Mit", "Trod\u00b7deln", "werd'", "ihm", "M\u00e4hn'", "und", "Schweif", "be\u00b7h\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Satteldecke bis zum Knie verl\u00e4ngt.", "tokens": ["Die", "Sat\u00b7tel\u00b7de\u00b7cke", "bis", "zum", "Knie", "ver\u00b7l\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Gem\u00e4chlichkeit und Gravit\u00e4t vereint", "tokens": ["Ge\u00b7m\u00e4ch\u00b7lich\u00b7keit", "und", "Gra\u00b7vi\u00b7t\u00e4t", "ver\u00b7eint"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ein solches Pferd, das fast ein Maulthier scheint.", "tokens": ["Ein", "sol\u00b7ches", "Pferd", ",", "das", "fast", "ein", "Mault\u00b7hier", "scheint", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ein k\u00fchner Reiter warst du ehedem,", "tokens": ["Ein", "k\u00fch\u00b7ner", "Rei\u00b7ter", "warst", "du", "e\u00b7he\u00b7dem", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Doch mit den Jahren macht man sich's bequem.", "tokens": ["Doch", "mit", "den", "Jah\u00b7ren", "macht", "man", "sich's", "be\u00b7quem", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PIS", "PRF", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Zu heftig sch\u00fcttelt ein entschied'ner Trab,", "tokens": ["Zu", "hef\u00b7tig", "sch\u00fct\u00b7telt", "ein", "ent\u00b7schie\u00b7d'\u00b7ner", "Trab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.10": {"text": "Und im Galopp st\u00fcrzt man wohl gar hinab;", "tokens": ["Und", "im", "Ga\u00b7lopp", "st\u00fcrzt", "man", "wohl", "gar", "hin\u00b7ab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PIS", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.11": {"text": "Bei'm Pa\u00dfgang aber schl\u00e4ft man allenfalls,", "tokens": ["Bei'm", "Pa\u00df\u00b7gang", "a\u00b7ber", "schl\u00e4ft", "man", "al\u00b7len\u00b7falls", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "PIS", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und legt dem Thier die Z\u00fcgel auf den Hals. \u2013", "tokens": ["Und", "legt", "dem", "Thier", "die", "Z\u00fc\u00b7gel", "auf", "den", "Hals", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Drum, Zelter, sei nicht allzu hochgemuth,", "tokens": ["Drum", ",", "Zel\u00b7ter", ",", "sei", "nicht", "all\u00b7zu", "hoch\u00b7ge\u00b7muth", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "NN", "$,", "VAFIN", "PTKNEG", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Weil sich auf dir ein Dichter ausgeruht.", "tokens": ["Weil", "sich", "auf", "dir", "ein", "Dich\u00b7ter", "aus\u00b7ge\u00b7ruht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "APPR", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dein Knochenbau, zwar kr\u00e4ftig, aber grob,", "tokens": ["Dein", "Kno\u00b7chen\u00b7bau", ",", "zwar", "kr\u00e4f\u00b7tig", ",", "a\u00b7ber", "grob", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Taugt nicht zu kleinen Volten und Red opp.", "tokens": ["Taugt", "nicht", "zu", "klei\u00b7nen", "Vol\u00b7ten", "und", "Red", "opp", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "ADJA", "NN", "KON", "NN", "NE", "$."], "meter": "-+-+-+-+++", "measure": "zehnsilber"}, "line.5": {"text": "Tritt ehrbar auf! la\u00df die Curbetten sein,", "tokens": ["Tritt", "ehr\u00b7bar", "auf", "!", "la\u00df", "die", "Cur\u00b7bet\u00b7ten", "sein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKVZ", "$.", "VVIMP", "ART", "NN", "VAINF", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und bilde nichts vom Pegasus dir ein!", "tokens": ["Und", "bil\u00b7de", "nichts", "vom", "Pe\u00b7ga\u00b7sus", "dir", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "APPRART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}