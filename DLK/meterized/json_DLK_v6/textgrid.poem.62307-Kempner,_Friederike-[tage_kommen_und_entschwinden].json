{"textgrid.poem.62307": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[tage kommen und entschwinden]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tage kommen und entschwinden,", "tokens": ["Ta\u00b7ge", "kom\u00b7men", "und", "ent\u00b7schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "KON", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jahre kommen und vergeh'n,", "tokens": ["Jah\u00b7re", "kom\u00b7men", "und", "ver\u00b7geh'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und kein Mensch kann es ergr\u00fcnden", "tokens": ["Und", "kein", "Mensch", "kann", "es", "er\u00b7gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieses Kommen, dieses Geh'n.", "tokens": ["Die\u00b7ses", "Kom\u00b7men", ",", "die\u00b7ses", "Geh'", "n."], "token_info": ["word", "word", "punct", "word", "word", "abbreviation"], "pos": ["PDAT", "NN", "$,", "PDAT", "NN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Gott nur kann das R\u00e4tsel wissen,", "tokens": ["Gott", "nur", "kann", "das", "R\u00e4t\u00b7sel", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Logisch la\u00dft uns ihm vertrau'n,", "tokens": ["Lo\u00b7gisch", "la\u00dft", "uns", "ihm", "ver\u00b7trau'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fest auf seine Hilfe bau'n,", "tokens": ["Fest", "auf", "sei\u00b7ne", "Hil\u00b7fe", "bau'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Froh den Tag, das Jahr begr\u00fc\u00dfen.", "tokens": ["Froh", "den", "Tag", ",", "das", "Jahr", "be\u00b7gr\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und ein jeder Tag, er bringe", "tokens": ["Und", "ein", "je\u00b7der", "Tag", ",", "er", "brin\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "PIAT", "NN", "$,", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Uns die allerbesten Dinge,", "tokens": ["Uns", "die", "al\u00b7ler\u00b7bes\u00b7ten", "Din\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mut und Kraft und Sonnenschein,", "tokens": ["Mut", "und", "Kraft", "und", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Was wir taten mag gedeih'n,", "tokens": ["Was", "wir", "ta\u00b7ten", "mag", "ge\u00b7deih'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVINF", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Was wir w\u00fcnschen bald gelinge", "tokens": ["Was", "wir", "w\u00fcn\u00b7schen", "bald", "ge\u00b7lin\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "VVFIN", "ADV", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Uns und allen, welche rein.", "tokens": ["Uns", "und", "al\u00b7len", ",", "wel\u00b7che", "rein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "KON", "PIS", "$,", "PRELS", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}