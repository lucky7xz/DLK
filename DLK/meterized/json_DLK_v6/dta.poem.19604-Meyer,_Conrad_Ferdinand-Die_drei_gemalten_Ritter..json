{"dta.poem.19604": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Die drei gemalten Ritter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "\u201efrau Berte, h\u00f6rt: Ihr d\u00fcrftet nun", "tokens": ["\u201e", "frau", "Ber\u00b7te", ",", "h\u00f6rt", ":", "Ihr", "d\u00fcrf\u00b7tet", "nun"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["$(", "ADJD", "NN", "$,", "VVFIN", "$.", "PPER", "VMFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir einmal einen Gefallen thun!\u201c", "tokens": ["Mir", "ein\u00b7mal", "ei\u00b7nen", "Ge\u00b7fal\u00b7len", "thun", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "\u2014 \u201eWas denkt Ihr, Graf? Wohin denket Ihr?", "tokens": ["\u201e", "Was", "denkt", "Ihr", ",", "Graf", "?", "Wo\u00b7hin", "den\u00b7ket", "Ihr", "?"], "token_info": ["punct", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "$(", "PWS", "VVFIN", "PPER", "$,", "NE", "$.", "PWAV", "VVFIN", "PPER", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Vor den drei gemalten Rittern hier?\u201c", "tokens": ["Vor", "den", "drei", "ge\u00b7mal\u00b7ten", "Rit\u00b7tern", "hier", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "CARD", "ADJA", "NN", "ADV", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Drei Ritter prahlen auf der Wand", "tokens": ["Drei", "Rit\u00b7ter", "prah\u00b7len", "auf", "der", "Wand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit rollenden Augen, am Dolch die Hand.", "tokens": ["Mit", "rol\u00b7len\u00b7den", "Au\u00b7gen", ",", "am", "Dolch", "die", "Hand", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "\u201ewer, Frau, ist diese Ritterschaft?\u201c", "tokens": ["\u201e", "wer", ",", "Frau", ",", "ist", "die\u00b7se", "Rit\u00b7ter\u00b7schaft", "?", "\u201c"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "$,", "NN", "$,", "VAFIN", "PDAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u2014 \u201eDrei Vettern und alle drei tugendhaft!", "tokens": ["\u201e", "Drei", "Vet\u00b7tern", "und", "al\u00b7le", "drei", "tu\u00b7gend\u00b7haft", "!"], "token_info": ["punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "CARD", "NN", "KON", "PIAT", "CARD", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.5": {"line.1": {"text": "Gelobt Ihr, Graf, die Ehe mir", "tokens": ["Ge\u00b7lobt", "Ihr", ",", "Graf", ",", "die", "E\u00b7he", "mir"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "NE", "$,", "ART", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bei den drei gemalten Rittern hier,", "tokens": ["Bei", "den", "drei", "ge\u00b7mal\u00b7ten", "Rit\u00b7tern", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "CARD", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Will ich \u2014 Ihr la\u00dft es doch nicht ruhn \u2014", "tokens": ["Will", "ich", "Ihr", "la\u00dft", "es", "doch", "nicht", "ruhn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$(", "PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Euch einmal einen Gefallen thun.\u201c", "tokens": ["Euch", "ein\u00b7mal", "ei\u00b7nen", "Ge\u00b7fal\u00b7len", "thun", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Das Gr\u00e4flein zwinkert den Rittern zu.", "tokens": ["Das", "Gr\u00e4f\u00b7lein", "zwin\u00b7kert", "den", "Rit\u00b7tern", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "(\u201eFrau Berte, welch eine Gans bist du!\u201c)", "tokens": ["(", "\u201e", "Frau", "Ber\u00b7te", ",", "welch", "ei\u00b7ne", "Gans", "bist", "du", "!", "\u201c", ")"], "token_info": ["punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "$(", "NN", "NE", "$,", "PWAT", "ART", "NN", "VAFIN", "PPER", "$.", "$(", "$("], "meter": "-+-++-+-+", "measure": "unknown.measure.penta"}}, "stanza.8": {"line.1": {"text": "Das Gr\u00e4flein hebt die Finger flink.", "tokens": ["Das", "Gr\u00e4f\u00b7lein", "hebt", "die", "Fin\u00b7ger", "flink", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "(\u201eFrau Berte, du bist ein dummes Ding!\u201c)", "tokens": ["(", "\u201e", "Frau", "Ber\u00b7te", ",", "du", "bist", "ein", "dum\u00b7mes", "Ding", "!", "\u201c", ")"], "token_info": ["punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "$(", "NN", "NE", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$.", "$(", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "\u201etrautlieb, ich schw\u00f6r' und beschw\u00f6r' es dir", "tokens": ["\u201e", "traut\u00b7lieb", ",", "ich", "schw\u00f6r'", "und", "be\u00b7schw\u00f6r'", "es", "dir"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "$,", "PPER", "VVFIN", "KON", "VVFIN", "PPER", "PPER"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Bei den drei gemalten Rittern hier!\u201c", "tokens": ["Bei", "den", "drei", "ge\u00b7mal\u00b7ten", "Rit\u00b7tern", "hier", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "CARD", "ADJA", "NN", "ADV", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Jetzt rufen aus einem Mund die Drei:", "tokens": ["Jetzt", "ru\u00b7fen", "aus", "ei\u00b7nem", "Mund", "die", "Drei", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ART", "CARD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u201ees ist geredet und bleibt dabei!\u201c", "tokens": ["\u201e", "es", "ist", "ge\u00b7re\u00b7det", "und", "bleibt", "da\u00b7bei", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "VVPP", "KON", "VVFIN", "PAV", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.11": {"line.1": {"text": "Die Wand versinkt: dahinter stehn", "tokens": ["Die", "Wand", "ver\u00b7sinkt", ":", "da\u00b7hin\u00b7ter", "stehn"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVPP", "$.", "PAV", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drei g\u00fclt'ge Zeugen. So ist's geschehn.", "tokens": ["Drei", "g\u00fclt'\u00b7ge", "Zeu\u00b7gen", ".", "So", "ist's", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "$.", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}