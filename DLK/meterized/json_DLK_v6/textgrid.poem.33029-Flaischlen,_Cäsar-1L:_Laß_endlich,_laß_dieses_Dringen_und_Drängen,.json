{"textgrid.poem.33029": {"metadata": {"author": {"name": "Flaischlen, C\u00e4sar", "birth": "N.A.", "death": "N.A."}, "title": "1L: La\u00df endlich, la\u00df dieses Dringen und Dr\u00e4ngen,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df endlich, la\u00df dieses Dringen und Dr\u00e4ngen,", "tokens": ["La\u00df", "end\u00b7lich", ",", "la\u00df", "die\u00b7ses", "Drin\u00b7gen", "und", "Dr\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "VVIMP", "PDAT", "NN", "KON", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "dies m\u00fcdemachende Zwingen und Zw\u00e4ngen ...", "tokens": ["dies", "m\u00fc\u00b7de\u00b7ma\u00b7chen\u00b7de", "Zwin\u00b7gen", "und", "Zw\u00e4n\u00b7gen", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "KON", "NN", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "was einer tun kann, hast du getan!", "tokens": ["was", "ei\u00b7ner", "tun", "kann", ",", "hast", "du", "ge\u00b7tan", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVINF", "VMFIN", "$,", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "und mehr vielleicht!", "tokens": ["und", "mehr", "viel\u00b7leicht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Nun mache Rast", "tokens": ["Nun", "ma\u00b7che", "Rast"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "am gr\u00fcnen Hang ...", "tokens": ["am", "gr\u00fc\u00b7nen", "Hang", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "es mu\u00df jetzt f\u00fcr sich selber sorgen,", "tokens": ["es", "mu\u00df", "jetzt", "f\u00fcr", "sich", "sel\u00b7ber", "sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "es mu\u00df sich ohne deinen Rat", "tokens": ["es", "mu\u00df", "sich", "oh\u00b7ne", "dei\u00b7nen", "Rat"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "aus eigener Kraft jetzt weiterwenden", "tokens": ["aus", "ei\u00b7ge\u00b7ner", "Kraft", "jetzt", "wei\u00b7ter\u00b7wen\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADV", "ADJA"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "und sich zu seinem Ziel vollenden!", "tokens": ["und", "sich", "zu", "sei\u00b7nem", "Ziel", "voll\u00b7en\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du aber r\u00fcste dich und reife", "tokens": ["Du", "a\u00b7ber", "r\u00fcs\u00b7te", "dich", "und", "rei\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "zu neuen Schaffens froher Tat!", "tokens": ["zu", "neu\u00b7en", "Schaf\u00b7fens", "fro\u00b7her", "Tat", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}