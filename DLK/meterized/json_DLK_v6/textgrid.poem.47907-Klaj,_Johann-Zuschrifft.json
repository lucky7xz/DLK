{"textgrid.poem.47907": {"metadata": {"author": {"name": "Klaj, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Zuschrifft", "genre": "verse", "period": "N.A.", "pub_year": 1636, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kein Etna hat also gehitzet und gebrant", "tokens": ["Kein", "Et\u00b7na", "hat", "al\u00b7so", "ge\u00b7hit\u00b7zet", "und", "ge\u00b7brant"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "VVPP", "KON", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "als von der Kriegesglut das edle Teutsche Land", "tokens": ["als", "von", "der", "Krie\u00b7ges\u00b7glut", "das", "ed\u00b7le", "Teut\u00b7sche", "Land"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "nun in die dreissig Jahr; Er speye gleich Salpeter/", "tokens": ["nun", "in", "die", "dreis\u00b7sig", "Jahr", ";", "Er", "spey\u00b7e", "gleich", "Sal\u00b7pe\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "CARD", "NN", "$.", "PPER", "VVFIN", "ADV", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Pech/ Schwefel/ Kohlen/ Hartz; der Himmel werde r\u00f6ter/", "tokens": ["Pech", "/", "Schwe\u00b7fel", "/", "Koh\u00b7len", "/", "Hartz", ";", "der", "Him\u00b7mel", "wer\u00b7de", "r\u00f6\u00b7ter", "/"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "NN", "$(", "NN", "$(", "NE", "$.", "ART", "NN", "VAFIN", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "ob dieses Brenners Dampf; Der Ochsen M\u00fch verheert/", "tokens": ["ob", "die\u00b7ses", "Bren\u00b7ners", "Dampf", ";", "Der", "Och\u00b7sen", "M\u00fch", "ver\u00b7heert", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "NN", "$.", "ART", "NN", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "der Ochse mit dem Heu/ Vieh/ Futter/ aufgezehrt;", "tokens": ["der", "O\u00b7chse", "mit", "dem", "Heu", "/", "Vieh", "/", "Fut\u00b7ter", "/", "auf\u00b7ge\u00b7zehrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$(", "NN", "$(", "NN", "$(", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die St\u00e4dte stehn in Furcht/ die D\u00f6rfer in den Flammen/", "tokens": ["Die", "St\u00e4d\u00b7te", "stehn", "in", "Furcht", "/", "die", "D\u00f6r\u00b7fer", "in", "den", "Flam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$(", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "das halb erstorbne Volck das raffe was zusammen", "tokens": ["das", "halb", "ers\u00b7torb\u00b7ne", "Volck", "das", "raf\u00b7fe", "was", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN", "PDS", "VVFIN", "PIS", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "und lauffe/ was es kan/ der Weiber Zehrenflut", "tokens": ["und", "lauf\u00b7fe", "/", "was", "es", "kan", "/", "der", "Wei\u00b7ber", "Zeh\u00b7ren\u00b7flut"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$(", "PWS", "PPER", "VMFIN", "$(", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "flie\u00df h\u00e4uffig/ l\u00e4uffig ab/ als wolte sie die Glut", "tokens": ["flie\u00df", "h\u00e4uf\u00b7fig", "/", "l\u00e4uf\u00b7fig", "ab", "/", "als", "wol\u00b7te", "sie", "die", "Glut"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "$(", "ADJD", "PTKVZ", "$(", "KOUS", "VMFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "verleschen/ die nichts lescht. Das Teutsche Kriegesfeuer", "tokens": ["ver\u00b7le\u00b7schen", "/", "die", "nichts", "lescht", ".", "Das", "Teut\u00b7sche", "Krie\u00b7ges\u00b7feu\u00b7er"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "$(", "ART", "PIS", "VVFIN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "hat zehnmal mehr gebrennt/ mit wildem Vngeheuer", "tokens": ["hat", "zehn\u00b7mal", "mehr", "ge\u00b7brennt", "/", "mit", "wil\u00b7dem", "Vn\u00b7ge\u00b7heu\u00b7er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADV", "VVPP", "$(", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "entz\u00fcndet alle Welt/ unschwesterlich gehaust/", "tokens": ["ent\u00b7z\u00fcn\u00b7det", "al\u00b7le", "Welt", "/", "un\u00b7schwes\u00b7ter\u00b7lich", "ge\u00b7haust", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$(", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "da\u00df einem/ der es sagt und der es h\u00f6ret/ graust.", "tokens": ["da\u00df", "ei\u00b7nem", "/", "der", "es", "sagt", "und", "der", "es", "h\u00f6\u00b7ret", "/", "graust", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ART", "$(", "PRELS", "PPER", "VVFIN", "KON", "PRELS", "PPER", "VVFIN", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Kein Mensch/ kein Engel nicht die Glut entwerffen k\u00f6nte/", "tokens": ["Kein", "Mensch", "/", "kein", "En\u00b7gel", "nicht", "die", "Glut", "ent\u00b7werf\u00b7fen", "k\u00f6n\u00b7te", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$(", "PIAT", "NN", "PTKNEG", "ART", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "wanngleich das Himmelblat Papyr/ das Weltmeer Dinte/", "tokens": ["wann\u00b7gleich", "das", "Him\u00b7mel\u00b7blat", "Pa\u00b7pyr", "/", "das", "Welt\u00b7meer", "Din\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "$(", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "der Wald ein Schreibezeug. Kein Regenna\u00df hier lescht/", "tokens": ["der", "Wald", "ein", "Schrei\u00b7be\u00b7zeug", ".", "Kein", "Re\u00b7gen\u00b7na\u00df", "hier", "lescht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$.", "PIAT", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "wie sehr es immer tropft/ wie starck es immer w\u00e4scht.", "tokens": ["wie", "sehr", "es", "im\u00b7mer", "tropft", "/", "wie", "starck", "es", "im\u00b7mer", "w\u00e4scht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "ADV", "VVFIN", "$(", "PWAV", "ADJD", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ach! Mutter Teutschland rufft: Ich bin im Brand versuncken/", "tokens": ["Ach", "!", "Mut\u00b7ter", "Teutschland", "rufft", ":", "Ich", "bin", "im", "Brand", "ver\u00b7sun\u00b7cken", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "NN", "NE", "VVFIN", "$.", "PPER", "VAFIN", "APPRART", "NN", "VVINF", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "ist niemand/ der mich tr\u00fcg au\u00df Schwefelblauen Funcken?", "tokens": ["ist", "nie\u00b7mand", "/", "der", "mich", "tr\u00fcg", "au\u00df", "Schwe\u00b7felb\u00b7lau\u00b7en", "Fun\u00b7cken", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "$(", "PRELS", "PPER", "VVFIN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Hebt auf und tragt mich fort/ greifft unbesorget an/", "tokens": ["Hebt", "auf", "und", "tragt", "mich", "fort", "/", "greifft", "un\u00b7be\u00b7sor\u00b7get", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "VVFIN", "PPER", "PTKVZ", "$(", "VVFIN", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "der Eltern liebt und tr\u00e4gt/ kein Feuer schaden kan/", "tokens": ["der", "El\u00b7tern", "liebt", "und", "tr\u00e4gt", "/", "kein", "Feu\u00b7er", "scha\u00b7den", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$(", "PIAT", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "wie liechterloh es brennt. Die Gluten m\u00fcssen weichen", "tokens": ["wie", "liech\u00b7ter\u00b7loh", "es", "brennt", ".", "Die", "Glu\u00b7ten", "m\u00fcs\u00b7sen", "wei\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "VVFIN", "$.", "ART", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "dem/ den die Mutter liebt. Sie geben gar ein Zeichen", "tokens": ["dem", "/", "den", "die", "Mut\u00b7ter", "liebt", ".", "Sie", "ge\u00b7ben", "gar", "ein", "Zei\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "$(", "ART", "ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "der Lieb zur Mutterlieb; Wie jenem Br\u00fcderpar/", "tokens": ["der", "Lieb", "zur", "Mut\u00b7ter\u00b7lieb", ";", "Wie", "je\u00b7nem", "Br\u00fc\u00b7der\u00b7par", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$.", "PWAV", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "das hertzhafft au\u00df dem Schwal de\u00df Vatters graue Har", "tokens": ["das", "hertz\u00b7hafft", "au\u00df", "dem", "Schwal", "de\u00df", "Vat\u00b7ters", "grau\u00b7e", "Har"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "der krummen Mutter Hals \u00fcm ihre H\u00e4lse schluge", "tokens": ["der", "krum\u00b7men", "Mut\u00b7ter", "Hals", "\u00fcm", "ih\u00b7re", "H\u00e4l\u00b7se", "schlu\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "ADJD", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "als eine s\u00fcsse Last au\u00df Etnens Feuer truge/", "tokens": ["als", "ei\u00b7ne", "s\u00fcs\u00b7se", "Last", "au\u00df", "Et\u00b7nens", "Feu\u00b7er", "tru\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "NE", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "als ihres Reichthums Gut; die Flammen machten Weg", "tokens": ["als", "ih\u00b7res", "Reicht\u00b7hums", "Gut", ";", "die", "Flam\u00b7men", "mach\u00b7ten", "Weg"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "$.", "ART", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "au\u00df Furcht der Gottesfurcht. Der Erdbau bahnte Steg/", "tokens": ["au\u00df", "Furcht", "der", "Got\u00b7tes\u00b7furcht", ".", "Der", "Erd\u00b7bau", "bahn\u00b7te", "Steg", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$.", "ART", "NN", "VVFIN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "damit das seltne Thun auf ewig m\u00f6chte bleiben/", "tokens": ["da\u00b7mit", "das", "selt\u00b7ne", "Thun", "auf", "e\u00b7wig", "m\u00f6ch\u00b7te", "blei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "APPR", "ADJD", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "in B\u00fccher und in Stein durch kluge H\u00e4nde schreiben;", "tokens": ["in", "B\u00fc\u00b7cher", "und", "in", "Stein", "durch", "klu\u00b7ge", "H\u00e4n\u00b7de", "schrei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Wie man denn diesem Par ob treuer Kinderpflicht", "tokens": ["Wie", "man", "denn", "die\u00b7sem", "Par", "ob", "treu\u00b7er", "Kin\u00b7der\u00b7pflicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "ADV", "PDAT", "NN", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Gedichte hat Gedicht und Seulen aufgericht.", "tokens": ["Ge\u00b7dich\u00b7te", "hat", "Ge\u00b7dicht", "und", "Seu\u00b7len", "auf\u00b7ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Di\u00df habet ihr geh\u00f6rt/ ihr Teutschen Gottes Degen/", "tokens": ["Di\u00df", "ha\u00b7bet", "ihr", "ge\u00b7h\u00f6rt", "/", "ihr", "Teut\u00b7schen", "Got\u00b7tes", "De\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "VVFIN", "$(", "PPOSAT", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "weil nun die freche Glut nicht allerdiengs zu legen/", "tokens": ["weil", "nun", "die", "fre\u00b7che", "Glut", "nicht", "al\u00b7ler\u00b7diengs", "zu", "le\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "PTKNEG", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "so habt ihr aufgefast das liebe Mutterland", "tokens": ["so", "habt", "ihr", "auf\u00b7ge\u00b7fast", "das", "lie\u00b7be", "Mut\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "mit Sorgen/ Schwei\u00df und Flei\u00df getragen au\u00df dem Brand.", "tokens": ["mit", "Sor\u00b7gen", "/", "Schwei\u00df", "und", "Flei\u00df", "ge\u00b7tra\u00b7gen", "au\u00df", "dem", "Brand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "NN", "KON", "NN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Auch Teutschland gr\u00e4met sich nit \u00fcber seine wund\u1ebd/", "tokens": ["Auch", "Teutschland", "gr\u00e4\u00b7met", "sich", "nit", "\u00fc\u00b7ber", "sei\u00b7ne", "wun\u00b7d\u1ebd", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VVFIN", "PRF", "PTKNEG", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "erfreut sich mehr/ da\u00df sie so liebe S\u00f6hne funden/", "tokens": ["er\u00b7freut", "sich", "mehr", "/", "da\u00df", "sie", "so", "lie\u00b7be", "S\u00f6h\u00b7ne", "fun\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "$(", "KOUS", "PPER", "ADV", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "