{"textgrid.poem.48543": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1. Danklied", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Billich ists, da\u00df wir uns freuen", "tokens": ["Bil\u00b7lich", "ists", ",", "da\u00df", "wir", "uns", "freu\u00b7en"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "$,", "KOUS", "PPER", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "und mit lautem Jauchzen schreien:", "tokens": ["und", "mit", "lau\u00b7tem", "Jauch\u00b7zen", "schrei\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lob sei Gott und seiner Macht,", "tokens": ["Lob", "sei", "Gott", "und", "sei\u00b7ner", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "der die stolzen Feinde beuget,", "tokens": ["der", "die", "stol\u00b7zen", "Fein\u00b7de", "beu\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "und mit seiner Allmacht zeuget,", "tokens": ["und", "mit", "sei\u00b7ner", "All\u00b7macht", "zeu\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "da\u00df er uns noch nimmt in Acht!", "tokens": ["da\u00df", "er", "uns", "noch", "nimmt", "in", "Acht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVFIN", "APPR", "CARD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Zweimal kamen sie gezogen,", "tokens": ["Zwei\u00b7mal", "ka\u00b7men", "sie", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "zweimal sind sie auch geflogen,", "tokens": ["zwei\u00b7mal", "sind", "sie", "auch", "ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "nicht ohn' m\u00e4chtigen Verlust.", "tokens": ["nicht", "ohn'", "m\u00e4ch\u00b7ti\u00b7gen", "Ver\u00b7lust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schreit, ihr Jungen, ruft ihr Alten:", "tokens": ["Schreit", ",", "ihr", "Jun\u00b7gen", ",", "ruft", "ihr", "Al\u00b7ten", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "zweimal hat das Feld erhalten", "tokens": ["zwei\u00b7mal", "hat", "das", "Feld", "er\u00b7hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gott und unser Held ", "tokens": ["Gott", "und", "un\u00b7ser", "Held"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Held ", "tokens": ["Held"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "du gel\u00fcckesvoller Sieger", "tokens": ["du", "ge\u00b7l\u00fc\u00b7ckes\u00b7vol\u00b7ler", "Sie\u00b7ger"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "vor und in und nach dem Fall',", "tokens": ["vor", "und", "in", "und", "nach", "dem", "Fall'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "KON", "APPR", "KON", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "auf was Arten, auf was Weisen", "tokens": ["auf", "was", "Ar\u00b7ten", ",", "auf", "was", "Wei\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PRELS", "NN", "$,", "APPR", "PRELS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "soll man deine Taten preisen", "tokens": ["soll", "man", "dei\u00b7ne", "Ta\u00b7ten", "prei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "hier und dort und \u00fcberall?", "tokens": ["hier", "und", "dort", "und", "\u00fc\u00b7be\u00b7rall", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "KON", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Held, du kamest her vom Weiten,", "tokens": ["Held", ",", "du", "ka\u00b7mest", "her", "vom", "Wei\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "da\u00df du vor uns m\u00f6chtest streiten;", "tokens": ["da\u00df", "du", "vor", "uns", "m\u00f6ch\u00b7test", "strei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Held, du kamest, Held, du strittst,", "tokens": ["Held", ",", "du", "ka\u00b7mest", ",", "Held", ",", "du", "strittst", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "$,", "NN", "$,", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Held, du siegest auch im Sterben:", "tokens": ["Held", ",", "du", "sie\u00b7gest", "auch", "im", "Ster\u00b7ben", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Held, wie k\u00f6nnen wir verderben,", "tokens": ["Held", ",", "wie", "k\u00f6n\u00b7nen", "wir", "ver\u00b7der\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "weil du itzt noch f\u00fcr uns trittst?", "tokens": ["weil", "du", "itzt", "noch", "f\u00fcr", "uns", "trittst", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Deine R\u00e4te, deine Werke,", "tokens": ["Dei\u00b7ne", "R\u00e4\u00b7te", ",", "dei\u00b7ne", "Wer\u00b7ke", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "deine ritterliche St\u00e4rke", "tokens": ["dei\u00b7ne", "rit\u00b7ter\u00b7li\u00b7che", "St\u00e4r\u00b7ke"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "ruft aus, was nur rufen kan.", "tokens": ["ruft", "aus", ",", "was", "nur", "ru\u00b7fen", "kan", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PRELS", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die bezwungnen Str\u00f6me brausen,", "tokens": ["Die", "be\u00b7zwung\u00b7nen", "Str\u00f6\u00b7me", "brau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "die verbundnen L\u00fcfte sausen", "tokens": ["die", "ver\u00b7bund\u00b7nen", "L\u00fcf\u00b7te", "sau\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "was du, Helfer, hast getan.", "tokens": ["was", "du", ",", "Hel\u00b7fer", ",", "hast", "ge\u00b7tan", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "$,", "NN", "$,", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Elbe, F\u00fcrstin unsrer Fl\u00fcsse,", "tokens": ["El\u00b7be", ",", "F\u00fcrs\u00b7tin", "uns\u00b7rer", "Fl\u00fcs\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "mach dich auf die feuchten F\u00fc\u00dfe,", "tokens": ["mach", "dich", "auf", "die", "feuch\u00b7ten", "F\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "eile, laufe Nacht und Tag,", "tokens": ["ei\u00b7le", ",", "lau\u00b7fe", "Nacht", "und", "Tag", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "meld es mit beredten Wellen,", "tokens": ["meld", "es", "mit", "be\u00b7red\u00b7ten", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "da\u00df die Ufer widersch\u00e4llen,", "tokens": ["da\u00df", "die", "U\u00b7fer", "wi\u00b7der\u00b7sch\u00e4l\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "wie der Feind vor dir erschrak!", "tokens": ["wie", "der", "Feind", "vor", "dir", "er\u00b7schrak", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Die erblasseten Illyrer", "tokens": ["Die", "er\u00b7blas\u00b7se\u00b7ten", "Il\u00b7ly\u00b7rer"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "wichen mitsampt ihrem F\u00fchrer", "tokens": ["wi\u00b7chen", "mit\u00b7sampt", "ih\u00b7rem", "F\u00fch\u00b7rer"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "hinter sich und fielen hin,", "tokens": ["hin\u00b7ter", "sich", "und", "fie\u00b7len", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "wie f\u00fcr Jovis Donnerkeilen,", "tokens": ["wie", "f\u00fcr", "Jo\u00b7vis", "Don\u00b7ner\u00b7kei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "wie f\u00fcr Herkuls heilgen S\u00e4ulen,", "tokens": ["wie", "f\u00fcr", "Her\u00b7kuls", "heil\u00b7gen", "S\u00e4u\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "die man nicht soll \u00fcberziehn.", "tokens": ["die", "man", "nicht", "soll", "\u00fc\u00b7ber\u00b7ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Sch\u00f6ne Stadt, der fromme Himmel,", "tokens": ["Sch\u00f6\u00b7ne", "Stadt", ",", "der", "from\u00b7me", "Him\u00b7mel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "der verschuf ein solch Get\u00fcmmel,", "tokens": ["der", "ver\u00b7schuf", "ein", "solch", "Ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "ART", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "ein solch Schrecken in dem Feind',", "tokens": ["ein", "solch", "Schre\u00b7cken", "in", "dem", "Feind'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "da\u00df der sch\u00e4ndlich muste fliehen,", "tokens": ["da\u00df", "der", "sch\u00e4nd\u00b7lich", "mus\u00b7te", "flie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "der dich grimmig auszuziehen", "tokens": ["der", "dich", "grim\u00b7mig", "aus\u00b7zu\u00b7zie\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "ADJD", "VVIZU"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "und zu pl\u00fcndern war gemeint.", "tokens": ["und", "zu", "pl\u00fcn\u00b7dern", "war", "ge\u00b7meint", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKZU", "VVINF", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Seid nun froh, ihr frommen B\u00fcrger!", "tokens": ["Seid", "nun", "froh", ",", "ihr", "from\u00b7men", "B\u00fcr\u00b7ger", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADV", "ADJD", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Er ist tot, der wilde W\u00fcrger,", "tokens": ["Er", "ist", "tot", ",", "der", "wil\u00b7de", "W\u00fcr\u00b7ger", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "er ist tot und ihr seid frei.", "tokens": ["er", "ist", "tot", "und", "ihr", "seid", "frei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "PPER", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihr und wir und Alle sagen,", "tokens": ["Ihr", "und", "wir", "und", "Al\u00b7le", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "PPER", "KON", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "da\u00df sich Gott f\u00fcr uns geschlagen,", "tokens": ["da\u00df", "sich", "Gott", "f\u00fcr", "uns", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "da\u00df die Ehre seine sei.", "tokens": ["da\u00df", "die", "Eh\u00b7re", "sei\u00b7ne", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Ist schon unser Heiland blieben,", "tokens": ["Ist", "schon", "un\u00b7ser", "Hei\u00b7land", "blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gott hat Einen schon verschrieben,", "tokens": ["Gott", "hat", "Ei\u00b7nen", "schon", "ver\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "der ihn r\u00e4chen kan und soll,", "tokens": ["der", "ihn", "r\u00e4\u00b7chen", "kan", "und", "soll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "VMFIN", "KON", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "ihn und uns und alle Frommen.", "tokens": ["ihn", "und", "uns", "und", "al\u00b7le", "From\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "PPER", "KON", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Kommt er? Ja, er ist schon kommen.", "tokens": ["Kommt", "er", "?", "Ja", ",", "er", "ist", "schon", "kom\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "PTKANT", "$,", "PPER", "VAFIN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Luthrische, gehabt euch wol!", "tokens": ["Luth\u00b7ri\u00b7sche", ",", "ge\u00b7habt", "euch", "wol", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VAPP", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}