{"textgrid.poem.61977": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "Die Jagd", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hell der Himmel ist erleuchtet,", "tokens": ["Hell", "der", "Him\u00b7mel", "ist", "er\u00b7leuch\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sonnenstrahlen hin und her,", "tokens": ["Son\u00b7nen\u00b7strah\u00b7len", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Frischer Tau den Rasen feuchtet,", "tokens": ["Fri\u00b7scher", "Tau", "den", "Ra\u00b7sen", "feuch\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Silbern gl\u00e4nzt das Jagdgewehr.", "tokens": ["Sil\u00b7bern", "gl\u00e4nzt", "das", "Jagd\u00b7ge\u00b7wehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Eine Jagd ist's! Blutig jagend", "tokens": ["Ei\u00b7ne", "Jagd", "ist's", "!", "Blu\u00b7tig", "ja\u00b7gend"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VAFIN", "$.", "ADJD", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eilt der J\u00e4ger durch den Wald,", "tokens": ["Eilt", "der", "J\u00e4\u00b7ger", "durch", "den", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fcr das B\u00f6se Alles wagend,", "tokens": ["F\u00fcr", "das", "B\u00f6\u00b7se", "Al\u00b7les", "wa\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PIS", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mordruf weit und breit erschallt!", "tokens": ["Mord\u00b7ruf", "weit", "und", "breit", "er\u00b7schallt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Aufgescheucht flieh'n junge Rehe", "tokens": ["Auf\u00b7ge\u00b7scheucht", "flieh'n", "jun\u00b7ge", "Re\u00b7he"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von dem blut'gen Schauplatz fort,", "tokens": ["Von", "dem", "blut'\u00b7gen", "Schau\u00b7platz", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch der J\u00e4ger Todesn\u00e4he", "tokens": ["Doch", "der", "J\u00e4\u00b7ger", "To\u00b7des\u00b7n\u00e4\u00b7he"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eilet nach von Ort zu Ort.", "tokens": ["Ei\u00b7let", "nach", "von", "Ort", "zu", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPR", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Mit der Hast, dem wilden Grimme,", "tokens": ["Mit", "der", "Hast", ",", "dem", "wil\u00b7den", "Grim\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der das B\u00f6se gern besch\u00f6nt,", "tokens": ["Der", "das", "B\u00f6\u00b7se", "gern", "be\u00b7sch\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der bet\u00e4ubend jene Stimme", "tokens": ["Der", "be\u00b7t\u00e4u\u00b7bend", "je\u00b7ne", "Stim\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ernsten Mahnens wild verh\u00f6hnt.", "tokens": ["Erns\u00b7ten", "Mah\u00b7nens", "wild", "ver\u00b7h\u00f6hnt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Bei dem blut'gen Reh daneben", "tokens": ["Bei", "dem", "blut'\u00b7gen", "Reh", "da\u00b7ne\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "PAV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Steht der Sch\u00fctze, blutig rot:", "tokens": ["Steht", "der", "Sch\u00fct\u00b7ze", ",", "blu\u00b7tig", "rot", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJD", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbr\u00e4che Gott, mein schuldlos Leben\u00ab \u2013", "tokens": ["\u00bb", "r\u00e4\u00b7che", "Gott", ",", "mein", "schuld\u00b7los", "Le\u00b7ben", "\u00ab", "\u2013"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "PPOSAT", "ADJD", "NN", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fleht das Tier vor seinem Tod.", "tokens": ["Fleht", "das", "Tier", "vor", "sei\u00b7nem", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Trotzig gl\u00e4nzt des Waidmanns Miene", "tokens": ["Trot\u00b7zig", "gl\u00e4nzt", "des", "Waid\u00b7manns", "Mie\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bei des jungen Rehes Blut", "tokens": ["Bei", "des", "jun\u00b7gen", "Re\u00b7hes", "Blut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es war, als wenn's ihm schiene", "tokens": ["Und", "es", "war", ",", "als", "wenn's", "ihm", "schie\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "$,", "KOUS", "PIS", "PPER", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Heute h\u00e4tt' er Gl\u00fcck und Gut! \u2013", "tokens": ["Heu\u00b7te", "h\u00e4tt'", "er", "Gl\u00fcck", "und", "Gut", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "KON", "ADJD", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "\u00bbo, da\u00df ich den Bock erwische\u00ab", "tokens": ["\u00bb", "o", ",", "da\u00df", "ich", "den", "Bock", "er\u00b7wi\u00b7sche", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "$,", "KOUS", "PPER", "ART", "NE", "ADJA", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und so st\u00fcrzt er rasend fort,", "tokens": ["Und", "so", "st\u00fcrzt", "er", "ra\u00b7send", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und bleibt h\u00e4ngen im Geb\u00fcsche,", "tokens": ["Und", "bleibt", "h\u00e4n\u00b7gen", "im", "Ge\u00b7b\u00fc\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVFIN", "APPRART", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Fremdes Ro\u00df, es tummelt dort;", "tokens": ["Frem\u00b7des", "Ro\u00df", ",", "es", "tum\u00b7melt", "dort", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Schleift den J\u00e4ger zu der Haide,", "tokens": ["Schleift", "den", "J\u00e4\u00b7ger", "zu", "der", "Hai\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo das Tier getroffen liegt,", "tokens": ["Wo", "das", "Tier", "ge\u00b7trof\u00b7fen", "liegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Still am Boden liegen Beide,", "tokens": ["Still", "am", "Bo\u00b7den", "lie\u00b7gen", "Bei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVFIN", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schuldlos Reh hat obgesiegt!", "tokens": ["Schuld\u00b7los", "Reh", "hat", "ob\u00b7ge\u00b7siegt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Endlich macht es eine Runde,", "tokens": ["End\u00b7lich", "macht", "es", "ei\u00b7ne", "Run\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Endlich steht das wilde Ro\u00df,", "tokens": ["End\u00b7lich", "steht", "das", "wil\u00b7de", "Ro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch in selbiger Sekunde", "tokens": ["Doch", "in", "sel\u00b7bi\u00b7ger", "Se\u00b7kun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Geht des J\u00e4gers B\u00fcchse los!", "tokens": ["Geht", "des", "J\u00e4\u00b7gers", "B\u00fcch\u00b7se", "los", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "J\u00e4ger schaut's mit stierem Blicke,", "tokens": ["J\u00e4\u00b7ger", "schaut's", "mit", "stie\u00b7rem", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schmerz durchzuckt sein Angesicht:", "tokens": ["Schmerz", "durch\u00b7zuckt", "sein", "An\u00b7ge\u00b7sicht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "J\u00e4ger, traue Deinem Gl\u00fccke,", "tokens": ["J\u00e4\u00b7ger", ",", "trau\u00b7e", "Dei\u00b7nem", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deiner wilden Jagdlust nicht! \u2013", "tokens": ["Dei\u00b7ner", "wil\u00b7den", "Jagd\u00b7lust", "nicht", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKNEG", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}