{"dta.poem.2041": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Hirsch .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer kann einen edlen Hirsch ohn Bewundrung und", "tokens": ["Wer", "kann", "ei\u00b7nen", "ed\u00b7len", "Hirsch", "ohn", "Be\u00b7wund\u00b7rung", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "ART", "ADJA", "NN", "APPR", "NN", "KON"], "meter": "----+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vergn\u00fcgen,", "tokens": ["Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "In so rasch- und munterm Anstand, laufen, stehen oder", "tokens": ["In", "so", "rasch", "und", "mun\u00b7term", "An\u00b7stand", ",", "lau\u00b7fen", ",", "ste\u00b7hen", "o\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "ADV", "TRUNC", "KON", "ADJA", "NN", "$,", "VVFIN", "$,", "VVFIN", "KON"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "liegen,", "tokens": ["lie\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Fliehen oder weiden seh\u2019n? Seine herrliche Gestalt,", "tokens": ["Flie\u00b7hen", "o\u00b7der", "wei\u00b7den", "seh'n", "?", "Sei\u00b7ne", "herr\u00b7li\u00b7che", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "VVINF", "$.", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Sein so leicht- als fester Tritt, zieh\u2019n mit fr\u00f6licher Gewalt", "tokens": ["Sein", "so", "leicht", "als", "fes\u00b7ter", "Tritt", ",", "zieh'n", "mit", "fr\u00f6\u00b7li\u00b7cher", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADV", "TRUNC", "KON", "ADJA", "NN", "$,", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "Ein betrachtend Aug\u2019 auf ihn. Sein erhabenes Geweih,", "tokens": ["Ein", "be\u00b7trach\u00b7tend", "Aug'", "auf", "ihn", ".", "Sein", "er\u00b7ha\u00b7be\u00b7nes", "Ge\u00b7weih", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPR", "PPER", "$.", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Die benervte schlanke Schenkel, kurz, des ganzen K\u00f6r-", "tokens": ["Die", "be\u00b7nerv\u00b7te", "schlan\u00b7ke", "Schen\u00b7kel", ",", "kurz", ",", "des", "gan\u00b7zen", "K\u00f6r"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,", "ADJD", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.9": {"text": "pers Bau", "tokens": ["pers", "Bau"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.10": {"text": "Zeiget einen weisen Urstand, leget eine Macht zur Schau,", "tokens": ["Zei\u00b7get", "ei\u00b7nen", "wei\u00b7sen", "Ur\u00b7stand", ",", "le\u00b7get", "ei\u00b7ne", "Macht", "zur", "Schau", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.11": {"text": "Und weist eine Lieb\u2019 und Vorsorg\u2019 auch zugleich f\u00fcr uns", "tokens": ["Und", "weist", "ei\u00b7ne", "Lieb'", "und", "Vor\u00b7sor\u00b7g'", "auch", "zu\u00b7gleich", "f\u00fcr", "uns"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "KON", "NN", "ADV", "ADV", "APPR", "PPER"], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "dabey,", "tokens": ["da\u00b7bey", ","], "token_info": ["word", "punct"], "pos": ["PAV", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.13": {"text": "Da sein angenehmes Fleisch, das er uns zur Kost gewehrt,", "tokens": ["Da", "sein", "an\u00b7ge\u00b7neh\u00b7mes", "Fleisch", ",", "das", "er", "uns", "zur", "Kost", "ge\u00b7wehrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "$,", "PRELS", "PPER", "PRF", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "Uns, auf so verschiedne Weis\u2019 zugericht, ergetzt und n\u00e4hrt.", "tokens": ["Uns", ",", "auf", "so", "ver\u00b7schied\u00b7ne", "Weis'", "zu\u00b7ge\u00b7richt", ",", "er\u00b7getzt", "und", "n\u00e4hrt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "ADV", "ADJA", "NN", "VVPP", "$,", "VVPP", "KON", "VVFIN", "$."], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Sollte denn der Mensch nicht billig, wie in allen andern", "tokens": ["Soll\u00b7te", "denn", "der", "Mensch", "nicht", "bil\u00b7lig", ",", "wie", "in", "al\u00b7len", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ART", "NN", "PTKNEG", "ADJD", "$,", "PWAV", "APPR", "PIAT", "ADJA"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.16": {"text": "Werken,", "tokens": ["Wer\u00b7ken", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.17": {"text": "Auch in diesem sch\u00f6nen Thier\u2019 Spuren einer Gottheit", "tokens": ["Auch", "in", "die\u00b7sem", "sch\u00f6\u00b7nen", "Thier'", "Spu\u00b7ren", "ei\u00b7ner", "Got\u00b7theit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PDAT", "ADJA", "NN", "NN", "ART", "NN"], "meter": "--+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "merken?", "tokens": ["mer\u00b7ken", "?"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.19": {"text": "Und, voll Lust und Dank, begreifen, finden, f\u00fchlen,", "tokens": ["Und", ",", "voll", "Lust", "und", "Dank", ",", "be\u00b7grei\u00b7fen", ",", "fin\u00b7den", ",", "f\u00fch\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "$,", "ADJD", "NN", "KON", "NN", "$,", "VVINF", "$,", "VVINF", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.20": {"text": "schmecken, seh'n,", "tokens": ["schme\u00b7cken", ",", "seh'n", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.21": {"text": "Da\u00df sein\u2019 Allmacht zu bewundern, seine Weisheit zu er-", "tokens": ["Da\u00df", "sein'", "All\u00b7macht", "zu", "be\u00b7wun\u00b7dern", ",", "sei\u00b7ne", "Weis\u00b7heit", "zu", "er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PTKZU", "VVINF", "$,", "PPOSAT", "NN", "APPR", "TRUNC"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "h\u00f6h'n?", "tokens": ["h\u00f6h'n", "?"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}}}}}