{"dta.poem.6079": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Sonne, den Planeten,  \n und vom Firmament.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die C\u00f6rper, deren Glantz und funckelnde Figur,", "tokens": ["Die", "C\u00f6r\u00b7per", ",", "de\u00b7ren", "Glantz", "und", "fun\u00b7ckeln\u00b7de", "Fi\u00b7gur", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELAT", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Jhr\u2019 unbekannte Quell und zweifelhaffte Spur,", "tokens": ["Ih\u00b7r'", "un\u00b7be\u00b7kann\u00b7te", "Quell", "und", "zwei\u00b7fel\u00b7haff\u00b7te", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "Bey denen, welche weise seyn,", "tokens": ["Bey", "de\u00b7nen", ",", "wel\u00b7che", "wei\u00b7se", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "$,", "PRELS", "ADJD", "VAINF", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Viel wichtige Vermuthungen erwecken;", "tokens": ["Viel", "wich\u00b7ti\u00b7ge", "Ver\u00b7mu\u00b7thun\u00b7gen", "er\u00b7we\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die w\u00fcrcken, durch den neuen Strahl und Schein,", "tokens": ["Die", "w\u00fcr\u00b7cken", ",", "durch", "den", "neu\u00b7en", "Strahl", "und", "Schein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVINF", "$,", "APPR", "ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Den Unvern\u00fcnfftigen offt ein fatales Schrecken:", "tokens": ["Den", "Un\u00b7ver\u00b7n\u00fcnff\u00b7ti\u00b7gen", "offt", "ein", "fa\u00b7ta\u00b7les", "Schre\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da man sie, f\u00fcr die Grossen dieser Welt,", "tokens": ["Da", "man", "sie", ",", "f\u00fcr", "die", "Gros\u00b7sen", "die\u00b7ser", "Welt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "$,", "APPR", "ART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Vor ein gef\u00e4hrlich Zeichen h\u00e4lt.", "tokens": ["Vor", "ein", "ge\u00b7f\u00e4hr\u00b7lich", "Zei\u00b7chen", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Welch eine Zahl so eiteler Gedichte", "tokens": ["Welch", "ei\u00b7ne", "Zahl", "so", "ei\u00b7te\u00b7ler", "Ge\u00b7dich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ART", "NN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Erz\u00e4hlen uns hievon doch die Geschichte!", "tokens": ["Er\u00b7z\u00e4h\u00b7len", "uns", "hie\u00b7von", "doch", "die", "Ge\u00b7schich\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PAV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Allein,", "tokens": ["Al\u00b7lein", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.12": {"text": "Weswegen sollen sie doch so gef\u00e4hrlich seyn?", "tokens": ["Wes\u00b7we\u00b7gen", "sol\u00b7len", "sie", "doch", "so", "ge\u00b7f\u00e4hr\u00b7lich", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die Sonne macht sie hell, auf eine kurtze Weile.", "tokens": ["Die", "Son\u00b7ne", "macht", "sie", "hell", ",", "auf", "ei\u00b7ne", "kurt\u00b7ze", "Wei\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Indem sie nun von uns so ferne;", "tokens": ["In\u00b7dem", "sie", "nun", "von", "uns", "so", "fer\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPER", "ADV", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "So sind die grausen Schrecken Pfeile", "tokens": ["So", "sind", "die", "grau\u00b7sen", "Schre\u00b7cken", "Pfei\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Der fremden Sterne,", "tokens": ["Der", "frem\u00b7den", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.17": {"text": "Ein albern eingebildet Dr\u00e4uen,", "tokens": ["Ein", "al\u00b7bern", "ein\u00b7ge\u00b7bil\u00b7det", "Dr\u00e4u\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVPP", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Das keine kluge Geister schenen.", "tokens": ["Das", "kei\u00b7ne", "klu\u00b7ge", "Geis\u00b7ter", "sche\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}