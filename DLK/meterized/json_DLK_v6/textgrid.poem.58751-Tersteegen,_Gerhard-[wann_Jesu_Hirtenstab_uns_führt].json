{"textgrid.poem.58751": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "[wann Jesu Hirtenstab uns f\u00fchrt]", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das herrliche Reich Jesu Christi auf Erden, Jes. 11, 6-9", "tokens": ["Das", "herr\u00b7li\u00b7che", "Reich", "Je\u00b7su", "Chris\u00b7ti", "auf", "Er\u00b7den", ",", "Jes", ".", "11", ",", "6", "9"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "number", "punct", "number", "word", "number"], "pos": ["ART", "ADJA", "NN", "NE", "NE", "APPR", "NN", "$,", "NE", "$.", "CARD", "$,", "CARD", "$(", "CARD"], "meter": "-+--++-+--+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Wann Jesu Hirtenstab uns f\u00fchrt,", "tokens": ["Wann", "Je\u00b7su", "Hir\u00b7ten\u00b7stab", "uns", "f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Tigertier die Wut verliert;", "tokens": ["Ein", "Ti\u00b7ger\u00b7tier", "die", "Wut", "ver\u00b7liert", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf seines sanften Geistes Winken", "tokens": ["Auf", "sei\u00b7nes", "sanf\u00b7ten", "Geis\u00b7tes", "Win\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der L\u00f6we seinen Grimm l\u00e4\u00dft sinken,", "tokens": ["Der", "L\u00f6\u00b7we", "sei\u00b7nen", "Grimm", "l\u00e4\u00dft", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "PPOSAT", "NE", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Wolf wird gar zum L\u00e4mmelein,", "tokens": ["Der", "Wolf", "wird", "gar", "zum", "L\u00e4m\u00b7mel\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein gro\u00dfer Mann zum Kinde klein.", "tokens": ["Ein", "gro\u00b7\u00dfer", "Mann", "zum", "Kin\u00b7de", "klein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Komm, g\u00fcldne Zeit, erf\u00fcll die Erde,", "tokens": ["Komm", ",", "g\u00fcld\u00b7ne", "Zeit", ",", "er\u00b7f\u00fcll", "die", "Er\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df nur ", "tokens": ["Da\u00df", "nur"], "token_info": ["word", "word"], "pos": ["KOUS", "ADV"], "meter": "+-", "measure": "trochaic.single"}}}}}