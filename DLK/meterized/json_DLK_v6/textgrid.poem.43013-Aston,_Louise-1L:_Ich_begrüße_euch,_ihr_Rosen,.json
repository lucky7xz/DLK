{"textgrid.poem.43013": {"metadata": {"author": {"name": "Aston, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich begr\u00fc\u00dfe euch, ihr Rosen,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich begr\u00fc\u00dfe euch, ihr Rosen,", "tokens": ["Ich", "be\u00b7gr\u00fc\u00b7\u00dfe", "euch", ",", "ihr", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der Freiheit wilder Pracht,", "tokens": ["In", "der", "Frei\u00b7heit", "wil\u00b7der", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eingewiegt von Sturmestosen,", "tokens": ["Ein\u00b7ge\u00b7wiegt", "von", "Stur\u00b7mes\u00b7to\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gro\u00dfges\u00e4ugt vom Thau der Nacht!", "tokens": ["Gro\u00df\u00b7ge\u00b7s\u00e4ugt", "vom", "Thau", "der", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nicht im traulichen Gehege,", "tokens": ["Nicht", "im", "trau\u00b7li\u00b7chen", "Ge\u00b7he\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.6": {"text": "In des Gartens Mutterscho\u00df,", "tokens": ["In", "des", "Gar\u00b7tens", "Mut\u00b7ter\u00b7scho\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ohne eines G\u00e4rtners Pflege", "tokens": ["Oh\u00b7ne", "ei\u00b7nes", "G\u00e4rt\u00b7ners", "Pfle\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wird das Kind der Berge gro\u00df.", "tokens": ["Wird", "das", "Kind", "der", "Ber\u00b7ge", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wolken, die sich niedersenken,", "tokens": ["Wol\u00b7ken", ",", "die", "sich", "nie\u00b7der\u00b7sen\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wolken voll Gewitterglut,", "tokens": ["Wol\u00b7ken", "voll", "Ge\u00b7wit\u00b7ter\u00b7glut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00fcssen seine Kelche tr\u00e4nken,", "tokens": ["M\u00fcs\u00b7sen", "sei\u00b7ne", "Kel\u00b7che", "tr\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tr\u00e4nken mit lebend'ger Flut.", "tokens": ["Tr\u00e4n\u00b7ken", "mit", "le\u00b7ben\u00b7d'\u00b7ger", "Flut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Dr\u00fcberhin im Abendrothe", "tokens": ["Dr\u00fc\u00b7berh\u00b7in", "im", "A\u00b7ben\u00b7dro\u00b7the"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Tr\u00e4umerisch die H\u00f6h'n ergl\u00fch'n,", "tokens": ["Tr\u00e4u\u00b7me\u00b7risch", "die", "H\u00f6h'n", "er\u00b7gl\u00fch'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und der Blitz, der irre Bote,", "tokens": ["Und", "der", "Blitz", ",", "der", "ir\u00b7re", "Bo\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Gr\u00fc\u00dft es im Vor\u00fcberflieh'n.", "tokens": ["Gr\u00fc\u00dft", "es", "im", "Vor\u00b7\u00fcber\u00b7flieh'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "NE"], "meter": "++-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Einen Kranz von wilden Rosen", "tokens": ["Ei\u00b7nen", "Kranz", "von", "wil\u00b7den", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wand das Schicksal mir in's Haar,", "tokens": ["Wand", "das", "Schick\u00b7sal", "mir", "in's", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mir, der Fremden, Heimathlosen,", "tokens": ["Mir", ",", "der", "Frem\u00b7den", ",", "Hei\u00b7math\u00b7lo\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In den St\u00fcrmen der Gefahr.", "tokens": ["In", "den", "St\u00fcr\u00b7men", "der", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wilde Rosen: \u2013 ", "tokens": ["Wil\u00b7de", "Ro\u00b7sen", ":", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$.", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Voll von Lebens-Uebermuth,", "tokens": ["Voll", "von", "Le\u00b7bens\u00b7Ue\u00b7ber\u00b7muth", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wuchernd auf in \u00fcpp'gen Ranken,", "tokens": ["Wu\u00b7chernd", "auf", "in", "\u00fcpp'\u00b7gen", "Ran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Prangend in Gewitterglut!", "tokens": ["Pran\u00b7gend", "in", "Ge\u00b7wit\u00b7ter\u00b7glut", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch zu fr\u00fch ins wilde Leben", "tokens": ["Doch", "zu", "fr\u00fch", "ins", "wil\u00b7de", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKA", "ADJD", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trieb mich eine finstre Macht;", "tokens": ["Trieb", "mich", "ei\u00b7ne", "finst\u00b7re", "Macht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meiner Jugend Bilder schweben", "tokens": ["Mei\u00b7ner", "Ju\u00b7gend", "Bil\u00b7der", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einsam durch den Traum der Nacht!", "tokens": ["Ein\u00b7sam", "durch", "den", "Traum", "der", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und von Mi\u00dfgeschick zerschmettert", "tokens": ["Und", "von", "Mi\u00df\u00b7ge\u00b7schick", "zer\u00b7schmet\u00b7tert"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Klagt in Trauer mein Gem\u00fcth:", "tokens": ["Klagt", "in", "Trau\u00b7er", "mein", "Ge\u00b7m\u00fcth", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Meine Rosen sind entbl\u00e4ttert,", "tokens": ["Mei\u00b7ne", "Ro\u00b7sen", "sind", "ent\u00b7bl\u00e4t\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ihre Farbenpracht vergl\u00fcht!", "tokens": ["Ih\u00b7re", "Far\u00b7ben\u00b7pracht", "ver\u00b7gl\u00fcht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Einsam, wie dem Geisterzuge", "tokens": ["Ein\u00b7sam", ",", "wie", "dem", "Geis\u00b7ter\u00b7zu\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blinde Seher einst gelauscht;", "tokens": ["Blin\u00b7de", "Se\u00b7her", "einst", "ge\u00b7lauscht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lausch' ich dem Gedankenfluge,", "tokens": ["Lausch'", "ich", "dem", "Ge\u00b7dan\u00b7ken\u00b7flu\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der im Sturm vor\u00fcberrauscht \u2013", "tokens": ["Der", "im", "Sturm", "vor\u00b7\u00fc\u00b7berr\u00b7auscht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Meines Lebens irre Geister,", "tokens": ["Mei\u00b7nes", "Le\u00b7bens", "ir\u00b7re", "Geis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Haltet ein auf mein Gehei\u00df!", "tokens": ["Hal\u00b7tet", "ein", "auf", "mein", "Ge\u00b7hei\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Euch beschw\u00f6rt der Zaubermeister,", "tokens": ["Euch", "be\u00b7schw\u00f6rt", "der", "Zau\u00b7ber\u00b7meis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Bannt euch in der Dichtung Kreis!", "tokens": ["Bannt", "euch", "in", "der", "Dich\u00b7tung", "Kreis", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und aus sch\u00f6nen, gl\u00fch'nden N\u00e4chten,", "tokens": ["Und", "aus", "sch\u00f6\u00b7nen", ",", "gl\u00fch'n\u00b7den", "N\u00e4ch\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und aus Tr\u00e4umen frei und k\u00fchn,", "tokens": ["Und", "aus", "Tr\u00e4u\u00b7men", "frei", "und", "k\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Will ich wilde Rosen flechten,", "tokens": ["Will", "ich", "wil\u00b7de", "Ro\u00b7sen", "flech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die in ihrem Thau erbl\u00fch'n!", "tokens": ["Die", "in", "ih\u00b7rem", "Thau", "er\u00b7bl\u00fch'n", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Flechten mir der Dichtung Rosen", "tokens": ["Flech\u00b7ten", "mir", "der", "Dich\u00b7tung", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In der Freiheit wilder Pracht,", "tokens": ["In", "der", "Frei\u00b7heit", "wil\u00b7der", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Eingewiegt von Liebeskosen,", "tokens": ["Ein\u00b7ge\u00b7wiegt", "von", "Lie\u00b7bes\u00b7ko\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Gro\u00dfges\u00e4ugt vom Thau der Nacht!", "tokens": ["Gro\u00df\u00b7ge\u00b7s\u00e4ugt", "vom", "Thau", "der", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}