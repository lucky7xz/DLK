{"dta.poem.21006": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "LxX.  \n Eines alten Br\u00e4utigams.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Cupido jagte mir die Pfeile nach dem Hertzen/", "tokens": ["Cu\u00b7pi\u00b7do", "jag\u00b7te", "mir", "die", "Pfei\u00b7le", "nach", "dem", "Hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.2": {"text": "Es gab mir wenig Krafft und nicht geringe Schmer-", "tokens": ["Es", "gab", "mir", "we\u00b7nig", "Krafft", "und", "nicht", "ge\u00b7rin\u00b7ge", "Schmer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "KON", "PTKNEG", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "tzen.", "tokens": ["tzen", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Der Wille war bereit/ die Sehnen fehlten mir/", "tokens": ["Der", "Wil\u00b7le", "war", "be\u00b7reit", "/", "die", "Seh\u00b7nen", "fehl\u00b7ten", "mir", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$(", "ART", "NN", "VVFIN", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mein Lieb k\u00fcst frisches Fleisch/ ich faule schon al-", "tokens": ["Mein", "Lieb", "k\u00fcst", "fri\u00b7sches", "Fleisch", "/", "ich", "fau\u00b7le", "schon", "al"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJA", "NN", "$(", "PPER", "VVFIN", "ADV", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "hier.", "tokens": ["hier", "."], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-", "measure": "single.down"}}}}}