{"dta.poem.563": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Neu-Jahrs-Wunsch an eine vornehme  \n G\u00f6nnerinn im Nahmen eines andern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Als noch das alte Rom Minerven Opffer brachte/", "tokens": ["Als", "noch", "das", "al\u00b7te", "Rom", "Mi\u00b7ner\u00b7ven", "Opf\u00b7fer", "brach\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NE", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Wie es den Eichen Krantz um ", "tokens": ["Wie", "es", "den", "Ei\u00b7chen", "Krantz", "um"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und gantz erfreuet sprach: Nun ist das neue Jahr.", "tokens": ["Und", "gantz", "er\u00b7freu\u00b7et", "sprach", ":", "Nun", "ist", "das", "neu\u00b7e", "Jahr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "VVFIN", "$.", "ADV", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da sah man die bem\u00fcht mit W\u00fcnschen und mit Gaben/", "tokens": ["Da", "sah", "man", "die", "be\u00b7m\u00fcht", "mit", "W\u00fcn\u00b7schen", "und", "mit", "Ga\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ART", "VVFIN", "APPR", "NN", "KON", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die ein danckbares Hertz vermeyneten zu haben.", "tokens": ["Die", "ein", "dan\u00b7ck\u00b7ba\u00b7res", "Hertz", "ver\u00b7mey\u00b7ne\u00b7ten", "zu", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVINF", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.7": {"text": "Der weise ", "tokens": ["Der", "wei\u00b7se"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Wenn er die Danckbarkeit die sch\u00f6nste Tugend nennt.", "tokens": ["Wenn", "er", "die", "Dan\u00b7ck\u00b7bar\u00b7keit", "die", "sch\u00f6ns\u00b7te", "Tu\u00b7gend", "nennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Sie ist des Himmels-Bild und eine Brut von oben/", "tokens": ["Sie", "ist", "des", "Him\u00b7mels\u00b7Bild", "und", "ei\u00b7ne", "Brut", "von", "o\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "ART", "NN", "APPR", "ADV", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Aus deren Trieben man den sch\u00f6nen Ursprung kennt/", "tokens": ["Aus", "de\u00b7ren", "Trie\u00b7ben", "man", "den", "sch\u00f6\u00b7nen", "Ur\u00b7sprung", "kennt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PIS", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die G\u00f6tter lieben sie/ und alle die sie lieben/", "tokens": ["Die", "G\u00f6t\u00b7ter", "lie\u00b7ben", "sie", "/", "und", "al\u00b7le", "die", "sie", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$(", "KON", "PIAT", "PRELS", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ja sie befehlen selbst wir sollen solche \u00fcben.", "tokens": ["Ja", "sie", "be\u00b7feh\u00b7len", "selbst", "wir", "sol\u00b7len", "sol\u00b7che", "\u00fc\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VVFIN", "ADV", "PPER", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Jetzt da der frohe Tag nun wiederum erscheinet/", "tokens": ["Jetzt", "da", "der", "fro\u00b7he", "Tag", "nun", "wie\u00b7de\u00b7rum", "er\u00b7schei\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "ADV", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Den Tag/ den uns der Herr zur Freud und Lust gemacht/", "tokens": ["Den", "Tag", "/", "den", "uns", "der", "Herr", "zur", "Freud", "und", "Lust", "ge\u00b7macht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "PPER", "ART", "NN", "APPRART", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Der Tag/ der Freude bringt/ an dem kein Auge weinet/", "tokens": ["Der", "Tag", "/", "der", "Freu\u00b7de", "bringt", "/", "an", "dem", "kein", "Au\u00b7ge", "wei\u00b7net", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "VVFIN", "$(", "APPR", "ART", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "An welchem jedermann mit vollem Munde lacht/", "tokens": ["An", "wel\u00b7chem", "je\u00b7der\u00b7mann", "mit", "vol\u00b7lem", "Mun\u00b7de", "lacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIS", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "So soll auch meine Pflicht erkenntlich sich erweisen/", "tokens": ["So", "soll", "auch", "mei\u00b7ne", "Pflicht", "er\u00b7kennt\u00b7lich", "sich", "er\u00b7wei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "PPOSAT", "NN", "VVFIN", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und die geno\u00dfne Gunst mit hohen Dancke preisen.", "tokens": ["Und", "die", "ge\u00b7no\u00df\u00b7ne", "Gunst", "mit", "ho\u00b7hen", "Dan\u00b7cke", "prei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Sie ists Hochwertheste/ die so viel G\u00fctigkeiten/", "tokens": ["Sie", "ists", "Hoch\u00b7wert\u00b7hes\u00b7te", "/", "die", "so", "viel", "G\u00fc\u00b7tig\u00b7kei\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$(", "ART", "ADV", "PIAT", "NN", "$("], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Ja tausend und noch mehr mir unverdient erzeigt/", "tokens": ["Ja", "tau\u00b7send", "und", "noch", "mehr", "mir", "un\u00b7ver\u00b7di\u00b7ent", "er\u00b7zeigt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "CARD", "KON", "ADV", "ADV", "PPER", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.21": {"text": "Sie ist es/ deren Gunst/ sich noch zu allen Zeiten", "tokens": ["Sie", "ist", "es", "/", "de\u00b7ren", "Gunst", "/", "sich", "noch", "zu", "al\u00b7len", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "$(", "PRELAT", "NN", "$(", "PRF", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Wenn ich etwas bedurfft/ mit Raht zu mir geneigt.", "tokens": ["Wenn", "ich", "et\u00b7was", "be\u00b7durfft", "/", "mit", "Raht", "zu", "mir", "ge\u00b7neigt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "$(", "APPR", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Was sage ich mit Raht/ in Worten und in Wercken/", "tokens": ["Was", "sa\u00b7ge", "ich", "mit", "Raht", "/", "in", "Wor\u00b7ten", "und", "in", "Wer\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "NN", "$(", "APPR", "NN", "KON", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Lie\u00df sie ihr g\u00fctigs Hertz in allen Sachen mercken", "tokens": ["Lie\u00df", "sie", "ihr", "g\u00fc\u00b7tigs", "Hertz", "in", "al\u00b7len", "Sa\u00b7chen", "mer\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "APPR", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Vor alle solche Gunst/ ist mein erfreut Gem\u00fchte/", "tokens": ["Vor", "al\u00b7le", "sol\u00b7che", "Gunst", "/", "ist", "mein", "er\u00b7freut", "Ge\u00b7m\u00fch\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PIAT", "NN", "$(", "VAFIN", "PPOSAT", "VVFIN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Nun es die Zeit befiehlt/ mit Lob und Danck bereit/", "tokens": ["Nun", "es", "die", "Zeit", "be\u00b7fiehlt", "/", "mit", "Lob", "und", "Danck", "be\u00b7reit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "NN", "VVFIN", "$(", "APPR", "NN", "KON", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Gerechter Himmel gib vor alle ihre G\u00fcte", "tokens": ["Ge\u00b7rech\u00b7ter", "Him\u00b7mel", "gib", "vor", "al\u00b7le", "ih\u00b7re", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "VVIMP", "APPR", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Jhr die Belohnung hier und dort in Ewigkeit/", "tokens": ["Ihr", "die", "Be\u00b7loh\u00b7nung", "hier", "und", "dort", "in", "E\u00b7wig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADV", "KON", "ADV", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "La\u00df nie den Ungl\u00fccks Wind auf ihre Wohnung wehen/", "tokens": ["La\u00df", "nie", "den", "Un\u00b7gl\u00fccks", "Wind", "auf", "ih\u00b7re", "Woh\u00b7nung", "we\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "La\u00df sie in h\u00f6chster Lust auf lauter Rosen gehen.", "tokens": ["La\u00df", "sie", "in", "h\u00f6chs\u00b7ter", "Lust", "auf", "lau\u00b7ter", "Ro\u00b7sen", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ADJA", "NN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Dis ist der treue Wunsch/ den mein Gem\u00fchte heget/", "tokens": ["Dis", "ist", "der", "treu\u00b7e", "Wunsch", "/", "den", "mein", "Ge\u00b7m\u00fch\u00b7te", "he\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$(", "ART", "PPOSAT", "NN", "VVFIN", "$("], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.32": {"text": "Der Himmel ists der Ja und Amen dazu spricht/", "tokens": ["Der", "Him\u00b7mel", "ists", "der", "Ja", "und", "A\u00b7men", "da\u00b7zu", "spricht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "KON", "NN", "PAV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "So lange sich mein Hertz in meinem Busen reget/", "tokens": ["So", "lan\u00b7ge", "sich", "mein", "Hertz", "in", "mei\u00b7nem", "Bu\u00b7sen", "re\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PRF", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "So lange l\u00e4st mein Mund den heissen Seuffzer nicht.", "tokens": ["So", "lan\u00b7ge", "l\u00e4st", "mein", "Mund", "den", "heis\u00b7sen", "Seuff\u00b7zer", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Sie/ und ihr gantzes Haus mu\u00df sich mit Anmuth weiden/", "tokens": ["Sie", "/", "und", "ihr", "gant\u00b7zes", "Haus", "mu\u00df", "sich", "mit", "An\u00b7muth", "wei\u00b7den", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "KON", "PPOSAT", "ADJA", "NN", "VMFIN", "PRF", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Bis sie der H\u00f6chste nimmt in jene Himmels-Freuden.", "tokens": ["Bis", "sie", "der", "H\u00f6chs\u00b7te", "nimmt", "in", "je\u00b7ne", "Him\u00b7mels\u00b7Freu\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}