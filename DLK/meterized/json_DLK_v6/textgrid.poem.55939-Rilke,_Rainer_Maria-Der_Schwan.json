{"textgrid.poem.55939": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Der Schwan", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Diese M\u00fchsal, durch noch Ungetanes", "tokens": ["Die\u00b7se", "M\u00fch\u00b7sal", ",", "durch", "noch", "Un\u00b7ge\u00b7ta\u00b7nes"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDAT", "NN", "$,", "APPR", "ADV", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "schwer und wie gebunden hinzugehn,", "tokens": ["schwer", "und", "wie", "ge\u00b7bun\u00b7den", "hin\u00b7zu\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "PWAV", "VVPP", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "gleicht dem ungeschaffnen Gang des Schwanes.", "tokens": ["gleicht", "dem", "un\u00b7ge\u00b7schaff\u00b7nen", "Gang", "des", "Schwa\u00b7nes", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Und das Sterben, dieses Nichtmehrfassen", "tokens": ["Und", "das", "Ster\u00b7ben", ",", "die\u00b7ses", "Nicht\u00b7mehr\u00b7fas\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PDAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "jenes Grunds, auf dem wir t\u00e4glich stehn,", "tokens": ["je\u00b7nes", "Grunds", ",", "auf", "dem", "wir", "t\u00e4g\u00b7lich", "stehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "APPR", "PRELS", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "seinem \u00e4ngstlichen Sich-Niederlassen \u2013:", "tokens": ["sei\u00b7nem", "\u00e4ngst\u00b7li\u00b7chen", "Sich\u00b7Nie\u00b7der\u00b7las\u00b7sen", "\u2013", ":"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "in die Wasser, die ihn sanft empfangen", "tokens": ["in", "die", "Was\u00b7ser", ",", "die", "ihn", "sanft", "emp\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "und die sich, wie gl\u00fccklich und vergangen,", "tokens": ["und", "die", "sich", ",", "wie", "gl\u00fcck\u00b7lich", "und", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELS", "PRF", "$,", "PWAV", "ADJD", "KON", "VVPP", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "unter ihm zur\u00fcckziehn, Flut um Flut;", "tokens": ["un\u00b7ter", "ihm", "zu\u00b7r\u00fcck\u00b7ziehn", ",", "Flut", "um", "Flut", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVINF", "$,", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "w\u00e4hrend er unendlich still und sicher", "tokens": ["w\u00e4h\u00b7rend", "er", "un\u00b7end\u00b7lich", "still", "und", "si\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "immer m\u00fcndiger und k\u00f6niglicher", "tokens": ["im\u00b7mer", "m\u00fcn\u00b7di\u00b7ger", "und", "k\u00f6\u00b7nig\u00b7li\u00b7cher"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "und gelassener zu ziehn geruht.", "tokens": ["und", "ge\u00b7las\u00b7se\u00b7ner", "zu", "ziehn", "ge\u00b7ruht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKZU", "VVINF", "VVPP", "$."], "meter": "+-+---+-+", "measure": "unknown.measure.tetra"}}}}}