{"textgrid.poem.57097": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Er wies nichts ab in diesen Wintertagen,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er wies nichts ab in diesen Wintertagen,", "tokens": ["Er", "wies", "nichts", "ab", "in", "die\u00b7sen", "Win\u00b7ter\u00b7ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PTKVZ", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "er wollte gehn, wohin man immer schriebe:", "tokens": ["er", "woll\u00b7te", "gehn", ",", "wo\u00b7hin", "man", "im\u00b7mer", "schrie\u00b7be", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "PWAV", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nur um ihr nachts im Wagen dann zu sagen,", "tokens": ["Nur", "um", "ihr", "nachts", "im", "Wa\u00b7gen", "dann", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "ADV", "APPRART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "wie sehr er sie, wie sehr er sie nur liebe.", "tokens": ["wie", "sehr", "er", "sie", ",", "wie", "sehr", "er", "sie", "nur", "lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "PPER", "$,", "PWAV", "ADV", "PPER", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und sie zu k\u00fcssen in erl\u00f6stem Jubel", "tokens": ["Und", "sie", "zu", "k\u00fcs\u00b7sen", "in", "er\u00b7l\u00f6s\u00b7tem", "Ju\u00b7bel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "PTKZU", "VVINF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "im dunklen Wagen leis und ohne Ende,", "tokens": ["im", "dunk\u00b7len", "Wa\u00b7gen", "leis", "und", "oh\u00b7ne", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJD", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und ihr zu sagen, wie nach all dem Trubel", "tokens": ["und", "ihr", "zu", "sa\u00b7gen", ",", "wie", "nach", "all", "dem", "Tru\u00b7bel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "PTKZU", "VVINF", "$,", "PWAV", "APPR", "PIAT", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "er nur an ihr, in ihr Gen\u00fcgen f\u00e4nde.", "tokens": ["er", "nur", "an", "ihr", ",", "in", "ihr", "Ge\u00b7n\u00fc\u00b7gen", "f\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPER", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+---+-+-+-", "measure": "dactylic.init"}}}}}