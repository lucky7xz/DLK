{"dta.poem.6924": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtungen \u00fcber die Jdeen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Unendliches Gemisch unz\u00e4hlbarer Gestalten,", "tokens": ["Un\u00b7end\u00b7li\u00b7ches", "Ge\u00b7misch", "un\u00b7z\u00e4hl\u00b7ba\u00b7rer", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Die allezeit verschwinden,", "tokens": ["Die", "al\u00b7le\u00b7zeit", "ver\u00b7schwin\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und allezeit sich wieder finden,", "tokens": ["Und", "al\u00b7le\u00b7zeit", "sich", "wie\u00b7der", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf einmal in den Sitz der Sinnen dringen;", "tokens": ["Auf", "ein\u00b7mal", "in", "den", "Sitz", "der", "Sin\u00b7nen", "drin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wenn ihre Regungen, die stet- und lebendig,", "tokens": ["Wenn", "ih\u00b7re", "Re\u00b7gun\u00b7gen", ",", "die", "stet", "und", "le\u00b7ben\u00b7dig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "ART", "TRUNC", "KON", "ADJD", "$,"], "meter": "-+--+--+-+--", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Nicht von Natur unc\u00f6rperlich?", "tokens": ["Nicht", "von", "Na\u00b7tur", "un\u00b7c\u00f6r\u00b7per\u00b7lich", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Wir \u00f6ffnen nicht so bald die Augenlieder,", "tokens": ["Wir", "\u00f6ff\u00b7nen", "nicht", "so", "bald", "die", "Au\u00b7gen\u00b7lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "So l\u00e4sset alles gleich bis zu des Himmels H\u00f6hn,", "tokens": ["So", "l\u00e4s\u00b7set", "al\u00b7les", "gleich", "bis", "zu", "des", "Him\u00b7mels", "H\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "In einem Augenblick von uns sich sehn.", "tokens": ["In", "ei\u00b7nem", "Au\u00b7gen\u00b7blick", "von", "uns", "sich", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Verschliessen wir dieselben wieder;", "tokens": ["Ver\u00b7schlies\u00b7sen", "wir", "die\u00b7sel\u00b7ben", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "So sieht man Himmel, Licht und Sternen,", "tokens": ["So", "sieht", "man", "Him\u00b7mel", ",", "Licht", "und", "Ster\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Jm Augenblick verschwindend, sich entfernen.", "tokens": ["Jm", "Au\u00b7gen\u00b7blick", "ver\u00b7schwin\u00b7dend", ",", "sich", "ent\u00b7fer\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Was vor Gem\u00e4hld\u2019 und Z\u00fcg\u2019 sind f\u00e4hig sich zu ziehn", "tokens": ["Was", "vor", "Ge\u00b7m\u00e4hld'", "und", "Z\u00fcg'", "sind", "f\u00e4\u00b7hig", "sich", "zu", "ziehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "NN", "KON", "NN", "VAFIN", "ADJD", "PRF", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Vom Firmament, und wieder zu entfliehn?", "tokens": ["Vom", "Fir\u00b7ma\u00b7ment", ",", "und", "wie\u00b7der", "zu", "ent\u00b7fliehn", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KON", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Die Handlung ist nicht leiblich, das Empfinden", "tokens": ["Die", "Hand\u00b7lung", "ist", "nicht", "leib\u00b7lich", ",", "das", "Emp\u00b7fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADJD", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Ist blo\u00df im C\u00f6rper nur zu finden:", "tokens": ["Ist", "blo\u00df", "im", "C\u00f6r\u00b7per", "nur", "zu", "fin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPRART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Die schnellen Mittel hat des SCHOEPFFERS Huld er-", "tokens": ["Die", "schnel\u00b7len", "Mit\u00b7tel", "hat", "des", "ScHOE\u00b7PF\u00b7FERS", "Huld", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NE", "NE", "TRUNC"], "meter": "-+-+-+-++-+-", "measure": "unknown.measure.hexa"}, "line.18": {"text": "lesen,", "tokens": ["le\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.19": {"text": "Da\u00df Er mit uns verb\u00e4nd\u2019 ein nie verg\u00e4nglich Wesen.", "tokens": ["Da\u00df", "Er", "mit", "uns", "ver\u00b7b\u00e4nd'", "ein", "nie", "ver\u00b7g\u00e4ng\u00b7lich", "We\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVFIN", "ART", "ADV", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Es nehmen keinen Raum die Vorw\u00fcrff\u2019 in uns ein.", "tokens": ["Es", "neh\u00b7men", "kei\u00b7nen", "Raum", "die", "Vor\u00b7w\u00fcrff'", "in", "uns", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "In den Bewundrungen, die uns erreget seyn,", "tokens": ["In", "den", "Be\u00b7wund\u00b7run\u00b7gen", ",", "die", "uns", "er\u00b7re\u00b7get", "seyn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "VAINF", "$,"], "meter": "++-+--+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.22": {"text": "Durch Kunst des SEHOEPFFERS, sieht des gr\u00f6sten", "tokens": ["Durch", "Kunst", "des", "Se\u00b7HOE\u00b7PF\u00b7FERS", ",", "sieht", "des", "gr\u00f6s\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN", "$,", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Weisen Blick", "tokens": ["Wei\u00b7sen", "Blick"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.24": {"text": "Nichts als ", "tokens": ["Nichts", "als"], "token_info": ["word", "word"], "pos": ["PIS", "KOKOM"], "meter": "+-", "measure": "trochaic.single"}}}}}