{"textgrid.poem.64010": {"metadata": {"author": {"name": "Hille, Peter", "birth": "N.A.", "death": "N.A."}, "title": "Salome", "genre": "verse", "period": "N.A.", "pub_year": 1879, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meines Blutes b\u00f6ser Reigen,", "tokens": ["Mei\u00b7nes", "Blu\u00b7tes", "b\u00f6\u00b7ser", "Rei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mordend, flehend.", "tokens": ["Mor\u00b7dend", ",", "fle\u00b7hend", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Sollst dich einem K\u00f6nig zeigen \u2013", "tokens": ["Sollst", "dich", "ei\u00b7nem", "K\u00f6\u00b7nig", "zei\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mordend flehend.", "tokens": ["Mor\u00b7dend", "fle\u00b7hend", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Sollst umschlingen,", "tokens": ["Sollst", "um\u00b7schlin\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Und umzwingen", "tokens": ["Und", "um\u00b7zwin\u00b7gen"], "token_info": ["word", "word"], "pos": ["KON", "VVIZU"], "meter": "--+-", "measure": "anapaest.init"}, "line.3": {"text": "Dir ein Haupt,", "tokens": ["Dir", "ein", "Haupt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Schwer von strengem Haar umlaubt.", "tokens": ["Schwer", "von", "stren\u00b7gem", "Haar", "um\u00b7laubt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Dieses Haupt hat sterben m\u00fcssen,", "tokens": ["Die\u00b7ses", "Haupt", "hat", "ster\u00b7ben", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "VVINF", "VMINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nun kann meine Inbrunst k\u00fcssen", "tokens": ["Nun", "kann", "mei\u00b7ne", "In\u00b7brunst", "k\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hassend heute, morgen klagend,", "tokens": ["Has\u00b7send", "heu\u00b7te", ",", "mor\u00b7gen", "kla\u00b7gend", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ADV", "$,", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Drohend es im Herzen tragend.", "tokens": ["Dro\u00b7hend", "es", "im", "Her\u00b7zen", "tra\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Meines Blutes b\u00f6ser Reigen,", "tokens": ["Mei\u00b7nes", "Blu\u00b7tes", "b\u00f6\u00b7ser", "Rei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mordend, flehend ...", "tokens": ["Mor\u00b7dend", ",", "fle\u00b7hend", "..."], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}