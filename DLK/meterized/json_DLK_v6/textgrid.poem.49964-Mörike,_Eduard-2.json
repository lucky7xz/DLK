{"textgrid.poem.49964": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "2", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00e4llt dir vielleicht in sp\u00e4ten Tagen", "tokens": ["F\u00e4llt", "dir", "viel\u00b7leicht", "in", "sp\u00e4\u00b7ten", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wieder ein, dies Stammbuch aufzuschlagen,", "tokens": ["Wie\u00b7der", "ein", ",", "dies", "Stamm\u00b7buch", "auf\u00b7zu\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "PDS", "NN", "VVIZU", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und schaust dann auch dies Bl\u00e4ttlein an,", "tokens": ["Und", "schaust", "dann", "auch", "dies", "Bl\u00e4tt\u00b7lein", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "PDS", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit einem lieben Freund etwan,", "tokens": ["Mit", "ei\u00b7nem", "lie\u00b7ben", "Freund", "et\u00b7wan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da sagst du von mir wohl dies und jenes,", "tokens": ["Da", "sagst", "du", "von", "mir", "wohl", "dies", "und", "je\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "ADV", "PDS", "KON", "PDS", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Nicht allzu Schlimmes, noch allzu Sch\u00f6nes:", "tokens": ["Nicht", "all\u00b7zu", "Schlim\u00b7mes", ",", "noch", "all\u00b7zu", "Sch\u00f6\u00b7nes", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PTKA", "ADJA", "$,", "ADV", "PTKA", "ADJA", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Er war im ganzen ein guter Mann,", "tokens": ["Er", "war", "im", "gan\u00b7zen", "ein", "gu\u00b7ter", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und uns besonders zugetan.", "tokens": ["Und", "uns", "be\u00b7son\u00b7ders", "zu\u00b7ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ich hoffe denn auch insofern,", "tokens": ["Ich", "hof\u00b7fe", "denn", "auch", "in\u00b7so\u00b7fern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Er sitzt in einem guten Stern.", "tokens": ["Er", "sitzt", "in", "ei\u00b7nem", "gu\u00b7ten", "Stern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Meine Mutter schickt' ihm einmal durch mich", "tokens": ["Mei\u00b7ne", "Mut\u00b7ter", "schickt'", "ihm", "ein\u00b7mal", "durch", "mich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "APPR", "PPER"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.12": {"text": "Einen G\u00e4nsebraten s\u00e4uberlich", "tokens": ["Ei\u00b7nen", "G\u00e4n\u00b7se\u00b7bra\u00b7ten", "s\u00e4u\u00b7ber\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.13": {"text": "Mit einem feinen Salat ins Haus,", "tokens": ["Mit", "ei\u00b7nem", "fei\u00b7nen", "Sa\u00b7lat", "ins", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Das schmeckte ihm ganz \u00fcberaus.", "tokens": ["Das", "schmeck\u00b7te", "ihm", "ganz", "\u00fc\u00b7be\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Er meinte, das G\u00e4nsestopfen hienieden", "tokens": ["Er", "mein\u00b7te", ",", "das", "G\u00e4n\u00b7ses\u00b7top\u00b7fen", "hien\u00b7ie\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "VVFIN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Sei drum nicht absolut zu verbieten,", "tokens": ["Sei", "drum", "nicht", "ab\u00b7so\u00b7lut", "zu", "ver\u00b7bie\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PAV", "PTKNEG", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Es sei halt f\u00fcr ein Pr\u00e4latenessen \u2013", "tokens": ["Es", "sei", "halt", "f\u00fcr", "ein", "Pr\u00e4\u00b7la\u00b7te\u00b7nes\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Kurz, r\u00fchmte den Imbi\u00df ungemessen.", "tokens": ["Kurz", ",", "r\u00fchm\u00b7te", "den", "Im\u00b7bi\u00df", "un\u00b7ge\u00b7mes\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Deswegen ich gern glauben mag,", "tokens": ["Des\u00b7we\u00b7gen", "ich", "gern", "glau\u00b7ben", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Es habe sein Herz bis diesen Tag", "tokens": ["Es", "ha\u00b7be", "sein", "Herz", "bis", "die\u00b7sen", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "APPR", "PDAT", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "Weder den Braten, noch mich vergessen.", "tokens": ["We\u00b7der", "den", "Bra\u00b7ten", ",", "noch", "mich", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ADV", "PPER", "VVPP", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}