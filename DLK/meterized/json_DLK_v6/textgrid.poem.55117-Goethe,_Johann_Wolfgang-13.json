{"textgrid.poem.55117": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "13", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am J\u00fcngsten Tag, wenn die Posaunen schallen", "tokens": ["Am", "J\u00fcng\u00b7sten", "Tag", ",", "wenn", "die", "Po\u00b7sau\u00b7nen", "schal\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "KOUS", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und alles aus ist mit dem Erdeleben,", "tokens": ["Und", "al\u00b7les", "aus", "ist", "mit", "dem", "Er\u00b7de\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sind wir verpflichtet, Rechenschaft zu geben", "tokens": ["Sind", "wir", "ver\u00b7pflich\u00b7tet", ",", "Re\u00b7chen\u00b7schaft", "zu", "ge\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von jedem Wort, das unn\u00fctz uns entfallen.", "tokens": ["Von", "je\u00b7dem", "Wort", ",", "das", "un\u00b7n\u00fctz", "uns", "ent\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie wird's nun werden mit den Worten allen,", "tokens": ["Wie", "wird's", "nun", "wer\u00b7den", "mit", "den", "Wor\u00b7ten", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "VAFIN", "APPR", "ART", "NN", "PIAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In welchen ich so liebevoll mein Streben", "tokens": ["In", "wel\u00b7chen", "ich", "so", "lie\u00b7be\u00b7voll", "mein", "Stre\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "PPER", "ADV", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Um deine Gunst dir an den Tag gegeben,", "tokens": ["Um", "dei\u00b7ne", "Gunst", "dir", "an", "den", "Tag", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn diese blo\u00df an deinem Ohr verhallen?", "tokens": ["Wenn", "die\u00b7se", "blo\u00df", "an", "dei\u00b7nem", "Ohr", "ver\u00b7hal\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Darum bedenk, o Liebchen! dein Gewissen,", "tokens": ["Da\u00b7rum", "be\u00b7denk", ",", "o", "Lieb\u00b7chen", "!", "dein", "Ge\u00b7wis\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "FM", "NN", "$.", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bedenk im Ernst, wie lange du gezaudert,", "tokens": ["Be\u00b7denk", "im", "Ernst", ",", "wie", "lan\u00b7ge", "du", "ge\u00b7zau\u00b7dert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "PWAV", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df nicht der Welt solch Leiden widerfahre.", "tokens": ["Da\u00df", "nicht", "der", "Welt", "solch", "Lei\u00b7den", "wi\u00b7der\u00b7fah\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Werd ich berechnen und entschuld'gen m\u00fcssen,", "tokens": ["Werd", "ich", "be\u00b7rech\u00b7nen", "und", "ent\u00b7schuld'\u00b7gen", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "KON", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was alles unn\u00fctz ich vor dir geplaudert,", "tokens": ["Was", "al\u00b7les", "un\u00b7n\u00fctz", "ich", "vor", "dir", "ge\u00b7plau\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADJD", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So wird der J\u00fcngste Tag zum vollen Jahre.", "tokens": ["So", "wird", "der", "J\u00fcngs\u00b7te", "Tag", "zum", "vol\u00b7len", "Jah\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}