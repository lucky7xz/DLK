{"dta.poem.492": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als sie baht/ er m\u00f6chte ihr gut Wetter auf  \n die W\u00e4sche machen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Nun soll ich/ wehrtes Kind/ ein Wetter-macher seyn?", "tokens": ["Nun", "soll", "ich", "/", "wehr\u00b7tes", "Kind", "/", "ein", "Wet\u00b7ter\u00b7ma\u00b7cher", "seyn", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$(", "ADJA", "NN", "$(", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich nehm\u2019 es willig an/ und bin damit zu frieden/", "tokens": ["Ich", "nehm'", "es", "wil\u00b7lig", "an", "/", "und", "bin", "da\u00b7mit", "zu", "frie\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$(", "KON", "VAFIN", "PAV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da mir dein sch\u00f6nster Mund hat solch ein Amt beschieden/", "tokens": ["Da", "mir", "dein", "sch\u00f6ns\u00b7ter", "Mund", "hat", "solch", "ein", "Amt", "be\u00b7schie\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "VAFIN", "PIAT", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil ich dir gestern gab vergn\u00fcgten Sonnenschein.", "tokens": ["Weil", "ich", "dir", "ge\u00b7stern", "gab", "ver\u00b7gn\u00fcg\u00b7ten", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVFIN", "ADJA", "NN", "$."], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.5": {"text": "Bey dir ist mir bisher die Sonne untergangen/", "tokens": ["Bey", "dir", "ist", "mir", "bis\u00b7her", "die", "Son\u00b7ne", "un\u00b7ter\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$("], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Der Himmel hat sich stets vor mir mit Flohr umh\u00fcllt;", "tokens": ["Der", "Him\u00b7mel", "hat", "sich", "stets", "vor", "mir", "mit", "Flohr", "um\u00b7h\u00fcllt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PRF", "ADV", "APPR", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Drum/ we\u00f1 nun/ wie du sagst stets wird mein Wunsch erf\u00fcllt/", "tokens": ["Drum", "/", "we\u00f1", "nun", "/", "wie", "du", "sagst", "stets", "wird", "mein", "Wunsch", "er\u00b7f\u00fcllt", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$(", "PWS", "ADV", "$(", "PWAV", "PPER", "VVFIN", "ADV", "VAFIN", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So la\u00df mir deine Sonn\u2019heut an zu scheinen fangen.", "tokens": ["So", "la\u00df", "mir", "dei\u00b7ne", "Sonn'\u00b7heut", "an", "zu", "schei\u00b7nen", "fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PPOSAT", "NN", "PTKVZ", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}