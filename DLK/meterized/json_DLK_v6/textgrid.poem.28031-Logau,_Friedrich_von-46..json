{"textgrid.poem.28031": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "46.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weil irren Menschlich ist, k\u00fcmmt kl\u00e4rlich an den Tag,", "tokens": ["Weil", "ir\u00b7ren", "Menschlich", "ist", ",", "k\u00fcmmt", "kl\u00e4r\u00b7lich", "an", "den", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VAFIN", "$,", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da\u00df Weiber man nur auch f\u00fcr Menschen rechnen mag.", "tokens": ["Da\u00df", "Wei\u00b7ber", "man", "nur", "auch", "f\u00fcr", "Men\u00b7schen", "rech\u00b7nen", "mag."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KOUS", "NN", "PIS", "ADV", "ADV", "APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es irrte Grunnia zum t\u00fcgen menschlich nu:", "tokens": ["Es", "irr\u00b7te", "Grun\u00b7nia", "zum", "t\u00fc\u00b7gen", "menschlich", "nu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "APPRART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie solte gehn zum Mann und gieng zum Knechte zu.", "tokens": ["Sie", "sol\u00b7te", "gehn", "zum", "Mann", "und", "gieng", "zum", "Knech\u00b7te", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "APPRART", "NN", "KON", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}