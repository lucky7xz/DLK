{"textgrid.poem.67514": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Die Wage", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unter den Sternen h\u00f6rt' ich klingen die goldene Wage;", "tokens": ["Un\u00b7ter", "den", "Ster\u00b7nen", "h\u00f6rt'", "ich", "klin\u00b7gen", "die", "gol\u00b7de\u00b7ne", "Wa\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Strebend im Gleichgewicht, t\u00f6nte sie Allen den Schall:", "tokens": ["Stre\u00b7bend", "im", "Gleich\u00b7ge\u00b7wicht", ",", "t\u00f6n\u00b7te", "sie", "Al\u00b7len", "den", "Schall", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$,", "VVFIN", "PPER", "NE", "ART", "NN", "$."], "meter": "+--+-++--+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Und vom Felsenaltar rief ihn die Echo zur\u00fcck.", "tokens": ["Und", "vom", "Fel\u00b7sen\u00b7al\u00b7tar", "rief", "ihn", "die", "E\u00b7cho", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Wie der Hagel anschie\u00dft und in gleichem Maa\u00dfe zur\u00fcckprallt,", "tokens": ["Wie", "der", "Ha\u00b7gel", "an\u00b7schie\u00dft", "und", "in", "glei\u00b7chem", "Maa\u00b7\u00dfe", "zu\u00b7r\u00fcck\u00b7prallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "KON", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Hier der gl\u00e4nzende Strahl, dort der geworfene Ball:", "tokens": ["Hier", "der", "gl\u00e4n\u00b7zen\u00b7de", "Strahl", ",", "dort", "der", "ge\u00b7wor\u00b7fe\u00b7ne", "Ball", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "ADV", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Also trifft sich im Winkel, im innersten Herzen des Menschen,", "tokens": ["Al\u00b7so", "trifft", "sich", "im", "Win\u00b7kel", ",", "im", "in\u00b7ners\u00b7ten", "Her\u00b7zen", "des", "Men\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPRART", "NN", "$,", "APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "--+--+-+-+-+--+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Gleiches mit Gleichem; es paart immer sich Folge mit That.", "tokens": ["Glei\u00b7ches", "mit", "Glei\u00b7chem", ";", "es", "paart", "im\u00b7mer", "sich", "Fol\u00b7ge", "mit", "That", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$.", "PPER", "VVFIN", "ADV", "PRF", "NN", "APPR", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.8": {"text": "Lohn dem Guten und Strafe dem B\u00f6sen. Im menschlichen Herzen", "tokens": ["Lohn", "dem", "Gu\u00b7ten", "und", "Stra\u00b7fe", "dem", "B\u00f6\u00b7sen", ".", "Im", "menschli\u00b7chen", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "KON", "NN", "ART", "NN", "$.", "APPRART", "ADJA", "NN"], "meter": "+-+--+--+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "Thront der Richter und w\u00e4gt, klaget und zeuget und spricht,", "tokens": ["Thront", "der", "Rich\u00b7ter", "und", "w\u00e4gt", ",", "kla\u00b7get", "und", "zeu\u00b7get", "und", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.10": {"text": "Vor ihm das offene Buch. Im Weltgerichte der V\u00f6lker,", "tokens": ["Vor", "ihm", "das", "of\u00b7fe\u00b7ne", "Buch", ".", "Im", "Welt\u00b7ge\u00b7rich\u00b7te", "der", "V\u00f6l\u00b7ker", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "$.", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.11": {"text": "In der Tyrannen Herz, selbst in des Heuchelnden Brust", "tokens": ["In", "der", "Ty\u00b7ran\u00b7nen", "Herz", ",", "selbst", "in", "des", "Heu\u00b7cheln\u00b7den", "Brust"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "$,", "ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.12": {"text": "T\u00f6nt die Stimme der Angst, des Vorwurfs, Neides und Abscheus;", "tokens": ["T\u00f6nt", "die", "Stim\u00b7me", "der", "Angst", ",", "des", "Vor\u00b7wurfs", ",", "Nei\u00b7des", "und", "Ab\u00b7scheus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,", "ART", "NN", "$,", "NN", "KON", "NE", "$."], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.13": {"text": "Nachts und am Tag ert\u00f6nt bellend der h\u00f6llische Schlund.", "tokens": ["Nachts", "und", "am", "Tag", "er\u00b7t\u00f6nt", "bel\u00b7lend", "der", "h\u00f6l\u00b7li\u00b7sche", "Schlund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "APPRART", "NN", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.14": {"text": "Aber im Herzen des Frommen ist Ruh; er kennt seine That nicht;", "tokens": ["A\u00b7ber", "im", "Her\u00b7zen", "des", "From\u00b7men", "ist", "Ruh", ";", "er", "kennt", "sei\u00b7ne", "That", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "VAFIN", "NN", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "+--+--+--+-++-+-", "measure": "dactylic.tri.plus"}, "line.15": {"text": "Doch ihn lohnet sein Werk sicher mit frohem Genu\u00df.", "tokens": ["Doch", "ihn", "loh\u00b7net", "sein", "Werk", "si\u00b7cher", "mit", "fro\u00b7hem", "Ge\u00b7nu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.16": {"text": "Auch in dem Kommen des Weltgerichts ert\u00f6net die Wage;", "tokens": ["Auch", "in", "dem", "Kom\u00b7men", "des", "Welt\u00b7ge\u00b7richts", "er\u00b7t\u00f6\u00b7net", "die", "Wa\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.17": {"text": "H\u00f6ret Ihr, V\u00f6lker, nicht kommen den m\u00e4chtigen Tritt?", "tokens": ["H\u00f6\u00b7ret", "Ihr", ",", "V\u00f6l\u00b7ker", ",", "nicht", "kom\u00b7men", "den", "m\u00e4ch\u00b7ti\u00b7gen", "Tritt", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "PTKNEG", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.18": {"text": "Seufzend h\u00f6ret Ihr nicht; doch er kommt! Die bekr\u00e4nzete S\u00e4ule", "tokens": ["Seuf\u00b7zend", "h\u00f6\u00b7ret", "Ihr", "nicht", ";", "doch", "er", "kommt", "!", "Die", "be\u00b7kr\u00e4n\u00b7ze\u00b7te", "S\u00e4u\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PTKNEG", "$.", "KON", "PPER", "VVFIN", "$.", "ART", "ADJA", "NN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.19": {"text": "Geht aus Wolken hervor, Gro\u00dfmuth und stille Geduld.", "tokens": ["Geht", "aus", "Wol\u00b7ken", "her\u00b7vor", ",", "Gro\u00df\u00b7muth", "und", "stil\u00b7le", "Ge\u00b7duld", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PTKVZ", "$,", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.20": {"text": "Und jetzt gl\u00e4nzet die Wage: \u00bbWas Ihr dem Geringsten der Menschen", "tokens": ["Und", "jetzt", "gl\u00e4n\u00b7zet", "die", "Wa\u00b7ge", ":", "\u00bb", "Was", "Ihr", "dem", "Ge\u00b7rings\u00b7ten", "der", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$.", "$(", "PWS", "PPER", "ART", "NN", "ART", "NN"], "meter": "--+--+---+-+--+-", "measure": "anapaest.di.plus"}, "line.21": {"text": "Thatet, thatet Ihr mir. Kommt und genie\u00dfet den Lohn!\u00ab", "tokens": ["Tha\u00b7tet", ",", "tha\u00b7tet", "Ihr", "mir", ".", "Kommt", "und", "ge\u00b7nie\u00b7\u00dfet", "den", "Lohn", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "PPER", "$.", "VVFIN", "KON", "VVFIN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}}}}}