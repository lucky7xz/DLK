{"textgrid.poem.55026": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Mit einem gemalten Band", "genre": "verse", "period": "N.A.", "pub_year": 1771, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kleine Blumen, kleine Bl\u00e4tter", "tokens": ["Klei\u00b7ne", "Blu\u00b7men", ",", "klei\u00b7ne", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Streuen mir mit leichter Hand", "tokens": ["Streu\u00b7en", "mir", "mit", "leich\u00b7ter", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gute junge Fr\u00fchlingsg\u00f6tter", "tokens": ["Gu\u00b7te", "jun\u00b7ge", "Fr\u00fch\u00b7lings\u00b7g\u00f6t\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "T\u00e4ndelnd auf ein luftig Band.", "tokens": ["T\u00e4n\u00b7delnd", "auf", "ein", "luf\u00b7tig", "Band", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Zephyr, nimm's auf deine Fl\u00fcgel,", "tokens": ["Ze\u00b7phyr", ",", "nim\u00b7m's", "auf", "dei\u00b7ne", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Schling's um meiner Liebsten Kleid;", "tokens": ["Schling's", "um", "mei\u00b7ner", "Liebs\u00b7ten", "Kleid", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und so tritt sie vor den Spiegel", "tokens": ["Und", "so", "tritt", "sie", "vor", "den", "Spie\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "All in ihrer Munterkeit.", "tokens": ["All", "in", "ih\u00b7rer", "Mun\u00b7ter\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sieht mit Rosen sich umgeben,", "tokens": ["Sieht", "mit", "Ro\u00b7sen", "sich", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Selbst wie eine Rose jung.", "tokens": ["Selbst", "wie", "ei\u00b7ne", "Ro\u00b7se", "jung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Einen Blick, geliebtes Leben!", "tokens": ["Ei\u00b7nen", "Blick", ",", "ge\u00b7lieb\u00b7tes", "Le\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ich bin belohnt genung.", "tokens": ["Und", "ich", "bin", "be\u00b7lohnt", "ge\u00b7nung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "VVPP", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "F\u00fchle, was dies Herz empfindet,", "tokens": ["F\u00fch\u00b7le", ",", "was", "dies", "Herz", "emp\u00b7fin\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "PDS", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Reiche frei mir deine Hand,", "tokens": ["Rei\u00b7che", "frei", "mir", "dei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und das Band, das uns verbindet,", "tokens": ["Und", "das", "Band", ",", "das", "uns", "ver\u00b7bin\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sei kein schwaches Rosenband!", "tokens": ["Sei", "kein", "schwa\u00b7ches", "Ro\u00b7sen\u00b7band", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}