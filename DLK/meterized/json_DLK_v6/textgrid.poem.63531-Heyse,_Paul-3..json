{"textgrid.poem.63531": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Drunten auf der Gassen", "tokens": ["Drun\u00b7ten", "auf", "der", "Gas\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Stand ich, sein zu passen;", "tokens": ["Stand", "ich", ",", "sein", "zu", "pas\u00b7sen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VAINF", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Schlugen Nachtigallen", "tokens": ["Schlu\u00b7gen", "Nach\u00b7ti\u00b7gal\u00b7len"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "An den Fenstern allen,", "tokens": ["An", "den", "Fens\u00b7tern", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PIAT", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Und ich blieb alleine", "tokens": ["Und", "ich", "blieb", "al\u00b7lei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Bei der Blitze Scheine,", "tokens": ["Bei", "der", "Blit\u00b7ze", "Schei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Bis die Nacht gewichen,", "tokens": ["Bis", "die", "Nacht", "ge\u00b7wi\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Und da bin ich frierend heimgeschlichen.", "tokens": ["Und", "da", "bin", "ich", "frie\u00b7rend", "heim\u00b7ge\u00b7schli\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "\u00dcber meine Wangen", "tokens": ["\u00dc\u00b7ber", "mei\u00b7ne", "Wan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ist der Tau gegangen,", "tokens": ["Ist", "der", "Tau", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und nun l\u00f6s' ich stille", "tokens": ["Und", "nun", "l\u00f6s'", "ich", "stil\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Meiner Locken F\u00fclle.", "tokens": ["Mei\u00b7ner", "Lo\u00b7cken", "F\u00fcl\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Da\u00df ein Sturm erginge,", "tokens": ["Da\u00df", "ein", "Sturm", "er\u00b7gin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Sich darin verfinge,", "tokens": ["Sich", "da\u00b7rin", "ver\u00b7fin\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "PAV", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Mich zum Himmel tr\u00fcge \u2013", "tokens": ["Mich", "zum", "Him\u00b7mel", "tr\u00fc\u00b7ge", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVFIN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Weit hinweg aus dieser Welt der L\u00fcge!", "tokens": ["Weit", "hin\u00b7weg", "aus", "die\u00b7ser", "Welt", "der", "L\u00fc\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "PDAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}