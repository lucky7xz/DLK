{"textgrid.poem.62788": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nach des Ulysses Koch und R\u00e4then", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nach des Ulysses Koch und R\u00e4then", "tokens": ["Nach", "des", "U\u00b7lys\u00b7ses", "Koch", "und", "R\u00e4\u00b7then"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NE", "NE", "KON", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Ber\u00fchrte Circens Wunderstab", "tokens": ["Be\u00b7r\u00fchr\u00b7te", "Cir\u00b7cens", "Wun\u00b7der\u00b7stab"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zuletzt auch seinen Hofpoeten,", "tokens": ["Zu\u00b7letzt", "auch", "sei\u00b7nen", "Hof\u00b7po\u00b7e\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem er die freye Tafel gab.", "tokens": ["Dem", "er", "die", "frey\u00b7e", "Ta\u00b7fel", "gab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er fleht; allein da half kein Flehen:", "tokens": ["Er", "fleht", ";", "al\u00b7lein", "da", "half", "kein", "Fle\u00b7hen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ADV", "ADV", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Werd eine Gans, rief sie. Doch er", "tokens": ["Werd", "ei\u00b7ne", "Gans", ",", "rief", "sie", ".", "Doch", "er"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ART", "NN", "$,", "VVFIN", "PPER", "$.", "KON", "PPER"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Blieb unverwandelt vor ihr stehen", "tokens": ["Blieb", "un\u00b7ver\u00b7wan\u00b7delt", "vor", "ihr", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und sagte seine Verse her.", "tokens": ["Und", "sag\u00b7te", "sei\u00b7ne", "Ver\u00b7se", "her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}