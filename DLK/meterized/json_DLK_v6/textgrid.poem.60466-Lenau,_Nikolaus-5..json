{"textgrid.poem.60466": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gerne sehn wir sch\u00f6ne Spiegel", "tokens": ["Ger\u00b7ne", "sehn", "wir", "sch\u00f6\u00b7ne", "Spie\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Im Gemache sch\u00f6ner Frauen;", "tokens": ["Im", "Ge\u00b7ma\u00b7che", "sch\u00f6\u00b7ner", "Frau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00f6ge froh ihr holdes Antlitz", "tokens": ["M\u00f6\u00b7ge", "froh", "ihr", "hol\u00b7des", "Ant\u00b7litz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADJD", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihnen draus entgegenschauen!", "tokens": ["Ih\u00b7nen", "draus", "ent\u00b7ge\u00b7gen\u00b7schau\u00b7en", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PAV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Hat ja selbst Natur, die ernste,", "tokens": ["Hat", "ja", "selbst", "Na\u00b7tur", ",", "die", "erns\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nichts so sch\u00f6n gemacht auf Erden,", "tokens": ["Nichts", "so", "sch\u00f6n", "ge\u00b7macht", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "ADJD", "VVPP", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie den Spiegel, drin sie anschaut", "tokens": ["Wie", "den", "Spie\u00b7gel", ",", "drin", "sie", "an\u00b7schaut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "$,", "ADV", "PPER", "VVFIN"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Ihre Z\u00fcge und Geb\u00e4rden.", "tokens": ["Ih\u00b7re", "Z\u00fc\u00b7ge", "und", "Ge\u00b7b\u00e4r\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie betrachtet durch des reinen", "tokens": ["Sie", "be\u00b7trach\u00b7tet", "durch", "des", "rei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Menschenauges Zauberspiegel", "tokens": ["Men\u00b7schen\u00b7au\u00b7ges", "Zau\u00b7ber\u00b7spie\u00b7gel"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihrer Z\u00fcge sch\u00f6ne R\u00e4tsel,", "tokens": ["Ih\u00b7rer", "Z\u00fc\u00b7ge", "sch\u00f6\u00b7ne", "R\u00e4t\u00b7sel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ein l\u00e4chelnd Gottessiegel.", "tokens": ["Wie", "ein", "l\u00e4\u00b7chelnd", "Got\u00b7tes\u00b7sie\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJD", "NN", "$."], "meter": "--+-+-++", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "Rings hinaus in alle ", "tokens": ["Rings", "hin\u00b7aus", "in", "al\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APZR", "APPR", "PIAT"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ist das Weltmeer hingegossen,", "tokens": ["Ist", "das", "Welt\u00b7meer", "hin\u00b7ge\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch ein Ozean der ", "tokens": ["Doch", "ein", "O\u00b7ze\u00b7an", "der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ist das Auge, eng umschlossen.", "tokens": ["Ist", "das", "Au\u00b7ge", ",", "eng", "um\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Welten schwimmen auf den Fluten", "tokens": ["Wel\u00b7ten", "schwim\u00b7men", "auf", "den", "Flu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieses Meers an uns heran,", "tokens": ["Die\u00b7ses", "Meers", "an", "uns", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In den ewgen Geist hinunter", "tokens": ["In", "den", "ew\u00b7gen", "Geist", "hin\u00b7un\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "APPO"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Reicht der stille Ozean.", "tokens": ["Reicht", "der", "stil\u00b7le", "O\u00b7ze\u00b7an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Lieben kann ich Ungeschautes,", "tokens": ["Lie\u00b7ben", "kann", "ich", "Un\u00b7ge\u00b7schau\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "VMFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Klang es hold mir; doch anbeten", "tokens": ["Klang", "es", "hold", "mir", ";", "doch", "an\u00b7be\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PPER", "ADJD", "PPER", "$.", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Werd ich nur, was sch\u00f6n und g\u00f6ttlich", "tokens": ["Werd", "ich", "nur", ",", "was", "sch\u00f6n", "und", "g\u00f6tt\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "$,", "PRELS", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vor das Auge mir getreten.", "tokens": ["Vor", "das", "Au\u00b7ge", "mir", "ge\u00b7tre\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Schauen ist die h\u00f6chste Wonne;", "tokens": ["Schau\u00b7en", "ist", "die", "h\u00f6chs\u00b7te", "Won\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wehe, wer das Licht verloren!", "tokens": ["We\u00b7he", ",", "wer", "das", "Licht", "ver\u00b7lo\u00b7ren", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jedes Gl\u00fcck ist seinem Dunkel", "tokens": ["Je\u00b7des", "Gl\u00fcck", "ist", "sei\u00b7nem", "Dun\u00b7kel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ein Gr\u00fc\u00dfen vor den Toren;", "tokens": ["Wie", "ein", "Gr\u00fc\u00b7\u00dfen", "vor", "den", "To\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Jeder Schmerz wird doppelt heftig", "tokens": ["Je\u00b7der", "Schmerz", "wird", "dop\u00b7pelt", "hef\u00b7tig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "ADJD", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der Brust dem Blinden schlagen,", "tokens": ["In", "der", "Brust", "dem", "Blin\u00b7den", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil die M\u00e4chte ihm des Lebens", "tokens": ["Weil", "die", "M\u00e4ch\u00b7te", "ihm", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jeden stillen Trost versagen.", "tokens": ["Je\u00b7den", "stil\u00b7len", "Trost", "ver\u00b7sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Weinen h\u00f6rt er die Entr\u00fcckten,", "tokens": ["Wei\u00b7nen", "h\u00f6rt", "er", "die", "Ent\u00b7r\u00fcck\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lachen h\u00f6rt er sie beklommen,", "tokens": ["La\u00b7chen", "h\u00f6rt", "er", "sie", "be\u00b7klom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch der Wehmut stilles L\u00e4cheln", "tokens": ["Doch", "der", "Weh\u00b7mut", "stil\u00b7les", "L\u00e4\u00b7cheln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ihr Trost ist ihm genommen.", "tokens": ["Und", "ihr", "Trost", "ist", "ihm", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Tiefer st\u00fcrzt der Schmerz beim Anruf", "tokens": ["Tie\u00b7fer", "st\u00fcrzt", "der", "Schmerz", "beim", "An\u00b7ruf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gleich dem Hirsche, dem erschrocknen,", "tokens": ["Gleich", "dem", "Hir\u00b7sche", ",", "dem", "er\u00b7schrock\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In die Wildnis; doch das stumme", "tokens": ["In", "die", "Wild\u00b7nis", ";", "doch", "das", "stum\u00b7me"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$.", "ADV", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "L\u00e4cheln kann das Auge trocknen", "tokens": ["L\u00e4\u00b7cheln", "kann", "das", "Au\u00b7ge", "trock\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "ART", "NN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Ziska hat gen Rabys Mauern", "tokens": ["Zis\u00b7ka", "hat", "gen", "Ra\u00b7bys", "Mau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "APPR", "NE", "NN"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.2": {"text": "Seines Heeres Sturm gewendet,", "tokens": ["Sei\u00b7nes", "Hee\u00b7res", "Sturm", "ge\u00b7wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Als ein Pfeil ihm auch das zweite", "tokens": ["Als", "ein", "Pfeil", "ihm", "auch", "das", "zwei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPER", "ADV", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auge trifft, er ist geblendet.", "tokens": ["Au\u00b7ge", "trifft", ",", "er", "ist", "ge\u00b7blen\u00b7det", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PPER", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Tiefer wird er nun betrauern", "tokens": ["Tie\u00b7fer", "wird", "er", "nun", "be\u00b7trau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hussens Tod, des edlen Helden,", "tokens": ["Hus\u00b7sens", "Tod", ",", "des", "ed\u00b7len", "Hel\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hei\u00dfer, wilder, schreckenvoller", "tokens": ["Hei\u00b7\u00dfer", ",", "wil\u00b7der", ",", "schre\u00b7cken\u00b7vol\u00b7ler"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "ADJA", "$,", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird sein Zorn der Welt sich melden.", "tokens": ["Wird", "sein", "Zorn", "der", "Welt", "sich", "mel\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ART", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}