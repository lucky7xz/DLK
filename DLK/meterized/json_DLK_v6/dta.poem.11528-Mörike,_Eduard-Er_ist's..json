{"dta.poem.11528": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Er ist's.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Fr\u00fchling l\u00e4\u00dft sein blaues Band", "tokens": ["Fr\u00fch\u00b7ling", "l\u00e4\u00dft", "sein", "blau\u00b7es", "Band"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wieder flattern durch die L\u00fcfte,", "tokens": ["Wie\u00b7der", "flat\u00b7tern", "durch", "die", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "S\u00fc\u00dfe, wohlbekannte D\u00fcfte", "tokens": ["S\u00fc\u00b7\u00dfe", ",", "wohl\u00b7be\u00b7kann\u00b7te", "D\u00fcf\u00b7te"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Streifen ahnungsvoll das Land.", "tokens": ["Strei\u00b7fen", "ah\u00b7nungs\u00b7voll", "das", "Land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Veilchen tr\u00e4umen schon,", "tokens": ["Veil\u00b7chen", "tr\u00e4u\u00b7men", "schon", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Wollen balde kommen.", "tokens": ["Wol\u00b7len", "bal\u00b7de", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "\u2014 Horch, von fern ein leiser Harfenton!", "tokens": ["Horch", ",", "von", "fern", "ein", "lei\u00b7ser", "Har\u00b7fen\u00b7ton", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "$,", "APPR", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Fr\u00fchling, ja du bist's,", "tokens": ["Fr\u00fch\u00b7ling", ",", "ja", "du", "bist's", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "PPER", "NE", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Fr\u00fchling, ja du bist's!", "tokens": ["Fr\u00fch\u00b7ling", ",", "ja", "du", "bist's", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "PPER", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.10": {"text": "Dich hab' ich vernommen!", "tokens": ["Dich", "hab'", "ich", "ver\u00b7nom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}