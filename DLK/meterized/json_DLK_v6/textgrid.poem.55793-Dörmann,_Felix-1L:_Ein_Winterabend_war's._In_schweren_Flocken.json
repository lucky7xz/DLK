{"textgrid.poem.55793": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Winterabend war's. In schweren Flocken", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Winterabend war's. In schweren Flocken", "tokens": ["Ein", "Win\u00b7ter\u00b7a\u00b7bend", "wa\u00b7r'", "s.", "In", "schwe\u00b7ren", "Flo\u00b7cken"], "token_info": ["word", "word", "word", "abbreviation", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sank ... langsam ... Schnee auf Schnee. \u2013 Verschimmert war", "tokens": ["Sank", "...", "lang\u00b7sam", "...", "Schnee", "auf", "Schnee", ".", "\u2013", "Ver\u00b7schim\u00b7mert", "war"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["NN", "$(", "ADJD", "$(", "NN", "APPR", "NN", "$.", "$(", "VVPP", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der letzte Tagesschein, und tr\u00fcbes Licht", "tokens": ["Der", "letz\u00b7te", "Ta\u00b7ges\u00b7schein", ",", "und", "tr\u00fc\u00b7bes", "Licht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Stra\u00dfe glitt in's d\u00e4mm'rige Gemach.", "tokens": ["Der", "Stra\u00b7\u00dfe", "glitt", "in's", "d\u00e4m\u00b7m'\u00b7ri\u00b7ge", "Ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Ich war allein ...", "tokens": ["Ich", "war", "al\u00b7lein", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Wie zornig-dumpfes Meeresbrausen grollte", "tokens": ["Wie", "zor\u00b7nig\u00b7dump\u00b7fes", "Mee\u00b7res\u00b7brau\u00b7sen", "groll\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der L\u00e4rm des Lebens in mein stilles Heim,", "tokens": ["Der", "L\u00e4rm", "des", "Le\u00b7bens", "in", "mein", "stil\u00b7les", "Heim", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und schrilles Pfeifen und verworrenes Rufen", "tokens": ["Und", "schril\u00b7les", "Pfei\u00b7fen", "und", "ver\u00b7wor\u00b7re\u00b7nes", "Ru\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Und tausend andere T\u00f6ne gellten drein ...", "tokens": ["Und", "tau\u00b7send", "an\u00b7de\u00b7re", "T\u00f6\u00b7ne", "gell\u00b7ten", "drein", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "ADJA", "NN", "VVFIN", "PTKVZ", "$("], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Mir aber blieben diese T\u00f6ne fremd", "tokens": ["Mir", "a\u00b7ber", "blie\u00b7ben", "die\u00b7se", "T\u00f6\u00b7ne", "fremd"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PDAT", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und fern und unverstanden, wie sonst nie.", "tokens": ["Und", "fern", "und", "un\u00b7ver\u00b7stan\u00b7den", ",", "wie", "sonst", "nie", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "$,", "PWAV", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Versunken war f\u00fcr mich der Lebensreigen,", "tokens": ["Ver\u00b7sun\u00b7ken", "war", "f\u00fcr", "mich", "der", "Le\u00b7bens\u00b7rei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Der sich vor meinen Fenstern tosend schwang ...", "tokens": ["Der", "sich", "vor", "mei\u00b7nen", "Fens\u00b7tern", "to\u00b7send", "schwang", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PPOSAT", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Vergangenes war erwacht, und Todtes lebte", "tokens": ["Ver\u00b7gan\u00b7ge\u00b7nes", "war", "er\u00b7wacht", ",", "und", "Tod\u00b7tes", "leb\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "VVPP", "$,", "KON", "NN", "VVFIN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nach trostlos-stumpfen Jahren in mir auf \u2013", "tokens": ["Nach", "trost\u00b7los\u00b7stump\u00b7fen", "Jah\u00b7ren", "in", "mir", "auf", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPER", "APPR", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und einer Seele dacht' ich, die gestorben", "tokens": ["Und", "ei\u00b7ner", "See\u00b7le", "dacht'", "ich", ",", "die", "ge\u00b7stor\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "$,", "PRELS", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "An ihrer grenzenlosen Einsamkeit", "tokens": ["An", "ih\u00b7rer", "gren\u00b7zen\u00b7lo\u00b7sen", "Ein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und ihrer Liebe ...", "tokens": ["Und", "ih\u00b7rer", "Lie\u00b7be", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}}}}}