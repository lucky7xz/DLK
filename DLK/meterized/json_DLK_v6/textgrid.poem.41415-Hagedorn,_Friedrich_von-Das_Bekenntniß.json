{"textgrid.poem.41415": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Das Bekenntni\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein feuriger Galan, der schlechten Dank erwarb,", "tokens": ["Ein", "feu\u00b7ri\u00b7ger", "Ga\u00b7lan", ",", "der", "schlech\u00b7ten", "Dank", "er\u00b7warb", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nicht viel r\u00fchmlicher, als Pherecydes, starb,", "tokens": ["Und", "nicht", "viel", "r\u00fchm\u00b7li\u00b7cher", ",", "als", "Pher\u00b7e\u00b7cy\u00b7des", ",", "starb", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "ADJD", "$,", "KOUS", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bekannte, was an ihm bereits unheilbar worden,", "tokens": ["Be\u00b7kann\u00b7te", ",", "was", "an", "ihm", "be\u00b7reits", "un\u00b7heil\u00b7bar", "wor\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "PPER", "ADV", "ADJD", "VAPP", "$,"], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Dem Priester Francion vom Carmeliterorden,", "tokens": ["Dem", "Pries\u00b7ter", "Fran\u00b7ci\u00b7on", "vom", "Car\u00b7me\u00b7li\u00b7te\u00b7ror\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und sprach: Wie straft mich jetzt des Lasters Sch\u00e4ndlichkeit!", "tokens": ["Und", "sprach", ":", "Wie", "straft", "mich", "jetzt", "des", "Las\u00b7ters", "Sch\u00e4nd\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PWAV", "VVFIN", "PPER", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ach kennt' ich, so wie ihr, doch keine L\u00fcsternheit;", "tokens": ["Ach", "kennt'", "ich", ",", "so", "wie", "ihr", ",", "doch", "kei\u00b7ne", "L\u00fcs\u00b7tern\u00b7heit", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "$,", "ADV", "KOKOM", "PPER", "$,", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So h\u00e4tt' ich diesen Tod nicht Julien zu danken!", "tokens": ["So", "h\u00e4tt'", "ich", "die\u00b7sen", "Tod", "nicht", "Ju\u00b7li\u00b7en", "zu", "dan\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PDAT", "NN", "PTKNEG", "NE", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie? Julien? o schweigt! versetzt der M\u00f6nch dem Kranken.", "tokens": ["Wie", "?", "Ju\u00b7li\u00b7en", "?", "o", "schweigt", "!", "ver\u00b7setzt", "der", "M\u00f6nch", "dem", "Kran\u00b7ken", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "NE", "$.", "FM", "VVFIN", "$.", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den L\u00fcgen bin ich gram; das ist des Ordens Pflicht.", "tokens": ["Den", "L\u00fc\u00b7gen", "bin", "ich", "gram", ";", "das", "ist", "des", "Or\u00b7dens", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "NE", "$.", "PDS", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Verleumdet Juliens gesunde Sch\u00f6nheit nicht.", "tokens": ["Ver\u00b7leum\u00b7det", "Ju\u00b7li\u00b7ens", "ge\u00b7sun\u00b7de", "Sch\u00f6n\u00b7heit", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "W\u00e4r' ein so schn\u00f6des Gift bei Julchen eingerissen,", "tokens": ["W\u00e4r'", "ein", "so", "schn\u00f6\u00b7des", "Gift", "bei", "Jul\u00b7chen", "ein\u00b7ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADV", "ADJA", "NN", "APPR", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Pater Guardian, und ich, wir m\u00fc\u00dften's wissen.", "tokens": ["Der", "Pa\u00b7ter", "Gu\u00b7ar\u00b7di\u00b7an", ",", "und", "ich", ",", "wir", "m\u00fc\u00df\u00b7ten's", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "KON", "PPER", "$,", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}