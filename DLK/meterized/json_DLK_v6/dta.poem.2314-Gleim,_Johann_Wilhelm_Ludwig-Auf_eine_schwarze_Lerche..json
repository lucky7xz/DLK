{"dta.poem.2314": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Auf eine schwarze Lerche.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905198996", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Lerche! mit dem schwarzen Kopfe,", "tokens": ["Ler\u00b7che", "!", "mit", "dem", "schwar\u00b7zen", "Kop\u00b7fe", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit dem gl\u00e4nzend schwarzen Schnabel,", "tokens": ["Mit", "dem", "gl\u00e4n\u00b7zend", "schwar\u00b7zen", "Schna\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sage! bist du nicht ein H\u00e4hnchen?", "tokens": ["Sa\u00b7ge", "!", "bist", "du", "nicht", "ein", "H\u00e4hn\u00b7chen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VAFIN", "PPER", "PTKNEG", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deine freie Vogelmine", "tokens": ["Dei\u00b7ne", "frei\u00b7e", "Vo\u00b7gel\u00b7mi\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ist so m\u00e4nnlich, wie die meine,", "tokens": ["Ist", "so", "m\u00e4nn\u00b7lich", ",", "wie", "die", "mei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "$,", "PWAV", "ART", "PPOSAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und deshalb lobt dich mein M\u00e4dchen.", "tokens": ["Und", "des\u00b7halb", "lobt", "dich", "mein", "M\u00e4d\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Sage! hast du denn kein Weibgen?", "tokens": ["Sa\u00b7ge", "!", "hast", "du", "denn", "kein", "Weib\u00b7gen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VAFIN", "PPER", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sind dir keine Kinder \u00e4hnlich?", "tokens": ["Sind", "dir", "kei\u00b7ne", "Kin\u00b7der", "\u00e4hn\u00b7lich", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Oder, hast du keine Schwestern?", "tokens": ["O\u00b7der", ",", "hast", "du", "kei\u00b7ne", "Schwes\u00b7tern", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VAFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Wo sind deine Anverwandten?", "tokens": ["Wo", "sind", "dei\u00b7ne", "An\u00b7ver\u00b7wand\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Gleicht dein Vater dir an Farbe?", "tokens": ["Gleicht", "dein", "Va\u00b7ter", "dir", "an", "Far\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Oder, was hat ihn bewogen,", "tokens": ["O\u00b7der", ",", "was", "hat", "ihn", "be\u00b7wo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Da\u00df er dich so schwarz gef\u00e4rbet?", "tokens": ["Da\u00df", "er", "dich", "so", "schwarz", "ge\u00b7f\u00e4r\u00b7bet", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Denn es gleicht dir ia kein Bruder.", "tokens": ["Denn", "es", "gleicht", "dir", "i\u00b7a", "kein", "Bru\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "PIAT", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.15": {"text": "Vogel! schaffe mir geschwinde", "tokens": ["Vo\u00b7gel", "!", "schaf\u00b7fe", "mir", "ge\u00b7schwin\u00b7de"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "VVFIN", "PPER", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Junge Lerchen, die dir gleichen;", "tokens": ["Jun\u00b7ge", "Ler\u00b7chen", ",", "die", "dir", "glei\u00b7chen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Ja! du must dich gleich verlieben.", "tokens": ["Ja", "!", "du", "must", "dich", "gleich", "ver\u00b7lie\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Sich! hier ist f\u00fcr dich ein Weibgen.", "tokens": ["Sich", "!", "hier", "ist", "f\u00fcr", "dich", "ein", "Weib\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "$.", "ADV", "VAFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Sieh! mein M\u00e4dchen soll dirs geben,", "tokens": ["Sieh", "!", "mein", "M\u00e4d\u00b7chen", "soll", "dirs", "ge\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPOSAT", "NN", "VMFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Nimms und schaffe mir Brunetten.", "tokens": ["Nimms", "und", "schaf\u00b7fe", "mir", "Bru\u00b7net\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "PPER", "NN", "$."], "meter": "+-+-+++-", "measure": "unknown.measure.penta"}, "line.21": {"text": "Ich will sehn, ob deine Br\u00fcder", "tokens": ["Ich", "will", "sehn", ",", "ob", "dei\u00b7ne", "Br\u00fc\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "KOUS", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Ebenfals Brunetten lieben.", "tokens": ["E\u00b7ben\u00b7fals", "Bru\u00b7net\u00b7ten", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVINF", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.23": {"text": "M\u00e4dchen, sieh! er wird sich paaren,", "tokens": ["M\u00e4d\u00b7chen", ",", "sieh", "!", "er", "wird", "sich", "paa\u00b7ren", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "$.", "PPER", "VAFIN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "M\u00e4dchen, sieh! er ist kein H\u00e4hnchen.", "tokens": ["M\u00e4d\u00b7chen", ",", "sieh", "!", "er", "ist", "kein", "H\u00e4hn\u00b7chen", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "$.", "PPER", "VAFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Sieh! wie artig kan man irren!", "tokens": ["Sieh", "!", "wie", "ar\u00b7tig", "kan", "man", "ir\u00b7ren", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAV", "ADJD", "VMFIN", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Ist dein Weibgen doch ein H\u00e4hnchen.", "tokens": ["Ist", "dein", "Weib\u00b7gen", "doch", "ein", "H\u00e4hn\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Gleicht dir doch mein Fr\u00fclingsbote,", "tokens": ["Gleicht", "dir", "doch", "mein", "Fr\u00fc\u00b7lings\u00b7bo\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "An Geschlecht, und Lust, und Farbe", "tokens": ["An", "Ge\u00b7schlecht", ",", "und", "Lust", ",", "und", "Far\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "KON", "NN", "$,", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Wie er mir an Freiheit gleichet.", "tokens": ["Wie", "er", "mir", "an", "Frei\u00b7heit", "glei\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Da! ich schenk ihn dir den Vogel,", "tokens": ["Da", "!", "ich", "schenk", "ihn", "dir", "den", "Vo\u00b7gel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VVFIN", "PPER", "PPER", "ART", "NE", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.31": {"text": "Unvergleichliche Brunette!", "tokens": ["Un\u00b7ver\u00b7gleich\u00b7li\u00b7che", "Bru\u00b7net\u00b7te", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.32": {"text": "Lieb\u2019 ihn, denn er ist dir \u00e4hnlich.", "tokens": ["Lieb'", "ihn", ",", "denn", "er", "ist", "dir", "\u00e4hn\u00b7lich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KON", "PPER", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Doris! ia, du kanst ia malen,", "tokens": ["Do\u00b7ris", "!", "i\u00b7a", ",", "du", "kanst", "i\u00b7a", "ma\u00b7len", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NE", "$,", "PPER", "VMFIN", "ADV", "VVINF", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.34": {"text": "Hurtig male mir den Vogel,", "tokens": ["Hur\u00b7tig", "ma\u00b7le", "mir", "den", "Vo\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Mal\u2019 ihn zwischen andre Lerchen,", "tokens": ["Mal'", "ihn", "zwi\u00b7schen", "and\u00b7re", "Ler\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Da\u00df man sieht, wie er sich paaret.", "tokens": ["Da\u00df", "man", "sieht", ",", "wie", "er", "sich", "paa\u00b7ret", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "$,", "PWAV", "PPER", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}