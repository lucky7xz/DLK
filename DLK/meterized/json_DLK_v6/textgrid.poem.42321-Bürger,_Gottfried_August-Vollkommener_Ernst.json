{"textgrid.poem.42321": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Vollkommener Ernst", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sprich, junger Freund, o sprich, was dich bewegt,", "tokens": ["Sprich", ",", "jun\u00b7ger", "Freund", ",", "o", "sprich", ",", "was", "dich", "be\u00b7wegt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "ADJA", "NN", "$,", "FM", "ADJD", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nach schn\u00f6dem Dichterruhm dich atemlos zu laufen?", "tokens": ["Nach", "schn\u00f6\u00b7dem", "Dich\u00b7ter\u00b7ruhm", "dich", "a\u00b7tem\u00b7los", "zu", "lau\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ha, diesen Dorn, den ach! mein Wohlsein in sich tr\u00e4gt,", "tokens": ["Ha", ",", "die\u00b7sen", "Dorn", ",", "den", "ach", "!", "mein", "Wohl\u00b7sein", "in", "sich", "tr\u00e4gt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PDAT", "NN", "$,", "ART", "XY", "$.", "PPOSAT", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Den Satans-Engel, der mein Gl\u00fcck mit F\u00e4usten schl\u00e4gt,", "tokens": ["Den", "Sa\u00b7tans\u00b7En\u00b7gel", ",", "der", "mein", "Gl\u00fcck", "mit", "F\u00e4us\u00b7ten", "schl\u00e4gt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wollt' ich \u2013 o k\u00f6nnt' ich nur! \u2013 spottwohlfeil dir verkaufen.", "tokens": ["Wollt'", "ich", "\u2013", "o", "k\u00f6nnt'", "ich", "nur", "!", "\u2013", "spott\u00b7wohl\u00b7feil", "dir", "ver\u00b7kau\u00b7fen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$(", "FM", "VMFIN", "PPER", "ADV", "$.", "$(", "VVIMP", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}