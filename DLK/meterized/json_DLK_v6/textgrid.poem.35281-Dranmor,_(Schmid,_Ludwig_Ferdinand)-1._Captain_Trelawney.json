{"textgrid.poem.35281": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "1. Captain Trelawney", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Kind, des Geistes Schwingen kaum entfaltend,", "tokens": ["Ein", "Kind", ",", "des", "Geis\u00b7tes", "Schwin\u00b7gen", "kaum", "ent\u00b7fal\u00b7tend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Las ich von Thaten, k\u00fchnen, wunderbaren,", "tokens": ["Las", "ich", "von", "Tha\u00b7ten", ",", "k\u00fch\u00b7nen", ",", "wun\u00b7der\u00b7ba\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "PPER", "APPR", "NN", "$,", "ADJA", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von Abenteuern, m\u00e4rchenhaft gestaltend", "tokens": ["Von", "A\u00b7bent\u00b7eu\u00b7ern", ",", "m\u00e4r\u00b7chen\u00b7haft", "ge\u00b7stal\u00b7tend"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Leben eines Dichters und Korsaren.", "tokens": ["Das", "Le\u00b7ben", "ei\u00b7nes", "Dich\u00b7ters", "und", "Kor\u00b7sa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dein Buch, Trelawney, war's, das thr\u00e4nennasse,", "tokens": ["Dein", "Buch", ",", "Tre\u00b7law\u00b7ney", ",", "wa\u00b7r's", ",", "das", "thr\u00e4\u00b7nen\u00b7nas\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "VAFIN", "$,", "ART", "NN", "$,"], "meter": "-++-+-+-+-+-", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Wie du's aus Indiens Meeren heimgetragen,", "tokens": ["Wie", "du's", "aus", "In\u00b7di\u00b7ens", "Mee\u00b7ren", "heim\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "NE", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Um es in tiefem und gerechtem Hasse", "tokens": ["Um", "es", "in", "tie\u00b7fem", "und", "ge\u00b7rech\u00b7tem", "Has\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PPER", "APPR", "ADJA", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Eitlen Pygm\u00e4en ins Gesicht zu schlagen.", "tokens": ["Eit\u00b7len", "Pyg\u00b7m\u00e4\u00b7en", "ins", "Ge\u00b7sicht", "zu", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Kamst du auch wieder mit gesenkter Lanze,", "tokens": ["Kamst", "du", "auch", "wie\u00b7der", "mit", "ge\u00b7senk\u00b7ter", "Lan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie standen da, bezwungen und geblendet", "tokens": ["Sie", "stan\u00b7den", "da", ",", "be\u00b7zwun\u00b7gen", "und", "ge\u00b7blen\u00b7det"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "VVPP", "KON", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von deiner Kriegstroph\u00e4en Zauberglanze,", "tokens": ["Von", "dei\u00b7ner", "Kriegs\u00b7trop\u00b7h\u00e4\u00b7en", "Zau\u00b7ber\u00b7glan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die sich von dir, dem J\u00fcngling, abgewendet,", "tokens": ["Die", "sich", "von", "dir", ",", "dem", "J\u00fcng\u00b7ling", ",", "ab\u00b7ge\u00b7wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PPER", "$,", "ART", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und wu\u00dften nicht, wie tapfer du gestritten,", "tokens": ["Und", "wu\u00df\u00b7ten", "nicht", ",", "wie", "tap\u00b7fer", "du", "ge\u00b7strit\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "PWAV", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verfolgt von niemals rastenden Gedanken,", "tokens": ["Ver\u00b7folgt", "von", "nie\u00b7mals", "ras\u00b7ten\u00b7den", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und da\u00df du tausendfachen Tod erlitten", "tokens": ["Und", "da\u00df", "du", "tau\u00b7send\u00b7fa\u00b7chen", "Tod", "er\u00b7lit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In deiner eignen Brust, der liebeskranken.", "tokens": ["In", "dei\u00b7ner", "eig\u00b7nen", "Brust", ",", "der", "lie\u00b7bes\u00b7kran\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ein junger Greis, von deiner V\u00e4ter Scholle,", "tokens": ["Ein", "jun\u00b7ger", "Greis", ",", "von", "dei\u00b7ner", "V\u00e4\u00b7ter", "Schol\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An Hoffnung arm, reich an Erinnerungen,", "tokens": ["An", "Hoff\u00b7nung", "arm", ",", "reich", "an", "E\u00b7rin\u00b7ne\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Griffst du hinein ins Herz, das \u00fcbervolle,", "tokens": ["Griffst", "du", "hin\u00b7ein", "ins", "Herz", ",", "das", "\u00fc\u00b7ber\u00b7vol\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und hast der Freiheit Hohelied gesungen.", "tokens": ["Und", "hast", "der", "Frei\u00b7heit", "Ho\u00b7he\u00b7lied", "ge\u00b7sun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Gewalt'ger Mann! Mein Held und mein Erretter!", "tokens": ["Ge\u00b7walt'\u00b7ger", "Mann", "!", "Mein", "Held", "und", "mein", "Er\u00b7ret\u00b7ter", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was du geliebt, verloren und gefeiert,", "tokens": ["Was", "du", "ge\u00b7liebt", ",", "ver\u00b7lo\u00b7ren", "und", "ge\u00b7fei\u00b7ert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das drang zu mir heran wie Fr\u00fchlingswetter,", "tokens": ["Das", "drang", "zu", "mir", "he\u00b7ran", "wie", "Fr\u00fch\u00b7lings\u00b7wet\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPER", "PTKVZ", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie Sonnenschein, von Pulverdampf umschleiert.", "tokens": ["Wie", "Son\u00b7nen\u00b7schein", ",", "von", "Pul\u00b7ver\u00b7dampf", "um\u00b7schlei\u00b7ert", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "O Kriegsfanfaren! Ruf aus fernen Zonen!", "tokens": ["O", "Kriegs\u00b7fan\u00b7fa\u00b7ren", "!", "Ruf", "aus", "fer\u00b7nen", "Zo\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O k\u00fchnes Tr\u00e4umen, knabenhaftes Sinnen!", "tokens": ["O", "k\u00fch\u00b7nes", "Tr\u00e4u\u00b7men", ",", "kna\u00b7ben\u00b7haf\u00b7tes", "Sin\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "War ich bestimmt, im Donner der Kanonen", "tokens": ["War", "ich", "be\u00b7stimmt", ",", "im", "Don\u00b7ner", "der", "Ka\u00b7no\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie du, Trelawney, Lorbeern zu gewinnen?", "tokens": ["Wie", "du", ",", "Tre\u00b7law\u00b7ney", ",", "Lor\u00b7beern", "zu", "ge\u00b7win\u00b7nen", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "NN", "$,", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Nein! Doch in meiner Jugend Phantasien,", "tokens": ["Nein", "!", "Doch", "in", "mei\u00b7ner", "Ju\u00b7gend", "Phan\u00b7ta\u00b7si\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "KON", "APPR", "PPOSAT", "NN", "NE", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Aus Wunden blutend, die ich hei\u00df erflehte,", "tokens": ["Aus", "Wun\u00b7den", "blu\u00b7tend", ",", "die", "ich", "hei\u00df", "er\u00b7fleh\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Lag ich vor jenem Banner auf den Knien,", "tokens": ["Lag", "ich", "vor", "je\u00b7nem", "Ban\u00b7ner", "auf", "den", "Kni\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PDAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das einst von sturmgepeitschten Masten wehte.", "tokens": ["Das", "einst", "von", "sturm\u00b7ge\u00b7peitschten", "Mas\u00b7ten", "weh\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Wohl! Was ich suchte: St\u00fcrme, Abenteuer,", "tokens": ["Wohl", "!", "Was", "ich", "such\u00b7te", ":", "St\u00fcr\u00b7me", ",", "A\u00b7bent\u00b7eu\u00b7er", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "$.", "PWS", "PPER", "VVFIN", "$.", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das hat das Schicksal reichlich mir gespendet;", "tokens": ["Das", "hat", "das", "Schick\u00b7sal", "reich\u00b7lich", "mir", "ge\u00b7spen\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nun steh' ich m\u00fcde am zerbrochnen Steuer,", "tokens": ["Nun", "steh'", "ich", "m\u00fc\u00b7de", "am", "zer\u00b7broch\u00b7nen", "Steu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und noch ist meine Reise nicht vollendet.", "tokens": ["Und", "noch", "ist", "mei\u00b7ne", "Rei\u00b7se", "nicht", "voll\u00b7en\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPOSAT", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Doch sieh! Mein Schwert blieb m\u00fc\u00dfig in der Scheide,", "tokens": ["Doch", "sieh", "!", "Mein", "Schwert", "blieb", "m\u00fc\u00b7\u00dfig", "in", "der", "Schei\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$.", "PPOSAT", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kein Feind bedrohte mich mit blanken Waffen,", "tokens": ["Kein", "Feind", "be\u00b7droh\u00b7te", "mich", "mit", "blan\u00b7ken", "Waf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich k\u00e4mpfte nur mit meinem innern Leide", "tokens": ["Ich", "k\u00e4mpf\u00b7te", "nur", "mit", "mei\u00b7nem", "in\u00b7nern", "Lei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und mit Phantomen, die ich selbst erschaffen.", "tokens": ["Und", "mit", "Phan\u00b7to\u00b7men", ",", "die", "ich", "selbst", "er\u00b7schaf\u00b7fen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "So ward ich \u00fcberholt von k\u00fchnern Schiffern;", "tokens": ["So", "ward", "ich", "\u00fc\u00b7berh\u00b7olt", "von", "k\u00fch\u00b7nern", "Schif\u00b7fern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie fuhren rasch vorbei zum sichern Ziele,", "tokens": ["Sie", "fuh\u00b7ren", "rasch", "vor\u00b7bei", "zum", "si\u00b7chern", "Zie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn ich im Traume rang mit goldnen Ziffern,", "tokens": ["Wenn", "ich", "im", "Trau\u00b7me", "rang", "mit", "gold\u00b7nen", "Zif\u00b7fern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verstrickt in meines Herzens Trauerspiele.", "tokens": ["Ver\u00b7strickt", "in", "mei\u00b7nes", "Her\u00b7zens", "Trau\u00b7er\u00b7spie\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Ach, bald verzagt auf sinkender Galeere", "tokens": ["Ach", ",", "bald", "ver\u00b7zagt", "auf", "sin\u00b7ken\u00b7der", "Ga\u00b7lee\u00b7re"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und bald berauscht von himmlischen Accorden,", "tokens": ["Und", "bald", "be\u00b7rauscht", "von", "himm\u00b7li\u00b7schen", "Ac\u00b7cor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So trieb ich hin und her auf hohem Meere", "tokens": ["So", "trieb", "ich", "hin", "und", "her", "auf", "ho\u00b7hem", "Mee\u00b7re"], "token_info": ["word", "word", "word", "wo