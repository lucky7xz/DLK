{"dta.poem.347": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An  Lysetten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Unvergleichliche Lysette", "tokens": ["Un\u00b7ver\u00b7gleich\u00b7li\u00b7che", "Ly\u00b7set\u00b7te"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine Sonne/ meine Lust/", "tokens": ["Mei\u00b7ne", "Son\u00b7ne", "/", "mei\u00b7ne", "Lust", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mache mir ein sanfftes Bette", "tokens": ["Ma\u00b7che", "mir", "ein", "sanff\u00b7tes", "Bet\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf den Rosen deiner Brust;", "tokens": ["Auf", "den", "Ro\u00b7sen", "dei\u00b7ner", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jhre Pracht", "tokens": ["Ih\u00b7re", "Pracht"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Hat gemacht", "tokens": ["Hat", "ge\u00b7macht"], "token_info": ["word", "word"], "pos": ["VAFIN", "VVPP"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Da\u00df mir Geist und Hertze brennen/", "tokens": ["Da\u00df", "mir", "Geist", "und", "Hert\u00b7ze", "bren\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "VVFIN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "So dich ihren Z\u00fcnd-Strick nennen.", "tokens": ["So", "dich", "ih\u00b7ren", "Z\u00fcn\u00b7dStrick", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "--+--+--", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Sch\u00f6nste G\u00f6ttin meiner Seelen/", "tokens": ["Sch\u00f6ns\u00b7te", "G\u00f6t\u00b7tin", "mei\u00b7ner", "See\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meines Lebens-Auffenhalt/", "tokens": ["Mei\u00b7nes", "Le\u00b7bens\u00b7Auf\u00b7fen\u00b7halt", "/"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die sich deiner Zier verm\u00e4hlen", "tokens": ["Die", "sich", "dei\u00b7ner", "Zier", "ver\u00b7m\u00e4h\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Labt die Englische Gestalt.", "tokens": ["Labt", "die", "Eng\u00b7li\u00b7sche", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und das Hertz/", "tokens": ["Und", "das", "Hertz", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Kan kein Schmertz", "tokens": ["Kan", "kein", "Schmertz"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PIAT", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Durch Betr\u00fcbni\u00df traurig machen/", "tokens": ["Durch", "Be\u00b7tr\u00fcb\u00b7ni\u00df", "trau\u00b7rig", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Welchen deine Augen lachen.", "tokens": ["Wel\u00b7chen", "dei\u00b7ne", "Au\u00b7gen", "la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Diese ungemeiue Freude", "tokens": ["Die\u00b7se", "un\u00b7ge\u00b7mei\u00b7ue", "Freu\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Treff\u2019 ich bey mir selber an/", "tokens": ["Treff'", "ich", "bey", "mir", "sel\u00b7ber", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPER", "ADV", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine Wohlfahrt geht in Seyde", "tokens": ["Mei\u00b7ne", "Wohl\u00b7fahrt", "geht", "in", "Sey\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf der sanfften Rosen-Bahn.", "tokens": ["Auf", "der", "sanff\u00b7ten", "Ro\u00b7sen\u00b7Bahn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich will auch", "tokens": ["Ich", "will", "auch"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Nach dem Brauch", "tokens": ["Nach", "dem", "Brauch"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Derer die best\u00e4ndig lieben/", "tokens": ["De\u00b7rer", "die", "be\u00b7st\u00e4n\u00b7dig", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "ADJD", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Keinen Wanckelmuth ver\u00fcben.", "tokens": ["Kei\u00b7nen", "Wan\u00b7ckel\u00b7muth", "ver\u00b7\u00fc\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}