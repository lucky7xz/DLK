{"textgrid.poem.56096": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Verk\u00fcndigung \u00fcber den Hirten", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seht auf, ihr M\u00e4nner. M\u00e4nner dort am Feuer,", "tokens": ["Seht", "auf", ",", "ihr", "M\u00e4n\u00b7ner", ".", "M\u00e4n\u00b7ner", "dort", "am", "Feu\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPOSAT", "NN", "$.", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "die ihr den grenzenlosen Himmel kennt,", "tokens": ["die", "ihr", "den", "gren\u00b7zen\u00b7lo\u00b7sen", "Him\u00b7mel", "kennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sterndeuter, hierher! Seht, ich bin ein neuer", "tokens": ["Stern\u00b7deu\u00b7ter", ",", "hier\u00b7her", "!", "Seht", ",", "ich", "bin", "ein", "neu\u00b7er"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PAV", "$.", "VVFIN", "$,", "PPER", "VAFIN", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "steigender Stern. Mein ganzes Wesen brennt", "tokens": ["stei\u00b7gen\u00b7der", "Stern", ".", "Mein", "gan\u00b7zes", "We\u00b7sen", "brennt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "und strahlt so stark und ist so ungeheuer", "tokens": ["und", "strahlt", "so", "stark", "und", "ist", "so", "un\u00b7ge\u00b7heu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "KON", "VAFIN", "ADV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "voll Licht, da\u00df mir das tiefe Firmament", "tokens": ["voll", "Licht", ",", "da\u00df", "mir", "das", "tie\u00b7fe", "Fir\u00b7ma\u00b7ment"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "NN", "$,", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "nicht mehr gen\u00fcgt. La\u00dft meinen Glanz hinein", "tokens": ["nicht", "mehr", "ge\u00b7n\u00fcgt", ".", "La\u00dft", "mei\u00b7nen", "Glanz", "hin\u00b7ein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "VVFIN", "$.", "VVIMP", "PPOSAT", "NN", "APZR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "in euer Dasein: Oh, die dunklen Blicke,", "tokens": ["in", "eu\u00b7er", "Da\u00b7sein", ":", "Oh", ",", "die", "dunk\u00b7len", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "ITJ", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "die dunklen Herzen, n\u00e4chtige Geschicke", "tokens": ["die", "dunk\u00b7len", "Her\u00b7zen", ",", "n\u00e4ch\u00b7ti\u00b7ge", "Ge\u00b7schi\u00b7cke"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "die euch erf\u00fcllen. Hirten, wie allein", "tokens": ["die", "euch", "er\u00b7f\u00fcl\u00b7len", ".", "Hir\u00b7ten", ",", "wie", "al\u00b7lein"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "PPER", "VVINF", "$.", "NN", "$,", "PWAV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "bin ich in euch. Auf einmal wird mir Raum.", "tokens": ["bin", "ich", "in", "euch", ".", "Auf", "ein\u00b7mal", "wird", "mir", "Raum", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "$.", "APPR", "ADV", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Stauntet ihr nicht: der gro\u00dfe Brotfruchtbaum", "tokens": ["Staun\u00b7tet", "ihr", "nicht", ":", "der", "gro\u00b7\u00dfe", "Brot\u00b7frucht\u00b7baum"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "$.", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.13": {"text": "warf einen Schatten. Ja, das kam von mir.", "tokens": ["warf", "ei\u00b7nen", "Schat\u00b7ten", ".", "Ja", ",", "das", "kam", "von", "mir", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "PTKANT", "$,", "PDS", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Ihr Unerschrockenen, o w\u00fc\u00dftet ihr,", "tokens": ["Ihr", "Un\u00b7er\u00b7schro\u00b7cke\u00b7nen", ",", "o", "w\u00fc\u00df\u00b7tet", "ihr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "FM", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "wie jetzt auf eurem schauenden Gesichte", "tokens": ["wie", "jetzt", "auf", "eu\u00b7rem", "schau\u00b7en\u00b7den", "Ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "die Zukunft scheint. In diesem starken Lichte", "tokens": ["die", "Zu\u00b7kunft", "scheint", ".", "In", "die\u00b7sem", "star\u00b7ken", "Lich\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "wird viel geschehen. Euch vertrau ichs, denn", "tokens": ["wird", "viel", "ge\u00b7sche\u00b7hen", ".", "Euch", "ver\u00b7trau", "ichs", ",", "denn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "ADV", "VVPP", "$.", "PPER", "VVFIN", "PIS", "$,", "KON"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "ihr seid verschwiegen; euch Gradgl\u00e4ubigen", "tokens": ["ihr", "seid", "ver\u00b7schwie\u00b7gen", ";", "euch", "Grad\u00b7gl\u00e4u\u00b7bi\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "$.", "PPER", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "redet hier alles. Glut und Regen spricht,", "tokens": ["re\u00b7det", "hier", "al\u00b7les", ".", "Glut", "und", "Re\u00b7gen", "spricht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIS", "$.", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.20": {"text": "der V\u00f6gel Zug, der Wind und was ihr seid,", "tokens": ["der", "V\u00f6\u00b7gel", "Zug", ",", "der", "Wind", "und", "was", "ihr", "seid", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "KON", "PWS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "keins \u00fcberwiegt und w\u00e4chst zur Eitelkeit", "tokens": ["keins", "\u00fc\u00b7berw\u00b7iegt", "und", "w\u00e4chst", "zur", "Ei\u00b7tel\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "VVPP", "KON", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "sich m\u00e4stend an. Ihr haltet nicht", "tokens": ["sich", "m\u00e4s\u00b7tend", "an", ".", "Ihr", "hal\u00b7tet", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "ADJD", "PTKVZ", "$.", "PPER", "VVFIN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "die Dinge auf im Zwischenraum der Brust", "tokens": ["die", "Din\u00b7ge", "auf", "im", "Zwi\u00b7schen\u00b7raum", "der", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "um sie zu qu\u00e4len. So wie seine Lust", "tokens": ["um", "sie", "zu", "qu\u00e4\u00b7len", ".", "So", "wie", "sei\u00b7ne", "Lust"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUI", "PPER", "PTKZU", "VVINF", "$.", "ADV", "KOKOM", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "durch einen Engel str\u00f6mt, so treibt durch euch", "tokens": ["durch", "ei\u00b7nen", "En\u00b7gel", "str\u00f6mt", ",", "so", "treibt", "durch", "euch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "das Irdische. Und wenn ein Dorngestr\u00e4uch", "tokens": ["das", "Ir\u00b7di\u00b7sche", ".", "Und", "wenn", "ein", "Dorn\u00b7ge\u00b7str\u00e4uch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "KON", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "aufflammte pl\u00f6tzlich, d\u00fcrfte noch aus ihm", "tokens": ["auf\u00b7flamm\u00b7te", "pl\u00f6tz\u00b7lich", ",", "d\u00fcrf\u00b7te", "noch", "aus", "ihm"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "$,", "VMFIN", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "der Ewige euch rufen, Cherubim,", "tokens": ["der", "E\u00b7wi\u00b7ge", "euch", "ru\u00b7fen", ",", "Che\u00b7ru\u00b7bim", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "PPER", "VVINF", "$,", "NE", "$,"], "meter": "-++--+--+-", "measure": "iambic.tetra.relaxed"}, "line.29": {"text": "wenn sie geruhten neben eurer Herde", "tokens": ["wenn", "sie", "ge\u00b7ruh\u00b7ten", "ne\u00b7ben", "eu\u00b7rer", "Her\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "einherzuschreiten, wunderten euch nicht:", "tokens": ["ein\u00b7her\u00b7zu\u00b7schrei\u00b7ten", ",", "wun\u00b7der\u00b7ten", "euch", "nicht", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "ihr st\u00fcrztet euch auf euer Angesicht,", "tokens": ["ihr", "st\u00fcrz\u00b7tet", "euch", "auf", "eu\u00b7er", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "betetet an und nenntet dies die Erde.", "tokens": ["be\u00b7te\u00b7tet", "an", "und", "nenn\u00b7tet", "dies", "die", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "VVFIN", "PDS", "ART", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Doch dieses war. Nun soll ein Neues sein,", "tokens": ["Doch", "die\u00b7ses", "war", ".", "Nun", "soll", "ein", "Neu\u00b7es", "sein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "$.", "ADV", "VMFIN", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "von dem der Erdkreis ringender sich weitet.", "tokens": ["von", "dem", "der", "Erd\u00b7kreis", "rin\u00b7gen\u00b7der", "sich", "wei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "ADV", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was ist ein D\u00f6rnicht uns: Gott f\u00fchlt sich ein", "tokens": ["Was", "ist", "ein", "D\u00f6r\u00b7nicht", "uns", ":", "Gott", "f\u00fchlt", "sich", "ein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "NN", "PPER", "$.", "NN", "VVFIN", "PRF", "ART"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "in einer Jungfrau Schoo\u00df. Ich bin der Schein", "tokens": ["in", "ei\u00b7ner", "Jung\u00b7frau", "Schoo\u00df", ".", "Ich", "bin", "der", "Schein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "$.", "PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "von ihrer Innigkeit, der euch geleitet.", "tokens": ["von", "ih\u00b7rer", "In\u00b7nig\u00b7keit", ",", "der", "euch", "ge\u00b7lei\u00b7tet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}