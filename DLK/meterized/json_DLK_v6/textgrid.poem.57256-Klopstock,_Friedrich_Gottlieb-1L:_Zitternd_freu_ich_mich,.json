{"textgrid.poem.57256": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zitternd freu ich mich,", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zitternd freu ich mich,", "tokens": ["Zit\u00b7ternd", "freu", "ich", "mich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "PRF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Und w\u00fcrd' es nicht glauben;", "tokens": ["Und", "w\u00fcrd'", "es", "nicht", "glau\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "W\u00e4re der grosse Verheisser", "tokens": ["W\u00e4\u00b7re", "der", "gros\u00b7se", "Ver\u00b7heis\u00b7ser"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Nicht der Ewige!", "tokens": ["Nicht", "der", "E\u00b7wi\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "$."], "meter": "+-+--", "measure": "unknown.measure.di"}}, "stanza.2": {"line.1": {"text": "Denn ich weiss es, ich f\u00fchl es:", "tokens": ["Denn", "ich", "weiss", "es", ",", "ich", "f\u00fchl", "es", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Ich bin ein S\u00fcnder!", "tokens": ["Ich", "bin", "ein", "S\u00fcn\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "W\u00fcsst' es, und f\u00fchlt' es,", "tokens": ["W\u00fcsst'", "es", ",", "und", "f\u00fchlt'", "es", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "VVFIN", "PPER", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Wenn auch das Gotteslicht", "tokens": ["Wenn", "auch", "das", "Got\u00b7tes\u00b7licht"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Heller mir meine Flecken nicht zeigte;", "tokens": ["Hel\u00b7ler", "mir", "mei\u00b7ne", "Fle\u00b7cken", "nicht", "zeig\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Vor meinen weiseren Blicken", "tokens": ["Vor", "mei\u00b7nen", "wei\u00b7se\u00b7ren", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Nicht enth\u00fcllte", "tokens": ["Nicht", "ent\u00b7h\u00fcll\u00b7te"], "token_info": ["word", "word"], "pos": ["PTKNEG", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Meiner verwundeten Seele Gestalt.", "tokens": ["Mei\u00b7ner", "ver\u00b7wun\u00b7de\u00b7ten", "See\u00b7le", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.4": {"line.1": {"text": "Mit gesunkenem Knie,", "tokens": ["Mit", "ge\u00b7sun\u00b7ke\u00b7nem", "Knie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Mit tiefanbetendem Staunen,", "tokens": ["Mit", "tie\u00b7fan\u00b7be\u00b7ten\u00b7dem", "Stau\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Freu ich mich!", "tokens": ["Freu", "ich", "mich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Ich werde Gott schaun!", "tokens": ["Ich", "wer\u00b7de", "Gott", "schaun", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Forsch ihm nach, dem g\u00f6ttlichsten Gedanken,", "tokens": ["Forsch", "ihm", "nach", ",", "dem", "g\u00f6tt\u00b7lichs\u00b7ten", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PTKVZ", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Den du zu denken vermagst,", "tokens": ["Den", "du", "zu", "den\u00b7ken", "ver\u00b7magst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "O die du n\u00e4her stets des Leibes Grabe,", "tokens": ["O", "die", "du", "n\u00e4\u00b7her", "stets", "des", "Lei\u00b7bes", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PRELS", "PPER", "ADJD", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aber ewig bist!", "tokens": ["A\u00b7ber", "e\u00b7wig", "bist", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Nicht, dass du wagtest,", "tokens": ["Nicht", ",", "dass", "du", "wag\u00b7test", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Zu gehn in das Allerheiligste!", "tokens": ["Zu", "gehn", "in", "das", "Al\u00b7ler\u00b7hei\u00b7ligs\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "ADJA", "$."], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Viel un\u00fcberdachte, nie gepriesene, nie gefeyerte,", "tokens": ["Viel", "un\u00b7\u00fc\u00b7berd\u00b7ach\u00b7te", ",", "nie", "ge\u00b7prie\u00b7se\u00b7ne", ",", "nie", "ge\u00b7fey\u00b7er\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "$,", "ADV", "ADJA", "$,", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+--+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.4": {"text": "Himlische Gnaden sind in dem Heiligthume.", "tokens": ["Him\u00b7li\u00b7sche", "Gna\u00b7den", "sind", "in", "dem", "Hei\u00b7lig\u00b7thu\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "APPR", "ART", "NN", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Aus der Ferne nur, nur Einen gemilderten Schimmer,", "tokens": ["Aus", "der", "Fer\u00b7ne", "nur", ",", "nur", "Ei\u00b7nen", "ge\u00b7mil\u00b7der\u00b7ten", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Damit ich nicht sterbe!", "tokens": ["Da\u00b7mit", "ich", "nicht", "ster\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Einen f\u00fcr mich durch Erdenacht gemilderten Schimmer", "tokens": ["Ei\u00b7nen", "f\u00fcr", "mich", "durch", "Er\u00b7de\u00b7nacht", "ge\u00b7mil\u00b7der\u00b7ten", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPER", "APPR", "NN", "ADJA", "NN"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Deiner Herlichkeit seh ich.", "tokens": ["Dei\u00b7ner", "Her\u00b7lich\u00b7keit", "seh", "ich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+++", "measure": "unknown.measure.penta"}}, "stanza.8": {"line.1": {"text": "Wie gross war der, der beten durfte:", "tokens": ["Wie", "gross", "war", "der", ",", "der", "be\u00b7ten", "durf\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "$,", "PRELS", "VVINF", "VMFIN", "$."], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Hab' ich Gnade vor dir gefunden, so lass mich", "tokens": ["Hab'", "ich", "Gna\u00b7de", "vor", "dir", "ge\u00b7fun\u00b7den", ",", "so", "lass", "mich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "NN", "APPR", "PPER", "VVPP", "$,", "ADV", "VVFIN", "PPER"], "meter": "+-+-+--+-++-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Deine Herlichkeit sehn!", "tokens": ["Dei\u00b7ne", "Her\u00b7lich\u00b7keit", "sehn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "So zum Unendlichen beten durft', und erh\u00f6rt ward!", "tokens": ["So", "zum", "Un\u00b7end\u00b7li\u00b7chen", "be\u00b7ten", "durft'", ",", "und", "er\u00b7h\u00f6rt", "ward", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVINF", "VMFIN", "$,", "KON", "VVPP", "VAFIN", "$."], "meter": "---+--+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "In das Land des Golgatha kam er nicht!", "tokens": ["In", "das", "Land", "des", "Gol\u00b7ga\u00b7tha", "kam", "er", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NE", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "An ihm r\u00e4cht' es ein fr\u00fcherer Tod;", "tokens": ["An", "ihm", "r\u00e4cht'", "es", "ein", "fr\u00fc\u00b7he\u00b7rer", "Tod", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Dass er Einmal, nur Einmal Gott nicht traute!", "tokens": ["Dass", "er", "Ein\u00b7mal", ",", "nur", "Ein\u00b7mal", "Gott", "nicht", "trau\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "ADV", "ADV", "NN", "PTKNEG", "VVFIN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Wie gross zeiget ihn selbst die Strafe!", "tokens": ["Wie", "gross", "zei\u00b7get", "ihn", "selbst", "die", "Stra\u00b7fe", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+---+-+-", "measure": "dactylic.init"}}, "stanza.10": {"line.1": {"text": "Ihn verbarg der Vater in eine Nacht des Berges,", "tokens": ["Ihn", "ver\u00b7barg", "der", "Va\u00b7ter", "in", "ei\u00b7ne", "Nacht", "des", "Ber\u00b7ges", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Als vor dem Endlichen vor\u00fcberging des Sohnes Herlichkeit!", "tokens": ["Als", "vor", "dem", "End\u00b7li\u00b7chen", "vor\u00b7\u00fc\u00b7ber\u00b7ging", "des", "Soh\u00b7nes", "Her\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Als die Posaun' auf Sinai schwieg,", "tokens": ["Als", "die", "Po\u00b7saun'", "auf", "Si\u00b7nai", "schwieg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und die Stimme der Donner! als Gott von Gott sprach!", "tokens": ["Und", "die", "Stim\u00b7me", "der", "Don\u00b7ner", "!", "als", "Gott", "von", "Gott", "sprach", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$.", "KOUS", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}}, "stanza.11": {"line.1": {"text": "Uneingeh\u00fcllt durch Nacht,", "tokens": ["Un\u00b7ein\u00b7ge\u00b7h\u00fcllt", "durch", "Nacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "In eines Tages Lichte,", "tokens": ["In", "ei\u00b7nes", "Ta\u00b7ges", "Lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das keine Schatten sichtbarer machen,", "tokens": ["Das", "kei\u00b7ne", "Schat\u00b7ten", "sicht\u00b7ba\u00b7rer", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Schauet er nun, so halten wirs, Jahrhunderte schon;", "tokens": ["Schau\u00b7et", "er", "nun", ",", "so", "hal\u00b7ten", "wirs", ",", "Jahr\u00b7hun\u00b7der\u00b7te", "schon", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "ADV", "VVFIN", "PIS", "$,", "NN", "ADV", "$."], "meter": "+--+-+---+--+", "measure": "iambic.penta.invert"}}, "stanza.12": {"line.1": {"text": "Ausser den Schranken der Zeit,", "tokens": ["Aus\u00b7ser", "den", "Schran\u00b7ken", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Ohn' Empfindung des Augenblicks,", "tokens": ["Ohn'", "Emp\u00b7fin\u00b7dung", "des", "Au\u00b7gen\u00b7blicks", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Dem der Augenblick folgt, schauet er nun", "tokens": ["Dem", "der", "Au\u00b7gen\u00b7blick", "folgt", ",", "schau\u00b7et", "er", "nun"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "ART", "NN", "VVFIN", "$,", "VVFIN", "PPER", "ADV"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Deine Herlichkeit, Heiliger! Heiliger! Heiliger!", "tokens": ["Dei\u00b7ne", "Her\u00b7lich\u00b7keit", ",", "Hei\u00b7li\u00b7ger", "!", "Hei\u00b7li\u00b7ger", "!", "Hei\u00b7li\u00b7ger", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$.", "NN", "$.", "NN", "$."], "meter": "+-+--+--+--+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.13": {"line.1": {"text": "Namloseste Wonne meiner Seele,", "tokens": ["Nam\u00b7lo\u00b7ses\u00b7te", "Won\u00b7ne", "mei\u00b7ner", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Gedanke des k\u00fcnftigen Schauns!", "tokens": ["Ge\u00b7dan\u00b7ke", "des", "k\u00fcnf\u00b7ti\u00b7gen", "Schauns", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Du bist meine grosse Zuversicht,", "tokens": ["Du", "bist", "mei\u00b7ne", "gros\u00b7se", "Zu\u00b7ver\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Du bist der Fels, auf dem ich steh, und gen Himmel schaue;", "tokens": ["Du", "bist", "der", "Fels", ",", "auf", "dem", "ich", "steh", ",", "und", "gen", "Him\u00b7mel", "schau\u00b7e", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,", "KON", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+---+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.14": {"line.1": {"text": "Wenn die Schrecken der S\u00fcnde,", "tokens": ["Wenn", "die", "Schre\u00b7cken", "der", "S\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Des Todes Schrecken", "tokens": ["Des", "To\u00b7des", "Schre\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "F\u00fcrchterlich drohn,", "tokens": ["F\u00fcrch\u00b7ter\u00b7lich", "drohn", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Mich niederzust\u00fcrzen!", "tokens": ["Mich", "nie\u00b7der\u00b7zu\u00b7st\u00fcr\u00b7zen", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.15": {"line.1": {"text": "Auf diesem Felsen, o du,", "tokens": ["Auf", "die\u00b7sem", "Fel\u00b7sen", ",", "o", "du", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "FM", "PPER", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Den nun die Todten Gottes schaun,", "tokens": ["Den", "nun", "die", "Tod\u00b7ten", "Got\u00b7tes", "schaun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Lass mich stehn, wenn die Allmacht", "tokens": ["Lass", "mich", "stehn", ",", "wenn", "die", "All\u00b7macht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "VVINF", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Des unbezwingbaren Todes mich ringsum einschliesst.", "tokens": ["Des", "un\u00b7be\u00b7zwing\u00b7ba\u00b7ren", "To\u00b7des", "mich", "ring\u00b7sum", "ein\u00b7schliesst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.16": {"line.1": {"text": "Erheb', o meine Seele, dich \u00fcber die Sterblichkeit,", "tokens": ["Er\u00b7heb'", ",", "o", "mei\u00b7ne", "See\u00b7le", ",", "dich", "\u00fc\u00b7ber", "die", "Sterb\u00b7lich\u00b7keit", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "FM", "PPOSAT", "NN", "$,", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Blick auf, und schau; und du wirst strahlenvoll", "tokens": ["Blick", "auf", ",", "und", "schau", ";", "und", "du", "wirst", "strah\u00b7len\u00b7voll"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$,", "KON", "PTKVZ", "$.", "KON", "PPER", "VAFIN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Des Vaters Klarheit.", "tokens": ["Des", "Va\u00b7ters", "Klar\u00b7heit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "In Jesus Christus Antlitz schaun!", "tokens": ["In", "Je\u00b7sus", "Chris\u00b7tus", "Ant\u00b7litz", "schaun", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "NE", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.17": {"line.1": {"text": "Hosianna! Hosianna! die F\u00fclle der Gottheit", "tokens": ["Ho\u00b7si\u00b7an\u00b7na", "!", "Ho\u00b7si\u00b7an\u00b7na", "!", "die", "F\u00fcl\u00b7le", "der", "Got\u00b7theit"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "NE", "$.", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Wohnt in dem Menschen Jesus Christus!", "tokens": ["Wohnt", "in", "dem", "Men\u00b7schen", "Je\u00b7sus", "Chris\u00b7tus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NE", "NE", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Kaum schallet der Cherubim Harfe noch, sie bebt!", "tokens": ["Kaum", "schal\u00b7let", "der", "Che\u00b7ru\u00b7bim", "Har\u00b7fe", "noch", ",", "sie", "bebt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NE", "NN", "ADV", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kaum t\u00f6net ihre Stimme noch, sie zittert, sie zittert!", "tokens": ["Kaum", "t\u00f6\u00b7net", "ih\u00b7re", "Stim\u00b7me", "noch", ",", "sie", "zit\u00b7tert", ",", "sie", "zit\u00b7tert", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADV", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.18": {"line.1": {"text": "Hosianna! Hosianna!", "tokens": ["Ho\u00b7si\u00b7an\u00b7na", "!", "Ho\u00b7si\u00b7an\u00b7na", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$.", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die F\u00fclle der Gottheit", "tokens": ["Die", "F\u00fcl\u00b7le", "der", "Got\u00b7theit"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wohnt in dem Menschen", "tokens": ["Wohnt", "in", "dem", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Jesus Christus!", "tokens": ["Je\u00b7sus", "Chris\u00b7tus", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.19": {"line.1": {"text": "Selbst damals, da einer der Gottesstrahlen auf unsere Welt,", "tokens": ["Selbst", "da\u00b7mals", ",", "da", "ei\u00b7ner", "der", "Got\u00b7tes\u00b7strah\u00b7len", "auf", "un\u00b7se\u00b7re", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PIS", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-+--+--+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Jene Blutweissagung heller leuchtet', erf\u00fcllt ward,", "tokens": ["Je\u00b7ne", "Blut\u00b7weis\u00b7sa\u00b7gung", "hel\u00b7ler", "leuch\u00b7tet'", ",", "er\u00b7f\u00fcllt", "ward", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJD", "VVFIN", "$,", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.3": {"text": "Da er verachtet, und elend war,", "tokens": ["Da", "er", "ver\u00b7ach\u00b7tet", ",", "und", "e\u00b7lend", "war", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "KON", "ADJD", "VAFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Als kein anderer Mensch verachtet, und elend war;", "tokens": ["Als", "kein", "an\u00b7de\u00b7rer", "Mensch", "ver\u00b7ach\u00b7tet", ",", "und", "e\u00b7lend", "war", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ADJA", "NN", "VVFIN", "$,", "KON", "ADJD", "VAFIN", "$."], "meter": "--+--+-+--+-+", "measure": "anapaest.di.plus"}}, "stanza.20": {"line.1": {"text": "Erblickten die Sterblichen nicht,", "tokens": ["Er\u00b7blick\u00b7ten", "die", "Sterb\u00b7li\u00b7chen", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKNEG", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Aber die Cherubim,", "tokens": ["A\u00b7ber", "die", "Che\u00b7ru\u00b7bim", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NE", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Des Vaters Klarheit", "tokens": ["Des", "Va\u00b7ters", "Klar\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "In dem Angesichte des Sohns!", "tokens": ["In", "dem", "An\u00b7ge\u00b7sich\u00b7te", "des", "Sohns", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.21": {"line.1": {"text": "Ich seh, ich sehe den Zeugen!", "tokens": ["Ich", "seh", ",", "ich", "se\u00b7he", "den", "Zeu\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sieben entsetzliche Mittern\u00e4chte", "tokens": ["Sie\u00b7ben", "ent\u00b7setz\u00b7li\u00b7che", "Mit\u00b7ter\u00b7n\u00e4ch\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["CARD", "ADJA", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Hatt' er gezweifelt' mit der Schmerzen b\u00e4ngsten", "tokens": ["Hatt'", "er", "ge\u00b7zwei\u00b7felt'", "mit", "der", "Schmer\u00b7zen", "b\u00e4ngs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "VVPP", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Anbetend gerungen!", "tokens": ["An\u00b7be\u00b7tend", "ge\u00b7run\u00b7gen", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.22": {"line.1": {"text": "Ich seh ihn!", "tokens": ["Ich", "seh", "ihn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Ihm erscheinet der Auferstandne!", "tokens": ["Ihm", "er\u00b7schei\u00b7net", "der", "Auf\u00b7er\u00b7stand\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Seine H\u00e4nde leget er in des G\u00f6ttlichen Wunden!", "tokens": ["Sei\u00b7ne", "H\u00e4n\u00b7de", "le\u00b7get", "er", "in", "des", "G\u00f6tt\u00b7li\u00b7chen", "Wun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Himmel und Erde vergehen um ihn!", "tokens": ["Him\u00b7mel", "und", "Er\u00b7de", "ver\u00b7ge\u00b7hen", "um", "ihn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "KOUI", "PPER", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.23": {"line.1": {"text": "Er sieht die Klarheit des Vaters im Angesichte des Sohns!", "tokens": ["Er", "sieht", "die", "Klar\u00b7heit", "des", "Va\u00b7ters", "im", "An\u00b7ge\u00b7sich\u00b7te", "des", "Sohns", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+--+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ich h\u00f6r', ich h\u00f6r' ihn! er ruft,", "tokens": ["Ich", "h\u00f6r'", ",", "ich", "h\u00f6r'", "ihn", "!", "er", "ruft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Himmel und Erde vergehen um ihn! er ruft:", "tokens": ["Him\u00b7mel", "und", "Er\u00b7de", "ver\u00b7ge\u00b7hen", "um", "ihn", "!", "er", "ruft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "KOUI", "PPER", "$.", "PPER", "VVFIN", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.4": {"text": "Mein Herr! und mein Gott!", "tokens": ["Mein", "Herr", "!", "und", "mein", "Gott", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "KON", "PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}