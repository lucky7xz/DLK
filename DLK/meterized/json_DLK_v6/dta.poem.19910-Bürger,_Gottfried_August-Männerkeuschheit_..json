{"dta.poem.19910": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "M\u00e4nnerkeuschheit .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1778", "urn": "urn:nbn:de:kobv:b4-20090519672", "language": ["de:0.99"], "booktitle": "B\u00fcrger, Gottfried August: Gedichte. G\u00f6ttingen, 1778."}, "poem": {"stanza.1": {"line.1": {"text": "Wer nie in schn\u00f6der Wollust Schoos               ", "tokens": ["Wer", "nie", "in", "schn\u00f6\u00b7der", "Wol\u00b7lust", "Schoos"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die F\u00fclle der Gesundheit gos,", "tokens": ["Die", "F\u00fcl\u00b7le", "der", "Ge\u00b7sund\u00b7heit", "gos", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den ziemt\u2019s, da\u00df er sich br\u00fcsten kan;", "tokens": ["Den", "ziemt's", ",", "da\u00df", "er", "sich", "br\u00fcs\u00b7ten", "kan", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "PRF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihn ziemt das Wort: Ich bin ein Man!", "tokens": ["Ihn", "ziemt", "das", "Wort", ":", "Ich", "bin", "ein", "Man", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "PPER", "VAFIN", "ART", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Denn er gedeiht und sprost empor,", "tokens": ["Denn", "er", "ge\u00b7deiht", "und", "sprost", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie auf der Wies\u2019 ein schlankes Rohr;", "tokens": ["Wie", "auf", "der", "Wies'", "ein", "schlan\u00b7kes", "Rohr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und lebt und webt, der Gottheit vol,", "tokens": ["Und", "lebt", "und", "webt", ",", "der", "Got\u00b7theit", "vol", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An Kraft und Sch\u00f6nheit ein Apol.", "tokens": ["An", "Kraft", "und", "Sch\u00f6n\u00b7heit", "ein", "A\u00b7pol", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Die G\u00f6tterkraft, die ihn durchfleust,", "tokens": ["Die", "G\u00f6t\u00b7ter\u00b7kraft", ",", "die", "ihn", "durch\u00b7fleust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Befl\u00fcgelt seinen Feuergeist,", "tokens": ["Be\u00b7fl\u00fc\u00b7gelt", "sei\u00b7nen", "Feu\u00b7er\u00b7geist", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und treibt, aus kalter D\u00e4mmerung,", "tokens": ["Und", "treibt", ",", "aus", "kal\u00b7ter", "D\u00e4m\u00b7me\u00b7rung", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Gen Himmel seinen Adlerschwung.", "tokens": ["Gen", "Him\u00b7mel", "sei\u00b7nen", "Ad\u00b7ler\u00b7schwung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Er badet sich im Sonnenmeer,", "tokens": ["Er", "ba\u00b7det", "sich", "im", "Son\u00b7nen\u00b7meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Klarheit str\u00f6met um ihn her.", "tokens": ["Und", "Klar\u00b7heit", "str\u00f6\u00b7met", "um", "ihn", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann wandelt sein verkl\u00e4rter Sin", "tokens": ["Dann", "wan\u00b7delt", "sein", "ver\u00b7kl\u00e4r\u00b7ter", "Sin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch alle Sch\u00f6pfung Gottes hin.", "tokens": ["Durch", "al\u00b7le", "Sch\u00f6p\u00b7fung", "Got\u00b7tes", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und er durchsp\u00e4ht, und w\u00e4gt, und mist,", "tokens": ["Und", "er", "durch\u00b7sp\u00e4ht", ",", "und", "w\u00e4gt", ",", "und", "mist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVPP", "$,", "KON", "VVFIN", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was in der Sch\u00f6pfung herlich ist,", "tokens": ["Was", "in", "der", "Sch\u00f6p\u00b7fung", "her\u00b7lich", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und stelt es dar in Red\u2019 und Sang,", "tokens": ["Und", "stelt", "es", "dar", "in", "Red'", "und", "Sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vol Harmonie, wie Himmelsklang.", "tokens": ["Vol", "Har\u00b7mo\u00b7nie", ",", "wie", "Him\u00b7mels\u00b7klang", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PWAV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "O schaut, wie er vol Majest\u00e4t,", "tokens": ["O", "schaut", ",", "wie", "er", "vol", "Ma\u00b7jes\u00b7t\u00e4t", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PWAV", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Gott, daher auf Erden geht!", "tokens": ["Ein", "Gott", ",", "da\u00b7her", "auf", "Er\u00b7den", "geht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PAV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er geht und steht in Herlichkeit,", "tokens": ["Er", "geht", "und", "steht", "in", "Her\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und steht um nichts; denn er gebeut.", "tokens": ["Und", "steht", "um", "nichts", ";", "denn", "er", "ge\u00b7beut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIS", "$.", "KON", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Sein Auge funkelt dunkelhell,", "tokens": ["Sein", "Au\u00b7ge", "fun\u00b7kelt", "dun\u00b7kel\u00b7hell", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie ein kristalner Schattenquell.", "tokens": ["Wie", "ein", "kris\u00b7tal\u00b7ner", "Schat\u00b7ten\u00b7quell", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sein Antliz stralt, wie Morgenrot;", "tokens": ["Sein", "Ant\u00b7liz", "stralt", ",", "wie", "Mor\u00b7gen\u00b7rot", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PWAV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf Nas\u2019 und Stirn herscht Machtgebot.", "tokens": ["Auf", "Nas'", "und", "Stirn", "herscht", "Macht\u00b7ge\u00b7bot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Das Machtgebot, das drauf regiert,", "tokens": ["Das", "Macht\u00b7ge\u00b7bot", ",", "das", "drauf", "re\u00b7giert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird hui! durch seinen Arm volf\u00fchrt.", "tokens": ["Wird", "hui", "!", "durch", "sei\u00b7nen", "Arm", "vol\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "$.", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn der schnelt aus, wie Federstal;", "tokens": ["Denn", "der", "schnelt", "aus", ",", "wie", "Fe\u00b7der\u00b7stal", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "PTKVZ", "$,", "PWAV", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Sein Schwerthieb ist ein Wetterstral.", "tokens": ["Sein", "Schwert\u00b7hieb", "ist", "ein", "Wet\u00b7ter\u00b7stral", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Das Ros f\u00fchlt seines Schenkels Macht,", "tokens": ["Das", "Ros", "f\u00fchlt", "sei\u00b7nes", "Schen\u00b7kels", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der nimmer wanket, nimmer kracht.", "tokens": ["Der", "nim\u00b7mer", "wan\u00b7ket", ",", "nim\u00b7mer", "kracht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er zw\u00e4ngt das Ros, vom Zwang\u2019 entw\u00f6nt,", "tokens": ["Er", "zw\u00e4ngt", "das", "Ros", ",", "vom", "Zwang'", "ent\u00b7w\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er zw\u00e4ngt das Ros, und horch! es st\u00f6hnt.", "tokens": ["Er", "zw\u00e4ngt", "das", "Ros", ",", "und", "horch", "!", "es", "st\u00f6hnt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KON", "VVIMP", "$.", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Er geht und steht in Herlichkeit,", "tokens": ["Er", "geht", "und", "steht", "in", "Her\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und fleht um nichts; denn er gebeut:", "tokens": ["Und", "fleht", "um", "nichts", ";", "denn", "er", "ge\u00b7beut", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIS", "$.", "KON", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und dennoch schaut, wo er sich zeigt,", "tokens": ["Und", "den\u00b7noch", "schaut", ",", "wo", "er", "sich", "zeigt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$,", "PWAV", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O schaut, wie ihm sich alles neigt!", "tokens": ["O", "schaut", ",", "wie", "ihm", "sich", "al\u00b7les", "neigt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PWAV", "PPER", "PRF", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Die edelsten der Jungfraun bl\u00fchn,", "tokens": ["Die", "e\u00b7dels\u00b7ten", "der", "Jung\u00b7fraun", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ART", "NN", "VVINF", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "Sie bl\u00fchn und duften nur f\u00fcr ihn.", "tokens": ["Sie", "bl\u00fchn", "und", "duf\u00b7ten", "nur", "f\u00fcr", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O Gl\u00fckliche, die er erkiest!", "tokens": ["O", "Gl\u00fck\u00b7li\u00b7che", ",", "die", "er", "er\u00b7kiest", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "O Selige, die sein geniest!", "tokens": ["O", "Se\u00b7li\u00b7ge", ",", "die", "sein", "ge\u00b7niest", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Die F\u00fclle seines Lebens gl\u00e4nzt,", "tokens": ["Die", "F\u00fcl\u00b7le", "sei\u00b7nes", "Le\u00b7bens", "gl\u00e4nzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Wein, von Rosen rund umkr\u00e4nzt.", "tokens": ["Wie", "Wein", ",", "von", "Ro\u00b7sen", "rund", "um\u00b7kr\u00e4nzt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "APPR", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sein gl\u00fcklich Weib, an seiner Brust,", "tokens": ["Sein", "gl\u00fck\u00b7lich", "Weib", ",", "an", "sei\u00b7ner", "Brust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Berauscht sich draus zu Lieb\u2019 und Lust.", "tokens": ["Be\u00b7rauscht", "sich", "draus", "zu", "Lieb'", "und", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PAV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Frohlockend blikt sie rund umher:", "tokens": ["Froh\u00b7lo\u00b7ckend", "blikt", "sie", "rund", "um\u00b7her", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201ewo sind der M\u00e4nner mehr, wie Er?\u201e", "tokens": ["\u201e", "wo", "sind", "der", "M\u00e4n\u00b7ner", "mehr", ",", "wie", "Er", "?", "\u201e"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "VAFIN", "ART", "NN", "ADV", "$,", "PWAV", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Fleuch, Z\u00e4rtling, fleuch! Sie spottet dein.", "tokens": ["Fleuch", ",", "Z\u00e4rt\u00b7ling", ",", "fleuch", "!", "Sie", "spot\u00b7tet", "dein", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "$.", "PPER", "VVFIN", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur Er nimt Bett\u2019 und Busen ein.", "tokens": ["Nur", "Er", "nimt", "Bett'", "und", "Bu\u00b7sen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER