{"textgrid.poem.64238": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das war ein Wort aus K\u00f6nigsmunde!", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das war ein Wort aus K\u00f6nigsmunde!", "tokens": ["Das", "war", "ein", "Wort", "aus", "K\u00f6\u00b7nigs\u00b7mun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das war ein Wort aus deutschem Geist!", "tokens": ["Das", "war", "ein", "Wort", "aus", "deut\u00b7schem", "Geist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aus Bayerland die hohe Kunde,", "tokens": ["Aus", "Bay\u00b7er\u00b7land", "die", "ho\u00b7he", "Kun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie sie ganz Deutschland mit sich rei\u00dft!", "tokens": ["Wie", "sie", "ganz", "Deutschland", "mit", "sich", "rei\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "NE", "APPR", "PRF", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbschweig,\u00ab sprach er, \u00bbdu Gezisch der Pfaffen!", "tokens": ["\u00bb", "schweig", ",", "\u00ab", "sprach", "er", ",", "\u00bb", "du", "Ge\u00b7zisch", "der", "Pfaf\u00b7fen", "!"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "$(", "VVFIN", "PPER", "$,", "$(", "PPER", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verstumme, welsch Verf\u00fchrerwort,", "tokens": ["Ver\u00b7stum\u00b7me", ",", "welsch", "Ver\u00b7f\u00fch\u00b7rer\u00b7wort", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PWAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf, meine Bayern! Zu den Waffen!", "tokens": ["Auf", ",", "mei\u00b7ne", "Bay\u00b7ern", "!", "Zu", "den", "Waf\u00b7fen", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "PPOSAT", "NN", "$.", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Rhein! Wir sind die Ersten dort!", "tokens": ["Zum", "Rhein", "!", "Wir", "sind", "die", "Ers\u00b7ten", "dort", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NE", "$.", "PPER", "VAFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Was alter Zwist und Wahn ges\u00fcndet,", "tokens": ["Was", "al\u00b7ter", "Zwist", "und", "Wahn", "ge\u00b7s\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verflogen sei's, wie Wind und Spreu:", "tokens": ["Ver\u00b7flo\u00b7gen", "sei's", ",", "wie", "Wind", "und", "Spreu", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PWAV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Schwertschlag sei's dem Feind verk\u00fcndet:", "tokens": ["Mit", "Schwert\u00b7schlag", "sei's", "dem", "Feind", "ver\u00b7k\u00fcn\u00b7det", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Echt ist wie Stahl die Bayerntreu'.\u00ab", "tokens": ["Echt", "ist", "wie", "Stahl", "die", "Bay\u00b7ern\u00b7treu'", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VAFIN", "KOKOM", "NN", "ART", "NE", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Das wird dir Deutschland ewig danken,", "tokens": ["Das", "wird", "dir", "Deutschland", "e\u00b7wig", "dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "NE", "ADJD", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da\u00df gro\u00df dich fand die gro\u00dfe Zeit:", "tokens": ["Da\u00df", "gro\u00df", "dich", "fand", "die", "gro\u00b7\u00dfe", "Zeit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Um deine Schl\u00e4fe seh' ich ranken", "tokens": ["Um", "dei\u00b7ne", "Schl\u00e4\u00b7fe", "seh'", "ich", "ran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Lorbeer der Unsterblichkeit.", "tokens": ["Den", "Lor\u00b7beer", "der", "U\u00b7nsterb\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "An eigner L\u00fcge wird zunichte", "tokens": ["An", "eig\u00b7ner", "L\u00fc\u00b7ge", "wird", "zu\u00b7nich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Lob, Ruhm und Titel, die nicht wahr:", "tokens": ["Lob", ",", "Ruhm", "und", "Ti\u00b7tel", ",", "die", "nicht", "wahr", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,", "PRELS", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dich aber nennt die Weltgeschichte", "tokens": ["Dich", "a\u00b7ber", "nennt", "die", "Welt\u00b7ge\u00b7schich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ludwig den Deutschen immerdar.", "tokens": ["Lud\u00b7wig", "den", "Deut\u00b7schen", "im\u00b7mer\u00b7dar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADV", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}