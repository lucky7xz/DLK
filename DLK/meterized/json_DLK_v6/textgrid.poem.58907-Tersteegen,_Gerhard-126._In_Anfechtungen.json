{"textgrid.poem.58907": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "126. In Anfechtungen", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn dich des Feindes List anficht,", "tokens": ["Wenn", "dich", "des", "Fein\u00b7des", "List", "an\u00b7ficht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So gib nicht acht auf seine R\u00e4nke,", "tokens": ["So", "gib", "nicht", "acht", "auf", "sei\u00b7ne", "R\u00e4n\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PTKNEG", "CARD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bleib ungest\u00f6rt und forsche nicht,", "tokens": ["Bleib", "un\u00b7ge\u00b7st\u00f6rt", "und", "for\u00b7sche", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Getrost dich nur in Jesus senke!", "tokens": ["Ge\u00b7trost", "dich", "nur", "in", "Je\u00b7sus", "sen\u00b7ke", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}