{"dta.poem.21163": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "An Herrn E - - .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Freund, Freund! die Jahre fliehn hin, so wie ein\nst\u00e4ubender Bach,", "tokens": ["Freund", ",", "Freund", "!", "die", "Jah\u00b7re", "fliehn", "hin", ",", "so", "wie", "ein", "st\u00e4u\u00b7ben\u00b7der", "Bach", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$.", "ART", "NN", "VVFIN", "PTKVZ", "$,", "ADV", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der von dem steilen Felsen fliegt;", "tokens": ["Der", "von", "dem", "stei\u00b7len", "Fel\u00b7sen", "fliegt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wie ein fliehender West, wenn er dem bl\u00fchenden", "tokens": ["Und", "wie", "ein", "flie\u00b7hen\u00b7der", "West", ",", "wenn", "er", "dem", "bl\u00fc\u00b7hen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "$,", "KOUS", "PPER", "ART", "ADJA"], "meter": "-+-+--+-+-+--", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "In schneller Flucht die Spitzen beugt.", "tokens": ["In", "schnel\u00b7ler", "Flucht", "die", "Spit\u00b7zen", "beugt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Meynst du, sie kommen zur\u00fcck, wenn sie uns einmal", "tokens": ["Meynst", "du", ",", "sie", "kom\u00b7men", "zu\u00b7r\u00fcck", ",", "wenn", "sie", "uns", "ein\u00b7mal"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "$,", "PPER", "VVFIN", "PTKVZ", "$,", "KOUS", "PPER", "PPER", "ADV"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nein, Freund, auf ewig sind sie hin.", "tokens": ["Nein", ",", "Freund", ",", "auf", "e\u00b7wig", "sind", "sie", "hin", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "$,", "APPR", "ADJD", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht W\u00fcnsche halten sie auf, und keine Leyer singt sie", "tokens": ["Nicht", "W\u00fcn\u00b7sche", "hal\u00b7ten", "sie", "auf", ",", "und", "kei\u00b7ne", "Le\u00b7yer", "singt", "sie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "PIAT", "NN", "VVFIN", "PPER"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Aus der Vergessenheit zur\u00fcck.", "tokens": ["Aus", "der", "Ver\u00b7ges\u00b7sen\u00b7heit", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und dennoch liebst du noch nicht? O Freund, besch\u00e4ft-", "tokens": ["Und", "den\u00b7noch", "liebst", "du", "noch", "nicht", "?", "O", "Freund", ",", "be\u00b7sch\u00e4ft"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "$.", "NE", "NN", "$,", "TRUNC"], "meter": "+---+-+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Da es noch zart und f\u00fchlend ist;", "tokens": ["Da", "es", "noch", "zart", "und", "f\u00fch\u00b7lend", "ist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "KON", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Eh unbarmherzig die Hand des Alters \u00fcber dich f\u00e4hrt,", "tokens": ["Eh", "un\u00b7barm\u00b7her\u00b7zig", "die", "Hand", "des", "Al\u00b7ters", "\u00fc\u00b7ber", "dich", "f\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Und Runzeln auf die Stirne kr\u00fcmmt.", "tokens": ["Und", "Run\u00b7zeln", "auf", "die", "Stir\u00b7ne", "kr\u00fcmmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Himmel schuf nicht umsonst dein leichtempfinden-", "tokens": ["Der", "Him\u00b7mel", "schuf", "nicht", "um\u00b7sonst", "dein", "leicht\u00b7emp\u00b7fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADV", "PPOSAT", "TRUNC"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Es mu\u00df doch wo ein M\u00e4dchen seyn,", "tokens": ["Es", "mu\u00df", "doch", "wo", "ein", "M\u00e4d\u00b7chen", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PWAV", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das auf den J\u00fcngling noch hoft, dem sie die Seufzer", "tokens": ["Das", "auf", "den", "J\u00fcng\u00b7ling", "noch", "hoft", ",", "dem", "sie", "die", "Seuf\u00b7zer"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "NN", "ADV", "VVFIN", "$,", "PRELS", "PPER", "ART", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und dem ihr loses Auge lacht.", "tokens": ["Und", "dem", "ihr", "lo\u00b7ses", "Au\u00b7ge", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Sie geht mit irrendem Schritt im \u00f6den Garten her-", "tokens": ["Sie", "geht", "mit", "ir\u00b7ren\u00b7dem", "Schritt", "im", "\u00f6\u00b7den", "Gar\u00b7ten", "her"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "APPRART", "ADJA", "NN", "TRUNC"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und windet einen Blumenstrau\u00df,", "tokens": ["Und", "win\u00b7det", "ei\u00b7nen", "Blu\u00b7men\u00b7strau\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sieht ihn sehnsuchtsvoll an; Die Thr\u00e4ne zittert", "tokens": ["Und", "sieht", "ihn", "sehn\u00b7suchts\u00b7voll", "an", ";", "Die", "Thr\u00e4\u00b7ne", "zit\u00b7tert"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Da\u00df sie ihn keinem schenken kan.", "tokens": ["Da\u00df", "sie", "ihn", "kei\u00b7nem", "schen\u00b7ken", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "O E \u2014, suche sie doch, damit das M\u00e4dchen nicht", "tokens": ["O", "E", ",", "su\u00b7che", "sie", "doch", ",", "da\u00b7mit", "das", "M\u00e4d\u00b7chen", "nicht"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$(", "$,", "VVFIN", "PPER", "ADV", "$,", "KOUS", "ART", "NN", "PTKNEG"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Da\u00df ihre sch\u00f6nen Tage fliehn!", "tokens": ["Da\u00df", "ih\u00b7re", "sch\u00f6\u00b7nen", "Ta\u00b7ge", "fliehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du bist ein Mensch, ein Poet. Gedoppelt ist dein", "tokens": ["Du", "bist", "ein", "Mensch", ",", "ein", "Po\u00b7et", ".", "Ge\u00b7dop\u00b7pelt", "ist", "dein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ART", "NN", "$.", "VVPP", "VAFIN", "PPOSAT"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Zu lieben, eh dein Lenz verstreicht.", "tokens": ["Zu", "lie\u00b7ben", ",", "eh", "dein", "Lenz", "ver\u00b7streicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "KOUS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}