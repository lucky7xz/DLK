{"textgrid.poem.35205": {"metadata": {"author": {"name": "Harsd\u00f6rffer, Georg Philipp", "birth": "N.A.", "death": "N.A."}, "title": "[sie zwar/ nach verrauschter Bach ihres Lebens/ sind gestorben]", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schaut die Heldenh\u00f6le hier/ wo der Helden Thaten lachen/", "tokens": ["Schaut", "die", "Hel\u00b7den\u00b7h\u00f6\u00b7le", "hier", "/", "wo", "der", "Hel\u00b7den", "Tha\u00b7ten", "la\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "$(", "PWAV", "ART", "NN", "NN", "VVINF", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Dapfrer Held\u0113/ die zwar sonst dekkt der d\u00fcstre Todes-Rach\u0113.", "tokens": ["Dapf\u00b7rer", "Held\u0113", "/", "die", "zwar", "sonst", "de\u00b7kkt", "der", "d\u00fcst\u00b7re", "To\u00b7des\u00b7Rach\u0113", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$(", "PRELS", "ADV", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Sie zwar/ nach verrauschter Bach ihres Lebens/ sind gestorb\u0113/", "tokens": ["Sie", "zwar", "/", "nach", "ver\u00b7rauschter", "Bach", "ih\u00b7res", "Le\u00b7bens", "/", "sind", "ge\u00b7stor\u00b7b\u0113", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "$(", "APPR", "ADJA", "NN", "PPOSAT", "NN", "$(", "VAFIN", "VVPP", "$("], "meter": "-+--++--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Doch ist ihres Namens Ruhm nicht in Lethes Pful", "tokens": ["Doch", "ist", "ih\u00b7res", "Na\u00b7mens", "Ruhm", "nicht", "in", "Le\u00b7thes", "Pful"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "NN", "PTKNEG", "APPR", "ADJA", "NN"], "meter": "-+-----+-+-+", "measure": "dactylic.init"}, "line.5": {"text": "Wo n\u00e4chst Klugheit Tugend ficht/ und den Muhte Glut bestrahlet/", "tokens": ["Wo", "n\u00e4chst", "Klug\u00b7heit", "Tu\u00b7gend", "ficht", "/", "und", "den", "Muh\u00b7te", "Glut", "be\u00b7strah\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "NN", "VVFIN", "$(", "KON", "ART", "NN", "NN", "VVFIN", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Siegt dem Sterben an die Faust/ Dapferkeit das Grabmahl pfahlet.", "tokens": ["Siegt", "dem", "Ster\u00b7ben", "an", "die", "Faust", "/", "Dap\u00b7fer\u00b7keit", "das", "Grab\u00b7mahl", "pfah\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$(", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-++-+-+-+-", "measure": "unknown.measure.octa.plus"}, "line.7": {"text": "Du/ der du in gleich\u0113 Thun bist mit blanker Hand bem\u00fchet/", "tokens": ["Du", "/", "der", "du", "in", "gleich\u0113", "Thun", "bist", "mit", "blan\u00b7ker", "Hand", "be\u00b7m\u00fc\u00b7het", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "PRELS", "PPER", "APPR", "ADJA", "NN", "VAFIN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "--+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Folge Diesen/ wann du wilt stehen/ wo Orion kniehet.", "tokens": ["Fol\u00b7ge", "Die\u00b7sen", "/", "wann", "du", "wilt", "ste\u00b7hen", "/", "wo", "O\u00b7rion", "knie\u00b7het", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "$(", "PWAV", "PPER", "VMFIN", "VVINF", "$(", "PWAV", "NN", "VVFIN", "$."], "meter": "+-+--+-+--+-+-", "measure": "trochaic.hexa.relaxed"}}}}}