{"dta.poem.8215": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "M\u00e4rchen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Beklagt euch nicht, wenn ich zu wenig sah!", "tokens": ["Be\u00b7klagt", "euch", "nicht", ",", "wenn", "ich", "zu", "we\u00b7nig", "sah", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "PPER", "PTKA", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nur Sommernachts passiren die Geschichten;", "tokens": ["Nur", "Som\u00b7mer\u00b7nachts", "pas\u00b7si\u00b7ren", "die", "Ge\u00b7schich\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Kaum graut die Nacht, so r\u00fcckt der Morgen nah,", "tokens": ["Kaum", "graut", "die", "Nacht", ",", "so", "r\u00fcckt", "der", "Mor\u00b7gen", "nah", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Kaum da\u00df den Wald die ersten Strahlen lichten,", "tokens": ["Kaum", "da\u00df", "den", "Wald", "die", "ers\u00b7ten", "Strah\u00b7len", "lich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "ART", "ADJA", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Entflieht mit ihrem Hof Titania;", "tokens": ["Ent\u00b7flieht", "mit", "ih\u00b7rem", "Hof", "Ti\u00b7ta\u00b7nia", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-++-+", "measure": "unknown.measure.penta"}, "line.6": {"text": "Auf Weg und Steg spaziren die Philister,", "tokens": ["Auf", "Weg", "und", "Steg", "spa\u00b7zi\u00b7ren", "die", "Phi\u00b7lis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Das wohlbekannte leidige Register.", "tokens": ["Das", "wohl\u00b7be\u00b7kann\u00b7te", "lei\u00b7di\u00b7ge", "Re\u00b7gis\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Kein Zauber w\u00e4chst f\u00fcr fromme B\u00fcrgersleute,", "tokens": ["Kein", "Zau\u00b7ber", "w\u00e4chst", "f\u00fcr", "from\u00b7me", "B\u00fcr\u00b7gers\u00b7leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Tags nur wissen, wie die Glocke geht,", "tokens": ["Die", "Tags", "nur", "wis\u00b7sen", ",", "wie", "die", "Glo\u00b7cke", "geht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die gr\u00fcndlich kennen gestern, morgen, heute,", "tokens": ["Die", "gr\u00fcnd\u00b7lich", "ken\u00b7nen", "ge\u00b7stern", ",", "mor\u00b7gen", ",", "heu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "ADJD", "VVINF", "VVINF", "$,", "ADV", "$,", "ADV", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Doch nicht die Zeit, die mitten drinn' besteht;", "tokens": ["Doch", "nicht", "die", "Zeit", ",", "die", "mit\u00b7ten", "drinn'", "be\u00b7steht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "$,", "PRELS", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich aber h\u00f6rte wohl das Waldgel\u00e4ute,", "tokens": ["Ich", "a\u00b7ber", "h\u00f6r\u00b7te", "wohl", "das", "Wald\u00b7ge\u00b7l\u00e4u\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein Sonntagskind ist immer der Poet;", "tokens": ["Ein", "Sonn\u00b7tags\u00b7kind", "ist", "im\u00b7mer", "der", "Po\u00b7et", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So la\u00dft euch denn in blanken Liederringen", "tokens": ["So", "la\u00dft", "euch", "denn", "in", "blan\u00b7ken", "Lie\u00b7der\u00b7rin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Von Reim zu Reim in's Land der M\u00e4rchen schwingen.", "tokens": ["Von", "Reim", "zu", "Reim", "in's", "Land", "der", "M\u00e4r\u00b7chen", "schwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}