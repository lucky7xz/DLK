{"textgrid.poem.47127": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich sah das Paradies mir offen,", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sah das Paradies mir offen,", "tokens": ["Ich", "sah", "das", "Pa\u00b7ra\u00b7dies", "mir", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch nur im Traume;", "tokens": ["Doch", "nur", "im", "Trau\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Denn wachend ist das nicht zu hoffen", "tokens": ["Denn", "wa\u00b7chend", "ist", "das", "nicht", "zu", "hof\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVPP", "VAFIN", "PDS", "PTKNEG", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Erdenraume.", "tokens": ["Im", "Er\u00b7den\u00b7rau\u00b7me", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Das Paradies wird nicht erworben,", "tokens": ["Das", "Pa\u00b7ra\u00b7dies", "wird", "nicht", "er\u00b7wor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Eh' man gestorben.", "tokens": ["Eh'", "man", "ge\u00b7stor\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "O Herz, wenn du es willst erwerben,", "tokens": ["O", "Herz", ",", "wenn", "du", "es", "willst", "er\u00b7wer\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "KOUS", "PPER", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So la\u00df uns sterben.", "tokens": ["So", "la\u00df", "uns", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}