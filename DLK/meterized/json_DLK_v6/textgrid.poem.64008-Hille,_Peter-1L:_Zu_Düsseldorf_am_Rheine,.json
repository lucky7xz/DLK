{"textgrid.poem.64008": {"metadata": {"author": {"name": "Hille, Peter", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zu D\u00fcsseldorf am Rheine,", "genre": "verse", "period": "N.A.", "pub_year": 1879, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu D\u00fcsseldorf am Rheine,", "tokens": ["Zu", "D\u00fcs\u00b7sel\u00b7dorf", "am", "Rhei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPRART", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da musiziert ein Haus,", "tokens": ["Da", "mu\u00b7si\u00b7ziert", "ein", "Haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie wirft es seine Scheine", "tokens": ["Wie", "wirft", "es", "sei\u00b7ne", "Schei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So sp\u00e4t und ganz alleine", "tokens": ["So", "sp\u00e4t", "und", "ganz", "al\u00b7lei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Hin \u00fcber weiche Fluten", "tokens": ["Hin", "\u00fc\u00b7ber", "wei\u00b7che", "Flu\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und in die Nacht hinaus.", "tokens": ["Und", "in", "die", "Nacht", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und in dem alten Hause", "tokens": ["Und", "in", "dem", "al\u00b7ten", "Hau\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Trio findest du,", "tokens": ["Ein", "Trio", "fin\u00b7dest", "du", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Trepphoch die Bauernklause:", "tokens": ["Trepp\u00b7hoch", "die", "Bau\u00b7ern\u00b7klau\u00b7se", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Auge bohrt das grause \u2013", "tokens": ["Das", "Au\u00b7ge", "bohrt", "das", "grau\u00b7se", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das ist allein das Eine \u2013", "tokens": ["Das", "ist", "al\u00b7lein", "das", "Ei\u00b7ne", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ART", "ART", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Geig' geht immerzu.", "tokens": ["Die", "Geig'", "geht", "im\u00b7mer\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ein jammerstumm Gequ\u00e4le,", "tokens": ["Ein", "jam\u00b7mer\u00b7stumm", "Ge\u00b7qu\u00e4\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von allen Lastern krank", "tokens": ["Von", "al\u00b7len", "Las\u00b7tern", "krank"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hintastend Blickgeschwehle", "tokens": ["Hin\u00b7tas\u00b7tend", "Blick\u00b7ge\u00b7schweh\u00b7le"], "token_info": ["word", "word"], "pos": ["VVPP", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein \u00c4chzen in der Seele \u2013", "tokens": ["Ein", "\u00c4ch\u00b7zen", "in", "der", "See\u00b7le", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Gesund nur ist die Fiedel,", "tokens": ["Ge\u00b7sund", "nur", "ist", "die", "Fie\u00b7del", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und H\u00f6lle schl\u00fcrft den Trank.", "tokens": ["Und", "H\u00f6l\u00b7le", "schl\u00fcrft", "den", "Trank", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ein Barde da der zweite,", "tokens": ["Ein", "Bar\u00b7de", "da", "der", "zwei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Feder am Barett,", "tokens": ["Die", "Fe\u00b7der", "am", "Ba\u00b7rett", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Tritt hin zu seiner Seite,", "tokens": ["Tritt", "hin", "zu", "sei\u00b7ner", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sein Wams spannt in die Weite:", "tokens": ["Sein", "Wams", "spannt", "in", "die", "Wei\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein deutscher Strom sein Singen,", "tokens": ["Ein", "deut\u00b7scher", "Strom", "sein", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein Strom nur etwas fett.", "tokens": ["Ein", "Strom", "nur", "et\u00b7was", "fett", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sonst recht ein Minnes\u00e4nger", "tokens": ["Sonst", "recht", "ein", "Min\u00b7ne\u00b7s\u00e4n\u00b7ger"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus bunter Ritterzeit,", "tokens": ["Aus", "bun\u00b7ter", "Rit\u00b7ter\u00b7zeit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So recht ein Herzbedr\u00e4nger,", "tokens": ["So", "recht", "ein", "Herz\u00b7be\u00b7dr\u00e4n\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein G\u00fcldendankempf\u00e4nger", "tokens": ["Ein", "G\u00fcl\u00b7den\u00b7dan\u00b7kemp\u00b7f\u00e4n\u00b7ger"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "In blauen Lockenpr\u00e4chten \u2013", "tokens": ["In", "blau\u00b7en", "Lo\u00b7cken\u00b7pr\u00e4ch\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So frank, so frei, so weit.", "tokens": ["So", "frank", ",", "so", "frei", ",", "so", "weit", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Des Sinnes frohe Freite", "tokens": ["Des", "Sin\u00b7nes", "fro\u00b7he", "Frei\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das blaue Auge warm,", "tokens": ["Das", "blau\u00b7e", "Au\u00b7ge", "warm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ist ein Hochgeschreite,", "tokens": ["Und", "ist", "ein", "Hoch\u00b7ge\u00b7schrei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Viel k\u00fchne Nackenbreite,", "tokens": ["Viel", "k\u00fch\u00b7ne", "Na\u00b7cken\u00b7brei\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Glieder Mannesbl\u00fcte,", "tokens": ["Die", "Glie\u00b7der", "Man\u00b7nes\u00b7bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Leicht, gut und ohne Harm.", "tokens": ["Leicht", ",", "gut", "und", "oh\u00b7ne", "Harm", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "KON", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und neben M\u00e4hrens Sohne", "tokens": ["Und", "ne\u00b7ben", "M\u00e4h\u00b7rens", "Soh\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "NE", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Am kleinen Tisch zu dritt,", "tokens": ["Am", "klei\u00b7nen", "Tisch", "zu", "dritt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKZU", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der tr\u00e4gt die B\u00fcrgerkrone,", "tokens": ["Der", "tr\u00e4gt", "die", "B\u00fcr\u00b7ger\u00b7kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von Leichtsinn keine Bohne,", "tokens": ["Von", "Leicht\u00b7sinn", "kei\u00b7ne", "Boh\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der pustet Klarinette,", "tokens": ["Der", "pus\u00b7tet", "Kla\u00b7ri\u00b7net\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Trinkt dann gemessen mit.", "tokens": ["Trinkt", "dann", "ge\u00b7mes\u00b7sen", "mit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VVPP", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.8": {"line.1": {"text": "Schwarz Buckel mit Manschetten", "tokens": ["Schwarz", "Bu\u00b7ckel", "mit", "Man\u00b7schet\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Setzt zu den G\u00e4sten sich,", "tokens": ["Setzt", "zu", "den", "G\u00e4s\u00b7ten", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PRF", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Goldk\u00f6pfig hochadretten,", "tokens": ["Gold\u00b7k\u00f6p\u00b7fig", "hoc\u00b7had\u00b7ret\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In Themis Wagenwetten,", "tokens": ["In", "The\u00b7mis", "Wa\u00b7gen\u00b7wet\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Als Advokat verschlagen,", "tokens": ["Als", "Ad\u00b7vo\u00b7kat", "ver\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hochausbesitzerlich.", "tokens": ["Hoch\u00b7aus\u00b7be\u00b7sit\u00b7zer\u00b7lich", "."], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Agrarierz\u00e4hren flossen", "tokens": ["Ag\u00b7ra\u00b7rier\u00b7z\u00e4h\u00b7ren", "flos\u00b7sen"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als wie ein goldner Bach,", "tokens": ["Als", "wie", "ein", "gold\u00b7ner", "Bach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Noch eilig hingegossen,", "tokens": ["Noch", "ei\u00b7lig", "hin\u00b7ge\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Um zweie wird geschlossen,", "tokens": ["Um", "zwei\u00b7e", "wird", "ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Kellner gehn und r\u00e4umen", "tokens": ["Die", "Kell\u00b7ner", "gehn", "und", "r\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "KON", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Man f\u00e4hrt aus j\u00e4hen Tr\u00e4umen \u2013", "tokens": ["Man", "f\u00e4hrt", "aus", "j\u00e4\u00b7hen", "Tr\u00e4u\u00b7men", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Jach empor.", "tokens": ["Jach", "em\u00b7por", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}