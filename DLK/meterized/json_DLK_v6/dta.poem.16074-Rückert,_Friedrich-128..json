{"dta.poem.16074": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "128.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Jugend war mir tr\u00fcb umw\u00f6lkt durch meine Schuld,", "tokens": ["Die", "Ju\u00b7gend", "war", "mir", "tr\u00fcb", "um\u00b7w\u00f6lkt", "durch", "mei\u00b7ne", "Schuld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und da\u00df mein Alter nun hell ward, ist Gottes Huld.", "tokens": ["Und", "da\u00df", "mein", "Al\u00b7ter", "nun", "hell", "ward", ",", "ist", "Got\u00b7tes", "Huld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "ADV", "ADJD", "VAFIN", "$,", "VAFIN", "NN", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Wie d\u00fcrft' ich gegen dich mit meinen Gaben prahlen?", "tokens": ["Wie", "d\u00fcrft'", "ich", "ge\u00b7gen", "dich", "mit", "mei\u00b7nen", "Ga\u00b7ben", "prah\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "APPR", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nie kann ich meine Schuld, nie deine Huld bezahlen.", "tokens": ["Nie", "kann", "ich", "mei\u00b7ne", "Schuld", ",", "nie", "dei\u00b7ne", "Huld", "be\u00b7zah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["A