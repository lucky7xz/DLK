{"dta.poem.4090": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Thr\u00e4nenregen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Im k\u00fchlen Erlendach,", "tokens": ["Im", "k\u00fch\u00b7len", "Er\u00b7len\u00b7dach", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wir schauten so traulich zusammen", "tokens": ["Wir", "schau\u00b7ten", "so", "trau\u00b7lich", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Hinab in den rieselnden Bach.", "tokens": ["Hin\u00b7ab", "in", "den", "rie\u00b7seln\u00b7den", "Bach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Der Mond war auch gekommen,", "tokens": ["Der", "Mond", "war", "auch", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Sternlein hinterdrein,", "tokens": ["Die", "Stern\u00b7lein", "hin\u00b7ter\u00b7drein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und schauten so traulich zusammen", "tokens": ["Und", "schau\u00b7ten", "so", "trau\u00b7lich", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "In den silbernen Spiegel hinein.", "tokens": ["In", "den", "sil\u00b7ber\u00b7nen", "Spie\u00b7gel", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.3": {"line.1": {"text": "Ich sah nach keinem Monde,", "tokens": ["Ich", "sah", "nach", "kei\u00b7nem", "Mon\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nach keinem Sternenschein,", "tokens": ["Nach", "kei\u00b7nem", "Ster\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich schaute nach ihrem Bilde,", "tokens": ["Ich", "schau\u00b7te", "nach", "ih\u00b7rem", "Bil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Nach ihren Augen allein.", "tokens": ["Nach", "ih\u00b7ren", "Au\u00b7gen", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Und sahe sie nicken und blicken", "tokens": ["Und", "sa\u00b7he", "sie", "ni\u00b7cken", "und", "bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "VVINF", "KON", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Herauf aus dem seligen Bach,", "tokens": ["Her\u00b7auf", "aus", "dem", "se\u00b7li\u00b7gen", "Bach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Die Bl\u00fcmlein am Ufer, die blauen,", "tokens": ["Die", "Bl\u00fcm\u00b7lein", "am", "U\u00b7fer", ",", "die", "blau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Sie nickten und blickten ihr nach.", "tokens": ["Sie", "nick\u00b7ten", "und", "blick\u00b7ten", "ihr", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Und in den Bach versunken", "tokens": ["Und", "in", "den", "Bach", "ver\u00b7sun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der ganze Himmel schien,", "tokens": ["Der", "gan\u00b7ze", "Him\u00b7mel", "schien", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wollte mich mit hinunter", "tokens": ["Und", "woll\u00b7te", "mich", "mit", "hin\u00b7un\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PRF", "APPR", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "In seine Tiefe ziehn.", "tokens": ["In", "sei\u00b7ne", "Tie\u00b7fe", "ziehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und \u00fcber den Wolken und Sternen", "tokens": ["Und", "\u00fc\u00b7ber", "den", "Wol\u00b7ken", "und", "Ster\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Da rieselte munter der Bach,", "tokens": ["Da", "rie\u00b7sel\u00b7te", "mun\u00b7ter", "der", "Bach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und rief mit Singen und Klingen:", "tokens": ["Und", "rief", "mit", "Sin\u00b7gen", "und", "Klin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Geselle, Geselle, mir nach.", "tokens": ["Ge\u00b7sel\u00b7le", ",", "Ge\u00b7sel\u00b7le", ",", "mir", "nach", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PPER", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.7": {"line.1": {"text": "Da gingen die Augen mir \u00fcber,", "tokens": ["Da", "gin\u00b7gen", "die", "Au\u00b7gen", "mir", "\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "APPR", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Da ward es im Spiegel so kraus:", "tokens": ["Da", "ward", "es", "im", "Spie\u00b7gel", "so", "kraus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPRART", "NN", "ADV", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Sie sprach: Es kommt ein Regen,", "tokens": ["Sie", "sprach", ":", "Es", "kommt", "ein", "Re\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ade, ich geh' nach Haus.", "tokens": ["A\u00b7de", ",", "ich", "geh'", "nach", "Haus", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}