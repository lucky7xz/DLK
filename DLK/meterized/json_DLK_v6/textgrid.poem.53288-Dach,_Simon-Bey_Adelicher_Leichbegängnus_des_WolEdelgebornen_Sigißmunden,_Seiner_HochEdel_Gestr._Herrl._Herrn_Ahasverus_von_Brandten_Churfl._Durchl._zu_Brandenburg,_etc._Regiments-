{"textgrid.poem.53288": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Bey Adelicher Leichbeg\u00e4ngnus des WolEdelgebornen Sigi\u00dfmunden, Seiner HochEdel Gestr. Herrl. Herrn Ahasverus von Brandten Churfl. Durchl. zu Brandenburg, etc. Regiments-Rahts vnd OberMarschallen hertzgeliebten S\u00f6hnleins, welches im Herrn eingeschlaffen den 27. Aprilis 1641. im 6. Jahr seines Alters", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df sterben, was bald sterben kan!", "tokens": ["La\u00df", "ster\u00b7ben", ",", "was", "bald", "ster\u00b7ben", "kan", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVINF", "$,", "PRELS", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Welt ist so beschaffen,", "tokens": ["Die", "Welt", "ist", "so", "be\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df dem erst wol ist umb vnd an,", "tokens": ["Da\u00df", "dem", "erst", "wol", "ist", "umb", "vnd", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADV", "ADV", "VAFIN", "APPR", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der seelig eingeschlaffen:", "tokens": ["Der", "see\u00b7lig", "ein\u00b7ge\u00b7schlaf\u00b7fen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Was wohnen hie f\u00fcr Plagen nicht,", "tokens": ["Was", "woh\u00b7nen", "hie", "f\u00fcr", "Pla\u00b7gen", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "APPR", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die vns doch auch aus diesem Liecht", "tokens": ["Die", "vns", "doch", "auch", "aus", "die\u00b7sem", "Liecht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nach vielem Leid erst raffen?", "tokens": ["Nach", "vie\u00b7lem", "Leid", "erst", "raf\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wir gehen alle diesen Gang.", "tokens": ["Wir", "ge\u00b7hen", "al\u00b7le", "die\u00b7sen", "Gang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Dampff nur wirfft vns nieder", "tokens": ["Ein", "Dampff", "nur", "wirfft", "vns", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vnd machet uns wol sterbe-kranck,", "tokens": ["Vnd", "ma\u00b7chet", "uns", "wol", "ster\u00b7be\u00b7kranck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Entfleischet alle Glieder:", "tokens": ["Ent\u00b7flei\u00b7schet", "al\u00b7le", "Glie\u00b7der", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dann nimbt nach grosser Angst vnd Pein", "tokens": ["Dann", "nimbt", "nach", "gros\u00b7ser", "Angst", "vnd", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Tod uns s\u00e4mptlich zu sich ein,", "tokens": ["Der", "Tod", "uns", "s\u00e4mpt\u00b7lich", "zu", "sich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADJD", "APPR", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Vnd schicket keinen wieder.", "tokens": ["Vnd", "schi\u00b7cket", "kei\u00b7nen", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Weil ich nun dieses richtig weis,", "tokens": ["Weil", "ich", "nun", "die\u00b7ses", "rich\u00b7tig", "weis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PDAT", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was hab' ich dessen Frommen,", "tokens": ["Was", "hab'", "ich", "des\u00b7sen", "From\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PDS", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ob ich ein Kind, ob ich ein Grei\u00df", "tokens": ["Ob", "ich", "ein", "Kind", ",", "ob", "ich", "ein", "Grei\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von hie werd' hingenommen?", "tokens": ["Von", "hie", "werd'", "hin\u00b7ge\u00b7nom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wer zeitig stirbt, hat minder Noht,", "tokens": ["Wer", "zei\u00b7tig", "stirbt", ",", "hat", "min\u00b7der", "Noht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVFIN", "$,", "VAFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kan vielem Vnfall durch den Todt", "tokens": ["Kan", "vie\u00b7lem", "Vn\u00b7fall", "durch", "den", "Todt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Fein aus dem Wege kommen.", "tokens": ["Fein", "aus", "dem", "We\u00b7ge", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Sein unbeflecktes Vnschuld-Kleid", "tokens": ["Sein", "un\u00b7be\u00b7fleck\u00b7tes", "Vn\u00b7schuld\u00b7Kleid"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird dort jhn hoch erheben,", "tokens": ["Wird", "dort", "jhn", "hoch", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vnd auch f\u00fcr vielen Alten weit", "tokens": ["Vnd", "auch", "f\u00fcr", "vie\u00b7len", "Al\u00b7ten", "weit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PIAT", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Vorzugs Preis ihm geben;", "tokens": ["Des", "Vor\u00b7zugs", "Preis", "ihm", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der heilgen Engel weisse Schaar,", "tokens": ["Der", "heil\u00b7gen", "En\u00b7gel", "weis\u00b7se", "Schaar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die hie stets sein Geleits-Volck war,", "tokens": ["Die", "hie", "stets", "sein", "Ge\u00b7leits\u00b7Volck", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wird dort auch umb jhn schweben.", "tokens": ["Wird", "dort", "auch", "umb", "jhn", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "La\u00df sterben, was bald sterben kan!", "tokens": ["La\u00df", "ster\u00b7ben", ",", "was", "bald", "ster\u00b7ben", "kan", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVINF", "$,", "PRELS", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gott l\u00e4st gebohren werden,", "tokens": ["Gott", "l\u00e4st", "ge\u00b7boh\u00b7ren", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gebeut nicht minder auch, wenn man", "tokens": ["Ge\u00b7beut", "nicht", "min\u00b7der", "auch", ",", "wenn", "man"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PTKNEG", "ADV", "ADV", "$,", "KOUS", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sol scheiden von der Erden:", "tokens": ["Sol", "schei\u00b7den", "von", "der", "Er\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wer klug ist, giebt Ihm Ehr vnd Preis,", "tokens": ["Wer", "klug", "ist", ",", "giebt", "Ihm", "Ehr", "vnd", "Preis", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vnd sieht, da\u00df er zu folgen wei\u00df", "tokens": ["Vnd", "sieht", ",", "da\u00df", "er", "zu", "fol\u00b7gen", "wei\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "PTKZU", "VVINF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mit frewdigen Geberden.", "tokens": ["Mit", "frew\u00b7di\u00b7gen", "Ge\u00b7ber\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}