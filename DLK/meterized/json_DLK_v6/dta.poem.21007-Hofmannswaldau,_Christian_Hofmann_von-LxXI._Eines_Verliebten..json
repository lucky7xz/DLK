{"dta.poem.21007": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "LxXI.  \n Eines Verliebten.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Der hier begraben liegt/ ist aus der Buhler Orden/", "tokens": ["Der", "hier", "be\u00b7gra\u00b7ben", "liegt", "/", "ist", "aus", "der", "Buh\u00b7ler", "Or\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "VVFIN", "$(", "VAFIN", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht wunder dich zu sehr/ da\u00df er zur Asche worden/", "tokens": ["Nicht", "wun\u00b7der", "dich", "zu", "sehr", "/", "da\u00df", "er", "zur", "A\u00b7sche", "wor\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "PRF", "APPR", "ADV", "$(", "KOUS", "PPER", "APPRART", "NN", "VAPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sein Leib war voller Glut und voller Flam\u0303enschein/", "tokens": ["Sein", "Leib", "war", "vol\u00b7ler", "Glut", "und", "vol\u00b7ler", "Flam\u0303en\u00b7schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJA", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie solte denn der Mann nicht Asche worden seyn.", "tokens": ["Wie", "sol\u00b7te", "denn", "der", "Mann", "nicht", "A\u00b7sche", "wor\u00b7den", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ADV", "ART", "NN", "PTKNEG", "NN", "VAPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}