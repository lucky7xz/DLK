{"textgrid.poem.44509": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Entgegnung", "genre": "verse", "period": "N.A.", "pub_year": 1841, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gabst du schon auf die Poesie?", "tokens": ["Gabst", "du", "schon", "auf", "die", "Poe\u00b7sie", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Ich nicht!", "tokens": ["Ich", "nicht", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "PTKNEG", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "W\u00e4rs nicht geg\u00f6nnt zu schreiben mehr,", "tokens": ["W\u00e4rs", "nicht", "ge\u00b7g\u00f6nnt", "zu", "schrei\u00b7ben", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "VVPP", "PTKZU", "VVINF", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So lebt ich ein Gedicht.", "tokens": ["So", "lebt", "ich", "ein", "Ge\u00b7dicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Verachten, was der P\u00f6bel ehrt,", "tokens": ["Ver\u00b7ach\u00b7ten", ",", "was", "der", "P\u00f6\u00b7bel", "ehrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich selbst genug,", "tokens": ["Sich", "selbst", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Zum Schlimmen nie, durch nichts bekehrt,", "tokens": ["Zum", "Schlim\u00b7men", "nie", ",", "durch", "nichts", "be\u00b7kehrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$,", "APPR", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und fest statt klug;", "tokens": ["Und", "fest", "statt", "klug", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Denn nicht die Gaben sinds, was fehlt,", "tokens": ["Denn", "nicht", "die", "Ga\u00b7ben", "sinds", ",", "was", "fehlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "VAFIN", "$,", "PWS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Verse Pracht;", "tokens": ["Der", "Ver\u00b7se", "Pracht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Der Sinn ists, h\u00f6her als die Welt,", "tokens": ["Der", "Sinn", "ists", ",", "h\u00f6\u00b7her", "als", "die", "Welt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was Dichter macht.", "tokens": ["Was", "Dich\u00b7ter", "macht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Und w\u00e4r der Jugend nur geg\u00f6nnt,", "tokens": ["Und", "w\u00e4r", "der", "Ju\u00b7gend", "nur", "ge\u00b7g\u00f6nnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So Kraft als Schwung,", "tokens": ["So", "Kraft", "als", "Schwung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KOUS", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wer Vorteil nie von Ehren trennt,", "tokens": ["Wer", "Vor\u00b7teil", "nie", "von", "Eh\u00b7ren", "trennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bleibt ewig jung.", "tokens": ["Bleibt", "e\u00b7wig", "jung", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Drum schrecke andre, was da droht,", "tokens": ["Drum", "schre\u00b7cke", "and\u00b7re", ",", "was", "da", "droht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIS", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mich nicht,", "tokens": ["Mich", "nicht", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "PTKNEG", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Und einst im Sterben sei mein Tod", "tokens": ["Und", "einst", "im", "Ster\u00b7ben", "sei", "mein", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch ein Gedicht.", "tokens": ["Noch", "ein", "Ge\u00b7dicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}