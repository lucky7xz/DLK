{"textgrid.poem.65669": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Konstantin Kanari", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Konstantin Kanari hei\u00df' ich, der ich lieg' in dieser Gruft.", "tokens": ["Kons\u00b7tan\u00b7tin", "Ka\u00b7na\u00b7ri", "hei\u00df'", "ich", ",", "der", "ich", "lieg'", "in", "die\u00b7ser", "Gruft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "PPER", "$,", "PRELS", "PPER", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Zwei Osmanenflotten hab' ich fliegen lassen in die Luft,", "tokens": ["Zwei", "Os\u00b7ma\u00b7nen\u00b7flot\u00b7ten", "hab'", "ich", "flie\u00b7gen", "las\u00b7sen", "in", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "PPER", "VVINF", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Bin auf meinem Bett gestorben in dem Herrn, als guter Christ:", "tokens": ["Bin", "auf", "mei\u00b7nem", "Bett", "ge\u00b7stor\u00b7ben", "in", "dem", "Herrn", ",", "als", "gu\u00b7ter", "Christ", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "APPR", "ART", "NN", "$,", "KOUS", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Nur ", "tokens": ["Nur"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Da\u00df ich mit der dritten Flotte unsrer Feind' auf hohem Meer", "tokens": ["Da\u00df", "ich", "mit", "der", "drit\u00b7ten", "Flot\u00b7te", "uns\u00b7rer", "Feind'", "auf", "ho\u00b7hem", "Meer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "--+-+-+-+-+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "Mitten unter Blitz und Donner in den Tod geflogen w\u00e4r'.", "tokens": ["Mit\u00b7ten", "un\u00b7ter", "Blitz", "und", "Don\u00b7ner", "in", "den", "Tod", "ge\u00b7flo\u00b7gen", "w\u00e4r'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "APPR", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Hier in freie Erde haben meinen Leib sie eingesenkt \u2013", "tokens": ["Hier", "in", "frei\u00b7e", "Er\u00b7de", "ha\u00b7ben", "mei\u00b7nen", "Leib", "sie", "ein\u00b7ge\u00b7senkt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "PPER", "VVPP", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Gieb, mein Gott, da\u00df frei sie bleibe, bis mein Leib sie wieder sprengt!", "tokens": ["Gieb", ",", "mein", "Gott", ",", "da\u00df", "frei", "sie", "blei\u00b7be", ",", "bis", "mein", "Leib", "sie", "wie\u00b7der", "sprengt", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PPOSAT", "NN", "$,", "KOUS", "ADJD", "PPER", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}