{"dta.poem.10778": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Friderici Lingelshemij   Hochzeitlied an seine  \n Schwester Fraw Salome/ Herrn   D. Petri de  \n Spin\u00e2  Hochzeiterin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Wann aller guter ding sein drey/", "tokens": ["Wann", "al\u00b7ler", "gu\u00b7ter", "ding", "sein", "drey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "ADJA", "NN", "PPOSAT", "CARD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gleich wie das Sprichwort lehret/", "tokens": ["Gleich", "wie", "das", "Sprich\u00b7wort", "leh\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vnd da\u00df gar nichts vollkommen sey", "tokens": ["Vnd", "da\u00df", "gar", "nichts", "voll\u00b7kom\u00b7men", "sey"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "PIS", "ADJD", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das sich mit dreymahl keret/", "tokens": ["Das", "sich", "mit", "drey\u00b7mahl", "ke\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPR", "ADV", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So w\u00fcnschen wir dir billich gl\u00fcck/", "tokens": ["So", "w\u00fcn\u00b7schen", "wir", "dir", "bil\u00b7lich", "gl\u00fcck", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vnd thun vns h\u00f6chlich frewen/", "tokens": ["Vnd", "thun", "vns", "h\u00f6ch\u00b7lich", "fre\u00b7wen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Weils Gott der Herr dahingeschickt/", "tokens": ["Weils", "Gott", "der", "Herr", "da\u00b7hin\u00b7ge\u00b7schickt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df du nun zehlst mit dreyen.", "tokens": ["Da\u00df", "du", "nun", "zehlst", "mit", "drey\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "APPR", "CARD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Zweymahl hastu das schwere leidt", "tokens": ["Zwey\u00b7mahl", "has\u00b7tu", "das", "schwe\u00b7re", "leidt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Gedultig m\u00fcssen tragen/", "tokens": ["Ge\u00b7dul\u00b7tig", "m\u00fcs\u00b7sen", "tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Die dritte rew dich g\u00e4ntzlich freyt", "tokens": ["Die", "drit\u00b7te", "rew", "dich", "g\u00e4ntz\u00b7lich", "freyt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Von sorgen vnd von klagen.", "tokens": ["Von", "sor\u00b7gen", "vnd", "von", "kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Ehe dann der Sonnenschein von fern", "tokens": ["E\u00b7he", "dann", "der", "Son\u00b7nen\u00b7schein", "von", "fern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "NN", "APPR", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.14": {"text": "Die Erd komt zubekleiden/", "tokens": ["Die", "Erd", "komt", "zu\u00b7be\u00b7klei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Mu\u00df nit zuvor der Morgenstern", "tokens": ["Mu\u00df", "nit", "zu\u00b7vor", "der", "Mor\u00b7gens\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PTKNEG", "ADV", "ART", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.16": {"text": "Die dunckle Nacht vertreiben?", "tokens": ["Die", "dunck\u00b7le", "Nacht", "ver\u00b7trei\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Die sch\u00f6ne G\u00f6ttin folgt darauff/", "tokens": ["Die", "sch\u00f6\u00b7ne", "G\u00f6t\u00b7tin", "folgt", "dar\u00b7auff", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PAV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Die Morgenr\u00f6th genennet/", "tokens": ["Die", "Mor\u00b7gen\u00b7r\u00f6th", "ge\u00b7nen\u00b7net", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Vnd den ererst in vollem Lauff", "tokens": ["Vnd", "den", "er\u00b7erst", "in", "vol\u00b7lem", "Lauff"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Die helle Sonn gerennet/", "tokens": ["Die", "hel\u00b7le", "Sonn", "ge\u00b7ren\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Als viel Tugent in dreyen stickt/", "tokens": ["Als", "viel", "Tu\u00b7gent", "in", "drey\u00b7en", "stickt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "APPR", "CARD", "VVFIN", "$("], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.22": {"text": "Das ist nit au\u00dfzusprechen/", "tokens": ["Das", "ist", "nit", "au\u00df\u00b7zu\u00b7spre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Der wer gelert vnd wolgeschickt/", "tokens": ["Der", "wer", "ge\u00b7lert", "vnd", "wol\u00b7ge\u00b7schickt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PWS", "VVPP", "KON", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Ders all wist au\u00dfzurechen/", "tokens": ["Ders", "all", "wist", "au\u00df\u00b7zu\u00b7re\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Allein nun (Schwester) nit mehr acht", "tokens": ["Al\u00b7lein", "nun", "(", "Schwes\u00b7ter", ")", "nit", "mehr", "acht"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "$(", "NN", "$(", "PTKNEG", "ADV", "CARD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Das Vngl\u00fcck so vergangen/", "tokens": ["Das", "Vn\u00b7gl\u00fcck", "so", "ver\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "Die Nacht die ist zu endt gebracht/", "tokens": ["Die", "Nacht", "die", "ist", "zu", "endt", "ge\u00b7bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VAFIN", "PTKA", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Die Sonn ist vffgegangen/", "tokens": ["Die", "Sonn", "ist", "vff\u00b7ge\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.29": {"text": "Das Gl\u00fcck dir kompt mit haussen bey/", "tokens": ["Das", "Gl\u00fcck", "dir", "kompt", "mit", "haus\u00b7sen", "bey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "APPR", "NN", "APPR", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Da ist kein fehl zu sp\u00fcren/", "tokens": ["Da", "ist", "kein", "fehl", "zu", "sp\u00fc\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": "F\u00fcr ein Gl\u00fcck wirstu haben drey/", "tokens": ["F\u00fcr", "ein", "Gl\u00fcck", "wirs\u00b7tu", "ha\u00b7ben", "drey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VAFIN", "CARD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Drey solt inn Schilde f\u00fchren.", "tokens": ["Drey", "solt", "inn", "Schil\u00b7de", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.33": {"text": "Vnd wann mein Wunsch kan werden wahr/", "tokens": ["Vnd", "wann", "mein", "Wunsch", "kan", "wer\u00b7den", "wahr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPOSAT", "NN", "VMFIN", "VAFIN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "W", "tokens": ["W"], "token_info": ["word"], "pos": ["XY"], "meter": "+", "measure": "single.up"}, "line.35": {"text": "Vil mehr/ dann drey mahl dreissig jahr/", "tokens": ["Vil", "mehr", "/", "dann", "drey", "mahl", "dreis\u00b7sig", "jahr", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$(", "ADV", "CARD", "ADV", "CARD", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Mein Bruder ", "tokens": ["Mein", "Bru\u00b7der"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.37": {"text": "Der S\u00f6hn wirst haben drey mahl drey/", "tokens": ["Der", "S\u00f6hn", "wirst", "ha\u00b7ben", "drey", "mahl", "drey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VAFIN", "CARD", "ADV", "CARD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Auch sovil T\u00f6chtersehen/", "tokens": ["Auch", "so\u00b7vil", "T\u00f6ch\u00b7ter\u00b7se\u00b7hen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.39": {"text": "Darzu in wahrer Lieb vnd Trew", "tokens": ["Dar\u00b7zu", "in", "wah\u00b7rer", "Lieb", "vnd", "Trew"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Die gantze Eh begehen.", "tokens": ["Die", "gant\u00b7ze", "Eh", "be\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}