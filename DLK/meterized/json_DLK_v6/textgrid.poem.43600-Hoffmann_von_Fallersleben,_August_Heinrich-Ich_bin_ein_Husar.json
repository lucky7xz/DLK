{"textgrid.poem.43600": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ich bin ein Husar", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hurrah! ich bin ein Husar!", "tokens": ["Hur\u00b7rah", "!", "ich", "bin", "ein", "Hu\u00b7sar", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich komme wie V\u00f6gel geflogen,", "tokens": ["Ich", "kom\u00b7me", "wie", "V\u00f6\u00b7gel", "ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Ich habe den S\u00e4bel gezogen,", "tokens": ["Ich", "ha\u00b7be", "den", "S\u00e4\u00b7bel", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Das Pistol in der Hand,", "tokens": ["Das", "Pis\u00b7tol", "in", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Den Hahn gespannt.", "tokens": ["Den", "Hahn", "ge\u00b7spannt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "So jag' ich keck in der Feinde Reih'n,", "tokens": ["So", "jag'", "ich", "keck", "in", "der", "Fein\u00b7de", "Reih'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Ich hau' und ich schie\u00dfe tapfer drein:", "tokens": ["Ich", "hau'", "und", "ich", "schie\u00b7\u00dfe", "tap\u00b7fer", "drein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "PPER", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Piff paff puff!", "tokens": ["Piff", "paff", "puff", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.it", "FM.it", "FM.it", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Hurrah!", "tokens": ["Hur\u00b7rah", "!"], "token_info": ["word", "punct"], "pos": ["ITJ", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "Hurrah! ich bin ein Husar!", "tokens": ["Hur\u00b7rah", "!", "ich", "bin", "ein", "Hu\u00b7sar", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Feinde kommen geritten,", "tokens": ["Die", "Fein\u00b7de", "kom\u00b7men", "ge\u00b7rit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Nun hei\u00dft es muthig gestritten!", "tokens": ["Nun", "hei\u00dft", "es", "mut\u00b7hig", "ge\u00b7strit\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die Standarte voran!", "tokens": ["Die", "Stand\u00b7ar\u00b7te", "vo\u00b7ran", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Frisch drauf und dran!", "tokens": ["Frisch", "drauf", "und", "dran", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "KON", "PAV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Ich jage keck in der Feinde Reih'n", "tokens": ["Ich", "ja\u00b7ge", "keck", "in", "der", "Fein\u00b7de", "Reih'n"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Ich hau' und ich schie\u00df', und der Sieg ist mein!", "tokens": ["Ich", "hau'", "und", "ich", "schie\u00df'", ",", "und", "der", "Sieg", "ist", "mein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "PPER", "VVFIN", "$,", "KON", "ART", "NN", "VAFIN", "PPOSAT", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Piff paff puff!", "tokens": ["Piff", "paff", "puff", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.it", "FM.it", "FM.it", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Hurrah!", "tokens": ["Hur\u00b7rah", "!"], "token_info": ["word", "punct"], "pos": ["ITJ", "$."], "meter": "-+", "measure": "iambic.single"}}}}}