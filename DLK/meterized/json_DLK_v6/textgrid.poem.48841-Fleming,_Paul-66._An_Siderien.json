{"textgrid.poem.48841": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "66. An Siderien", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du auch, ", "tokens": ["Du", "auch", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "ADV", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "bei meiner ", "tokens": ["bei", "mei\u00b7ner"], "token_info": ["word", "word"], "pos": ["APPR", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "durch meiner Verse Schwung, darzu mir Fl\u00fcgel macht", "tokens": ["durch", "mei\u00b7ner", "Ver\u00b7se", "Schwung", ",", "dar\u00b7zu", "mir", "Fl\u00fc\u00b7gel", "macht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,", "PAV", "PPER", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "die, welche Vater hei\u00dft Diespiters Gehirne.", "tokens": ["die", ",", "wel\u00b7che", "Va\u00b7ter", "hei\u00dft", "Dies\u00b7pi\u00b7ters", "Ge\u00b7hir\u00b7ne", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Auch du verdienest das, du himmelssch\u00f6ne Dirne,", "tokens": ["Auch", "du", "ver\u00b7die\u00b7nest", "das", ",", "du", "him\u00b7mels\u00b7sch\u00f6\u00b7ne", "Dir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PDS", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "weil aller Tugend Zier aus deiner Sch\u00f6nheit lacht,", "tokens": ["weil", "al\u00b7ler", "Tu\u00b7gend", "Zier", "aus", "dei\u00b7ner", "Sch\u00f6n\u00b7heit", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "darum ich denn auch dich mir hatte zugedacht,", "tokens": ["da\u00b7rum", "ich", "denn", "auch", "dich", "mir", "hat\u00b7te", "zu\u00b7ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADV", "ADV", "PPER", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "um welches aber ich nicht mit den Himmeln z\u00fcrne.", "tokens": ["um", "wel\u00b7ches", "a\u00b7ber", "ich", "nicht", "mit", "den", "Him\u00b7meln", "z\u00fcr\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADV", "PPER", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sei willig, edles Bild, und schwing dich auf mit mir", "tokens": ["Sei", "wil\u00b7lig", ",", "ed\u00b7les", "Bild", ",", "und", "schwing", "dich", "auf", "mit", "mir"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VAIMP", "ADJD", "$,", "ADJA", "NN", "$,", "KON", "VVFIN", "PRF", "APPR", "APPR", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und stelle deinen Glanz dem Angelsterne f\u00fcr,", "tokens": ["und", "stel\u00b7le", "dei\u00b7nen", "Glanz", "dem", "An\u00b7gels\u00b7ter\u00b7ne", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ART", "NN", "APPR", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "da\u00df, weil ich f\u00f6rderhin mu\u00df auf den Wellen schweben,", "tokens": ["da\u00df", ",", "weil", "ich", "f\u00f6r\u00b7der\u00b7hin", "mu\u00df", "auf", "den", "Wel\u00b7len", "schwe\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PPER", "ADV", "VMFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "die noch kein deutsches Schiff bisher gesuchet auf,", "tokens": ["die", "noch", "kein", "deut\u00b7sches", "Schiff", "bis\u00b7her", "ge\u00b7su\u00b7chet", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIAT", "ADJA", "NN", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "nach dir, mein Leitstern, ich stets richte meinen Lauf,", "tokens": ["nach", "dir", ",", "mein", "Leits\u00b7tern", ",", "ich", "stets", "rich\u00b7te", "mei\u00b7nen", "Lauf", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPOSAT", "NN", "$,", "PPER", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bin ich denn ohne dich, so bin ich um mein Leben.", "tokens": ["Bin", "ich", "denn", "oh\u00b7ne", "dich", ",", "so", "bin", "ich", "um", "mein", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "PPER", "$,", "ADV", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}