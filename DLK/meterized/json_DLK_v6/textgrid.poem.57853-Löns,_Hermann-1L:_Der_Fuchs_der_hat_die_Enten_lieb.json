{"textgrid.poem.57853": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Fuchs der hat die Enten lieb", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Fuchs der hat die Enten lieb", "tokens": ["Der", "Fuchs", "der", "hat", "die", "En\u00b7ten", "lieb"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "ART", "VAFIN", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und holt sie, wo er kann;", "tokens": ["Und", "holt", "sie", ",", "wo", "er", "kann", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Jedoch die jungen M\u00e4gdelein,", "tokens": ["Je\u00b7doch", "die", "jun\u00b7gen", "M\u00e4g\u00b7del\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die liebt der J\u00e4gersmann.", "tokens": ["Die", "liebt", "der", "J\u00e4\u00b7gers\u00b7mann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er liebt sie in dem gr\u00fcnen Wald", "tokens": ["Er", "liebt", "sie", "in", "dem", "gr\u00fc\u00b7nen", "Wald"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und auf der braunen Heid;", "tokens": ["Und", "auf", "der", "brau\u00b7nen", "Heid", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er liebt sie um die Mitternacht", "tokens": ["Er", "liebt", "sie", "um", "die", "Mit\u00b7ter\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und um die Abendzeit.", "tokens": ["Und", "um", "die", "A\u00b7bend\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er liebt sie auch am hellen Tag,", "tokens": ["Er", "liebt", "sie", "auch", "am", "hel\u00b7len", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er liebt sie hei\u00df und treu;", "tokens": ["Er", "liebt", "sie", "hei\u00df", "und", "treu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er liebt nicht eine ganz allein,", "tokens": ["Er", "liebt", "nicht", "ei\u00b7ne", "ganz", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er liebt auch zwei und drei.", "tokens": ["Er", "liebt", "auch", "zwei", "und", "drei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "CARD", "KON", "CARD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die eine liebt er offenbar,", "tokens": ["Die", "ei\u00b7ne", "liebt", "er", "of\u00b7fen\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auch wenn er sie nicht freit;", "tokens": ["Auch", "wenn", "er", "sie", "nicht", "freit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PPER", "PTKNEG", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die andre liebt er bei der Nacht", "tokens": ["Die", "and\u00b7re", "liebt", "er", "bei", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In aller Heimlichkeit.", "tokens": ["In", "al\u00b7ler", "Heim\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und geht ein M\u00e4dchen in den Wald,", "tokens": ["Und", "geht", "ein", "M\u00e4d\u00b7chen", "in", "den", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ist es ganz allein,", "tokens": ["Und", "ist", "es", "ganz", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und trifft sie dort den J\u00e4ger an,", "tokens": ["Und", "trifft", "sie", "dort", "den", "J\u00e4\u00b7ger", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein eigen mu\u00df sie sein.", "tokens": ["Sein", "ei\u00b7gen", "mu\u00df", "sie", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}