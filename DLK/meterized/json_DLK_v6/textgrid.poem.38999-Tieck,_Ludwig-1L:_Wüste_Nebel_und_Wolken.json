{"textgrid.poem.38999": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: W\u00fcste Nebel und Wolken", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "W\u00fcste Nebel und Wolken", "tokens": ["W\u00fcs\u00b7te", "Ne\u00b7bel", "und", "Wol\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Ziehn \u00fcber die zackigen Berge,", "tokens": ["Ziehn", "\u00fc\u00b7ber", "die", "za\u00b7cki\u00b7gen", "Ber\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Durch die \u00f6de Landschaft:", "tokens": ["Durch", "die", "\u00f6\u00b7de", "Land\u00b7schaft", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Weithin alles tr\u00fcb und finster,", "tokens": ["Weit\u00b7hin", "al\u00b7les", "tr\u00fcb", "und", "fins\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Kein Sonnenschimmer bricht", "tokens": ["Kein", "Son\u00b7nen\u00b7schim\u00b7mer", "bricht"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die schweren Wolkenmassen.", "tokens": ["Die", "schwe\u00b7ren", "Wol\u00b7ken\u00b7mas\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wie ausgebrannte Gebirge,", "tokens": ["Wie", "aus\u00b7ge\u00b7brann\u00b7te", "Ge\u00b7bir\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Wie eine gestorbene Welt", "tokens": ["Wie", "ei\u00b7ne", "ge\u00b7stor\u00b7be\u00b7ne", "Welt"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.9": {"text": "So weit das Auge \u00e4ngstlich schaut.", "tokens": ["So", "weit", "das", "Au\u00b7ge", "\u00e4ngst\u00b7lich", "schaut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Da denk ich der vielen", "tokens": ["Da", "denk", "ich", "der", "vie\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "PIAT"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Qualvollen N\u00e4chte,", "tokens": ["Qual\u00b7vol\u00b7len", "N\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Ohne Schlaf und Erquickung,", "tokens": ["Oh\u00b7ne", "Schlaf", "und", "Er\u00b7quic\u00b7kung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.13": {"text": "Und rund umher steht jene Angst", "tokens": ["Und", "rund", "um\u00b7her", "steht", "je\u00b7ne", "Angst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "PTKVZ", "VVFIN", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "In Fels und Berg mir vorgemalt.", "tokens": ["In", "Fels", "und", "Berg", "mir", "vor\u00b7ge\u00b7malt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}