{"textgrid.poem.58165": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "203. Lieben im Leiden", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Leiden denk: Gott bleibet Gott,", "tokens": ["Im", "Lei\u00b7den", "denk", ":", "Gott", "blei\u00b7bet", "Gott", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$.", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich sei vergn\u00fcgt, ich sei betr\u00fcbet;", "tokens": ["Ich", "sei", "ver\u00b7gn\u00fcgt", ",", "ich", "sei", "be\u00b7tr\u00fc\u00b7bet", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "PPER", "VAFIN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Drum billig er in Kreuz und Not", "tokens": ["Drum", "bil\u00b7lig", "er", "in", "Kreuz", "und", "Not"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ADJD", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gleichwie im Trost mu\u00df sein geliebet!", "tokens": ["Gleich\u00b7wie", "im", "Trost", "mu\u00df", "sein", "ge\u00b7lie\u00b7bet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VMFIN", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}