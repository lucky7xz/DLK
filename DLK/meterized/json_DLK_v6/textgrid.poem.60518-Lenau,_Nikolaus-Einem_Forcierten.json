{"textgrid.poem.60518": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Einem Forcierten", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu besiegen deine schwere", "tokens": ["Zu", "be\u00b7sie\u00b7gen", "dei\u00b7ne", "schwe\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "PPOSAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ungelenkigkeit,", "tokens": ["Un\u00b7ge\u00b7len\u00b7kig\u00b7keit", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Bist du tanzen in die Lehre", "tokens": ["Bist", "du", "tan\u00b7zen", "in", "die", "Leh\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gangen zu Sankt Veit.", "tokens": ["Gan\u00b7gen", "zu", "Sankt", "Veit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVFIN", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Und der wackre Meister bl\u00e4ute", "tokens": ["Und", "der", "wack\u00b7re", "Meis\u00b7ter", "bl\u00e4u\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In den Leib dir ganz", "tokens": ["In", "den", "Leib", "dir", "ganz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPER", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Seinen Rhythmus, und die Leute", "tokens": ["Sei\u00b7nen", "Rhyth\u00b7mus", ",", "und", "die", "Leu\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lobten deinen Tanz.", "tokens": ["Lob\u00b7ten", "dei\u00b7nen", "Tanz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Schief ist all dein Hirn gebeutelt,", "tokens": ["Schief", "ist", "all", "dein", "Hirn", "ge\u00b7beu\u00b7telt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIAT", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jedes Glied verdreht;", "tokens": ["Je\u00b7des", "Glied", "ver\u00b7dreht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Drum wer tanzend nicht sanktveitelt;", "tokens": ["Drum", "wer", "tan\u00b7zend", "nicht", "sankt\u00b7vei\u00b7telt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PWS", "ADJD", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "D\u00fcnkt dir kein Poet.", "tokens": ["D\u00fcnkt", "dir", "kein", "Po\u00b7et", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}