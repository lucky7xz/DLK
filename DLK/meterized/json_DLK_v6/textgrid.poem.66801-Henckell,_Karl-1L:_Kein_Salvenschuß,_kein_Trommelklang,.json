{"textgrid.poem.66801": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kein Salvenschu\u00df, kein Trommelklang,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kein Salvenschu\u00df, kein Trommelklang,", "tokens": ["Kein", "Sal\u00b7ven\u00b7schu\u00df", ",", "kein", "Trom\u00b7mel\u00b7klang", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als sie den Freund begruben,", "tokens": ["Als", "sie", "den", "Freund", "be\u00b7gru\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kein Sonnenschein, kein Lerchensang \u2013", "tokens": ["Kein", "Son\u00b7nen\u00b7schein", ",", "kein", "Ler\u00b7chen\u00b7sang", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "War ", "tokens": ["War"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Im Kampf mit gro\u00dfen Buben.", "tokens": ["Im", "Kampf", "mit", "gro\u00b7\u00dfen", "Bu\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Herbstwind pfiff, sein Heulen schwoll,", "tokens": ["Der", "Herbst\u00b7wind", "pfiff", ",", "sein", "Heu\u00b7len", "schwoll", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Weiden seufzten schaurig;", "tokens": ["Die", "Wei\u00b7den", "seufz\u00b7ten", "schau\u00b7rig", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Schaufel voll, die Erde scholl,", "tokens": ["Die", "Schau\u00b7fel", "voll", ",", "die", "Er\u00b7de", "scholl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verschr\u00e4nkten Armes stand der Groll", "tokens": ["Ver\u00b7schr\u00e4nk\u00b7ten", "Ar\u00b7mes", "stand", "der", "Groll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Am Grabe stumm und traurig.", "tokens": ["Am", "Gra\u00b7be", "stumm", "und", "trau\u00b7rig", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Kein Pfarrer drosch Unsterblichkeit,", "tokens": ["Kein", "Pfar\u00b7rer", "drosch", "U\u00b7nsterb\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Kein Pfaffe pl\u00e4rrte Messen;", "tokens": ["Kein", "Pfaf\u00b7fe", "pl\u00e4rr\u00b7te", "Mes\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein heilig Leid, ein schweigend Leid.", "tokens": ["Ein", "hei\u00b7lig", "Leid", ",", "ein", "schwei\u00b7gend", "Leid", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In ihrem dunklen Feierkleid", "tokens": ["In", "ih\u00b7rem", "dunk\u00b7len", "Fei\u00b7er\u00b7kleid"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wehklagten die Zypressen.", "tokens": ["Weh\u00b7klag\u00b7ten", "die", "Zyp\u00b7res\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}}, "stanza.4": {"line.1": {"text": "So blieb die Menge dr\u00e4ngend stehn,", "tokens": ["So", "blieb", "die", "Men\u00b7ge", "dr\u00e4n\u00b7gend", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als sich das Grab geschlossen.", "tokens": ["Als", "sich", "das", "Grab", "ge\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da dr\u00f6hnt' es: Auseinandergehn!", "tokens": ["Da", "dr\u00f6hnt'", "es", ":", "Aus\u00b7ein\u00b7an\u00b7der\u00b7gehn", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und schon war Helm an Helm zu sehn \u2013", "tokens": ["Und", "schon", "war", "Helm", "an", "Helm", "zu", "sehn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "NN", "APPR", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Des Himmels Z\u00e4hren flossen.", "tokens": ["Des", "Him\u00b7mels", "Z\u00e4h\u00b7ren", "flos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nun flog ein Kranz mit rotem Band", "tokens": ["Nun", "flog", "ein", "Kranz", "mit", "ro\u00b7tem", "Band"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wohl auf des Grabes Mitte;", "tokens": ["Wohl", "auf", "des", "Gra\u00b7bes", "Mit\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und als er auf den H\u00fcgel sank,", "tokens": ["Und", "als", "er", "auf", "den", "H\u00fc\u00b7gel", "sank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da zogen schon die W\u00e4chter blank", "tokens": ["Da", "zo\u00b7gen", "schon", "die", "W\u00e4ch\u00b7ter", "blank"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Zucht und frommen Sitte.", "tokens": ["Der", "Zucht", "und", "from\u00b7men", "Sit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Von Leichenstein zu Leichenstein", "tokens": ["Von", "Lei\u00b7chen\u00b7stein", "zu", "Lei\u00b7chen\u00b7stein"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Klingen aus den Scheiden!", "tokens": ["Die", "Klin\u00b7gen", "aus", "den", "Schei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Auf Sch\u00e4delstatt und Totenbein", "tokens": ["Auf", "Sch\u00e4\u00b7del\u00b7statt", "und", "To\u00b7ten\u00b7bein"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie hieben in die Menschen ein \u2013", "tokens": ["Sie", "hie\u00b7ben", "in", "die", "Men\u00b7schen", "ein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da weinten alle Weiden.", "tokens": ["Da", "wein\u00b7ten", "al\u00b7le", "Wei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Das freche L\u00e4rmen klirrt' ans Ohr", "tokens": ["Das", "fre\u00b7che", "L\u00e4r\u00b7men", "klirrt'", "ans", "Ohr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der schlummernden Gerippe;", "tokens": ["Der", "schlum\u00b7mern\u00b7den", "Ge\u00b7rip\u00b7pe", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Entsetzen schlug den bleichen Chor,", "tokens": ["Ent\u00b7set\u00b7zen", "schlug", "den", "blei\u00b7chen", "Chor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und schwerbeleidigt fuhr empor", "tokens": ["Und", "schwer\u00b7be\u00b7lei\u00b7digt", "fuhr", "em\u00b7por"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Toten stille Sippe.", "tokens": ["Der", "To\u00b7ten", "stil\u00b7le", "Sip\u00b7pe", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Der Regen go\u00df, der Sturm schrie auf,", "tokens": ["Der", "Re\u00b7gen", "go\u00df", ",", "der", "Sturm", "schrie", "auf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Blut flo\u00df um Kreuz und H\u00fcgel,", "tokens": ["Blut", "flo\u00df", "um", "Kreuz", "und", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und ruhig von des Kranzes Schlauf", "tokens": ["Und", "ru\u00b7hig", "von", "des", "Kran\u00b7zes", "Schlauf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Vogel stieg gen Himmel auf", "tokens": ["Ein", "Vo\u00b7gel", "stieg", "gen", "Him\u00b7mel", "auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "APPR", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit purpurrotem Fl\u00fcgel ...", "tokens": ["Mit", "pur\u00b7pur\u00b7ro\u00b7tem", "Fl\u00fc\u00b7gel", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}