{"textgrid.poem.54273": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "Grabschrifft eines Schneiders", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich meynte, da\u00df der Tod mich noch verschonen solte,", "tokens": ["Ich", "meyn\u00b7te", ",", "da\u00df", "der", "Tod", "mich", "noch", "ver\u00b7scho\u00b7nen", "sol\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dieweil ich ihm umsonst ein Kleidgen machen wolte;", "tokens": ["Die\u00b7weil", "ich", "ihm", "um\u00b7sonst", "ein", "Kleid\u00b7gen", "ma\u00b7chen", "wol\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Allein es bliesse mir der schlaue Fuchs ins Ohr,", "tokens": ["Al\u00b7lein", "es", "blies\u00b7se", "mir", "der", "schlau\u00b7e", "Fuchs", "ins", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "ART", "ADJA", "NE", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Er brauchte weder Kleid, Sourtout, noch Roquelaur.", "tokens": ["Er", "brauch\u00b7te", "we\u00b7der", "Kleid", ",", "Sour\u00b7tout", ",", "noch", "Ro\u00b7que\u00b7laur", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "NN", "$,", "NN", "$,", "ADV", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}