{"textgrid.poem.24080": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Erwachen in den grellen Tag", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was war das f\u00fcr ein wunderreicher Traum!", "tokens": ["Was", "war", "das", "f\u00fcr", "ein", "wun\u00b7der\u00b7rei\u00b7cher", "Traum", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PDS", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er hat mein Herz so innig warm begl\u00fcckt ...", "tokens": ["Er", "hat", "mein", "Herz", "so", "in\u00b7nig", "warm", "be\u00b7gl\u00fcckt", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Er f\u00fchrte mich auf gr\u00fcne Wiesen aus", "tokens": ["Er", "f\u00fchr\u00b7te", "mich", "auf", "gr\u00fc\u00b7ne", "Wie\u00b7sen", "aus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Voll Fr\u00fchlingsblumen, \u2013 jeder Bl\u00fcte gab", "tokens": ["Voll", "Fr\u00fch\u00b7lings\u00b7blu\u00b7men", ",", "\u2013", "je\u00b7der", "Bl\u00fc\u00b7te", "gab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ADJD", "NN", "$,", "$(", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von Sonnengold er einen Glorienschein.", "tokens": ["Von", "Son\u00b7nen\u00b7gold", "er", "ei\u00b7nen", "Glo\u00b7ri\u00b7en\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Hell war der Himmel und unendlich weit,", "tokens": ["Hell", "war", "der", "Him\u00b7mel", "und", "un\u00b7end\u00b7lich", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "KON", "ADJD", "ADJD", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Leis wimpelte von s\u00e4ftevollen Zweigen,", "tokens": ["Leis", "wim\u00b7pel\u00b7te", "von", "s\u00e4f\u00b7te\u00b7vol\u00b7len", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die gl\u00e4nzend \u00fcberquollen in dem Licht", "tokens": ["Die", "gl\u00e4n\u00b7zend", "\u00fc\u00b7berq\u00b7uol\u00b7len", "in", "dem", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Des jungen Lenzen, unber\u00fchrtes Gr\u00fcn.", "tokens": ["Des", "jun\u00b7gen", "Len\u00b7zen", ",", "un\u00b7be\u00b7r\u00fchr\u00b7tes", "Gr\u00fcn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und alles war voll Gl\u00fcck, voll Gl\u00fcck auch ich;", "tokens": ["Und", "al\u00b7les", "war", "voll", "Gl\u00fcck", ",", "voll", "Gl\u00fcck", "auch", "ich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ADJD", "NN", "$,", "ADJD", "NN", "ADV", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Sonnenst\u00e4ubchen Gl\u00fcck: so f\u00fchlt ich mich.", "tokens": ["Ein", "Son\u00b7nen\u00b7st\u00e4ub\u00b7chen", "Gl\u00fcck", ":", "so", "f\u00fchlt", "ich", "mich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$.", "ADV", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und durch die Welten wirbelte ich hin;", "tokens": ["Und", "durch", "die", "Wel\u00b7ten", "wir\u00b7bel\u00b7te", "ich", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Licht war mein Herz, und meine Augen Glanz.", "tokens": ["Licht", "war", "mein", "Herz", ",", "und", "mei\u00b7ne", "Au\u00b7gen", "Glanz", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$,", "KON", "PPOSAT", "NN", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "Die Wiese mit den Blumen ... Langsam schritt", "tokens": ["Die", "Wie\u00b7se", "mit", "den", "Blu\u00b7men", "...", "Lang\u00b7sam", "schritt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$(", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich durch das gr\u00fcne Rauschemeer, ich f\u00fchrte", "tokens": ["Ich", "durch", "das", "gr\u00fc\u00b7ne", "Rau\u00b7sche\u00b7meer", ",", "ich", "f\u00fchr\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Am Arm ein M\u00e4dchen, und an meiner Brust", "tokens": ["Am", "Arm", "ein", "M\u00e4d\u00b7chen", ",", "und", "an", "mei\u00b7ner", "Brust"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "KON", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "H\u00f6rt ich ein Klopfen, das wie Liebe klang,", "tokens": ["H\u00f6rt", "ich", "ein", "Klop\u00b7fen", ",", "das", "wie", "Lie\u00b7be", "klang", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,", "PRELS", "KOKOM", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So fragend zag und bittebang und tief;", "tokens": ["So", "fra\u00b7gend", "zag", "und", "bit\u00b7te\u00b7bang", "und", "tief", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "KON", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und zweier Augen hei\u00dfe Seligkeit,", "tokens": ["Und", "zwei\u00b7er", "Au\u00b7gen", "hei\u00b7\u00dfe", "Se\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ein Rosenhimmel, aller Gnaden voll,", "tokens": ["Ein", "Ro\u00b7sen\u00b7him\u00b7mel", ",", "al\u00b7ler", "Gna\u00b7den", "voll", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Sah mir ins Herz und hellte mir ein Gl\u00fcck,", "tokens": ["Sah", "mir", "ins", "Herz", "und", "hell\u00b7te", "mir", "ein", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "KON", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Das nie ich wu\u00dfte, das mein Sehnen war", "tokens": ["Das", "nie", "ich", "wu\u00df\u00b7te", ",", "das", "mein", "Seh\u00b7nen", "war"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "PPER", "VVFIN", "$,", "PRELS", "PPOSAT", "NN", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Durch lange, arme, liebeleere Zeit.", "tokens": ["Durch", "lan\u00b7ge", ",", "ar\u00b7me", ",", "lie\u00b7be\u00b7lee\u00b7re", "Zeit", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADV", "$,", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Das war die Liebe.", "tokens": ["Das", "war", "die", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Leise wie ein Traum", "tokens": ["Lei\u00b7se", "wie", "ein", "Traum"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Ist sie durch Seele mir und Sinn geweht,", "tokens": ["Ist", "sie", "durch", "See\u00b7le", "mir", "und", "Sinn", "ge\u00b7weht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "PPER", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und ich war selig. Rosen sah ich rings,", "tokens": ["Und", "ich", "war", "se\u00b7lig", ".", "Ro\u00b7sen", "sah", "ich", "rings", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "$.", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-++--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und Rosen deckten mir die ganze Welt,", "tokens": ["Und", "Ro\u00b7sen", "deck\u00b7ten", "mir", "die", "gan\u00b7ze", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Welt voll Gr\u00e4uel, Traumesrosen deckten", "tokens": ["Die", "Welt", "voll", "Gr\u00e4u\u00b7el", ",", "Trau\u00b7mes\u00b7ro\u00b7sen", "deck\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADJD", "NN", "$,", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mit Bl\u00fctenranken mir die Wahrheit zu.", "tokens": ["Mit", "Bl\u00fc\u00b7ten\u00b7ran\u00b7ken", "mir", "die", "Wahr\u00b7heit", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Die Sonne sah ich nur: ich sah nur dich;", "tokens": ["Die", "Son\u00b7ne", "sah", "ich", "nur", ":", "ich", "sah", "nur", "dich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$.", "PPER", "VVFIN", "ADV", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Augen gingen \u00fcber mir vor Glanz,", "tokens": ["Die", "Au\u00b7gen", "gin\u00b7gen", "\u00fc\u00b7ber", "mir", "vor", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ergie\u00dfen wollte sich das Herz vor Gl\u00fcck,", "tokens": ["Er\u00b7gie\u00b7\u00dfen", "woll\u00b7te", "sich", "das", "Herz", "vor", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PRF", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bang \u00fcberselig str\u00f6men in den Tod, \u2013", "tokens": ["Bang", "\u00fc\u00b7ber\u00b7se\u00b7lig", "str\u00f6\u00b7men", "in", "den", "Tod", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ADJD", "VVFIN", "APPR", "ART", "NN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da wacht ich pl\u00f6tzlich unter Thr\u00e4nen auf.", "tokens": ["Da", "wacht", "ich", "pl\u00f6tz\u00b7lich", "un\u00b7ter", "Thr\u00e4\u00b7nen", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Was ich als Sonne selig angesehn,", "tokens": ["Was", "ich", "als", "Son\u00b7ne", "se\u00b7lig", "an\u00b7ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "KOUS", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Als aller Liebe, aller Sch\u00f6nheit Herd \u2013", "tokens": ["Als", "al\u00b7ler", "Lie\u00b7be", ",", "al\u00b7ler", "Sch\u00f6n\u00b7heit", "Herd", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "$,", "PIAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein einziger Blick verriet mir blitzesgrell,", "tokens": ["Ein", "ein\u00b7zi\u00b7ger", "Blick", "ver\u00b7riet", "mir", "blit\u00b7zes\u00b7grell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Da\u00df eine L\u00fcge meine Sonne war,", "tokens": ["Da\u00df", "ei\u00b7ne", "L\u00fc\u00b7ge", "mei\u00b7ne", "Son\u00b7ne", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein sch\u00f6ner, b\u00f6ser, liebeleerer Stern.", "tokens": ["Ein", "sch\u00f6\u00b7ner", ",", "b\u00f6\u00b7ser", ",", "lie\u00b7be\u00b7lee\u00b7rer", "Stern", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Der Traum ist aus. Ich starre in mein Herz,", "tokens": ["Der", "Traum", "ist", "aus", ".", "Ich", "star\u00b7re", "in", "mein", "Herz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKVZ", "$.", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich weine in mein Herz: die Thr\u00e4ne f\u00e4llt", "tokens": ["Ich", "wei\u00b7ne", "in", "mein", "Herz", ":", "die", "Thr\u00e4\u00b7ne", "f\u00e4llt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In einen Krater, krustig ausgebrannt.", "tokens": ["In", "ei\u00b7nen", "Kra\u00b7ter", ",", "krus\u00b7tig", "aus\u00b7ge\u00b7brannt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der hei\u00dfe Lavastrom der Liebe ward", "tokens": ["Der", "hei\u00b7\u00dfe", "La\u00b7vast\u00b7rom", "der", "Lie\u00b7be", "ward"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Zu Stein.", "tokens": ["Zu", "Stein", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.10": {"line.1": {"text": "Ich will die Tage nutzen. Kalt", "tokens": ["Ich", "will", "die", "Ta\u00b7ge", "nut\u00b7zen", ".", "Kalt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$.", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Will deine L\u00fcge ich einmei\u00dfeln ihm.", "tokens": ["Will", "dei\u00b7ne", "L\u00fc\u00b7ge", "ich", "ein\u00b7mei\u00b7\u00dfeln", "ihm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}