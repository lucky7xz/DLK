{"textgrid.poem.58774": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du tr\u00e4gst die B\u00f6sen; o Geduld!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du tr\u00e4gst die B\u00f6sen; o Geduld!", "tokens": ["Du", "tr\u00e4gst", "die", "B\u00f6\u00b7sen", ";", "o", "Ge\u00b7duld", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "FM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du strafst nicht bald, nicht wie's verschuld't,", "tokens": ["Du", "strafst", "nicht", "bald", ",", "nicht", "wie's", "ver\u00b7schuld't", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "$,", "PTKNEG", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Wohltun unerm\u00fcdet bleibet.", "tokens": ["Dein", "Wohl\u00b7tun", "un\u00b7er\u00b7m\u00fc\u00b7det", "blei\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du rufst und lockst so g\u00fctiglich,", "tokens": ["Du", "rufst", "und", "lockst", "so", "g\u00fc\u00b7tig\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du wartest so langm\u00fctiglich,", "tokens": ["Du", "war\u00b7test", "so", "lang\u00b7m\u00fc\u00b7tig\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bis sich das Herz dir ganz verschreibet.", "tokens": ["Bis", "sich", "das", "Herz", "dir", "ganz", "ver\u00b7schrei\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "O Gotteslangmut, o Geduld,", "tokens": ["O", "Got\u00b7tes\u00b7lang\u00b7mut", ",", "o", "Ge\u00b7duld", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ich brauch' noch t\u00e4glich deiner Huld.", "tokens": ["Ich", "brauch'", "noch", "t\u00e4g\u00b7lich", "dei\u00b7ner", "Huld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Halleluja! Halleluja!", "tokens": ["Hal\u00b7le\u00b7lu\u00b7ja", "!", "Hal\u00b7le\u00b7lu\u00b7ja", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ITJ", "$.", "ITJ", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}}}}