{"textgrid.poem.24895": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Umrank' mich fester, duftendes Gestr\u00e4uch!", "tokens": ["Um\u00b7rank'", "mich", "fes\u00b7ter", ",", "duf\u00b7ten\u00b7des", "Ge\u00b7str\u00e4uch", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "W\u00f6lbt \u00fcber mir, ihr luft'gen Bogen, euch", "tokens": ["W\u00f6lbt", "\u00fc\u00b7ber", "mir", ",", "ihr", "luft'\u00b7gen", "Bo\u00b7gen", ",", "euch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "APPR", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zu einer Halle sel'ger Tr\u00e4ume!", "tokens": ["Zu", "ei\u00b7ner", "Hal\u00b7le", "sel'\u00b7ger", "Tr\u00e4u\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Brich, frischer Wind, aus der Limonenschlucht", "tokens": ["Brich", ",", "fri\u00b7scher", "Wind", ",", "aus", "der", "Li\u00b7mo\u00b7nen\u00b7schlucht"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "ADJA", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und sch\u00fcttle mir die Lust, die reife Frucht,", "tokens": ["Und", "sch\u00fctt\u00b7le", "mir", "die", "Lust", ",", "die", "rei\u00b7fe", "Frucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Vom Wipfel der Orangenb\u00e4ume!", "tokens": ["Vom", "Wip\u00b7fel", "der", "O\u00b7ran\u00b7gen\u00b7b\u00e4u\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Komm, Lebensspender! Komm, ersehnter Ost!", "tokens": ["Komm", ",", "Le\u00b7bens\u00b7spen\u00b7der", "!", "Komm", ",", "er\u00b7sehn\u00b7ter", "Ost", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$.", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den Becher f\u00fclle mir mit Freudenmost", "tokens": ["Den", "Be\u00b7cher", "f\u00fcl\u00b7le", "mir", "mit", "Freu\u00b7den\u00b7most"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Im Purpurquell der Morgenr\u00f6te,", "tokens": ["Im", "Pur\u00b7pur\u00b7quell", "der", "Mor\u00b7gen\u00b7r\u00f6\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und gie\u00df den Fr\u00fchling auf den Rosenstrauch,", "tokens": ["Und", "gie\u00df", "den", "Fr\u00fch\u00b7ling", "auf", "den", "Ro\u00b7sen\u00b7strauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da\u00df Knosp' an Knospe sich mit Balsamhauch", "tokens": ["Da\u00df", "Knosp'", "an", "Knos\u00b7pe", "sich", "mit", "Bal\u00b7sam\u00b7hauch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPR", "NN", "PRF", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Erschlie\u00dft beim Nachtigallgefl\u00f6te!", "tokens": ["Er\u00b7schlie\u00dft", "beim", "Nach\u00b7ti\u00b7gall\u00b7ge\u00b7fl\u00f6\u00b7te", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ihr Genien dieser Zauberburg, erwacht!", "tokens": ["Ihr", "Ge\u00b7ni\u00b7en", "die\u00b7ser", "Zau\u00b7ber\u00b7burg", ",", "er\u00b7wacht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "PDAT", "NN", "$,", "VVPP", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Das Haupt erhebt aus eurer Grabesnacht,", "tokens": ["Das", "Haupt", "er\u00b7hebt", "aus", "eu\u00b7rer", "Gra\u00b7bes\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Um eure Stirne Rosenkr\u00e4nze!", "tokens": ["Um", "eu\u00b7re", "Stir\u00b7ne", "Ro\u00b7sen\u00b7kr\u00e4n\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aus Hallen und Gew\u00f6lben steigt hervor", "tokens": ["Aus", "Hal\u00b7len", "und", "Ge\u00b7w\u00f6l\u00b7ben", "steigt", "her\u00b7vor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und ruft die sch\u00f6ne alte Zeit empor,", "tokens": ["Und", "ruft", "die", "sch\u00f6\u00b7ne", "al\u00b7te", "Zeit", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df sie erbl\u00fcht in neuem Lenze!", "tokens": ["Da\u00df", "sie", "er\u00b7bl\u00fcht", "in", "neu\u00b7em", "Len\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Sch\u00e4tze hebt mir, die in goldnen Truhn,", "tokens": ["Die", "Sch\u00e4t\u00b7ze", "hebt", "mir", ",", "die", "in", "gold\u00b7nen", "Truhn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "PRELS", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von Geisterhut bewacht, im Boden ruhn!", "tokens": ["Von", "Geis\u00b7ter\u00b7hut", "be\u00b7wacht", ",", "im", "Bo\u00b7den", "ruhn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Taucht in das Becken der Cisterne", "tokens": ["Taucht", "in", "das", "Be\u00b7cken", "der", "Cis\u00b7ter\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "++-+--+--", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Und sch\u00f6pft in Schalen von Krystall den Quell", "tokens": ["Und", "sch\u00f6pft", "in", "Scha\u00b7len", "von", "Krys\u00b7tall", "den", "Quell"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "APPR", "NN", "ART", "NN"], "meter": "-+-+--++-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Des alten Gl\u00fccks, der drinnen flutet, hell", "tokens": ["Des", "al\u00b7ten", "Gl\u00fccks", ",", "der", "drin\u00b7nen", "flu\u00b7tet", ",", "hell"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Vom Schimmer unterird'scher Sterne!", "tokens": ["Vom", "Schim\u00b7mer", "un\u00b7ter\u00b7ird'\u00b7scher", "Ster\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Sie nahen, ja sie nahen, die ich rief!", "tokens": ["Sie", "na\u00b7hen", ",", "ja", "sie", "na\u00b7hen", ",", "die", "ich", "rief", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "ADV", "PPER", "ADJA", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Hoffnung, die an jenem Brunnen schlief,", "tokens": ["Die", "Hoff\u00b7nung", ",", "die", "an", "je\u00b7nem", "Brun\u00b7nen", "schlief", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Erhebt sich mit dem Lilienstabe;", "tokens": ["Er\u00b7hebt", "sich", "mit", "dem", "Li\u00b7li\u00b7en\u00b7sta\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Auf ihren Wink erstehn in bunter Schar", "tokens": ["Auf", "ih\u00b7ren", "Wink", "er\u00b7stehn", "in", "bun\u00b7ter", "Schar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Freuden mit dem Silberfl\u00fcgelpaar", "tokens": ["Die", "Freu\u00b7den", "mit", "dem", "Sil\u00b7ber\u00b7fl\u00fc\u00b7gel\u00b7paar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Aus dem jahrhundertalten Grabe.", "tokens": ["Aus", "dem", "jahr\u00b7hun\u00b7der\u00b7tal\u00b7ten", "Gra\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.6": {"line.1": {"text": "Und andre Geister viel, ein luft'ger Schwarm,", "tokens": ["Und", "and\u00b7re", "Geis\u00b7ter", "viel", ",", "ein", "luft'\u00b7ger", "Schwarm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADV", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nahn mir; es nahen W\u00fcnsche Arm in Arm", "tokens": ["Nahn", "mir", ";", "es", "na\u00b7hen", "W\u00fcn\u00b7sche", "Arm", "in", "Arm"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$.", "PPER", "ADJA", "NN", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und Tr\u00e4ume und Erinnerungen;", "tokens": ["Und", "Tr\u00e4u\u00b7me", "und", "E\u00b7rin\u00b7ne\u00b7run\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Echo alter Stimmen weckt ihr Tritt;", "tokens": ["Das", "E\u00b7cho", "al\u00b7ter", "Stim\u00b7men", "weckt", "ihr", "Tritt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sie bringen lang verschollne Lieder mit,", "tokens": ["Sie", "brin\u00b7gen", "lang", "ver\u00b7scholl\u00b7ne", "Lie\u00b7der", "mit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die einst in diesem Saal geklungen.", "tokens": ["Die", "einst", "in", "die\u00b7sem", "Saal", "ge\u00b7klun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Und Bilder seh' ich, wie ich nie gesehn,", "tokens": ["Und", "Bil\u00b7der", "seh'", "ich", ",", "wie", "ich", "nie", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "$,", "PWAV", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und Stimmen h\u00f6r' ich unsichtbarer Feen;", "tokens": ["Und", "Stim\u00b7men", "h\u00f6r'", "ich", "un\u00b7sicht\u00b7ba\u00b7rer", "Feen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Rauschen t\u00f6nt wie Fl\u00fcgelschlagen!", "tokens": ["Ein", "Rau\u00b7schen", "t\u00f6nt", "wie", "Fl\u00fc\u00b7gel\u00b7schla\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo bin ich? Wird des Schlosses Wunderbau", "tokens": ["Wo", "bin", "ich", "?", "Wird", "des", "Schlos\u00b7ses", "Wun\u00b7der\u00b7bau"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "$.", "VAFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Hoch \u00fcber Land und Meer durchs Aetherblau", "tokens": ["Hoch", "\u00fc\u00b7ber", "Land", "und", "Meer", "durchs", "A\u00b7e\u00b7ther\u00b7blau"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "APPRART", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Von Geisterhand dahingetragen?", "tokens": ["Von", "Geis\u00b7ter\u00b7hand", "da\u00b7hin\u00b7ge\u00b7tra\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Zum Himmel hebt es mich in m\u00e4cht'gem Schwung;", "tokens": ["Zum", "Him\u00b7mel", "hebt", "es", "mich", "in", "m\u00e4cht'\u00b7gem", "Schwung", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Tief unten sinkt die Welt in D\u00e4mmerung;", "tokens": ["Tief", "un\u00b7ten", "sinkt", "die", "Welt", "in", "D\u00e4m\u00b7me\u00b7rung", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Ich atme frei von Erdenbanden;", "tokens": ["Ich", "at\u00b7me", "frei", "von", "Er\u00b7den\u00b7ban\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Gl\u00fccke sonn' ich mich, dem ew'gen Tag,", "tokens": ["Im", "Gl\u00fc\u00b7cke", "sonn'", "ich", "mich", ",", "dem", "ew'\u00b7gen", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "PPER", "PRF", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und h\u00f6re nur von fern den Wellenschlag", "tokens": ["Und", "h\u00f6\u00b7re", "nur", "von", "fern", "den", "Wel\u00b7len\u00b7schlag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Des Lebens aus der Tiefe branden.", "tokens": ["Des", "Le\u00b7bens", "aus", "der", "Tie\u00b7fe", "bran\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}