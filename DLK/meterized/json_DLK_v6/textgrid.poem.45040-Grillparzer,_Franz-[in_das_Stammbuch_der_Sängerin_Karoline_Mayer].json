{"textgrid.poem.45040": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[in das Stammbuch der S\u00e4ngerin Karoline Mayer]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich stand an deiner Wiege \u2013", "tokens": ["Ich", "stand", "an", "dei\u00b7ner", "Wie\u00b7ge", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hei\u00dft das, an der Wiege der Kunst,", "tokens": ["Hei\u00dft", "das", ",", "an", "der", "Wie\u00b7ge", "der", "Kunst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "$,", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Und sah dich wachsen und reifen,", "tokens": ["Und", "sah", "dich", "wach\u00b7sen", "und", "rei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Umstrahlt von des Himmels Gunst.", "tokens": ["Um\u00b7strahlt", "von", "des", "Him\u00b7mels", "Gunst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Die Schleier hab ich durchdrungen,", "tokens": ["Die", "Schlei\u00b7er", "hab", "ich", "durch\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Mit denen du gern dich umh\u00fcllst,", "tokens": ["Mit", "de\u00b7nen", "du", "gern", "dich", "um\u00b7h\u00fcllst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Und was ich dort las, das wei\u00df ich:", "tokens": ["Und", "was", "ich", "dort", "las", ",", "das", "wei\u00df", "ich", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VVFIN", "$,", "PDS", "VVFIN", "PPER", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Du kannst, was immer du willst.", "tokens": ["Du", "kannst", ",", "was", "im\u00b7mer", "du", "willst", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PRELS", "ADV", "PPER", "VMFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}