{"textgrid.poem.53090": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Lesbia tota favet", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lesbia, mein Leben,", "tokens": ["Les\u00b7bia", ",", "mein", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Hat sich mir ergeben", "tokens": ["Hat", "sich", "mir", "er\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PRF", "PPER", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "In gew\u00fcnschter Pflicht,", "tokens": ["In", "ge\u00b7w\u00fcnschter", "Pflicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Ich wil bey jhr stehen,", "tokens": ["Ich", "wil", "bey", "jhr", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Bi\u00df ich werde gehen", "tokens": ["Bi\u00df", "ich", "wer\u00b7de", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VAFIN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Hie au\u00df diesem Licht:", "tokens": ["Hie", "au\u00df", "die\u00b7sem", "Licht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Was vor Leid", "tokens": ["Was", "vor", "Leid"], "token_info": ["word", "word", "word"], "pos": ["PWS", "APPR", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Ich jeder zeit", "tokens": ["Ich", "je\u00b7der", "zeit"], "token_info": ["word", "word", "word"], "pos": ["PPER", "PIAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Vmb sie hab' ertragen m\u00fcssen,", "tokens": ["Vmb", "sie", "hab'", "er\u00b7tra\u00b7gen", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VAFIN", "VVINF", "VMINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Wil ich jetzt beschliessen.", "tokens": ["Wil", "ich", "jetzt", "be\u00b7schlies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Die gew\u00fcnschten Frewden,", "tokens": ["Die", "ge\u00b7w\u00fcnschten", "Frew\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "So sie vor mein Leiden", "tokens": ["So", "sie", "vor", "mein", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Mir ertheilen wil,", "tokens": ["Mir", "er\u00b7thei\u00b7len", "wil", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VMFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Sol kein Leid beschweren,", "tokens": ["Sol", "kein", "Leid", "be\u00b7schwe\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Ja sie sollen wehren", "tokens": ["Ja", "sie", "sol\u00b7len", "weh\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKANT", "PPER", "VMFIN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Ohne maa\u00df vnd Ziel:", "tokens": ["Oh\u00b7ne", "maa\u00df", "vnd", "Ziel", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Ihre Zier", "tokens": ["Ih\u00b7re", "Zier"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Wil einig mir", "tokens": ["Wil", "ei\u00b7nig", "mir"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "ADJD", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Sich in allen Liebes-f\u00e4llen", "tokens": ["Sich", "in", "al\u00b7len", "Lie\u00b7bes\u00b7f\u00e4l\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Zn gebothe stellen.", "tokens": ["Zn", "ge\u00b7bo\u00b7the", "stel\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Aller Pracht vnd Prangen", "tokens": ["Al\u00b7ler", "Pracht", "vnd", "Pran\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ihrer s\u00fcssen Wangen,", "tokens": ["Ih\u00b7rer", "s\u00fcs\u00b7sen", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ihr Korallen-Mund:", "tokens": ["Ihr", "Ko\u00b7ral\u00b7len\u00b7Mund", ":"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Ihre zarten H\u00e4nde,", "tokens": ["Ih\u00b7re", "zar\u00b7ten", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Ihrer Armen B\u00e4nde", "tokens": ["Ih\u00b7rer", "Ar\u00b7men", "B\u00e4n\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Sind mir nun vergunt:", "tokens": ["Sind", "mir", "nun", "ver\u00b7gunt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Ehe mu\u00df", "tokens": ["E\u00b7he", "mu\u00df"], "token_info": ["word", "word"], "pos": ["NN", "VMFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Ein vberflu\u00df", "tokens": ["Ein", "vberf\u00b7lu\u00df"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Als ein Mangel in den Sachen", "tokens": ["Als", "ein", "Man\u00b7gel", "in", "den", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Mich verdrossen machen.", "tokens": ["Mich", "ver\u00b7dros\u00b7sen", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Sind im Obst viel Kerne,", "tokens": ["Sind", "im", "Obst", "viel", "Ker\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Viel am Himmel Sterne,", "tokens": ["Viel", "am", "Him\u00b7mel", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wirfft der Nord viel Schnee:", "tokens": ["Wirfft", "der", "Nord", "viel", "Schnee", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PIAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Sind viel rauhe Wellen,", "tokens": ["Sind", "viel", "rau\u00b7he", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Wenn die Wind bellen", "tokens": ["Wenn", "die", "Wind", "bel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVINF"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.6": {"text": "Auff der w\u00fcsten See:", "tokens": ["Auff", "der", "w\u00fcs\u00b7ten", "See", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVFIN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Mehr sind K\u00fcss,", "tokens": ["Mehr", "sind", "K\u00fcss", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Ich wei\u00df gewi\u00df,", "tokens": ["Ich", "wei\u00df", "ge\u00b7wi\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Die sie mir zum Liebes Zeichen", "tokens": ["Die", "sie", "mir", "zum", "Lie\u00b7bes", "Zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PPER", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Wird mit Willen reichen.", "tokens": ["Wird", "mit", "Wil\u00b7len", "rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Solt ich solcher massen", "tokens": ["Solt", "ich", "sol\u00b7cher", "mas\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PIAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Mich gerewen lassen", "tokens": ["Mich", "ge\u00b7re\u00b7wen", "las\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVPP", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Meiner Sorg vnd Pein?", "tokens": ["Mei\u00b7ner", "Sorg", "vnd", "Pein", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Wer auff sein Verdriessen", "tokens": ["Wer", "auff", "sein", "Ver\u00b7dries\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "APPR", "PPOSAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Die\u00df hat zu geniessen,", "tokens": ["Die\u00df", "hat", "zu", "ge\u00b7nies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Kan nicht Elend seyn:", "tokens": ["Kan", "nicht", "E\u00b7lend", "seyn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "VVPP", "VAINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Elend kan", "tokens": ["E\u00b7lend", "kan"], "token_info": ["word", "word"], "pos": ["NN", "VMFIN"], "meter": "---", "measure": "unknown.measure.zero"}, "line.8": {"text": "Nicht sein der Mann,", "tokens": ["Nicht", "sein", "der", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Dem sein Lieb auff alles Leiden", "tokens": ["Dem", "sein", "Lieb", "auff", "al\u00b7les", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Lohnt mit solchen Frewden.", "tokens": ["Lohnt", "mit", "sol\u00b7chen", "Frew\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}