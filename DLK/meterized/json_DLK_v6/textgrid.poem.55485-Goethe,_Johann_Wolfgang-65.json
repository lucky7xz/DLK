{"textgrid.poem.55485": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "65", "genre": "verse", "period": "N.A.", "pub_year": 1818, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zarte, schattende Gebilde,", "tokens": ["Zar\u00b7te", ",", "schat\u00b7ten\u00b7de", "Ge\u00b7bil\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fliegt zu eurer K\u00fcnstlerin,", "tokens": ["Fliegt", "zu", "eu\u00b7rer", "K\u00fcnst\u00b7le\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df sie, freundlich, froh und milde,", "tokens": ["Da\u00df", "sie", ",", "freund\u00b7lich", ",", "froh", "und", "mil\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Immer sich nach ihrem Sinn", "tokens": ["Im\u00b7mer", "sich", "nach", "ih\u00b7rem", "Sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PRF", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Eine Welt von Schatten bilde;", "tokens": ["Ei\u00b7ne", "Welt", "von", "Schat\u00b7ten", "bil\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Denn das irdische Gefilde", "tokens": ["Denn", "das", "ir\u00b7di\u00b7sche", "Ge\u00b7fil\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Schattet oft nach eignem Sinn.", "tokens": ["Schat\u00b7tet", "oft", "nach", "eig\u00b7nem", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}