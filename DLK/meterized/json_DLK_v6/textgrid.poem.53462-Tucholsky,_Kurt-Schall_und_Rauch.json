{"textgrid.poem.53462": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Schall und Rauch", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Name ists, der Menschen zieret,", "tokens": ["Der", "Na\u00b7me", "ists", ",", "der", "Men\u00b7schen", "zie\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "weil er das Erdenpack sortieret \u2013", "tokens": ["weil", "er", "das", "Er\u00b7den\u00b7pack", "sor\u00b7tie\u00b7ret", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "bist du auch d\u00e4mlich, schief und krumm:", "tokens": ["bist", "du", "auch", "d\u00e4m\u00b7lich", ",", "schief", "und", "krumm", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$,", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du bist ein Individuum.", "tokens": ["Du", "bist", "ein", "In\u00b7di\u00b7vi\u00b7du\u00b7um", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Hier sieht man nun den Dichter walten.", "tokens": ["Hier", "sieht", "man", "nun", "den", "Dich\u00b7ter", "wal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er schafft nicht nur die Dichtgestalten,", "tokens": ["Er", "schafft", "nicht", "nur", "die", "Dicht\u00b7ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "nein, er benamset auch sein Kind \u2013", "tokens": ["nein", ",", "er", "be\u00b7nam\u00b7set", "auch", "sein", "Kind", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und nennt es Borkman oder Gynt.", "tokens": ["und", "nennt", "es", "Bork\u00b7man", "o\u00b7der", "Gynt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NE", "KON", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie aber, wenn er in den Dramen", "tokens": ["Wie", "a\u00b7ber", ",", "wenn", "er", "in", "den", "Dra\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "$,", "KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "gediegne b\u00fcrgerliche Namen", "tokens": ["ge\u00b7dieg\u00b7ne", "b\u00fcr\u00b7ger\u00b7li\u00b7che", "Na\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "benutzt und jener B\u00fcrger klagt,", "tokens": ["be\u00b7nutzt", "und", "je\u00b7ner", "B\u00fcr\u00b7ger", "klagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "damits der Richter untersagt?", "tokens": ["da\u00b7mits", "der", "Rich\u00b7ter", "un\u00b7ter\u00b7sagt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbdu wirst dich von dem Namen trennen!", "tokens": ["\u00bb", "du", "wirst", "dich", "von", "dem", "Na\u00b7men", "tren\u00b7nen", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mu\u00dft du ihn grade Barnhelm nennen?\u00ab", "tokens": ["Mu\u00dft", "du", "ihn", "gra\u00b7de", "Bar\u00b7nhelm", "nen\u00b7nen", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "NN", "VVINF", "$.", "$("], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der Richter sch\u00fcttelt das Barett:", "tokens": ["Der", "Rich\u00b7ter", "sch\u00fct\u00b7telt", "das", "Ba\u00b7rett", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbder Name macht den Kohl nicht fett!\u00ab", "tokens": ["\u00bb", "der", "Na\u00b7me", "macht", "den", "Kohl", "nicht", "fett", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "ART", "NN", "PTKNEG", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und kurz: Wir werden was ertragen!", "tokens": ["Und", "kurz", ":", "Wir", "wer\u00b7den", "was", "er\u00b7tra\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$.", "PPER", "VAFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schon sieht man Doktor Tassow klagen,", "tokens": ["Schon", "sieht", "man", "Dok\u00b7tor", "Tas\u00b7sow", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "NN", "NE", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "mit ihm in trautestem Verein", "tokens": ["mit", "ihm", "in", "trau\u00b7tes\u00b7tem", "Ver\u00b7ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "den Gr\u00fcnkramh\u00e4ndler Wallenstein.", "tokens": ["den", "Gr\u00fcn\u00b7kram\u00b7h\u00e4nd\u00b7ler", "Wal\u00b7len\u00b7stein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Dem Dichter f\u00e4llt in seine Leier", "tokens": ["Dem", "Dich\u00b7ter", "f\u00e4llt", "in", "sei\u00b7ne", "Lei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "auch der Ap'theker Florian Geyer \u2013", "tokens": ["auch", "der", "Ap'\u00b7the\u00b7ker", "Flo\u00b7ri\u00b7an", "Ge\u00b7yer", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NE", "NE", "$("], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "dem Dichter grausts mit einem Mal:", "tokens": ["dem", "Dich\u00b7ter", "grausts", "mit", "ei\u00b7nem", "Mal", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er numeriert sein Personal.", "tokens": ["Er", "nu\u00b7me\u00b7riert", "sein", "Per\u00b7so\u00b7nal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wie nennt man nun die Rechtsgelehrten,", "tokens": ["Wie", "nennt", "man", "nun", "die", "Rechts\u00b7ge\u00b7lehr\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PIS", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "die uns mit diesem Spruch beehrten?", "tokens": ["die", "uns", "mit", "die\u00b7sem", "Spruch", "be\u00b7ehr\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie nennt man also dies Gericht?", "tokens": ["Wie", "nennt", "man", "al\u00b7so", "dies", "Ge\u00b7richt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PIS", "ADV", "PDS", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hier wei\u00df ich keinen Namen nicht.", "tokens": ["Hier", "wei\u00df", "ich", "kei\u00b7nen", "Na\u00b7men", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}