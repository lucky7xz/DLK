{"textgrid.poem.35446": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sind mir die Schwingen denn gebrochen?", "tokens": ["Sind", "mir", "die", "Schwin\u00b7gen", "denn", "ge\u00b7bro\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "KON", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist mir die alte Kraft verraucht,", "tokens": ["Ist", "mir", "die", "al\u00b7te", "Kraft", "ver\u00b7raucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df ich nicht mehr des Herzens unstet Pochen,", "tokens": ["Da\u00df", "ich", "nicht", "mehr", "des", "Her\u00b7zens", "un\u00b7stet", "Po\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und was aus seinen dunklen Tiefen taucht", "tokens": ["Und", "was", "aus", "sei\u00b7nen", "dunk\u00b7len", "Tie\u00b7fen", "taucht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "APPR", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "In buntem, vielgestalt'gem Reigen,", "tokens": ["In", "bun\u00b7tem", ",", "viel\u00b7ge\u00b7stalt'\u00b7gem", "Rei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bemeistern kann? Schlo\u00df schon das Schweigen", "tokens": ["Be\u00b7meis\u00b7tern", "kann", "?", "Schlo\u00df", "schon", "das", "Schwei\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "$.", "NN", "ADV", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Die Dichterlippe \u2013 jenes gro\u00dfe Schweigen,", "tokens": ["Die", "Dich\u00b7ter\u00b7lip\u00b7pe", "\u2013", "je\u00b7nes", "gro\u00b7\u00dfe", "Schwei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Das Ekel, Ueberdru\u00df, Melancholie", "tokens": ["Das", "E\u00b7kel", ",", "Ue\u00b7berd\u00b7ru\u00df", ",", "Me\u00b7lan\u00b7cho\u00b7lie"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und Lebensunrast gro\u00dfs\u00e4ugt in der Brust?", "tokens": ["Und", "Le\u00b7ben\u00b7sun\u00b7rast", "gro\u00df\u00b7s\u00e4ugt", "in", "der", "Brust", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Verspr\u00fchte mir schon alle Jugendlust?", "tokens": ["Ver\u00b7spr\u00fch\u00b7te", "mir", "schon", "al\u00b7le", "Ju\u00b7gend\u00b7lust", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Verlor ihr Diadem die Poesie?", "tokens": ["Ver\u00b7lor", "ihr", "Di\u00b7a\u00b7dem", "die", "Poe\u00b7sie", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.12": {"text": "Sind meine Wurzeln welk? Mein Stamm verdorrt?", "tokens": ["Sind", "mei\u00b7ne", "Wur\u00b7zeln", "welk", "?", "Mein", "Stamm", "ver\u00b7dorrt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PTKVZ", "$.", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Mein Laub von tauber Asche \u00fcberstaubt?", "tokens": ["Mein", "Laub", "von", "tau\u00b7ber", "A\u00b7sche", "\u00fc\u00b7bers\u00b7taubt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Ich treibe fort und fort", "tokens": ["Ich", "trei\u00b7be", "fort", "und", "fort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "In einem uferlosen Ozean,", "tokens": ["In", "ei\u00b7nem", "u\u00b7fer\u00b7lo\u00b7sen", "O\u00b7ze\u00b7an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Gebeugt das Haupt,", "tokens": ["Ge\u00b7beugt", "das", "Haupt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.17": {"text": "Das Auge stier und brennend, tr\u00e4nenlos ...", "tokens": ["Das", "Au\u00b7ge", "stier", "und", "bren\u00b7nend", ",", "tr\u00e4\u00b7nen\u00b7los", "..."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$,", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Jedwedes Menschenlos", "tokens": ["Jed\u00b7we\u00b7des", "Men\u00b7schen\u00b7los"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "D\u00fcnkt mich nur ein Gewirr von Trug und Wahn,", "tokens": ["D\u00fcnkt", "mich", "nur", "ein", "Ge\u00b7wirr", "von", "Trug", "und", "Wahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Drin Afterweisheit, bl\u00f6der Aberwitz", "tokens": ["Drin", "Af\u00b7ter\u00b7weis\u00b7heit", ",", "bl\u00f6\u00b7der", "A\u00b7ber\u00b7witz"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Gehalt und Sinn und tiefre Ordnung finden ...", "tokens": ["Ge\u00b7halt", "und", "Sinn", "und", "tief\u00b7re", "Ord\u00b7nung", "fin\u00b7den", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Und zuckt einmal in diese z\u00e4he Nacht", "tokens": ["Und", "zuckt", "ein\u00b7mal", "in", "die\u00b7se", "z\u00e4\u00b7he", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Blutrot ein Blitz", "tokens": ["Blut\u00b7rot", "ein", "Blitz"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.24": {"text": "Aus einer h\u00f6hern Zone:", "tokens": ["Aus", "ei\u00b7ner", "h\u00f6\u00b7hern", "Zo\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Dann schau ich Frevel nur und S\u00fcnden", "tokens": ["Dann", "schau", "ich", "Fre\u00b7vel", "nur", "und", "S\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "ADV", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Und Schmach und Ohnmacht allerw\u00e4rts ...", "tokens": ["Und", "Schmach", "und", "Ohn\u00b7macht", "al\u00b7ler\u00b7w\u00e4rts", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Und dem zertretnen Schmerzenssohne", "tokens": ["Und", "dem", "zer\u00b7tret\u00b7nen", "Schmer\u00b7zens\u00b7soh\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Entschl\u00e4gt sich seines letzten Hoffnungsschimmers", "tokens": ["Ent\u00b7schl\u00e4gt", "sich", "sei\u00b7nes", "letz\u00b7ten", "Hoff\u00b7nungs\u00b7schim\u00b7mers"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Das zerborstne Herz ...", "tokens": ["Das", "zer\u00b7borst\u00b7ne", "Herz", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Was mich bewegt", "tokens": ["Was", "mich", "be\u00b7wegt"], "token_info": ["word", "word", "word"], "pos": ["PWS", "PPER", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "In meiner Jugend gro\u00dfen Schw\u00e4rmertagen:", "tokens": ["In", "mei\u00b7ner", "Ju\u00b7gend", "gro\u00b7\u00dfen", "Schw\u00e4r\u00b7mer\u00b7ta\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich mu\u00df ihm tr\u00e4nenlos entsagen ...", "tokens": ["Ich", "mu\u00df", "ihm", "tr\u00e4\u00b7nen\u00b7los", "ent\u00b7sa\u00b7gen", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Und das gewaltiger denn Raum und Zeit,", "tokens": ["Und", "das", "ge\u00b7wal\u00b7ti\u00b7ger", "denn", "Raum", "und", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Drin alles Sein sich hell und klar begreift:", "tokens": ["Drin", "al\u00b7les", "Sein", "sich", "hell", "und", "klar", "be\u00b7greift", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "PPOSAT", "PRF", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wir sind bestimmt, ziellos dahinzutreiben,", "tokens": ["Wir", "sind", "be\u00b7stimmt", ",", "ziel\u00b7los", "da\u00b7hin\u00b7zu\u00b7trei\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "ADJD", "PAV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und unser Schicksal will's, da\u00df aller Bl\u00fctentr\u00e4ume", "tokens": ["Und", "un\u00b7ser", "Schick\u00b7sal", "will's", ",", "da\u00df", "al\u00b7ler", "Bl\u00fc\u00b7ten\u00b7tr\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "$,", "KOUS", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}