{"dta.poem.18527": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als Hr. Joh. Friedr. Falckner in  \n Leipzig als B\u00fcrgermeister aufge-  \n f\u00fchret wurde im nahmen  \n eines andern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die sonne/ grosser Mann/ bringt heute so ein licht/", "tokens": ["Die", "son\u00b7ne", "/", "gros\u00b7ser", "Mann", "/", "bringt", "heu\u00b7te", "so", "ein", "licht", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADJA", "NN", "$(", "VVFIN", "ADV", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus dessen schoo\u00df dein ruhm mit hundert strahlen bricht:", "tokens": ["Aus", "des\u00b7sen", "schoo\u00df", "dein", "ruhm", "mit", "hun\u00b7dert", "strah\u00b7len", "bricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "VVFIN", "PPOSAT", "NN", "APPR", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die w\u00fcrde deiner brust wird als wie neu gebohren.", "tokens": ["Die", "w\u00fcr\u00b7de", "dei\u00b7ner", "brust", "wird", "als", "wie", "neu", "ge\u00b7boh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "VAFIN", "KOUS", "KOKOM", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und morgen gleich darauff tritt dein geburts-fest ein/", "tokens": ["Und", "mor\u00b7gen", "gleich", "dar\u00b7auff", "tritt", "dein", "ge\u00b7burts\u00b7fest", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PAV", "VVFIN", "PPOSAT", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Als solte selbst die zeit ein wahrer zeuge seyn/", "tokens": ["Als", "sol\u00b7te", "selbst", "die", "zeit", "ein", "wah\u00b7rer", "zeu\u00b7ge", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "ADV", "ART", "NN", "ART", "ADJA", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es sey dir ehr und ruhm zur mutter auserkohren.", "tokens": ["Es", "sey", "dir", "ehr", "und", "ruhm", "zur", "mut\u00b7ter", "au\u00b7ser\u00b7koh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NN", "KON", "NN", "APPRART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Denn heut\u2019 erkl\u00e4ren dich die v\u00e4ter dieser stadt/", "tokens": ["Denn", "heut'", "er\u00b7kl\u00e4\u00b7ren", "dich", "die", "v\u00e4\u00b7ter", "die\u00b7ser", "stadt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die ancker/ derer treu noch nie gewaucket hat/", "tokens": ["Die", "an\u00b7cker", "/", "de\u00b7rer", "treu", "noch", "nie", "ge\u00b7wauc\u00b7ket", "hat", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PDS", "ADJD", "ADV", "ADV", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Zu einem oberhaupt der weisen linden-r\u00e4the.", "tokens": ["Zu", "ei\u00b7nem", "o\u00b7ber\u00b7haupt", "der", "wei\u00b7sen", "lin\u00b7den\u00b7r\u00e4\u00b7the", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Du machst die dritte zahl vollkommen wieder gut/", "tokens": ["Du", "machst", "die", "drit\u00b7te", "zahl", "voll\u00b7kom\u00b7men", "wie\u00b7der", "gut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ADJD", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Von der es immer scheint/ wenn sie den ausspruch thut/", "tokens": ["Von", "der", "es", "im\u00b7mer", "scheint", "/", "wenn", "sie", "den", "aus\u00b7spruch", "thut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "VVFIN", "$(", "KOUS", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Als ob ihn Pythii ber\u00fchmter drey-fu\u00df th\u00e4te.", "tokens": ["Als", "ob", "ihn", "Py\u00b7thii", "be\u00b7r\u00fchm\u00b7ter", "drey\u00b7fu\u00df", "th\u00e4\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "NE", "ADJA", "NN", "VVFIN", "$."], "meter": "+---+-+-+-+-", "measure": "dactylic.init"}, "line.13": {"text": "Die linden sind daher zu lauter lust erregt/", "tokens": ["Die", "lin\u00b7den", "sind", "da\u00b7her", "zu", "lau\u00b7ter", "lust", "er\u00b7regt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "PAV", "APPR", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Indem ein holder West ihr gr\u00fcnes laub bewegt/", "tokens": ["In\u00b7dem", "ein", "hol\u00b7der", "West", "ihr", "gr\u00fc\u00b7nes", "laub", "be\u00b7wegt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Jhr frohes wachsthum wird in hoffnung schon erh\u00f6het.", "tokens": ["Ihr", "fro\u00b7hes", "wach\u00b7sthum", "wird", "in", "hoff\u00b7nung", "schon", "er\u00b7h\u00f6\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "APPR", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und so des falckens flug was gl\u00fcckliches bedeut;", "tokens": ["Und", "so", "des", "fal\u00b7ckens", "flug", "was", "gl\u00fcck\u00b7li\u00b7ches", "be\u00b7deut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "PIS", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wird ihnen billig auch nur gutes prophezeyt/", "tokens": ["Wird", "ih\u00b7nen", "bil\u00b7lig", "auch", "nur", "gu\u00b7tes", "pro\u00b7phe\u00b7zeyt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "ADV", "ADV", "ADJA", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Nachdem ein Falckner itzt auf ihrem gipffel stehet.", "tokens": ["Nach\u00b7dem", "ein", "Falck\u00b7ner", "itzt", "auf", "ih\u00b7rem", "gipf\u00b7fel", "ste\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Dich hat/ noch ehe dir di\u00df pfund ist zugezehlt/", "tokens": ["Dich", "hat", "/", "noch", "e\u00b7he", "dir", "di\u00df", "pfund", "ist", "zu\u00b7ge\u00b7zehlt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "ADV", "KOUS", "PPER", "PDS", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Gott und der b\u00fcrger hertz schon l\u00e4ngst dazu erwehlt.", "tokens": ["Gott", "und", "der", "b\u00fcr\u00b7ger", "hertz", "schon", "l\u00e4ngst", "da\u00b7zu", "er\u00b7wehlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "ADJA", "NN", "ADV", "ADV", "PAV", "VVPP", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.21": {"text": "Die wahl ist so nach wunsch als nach verdienst geschehen/", "tokens": ["Die", "wahl", "ist", "so", "nach", "wunsch", "als", "nach", "ver\u00b7dienst", "ge\u00b7sche\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "ADJD", "KOKOM", "APPR", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Jhr kluges absehn ist auf lauter heil gericht;", "tokens": ["Ihr", "klu\u00b7ges", "ab\u00b7sehn", "ist", "auf", "lau\u00b7ter", "heil", "ge\u00b7richt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVINF", "VAFIN", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Denn diese werthe stadt und selbst das Rath-hau\u00df spricht:", "tokens": ["Denn", "die\u00b7se", "wert\u00b7he", "stadt", "und", "selbst", "das", "Ra\u00b7th\u00b7hau\u00df", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ADJD", "KON", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.24": {"text": "Man mu\u00df bey tr\u00fcber zeit mit falcken-augen sehen.", "tokens": ["Man", "mu\u00df", "bey", "tr\u00fc\u00b7ber", "zeit", "mit", "fal\u00b7cken\u00b7au\u00b7gen", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "ADJA", "NN", "APPR", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Die wei\u00dfheit/ die dich schm\u00fcckt/ die treue die dich ziert/", "tokens": ["Die", "wei\u00df\u00b7heit", "/", "die", "dich", "schm\u00fcckt", "/", "die", "treu\u00b7e", "die", "dich", "ziert", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "VVFIN", "$(", "ART", "ADJA", "PRELS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und dein gerechtes thun bleibt von mir unber\u00fchrt.", "tokens": ["Und", "dein", "ge\u00b7rech\u00b7tes", "thun", "bleibt", "von", "mir", "un\u00b7be\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "VVINF", "VVFIN", "APPR", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Denn deine tugend mu\u00df ein gr\u00f6sser Herold preisen;", "tokens": ["Denn", "dei\u00b7ne", "tu\u00b7gend", "mu\u00df", "ein", "gr\u00f6s\u00b7ser", "He\u00b7rold", "prei\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und wie ein Elephant den eignen schatten flieht/", "tokens": ["Und", "wie", "ein", "E\u00b7le\u00b7phant", "den", "eig\u00b7nen", "schat\u00b7ten", "flieht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ART", "ADJA", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "So wei\u00df ich/ da\u00df dein geist es niemals gerne sieht/", "tokens": ["So", "wei\u00df", "ich", "/", "da\u00df", "dein", "geist", "es", "nie\u00b7mals", "ger\u00b7ne", "sieht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "KOUS", "PPOSAT", "NN", "PPER", "ADV", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Wenn man durch loben dir will einen dienst erweisen.", "tokens": ["Wenn", "man", "durch", "lo\u00b7ben", "dir", "will", "ei\u00b7nen", "dienst", "er\u00b7wei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "VVFIN", "PPER", "VMFIN", "ART", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ja weil ich dieses auch/ was du nur mir gethan/", "tokens": ["Ja", "weil", "ich", "die\u00b7ses", "auch", "/", "was", "du", "nur", "mir", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "KOUS", "PPER", "PDS", "ADV", "$(", "PWS", "PPER", "ADV", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Hier/ mein Mec\u00e4nas/ nicht nach w\u00fcrden r\u00fchmen kan\u2019/", "tokens": ["Hier", "/", "mein", "Me\u00b7c\u00e4\u00b7nas", "/", "nicht", "nach", "w\u00fcr\u00b7den", "r\u00fch\u00b7men", "kan'", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "PPOSAT", "NN", "$(", "PTKNEG", "APPR", "VAFIN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Wie k\u00f6nte denn mein kiel die nutzbarkeit beschreiben/", "tokens": ["Wie", "k\u00f6n\u00b7te", "denn", "mein", "kiel", "die", "nutz\u00b7bar\u00b7keit", "be\u00b7schrei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ADV", "PPOSAT", "VVFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Die dein ge\u00fcbter witz der gantzen stadt erzeigt?", "tokens": ["Die", "dein", "ge\u00b7\u00fcb\u00b7ter", "witz", "der", "gant\u00b7zen", "stadt", "er\u00b7zeigt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Was wunder/ wenn di\u00df blat von deinen thaten schweigt/", "tokens": ["Was", "wun\u00b7der", "/", "wenn", "di\u00df", "blat", "von", "dei\u00b7nen", "tha\u00b7ten", "schweigt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "$(", "KOUS", "PDS", "VVFIN", "APPR", "PPOSAT", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und wenn ein blosser wunsch auch meine zuflucht bleibet?", "tokens": ["Und", "wenn", "ein", "blos\u00b7ser", "wunsch", "auch", "mei\u00b7ne", "zu\u00b7flucht", "blei\u00b7bet", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "So stehe nun und bl\u00fch du schatten-reicher baum/", "tokens": ["So", "ste\u00b7he", "nun", "und", "bl\u00fch", "du", "schat\u00b7ten\u00b7rei\u00b7cher", "baum", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "KON", "VVFIN", "PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Und deiner zweige pracht vergr\u00f6sser h\u00f6h\u2019 und raum.", "tokens": ["Und", "dei\u00b7ner", "zwei\u00b7ge", "pracht", "ver\u00b7gr\u00f6s\u00b7ser", "h\u00f6h'", "und", "raum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "ADJD", "ADJD", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Begehe noch sehr offt geburts- und ehren-tage.", "tokens": ["Be\u00b7ge\u00b7he", "noch", "sehr", "offt", "ge\u00b7burts", "und", "eh\u00b7ren\u00b7ta\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADV", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Sey k\u00fcnfftig sonderlich im reisen auch begl\u00fcckt/", "tokens": ["Sey", "k\u00fcnff\u00b7tig", "son\u00b7der\u00b7lich", "im", "rei\u00b7sen", "auch", "be\u00b7gl\u00fcckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "ADJD", "APPRART", "VVFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.