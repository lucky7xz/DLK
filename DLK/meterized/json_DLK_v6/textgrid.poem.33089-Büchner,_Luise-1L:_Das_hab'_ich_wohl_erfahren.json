{"textgrid.poem.33089": {"metadata": {"author": {"name": "B\u00fcchner, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das hab' ich wohl erfahren", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das hab' ich wohl erfahren", "tokens": ["Das", "hab'", "ich", "wohl", "er\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In manchen bitt'ren Jahren,", "tokens": ["In", "man\u00b7chen", "bitt'\u00b7ren", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es giebt f\u00fcr mich kein Gl\u00fcck!", "tokens": ["Es", "giebt", "f\u00fcr", "mich", "kein", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wo Andre Rosen brechen,", "tokens": ["Wo", "And\u00b7re", "Ro\u00b7sen", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mich nur die Dornen stechen:", "tokens": ["Mich", "nur", "die", "Dor\u00b7nen", "ste\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So will es mein Geschick.", "tokens": ["So", "will", "es", "mein", "Ge\u00b7schick", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nie streckt' ich meine H\u00e4nde", "tokens": ["Nie", "streckt'", "ich", "mei\u00b7ne", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nach reichster Gl\u00fcckesspende,", "tokens": ["Nach", "reichs\u00b7ter", "Gl\u00fc\u00b7ckes\u00b7spen\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich brauche wenig nur:", "tokens": ["Ich", "brau\u00b7che", "we\u00b7nig", "nur", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ein freundliches Verstehen,", "tokens": ["Ein", "freund\u00b7li\u00b7ches", "Ver\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein geistiges Umwehen,", "tokens": ["Ein", "geis\u00b7ti\u00b7ges", "Um\u00b7we\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und Tr\u00f6sterin Natur.", "tokens": ["Und", "Tr\u00f6s\u00b7te\u00b7rin", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Allein: \u00bbdu sollst entbehren,", "tokens": ["Al\u00b7lein", ":", "\u00bb", "du", "sollst", "ent\u00b7beh\u00b7ren", ","], "token_info": ["word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "$(", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Entbehrend dich verzehren!\u00ab", "tokens": ["Ent\u00b7beh\u00b7rend", "dich", "ver\u00b7zeh\u00b7ren", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VVPP", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So sprach das Leben hart.", "tokens": ["So", "sprach", "das", "Le\u00b7ben", "hart", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Was n\u00fctzet eitle Klage,", "tokens": ["Was", "n\u00fct\u00b7zet", "eit\u00b7le", "Kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Was n\u00fctzet mir die Frage,", "tokens": ["Was", "n\u00fct\u00b7zet", "mir", "die", "Fra\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Warum dies Loos mir ward?", "tokens": ["Wa\u00b7rum", "dies", "Loos", "mir", "ward", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "NN", "PPER", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich gehe ruhig weiter,", "tokens": ["Ich", "ge\u00b7he", "ru\u00b7hig", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Geduld ist mein Begleiter,", "tokens": ["Ge\u00b7duld", "ist", "mein", "Be\u00b7glei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein kalter, trockner Freund;", "tokens": ["Ein", "kal\u00b7ter", ",", "trock\u00b7ner", "Freund", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Regt sich mein Geist zum K\u00e4mpfen,", "tokens": ["Regt", "sich", "mein", "Geist", "zum", "K\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wird er den Aufschwung d\u00e4mpfen,", "tokens": ["Wird", "er", "den", "Auf\u00b7schwung", "d\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Da\u00df er sich selbst verneint!", "tokens": ["Da\u00df", "er", "sich", "selbst", "ver\u00b7neint", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Hebt Phantasie die Schwingen,", "tokens": ["Hebt", "Phan\u00b7ta\u00b7sie", "die", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Entz\u00fcckung mir zu bringen,", "tokens": ["Ent\u00b7z\u00fc\u00b7ckung", "mir", "zu", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die meine Sehnsucht stillt;", "tokens": ["Die", "mei\u00b7ne", "Sehn\u00b7sucht", "stillt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Flieh' ich zur\u00fcck zur Wahrheit", "tokens": ["Flieh'", "ich", "zu\u00b7r\u00fcck", "zur", "Wahr\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und seh' in bitt'rer Klarheit,", "tokens": ["Und", "seh'", "in", "bitt'\u00b7rer", "Klar\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Es war ein t\u00e4uschend Bild.", "tokens": ["Es", "war", "ein", "t\u00e4u\u00b7schend", "Bild", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ist's wahr, da\u00df solche Seelen,", "tokens": ["Ist's", "wahr", ",", "da\u00df", "sol\u00b7che", "See\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "KOUS", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die sich nichts mehr verhehlen,", "tokens": ["Die", "sich", "nichts", "mehr", "ver\u00b7heh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "PIS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Schon sind des Todes Raub \u2013", "tokens": ["Schon", "sind", "des", "To\u00b7des", "Raub", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mu\u00df bald mein Geist entschweben,", "tokens": ["Mu\u00df", "bald", "mein", "Geist", "ent\u00b7schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dies t\u00e4uschungsleere Leben", "tokens": ["Dies", "t\u00e4u\u00b7schungs\u00b7lee\u00b7re", "Le\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["PDS", "VVFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hinsinken in den Staub!", "tokens": ["Hin\u00b7sin\u00b7ken", "in", "den", "Staub", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}