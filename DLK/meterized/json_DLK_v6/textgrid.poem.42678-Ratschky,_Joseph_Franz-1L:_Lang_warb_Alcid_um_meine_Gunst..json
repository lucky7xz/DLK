{"textgrid.poem.42678": {"metadata": {"author": {"name": "Ratschky, Joseph Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Lang warb Alcid um meine Gunst.", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lang warb Alcid um meine Gunst.", "tokens": ["Lang", "warb", "Al\u00b7cid", "um", "mei\u00b7ne", "Gunst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Herz zwar schlug ihm laut entgegen:", "tokens": ["Mein", "Herz", "zwar", "schlug", "ihm", "laut", "ent\u00b7ge\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Allein vertraut mit Amors Kunst,", "tokens": ["Al\u00b7lein", "ver\u00b7traut", "mit", "A\u00b7mors", "Kunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "That ich versch\u00e4mt, wie M\u00e4dchen pflegen.", "tokens": ["That", "ich", "ver\u00b7sch\u00e4mt", ",", "wie", "M\u00e4d\u00b7chen", "pfle\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVPP", "$,", "PWAV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn er mir schmachtend Liebe schwur,", "tokens": ["Wenn", "er", "mir", "schmach\u00b7tend", "Lie\u00b7be", "schwur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "War ich zum Schein zerstreut und fl\u00fcchtig,", "tokens": ["War", "ich", "zum", "Schein", "zer\u00b7streut", "und", "fl\u00fcch\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "VVPP", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und wagt' er auch ein K\u00fcsschen nur,", "tokens": ["Und", "wagt'", "er", "auch", "ein", "K\u00fcs\u00b7schen", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So hiess es: junger Herr, h\u00fcbsch z\u00fcchtig!", "tokens": ["So", "hiess", "es", ":", "jun\u00b7ger", "Herr", ",", "h\u00fcbsch", "z\u00fcch\u00b7tig", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "ADJA", "NN", "$,", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Vergebens h\u00f6rt' ich ihn betr\u00fcbt", "tokens": ["Ver\u00b7ge\u00b7bens", "h\u00f6rt'", "ich", "ihn", "be\u00b7tr\u00fcbt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Schicksal meine H\u00e4rte klagen;", "tokens": ["Dem", "Schick\u00b7sal", "mei\u00b7ne", "H\u00e4r\u00b7te", "kla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Denn wenn man noch so feurig liebt,", "tokens": ["Denn", "wenn", "man", "noch", "so", "feu\u00b7rig", "liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Man darf's aus Sittsamkeit nicht sagen,", "tokens": ["Man", "dar\u00b7f's", "aus", "Sitt\u00b7sam\u00b7keit", "nicht", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PAV", "APPR", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Bat er oft gar zu ungest\u00fcm", "tokens": ["Bat", "er", "oft", "gar", "zu", "un\u00b7ge\u00b7st\u00fcm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PTKA", "ADJD"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.6": {"text": "Um diess und das, so scholt ich t\u00fcchtig", "tokens": ["Um", "diess", "und", "das", ",", "so", "scholt", "ich", "t\u00fcch\u00b7tig"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUI", "PDS", "KON", "PDS", "$,", "ADV", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ihn aus, und gab halbl\u00e4chelnd ihm", "tokens": ["Ihn", "aus", ",", "und", "gab", "halb\u00b7l\u00e4\u00b7chelnd", "ihm"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "PTKVZ", "$,", "KON", "VVFIN", "ADJD", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Lehre: junger Herr, h\u00fcbsch z\u00fcchtig!", "tokens": ["Die", "Leh\u00b7re", ":", "jun\u00b7ger", "Herr", ",", "h\u00fcbsch", "z\u00fcch\u00b7tig", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ADJA", "NN", "$,", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch allgemach erhielt Alcid", "tokens": ["Doch", "all\u00b7ge\u00b7mach", "er\u00b7hielt", "Al\u00b7cid"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Herz, nach dem er lang gegeitzet.", "tokens": ["Mein", "Herz", ",", "nach", "dem", "er", "lang", "ge\u00b7geit\u00b7zet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "PRELS", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ach aber welch ein Un erschied!", "tokens": ["Ach", "a\u00b7ber", "welch", "ein", "Un", "er\u00b7schied", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "PIAT", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Seit uns der Priester traute, reitzet", "tokens": ["Seit", "uns", "der", "Pries\u00b7ter", "trau\u00b7te", ",", "reit\u00b7zet"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PPER", "ART", "NN", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ihn auch die h\u00f6chste Gunst nicht sehr,", "tokens": ["Ihn", "auch", "die", "h\u00f6chs\u00b7te", "Gunst", "nicht", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "ADJA", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und sonst war ihm ein Blick schon wichtig.", "tokens": ["Und", "sonst", "war", "ihm", "ein", "Blick", "schon", "wich\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.7": {"text": "Nun str\u00e4ub' ich mich gewiss nicht mehr;", "tokens": ["Nun", "str\u00e4ub'", "ich", "mich", "ge\u00b7wiss", "nicht", "mehr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Denn itzt ist er nur allzuz\u00fcchtig.", "tokens": ["Denn", "itzt", "ist", "er", "nur", "all\u00b7zu\u00b7z\u00fcch\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+---+-+-", "measure": "dactylic.init"}}}}}