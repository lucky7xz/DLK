{"textgrid.poem.44541": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Diplomatisch", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Halt dich entfernt, geh nicht nach Stolzenfels.", "tokens": ["Halt", "dich", "ent\u00b7fernt", ",", "geh", "nicht", "nach", "Stol\u00b7zen\u00b7fels", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVPP", "$,", "VVFIN", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du bist kein Fels, drum sei nicht stolz.", "tokens": ["Du", "bist", "kein", "Fels", ",", "drum", "sei", "nicht", "stolz", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,", "PAV", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und w\u00e4rst dus auch, nicht stolz, mein ich, ein Fels,", "tokens": ["Und", "w\u00e4rst", "dus", "auch", ",", "nicht", "stolz", ",", "mein", "ich", ",", "ein", "Fels", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NE", "ADV", "$,", "PTKNEG", "ADJD", "$,", "PPOSAT", "PPER", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die kleinen Wellen waschen unverzagt,", "tokens": ["Die", "klei\u00b7nen", "Wel\u00b7len", "wa\u00b7schen", "un\u00b7ver\u00b7zagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Bis Teil auf Teil das stolze Haupt sich neigt", "tokens": ["Bis", "Teil", "auf", "Teil", "das", "stol\u00b7ze", "Haupt", "sich", "neigt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN", "ART", "ADJA", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und donnernd es herabst\u00fcrzt in den Schlund,", "tokens": ["Und", "don\u00b7nernd", "es", "her\u00b7ab\u00b7st\u00fcrzt", "in", "den", "Schlund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Den Spiegel \u00fcberragend kaum der Fluten.", "tokens": ["Den", "Spie\u00b7gel", "\u00fc\u00b7berr\u00b7a\u00b7gend", "kaum", "der", "Flu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Sieh nur den Rhein, wie f\u00fcgsam, er sich kr\u00fcmmt,", "tokens": ["Sieh", "nur", "den", "Rhein", ",", "wie", "f\u00fcg\u00b7sam", ",", "er", "sich", "kr\u00fcmmt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NE", "$,", "PWAV", "ADJD", "$,", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Den Ufern folgend, die ihm Gott gesetzt:", "tokens": ["Den", "U\u00b7fern", "fol\u00b7gend", ",", "die", "ihm", "Gott", "ge\u00b7setzt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "PRELS", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "So macht er Wiesen gr\u00fcn und tr\u00e4gt das Schiff", "tokens": ["So", "macht", "er", "Wie\u00b7sen", "gr\u00fcn", "und", "tr\u00e4gt", "das", "Schiff"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "ADJD", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und ist der Segen einer kleinen Welt.", "tokens": ["Und", "ist", "der", "Se\u00b7gen", "ei\u00b7ner", "klei\u00b7nen", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Doch weht der Wind her vom Johannisberg,", "tokens": ["Doch", "weht", "der", "Wind", "her", "vom", "Jo\u00b7han\u00b7nis\u00b7berg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APZR", "APPRART", "NE", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Dann t\u00fcrmen sich die Fluten, und er schl\u00e4gt", "tokens": ["Dann", "t\u00fcr\u00b7men", "sich", "die", "Flu\u00b7ten", ",", "und", "er", "schl\u00e4gt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$,", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Die Ufer, die ihn machen erst zum Strom.", "tokens": ["Die", "U\u00b7fer", ",", "die", "ihn", "ma\u00b7chen", "erst", "zum", "Strom", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Denn ohne Ufer w\u00e4r er \u00dcberschwemmung,", "tokens": ["Denn", "oh\u00b7ne", "U\u00b7fer", "w\u00e4r", "er", "\u00dc\u00b7bersc\u00b7hwem\u00b7mung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VAFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Ein gr\u00e4ulich Mittelding von See und Sumpf,", "tokens": ["Ein", "gr\u00e4u\u00b7lich", "Mit\u00b7tel\u00b7ding", "von", "See", "und", "Sumpf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Aus dem die Sonne feuchte Nebel zieht,", "tokens": ["Aus", "dem", "die", "Son\u00b7ne", "feuch\u00b7te", "Ne\u00b7bel", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Von Unken nur bewohnt und ihrer Brut.", "tokens": ["Von", "Un\u00b7ken", "nur", "be\u00b7wohnt", "und", "ih\u00b7rer", "Brut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVPP", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Nach Bingen geh, sieh dort den M\u00e4useturm,", "tokens": ["Nach", "Bin\u00b7gen", "geh", ",", "sieh", "dort", "den", "M\u00e4u\u00b7se\u00b7turm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "VVIMP", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Wo Schwaches einst dem M\u00e4chtgen obgesiegt,", "tokens": ["Wo", "Schwa\u00b7ches", "einst", "dem", "M\u00e4cht\u00b7gen", "ob\u00b7ge\u00b7siegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Weil aufgespeichert er f\u00fcr sich das Gut,", "tokens": ["Weil", "auf\u00b7ge\u00b7spei\u00b7chert", "er", "f\u00fcr", "sich", "das", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVPP", "PPER", "APPR", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Das Gott, der Sonne gleich, f\u00fcr alle schuf.", "tokens": ["Das", "Gott", ",", "der", "Son\u00b7ne", "gleich", ",", "f\u00fcr", "al\u00b7le", "schuf", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ADV", "$,", "APPR", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Vertrau nicht allzufest auf Stamm und Art,", "tokens": ["Ver\u00b7trau", "nicht", "all\u00b7zu\u00b7fest", "auf", "Stamm", "und", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Der Adler auch ist K\u00f6nig nur der Luft,", "tokens": ["Der", "Ad\u00b7ler", "auch", "ist", "K\u00f6\u00b7nig", "nur", "der", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Solang ihn hoch empor die Schwinge tr\u00e4gt;", "tokens": ["So\u00b7lang", "ihn", "hoch", "em\u00b7por", "die", "Schwin\u00b7ge", "tr\u00e4gt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PTKVZ", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Ist erst des Fl\u00fcgelschlages Kraft erlahmt,", "tokens": ["Ist", "erst", "des", "Fl\u00fc\u00b7gel\u00b7schla\u00b7ges", "Kraft", "er\u00b7lahmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Der Geist gewichen aus der stolzen Brust,", "tokens": ["Der", "Geist", "ge\u00b7wi\u00b7chen", "aus", "der", "stol\u00b7zen", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Gilt die Gestalt nur noch als Wappentier,", "tokens": ["Gilt", "die", "Ge\u00b7stalt", "nur", "noch", "als", "Wap\u00b7pen\u00b7tier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADV", "KOUS", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.29": {"text": "Das M\u00e4use fressen wie den heilgen Bischof,", "tokens": ["Das", "M\u00e4u\u00b7se", "fres\u00b7sen", "wie", "den", "heil\u00b7gen", "Bi\u00b7schof", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.30": {"text": "Der heilig war, doch fromm nicht und nicht gut.", "tokens": ["Der", "hei\u00b7lig", "war", ",", "doch", "fromm", "nicht", "und", "nicht", "gut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "$,", "ADV", "ADJD", "PTKNEG", "KON", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}