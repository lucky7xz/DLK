{"textgrid.poem.28848": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "63.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Reichstag ist nicht weit,", "tokens": ["Ein", "Reichs\u00b7tag", "ist", "nicht", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Da aller Glaubens-Streit", "tokens": ["Da", "al\u00b7ler", "Glau\u00b7bens\u00b7Streit"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wird gantz beschieden werden,", "tokens": ["Wird", "gantz", "be\u00b7schie\u00b7den", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wann Gott hier von der Erden", "tokens": ["Wann", "Gott", "hier", "von", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wird haben alle Welt", "tokens": ["Wird", "ha\u00b7ben", "al\u00b7le", "Welt"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "F\u00fcr seinen Thron gestellt.", "tokens": ["F\u00fcr", "sei\u00b7nen", "Thron", "ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}