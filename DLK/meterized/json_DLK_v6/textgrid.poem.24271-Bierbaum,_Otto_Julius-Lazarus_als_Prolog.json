{"textgrid.poem.24271": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Lazarus als Prolog", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schon hatten die Lemuren", "tokens": ["Schon", "hat\u00b7ten", "die", "Le\u00b7mu\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Grab mir tief gemacht,", "tokens": ["Das", "Grab", "mir", "tief", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und meine Arme fuhren", "tokens": ["Und", "mei\u00b7ne", "Ar\u00b7me", "fuh\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ins Leichenhemd der Nacht;", "tokens": ["Ins", "Lei\u00b7chen\u00b7hemd", "der", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich h\u00f6rte schon", "tokens": ["Ich", "h\u00f6r\u00b7te", "schon"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Den Willkommenshohn:", "tokens": ["Den", "Will\u00b7kom\u00b7mens\u00b7hohn", ":"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Des Zerberus Gebelle,", "tokens": ["Des", "Zer\u00b7be\u00b7rus", "Ge\u00b7bel\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der jenes dunkle, niedere Tor bewacht.", "tokens": ["Der", "je\u00b7nes", "dunk\u00b7le", ",", "nie\u00b7de\u00b7re", "Tor", "be\u00b7wacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "ADJA", "$,", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Da ward es pl\u00f6tzlich helle:", "tokens": ["Da", "ward", "es", "pl\u00f6tz\u00b7lich", "hel\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie Rosen hat gelacht", "tokens": ["Wie", "Ro\u00b7sen", "hat", "ge\u00b7lacht"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Licht auf jener Schwelle,", "tokens": ["Das", "Licht", "auf", "je\u00b7ner", "Schwel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die jeden z\u00f6gern macht.", "tokens": ["Die", "je\u00b7den", "z\u00f6\u00b7gern", "macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Nacht verschwand:", "tokens": ["Die", "Nacht", "ver\u00b7schwand", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "An Deiner Hand", "tokens": ["An", "Dei\u00b7ner", "Hand"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Ging ich auf Rosenspuren", "tokens": ["Ging", "ich", "auf", "Ro\u00b7sen\u00b7spu\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zur\u00fcck ins Leben: Lazarus, erwacht.", "tokens": ["Zu\u00b7r\u00fcck", "ins", "Le\u00b7ben", ":", "La\u00b7za\u00b7rus", ",", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$.", "NE", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}