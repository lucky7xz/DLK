{"textgrid.poem.44655": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "An die Hofkammer", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr wollt mich nicht in eurem Zunftvereine?", "tokens": ["Ihr", "wollt", "mich", "nicht", "in", "eu\u00b7rem", "Zunft\u00b7ver\u00b7ei\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gut! dahin taug ich auch nicht sonderlich!", "tokens": ["Gut", "!", "da\u00b7hin", "taug", "ich", "auch", "nicht", "son\u00b7der\u00b7lich", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "PAV", "VVFIN", "PPER", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nie barg ich es: ich hasse das Gemeine,", "tokens": ["Nie", "barg", "ich", "es", ":", "ich", "has\u00b7se", "das", "Ge\u00b7mei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$.", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was Wunder, wenn auch das Gemeine mich?", "tokens": ["Was", "Wun\u00b7der", ",", "wenn", "auch", "das", "Ge\u00b7mei\u00b7ne", "mich", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "$,", "KOUS", "ADV", "ART", "NN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}