{"textgrid.poem.24380": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Als ein gro\u00dfer Minister und General hingerichtet ward", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Adel war ber\u00fchmt. Die Feder samt den Degen", "tokens": ["Mein", "A\u00b7del", "war", "be\u00b7r\u00fchmt", ".", "Die", "Fe\u00b7der", "samt", "den", "De\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$.", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Pflag meiner Ahnen-Glantz was herrlichs beyzulegen.", "tokens": ["Pflag", "mei\u00b7ner", "Ah\u00b7nen\u00b7Glantz", "was", "herr\u00b7lichs", "bey\u00b7zu\u00b7le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PWS", "PIS", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nicht schimpfe meinen Tod; brachte meinen Muth/", "tokens": ["Nicht", "schimp\u00b7fe", "mei\u00b7nen", "Tod", ";", "brach\u00b7te", "mei\u00b7nen", "Muth", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPOSAT", "NN", "$.", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-++-+-+", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Vor Freyheit/ Gut und Ehr verspritzt mein edles Blut.", "tokens": ["Vor", "Frey\u00b7heit", "/", "Gut", "und", "Ehr", "ver\u00b7spritzt", "mein", "ed\u00b7les", "Blut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "ADJD", "KON", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+