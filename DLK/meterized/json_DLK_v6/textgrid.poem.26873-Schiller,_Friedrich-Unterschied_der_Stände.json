{"textgrid.poem.26873": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Unterschied der St\u00e4nde", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auch in der sittlichen Welt ist ein Adel; gemeine Naturen", "tokens": ["Auch", "in", "der", "sitt\u00b7li\u00b7chen", "Welt", "ist", "ein", "A\u00b7del", ";", "ge\u00b7mei\u00b7ne", "Na\u00b7tu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VAFIN", "ART", "NN", "$.", "ADJA", "NN"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Zahlen mit dem, was sie tun, sch\u00f6ne mit dem, was sie sind.", "tokens": ["Zah\u00b7len", "mit", "dem", ",", "was", "sie", "tun", ",", "sch\u00f6\u00b7ne", "mit", "dem", ",", "was", "sie", "sind", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "$,", "PRELS", "PPER", "VVINF", "$,", "VVFIN", "APPR", "ART", "$,", "PRELS", "PPER", "VAFIN", "$."], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}}}}}