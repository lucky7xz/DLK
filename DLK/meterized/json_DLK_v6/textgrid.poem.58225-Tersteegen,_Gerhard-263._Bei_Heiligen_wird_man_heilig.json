{"textgrid.poem.58225": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "263. Bei Heiligen wird man heilig", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit Gottes Freunden Freundschaft pflegen,", "tokens": ["Mit", "Got\u00b7tes", "Freun\u00b7den", "Freund\u00b7schaft", "pfle\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bringt Gnade, Kraft und tausend Segen;", "tokens": ["Bringt", "Gna\u00b7de", ",", "Kraft", "und", "tau\u00b7send", "Se\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "NN", "KON", "CARD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wer innig steht mit Gott gemein,", "tokens": ["Wer", "in\u00b7nig", "steht", "mit", "Gott", "ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVFIN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zieht andre Seelen mit hinein.", "tokens": ["Zieht", "and\u00b7re", "See\u00b7len", "mit", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "APPR", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}