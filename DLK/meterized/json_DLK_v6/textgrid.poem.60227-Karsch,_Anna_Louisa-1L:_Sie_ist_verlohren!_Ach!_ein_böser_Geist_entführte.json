{"textgrid.poem.60227": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sie ist verlohren! Ach! ein b\u00f6ser Geist entf\u00fchrte", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie ist verlohren! Ach! ein b\u00f6ser Geist entf\u00fchrte", "tokens": ["Sie", "ist", "ver\u00b7loh\u00b7ren", "!", "Ach", "!", "ein", "b\u00f6\u00b7ser", "Geist", "ent\u00b7f\u00fchr\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "$.", "ITJ", "$.", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die goldne Feder mir \u2013 du g\u00f6ttlicher Apoll!", "tokens": ["Die", "gold\u00b7ne", "Fe\u00b7der", "mir", "\u2013", "du", "g\u00f6tt\u00b7li\u00b7cher", "A\u00b7poll", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "$(", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wo klagender Gesang dich je zum Mitleid r\u00fchrte,", "tokens": ["Wo", "kla\u00b7gen\u00b7der", "Ge\u00b7sang", "dich", "je", "zum", "Mit\u00b7leid", "r\u00fchr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So sage mir, wo ich sie wieder finden soll.", "tokens": ["So", "sa\u00b7ge", "mir", ",", "wo", "ich", "sie", "wie\u00b7der", "fin\u00b7den", "soll", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "PPER", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nicht in den Fl\u00fcgeln weisser Schw\u00e4ne,", "tokens": ["Nicht", "in", "den", "Fl\u00fc\u00b7geln", "weis\u00b7ser", "Schw\u00e4\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In schwarzen Raben nicht. Kein Vogel in dem Reich", "tokens": ["In", "schwar\u00b7zen", "Ra\u00b7ben", "nicht", ".", "Kein", "Vo\u00b7gel", "in", "dem", "Reich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PTKNEG", "$.", "PIAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Des Fl\u00fcgelvolks hat Federn, die ihr gleich", "tokens": ["Des", "Fl\u00fc\u00b7gel\u00b7volks", "hat", "Fe\u00b7dern", ",", "die", "ihr", "gleich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "NN", "$,", "PRELS", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "An Pracht und Dauer sind. O! h\u00f6re meine Thr\u00e4ne,", "tokens": ["An", "Pracht", "und", "Dau\u00b7er", "sind", ".", "O", "!", "h\u00f6\u00b7re", "mei\u00b7ne", "Thr\u00e4\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VAFIN", "$.", "NE", "$.", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die in der Seele niederf\u00e4llt!", "tokens": ["Die", "in", "der", "See\u00b7le", "nie\u00b7der\u00b7f\u00e4llt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "La\u00df einen Traum Orakelspr\u00fcche sagen,", "tokens": ["La\u00df", "ei\u00b7nen", "Traum", "O\u00b7ra\u00b7kel\u00b7spr\u00fc\u00b7che", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "La\u00df dich noch einmahl in der Welt", "tokens": ["La\u00df", "dich", "noch", "ein\u00b7mahl", "in", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Von mir als wie zu Delphos fragen!", "tokens": ["Von", "mir", "als", "wie", "zu", "Del\u00b7phos", "fra\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KOUS", "KOKOM", "APPR", "NE", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Freund! also klagte, redend mit Apollen,", "tokens": ["Freund", "!", "al\u00b7so", "klag\u00b7te", ",", "re\u00b7dend", "mit", "A\u00b7pol\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VVFIN", "$,", "VVPP", "APPR", "NN", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.2": {"text": "Die Muse heimlich meinen Gram.", "tokens": ["Die", "Mu\u00b7se", "heim\u00b7lich", "mei\u00b7nen", "Gram", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im tr\u00fcben Auge, aus der vollen", "tokens": ["Im", "tr\u00fc\u00b7ben", "Au\u00b7ge", ",", "aus", "der", "vol\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Argw\u00f6hnisch denkenden umw\u00f6lkten Seele, kam", "tokens": ["Arg\u00b7w\u00f6h\u00b7nisch", "den\u00b7ken\u00b7den", "um\u00b7w\u00f6lk\u00b7ten", "See\u00b7le", ",", "kam"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Kein heitrer Blick auf eine Reihe,", "tokens": ["Kein", "hei\u00b7trer", "Blick", "auf", "ei\u00b7ne", "Rei\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die um mich sa\u00df. O! wann selbst Thyrsis, meine Wahl,", "tokens": ["Die", "um", "mich", "sa\u00df", ".", "O", "!", "wann", "selbst", "Thyr\u00b7sis", ",", "mei\u00b7ne", "Wahl", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVFIN", "$.", "NE", "$.", "PWAV", "ADV", "NE", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mein Wunsch, und mein Gesang, wann er selbst dazumahl", "tokens": ["Mein", "Wunsch", ",", "und", "mein", "Ge\u00b7sang", ",", "wann", "er", "selbst", "da\u00b7zu\u00b7mahl"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "KON", "PPOSAT", "NN", "$,", "PWAV", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Gekommen w\u00e4r mit dir, wenn du und er mir neue", "tokens": ["Ge\u00b7kom\u00b7men", "w\u00e4r", "mit", "dir", ",", "wenn", "du", "und", "er", "mir", "neu\u00b7e"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "APPR", "PPER", "$,", "KOUS", "PPER", "KON", "PPER", "PPER", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Versicherung der Freundschaft vorgesagt;", "tokens": ["Ver\u00b7si\u00b7che\u00b7rung", "der", "Freund\u00b7schaft", "vor\u00b7ge\u00b7sagt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "So h\u00e4tte doch mein Auge noch geklagt.", "tokens": ["So", "h\u00e4t\u00b7te", "doch", "mein", "Au\u00b7ge", "noch", "ge\u00b7klagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich sa\u00df im Gram versenkt; und wann ich nun erwachte,", "tokens": ["Ich", "sa\u00df", "im", "Gram", "ver\u00b7senkt", ";", "und", "wann", "ich", "nun", "er\u00b7wach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "VVPP", "$.", "KON", "PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So rief ich in dem Thon der Wuth:", "tokens": ["So", "rief", "ich", "in", "dem", "Thon", "der", "Wuth", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Amint, hat das entwandte Gut,", "tokens": ["A\u00b7mint", ",", "hat", "das", "ent\u00b7wand\u00b7te", "Gut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Versteckt hats seine Hand, da\u00df er mich klagend machte!", "tokens": ["Ver\u00b7steckt", "hats", "sei\u00b7ne", "Hand", ",", "da\u00df", "er", "mich", "kla\u00b7gend", "mach\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "$,", "KOUS", "PPER", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "O du! der grossen Venus Kind,", "tokens": ["O", "du", "!", "der", "gros\u00b7sen", "Ve\u00b7nus", "Kind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du Amor! strafe den Amint;", "tokens": ["Du", "A\u00b7mor", "!", "stra\u00b7fe", "den", "A\u00b7mint", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "$.", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Nimm deinen K\u00f6cher, flieg' und spiele", "tokens": ["Nimm", "dei\u00b7nen", "K\u00f6\u00b7cher", ",", "flieg'", "und", "spie\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "$,", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Vor seinem Angesicht mit Pfeilen, und wenn er", "tokens": ["Vor", "sei\u00b7nem", "An\u00b7ge\u00b7sicht", "mit", "Pfei\u00b7len", ",", "und", "wenn", "er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "NN", "$,", "KON", "KOUS", "PPER"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Wie Mars nach einem greift und scherzet: Ach wie schwer!", "tokens": ["Wie", "Mars", "nach", "ei\u00b7nem", "greift", "und", "scher\u00b7zet", ":", "Ach", "wie", "schwer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "KON", "VVFIN", "$.", "NN", "KOKOM", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So sprich: Behalt ihn nur und f\u00fchle!", "tokens": ["So", "sprich", ":", "Be\u00b7halt", "ihn", "nur", "und", "f\u00fch\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$.", "VVFIN", "PPER", "ADV", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Dies sagt ich zu Cytherens Sohn;", "tokens": ["Dies", "sagt", "ich", "zu", "Cy\u00b7the\u00b7rens", "Sohn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "NE", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Und seine Mutter, meine Freundin, blickte", "tokens": ["Und", "sei\u00b7ne", "Mut\u00b7ter", ",", "mei\u00b7ne", "Freun\u00b7din", ",", "blick\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Befehl auf ihren Knaben schon;", "tokens": ["Be\u00b7fehl", "auf", "ih\u00b7ren", "Kna\u00b7ben", "schon", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich sahe, da\u00df sie ihn verschickte.", "tokens": ["Ich", "sa\u00b7he", ",", "da\u00df", "sie", "ihn", "ver\u00b7schick\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch bald kam er zur\u00fcck, sah traurig, sprach zu ihr:", "tokens": ["Doch", "bald", "kam", "er", "zu\u00b7r\u00fcck", ",", "sah", "trau\u00b7rig", ",", "sprach", "zu", "ihr", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PTKVZ", "$,", "VVFIN", "ADJD", "$,", "VVFIN", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Man mu\u00df sich vor dem J\u00fcngling scheuen;", "tokens": ["Man", "mu\u00df", "sich", "vor", "dem", "J\u00fcng\u00b7ling", "scheu\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er giebt nicht acht aufs Spiel, merckt nicht auf T\u00e4ndeleyen,", "tokens": ["Er", "giebt", "nicht", "acht", "aufs", "Spiel", ",", "merckt", "nicht", "auf", "T\u00e4n\u00b7de\u00b7le\u00b7yen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "CARD", "APPRART", "NN", "$,", "VVFIN", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.4": {"text": "Auch nimmt er keinen Pfeil von mir.", "tokens": ["Auch", "nimmt", "er", "kei\u00b7nen", "Pfeil", "von", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}