{"textgrid.poem.38562": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wann der Abend die Gefilde r\u00f6thet,", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann der Abend die Gefilde r\u00f6thet,", "tokens": ["Wann", "der", "A\u00b7bend", "die", "Ge\u00b7fil\u00b7de", "r\u00f6\u00b7thet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Alles sich im D\u00e4mmerlicht versch\u00f6nt,", "tokens": ["Al\u00b7les", "sich", "im", "D\u00e4m\u00b7mer\u00b7licht", "ver\u00b7sch\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PRF", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wann die Nachtigall im Gr\u00fcnen fl\u00f6tet,", "tokens": ["Wann", "die", "Nach\u00b7ti\u00b7gall", "im", "Gr\u00fc\u00b7nen", "fl\u00f6\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und des Dorfes Glocke t\u00f6nt;", "tokens": ["Und", "des", "Dor\u00b7fes", "Glo\u00b7cke", "t\u00f6nt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wann mit Golde sich die Wolken s\u00e4umen,", "tokens": ["Wann", "mit", "Gol\u00b7de", "sich", "die", "Wol\u00b7ken", "s\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wann des Baches Stimme leiser hallt,", "tokens": ["Wann", "des", "Ba\u00b7ches", "Stim\u00b7me", "lei\u00b7ser", "hallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und von duftbew\u00f6lkten Gartenb\u00e4umen", "tokens": ["Und", "von", "duft\u00b7be\u00b7w\u00f6lk\u00b7ten", "Gar\u00b7ten\u00b7b\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Bl\u00fcthenregen niederwallt;", "tokens": ["Bl\u00fc\u00b7then\u00b7re\u00b7gen", "nie\u00b7der\u00b7wallt", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Oder wann, mit hoher Ahndung Schauer,", "tokens": ["O\u00b7der", "wann", ",", "mit", "ho\u00b7her", "Ahn\u00b7dung", "Schau\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "$,", "APPR", "ADJA", "NN", "NE", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die verschwiegne Nacht vom Himmel sinkt,", "tokens": ["Die", "ver\u00b7schwieg\u00b7ne", "Nacht", "vom", "Him\u00b7mel", "sinkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und voll Sympathie und stiller Trauer", "tokens": ["Und", "voll", "Sym\u00b7pa\u00b7thie", "und", "stil\u00b7ler", "Trau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Jeder Stern herunterblinkt;", "tokens": ["Je\u00b7der", "Stern", "her\u00b7un\u00b7ter\u00b7blinkt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wann der volle Mond, mit bleichem Strale", "tokens": ["Wann", "der", "vol\u00b7le", "Mond", ",", "mit", "blei\u00b7chem", "Stra\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schwermuthsvoll wie ein getrennter Freund,", "tokens": ["Schwer\u00b7muths\u00b7voll", "wie", "ein", "ge\u00b7trenn\u00b7ter", "Freund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Auf die fr\u00fchen moosbewachs'nen Maale", "tokens": ["Auf", "die", "fr\u00fc\u00b7hen", "moos\u00b7be\u00b7wachs'\u00b7nen", "Maa\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Himmlischer Geliebten scheint:", "tokens": ["Himm\u00b7li\u00b7scher", "Ge\u00b7lieb\u00b7ten", "scheint", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Dann erwache, mit dem Himmelsklange", "tokens": ["Dann", "er\u00b7wa\u00b7che", ",", "mit", "dem", "Him\u00b7mels\u00b7klan\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Der Begeisterung, dein Saitenspiel,", "tokens": ["Der", "Be\u00b7geis\u00b7te\u00b7rung", ",", "dein", "Sai\u00b7ten\u00b7spiel", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Dann, o Laura, werde zum Gesange", "tokens": ["Dann", ",", "o", "Lau\u00b7ra", ",", "wer\u00b7de", "zum", "Ge\u00b7san\u00b7ge"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "FM", "FM", "$,", "VAFIN", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "S\u00fcsser Schwermuth dein Gef\u00fchl!", "tokens": ["S\u00fcs\u00b7ser", "Schwer\u00b7muth", "dein", "Ge\u00b7f\u00fchl", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}