{"dta.poem.12740": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als sie ihren rock auf dem fisch-  \n troge s\u00f6nnete.  \n L. R. B.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dorens bunter himmel liegt auf dem fischtrog ausgestreckt,", "tokens": ["Do\u00b7rens", "bun\u00b7ter", "him\u00b7mel", "liegt", "auf", "dem", "fischtrog", "aus\u00b7ge\u00b7streckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "VVFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+--+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Schade! da\u00df ihr zarter leib nicht zugleich mit drunter steckt,", "tokens": ["Scha\u00b7de", "!", "da\u00df", "ihr", "zar\u00b7ter", "leib", "nicht", "zu\u00b7gleich", "mit", "drun\u00b7ter", "steckt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "KOUS", "PPOSAT", "ADJA", "NN", "PTKNEG", "ADV", "APPR", "PAV", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Ich bem\u00fchte mich das recht derer sonnen zu gebrauchen,", "tokens": ["Ich", "be\u00b7m\u00fch\u00b7te", "mich", "das", "recht", "de\u00b7rer", "son\u00b7nen", "zu", "ge\u00b7brau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "PDS", "VVINF", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Da\u00df die frantzen rings umher wie die sp\u00e4ne solten rauchen,", "tokens": ["Da\u00df", "die", "frant\u00b7zen", "rings", "um\u00b7her", "wie", "die", "sp\u00e4\u00b7ne", "sol\u00b7ten", "rau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "PTKVZ", "KOKOM", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Jaden einflu\u00df meiner krafft wolt ich ihr so reichlich schencken,", "tokens": ["Ja\u00b7den", "ein\u00b7flu\u00df", "mei\u00b7ner", "krafft", "wolt", "ich", "ihr", "so", "reich\u00b7lich", "schen\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "VMFIN", "PPER", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Da\u00df sie binnen kurtzer zeit m\u00fcst\u2019 auf etwas anders dencken.", "tokens": ["Da\u00df", "sie", "bin\u00b7nen", "kurt\u00b7zer", "zeit", "m\u00fcst'", "auf", "et\u00b7was", "an\u00b7ders", "den\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "VMFIN", "APPR", "PIS", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+--+-+-", "measure": "trochaic.septa.relaxed"}}}}}