{"dta.poem.18415": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Innocentius  XII  R\u00f6mischer Pabst.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du wundervolles Rom/ erneure deine pracht/", "tokens": ["Du", "wun\u00b7der\u00b7vol\u00b7les", "Rom", "/", "er\u00b7neu\u00b7re", "dei\u00b7ne", "pracht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NE", "$(", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und la\u00df den alten ruhm nur im vergessen liegen;", "tokens": ["Und", "la\u00df", "den", "al\u00b7ten", "ruhm", "nur", "im", "ver\u00b7ges\u00b7sen", "lie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "ADJA", "NN", "ADV", "APPRART", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gedencke weiter nicht an deines C\u0153sars macht/", "tokens": ["Ge\u00b7den\u00b7cke", "wei\u00b7ter", "nicht", "an", "dei\u00b7nes", "C\u0153sars", "macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKNEG", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Nachdem ein Cato itzt auff deinen thron gestiegen.", "tokens": ["Nach\u00b7dem", "ein", "Ca\u00b7to", "itzt", "auff", "dei\u00b7nen", "thron", "ge\u00b7stie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NE", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}