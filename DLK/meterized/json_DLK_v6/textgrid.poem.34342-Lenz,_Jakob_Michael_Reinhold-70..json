{"textgrid.poem.34342": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "70.", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit sch\u00f6nen Steinen ausgeschm\u00fcckt,", "tokens": ["Mit", "sch\u00f6\u00b7nen", "Stei\u00b7nen", "aus\u00b7ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von frohen Lichtern angeblickt,", "tokens": ["Von", "fro\u00b7hen", "Lich\u00b7tern", "an\u00b7ge\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da sitzest du vielleicht anitzt,", "tokens": ["Da", "sit\u00b7zest", "du", "viel\u00b7leicht", "a\u00b7nitzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo doch dein Auge heller blitzt.", "tokens": ["Wo", "doch", "dein", "Au\u00b7ge", "hel\u00b7ler", "blitzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und denkest nicht, da\u00df hier in Nacht", "tokens": ["Und", "den\u00b7kest", "nicht", ",", "da\u00df", "hier", "in", "Nacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "KOUS", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein ausgeweintes Auge wacht,", "tokens": ["Ein", "aus\u00b7ge\u00b7wein\u00b7tes", "Au\u00b7ge", "wacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das \u00fcberall, wohin es flieht,", "tokens": ["Das", "\u00fc\u00b7be\u00b7rall", ",", "wo\u00b7hin", "es", "flieht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kein Mittel mich zu retten sieht.", "tokens": ["Kein", "Mit\u00b7tel", "mich", "zu", "ret\u00b7ten", "sieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dies Rei\u00dfen in der Stirn und Brust,", "tokens": ["Dies", "Rei\u00b7\u00dfen", "in", "der", "Stirn", "und", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "APPR", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Todesbote, meine Lust,", "tokens": ["Der", "To\u00b7des\u00b7bo\u00b7te", ",", "mei\u00b7ne", "Lust", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auch er, auch er l\u00e4\u00dft mich allein", "tokens": ["Auch", "er", ",", "auch", "er", "l\u00e4\u00dft", "mich", "al\u00b7lein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "$,", "ADV", "PPER", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ach der Bet\u00e4ubung dumpfer Pein.", "tokens": ["Ach", "der", "Be\u00b7t\u00e4u\u00b7bung", "dum\u00b7pfer", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Wo war ich doch, wer war ich doch? \u2013", "tokens": ["Wo", "war", "ich", "doch", ",", "wer", "war", "ich", "doch", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "$,", "PWS", "VAFIN", "PPER", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gef\u00fchl voll Angst! ich lebe noch.", "tokens": ["Ge\u00b7f\u00fchl", "voll", "Angst", "!", "ich", "le\u00b7be", "noch", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "$.", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich dachte schon, ich l\u00e4g in Ruh,", "tokens": ["Ich", "dach\u00b7te", "schon", ",", "ich", "l\u00e4g", "in", "Ruh", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Freundeshand die deckte zu.", "tokens": ["Und", "Freun\u00b7des\u00b7hand", "die", "deck\u00b7te", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ach aber Freundeshand bringt mir", "tokens": ["Ach", "a\u00b7ber", "Freun\u00b7des\u00b7hand", "bringt", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Kelch des Todes, und von dir,", "tokens": ["Den", "Kelch", "des", "To\u00b7des", ",", "und", "von", "dir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "KON", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von dir, von dir, mehr als der Tod", "tokens": ["Von", "dir", ",", "von", "dir", ",", "mehr", "als", "der", "Tod"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "APPR", "PPER", "$,", "PIAT", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was \u00fcberm Grabe schlimmer droht.", "tokens": ["Was", "\u00fc\u00b7berm", "Gra\u00b7be", "schlim\u00b7mer", "droht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Fern und verachtet und mi\u00dfkannt,", "tokens": ["Fern", "und", "ver\u00b7ach\u00b7tet", "und", "mi\u00df\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo niemand wei\u00df, wer mich verbannt!", "tokens": ["Wo", "nie\u00b7mand", "wei\u00df", ",", "wer", "mich", "ver\u00b7bannt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ach wie so gl\u00fccklich ist der Mann,", "tokens": ["Ach", "wie", "so", "gl\u00fcck\u00b7lich", "ist", "der", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ADV", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der dir zu F\u00fc\u00dfen sterben kann.", "tokens": ["Der", "dir", "zu", "F\u00fc\u00b7\u00dfen", "ster\u00b7ben", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ach w\u00e4rs auch nur vor deiner Th\u00fcr,", "tokens": ["Ach", "w\u00e4rs", "auch", "nur", "vor", "dei\u00b7ner", "Th\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VAFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vorm Thor der Stadt \u2013 nicht aber hier", "tokens": ["Vorm", "Thor", "der", "Stadt", "\u2013", "nicht", "a\u00b7ber", "hier"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "$(", "PTKNEG", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo ihn der Himmel selbst nicht kennt,", "tokens": ["Wo", "ihn", "der", "Him\u00b7mel", "selbst", "nicht", "kennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und kaum die Erd' ein Grabmal g\u00f6nnt!", "tokens": ["Und", "kaum", "die", "Erd'", "ein", "Grab\u00b7mal", "g\u00f6nnt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}