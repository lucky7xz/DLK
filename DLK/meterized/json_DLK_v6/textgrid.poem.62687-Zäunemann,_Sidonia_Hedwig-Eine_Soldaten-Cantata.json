{"textgrid.poem.62687": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Eine Soldaten-Cantata", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ins Feld!", "tokens": ["Ins", "Feld", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Wo man die Trommel r\u00fchret,", "tokens": ["Wo", "man", "die", "Trom\u00b7mel", "r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo man die Schwerder f\u00fchret,", "tokens": ["Wo", "man", "die", "Schwer\u00b7der", "f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da ist mein Sinn gestellt.", "tokens": ["Da", "ist", "mein", "Sinn", "ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So werd ich in dem Feld,", "tokens": ["So", "werd", "ich", "in", "dem", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ein Held.", "tokens": ["Ein", "Held", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "Weg tanzen, scherzen, spielen!", "tokens": ["Weg", "tan\u00b7zen", ",", "scher\u00b7zen", ",", "spie\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "VVINF", "$,", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Weg Putz und Z\u00e4rtlichkeit!", "tokens": ["Weg", "Putz", "und", "Z\u00e4rt\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Worauf das Aug der Jugend pflegt zu zielen,", "tokens": ["Wo\u00b7rauf", "das", "Aug", "der", "Ju\u00b7gend", "pflegt", "zu", "zie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das setze ich beysekt.", "tokens": ["Das", "set\u00b7ze", "ich", "bey\u00b7sekt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was Ruhm und Ehre schenket,", "tokens": ["Was", "Ruhm", "und", "Eh\u00b7re", "schen\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Woran die Nachwelt denket,", "tokens": ["Wo\u00b7ran", "die", "Nach\u00b7welt", "den\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Dasselbe liebe ich.", "tokens": ["Das\u00b7sel\u00b7be", "lie\u00b7be", "ich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Zwey Wege k\u00f6nnen mich,", "tokens": ["Zwey", "We\u00b7ge", "k\u00f6n\u00b7nen", "mich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VMFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Zum Ehren-Gipfel bringen;", "tokens": ["Zum", "Eh\u00b7ren\u00b7Gip\u00b7fel", "brin\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Zum ersten kan mirs durch den Kiel,", "tokens": ["Zum", "ers\u00b7ten", "kan", "mirs", "durch", "den", "Kiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VMFIN", "NE", "APPR", "ART", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Zum andern durch das Schwerd gelingen.", "tokens": ["Zum", "an\u00b7dern", "durch", "das", "Schwerd", "ge\u00b7lin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Tugend, Tapferkeit und Schwei\u00df,", "tokens": ["Tu\u00b7gend", ",", "Tap\u00b7fer\u00b7keit", "und", "Schwei\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schenken Ehre, Ruhm und Prei\u00df.", "tokens": ["Schen\u00b7ken", "Eh\u00b7re", ",", "Ruhm", "und", "Prei\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Drum la\u00df ich mir vor allen", "tokens": ["Drum", "la\u00df", "ich", "mir", "vor", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "APPR", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auch diese wohlgefallen,", "tokens": ["Auch", "die\u00b7se", "wohl\u00b7ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und sage auf der Klugheit ihr Gehei\u00df.", "tokens": ["Und", "sa\u00b7ge", "auf", "der", "Klug\u00b7heit", "ihr", "Ge\u00b7hei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die Tapferkeit erwirbet Ruhm und Prei\u00df", "tokens": ["Die", "Tap\u00b7fer\u00b7keit", "er\u00b7wir\u00b7bet", "Ruhm", "und", "Prei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich lasse andern gern", "tokens": ["Ich", "las\u00b7se", "an\u00b7dern", "gern"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Den Kern", "tokens": ["Den", "Kern"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Der Schriften und der B\u00fccher lesen;", "tokens": ["Der", "Schrif\u00b7ten", "und", "der", "B\u00fc\u00b7cher", "le\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich lasse ihnen gern das Wesen", "tokens": ["Ich", "las\u00b7se", "ih\u00b7nen", "gern", "das", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der kl\u00fcgsten M\u00e4nner wohl erkennen,", "tokens": ["Der", "kl\u00fcgs\u00b7ten", "M\u00e4n\u00b7ner", "wohl", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und vor dem Buch und Pult viel Oel verbrennen.", "tokens": ["Und", "vor", "dem", "Buch", "und", "Pult", "viel", "O\u00b7el", "ver\u00b7bren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "KON", "NN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Man lasse mir nur meinen Degen,", "tokens": ["Man", "las\u00b7se", "mir", "nur", "mei\u00b7nen", "De\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mit diesem geh ich meinem Feind,", "tokens": ["Mit", "die\u00b7sem", "geh", "ich", "mei\u00b7nem", "Feind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.10": {"text": "Erhitzt, getrost, voll Muth und Tapferkeit entgegen.", "tokens": ["Er\u00b7hitzt", ",", "ge\u00b7trost", ",", "voll", "Muth", "und", "Tap\u00b7fer\u00b7keit", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "$,", "ADJD", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ich will nicht rasten oder ruhn,", "tokens": ["Ich", "will", "nicht", "ras\u00b7ten", "o\u00b7der", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Ich kan denn meinem Willen ein Gen\u00fcge thun.", "tokens": ["Ich", "kan", "denn", "mei\u00b7nem", "Wil\u00b7len", "ein", "Ge\u00b7n\u00fc\u00b7ge", "thun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPOSAT", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich gehe auf den Feind, w\u00e4r er auch noch so gro\u00df,", "tokens": ["Ich", "ge\u00b7he", "auf", "den", "Feind", ",", "w\u00e4r", "er", "auch", "noch", "so", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "VAFIN", "PPER", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Beherzt und k\u00fchn, mit gleichem Entzweck lo\u00df.", "tokens": ["Be\u00b7herzt", "und", "k\u00fchn", ",", "mit", "glei\u00b7chem", "Ent\u00b7zweck", "lo\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Mein Arm soll ihn verderben,", "tokens": ["Mein", "Arm", "soll", "ihn", "ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Sein Todes-Schwei\u00df und Blut,", "tokens": ["Sein", "To\u00b7des\u00b7Schwei\u00df", "und", "Blut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Soll meinen Degen f\u00e4rben.", "tokens": ["Soll", "mei\u00b7nen", "De\u00b7gen", "f\u00e4r\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Und dieses soll mein Ruhm und Purpur heisen.", "tokens": ["Und", "die\u00b7ses", "soll", "mein", "Ruhm", "und", "Pur\u00b7pur", "hei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VMFIN", "PPOSAT", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "In dem Felde k\u00e4mpfen,", "tokens": ["In", "dem", "Fel\u00b7de", "k\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Und die Feinde d\u00e4mpfen,", "tokens": ["Und", "die", "Fein\u00b7de", "d\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ist die allersch\u00f6nste Ehr.", "tokens": ["Ist", "die", "al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "Ehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schlagen, streiten, ringen", "tokens": ["Schla\u00b7gen", ",", "strei\u00b7ten", ",", "rin\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Kan uns Palmen bringen.", "tokens": ["Kan", "uns", "Pal\u00b7men", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Drum so will ich bey dem Herr,", "tokens": ["Drum", "so", "will", "ich", "bey", "dem", "Herr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "In dem Felde k\u00e4mpfen,", "tokens": ["In", "dem", "Fel\u00b7de", "k\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Und die Feinde d\u00e4mpfen,", "tokens": ["Und", "die", "Fein\u00b7de", "d\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Denn die\u00df bringt die gr\u00f6\u00dfte Ehr.", "tokens": ["Denn", "die\u00df", "bringt", "die", "gr\u00f6\u00df\u00b7te", "Ehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Hier freu ich mich die Stadt mit zu best\u00fcrmen,", "tokens": ["Hier", "freu", "ich", "mich", "die", "Stadt", "mit", "zu", "be\u00b7st\u00fcr\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ART", "NN", "APPR", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und bin vergn\u00fcgt, wenn ich nach ihren Th\u00fcrmen/", "tokens": ["Und", "bin", "ver\u00b7gn\u00fcgt", ",", "wenn", "ich", "nach", "ih\u00b7ren", "Th\u00fcr\u00b7men", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVPP", "$,", "KOUS", "PPER", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Kugel richten soll.", "tokens": ["Die", "Ku\u00b7gel", "rich\u00b7ten", "soll", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Mauren, Schl\u00f6sser und Pal\u00e4ste", "tokens": ["Die", "Mau\u00b7ren", ",", "Schl\u00f6s\u00b7ser", "und", "Pa\u00b7l\u00e4s\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sind vor den donrenden Carthaunen nicht zu feste.", "tokens": ["Sind", "vor", "den", "don\u00b7ren\u00b7den", "Car\u00b7thau\u00b7nen", "nicht", "zu", "fes\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN", "PTKNEG", "PTKZU", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dort thue ich zur rechten Hand", "tokens": ["Dort", "thue", "ich", "zur", "rech\u00b7ten", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Den Feinden starken Widerstand.", "tokens": ["Den", "Fein\u00b7den", "star\u00b7ken", "Wi\u00b7der\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mein Stahl soll bald durch ihre Schwerder dringen,", "tokens": ["Mein", "Stahl", "soll", "bald", "durch", "ih\u00b7re", "Schwer\u00b7der", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Mit Freuden h\u00f6re ich die scharfen Sebel klingen,", "tokens": ["Mit", "Freu\u00b7den", "h\u00f6\u00b7re", "ich", "die", "schar\u00b7fen", "Se\u00b7bel", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ich scheue weder Schl\u00e4ge, Hieb noch Blut,", "tokens": ["Ich", "scheu\u00b7e", "we\u00b7der", "Schl\u00e4\u00b7ge", ",", "Hieb", "noch", "Blut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "NN", "$,", "ADV", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Ich bin beym Flug der Kugeln wohlgemuth.", "tokens": ["Ich", "bin", "beym", "Flug", "der", "Ku\u00b7geln", "wohl\u00b7ge\u00b7muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Ich f\u00fcrchte wahrlich auch", "tokens": ["Ich", "f\u00fcrch\u00b7te", "wahr\u00b7lich", "auch"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Mich im geringsten nicht vor Pulver und vor Rauch.", "tokens": ["Mich", "im", "ge\u00b7rings\u00b7ten", "nicht", "vor", "Pul\u00b7ver", "und", "vor", "Rauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "ADJA", "PTKNEG", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Kurz, ich will mir bey Kugeln und bey Fahnen,", "tokens": ["Kurz", ",", "ich", "will", "mir", "bey", "Ku\u00b7geln", "und", "bey", "Fah\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PPER", "VMFIN", "PPER", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Bey Waffen, Panzer, Spie\u00df und Degen,", "tokens": ["Bey", "Waf\u00b7fen", ",", "Pan\u00b7zer", ",", "Spie\u00df", "und", "De\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Den Weg zum Ehren-Tempel bahnen;", "tokens": ["Den", "Weg", "zum", "Eh\u00b7ren\u00b7Tem\u00b7pel", "bah\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Und ist mein Arm den Feinden \u00fcberlegen;", "tokens": ["Und", "ist", "mein", "Arm", "den", "Fein\u00b7den", "\u00fc\u00b7berl\u00b7e\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Ja, kan ich meine Hand, dem Degen und Chamaschen", "tokens": ["Ja", ",", "kan", "ich", "mei\u00b7ne", "Hand", ",", "dem", "De\u00b7gen", "und", "Cha\u00b7ma\u00b7schen"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "VMFIN", "PPER", "PPOSAT", "NN", "$,", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Mit meiner Feinde Blute waschen;", "tokens": ["Mit", "mei\u00b7ner", "Fein\u00b7de", "Blu\u00b7te", "wa\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "So wird man mich ins Held-Register schreiben,", "tokens": ["So", "wird", "man", "mich", "ins", "Held\u00b7Re\u00b7gis\u00b7ter", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "PRF", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Ich werde stets im Angedenken bleiben.", "tokens": ["Ich", "wer\u00b7de", "stets", "im", "An\u00b7ge\u00b7den\u00b7ken", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Und solt die Wahlstat auch die Seele von mir nehmen,", "tokens": ["Und", "solt", "die", "Wahl\u00b7stat", "auch", "die", "See\u00b7le", "von", "mir", "neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "ADV", "ART", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "So werd ich warlich mich des Bettes gar nicht sch\u00e4men.", "tokens": ["So", "werd", "ich", "war\u00b7lich", "mich", "des", "Bet\u00b7tes", "gar", "nicht", "sch\u00e4\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PPER", "ART", "NN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.he