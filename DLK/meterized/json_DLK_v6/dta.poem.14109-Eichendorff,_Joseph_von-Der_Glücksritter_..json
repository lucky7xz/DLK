{"dta.poem.14109": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Gl\u00fccksritter .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.57", "sv:0.42"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn Fortuna spr\u00f6de thut,", "tokens": ["Wenn", "For\u00b7tu\u00b7na", "spr\u00f6\u00b7de", "thut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "VVFIN", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.2": {"text": "La\u00df' ich sie in Ruh,", "tokens": ["La\u00df'", "ich", "sie", "in", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Singe recht und trinke gut,", "tokens": ["Sin\u00b7ge", "recht", "und", "trin\u00b7ke", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und Fortuna kriegt auch Muth,", "tokens": ["Und", "For\u00b7tu\u00b7na", "kriegt", "auch", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADV", "NN", "$,"], "meter": "-+----+", "measure": "dactylic.init"}, "line.5": {"text": "Setzt sich mit dazu.", "tokens": ["Setzt", "sich", "mit", "da\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PAV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}