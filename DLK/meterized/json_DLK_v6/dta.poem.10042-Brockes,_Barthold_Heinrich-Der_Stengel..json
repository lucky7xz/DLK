{"dta.poem.10042": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Stengel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bewundre, lieber Mensch, mit mir", "tokens": ["Be\u00b7wund\u00b7re", ",", "lie\u00b7ber", "Mensch", ",", "mit", "mir"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "ADV", "NN", "$,", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die schlancke, risch- und glatte Zier", "tokens": ["Die", "schlan\u00b7cke", ",", "risch", "und", "glat\u00b7te", "Zier"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "TRUNC", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur blo\u00df vom Stengel einer Bluhme!", "tokens": ["Nur", "blo\u00df", "vom", "Sten\u00b7gel", "ei\u00b7ner", "Bluh\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mich deucht, da\u00df, unserm GOTT zum Ruhme,", "tokens": ["Mich", "deucht", ",", "da\u00df", ",", "un\u00b7serm", "GoTT", "zum", "Ruh\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "$,", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich viel verwunderlichs iu dessen L\u00e4ng\u2019 und R\u00fcnde,", "tokens": ["Ich", "viel", "ver\u00b7wun\u00b7der\u00b7lichs", "i\u00b7u", "des\u00b7sen", "L\u00e4ng'", "und", "R\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "ADJA", "NN", "PRELAT", "NN", "KON", "NN", "$,"], "meter": "-+-+----+-+-+-", "measure": "unknown.measure.penta"}, "line.6": {"text": "Die beide sonder Fehler, finde.", "tokens": ["Die", "bei\u00b7de", "son\u00b7der", "Feh\u00b7ler", ",", "fin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es streckt ein Stengel sich recht nach der Linie,", "tokens": ["Es", "streckt", "ein", "Sten\u00b7gel", "sich", "recht", "nach", "der", "Li\u00b7nie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PRF", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ein wenig sich verd\u00fcnnet) in die H\u00f6h,", "tokens": ["Ein", "we\u00b7nig", "sich", "ver\u00b7d\u00fcn\u00b7net", ")", "in", "die", "H\u00f6h", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "PRF", "VVFIN", "$(", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gleich einer wol geformten Seule.", "tokens": ["Gleich", "ei\u00b7ner", "wol", "ge\u00b7form\u00b7ten", "Seu\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die biegsame Beschaffenheit,", "tokens": ["Die", "bieg\u00b7sa\u00b7me", "Be\u00b7schaf\u00b7fen\u00b7heit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das schlancke Wesen ihrer Theile,", "tokens": ["Das", "schlan\u00b7cke", "We\u00b7sen", "ih\u00b7rer", "Thei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Erh\u00e4lt sie, da\u00df auch starcke Winde", "tokens": ["Er\u00b7h\u00e4lt", "sie", ",", "da\u00df", "auch", "star\u00b7cke", "Win\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie nicht zerknicken,", "tokens": ["Sie", "nicht", "zer\u00b7kni\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Nicht zerst\u00fccken.", "tokens": ["Nicht", "zer\u00b7st\u00fc\u00b7cken", "."], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "So n\u00fctzt nicht weniger die vollenkommne R\u00fcnde", "tokens": ["So", "n\u00fctzt", "nicht", "we\u00b7ni\u00b7ger", "die", "vol\u00b7len\u00b7komm\u00b7ne", "R\u00fcn\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PTKNEG", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dazu, da\u00df nichts sich an sie setzen,", "tokens": ["Da\u00b7zu", ",", "da\u00df", "nichts", "sich", "an", "sie", "set\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "KOUS", "PIS", "PRF", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nichts an sie hafften, sie verletzen,", "tokens": ["Nichts", "an", "sie", "haff\u00b7ten", ",", "sie", "ver\u00b7let\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "APPR", "PPER", "VVFIN", "$,", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Feuchtigkeit sie nicht zur F\u00e4ulni\u00df bringen kann.", "tokens": ["Und", "Feuch\u00b7tig\u00b7keit", "sie", "nicht", "zur", "F\u00e4ul\u00b7ni\u00df", "brin\u00b7gen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPER", "PTKNEG", "APPRART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Absonderlich seh ich die Stell\u2019 erstaunet an,", "tokens": ["Ab\u00b7son\u00b7der\u00b7lich", "seh", "ich", "die", "Stell'", "er\u00b7stau\u00b7net", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Woselbst die Bluhme sich mit ihm verbindet,", "tokens": ["Wo\u00b7selbst", "die", "Bluh\u00b7me", "sich", "mit", "ihm", "ver\u00b7bin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PRF", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Indem man kein Gelenck, wie sehr man suchet, findet,", "tokens": ["In\u00b7dem", "man", "kein", "Ge\u00b7lenck", ",", "wie", "sehr", "man", "su\u00b7chet", ",", "fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PIS", "PIAT", "NN", "$,", "PWAV", "ADV", "PIS", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durch welche sie verkn\u00fcpft. Wer fasst, auf welche Weise", "tokens": ["Durch", "wel\u00b7che", "sie", "ver\u00b7kn\u00fcpft", ".", "Wer", "fasst", ",", "auf", "wel\u00b7che", "Wei\u00b7se"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "VVPP", "$.", "PWS", "VVFIN", "$,", "APPR", "PWAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Bl\u00e4tter, deren Zahl meist sechs in einem Kreise,", "tokens": ["Die", "Bl\u00e4t\u00b7ter", ",", "de\u00b7ren", "Zahl", "meist", "sechs", "in", "ei\u00b7nem", "Krei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELAT", "NN", "ADV", "CARD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So schnell daselbst entstehn?", "tokens": ["So", "schnell", "da\u00b7selbst", "ent\u00b7stehn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PAV", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wer fasset, wie es m\u00f6glich sey,", "tokens": ["Wer", "fas\u00b7set", ",", "wie", "es", "m\u00f6g\u00b7lich", "sey", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PWAV", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df ein so sch\u00f6nes Gr\u00fcn daselbst in roth, in wei\u00df,", "tokens": ["Da\u00df", "ein", "so", "sch\u00f6\u00b7nes", "Gr\u00fcn", "da\u00b7selbst", "in", "roth", ",", "in", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADV", "ADJA", "NN", "PAV", "APPR", "ADJD", "$,", "APPR", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "In gelb, in blau, und in so mancherley", "tokens": ["In", "gelb", ",", "in", "blau", ",", "und", "in", "so", "man\u00b7cher\u00b7ley"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "$,", "APPR", "ADJD", "$,", "KON", "APPR", "ADV", "PIAT"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Auf einmahl sich verkehre?", "tokens": ["Auf", "ein\u00b7mahl", "sich", "ver\u00b7keh\u00b7re", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PRF", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Kein Mensch. Und eben die\u00df gereichet GOTT zur Ehre.", "tokens": ["Kein", "Mensch", ".", "Und", "e\u00b7ben", "die\u00df", "ge\u00b7rei\u00b7chet", "GoTT", "zur", "Eh\u00b7re", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$.", "KON", "ADV", "PDS", "VVFIN", "NE", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der Menschen Einfalt selbst erhebt des Sch\u00f6pfers Preis:", "tokens": ["Der", "Men\u00b7schen", "Ein\u00b7falt", "selbst", "er\u00b7hebt", "des", "Sch\u00f6p\u00b7fers", "Preis", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Sie zeigt uns unser Nichts, und unsers GOttes Gr\u00f6sse,", "tokens": ["Sie", "zeigt", "uns", "un\u00b7ser", "Nichts", ",", "und", "un\u00b7sers", "Got\u00b7tes", "Gr\u00f6s\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "KON", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Des Sch\u00f6pfers Majest\u00e4t, und unsers Geistes Bl\u00f6sse.", "tokens": ["Des", "Sch\u00f6p\u00b7fers", "Ma\u00b7jes\u00b7t\u00e4t", ",", "und", "un\u00b7sers", "Geis\u00b7tes", "Bl\u00f6s\u00b7se", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "KON", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Erkennt denn einen GOtt in diesem Wunder-Wercke,", "tokens": ["Er\u00b7kennt", "denn", "ei\u00b7nen", "Gott", "in", "die\u00b7sem", "Wun\u00b7der\u00b7\u00b7Wer\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und lobt, in eurer Lust, Desselben Lieb und St\u00e4rcke.", "tokens": ["Und", "lobt", ",", "in", "eu\u00b7rer", "Lust", ",", "Des\u00b7sel\u00b7ben", "Lieb", "und", "St\u00e4r\u00b7cke", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "$,", "PDAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Wann nun den nahen GOtt euch gar der Stengel zeiget,", "tokens": ["Wann", "nun", "den", "na\u00b7hen", "Gott", "euch", "gar", "der", "Sten\u00b7gel", "zei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So h\u00fctet euch, verblendet das Gesicht,", "tokens": ["So", "h\u00fc\u00b7tet", "euch", ",", "ver\u00b7blen\u00b7det", "das", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So gar bey aller Pracht der sch\u00f6nen Bluhmen, nicht.", "tokens": ["So", "gar", "bey", "al\u00b7ler", "Pracht", "der", "sch\u00f6\u00b7nen", "Bluh\u00b7men", ",", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PIAT", "NN", "ART", "ADJA", "NN", "$,", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und seid doch k\u00fcnftighin, mehr als bisher, geneiget,", "tokens": ["Und", "seid", "doch", "k\u00fcnf\u00b7tig\u00b7hin", ",", "mehr", "als", "bis\u00b7her", ",", "ge\u00b7nei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "$,", "PIAT", "KOKOM", "ADV", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Durch euer eigenes Vergn\u00fcgen", "tokens": ["Durch", "eu\u00b7er", "ei\u00b7ge\u00b7nes", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "An Seinem Werck\u2019, euch selbst zu Jhm zu f\u00fcgen!", "tokens": ["An", "Sei\u00b7nem", "Werck", ",", "euch", "selbst", "zu", "Jhm", "zu", "f\u00fc\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPER", "ADV", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}