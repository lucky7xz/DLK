{"textgrid.poem.58453": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "491. Mensch, du tr\u00e4umst!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer nicht bei Gott im Herzen bleibt", "tokens": ["Wer", "nicht", "bei", "Gott", "im", "Her\u00b7zen", "bleibt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PTKNEG", "APPR", "NN", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und seine Zeit mit ihm vertreibt,", "tokens": ["Und", "sei\u00b7ne", "Zeit", "mit", "ihm", "ver\u00b7treibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hat seine Zeit vers\u00e4umet,", "tokens": ["Hat", "sei\u00b7ne", "Zeit", "ver\u00b7s\u00e4u\u00b7met", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als wenn er h\u00e4tt' getr\u00e4umet.", "tokens": ["Als", "wenn", "er", "h\u00e4tt'", "ge\u00b7tr\u00e4u\u00b7met", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}