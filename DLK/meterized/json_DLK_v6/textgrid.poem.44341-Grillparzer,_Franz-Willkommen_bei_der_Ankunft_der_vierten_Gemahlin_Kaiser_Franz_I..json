{"textgrid.poem.44341": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Willkommen bei der Ankunft der vierten Gemahlin Kaiser Franz I.", "genre": "verse", "period": "N.A.", "pub_year": 1816, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hab sie gesehen,", "tokens": ["Ich", "hab", "sie", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Apart und genau,", "tokens": ["A\u00b7part", "und", "ge\u00b7nau", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ich hab sie gesehen,", "tokens": ["Ich", "hab", "sie", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die herrliche Frau:", "tokens": ["Die", "herr\u00b7li\u00b7che", "Frau", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Ja, staunet nur, staunet!", "tokens": ["Ja", ",", "stau\u00b7net", "nur", ",", "stau\u00b7net", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "ADV", "$,", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ich stand dort am Rain", "tokens": ["Ich", "stand", "dort", "am", "Rain"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und trieb meine G\u00e4nse", "tokens": ["Und", "trieb", "mei\u00b7ne", "G\u00e4n\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ins Wasser hinein,", "tokens": ["Ins", "Was\u00b7ser", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Und wie wir so stehen,", "tokens": ["Und", "wie", "wir", "so", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ein jedes f\u00fcr sich,", "tokens": ["Ein", "je\u00b7des", "f\u00fcr", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "APPR", "PRF", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und schauen, der Entrich,", "tokens": ["Und", "schau\u00b7en", ",", "der", "Ent\u00b7rich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mein Pudel und ich,", "tokens": ["Mein", "Pu\u00b7del", "und", "ich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPER", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Da hebt sichs von ferne,", "tokens": ["Da", "hebt", "sichs", "von", "fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ADJA", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Da wirbelt der Staub,", "tokens": ["Da", "wir\u00b7belt", "der", "Staub", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Da kommt es gerasselt", "tokens": ["Da", "kommt", "es", "ge\u00b7ras\u00b7selt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVPP"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Durchs fallende Laub.", "tokens": ["Durchs", "fal\u00b7len\u00b7de", "Laub", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Ein Zug kommt geflogen", "tokens": ["Ein", "Zug", "kommt", "ge\u00b7flo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "VVPP"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "In goldener Pracht,", "tokens": ["In", "gol\u00b7de\u00b7ner", "Pracht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Wie Wolken, wenn morgens", "tokens": ["Wie", "Wol\u00b7ken", ",", "wenn", "mor\u00b7gens"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PWAV", "NN", "$,", "KOUS", "ADV"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die Sonne erwacht.", "tokens": ["Die", "Son\u00b7ne", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Und mitten ein Wagen,", "tokens": ["Und", "mit\u00b7ten", "ein", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ganz schlicht, ohne Glanz,", "tokens": ["Ganz", "schlicht", ",", "oh\u00b7ne", "Glanz", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUI", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Doch gl\u00e4nzt er vor allen,", "tokens": ["Doch", "gl\u00e4nzt", "er", "vor", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PIAT", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Er f\u00fchrt unsern Franz,", "tokens": ["Er", "f\u00fchrt", "un\u00b7sern", "Franz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.7": {"line.1": {"text": "Und an seiner Seite", "tokens": ["Und", "an", "sei\u00b7ner", "Sei\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Sa\u00df, lieblich und mild,", "tokens": ["Sa\u00df", ",", "lieb\u00b7lich", "und", "mild", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "In z\u00fcchtigem Schweigen", "tokens": ["In", "z\u00fcch\u00b7ti\u00b7gem", "Schwei\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ein Frauenbild.", "tokens": ["Ein", "Frau\u00b7en\u00b7bild", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Ha, dacht ich mir selber,", "tokens": ["Ha", ",", "dacht", "ich", "mir", "sel\u00b7ber", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "PPER", "ADV", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wer mag das wohl sein?", "tokens": ["Wer", "mag", "das", "wohl", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PDS", "ADV", "VAINF", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Dem Herren zur Seite", "tokens": ["Dem", "Her\u00b7ren", "zur", "Sei\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Mu\u00df Herrliches sein.", "tokens": ["Mu\u00df", "Herr\u00b7li\u00b7ches", "sein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "VAINF", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.9": {"line.1": {"text": "Ich schau ihr ins Auge,", "tokens": ["Ich", "schau", "ihr", "ins", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Da trifft mich ihr Blick,", "tokens": ["Da", "trifft", "mich", "ihr", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Noch denk ich mit Zittern", "tokens": ["Noch", "denk", "ich", "mit", "Zit\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und Wonne zur\u00fcck.", "tokens": ["Und", "Won\u00b7ne", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.10": {"line.1": {"text": "Daheim in der Kirche,", "tokens": ["Da\u00b7heim", "in", "der", "Kir\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Am hohen Altar,", "tokens": ["Am", "ho\u00b7hen", "Al\u00b7tar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Da stehet ein Bildnis,", "tokens": ["Da", "ste\u00b7het", "ein", "Bild\u00b7nis", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "So herrlich und klar:", "tokens": ["So", "herr\u00b7lich", "und", "klar", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.11": {"line.1": {"text": "Die Mutter des Heilands", "tokens": ["Die", "Mut\u00b7ter", "des", "Hei\u00b7lands"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Am Sternenthron,", "tokens": ["Am", "Ster\u00b7nen\u00b7thron", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "In liebenden Armen", "tokens": ["In", "lie\u00b7ben\u00b7den", "Ar\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Den g\u00f6ttlichen Sohn.", "tokens": ["Den", "g\u00f6tt\u00b7li\u00b7chen", "Sohn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.12": {"line.1": {"text": "Mit freundlicher Wehmut,", "tokens": ["Mit", "freund\u00b7li\u00b7cher", "Weh\u00b7mut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "So trostreich und lind,", "tokens": ["So", "trost\u00b7reich", "und", "lind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Verweilet ihr Auge", "tokens": ["Ver\u00b7wei\u00b7let", "ihr", "Au\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Am schlafenden Kind;", "tokens": ["Am", "schla\u00b7fen\u00b7den", "Kind", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.13": {"line.1": {"text": "Sie scheints zu geleiten", "tokens": ["Sie", "scheints", "zu", "ge\u00b7lei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKZU", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Auf k\u00fcnftiger Bahn \u2013", "tokens": ["Auf", "k\u00fcnf\u00b7ti\u00b7ger", "Bahn", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "So sah mich die Hohe,", "tokens": ["So", "sah", "mich", "die", "Ho\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die Liebliche an.", "tokens": ["Die", "Lieb\u00b7li\u00b7che", "an", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.14": {"line.1": {"text": "O Blick ohnegleichen,", "tokens": ["O", "Blick", "oh\u00b7ne\u00b7glei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Voll himmlischem Sinn,", "tokens": ["Voll", "himm\u00b7li\u00b7schem", "Sinn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Er stammet vom Himmel", "tokens": ["Er", "stam\u00b7met", "vom", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und f\u00fchret dahin.", "tokens": ["Und", "f\u00fch\u00b7ret", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.15": {"line.1": {"text": "Da stand ich und staunte,", "tokens": ["Da", "stand", "ich", "und", "staun\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Mein selbst nicht bewu\u00dft,", "tokens": ["Mein", "selbst", "nicht", "be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Mit tr\u00e4nenden Augen,", "tokens": ["Mit", "tr\u00e4\u00b7nen\u00b7den", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Mit schwellender Brust.", "tokens": ["Mit", "schwel\u00b7len\u00b7der", "Brust", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.16": {"line.1": {"text": "Jetzt l\u00e4chelt die Hohe,", "tokens": ["Jetzt", "l\u00e4\u00b7chelt", "die", "Ho\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Da fuhrs durch mich hin:", "tokens": ["Da", "fuhrs", "durch", "mich", "hin", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Es ist unsre Mutter,", "tokens": ["Es", "ist", "uns\u00b7re", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die Kaiserin!", "tokens": ["Die", "Kai\u00b7se\u00b7rin", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.17": {"line.1": {"text": "Nun will ich sie gr\u00fc\u00dfen,", "tokens": ["Nun", "will", "ich", "sie", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ich suche das Wort,", "tokens": ["Ich", "su\u00b7che", "das", "Wort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Da rauscht es vor\u00fcber,", "tokens": ["Da", "rauscht", "es", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die Holde war fort.", "tokens": ["Die", "Hol\u00b7de", "war", "fort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.18": {"line.1": {"text": "Ich Alberner r\u00fcckte", "tokens": ["Ich", "Al\u00b7ber\u00b7ner", "r\u00fcck\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["PPER", "NE", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Nicht einmal den Hut,", "tokens": ["Nicht", "ein\u00b7mal", "den", "Hut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Nun wird sie wohl glauben,", "tokens": ["Nun", "wird", "sie", "wohl", "glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ich sei ihr nicht gut,", "tokens": ["Ich", "sei", "ihr", "nicht", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.19": {"line.1": {"text": "Glaubt wohl, da\u00df in \u00d6streich", "tokens": ["Glaubt", "wohl", ",", "da\u00df", "in", "\u00d6s\u00b7treich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "KOUS", "APPR", "NE"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ein einziger sei,", "tokens": ["Ein", "ein\u00b7zi\u00b7ger", "sei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Der sich ihrer Ankunft,", "tokens": ["Der", "sich", "ih\u00b7rer", "An\u00b7kunft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Sich ihrer nicht freu!", "tokens": ["Sich", "ih\u00b7rer", "nicht", "freu", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "PTKNEG", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.20": {"line.1": {"text": "Noch heut soll sie kommen,", "tokens": ["Noch", "heut", "soll", "sie", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ich wei\u00df es, zur Stadt,", "tokens": ["Ich", "wei\u00df", "es", ",", "zur", "Stadt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "APPRART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Da sehet ihr gl\u00fccklichen", "tokens": ["Da", "se\u00b7het", "ihr", "gl\u00fcck\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVINF"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "St\u00e4dter euch satt.", "tokens": ["St\u00e4d\u00b7ter", "euch", "satt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.21": {"line.1": {"text": "Wenn ihr nun ihr zuruft", "tokens": ["Wenn", "ihr", "nun", "ihr", "zu\u00b7ruft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PPER", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "In Freudenergu\u00df,", "tokens": ["In", "Freu\u00b7de\u00b7ner\u00b7gu\u00df", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "So bringt ihr auch meinen", "tokens": ["So", "bringt", "ihr", "auch", "mei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPOSAT"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Versp\u00e4teten Gru\u00df,", "tokens": ["Ver\u00b7sp\u00e4\u00b7te\u00b7ten", "Gru\u00df", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.22": {"line.1": {"text": "Und sagt ihr: Der Junge", "tokens": ["Und", "sagt", "ihr", ":", "Der", "Jun\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$.", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Da drau\u00dfen am Bach,", "tokens": ["Da", "drau\u00b7\u00dfen", "am", "Bach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Er stehe an Liebe", "tokens": ["Er", "ste\u00b7he", "an", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Den Besten nicht nach.", "tokens": ["Den", "Bes\u00b7ten", "nicht", "nach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.23": {"line.1": {"text": "F\u00fcr sie unser Leben,", "tokens": ["F\u00fcr", "sie", "un\u00b7ser", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "F\u00fcr sie unser Blut!", "tokens": ["F\u00fcr", "sie", "un\u00b7ser", "Blut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Kein einzger in \u00d6streich,", "tokens": ["Kein", "einz\u00b7ger", "in", "\u00d6s\u00b7treich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "APPR", "NE", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "