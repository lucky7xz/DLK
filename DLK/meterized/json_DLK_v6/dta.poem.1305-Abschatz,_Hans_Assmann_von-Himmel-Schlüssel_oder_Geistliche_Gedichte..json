{"dta.poem.1305": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Himmel-Schl\u00fcssel  \n oder  \n Geistliche Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Was ist des Menschen Zeit und Leben/ als ein Tag/", "tokens": ["Was", "ist", "des", "Men\u00b7schen", "Zeit", "und", "Le\u00b7ben", "/", "als", "ein", "Tag", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "NN", "KON", "NN", "$(", "KOUS", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der einer d\u00fcstren Nacht bey nahe gleichen mag;", "tokens": ["Der", "ei\u00b7ner", "d\u00fcst\u00b7ren", "Nacht", "bey", "na\u00b7he", "glei\u00b7chen", "mag", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "APPR", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Morgen geht dahin/ eh des Verstandes Licht", "tokens": ["Der", "Mor\u00b7gen", "geht", "da\u00b7hin", "/", "eh", "des", "Ver\u00b7stan\u00b7des", "Licht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PAV", "$(", "KOUS", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sich von der Finsternis der jungen Jahr entbricht.", "tokens": ["Sich", "von", "der", "Fins\u00b7ter\u00b7nis", "der", "jun\u00b7gen", "Jahr", "ent\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der kurtze Mittag schlie\u00dft den zweifelhafften Schein", "tokens": ["Der", "kurt\u00b7ze", "Mit\u00b7tag", "schlie\u00dft", "den", "zwei\u00b7fel\u00b7haff\u00b7ten", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Ehren und des Gl\u00fccks in enge Schrancken ein/", "tokens": ["Der", "Eh\u00b7ren", "und", "des", "Gl\u00fccks", "in", "en\u00b7ge", "Schran\u00b7cken", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "APPR", "ADJA", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ein fr\u00fcher Abend raubt unfehlbar Sonn und Licht/", "tokens": ["Ein", "fr\u00fc\u00b7her", "A\u00b7bend", "raubt", "un\u00b7fehl\u00b7bar", "Sonn", "und", "Licht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df auch der Morgen offt und Mittag unterbricht/", "tokens": ["Da\u00df", "auch", "der", "Mor\u00b7gen", "offt", "und", "Mit\u00b7tag", "un\u00b7ter\u00b7bricht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADV", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Auff diesen Abend folgt des Grabes schwartze Nacht.", "tokens": ["Auff", "die\u00b7sen", "A\u00b7bend", "folgt", "des", "Gra\u00b7bes", "schwart\u00b7ze", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wohl dem/ der so den Tag des Lebens zugebracht/", "tokens": ["Wohl", "dem", "/", "der", "so", "den", "Tag", "des", "Le\u00b7bens", "zu\u00b7ge\u00b7bracht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$(", "ART", "ADV", "ART", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Da\u00df er die stille Nacht in s\u00fcsser Ruh verschlie\u00dft/", "tokens": ["Da\u00df", "er", "die", "stil\u00b7le", "Nacht", "in", "s\u00fcs\u00b7ser", "Ruh", "ver\u00b7schlie\u00dft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und fr\u00f6lich mit der Zeit den andern Morgen gr\u00fc\u00dft/", "tokens": ["Und", "fr\u00f6\u00b7lich", "mit", "der", "Zeit", "den", "an\u00b7dern", "Mor\u00b7gen", "gr\u00fc\u00dft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der einen ewigs Licht und Leben hoffen heist/", "tokens": ["Der", "ei\u00b7nen", "e\u00b7wigs", "Licht", "und", "Le\u00b7ben", "hof\u00b7fen", "heist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "KON", "NN", "VVINF", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Den andern in das Reich der steten Nacht verweist.", "tokens": ["Den", "an\u00b7dern", "in", "das", "Reich", "der", "ste\u00b7ten", "Nacht", "ver\u00b7weist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "O Licht/ von welchem ich empfangen Schein und Licht/", "tokens": ["O", "Licht", "/", "von", "wel\u00b7chem", "ich", "emp\u00b7fan\u00b7gen", "Schein", "und", "Licht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$(", "APPR", "PRELS", "PPER", "VVPP", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Mein Morgen wei\u00df ohn dich von keinem Lichte nicht/", "tokens": ["Mein", "Mor\u00b7gen", "wei\u00df", "ohn", "dich", "von", "kei\u00b7nem", "Lich\u00b7te", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PRF", "APPR", "PIAT", "NN", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Mein Mittag mu\u00df ohn dich seyn d\u00fcstre Mitternacht/", "tokens": ["Mein", "Mit\u00b7tag", "mu\u00df", "ohn", "dich", "seyn", "d\u00fcst\u00b7re", "Mit\u00b7ter\u00b7nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "APPR", "PPER", "VAINF", "VVFIN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Mein Abend wird ohn dich in Schrecken zugebracht/", "tokens": ["Mein", "A\u00b7bend", "wird", "ohn", "dich", "in", "Schre\u00b7cken", "zu\u00b7ge\u00b7bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "PRF", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Drauff folget eine Nacht/ die mir in Ewigkeit", "tokens": ["Drauff", "fol\u00b7get", "ei\u00b7ne", "Nacht", "/", "die", "mir", "in", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "NN", "$(", "PRELS", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Mit schwerer Finsternis und tr\u00fcben Schatten dr\u00e4ut.", "tokens": ["Mit", "schwe\u00b7rer", "Fins\u00b7ter\u00b7nis", "und", "tr\u00fc\u00b7ben", "Schat\u00b7ten", "dr\u00e4ut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Der Morgen ist nunmehr durch deine Gunst vorbey/", "tokens": ["Der", "Mor\u00b7gen", "ist", "nun\u00b7mehr", "durch", "dei\u00b7ne", "Gunst", "vor\u00b7bey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Darinn kein Augenblick von finstren Wercken frey/", "tokens": ["Da\u00b7rinn", "kein", "Au\u00b7gen\u00b7blick", "von", "finst\u00b7ren", "Wer\u00b7cken", "frey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIAT", "NN", "APPR", "ADJA", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Dieselben la\u00df/ verbannt vor deinem Angesicht/", "tokens": ["Die\u00b7sel\u00b7ben", "la\u00df", "/", "ver\u00b7bannt", "vor", "dei\u00b7nem", "An\u00b7ge\u00b7sicht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$(", "VVPP", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "In stete Finsterni\u00df/ mich weiter schrecken nicht;", "tokens": ["In", "ste\u00b7te", "Fins\u00b7ter\u00b7ni\u00df", "/", "mich", "wei\u00b7ter", "schre\u00b7cken", "nicht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$(", "PPER", "ADV", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Den Mittag meiner Zeit und Jahre tret ich an/", "tokens": ["Den", "Mit\u00b7tag", "mei\u00b7ner", "Zeit", "und", "Jah\u00b7re", "tret", "ich", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "KON", "NN", "VVFIN", "PPER", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Gieb/ da\u00df ich von dir Licht und Sonne sch\u00f6pffen kan/", "tokens": ["Gieb", "/", "da\u00df", "ich", "von", "dir", "Licht", "und", "Son\u00b7ne", "sch\u00f6pf\u00b7fen", "kan", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$(", "KOUS", "PPER", "APPR", "PPER", "NN", "KON", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Da\u00df sich mein Auge lenckt nach deiner Lehre Pol/", "tokens": ["Da\u00df", "sich", "mein", "Au\u00b7ge", "lenckt", "nach", "dei\u00b7ner", "Leh\u00b7re", "Pol", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Bi\u00df ich den eitlen Glantz der Welt gesegnen soll/", "tokens": ["Bi\u00df", "ich", "den", "eit\u00b7len", "Glantz", "der", "Welt", "ge\u00b7seg\u00b7nen", "soll", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ART", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Und von der tr\u00fcben Nacht des Todes unerschreckt/", "tokens": ["Und", "von", "der", "tr\u00fc\u00b7ben", "Nacht", "des", "To\u00b7des", "un\u00b7er\u00b7schreckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Zur frohen Ewigkeit von dir werd aufferweckt.", "tokens": ["Zur", "fro\u00b7hen", "E\u00b7wig\u00b7keit", "von", "dir", "werd", "auf\u00b7fer\u00b7weckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}