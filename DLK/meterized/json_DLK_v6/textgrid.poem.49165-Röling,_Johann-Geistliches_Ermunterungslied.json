{"textgrid.poem.49165": {"metadata": {"author": {"name": "R\u00f6ling, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Geistliches Ermunterungslied", "genre": "verse", "period": "N.A.", "pub_year": 1656, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf, erheb' dich, meine Seel'", "tokens": ["Auf", ",", "er\u00b7heb'", "dich", ",", "mei\u00b7ne", "Seel'"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "$,", "VVFIN", "PPER", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus der tiefen Schwermuths-H\u00f6hl'", "tokens": ["Aus", "der", "tie\u00b7fen", "Schwer\u00b7muths\u00b7H\u00f6hl'"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Hin zu jenen milden H\u00f6hen,", "tokens": ["Hin", "zu", "je\u00b7nen", "mil\u00b7den", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo dir so viel Jahre her", "tokens": ["Wo", "dir", "so", "viel", "Jah\u00b7re", "her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "PIAT", "NN", "APZR"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "So viel Liebe, Gut's und Ehr'", "tokens": ["So", "viel", "Lie\u00b7be", ",", "Gut's", "und", "Ehr'"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "$,", "NE", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Unverdienet ist geschehen,", "tokens": ["Un\u00b7ver\u00b7die\u00b7net", "ist", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und woher noch t\u00e4glich flie\u00dft,", "tokens": ["Und", "wo\u00b7her", "noch", "t\u00e4g\u00b7lich", "flie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Was dein Herz und Mund genie\u00dft.", "tokens": ["Was", "dein", "Herz", "und", "Mund", "ge\u00b7nie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Achte nicht, ob dir hierbei", "tokens": ["Ach\u00b7te", "nicht", ",", "ob", "dir", "hier\u00b7bei"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PTKNEG", "$,", "KOUS", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6ll' und Welt entgegen sei,", "tokens": ["H\u00f6ll'", "und", "Welt", "ent\u00b7ge\u00b7gen", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PTKVZ", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lass' sie schm\u00e4hen, w\u00fcthen, toben,", "tokens": ["Lass'", "sie", "schm\u00e4\u00b7hen", ",", "w\u00fct\u00b7hen", ",", "to\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dies hast du mit Gott gemein;", "tokens": ["Dies", "hast", "du", "mit", "Gott", "ge\u00b7mein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "APPR", "NN", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "H\u00fcll' dich fr\u00f6hlich in dich ein.", "tokens": ["H\u00fcll'", "dich", "fr\u00f6h\u00b7lich", "in", "dich", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+++", "measure": "unknown.measure.penta"}, "line.6": {"text": "Deine H\u00fclfe kommt von oben,", "tokens": ["Dei\u00b7ne", "H\u00fcl\u00b7fe", "kommt", "von", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Von den Bergen, von dem Thron", "tokens": ["Von", "den", "Ber\u00b7gen", ",", "von", "dem", "Thron"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Dessen, der dein grosser Lohn.", "tokens": ["Des\u00b7sen", ",", "der", "dein", "gros\u00b7ser", "Lohn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ja, da wohnt f\u00fcr dich nur Rath,", "tokens": ["Ja", ",", "da", "wohnt", "f\u00fcr", "dich", "nur", "Rath", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "VVFIN", "APPR", "PPER", "ADV", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo der seinen Wohnsitz hat,", "tokens": ["Wo", "der", "sei\u00b7nen", "Wohn\u00b7sitz", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der die Himmelskreis' erf\u00fcllet", "tokens": ["Der", "die", "Him\u00b7mels\u00b7kreis'", "er\u00b7f\u00fcl\u00b7let"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Erden Grund bestellt.", "tokens": ["Und", "der", "Er\u00b7den", "Grund", "be\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Liebt dich gleich die ganze Welt,", "tokens": ["Liebt", "dich", "gleich", "die", "gan\u00b7ze", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird dein Wunsch doch nicht gestillet,", "tokens": ["Wird", "dein", "Wunsch", "doch", "nicht", "ge\u00b7stil\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und ist dir dein Freund allein,", "tokens": ["Und", "ist", "dir", "dein", "Freund", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PPOSAT", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Kannst du unbek\u00fcmmert sein.", "tokens": ["Kannst", "du", "un\u00b7be\u00b7k\u00fcm\u00b7mert", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Dies macht, o sein Conterfeit,", "tokens": ["Dies", "macht", ",", "o", "sein", "Con\u00b7ter\u00b7feit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "FM", "PPOSAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da\u00df sein Bild der Ewigkeit", "tokens": ["Da\u00df", "sein", "Bild", "der", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deinem Wesen eingepr\u00e4get.", "tokens": ["Dei\u00b7nem", "We\u00b7sen", "ein\u00b7ge\u00b7pr\u00e4\u00b7get", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "G\u00f6ttlich bist du so von Blut,", "tokens": ["G\u00f6tt\u00b7lich", "bist", "du", "so", "von", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "G\u00f6ttlich mu\u00df auch sein das Gut", "tokens": ["G\u00f6tt\u00b7lich", "mu\u00df", "auch", "sein", "das", "Gut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VMFIN", "ADV", "PPOSAT", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Woraus sich dein Geist verpfleget.", "tokens": ["Wo\u00b7raus", "sich", "dein", "Geist", "ver\u00b7pfle\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Was der Erden zugeh\u00f6rt,", "tokens": ["Was", "der", "Er\u00b7den", "zu\u00b7ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ist nicht Furcht noch Liebe werth.", "tokens": ["Ist", "nicht", "Furcht", "noch", "Lie\u00b7be", "werth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "NN", "ADV", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ach mein Gott, dies wei\u00df mein Herz,", "tokens": ["Ach", "mein", "Gott", ",", "dies", "wei\u00df", "mein", "Herz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPOSAT", "NN", "$,", "PDS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und doch kehrt es unterw\u00e4rts!", "tokens": ["Und", "doch", "kehrt", "es", "un\u00b7ter\u00b7w\u00e4rts", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Bin ich noch so frisch zuweilen", "tokens": ["Bin", "ich", "noch", "so", "frisch", "zu\u00b7wei\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und erhebe mich zu dir,", "tokens": ["Und", "er\u00b7he\u00b7be", "mich", "zu", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Will mein Fleisch doch nicht mit mir;", "tokens": ["Will", "mein", "Fleisch", "doch", "nicht", "mit", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "PTKNEG", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dieser Zwist von beiden Theilen,", "tokens": ["Die\u00b7ser", "Zwist", "von", "bei\u00b7den", "Thei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Drin ich sonder Ende steh'", "tokens": ["Drin", "ich", "son\u00b7der", "En\u00b7de", "steh'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "APPR", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Thut mir mehr als schmerzlich weh.", "tokens": ["Thut", "mir", "mehr", "als", "schmerz\u00b7lich", "weh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "KOKOM", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Meine Kraft denn ist es nicht", "tokens": ["Mei\u00b7ne", "Kraft", "denn", "ist", "es", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "VAFIN", "PPER", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df ich mich zu dir aufricht,", "tokens": ["Da\u00df", "ich", "mich", "zu", "dir", "auf\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nein, nur du kannst diese geben.", "tokens": ["Nein", ",", "nur", "du", "kannst", "die\u00b7se", "ge\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "PPER", "VMFIN", "PDS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mach' mich deines Geistes voll,", "tokens": ["Mach'", "mich", "dei\u00b7nes", "Geis\u00b7tes", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn der zieht, so folg' ich wol,", "tokens": ["Wenn", "der", "zieht", ",", "so", "fol\u00b7g'", "ich", "wol", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Wenn der hebt, kann ich mich heben,", "tokens": ["Wenn", "der", "hebt", ",", "kann", "ich", "mich", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "VVFIN", "$,", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn der mir sein Bild h\u00e4lt f\u00fcr,", "tokens": ["Wenn", "der", "mir", "sein", "Bild", "h\u00e4lt", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PPER", "PPOSAT", "NN", "VVFIN", "APPR", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Seh' ich dich schon kenntlich hier.", "tokens": ["Seh'", "ich", "dich", "schon", "kennt\u00b7lich", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "ADJD", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Oder kann ich so auch nicht", "tokens": ["O\u00b7der", "kann", "ich", "so", "auch", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ADV", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Recht erf\u00fcllen meine Pflicht,", "tokens": ["Recht", "er\u00b7f\u00fcl\u00b7len", "mei\u00b7ne", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Herr, so zieh' mir mein Gebeine,", "tokens": ["Herr", ",", "so", "zieh'", "mir", "mein", "Ge\u00b7bei\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dies mein schweres Reiskleid ab,", "tokens": ["Dies", "mein", "schwe\u00b7res", "Reis\u00b7kleid", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lass' das s\u00e4ubern durch das Grab,", "tokens": ["Lass'", "das", "s\u00e4u\u00b7bern", "durch", "das", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PDS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Bis es rein vor dir erscheine;", "tokens": ["Bis", "es", "rein", "vor", "dir", "er\u00b7schei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nur sein Odem, deine Zier,", "tokens": ["Nur", "sein", "O\u00b7dem", ",", "dei\u00b7ne", "Zier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Gehe fort, mein Gott, zu dir.", "tokens": ["Ge\u00b7he", "fort", ",", "mein", "Gott", ",", "zu", "dir", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Hier will ich in deinem Licht", "tokens": ["Hier", "will", "ich", "in", "dei\u00b7nem", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Beides, Mund und Angesicht,", "tokens": ["Bei\u00b7des", ",", "Mund", "und", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Herr, zu deinem Thron erheben", "tokens": ["Herr", ",", "zu", "dei\u00b7nem", "Thron", "er\u00b7he\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und f\u00fcr alle deine G\u00fct'", "tokens": ["Und", "f\u00fcr", "al\u00b7le", "dei\u00b7ne", "G\u00fct'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dir mit himmlischem Gem\u00fcth", "tokens": ["Dir", "mit", "himm\u00b7li\u00b7schem", "Ge\u00b7m\u00fcth"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mein erfreutes Danklied geben.", "tokens": ["Mein", "er\u00b7freu\u00b7tes", "Dank\u00b7lied", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Liebster Gott, f\u00e4llt dies mir ein,", "tokens": ["Liebs\u00b7ter", "Gott", ",", "f\u00e4llt", "dies", "mir", "ein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PDS", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Glaub' ich schon bei dir zu sein!", "tokens": ["Glaub'", "ich", "schon", "bei", "dir", "zu", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPER", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}