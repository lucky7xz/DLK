{"dta.poem.7270": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Die Linde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Ich breite \u00fcber ihn mein Bl\u00e4tterdach", "tokens": ["Ich", "brei\u00b7te", "\u00fc\u00b7ber", "ihn", "mein", "Bl\u00e4t\u00b7ter\u00b7dach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So weit ich es vom Ufer strecken mag.", "tokens": ["So", "weit", "ich", "es", "vom", "U\u00b7fer", "stre\u00b7cken", "mag."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "ADJD", "PPER", "PPER", "APPRART", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Schau her, wie langaus meine Arme reichen,", "tokens": ["Schau", "her", ",", "wie", "lan\u00b7gaus", "mei\u00b7ne", "Ar\u00b7me", "rei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PWAV", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihm mit den F\u00e4chern das Gew\u00fcrm zu scheuchen,", "tokens": ["Ihm", "mit", "den", "F\u00e4\u00b7chern", "das", "Ge\u00b7w\u00fcrm", "zu", "scheu\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Das hundertfarbig zittert in der Luft.", "tokens": ["Das", "hun\u00b7dert\u00b7far\u00b7big", "zit\u00b7tert", "in", "der", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ich hauch' ihm meines Odems besten Duft,", "tokens": ["Ich", "hauch'", "ihm", "mei\u00b7nes", "O\u00b7dems", "bes\u00b7ten", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und auf sein Lager lass' ich niederfallen", "tokens": ["Und", "auf", "sein", "La\u00b7ger", "lass'", "ich", "nie\u00b7der\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die Lieblichste von meinen Bl\u00fcten allen;", "tokens": ["Die", "Lieb\u00b7lichs\u00b7te", "von", "mei\u00b7nen", "Bl\u00fc\u00b7ten", "al\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "PIAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und eine Bank lehnt sich an meinen Stamm,", "tokens": ["Und", "ei\u00b7ne", "Bank", "lehnt", "sich", "an", "mei\u00b7nen", "Stamm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--++-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Da schaut ein Dichter von dem Uferdamm,", "tokens": ["Da", "schaut", "ein", "Dich\u00b7ter", "von", "dem", "U\u00b7fer\u00b7damm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Den h\u00f6r' ich fl\u00fcstern wunderliche Weise,", "tokens": ["Den", "h\u00f6r'", "ich", "fl\u00fcs\u00b7tern", "wun\u00b7der\u00b7li\u00b7che", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Von mir und dir und der Libell' so leise,", "tokens": ["Von", "mir", "und", "dir", "und", "der", "Li\u00b7bell'", "so", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "PPER", "KON", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Da\u00df er den frommen Schl\u00e4fer nicht geweckt;", "tokens": ["Da\u00df", "er", "den", "from\u00b7men", "Schl\u00e4\u00b7fer", "nicht", "ge\u00b7weckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Sonst wahrlich h\u00e4tt' die Raupe ihn erschreckt,", "tokens": ["Sonst", "wahr\u00b7lich", "h\u00e4tt'", "die", "Rau\u00b7pe", "ihn", "er\u00b7schreckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Die ich geschleudert aus dem Bl\u00e4tterhag.", "tokens": ["Die", "ich", "ge\u00b7schleu\u00b7dert", "aus", "dem", "Bl\u00e4t\u00b7ter\u00b7hag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Wie grell die Sonne blitzt; schw\u00fcl wird der Tag.", "tokens": ["Wie", "grell", "die", "Son\u00b7ne", "blitzt", ";", "schw\u00fcl", "wird", "der", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVFIN", "$.", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "O k\u00f6nnt' ich! k\u00f6nnt' ich meine Wurzeln strecken", "tokens": ["O", "k\u00f6nnt'", "ich", "!", "k\u00f6nnt'", "ich", "mei\u00b7ne", "Wur\u00b7zeln", "stre\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "PPER", "$.", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Recht mitten in das tief kristall'ne Becken,", "tokens": ["Recht", "mit\u00b7ten", "in", "das", "tief", "kris\u00b7tall'\u00b7ne", "Be\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Den F\u00e4den gleich, die, gr\u00fcnlicher Asbest,", "tokens": ["Den", "F\u00e4\u00b7den", "gleich", ",", "die", ",", "gr\u00fcn\u00b7li\u00b7cher", "As\u00b7best", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "Schaun so behaglich aus dem Wassernest,", "tokens": ["Schaun", "so", "be\u00b7hag\u00b7lich", "aus", "dem", "Was\u00b7ser\u00b7nest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.21": {"text": "Wie mir zum Hohne, der im Sonnenbrande", "tokens": ["Wie", "mir", "zum", "Hoh\u00b7ne", ",", "der", "im", "Son\u00b7nen\u00b7bran\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPRART", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Hier einsam niederlechzt vom Uferrande.", "tokens": ["Hier", "ein\u00b7sam", "nie\u00b7der\u00b7lechzt", "vom", "Uf\u00b7er\u00b7ran\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}