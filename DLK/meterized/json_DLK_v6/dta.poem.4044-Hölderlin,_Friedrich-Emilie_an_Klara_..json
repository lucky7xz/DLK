{"dta.poem.4044": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Emilie an Klara .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Da schrieb er mir. Ja theures Herz! er ists,", "tokens": ["Da", "schrieb", "er", "mir", ".", "Ja", "theu\u00b7res", "Herz", "!", "er", "ists", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$.", "PTKANT", "ADJA", "NN", "$.", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den ich gesucht. Wie dieser J\u00fcngling mich", "tokens": ["Den", "ich", "ge\u00b7sucht", ".", "Wie", "die\u00b7ser", "J\u00fcng\u00b7ling", "mich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PPER", "VVPP", "$.", "PWAV", "PDAT", "NN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dem\u00fcthiget und hebt! Nun! lies es nur!", "tokens": ["De\u00b7m\u00fct\u00b7hi\u00b7get", "und", "hebt", "!", "Nun", "!", "lies", "es", "nur", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "$.", "ADV", "$.", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "\u201eso bist du's wieder und ich habe dich", "tokens": ["\u201e", "so", "bist", "du's", "wie\u00b7der", "und", "ich", "ha\u00b7be", "dich"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VAFIN", "PIS", "ADV", "KON", "PPER", "VAFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "\u201egegr\u00fc\u00dft, gefunden, habe dich noch Einmal", "tokens": ["\u201e", "ge\u00b7gr\u00fc\u00dft", ",", "ge\u00b7fun\u00b7den", ",", "ha\u00b7be", "dich", "noch", "Ein\u00b7mal"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "VVPP", "$,", "VVPP", "$,", "VAFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "\u201ein deiner frommen Ruh' gest\u00f6rt, du Kind", "tokens": ["\u201e", "in", "dei\u00b7ner", "from\u00b7men", "Ruh'", "ge\u00b7st\u00f6rt", ",", "du", "Kind"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "APPR", "PPOSAT", "ADJA", "NN", "VVPP", "$,", "PPER", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "\u201edes Himmels! \u2014 Nein, Emilie! du kanntest", "tokens": ["\u201e", "des", "Him\u00b7mels", "!", "Nein", ",", "E\u00b7mi\u00b7lie", "!", "du", "kann\u00b7test"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["$(", "ART", "NN", "$.", "$(", "PTKANT", "$,", "NE", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "\u201emich ja. Ich kann nicht fragen. Wir sind's,", "tokens": ["\u201e", "mich", "ja", ".", "Ich", "kann", "nicht", "fra\u00b7gen", ".", "Wir", "sin\u00b7d's", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPER", "ADV", "$.", "PPER", "VMFIN", "PTKNEG", "VVINF", "$.", "PPER", "VAFIN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "\u201edie L\u00e4ngstverwandten, die der Gott getraut,", "tokens": ["\u201e", "die", "L\u00e4ngst\u00b7ver\u00b7wand\u00b7ten", ",", "die", "der", "Gott", "ge\u00b7traut", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "\u201eund bleiben wird es, wie die Sonne droben.", "tokens": ["\u201e", "und", "blei\u00b7ben", "wird", "es", ",", "wie", "die", "Son\u00b7ne", "dro\u00b7ben", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VVINF", "VAFIN", "PPER", "$,", "PWAV", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "\u201eich bin voll Freude, sch\u00f6ne Seele! bin", "tokens": ["\u201e", "ich", "bin", "voll", "Freu\u00b7de", ",", "sch\u00f6\u00b7ne", "See\u00b7le", "!", "bin"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["$(", "PPER", "VAFIN", "ADJD", "NN", "$,", "ADJA", "NN", "$.", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "\u201eder neuen Melodien ungewohnt.", "tokens": ["\u201e", "der", "neu\u00b7en", "Me\u00b7lo\u00b7dien", "un\u00b7ge\u00b7wohnt", "."], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "\u201ees ist ein anders Lied, als jenes, so", "tokens": ["\u201e", "es", "ist", "ein", "an\u00b7ders", "Lied", ",", "als", "je\u00b7nes", ",", "so"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["$(", "PPER", "VAFIN", "ART", "ADV", "NN", "$,", "KOUS", "PDS", "$,", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "\u201edem J\u00fcnglinge die Parze lehrend singt,", "tokens": ["\u201e", "dem", "J\u00fcng\u00b7lin\u00b7ge", "die", "Par\u00b7ze", "leh\u00b7rend", "singt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-++--+-+-+", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "\u201ebis ihm, wie Wohllaut, ihre Weise t\u00f6nt;", "tokens": ["\u201e", "bis", "ihm", ",", "wie", "Wohl\u00b7laut", ",", "ih\u00b7re", "Wei\u00b7se", "t\u00f6nt", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPER", "$,", "PWAV", "NN", "$,", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "\u201edann g\u00f6nnt sie ihm, du Friedliche! von dir", "tokens": ["\u201e", "dann", "g\u00f6nnt", "sie", "ihm", ",", "du", "Fried\u00b7li\u00b7che", "!", "von", "dir"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "ADV", "VVFIN", "PPER", "PPER", "$,", "PPER", "NN", "$.", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "\u201eden s\u00fc\u00dfern Ton, den liebsten, einzigen,", "tokens": ["\u201e", "den", "s\u00fc\u00b7\u00dfern", "Ton", ",", "den", "liebs\u00b7ten", ",", "ein\u00b7zi\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "$,", "ADJA", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "\u201ezu h\u00f6ren. Mein? o sieh! du wirst in Lust", "tokens": ["\u201e", "zu", "h\u00f6\u00b7ren", ".", "Mein", "?", "o", "sieh", "!", "du", "wirst", "in", "Lust"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PTKZU", "VVINF", "$.", "PPOSAT", "$.", "FM", "FM", "$.", "PPER", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "\u201edie M\u00fche mir, und, was mein Herz gebeut,", "tokens": ["\u201e", "die", "M\u00fc\u00b7he", "mir", ",", "und", ",", "was", "mein", "Herz", "ge\u00b7beut", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "PPER", "$,", "KON", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "\u201edu wirst es all in heilge Liebe wandeln.", "tokens": ["\u201e", "du", "wirst", "es", "all", "in", "heil\u00b7ge", "Lie\u00b7be", "wan\u00b7deln", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PPER", "PIAT", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "\u201eund hab' ich mit Unm\u00f6glichem gerungen,", "tokens": ["\u201e", "und", "hab'", "ich", "mit", "Un\u00b7m\u00f6g\u00b7li\u00b7chem", "ge\u00b7run\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VAFIN", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "\u201eund mir die Brust zu Treu und Ruh geh\u00e4rtet,", "tokens": ["\u201e", "und", "mir", "die", "Brust", "zu", "Treu", "und", "Ruh", "ge\u00b7h\u00e4r\u00b7tet", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PPER", "ART", "NN", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "\u201edu w\u00e4rmest sie mit frommer Hoffnung mir,", "tokens": ["\u201e", "du", "w\u00e4r\u00b7mest", "sie", "mit", "from\u00b7mer", "Hoff\u00b7nung", "mir", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PPER", "APPR", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "\u201eda\u00df sie vertrauter mit dem Siege schl\u00e4gt.", "tokens": ["\u201e", "da\u00df", "sie", "ver\u00b7trau\u00b7ter", "mit", "dem", "Sie\u00b7ge", "schl\u00e4gt", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "\u201eund wenn das Urbild, das, wie Morgenlicht,", "tokens": ["\u201e", "und", "wenn", "das", "Ur\u00b7bild", ",", "das", ",", "wie", "Mor\u00b7gen\u00b7licht", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "KON", "KOUS", "ART", "NN", "$,", "PDS", "$,", "PWAV", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "\u201emir aus des Lebens dunkler Wolke stieg,", "tokens": ["\u201e", "mir", "aus", "des", "Le\u00b7bens", "dunk\u00b7ler", "Wol\u00b7ke", "stieg", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "APPR", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "\u201edas Himmlische, mir schwindet, seh' ich dich,", "tokens": ["\u201e", "das", "Himm\u00b7li\u00b7sche", ",", "mir", "schwin\u00b7det", ",", "seh'", "ich", "dich", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "\u201eund, eine sch\u00f6ne G\u00f6tterbotin, mahnst", "tokens": ["\u201e", "und", ",", "ei\u00b7ne", "sch\u00f6\u00b7ne", "G\u00f6t\u00b7ter\u00b7bo\u00b7tin", ",", "mahnst"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["$(", "KON", "$,", "ART", "ADJA", "NN", "$,", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "\u201edu l\u00e4chelnd mich an meinen Ph\u00f6bus wieder;", "tokens": ["\u201e", "du", "l\u00e4\u00b7chelnd", "mich", "an", "mei\u00b7nen", "Ph\u00f6\u00b7bus", "wie\u00b7der", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADJD", "PPER", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "\u201eund wenn ich z\u00fcrne, s\u00e4nftigest du mich.", "tokens": ["\u201e", "und", "wenn", "ich", "z\u00fcr\u00b7ne", ",", "s\u00e4nf\u00b7ti\u00b7gest", "du", "mich", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "KON", "KOUS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "\u201edein Sch\u00fcler bin ich dann, und lausch' und lerne.", "tokens": ["\u201e", "dein", "Sch\u00fc\u00b7ler", "bin", "ich", "dann", ",", "und", "lausch'", "und", "ler\u00b7ne", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "PPER", "ADV", "$,", "KON", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "\u201evon deinem Munde nehm' ich, Zauberin", "tokens": ["\u201e", "von", "dei\u00b7nem", "Mun\u00b7de", "nehm'", "ich", ",", "Zau\u00b7be\u00b7rin"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["$(", "APPR", "PPOSAT", "NN", "VVFIN", "PPER", "$,", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "\u201edes Ueberredens s\u00fc\u00dfe Gabe mir,", "tokens": ["\u201e", "des", "Ue\u00b7ber\u00b7re\u00b7dens", "s\u00fc\u00b7\u00dfe", "Ga\u00b7be", "mir", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "\u201eda\u00df sie die Geister freundlich mir bezwingt;", "tokens": ["\u201e", "da\u00df", "sie", "die", "Geis\u00b7ter", "freund\u00b7lich", "mir", "be\u00b7zwingt", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ART", "NN", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "\u201eund wenn ich ferne war von dir, und wund", "tokens": ["\u201e", "und", "wenn", "ich", "fer\u00b7ne", "war", "von", "dir", ",", "und", "wund"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "KON", "KOUS", "PPER", "ADV", "VAFIN", "APPR", "PPER", "$,", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "\u201eund m\u00fcd dir wiederkehre, heilst du mich,", "tokens": ["\u201e", "und", "m\u00fcd", "dir", "wie\u00b7der\u00b7keh\u00b7re", ",", "heilst", "du", "mich", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VMFIN", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "\u201eund singst in Ruhe mich, du holde Muse!", "tokens": ["\u201e", "und", "singst", "in", "Ru\u00b7he", "mich", ",", "du", "hol\u00b7de", "Mu\u00b7se", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VVFIN", "APPR", "NN", "PPER", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}