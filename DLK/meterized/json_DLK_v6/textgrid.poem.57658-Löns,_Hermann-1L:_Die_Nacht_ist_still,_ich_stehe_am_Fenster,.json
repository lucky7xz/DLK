{"textgrid.poem.57658": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Nacht ist still, ich stehe am Fenster,", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Nacht ist still, ich stehe am Fenster,", "tokens": ["Die", "Nacht", "ist", "still", ",", "ich", "ste\u00b7he", "am", "Fens\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Am Monde vor\u00fcber die Wolken fliehn,", "tokens": ["Am", "Mon\u00b7de", "vor\u00b7\u00fc\u00b7ber", "die", "Wol\u00b7ken", "fliehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Mit leisem Singen oben, hoch oben,", "tokens": ["Mit", "lei\u00b7sem", "Sin\u00b7gen", "o\u00b7ben", ",", "hoch", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "$,", "ADJD", "ADV", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Den Augen nicht sichtbar, die Singschw\u00e4ne ziehn.", "tokens": ["Den", "Au\u00b7gen", "nicht", "sicht\u00b7bar", ",", "die", "Sing\u00b7schw\u00e4\u00b7ne", "ziehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ADJD", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+++-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Das klingt und singt durch die n\u00e4chtliche Stille", "tokens": ["Das", "klingt", "und", "singt", "durch", "die", "n\u00e4cht\u00b7li\u00b7che", "Stil\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das singt und klingt und klingt und singt", "tokens": ["Das", "singt", "und", "klingt", "und", "klingt", "und", "singt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So sehnsuchtsvoll nach jenem Lande,", "tokens": ["So", "sehn\u00b7suchts\u00b7voll", "nach", "je\u00b7nem", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Lande, das ihnen als Heimat winkt.", "tokens": ["Dem", "Lan\u00b7de", ",", "das", "ih\u00b7nen", "als", "Hei\u00b7mat", "winkt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "KOUS", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Meine Gedanken, die wandern wie Schw\u00e4ne", "tokens": ["Mei\u00b7ne", "Ge\u00b7dan\u00b7ken", ",", "die", "wan\u00b7dern", "wie", "Schw\u00e4\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "VVFIN", "KOKOM", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Hell und schimmernd dahin durch die Nacht", "tokens": ["Hell", "und", "schim\u00b7mernd", "da\u00b7hin", "durch", "die", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "ADJD", "PAV", "APPR", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und singen Lieder in deine Tr\u00e4ume,", "tokens": ["Und", "sin\u00b7gen", "Lie\u00b7der", "in", "dei\u00b7ne", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Du schl\u00e4fst, und dein roter Mund, der lacht.", "tokens": ["Du", "schl\u00e4fst", ",", "und", "dein", "ro\u00b7ter", "Mund", ",", "der", "lacht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "PPOSAT", "ADJA", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}