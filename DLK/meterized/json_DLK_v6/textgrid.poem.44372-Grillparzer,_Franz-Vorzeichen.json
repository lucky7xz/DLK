{"textgrid.poem.44372": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Vorzeichen", "genre": "verse", "period": "N.A.", "pub_year": 1820, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Augen! meiner Hoffnung Sterne,", "tokens": ["Au\u00b7gen", "!", "mei\u00b7ner", "Hoff\u00b7nung", "Ster\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dioskuren meiner Fahrt!", "tokens": ["Dios\u00b7ku\u00b7ren", "mei\u00b7ner", "Fahrt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schimmert nicht so hell und feurig!", "tokens": ["Schim\u00b7mert", "nicht", "so", "hell", "und", "feu\u00b7rig", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn, das k\u00fcndet, sagt man, Sturm \u2013", "tokens": ["Denn", ",", "das", "k\u00fcn\u00b7det", ",", "sagt", "man", ",", "Sturm", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "$,", "PDS", "VVFIN", "$,", "VVFIN", "PIS", "$,", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und so ist es auch: \u2013 Er naht schon! \u2013", "tokens": ["Und", "so", "ist", "es", "auch", ":", "\u2013", "Er", "naht", "schon", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "$.", "$(", "PPER", "VVFIN", "ADV", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Denn ich f\u00fchls an meinem Beben,", "tokens": ["Denn", "ich", "f\u00fchls", "an", "mei\u00b7nem", "Be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Meinem Schwindeln, meinem Wanken,", "tokens": ["Mei\u00b7nem", "Schwin\u00b7deln", ",", "mei\u00b7nem", "Wan\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df die Wellen schon emp\u00f6rt;", "tokens": ["Da\u00df", "die", "Wel\u00b7len", "schon", "em\u00b7p\u00f6rt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "\u00dcberzieht sich noch der Himmel,", "tokens": ["\u00dc\u00b7berz\u00b7ieht", "sich", "noch", "der", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Jener Himmel, wo ihr leuchtet,", "tokens": ["Je\u00b7ner", "Him\u00b7mel", ",", "wo", "ihr", "leuch\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "O, dann rettet mich kein Gott!", "tokens": ["O", ",", "dann", "ret\u00b7tet", "mich", "kein", "Gott", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}