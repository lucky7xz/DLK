{"textgrid.poem.26631": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Andere m\u00f6gen dich jetzt im steigenden Sommer verlassen,", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Andere m\u00f6gen dich jetzt im steigenden Sommer verlassen,", "tokens": ["An\u00b7de\u00b7re", "m\u00f6\u00b7gen", "dich", "jetzt", "im", "stei\u00b7gen\u00b7den", "Som\u00b7mer", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ich doch bleibe dir treu, strahlendurchfunkelte Stadt.", "tokens": ["Ich", "doch", "blei\u00b7be", "dir", "treu", ",", "strah\u00b7len\u00b7durch\u00b7fun\u00b7kel\u00b7te", "Stadt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "ADJD", "$,", "VVFIN", "NN", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Nicht verlangt es mich mehr nach himmelan ragenden Gletschern,", "tokens": ["Nicht", "ver\u00b7langt", "es", "mich", "mehr", "nach", "him\u00b7me\u00b7lan", "ra\u00b7gen\u00b7den", "Glet\u00b7schern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "PRF", "ADV", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Nicht nach des nordischen Meers wogenumbraustem Gestad.", "tokens": ["Nicht", "nach", "des", "nor\u00b7di\u00b7schen", "Meers", "wo\u00b7gen\u00b7um\u00b7braus\u00b7tem", "Ge\u00b7stad", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Gern vertr\u00e4um' ich die Tage im Dunstkreis der stilleren Stra\u00dfen,", "tokens": ["Gern", "ver\u00b7tr\u00e4um'", "ich", "die", "Ta\u00b7ge", "im", "Dunst\u00b7kreis", "der", "stil\u00b7le\u00b7ren", "Stra\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.6": {"text": "Qu\u00e4len auch Hitze und Staub, giebt's doch Oasen genug.", "tokens": ["Qu\u00e4\u00b7len", "auch", "Hit\u00b7ze", "und", "Staub", ",", "giebt's", "doch", "O\u00b7a\u00b7sen", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "NN", "KON", "NN", "$,", "VVFIN", "ADV", "NN", "ADV", "$."], "meter": "+--+--+--++--+", "measure": "dactylic.tri.plus"}, "line.7": {"text": "Wohlig schl\u00fcrft sich am Morgen der Kaffee im Runde des Stadtparks,", "tokens": ["Woh\u00b7lig", "schl\u00fcrft", "sich", "am", "Mor\u00b7gen", "der", "Kaf\u00b7fee", "im", "Run\u00b7de", "des", "Stadt\u00b7parks", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "APPRART", "NN", "ART", "NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.8": {"text": "Liebliches Blumenarom mengt der Cigarre sich bei.", "tokens": ["Lieb\u00b7li\u00b7ches", "Blu\u00b7men\u00b7a\u00b7rom", "mengt", "der", "Ci\u00b7gar\u00b7re", "sich", "bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "PRF", "PTKVZ", "$."], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.9": {"text": "Brennt die Sonne dann hei\u00dfer, so find' ich schattige G\u00e4rten,", "tokens": ["Brennt", "die", "Son\u00b7ne", "dann", "hei\u00b7\u00dfer", ",", "so", "find'", "ich", "schat\u00b7ti\u00b7ge", "G\u00e4r\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADJA", "$,", "ADV", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Wo ein erquickliches Buch still und gesammelt man liest.", "tokens": ["Wo", "ein", "er\u00b7quick\u00b7li\u00b7ches", "Buch", "still", "und", "ge\u00b7sam\u00b7melt", "man", "liest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "PTKVZ", "KON", "VVFIN", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.11": {"text": "Ja, dann nimmst du mich auf, Erschloss'ner vom \u00bbSch\u00e4tzer der Menschheit\u00ab,", "tokens": ["Ja", ",", "dann", "nimmst", "du", "mich", "auf", ",", "Er\u00b7schloss'\u00b7ner", "vom", "\u00bb", "Sch\u00e4t\u00b7zer", "der", "Menschheit", "\u00ab", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PTKANT", "$,", "ADV", "VVFIN", "PPER", "PRF", "PTKVZ", "$,", "NN", "APPRART", "$(", "NN", "ART", "NN", "$(", "$,"], "meter": "--+--+-+--+--+", "measure": "anapaest.di.plus"}, "line.12": {"text": "In deiner breiten Alleen wipfelumdunkelte Ruh;", "tokens": ["In", "dei\u00b7ner", "brei\u00b7ten", "Al\u00b7leen", "wip\u00b7fel\u00b7um\u00b7dun\u00b7kel\u00b7te", "Ruh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "NN", "$."], "meter": "-+-+---+--+--+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Oder auch du, Belvedere, mit zierlich gehegten Terrassen,", "tokens": ["O\u00b7der", "auch", "du", ",", "Bel\u00b7ve\u00b7de\u00b7re", ",", "mit", "zier\u00b7lich", "ge\u00b7heg\u00b7ten", "Ter\u00b7ras\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "$,", "NN", "$,", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "--+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Still ins Weite hinaus schweift dort der sinnende Blick.", "tokens": ["Still", "ins", "Wei\u00b7te", "hin\u00b7aus", "schweift", "dort", "der", "sin\u00b7nen\u00b7de", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "APZR", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.15": {"text": "Traulich empf\u00e4ngt mich Sch\u00f6nbrunn, es winkt mir der gastliche Prater,", "tokens": ["Trau\u00b7lich", "emp\u00b7f\u00e4ngt", "mich", "Sch\u00f6n\u00b7brunn", ",", "es", "winkt", "mir", "der", "gast\u00b7li\u00b7che", "Pra\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "NN", "$,", "PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.16": {"text": "Wo dem d\u00fcrstenden Mann froh sich der Abend beschlie\u00dft.", "tokens": ["Wo", "dem", "d\u00fcrs\u00b7ten\u00b7den", "Mann", "froh", "sich", "der", "A\u00b7bend", "be\u00b7schlie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ADJD", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.17": {"text": "Sehn' ich mich dennoch nach k\u00fchleren Schatten, nach frischeren L\u00fcften,", "tokens": ["Sehn'", "ich", "mich", "den\u00b7noch", "nach", "k\u00fch\u00b7le\u00b7ren", "Schat\u00b7ten", ",", "nach", "fri\u00b7sche\u00b7ren", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.18": {"text": "F\u00fchren auch Schienen und Dampf rasch mich in's Volle hinein;", "tokens": ["F\u00fch\u00b7ren", "auch", "Schie\u00b7nen", "und", "Dampf", "rasch", "mich", "in's", "Vol\u00b7le", "hin\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "NN", "KON", "NN", "ADJD", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.19": {"text": "Rasch in ein gr\u00fcnes Bereich der herrlichsten Eichen und Buchen \u2013", "tokens": ["Rasch", "in", "ein", "gr\u00fc\u00b7nes", "Be\u00b7reich", "der", "herr\u00b7lichs\u00b7ten", "Ei\u00b7chen", "und", "Bu\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "KON", "NN", "$("], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.20": {"text": "Tief in des Wienerwalds quellendurchrieselte Pracht.", "tokens": ["Tief", "in", "des", "Wie\u00b7ner\u00b7walds", "quel\u00b7len\u00b7durc\u00b7hrie\u00b7sel\u00b7te", "Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "M\u00f6gen doch Andere jetzt dich pilgernd verlassen \u2013 ich bleibe:", "tokens": ["M\u00f6\u00b7gen", "doch", "An\u00b7de\u00b7re", "jetzt", "dich", "pil\u00b7gernd", "ver\u00b7las\u00b7sen", "\u2013", "ich", "blei\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PIS", "ADV", "PPER", "ADJD", "VVINF", "$(", "PPER", "VVFIN", "$."], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.22": {"text": "Liegt das Gute so nah', w\u00fcnsch' ich mir Besseres nicht!", "tokens": ["Liegt", "das", "Gu\u00b7te", "so", "nah'", ",", "w\u00fcn\u00b7sch'", "ich", "mir", "Bes\u00b7se\u00b7res", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "PTKVZ", "$,", "VVFIN", "PPER", "PPER", "NN", "PTKNEG", "$."], "meter": "+-+--+-+--+--+", "measure": "trochaic.hexa.relaxed"}}}}}