{"textgrid.poem.56268": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingslied", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl war der Winter ein harter Gast,", "tokens": ["Wohl", "war", "der", "Win\u00b7ter", "ein", "har\u00b7ter", "Gast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Den armen, den trauernden V\u00f6geln verha\u00dft,", "tokens": ["Den", "ar\u00b7men", ",", "den", "trau\u00b7ern\u00b7den", "V\u00f6\u00b7geln", "ver\u00b7ha\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Die fr\u00f6hlich wieder nun singen;", "tokens": ["Die", "fr\u00f6h\u00b7lich", "wie\u00b7der", "nun", "sin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Aus blauer Luft, auf gr\u00fcner Flur,", "tokens": ["Aus", "blau\u00b7er", "Luft", ",", "auf", "gr\u00fc\u00b7ner", "Flur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie h\u00f6rt man's munter erklingen!", "tokens": ["Wie", "h\u00f6rt", "man's", "mun\u00b7ter", "er\u00b7klin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PIS", "ADJD", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Und als sich der Wald aufs neue belaubt,", "tokens": ["Und", "als", "sich", "der", "Wald", "aufs", "neu\u00b7e", "be\u00b7laubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PRF", "ART", "NN", "APPRART", "ADJA", "VVPP", "$,"], "meter": "----+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Da hat es mir nicht zu weilen erlaubt,", "tokens": ["Da", "hat", "es", "mir", "nicht", "zu", "wei\u00b7len", "er\u00b7laubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "PTKNEG", "PTKZU", "VVINF", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich mu\u00dfte hinaus und wandern;", "tokens": ["Ich", "mu\u00df\u00b7te", "hin\u00b7aus", "und", "wan\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APZR", "KON", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Es singen so lustig die V\u00f6gel umher,", "tokens": ["Es", "sin\u00b7gen", "so", "lus\u00b7tig", "die", "V\u00f6\u00b7gel", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Ich singe mein Lied, wie die andern.", "tokens": ["Ich", "sin\u00b7ge", "mein", "Lied", ",", "wie", "die", "an\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PWAV", "ART", "ADJA", "$."], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Und komm ich ans Wirtshaus, so kehr ich ein:", "tokens": ["Und", "komm", "ich", "ans", "Wirts\u00b7haus", ",", "so", "kehr", "ich", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Frau Wirtin, Frau Wirtin, ein gut Glas Wein,", "tokens": ["Frau", "Wir\u00b7tin", ",", "Frau", "Wir\u00b7tin", ",", "ein", "gut", "Glas", "Wein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "NN", "NN", "$,", "ART", "ADJD", "NN", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Ich habe mich durstig gesungen.", "tokens": ["Ich", "ha\u00b7be", "mich", "durs\u00b7tig", "ge\u00b7sun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Da kommt mit dem Weine die Tochter sogleich", "tokens": ["Da", "kommt", "mit", "dem", "Wei\u00b7ne", "die", "Toch\u00b7ter", "sog\u00b7leich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "ADV"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "So munter zu mir gesprungen.", "tokens": ["So", "mun\u00b7ter", "zu", "mir", "ge\u00b7sprun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Der Wein, den du schenkest, er ist f\u00fcrwahr", "tokens": ["Der", "Wein", ",", "den", "du", "schen\u00b7kest", ",", "er", "ist", "f\u00fcr\u00b7wahr"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So rot wie dein Mund, wie dein Auge so klar,", "tokens": ["So", "rot", "wie", "dein", "Mund", ",", "wie", "dein", "Au\u00b7ge", "so", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "PPOSAT", "NN", "$,", "PWAV", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Gar kr\u00e4ftig und lieblich zu schl\u00fcrfen;", "tokens": ["Gar", "kr\u00e4f\u00b7tig", "und", "lieb\u00b7lich", "zu", "schl\u00fcr\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und darf ich dich ansehn und trinken den Wein,", "tokens": ["Und", "darf", "ich", "dich", "an\u00b7sehn", "und", "trin\u00b7ken", "den", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PRF", "VVINF", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "So werd ich wohl singen auch d\u00fcrfen.", "tokens": ["So", "werd", "ich", "wohl", "sin\u00b7gen", "auch", "d\u00fcr\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVFIN", "ADV", "VMFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "Ich habe so eben ein Lied mir erdacht,", "tokens": ["Ich", "ha\u00b7be", "so", "e\u00b7ben", "ein", "Lied", "mir", "er\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und hab es f\u00fcr dich ganz eigens gemacht,", "tokens": ["Und", "hab", "es", "f\u00fcr", "dich", "ganz", "ei\u00b7gens", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "APPR", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Hab's nimmer zuvor noch gesungen;", "tokens": ["Hab's", "nim\u00b7mer", "zu\u00b7vor", "noch", "ge\u00b7sun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So h\u00f6re mir zu, du rosige Maid,", "tokens": ["So", "h\u00f6\u00b7re", "mir", "zu", ",", "du", "ro\u00b7si\u00b7ge", "Maid", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und sprich: ob's gut mir gelungen?", "tokens": ["Und", "sprich", ":", "ob's", "gut", "mir", "ge\u00b7lun\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$.", "KOUS", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Ich liebe den Fr\u00fchling, des Waldes Gr\u00fcn,", "tokens": ["Ich", "lie\u00b7be", "den", "Fr\u00fch\u00b7ling", ",", "des", "Wal\u00b7des", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Der V\u00f6gel Gesang, der Bienen Bem\u00fchn,", "tokens": ["Der", "V\u00f6\u00b7gel", "Ge\u00b7sang", ",", "der", "Bie\u00b7nen", "Be\u00b7m\u00fchn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NE", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der Blumen Farben und D\u00fcfte,", "tokens": ["Der", "Blu\u00b7men", "Far\u00b7ben", "und", "D\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Den Strahl der Sonne, des Himmels Blau,", "tokens": ["Den", "Strahl", "der", "Son\u00b7ne", ",", "des", "Him\u00b7mels", "Blau", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Den Hauch der w\u00e4rmeren L\u00fcfte.", "tokens": ["Den", "Hauch", "der", "w\u00e4r\u00b7me\u00b7ren", "L\u00fcf\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Sieh dort am Tor, was die Schwalben tun,", "tokens": ["Sieh", "dort", "am", "Tor", ",", "was", "die", "Schwal\u00b7ben", "tun", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPRART", "NN", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie emsig sie fliegen, sie werden nicht ruhn,", "tokens": ["Wie", "em\u00b7sig", "sie", "flie\u00b7gen", ",", "sie", "wer\u00b7den", "nicht", "ruhn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "VVINF", "$,", "PPER", "VAFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Bis fertig ihr Nestchen sie schauen;", "tokens": ["Bis", "fer\u00b7tig", "ihr", "Nest\u00b7chen", "sie", "schau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Ich sang, wie die V\u00f6gel, mein munteres Lied,", "tokens": ["Ich", "sang", ",", "wie", "die", "V\u00f6\u00b7gel", ",", "mein", "mun\u00b7te\u00b7res", "Lied", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ART", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Verga\u00df, ein Nest mir zu bauen.", "tokens": ["Ver\u00b7ga\u00df", ",", "ein", "Nest", "mir", "zu", "bau\u00b7en", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Ich liebe, die frischer als Waldes-Gr\u00fcn,", "tokens": ["Ich", "lie\u00b7be", ",", "die", "fri\u00b7scher", "als", "Wal\u00b7des\u00b7Gr\u00fcn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "ADJD", "KOKOM", "NE", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Noch emsiger schafft als sich Bienen bem\u00fchn,", "tokens": ["Noch", "em\u00b7si\u00b7ger", "schafft", "als", "sich", "Bie\u00b7nen", "be\u00b7m\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "KOUS", "PRF", "NN", "VVINF", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Vor der die Rosen sich neigen,", "tokens": ["Vor", "der", "die", "Ro\u00b7sen", "sich", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Deren Blick mich erw\u00e4rmt wie der Sonne Strahl,", "tokens": ["De\u00b7ren", "Blick", "mich", "er\u00b7w\u00e4rmt", "wie", "der", "Son\u00b7ne", "Strahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "PPER", "VVFIN", "KOKOM", "ART", "NN", "NN", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Da\u00df Lieder dem Busen entsteigen.", "tokens": ["Da\u00df", "Lie\u00b7der", "dem", "Bu\u00b7sen", "ent\u00b7stei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.9": {"line.1": {"text": "Ich habe gesungen, was sagest du nun?", "tokens": ["Ich", "ha\u00b7be", "ge\u00b7sun\u00b7gen", ",", "was", "sa\u00b7gest", "du", "nun", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "PWS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Sieh dort am Tor, was die Schwalben tun!", "tokens": ["Sieh", "dort", "am", "Tor", ",", "was", "die", "Schwal\u00b7ben", "tun", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPRART", "NN", "$,", "PRELS", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Was sollt es uns nicht gelingen?", "tokens": ["Was", "sollt", "es", "uns", "nicht", "ge\u00b7lin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Frau Wirtin, Frau Mutter, sie kommt eben recht,", "tokens": ["Frau", "Wir\u00b7tin", ",", "Frau", "Mut\u00b7ter", ",", "sie", "kommt", "e\u00b7ben", "recht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "NN", "NN", "$,", "PPER", "VVFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.5": {"text": "Sie soll noch ihr ", "tokens": ["Sie", "soll", "noch", "ihr"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "PPOSAT"], "meter": "-+-+", "measure": "iambic.di"}}}}}