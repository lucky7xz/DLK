{"dta.poem.23452": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von Gottes Gnaden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sie haben sich von Gottes Gnaden", "tokens": ["Sie", "ha\u00b7ben", "sich", "von", "Got\u00b7tes", "Gna\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PRF", "APPR", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu Herren dieser Welt gemacht \u2014", "tokens": ["Zu", "Her\u00b7ren", "die\u00b7ser", "Welt", "ge\u00b7macht"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PDAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das k\u00f6nnt' uns weiter gar nicht schaden,", "tokens": ["Das", "k\u00f6nnt'", "uns", "wei\u00b7ter", "gar", "nicht", "scha\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "W\u00e4r's wahr, was sie sich ausgedacht.", "tokens": ["W\u00e4r's", "wahr", ",", "was", "sie", "sich", "aus\u00b7ge\u00b7dacht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "PRELS", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Denn w\u00e4ren sie die Gottesholden,", "tokens": ["Denn", "w\u00e4\u00b7ren", "sie", "die", "Got\u00b7tes\u00b7hol\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So sorgte Gott f\u00fcr sie allein,", "tokens": ["So", "sorg\u00b7te", "Gott", "f\u00fcr", "sie", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "APPR", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gar herrlich w\u00fcrd' er sie besolden,", "tokens": ["Gar", "herr\u00b7lich", "w\u00fcrd'", "er", "sie", "be\u00b7sol\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Uns aber w\u00fcrde wohler sein.", "tokens": ["Uns", "a\u00b7ber", "w\u00fcr\u00b7de", "woh\u00b7ler", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wir w\u00fcrden dann die Erde haben,", "tokens": ["Wir", "w\u00fcr\u00b7den", "dann", "die", "Er\u00b7de", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Himmel aber h\u00e4tten sie;", "tokens": ["Den", "Him\u00b7mel", "a\u00b7ber", "h\u00e4t\u00b7ten", "sie", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir k\u00f6nnen uns hienieden laben", "tokens": ["Wir", "k\u00f6n\u00b7nen", "uns", "hien\u00b7ie\u00b7den", "la\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch an dem Himmelreiche nie.", "tokens": ["Doch", "an", "dem", "Him\u00b7mel\u00b7rei\u00b7che", "nie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}