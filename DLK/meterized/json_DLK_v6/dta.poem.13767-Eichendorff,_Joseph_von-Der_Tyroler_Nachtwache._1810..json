{"dta.poem.13767": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Tyroler Nachtwache.  \n 1810.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Kommt nur heran mit Eurer List,", "tokens": ["Kommt", "nur", "he\u00b7ran", "mit", "Eu\u00b7rer", "List", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PTKVZ", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Leitern, Strick und Banden,", "tokens": ["Mit", "Lei\u00b7tern", ",", "Strick", "und", "Ban\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Herr doch noch viel st\u00e4rker ist,", "tokens": ["Der", "Herr", "doch", "noch", "viel", "st\u00e4r\u00b7ker", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Macht Euern Witz zu Schanden.", "tokens": ["Macht", "Eu\u00b7ern", "Witz", "zu", "Schan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wie war't Ihr klug! \u2014", "tokens": ["Wie", "wa\u00b7r't", "Ihr", "klug", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD", "$.", "$("], "meter": "----+", "measure": "unknown.measure.single"}, "line.6": {"text": "Nun schwindelt Trug", "tokens": ["Nun", "schwin\u00b7delt", "Trug"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Hinab vom Felsenrande \u2014", "tokens": ["Hin\u00b7ab", "vom", "Fel\u00b7sen\u00b7ran\u00b7de"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wie seid Ihr dumm! o Schande!", "tokens": ["Wie", "seid", "Ihr", "dumm", "!", "o", "Schan\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "$.", "FM", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}