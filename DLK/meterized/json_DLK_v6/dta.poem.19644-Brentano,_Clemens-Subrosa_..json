{"dta.poem.19644": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Subrosa .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mitten im Garten ist               ", "tokens": ["Mit\u00b7ten", "im", "Gar\u00b7ten", "ist"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VAFIN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Ein sch\u00f6nes Paradies,", "tokens": ["Ein", "sch\u00f6\u00b7nes", "Pa\u00b7ra\u00b7dies", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ist so sch\u00f6n anzusehn,", "tokens": ["Ist", "so", "sch\u00f6n", "an\u00b7zu\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df ich m\u00f6cht' drinnen gehn.", "tokens": ["Da\u00df", "ich", "m\u00f6cht'", "drin\u00b7nen", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Als ich im G\u00e4rtlein war,", "tokens": ["Als", "ich", "im", "G\u00e4rt\u00b7lein", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Nahm ich der Bl\u00fcmlein wahr,", "tokens": ["Nahm", "ich", "der", "Bl\u00fcm\u00b7lein", "wahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Brach mir ein R\u00f6selein,", "tokens": ["Brach", "mir", "ein", "R\u00f6\u00b7se\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das sollt mein eigen sein.", "tokens": ["Das", "sollt", "mein", "ei\u00b7gen", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPOSAT", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Das R\u00f6slein gl\u00e4nzt so fein,", "tokens": ["Das", "R\u00f6s\u00b7lein", "gl\u00e4nzt", "so", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wie Gold und Edelstein", "tokens": ["Wie", "Gold", "und", "E\u00b7del\u00b7stein"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "War so fein \u00fcberg\u00fcldt,", "tokens": ["War", "so", "fein", "\u00fc\u00b7berg\u00b7\u00fcldt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-++-+", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Da\u00df es mein Herz erf\u00fcllt.", "tokens": ["Da\u00df", "es", "mein", "Herz", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich nahm das R\u00f6slein fein,", "tokens": ["Ich", "nahm", "das", "R\u00f6s\u00b7lein", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Schlo\u00df es ins K\u00e4mmerlein,", "tokens": ["Schlo\u00df", "es", "ins", "K\u00e4m\u00b7mer\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "$,"], "meter": "++-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Stellt es an einen Ort,", "tokens": ["Stellt", "es", "an", "ei\u00b7nen", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da es ja nicht verdorrt.", "tokens": ["Da", "es", "ja", "nicht", "ver\u00b7dorrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Komm ich ins K\u00e4mmerlein,", "tokens": ["Komm", "ich", "ins", "K\u00e4m\u00b7mer\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Find nicht mein R\u00f6selein,", "tokens": ["Find", "nicht", "mein", "R\u00f6\u00b7se\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als ich herummer sah,", "tokens": ["Als", "ich", "her\u00b7um\u00b7mer", "sah", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sitzt ein sch\u00f6n Jungfrau da.", "tokens": ["Sitzt", "ein", "sch\u00f6n", "Jung\u00b7frau", "da", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJD", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Sprach, ach erschrick nur nicht,", "tokens": ["Sprach", ",", "ach", "er\u00b7schrick", "nur", "nicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "XY", "XY", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Denn ich bin dir verpflicht,", "tokens": ["Denn", "ich", "bin", "dir", "ver\u00b7pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denn ich bin dir vertraut,", "tokens": ["Denn", "ich", "bin", "dir", "ver\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Denn ich bin deine Braut.", "tokens": ["Denn", "ich", "bin", "dei\u00b7ne", "Braut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}