"und", "Traum", "f\u00fchlst", "du", "die", "schwe\u00b7re", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf deiner Brust von deinen K\u00fcmmernissen,", "tokens": ["Auf", "dei\u00b7ner", "Brust", "von", "dei\u00b7nen", "K\u00fcm\u00b7mer\u00b7nis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und unter deinem Kopf den feuchten Brand", "tokens": ["Und", "un\u00b7ter", "dei\u00b7nem", "Kopf", "den", "feuch\u00b7ten", "Brand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Von deinen wilden Tr\u00e4nen auf dem Kissen.", "tokens": ["Von", "dei\u00b7nen", "wil\u00b7den", "Tr\u00e4\u00b7nen", "auf", "dem", "Kis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Durch Schlaf und Traum winkt dir ein stummes Bild,", "tokens": ["Durch", "Schlaf", "und", "Traum", "winkt", "dir", "ein", "stum\u00b7mes", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das du nicht kennst, mit herrischer Geb\u00e4rde.", "tokens": ["Das", "du", "nicht", "kennst", ",", "mit", "her\u00b7ri\u00b7scher", "Ge\u00b7b\u00e4r\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "VVFIN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dem folgst du \u00fcber n\u00e4chtliches Gefild", "tokens": ["Dem", "folgst", "du", "\u00fc\u00b7ber", "n\u00e4cht\u00b7li\u00b7ches", "Ge\u00b7fild"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Durch Schlaf und Traum bis an den Rand der Erde.", "tokens": ["Durch", "Schlaf", "und", "Traum", "bis", "an", "den", "Rand", "der", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}