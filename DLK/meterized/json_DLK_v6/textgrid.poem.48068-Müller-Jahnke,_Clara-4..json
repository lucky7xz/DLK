{"textgrid.poem.48068": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Blumen pfl\u00fcckt ich auf der Fr\u00fchlingswiese,", "tokens": ["Blu\u00b7men", "pfl\u00fcckt", "ich", "auf", "der", "Fr\u00fch\u00b7lings\u00b7wie\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "blaue Blumen aus dem Paradiese,", "tokens": ["blau\u00b7e", "Blu\u00b7men", "aus", "dem", "Pa\u00b7ra\u00b7die\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Blumen mit den tiefen quellenfeuchten", "tokens": ["Blu\u00b7men", "mit", "den", "tie\u00b7fen", "quel\u00b7len\u00b7feuch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Augen, die wie Menschenaugen leuchten.", "tokens": ["Au\u00b7gen", ",", "die", "wie", "Men\u00b7schen\u00b7au\u00b7gen", "leuch\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "KOKOM", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Ach, aus diesen Kelchen la\u00df mich nippen:", "tokens": ["Ach", ",", "aus", "die\u00b7sen", "Kel\u00b7chen", "la\u00df", "mich", "nip\u00b7pen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "APPR", "PDAT", "NN", "VVIMP", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "d\u00fcrstend heb ich sie an hei\u00dfe Lippen,", "tokens": ["d\u00fcrs\u00b7tend", "heb", "ich", "sie", "an", "hei\u00b7\u00dfe", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "und ich trinke \u2013 und ein Sommersehnen", "tokens": ["und", "ich", "trin\u00b7ke", "\u2013", "und", "ein", "Som\u00b7mer\u00b7seh\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$(", "KON", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "tr\u00e4umt in mir, ich trinke ", "tokens": ["tr\u00e4umt", "in", "mir", ",", "ich", "trin\u00b7ke"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "APPR", "PPER", "$,", "PPER", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}