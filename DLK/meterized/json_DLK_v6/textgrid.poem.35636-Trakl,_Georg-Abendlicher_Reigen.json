{"textgrid.poem.35636": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Abendlicher Reigen", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Asternfelder braun und blau,", "tokens": ["As\u00b7tern\u00b7fel\u00b7der", "braun", "und", "blau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kinder spielen dort an Gr\u00fcften,", "tokens": ["Kin\u00b7der", "spie\u00b7len", "dort", "an", "Gr\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In den abendlichen L\u00fcften,", "tokens": ["In", "den", "a\u00b7bend\u00b7li\u00b7chen", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Hingehaucht in klaren L\u00fcften", "tokens": ["Hin\u00b7ge\u00b7haucht", "in", "kla\u00b7ren", "L\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "H\u00e4ngen M\u00f6ven silbergrau.", "tokens": ["H\u00e4n\u00b7gen", "M\u00f6\u00b7ven", "sil\u00b7ber\u00b7grau", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "H\u00f6rnerschall hallt in der Au.", "tokens": ["H\u00f6r\u00b7ner\u00b7schall", "hallt", "in", "der", "Au", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "In der alten Schenke schrein", "tokens": ["In", "der", "al\u00b7ten", "Schen\u00b7ke", "schrein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Toller auf verstimmte Geigen,", "tokens": ["Tol\u00b7ler", "auf", "ver\u00b7stimm\u00b7te", "Gei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "An den Fenstern rauscht ein Reigen,", "tokens": ["An", "den", "Fens\u00b7tern", "rauscht", "ein", "Rei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rauscht ein bunter Ringelreigen,", "tokens": ["Rauscht", "ein", "bun\u00b7ter", "Rin\u00b7gel\u00b7rei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Rasend und berauscht von Wein.", "tokens": ["Ra\u00b7send", "und", "be\u00b7rauscht", "von", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Fr\u00f6stelnd kommt die Nacht herein.", "tokens": ["Fr\u00f6s\u00b7telnd", "kommt", "die", "Nacht", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Lachen flattert auf, verweht,", "tokens": ["La\u00b7chen", "flat\u00b7tert", "auf", ",", "ver\u00b7weht", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sp\u00f6ttisch klimpert eine Laute,", "tokens": ["Sp\u00f6t\u00b7tisch", "klim\u00b7pert", "ei\u00b7ne", "Lau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Leise eine stille Raute,", "tokens": ["Lei\u00b7se", "ei\u00b7ne", "stil\u00b7le", "Rau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eine schwermutvolle Raute", "tokens": ["Ei\u00b7ne", "schwer\u00b7mut\u00b7vol\u00b7le", "Rau\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "An der Schwelle niedergeht.", "tokens": ["An", "der", "Schwel\u00b7le", "nie\u00b7der\u00b7geht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Klingklang! Eine Sichel m\u00e4ht.", "tokens": ["Kling\u00b7klang", "!", "Ei\u00b7ne", "Si\u00b7chel", "m\u00e4ht", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Traumhaft webt der Kerzen Schein,", "tokens": ["Traum\u00b7haft", "webt", "der", "Ker\u00b7zen", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Malt dies junge Fleisch verfallen,", "tokens": ["Malt", "dies", "jun\u00b7ge", "Fleisch", "ver\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDS", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Klingklang! H\u00f6rs im Nebel hallen,", "tokens": ["Kling\u00b7klang", "!", "H\u00f6rs", "im", "Ne\u00b7bel", "hal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NE", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nach dem Takt der Geigen hallen,", "tokens": ["Nach", "dem", "Takt", "der", "Gei\u00b7gen", "hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und vorbei tanzt nackt Gebein.", "tokens": ["Und", "vor\u00b7bei", "tanzt", "nackt", "Ge\u00b7bein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADJD", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Lange schaut der Mond herein.", "tokens": ["Lan\u00b7ge", "schaut", "der", "Mond", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}