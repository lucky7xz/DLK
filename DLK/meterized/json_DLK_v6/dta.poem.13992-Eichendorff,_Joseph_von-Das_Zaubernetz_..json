{"dta.poem.13992": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Das Zaubernetz .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Liebst Du, treu dem alten Sange", "tokens": ["Liebst", "Du", ",", "treu", "dem", "al\u00b7ten", "San\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "ADJD", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie dem S\u00e4nger, mich wahrhaftig:", "tokens": ["Wie", "dem", "S\u00e4n\u00b7ger", ",", "mich", "wahr\u00b7haf\u00b7tig", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PPER", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df' Dein Schlo\u00df, den sch\u00f6nen Garten,", "tokens": ["La\u00df'", "Dein", "Schlo\u00df", ",", "den", "sch\u00f6\u00b7nen", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fchr' Dich heim in Waldesprachten!", "tokens": ["F\u00fchr'", "Dich", "heim", "in", "Wal\u00b7des\u00b7prach\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}