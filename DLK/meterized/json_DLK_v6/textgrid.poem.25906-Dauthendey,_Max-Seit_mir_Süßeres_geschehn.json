{"textgrid.poem.25906": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Seit mir S\u00fc\u00dferes geschehn", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Rosenhimbeer sitzt im Tann,", "tokens": ["Die", "Ro\u00b7sen\u00b7him\u00b7beer", "sitzt", "im", "Tann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sieht zuckrig alle Leute an,", "tokens": ["Sieht", "zuck\u00b7rig", "al\u00b7le", "Leu\u00b7te", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch seit mir S\u00fc\u00dferes geschehn,", "tokens": ["Doch", "seit", "mir", "S\u00fc\u00b7\u00dfe\u00b7res", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "La\u00df ich die Beer den andern stehn.", "tokens": ["La\u00df", "ich", "die", "Beer", "den", "an\u00b7dern", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie's z\u00e4he Harz aus Rinden tropft,", "tokens": ["Wie's", "z\u00e4\u00b7he", "Harz", "aus", "Rin\u00b7den", "tropft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NE", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Herz die Poren mir verstopft,", "tokens": ["Mein", "Herz", "die", "Po\u00b7ren", "mir", "ver\u00b7stopft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df jeder Blick voll Inbrunst bittet", "tokens": ["Da\u00df", "je\u00b7der", "Blick", "voll", "In\u00b7brunst", "bit\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "ADJD", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und mit der Liebsten sich verkittet.", "tokens": ["Und", "mit", "der", "Liebs\u00b7ten", "sich", "ver\u00b7kit\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Wald nicht gro\u00dfe Worte dreht,", "tokens": ["Der", "Wald", "nicht", "gro\u00b7\u00dfe", "Wor\u00b7te", "dreht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf alter Erd sich's lautlos geht; \u2013", "tokens": ["Auf", "al\u00b7ter", "Erd", "sich's", "laut\u00b7los", "geht", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "NE", "ADJD", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mal wie d\u00fcrr Laub, so d\u00fcrftig ganz,", "tokens": ["Mal", "wie", "d\u00fcrr", "Laub", ",", "so", "d\u00fcrf\u00b7tig", "ganz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ADJD", "NN", "$,", "ADV", "ADJD", "ADV", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "F\u00e4llt aus den H\u00e4nden jeder Kranz.", "tokens": ["F\u00e4llt", "aus", "den", "H\u00e4n\u00b7den", "je\u00b7der", "Kranz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}