{"textgrid.poem.41145": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ich heut klecksographieret,", "tokens": ["Als", "ich", "heut", "kleck\u00b7so\u00b7gra\u00b7phie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Statt mit Tinte mit Kaffee,", "tokens": ["Statt", "mit", "Tin\u00b7te", "mit", "Kaf\u00b7fee", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Da kam schnell heranspazieret", "tokens": ["Da", "kam", "schnell", "her\u00b7an\u00b7spa\u00b7zie\u00b7ret"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die Frau R\u00e4tin Salome.", "tokens": ["Die", "Frau", "R\u00e4\u00b7tin", "Sa\u00b7lo\u00b7me", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "T\u00e4glich ging die zur Visite", "tokens": ["T\u00e4g\u00b7lich", "ging", "die", "zur", "Vi\u00b7si\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "APPRART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Einmal, wenn nicht zweimal gar,", "tokens": ["Ein\u00b7mal", ",", "wenn", "nicht", "zwei\u00b7mal", "gar", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PTKNEG", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Setzte sich auf Sofas Mitte,", "tokens": ["Setz\u00b7te", "sich", "auf", "So\u00b7fas", "Mit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NE", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.8": {"text": "Weil sie die Gelehrtste war.", "tokens": ["Weil", "sie", "die", "Ge\u00b7lehrts\u00b7te", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Angestaunt von den Frau Basen,", "tokens": ["An\u00b7ge\u00b7staunt", "von", "den", "Frau", "Ba\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "War sie solchen allen gut,", "tokens": ["War", "sie", "sol\u00b7chen", "al\u00b7len", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "PIS", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Jene nur das Kochbuch lasen,", "tokens": ["Je\u00b7ne", "nur", "das", "Koch\u00b7buch", "la\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Sie doch die Frau Wildermuth.", "tokens": ["Sie", "doch", "die", "Frau", "Wil\u00b7der\u00b7muth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "NE", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.13": {"text": "Sterbend sprach sie: \u00bbZur Visite", "tokens": ["Ster\u00b7bend", "sprach", "sie", ":", "\u00bb", "Zur", "Vi\u00b7si\u00b7te"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "$.", "$(", "APPRART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.14": {"text": "Mu\u00df ich, hebet mich zur H\u00f6h'!\u00ab", "tokens": ["Mu\u00df", "ich", ",", "he\u00b7bet", "mich", "zur", "H\u00f6h'", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "$,", "VVFIN", "PPER", "APPRART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Doch der Tod kam, sprach: \u00bbIch bitte", "tokens": ["Doch", "der", "Tod", "kam", ",", "sprach", ":", "\u00bb", "Ich", "bit\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "$,", "VVFIN", "$.", "$(", "PPER", "VVFIN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.16": {"text": "Sie zu mir heut zum Kaffee!\u00ab", "tokens": ["Sie", "zu", "mir", "heut", "zum", "Kaf\u00b7fee", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "APPR", "PPER", "ADV", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Weh! nun sitzt schon viele Wochen", "tokens": ["Weh", "!", "nun", "sitzt", "schon", "vie\u00b7le", "Wo\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "ADV", "VVFIN", "ADV", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Sie in Hades' Einsamkeit,", "tokens": ["Sie", "in", "Ha\u00b7des'", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NE", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Doch als sie Kaffee gerochen,", "tokens": ["Doch", "als", "sie", "Kaf\u00b7fee", "ge\u00b7ro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.20": {"text": "Hat sie herzlich das erfreut;", "tokens": ["Hat", "sie", "herz\u00b7lich", "das", "er\u00b7freut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "PDS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Sie ist gut, will oft zitieren", "tokens": ["Sie", "ist", "gut", ",", "will", "oft", "zi\u00b7tie\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "VMFIN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Sie, weil es ihr Freude schafft,", "tokens": ["Sie", ",", "weil", "es", "ihr", "Freu\u00b7de", "schafft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.23": {"text": "Gerne sie klecksographieren", "tokens": ["Ger\u00b7ne", "sie", "kleck\u00b7so\u00b7gra\u00b7phie\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PPER", "VVINF"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.24": {"text": "Mit des Kaffees duft'gem Saft.", "tokens": ["Mit", "des", "Kaf\u00b7fees", "duft'\u00b7gem", "Saft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Aber als ich's wollt' probieren", "tokens": ["A\u00b7ber", "als", "ich's", "wollt'", "pro\u00b7bie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PIS", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Sogar mit Mokkakaffee,", "tokens": ["So\u00b7gar", "mit", "Mok\u00b7ka\u00b7kaf\u00b7fee", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "Lie\u00df sie nimmer sich verf\u00fchren;", "tokens": ["Lie\u00df", "sie", "nim\u00b7mer", "sich", "ver\u00b7f\u00fch\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Deutlich ich daraus erseh',", "tokens": ["Deut\u00b7lich", "ich", "da\u00b7raus", "er\u00b7seh'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PAV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Da\u00df sie von der Erde Tand", "tokens": ["Da\u00df", "sie", "von", "der", "Er\u00b7de", "Tand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.30": {"text": "Reuig sich zu Gott gewandt.", "tokens": ["Reu\u00b7ig", "sich", "zu", "Gott", "ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PRF", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}