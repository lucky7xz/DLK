{"dta.poem.4191": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Inbr\u00fcnstiger Wunsch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aller C\u00f6rper, aller Geister Urstand, Vater, Grund und", "tokens": ["Al\u00b7ler", "C\u00f6r\u00b7per", ",", "al\u00b7ler", "Geis\u00b7ter", "Ur\u00b7stand", ",", "Va\u00b7ter", ",", "Grund", "und"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "PIAT", "NN", "NN", "$,", "NN", "$,", "NN", "KON"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Geist!", "tokens": ["Geist", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Gott! aus Dessen ew\u2019gen Wesen aller Wesen Wesen fleu\u00dft,", "tokens": ["Gott", "!", "aus", "Des\u00b7sen", "ew'\u00b7gen", "We\u00b7sen", "al\u00b7ler", "We\u00b7sen", "We\u00b7sen", "fleu\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "APPR", "ART", "ADJA", "NN", "PIAT", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Dessen Tiefe den Begriff aller Engel \u00fcbersteiget,", "tokens": ["Des\u00b7sen", "Tie\u00b7fe", "den", "Be\u00b7griff", "al\u00b7ler", "En\u00b7gel", "\u00fc\u00b7bers\u00b7tei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ART", "NN", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Dessen Wirklichkeit in Weisheit, Lieb\u2019 und Allmacht, uns", "tokens": ["Des\u00b7sen", "Wirk\u00b7lich\u00b7keit", "in", "Weis\u00b7heit", ",", "Lieb'", "und", "All\u00b7macht", ",", "uns"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PDAT", "NN", "APPR", "NN", "$,", "NN", "KON", "NN", "$,", "PPER"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.6": {"text": "allein,", "tokens": ["al\u00b7lein", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Blo\u00df, durch den durchsichtgen Vorhang der Gesch\u00f6pfe, sich", "tokens": ["Blo\u00df", ",", "durch", "den", "durch\u00b7sicht\u00b7gen", "Vor\u00b7hang", "der", "Ge\u00b7sch\u00f6p\u00b7fe", ",", "sich"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "$,", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$,", "PRF"], "meter": "+---+-+-+-+-+", "measure": "dactylic.init"}, "line.8": {"text": "nur zeiget!", "tokens": ["nur", "zei\u00b7get", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "La\u00df uns, durch Dein Werk, in Dich einzusehn, besch\u00e4ftigt", "tokens": ["La\u00df", "uns", ",", "durch", "Dein", "Werk", ",", "in", "Dich", "ein\u00b7zu\u00b7sehn", ",", "be\u00b7sch\u00e4f\u00b7tigt"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VVIMP", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,", "APPR", "PPER", "VVINF", "$,", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "seyn!", "tokens": ["seyn", "!"], "token_info": ["word", "punct"], "pos": ["VAINF", "$."], "meter": "+", "measure": "single.up"}, "line.11": {"text": "La\u00df, durch ihre Pracht ger\u00fchrt, uns Dir, durch ein", "tokens": ["La\u00df", ",", "durch", "ih\u00b7re", "Pracht", "ge\u00b7r\u00fchrt", ",", "uns", "Dir", ",", "durch", "ein"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,", "PPER", "PPER", "$,", "APPR", "ART"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.12": {"text": "th\u00e4tigs Wollen,", "tokens": ["th\u00e4\u00b7tigs", "Wol\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.13": {"text": "Unsers Geistes beste Fr\u00fcchte, Demuht und Bewundrung,", "tokens": ["Un\u00b7sers", "Geis\u00b7tes", "bes\u00b7te", "Fr\u00fcch\u00b7te", ",", "De\u00b7muht", "und", "Be\u00b7wund\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.14": {"text": "zollen!", "tokens": ["zol\u00b7len", "!"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}