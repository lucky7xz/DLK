{"dta.poem.23643": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Mein Vaterland.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Treue Liebe bis zum Grabe", "tokens": ["Treu\u00b7e", "Lie\u00b7be", "bis", "zum", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schw\u00f6r' ich dir mit Herz und Hand:", "tokens": ["Schw\u00f6r'", "ich", "dir", "mit", "Herz", "und", "Hand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was ich bin und was ich habe,", "tokens": ["Was", "ich", "bin", "und", "was", "ich", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VAFIN", "KON", "PWS", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dank' ich dir, mein Vaterland.", "tokens": ["Dank'", "ich", "dir", ",", "mein", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nicht in Worten nur und Liedern", "tokens": ["Nicht", "in", "Wor\u00b7ten", "nur", "und", "Lie\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "NN", "ADV", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist mein Herz zum Dank bereit;", "tokens": ["Ist", "mein", "Herz", "zum", "Dank", "be\u00b7reit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPRART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit der That will ich's erwiedern.", "tokens": ["Mit", "der", "That", "will", "ich's", "er\u00b7wie\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dir in Noth, in Kampf und Streit.", "tokens": ["Dir", "in", "Noth", ",", "in", "Kampf", "und", "Streit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "In der Freude wie im Leide", "tokens": ["In", "der", "Freu\u00b7de", "wie", "im", "Lei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KOKOM", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ruf ich's Freund und Feinden zu:", "tokens": ["Ruf", "ich's", "Freund", "und", "Fein\u00b7den", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ewig sind vereint wir beide,", "tokens": ["E\u00b7wig", "sind", "ver\u00b7eint", "wir", "bei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "VVPP", "PPER", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und mein Trost, mein Gl\u00fcck bist du.", "tokens": ["Und", "mein", "Trost", ",", "mein", "Gl\u00fcck", "bist", "du", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", 