{"textgrid.poem.54438": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "1L: In dieser letzten Zeit/ da Trew vnd Lieb erkalten/", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In dieser letzten Zeit/ da Trew vnd Lieb erkalten/", "tokens": ["In", "die\u00b7ser", "letz\u00b7ten", "Zeit", "/", "da", "Trew", "vnd", "Lieb", "er\u00b7kal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$(", "KOUS", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da all Auffrichtigkeit schier g\u00e3tz vnd gar verschwind/", "tokens": ["Da", "all", "Auf\u00b7frich\u00b7tig\u00b7keit", "schier", "g\u00e3tz", "vnd", "gar", "ver\u00b7schwind", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ADJA", "NN", "KON", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da man vom Christenthumb nicht viel mehr vbrig find/", "tokens": ["Da", "man", "vom", "Chris\u00b7ten\u00b7thumb", "nicht", "viel", "mehr", "vb\u00b7rig", "find", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPRART", "NN", "PTKNEG", "ADV", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da Heiligkeit vnd Trew v\u00f1 Gottes Furcht vor alten;", "tokens": ["Da", "Hei\u00b7lig\u00b7keit", "vnd", "Trew", "v\u00f1", "Got\u00b7tes", "Furcht", "vor", "al\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "APPR", "NN", "NN", "APPR", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Seid Ihr noch drauff bedacht/ die Tugend zuerhalten", "tokens": ["Seid", "Ihr", "noch", "drauff", "be\u00b7dacht", "/", "die", "Tu\u00b7gend", "zu\u00b7er\u00b7hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAIMP", "PPER", "ADV", "PAV", "VVPP", "$(", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vnd lehret mit der That/ wie jtzt ein Trawtes Kind", "tokens": ["Vnd", "leh\u00b7ret", "mit", "der", "That", "/", "wie", "jtzt", "ein", "Traw\u00b7tes", "Kind"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$(", "KOKOM", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Des H\u00f6chst\u1ebd nicht allein erken'/ die Grause S\u00fcnd/", "tokens": ["Des", "H\u00f6chst\u1ebd", "nicht", "al\u00b7lein", "er\u00b7ken'", "/", "die", "Grau\u00b7se", "S\u00fcnd", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ADV", "VVFIN", "$(", "ART", "NN", "NN", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Vnd seines Glaubens Ampt mit wissen sol verwalten;", "tokens": ["Vnd", "sei\u00b7nes", "Glau\u00b7bens", "Ampt", "mit", "wis\u00b7sen", "sol", "ver\u00b7wal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "APPR", "VVINF", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ihr heisset noch darzu das jeder sich beflei\u00df", "tokens": ["Ihr", "heis\u00b7set", "noch", "dar\u00b7zu", "das", "je\u00b7der", "sich", "be\u00b7flei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PAV", "ART", "PIAT", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Zu w\u00fcrcken offentlich di\u00df was sein Hertze wei\u00df", "tokens": ["Zu", "w\u00fcr\u00b7cken", "of\u00b7fent\u00b7lich", "di\u00df", "was", "sein", "Hert\u00b7ze", "wei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "ADJD", "PDS", "PRELS", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Da sonst wol mancher denckt/ es sey genung am wissen;", "tokens": ["Da", "sonst", "wol", "man\u00b7cher", "denckt", "/", "es", "sey", "ge\u00b7nung", "am", "wis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PIS", "VVFIN", "$(", "PPER", "VAFIN", "ADV", "APPRART", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wie Seelig seid Ihr doch!weil Ihr der Ehrenkron", "tokens": ["Wie", "See\u00b7lig", "seid", "Ihr", "doch", "!", "weil", "Ihr", "der", "Eh\u00b7ren\u00b7kron"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "ADV", "$.", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die nicht mit wissen nur erl\u00e3gt wird vor dem Thron", "tokens": ["Die", "nicht", "mit", "wis\u00b7sen", "nur", "er\u00b7l\u00e3gt", "wird", "vor", "dem", "Thron"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PTKNEG", "APPR", "VVFIN", "ADV", "VVPP", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Des Richters aller Welt/ solt w\u00fcrcklich auch genissen.", "tokens": ["Des", "Rich\u00b7ters", "al\u00b7ler", "Welt", "/", "solt", "w\u00fcrck\u00b7lich", "auch", "ge\u00b7nis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$(", "VMFIN", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}