{"textgrid.poem.54439": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "In Reverendi Clariss. Doctissimiq; Domini M. Pauli Gryphii", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Eyvers voll von Gott stets Tag vnd Nacht gelehret;", "tokens": ["Der", "Ey\u00b7vers", "voll", "von", "Gott", "stets", "Tag", "vnd", "Nacht", "ge\u00b7leh\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "NN", "ADV", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dehn Christus selbst erleucht; den Gottes Geist regirt/", "tokens": ["Dehn", "Chris\u00b7tus", "selbst", "er\u00b7leucht", ";", "den", "Got\u00b7tes", "Geist", "re\u00b7girt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "VVPP", "$.", "ART", "NN", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Christi Sch\u00e4fflin hat auff gr\u00fcne Weid gef\u00fchrt/", "tokens": ["Der", "Chris\u00b7ti", "Sch\u00e4ff\u00b7lin", "hat", "auff", "gr\u00fc\u00b7ne", "Weid", "ge\u00b7f\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NE", "VAFIN", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "(dem man das Hertz mit Angst/ das Gutt mit Fewr versehret/", "tokens": ["(", "dem", "man", "das", "Hertz", "mit", "Angst", "/", "das", "Gutt", "mit", "Fewr", "ver\u00b7seh\u00b7ret", "/"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "PIS", "ART", "NN", "APPR", "NN", "$(", "ART", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "D\u1ebd keiner Feinde glimpff noch schnauber je verkehret/", "tokens": ["D\u1ebd", "kei\u00b7ner", "Fein\u00b7de", "glimpff", "noch", "schnau\u00b7ber", "je", "ver\u00b7keh\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "VVFIN", "ADV", "ADV", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Den wahre Tugend hat mit Trost im Creutz ger\u00fchrt/", "tokens": ["Den", "wah\u00b7re", "Tu\u00b7gend", "hat", "mit", "Trost", "im", "Creutz", "ge\u00b7r\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "NN", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der einig nur gelehrt/ als seiner Lehr geb\u00fchrt/", "tokens": ["Der", "ei\u00b7nig", "nur", "ge\u00b7lehrt", "/", "als", "sei\u00b7ner", "Lehr", "ge\u00b7b\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "VVPP", "$(", "KOUS", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Den Wei\u00dfheit Ihr erkiest/ den Svada hoch verehret/", "tokens": ["Den", "Wei\u00df\u00b7heit", "Ihr", "er\u00b7kiest", "/", "den", "Sva\u00b7da", "hoch", "ver\u00b7eh\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$(", "ART", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Den hat der Feinde Grim ins Elend hin verjagt!", "tokens": ["Den", "hat", "der", "Fein\u00b7de", "Grim", "ins", "E\u00b7lend", "hin", "ver\u00b7jagt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "NE", "APPRART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ins Elend? Ey nicht so! wer sich auff diesen wagt/", "tokens": ["Ins", "E\u00b7lend", "?", "Ey", "nicht", "so", "!", "wer", "sich", "auff", "die\u00b7sen", "wagt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "NN", "PTKNEG", "ADV", "$.", "PWS", "PRF", "APPR", "PDAT", "VVFIN", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Der di\u00df sehr grosse Rundt der Erden auffgewabet/", "tokens": ["Der", "di\u00df", "sehr", "gros\u00b7se", "Rundt", "der", "Er\u00b7den", "auff\u00b7ge\u00b7wa\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDS", "ADV", "ADJA", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Mu\u00df finden seine st\u00e4t/ vnnd wo di\u00df Hau\u00df zu klein/", "tokens": ["Mu\u00df", "fin\u00b7den", "sei\u00b7ne", "st\u00e4t", "/", "vnnd", "wo", "di\u00df", "Hau\u00df", "zu", "klein", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "PPOSAT", "NN", "$(", "KON", "PWAV", "PDS", "NN", "PTKA", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Das Vieh vnd Menschen tr\u00e4gt/ in dem wir alle sein/", "tokens": ["Das", "Vieh", "vnd", "Men\u00b7schen", "tr\u00e4gt", "/", "in", "dem", "wir", "al\u00b7le", "sein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$(", "APPR", "PRELS", "PPER", "PIS", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So ist des Himmels-Schlo\u00df/ da man sich sicher trawet.", "tokens": ["So", "ist", "des", "Him\u00b7mels\u00b7Schlo\u00df", "/", "da", "man", "sich", "si\u00b7cher", "tra\u00b7wet", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$(", "KOUS", "PIS", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}}}}