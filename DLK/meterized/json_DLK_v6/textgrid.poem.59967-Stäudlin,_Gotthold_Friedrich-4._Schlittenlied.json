{"textgrid.poem.59967": {"metadata": {"author": {"name": "St\u00e4udlin, Gotthold Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "4. Schlittenlied", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liebchen, h\u00fcll' dich in den Pelz!", "tokens": ["Lieb\u00b7chen", ",", "h\u00fcll'", "dich", "in", "den", "Pelz", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVIMP", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Eilig la\u00df uns gehen!", "tokens": ["Ei\u00b7lig", "la\u00df", "uns", "ge\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVIMP", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "H\u00f6rst du nicht den Silberton?", "tokens": ["H\u00f6rst", "du", "nicht", "den", "Sil\u00b7ber\u00b7ton", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Siehst du nicht die Fackel schon", "tokens": ["Siehst", "du", "nicht", "die", "Fa\u00b7ckel", "schon"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Meines Schlittens wehen!", "tokens": ["Mei\u00b7nes", "Schlit\u00b7tens", "we\u00b7hen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "In den blanken Schlitten hier,", "tokens": ["In", "den", "blan\u00b7ken", "Schlit\u00b7ten", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo der kleine Sch\u00fctze", "tokens": ["Wo", "der", "klei\u00b7ne", "Sch\u00fct\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Vorne mit ge\u00fcbter Hand", "tokens": ["Vor\u00b7ne", "mit", "ge\u00b7\u00fcb\u00b7ter", "Hand"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Seinen Silberbogen spannt,", "tokens": ["Sei\u00b7nen", "Sil\u00b7ber\u00b7bo\u00b7gen", "spannt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Trautes Liebchen, sitze!", "tokens": ["Trau\u00b7tes", "Lieb\u00b7chen", ",", "sit\u00b7ze", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "R\u00fcstig, Schimmel! tummle dich,", "tokens": ["R\u00fcs\u00b7tig", ",", "Schim\u00b7mel", "!", "tumm\u00b7le", "dich", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "NN", "$.", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn er ist bestiegen!", "tokens": ["Denn", "er", "ist", "be\u00b7stie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Sch\u00fcttle dein Geschirr, da\u00df hell", "tokens": ["Sch\u00fctt\u00b7le", "dein", "Ge\u00b7schirr", ",", "da\u00df", "hell"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "KOUS", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Es erklingt und la\u00df uns schnell", "tokens": ["Es", "er\u00b7klingt", "und", "la\u00df", "uns", "schnell"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVIMP", "PPER", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Durch die Stra\u00dfen fliegen!", "tokens": ["Durch", "die", "Stra\u00b7\u00dfen", "flie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Schnatterst, Liebchen? \u2013 Nun wohlan!", "tokens": ["Schnat\u00b7terst", ",", "Lieb\u00b7chen", "?", "\u2013", "Nun", "wo\u00b7hlan", "!"], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$.", "$(", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00e4rm' ich dich mit K\u00fcssen!", "tokens": ["W\u00e4rm'", "ich", "dich", "mit", "K\u00fcs\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Reich' die Honiglippe mir,", "tokens": ["Reich'", "die", "Ho\u00b7ni\u00b7glip\u00b7pe", "mir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df durchs Herz sich mir und dir", "tokens": ["Da\u00df", "durchs", "Herz", "sich", "mir", "und", "dir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "PRF", "PPER", "KON", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wonneschauer gie\u00dfen.", "tokens": ["Won\u00b7ne\u00b7schau\u00b7er", "gie\u00b7\u00dfen", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Willst dich str\u00e4uben? \u2013 H\u00fcte dich", "tokens": ["Willst", "dich", "str\u00e4u\u00b7ben", "?", "\u2013", "H\u00fc\u00b7te", "dich"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word"], "pos": ["VMFIN", "PPER", "VVFIN", "$.", "$(", "NN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dort vor Venus' Sohne!", "tokens": ["Dort", "vor", "Ve\u00b7nus'", "Soh\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Blutig r\u00e4cht der B\u00f6sewicht", "tokens": ["Blu\u00b7tig", "r\u00e4cht", "der", "B\u00f6\u00b7se\u00b7wicht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich an Spr\u00f6den! W\u00e4hne nicht,", "tokens": ["Sich", "an", "Spr\u00f6\u00b7den", "!", "W\u00e4h\u00b7ne", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "$.", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df er dein verschone!", "tokens": ["Da\u00df", "er", "dein", "ver\u00b7scho\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Bravo! Dieser Ku\u00df war hei\u00df,", "tokens": ["Bra\u00b7vo", "!", "Die\u00b7ser", "Ku\u00df", "war", "hei\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PDAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Trotz dem neid'schen Schleier!", "tokens": ["Trotz", "dem", "nei\u00b7d'\u00b7schen", "Schlei\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Dringt schon der ins Mark mir ein;", "tokens": ["Dringt", "schon", "der", "ins", "Mark", "mir", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "APPRART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "O wie wird einst jener sein", "tokens": ["O", "wie", "wird", "einst", "je\u00b7ner", "sein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "VAFIN", "ADV", "PDAT", "PPOSAT"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "In der Brautnachtfeier.", "tokens": ["In", "der", "Braut\u00b7nacht\u00b7fei\u00b7er", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}