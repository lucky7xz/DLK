{"textgrid.poem.35659": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "22. An Selma", "genre": "verse", "period": "N.A.", "pub_year": 1785, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du jungfr\u00e4ulicher Geist, gleich den Vollendeten", "tokens": ["Du", "jung\u00b7fr\u00e4u\u00b7li\u00b7cher", "Geist", ",", "gleich", "den", "Voll\u00b7en\u00b7de\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "ADV", "ART", "NN"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Schon im Staube verkl\u00e4rt: schmachtet umsonst mein Blick,", "tokens": ["Schon", "im", "Stau\u00b7be", "ver\u00b7kl\u00e4rt", ":", "schmach\u00b7tet", "um\u00b7sonst", "mein", "Blick", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVPP", "$.", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Deiner Herrlichkeit Abglanz,", "tokens": ["Dei\u00b7ner", "Herr\u00b7lich\u00b7keit", "Ab\u00b7glanz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Jene Bl\u00fctengestalt, zu schaun?", "tokens": ["Je\u00b7ne", "Bl\u00fc\u00b7ten\u00b7ge\u00b7stalt", ",", "zu", "schaun", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Ach! so ward mir zur Qual dieses phantastische", "tokens": ["Ach", "!", "so", "ward", "mir", "zur", "Qual", "die\u00b7ses", "phan\u00b7tas\u00b7ti\u00b7sche"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "ADV", "VAFIN", "PPER", "APPRART", "NN", "PDAT", "ADJA"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Herz, das geniusk\u00fchn Zaubergebilde schafft,", "tokens": ["Herz", ",", "das", "ge\u00b7nius\u00b7k\u00fchn", "Zau\u00b7ber\u00b7ge\u00b7bil\u00b7de", "schafft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Dann in nichtiger Sehnsucht", "tokens": ["Dann", "in", "nich\u00b7ti\u00b7ger", "Sehn\u00b7sucht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nach dem fliehenden Traume strebt!", "tokens": ["Nach", "dem", "flie\u00b7hen\u00b7den", "Trau\u00b7me", "strebt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Traum? Den g\u00f6ttlichen Traum bildet' ihr Seraph mir!", "tokens": ["Traum", "?", "Den", "g\u00f6tt\u00b7li\u00b7chen", "Traum", "bil\u00b7det'", "ihr", "Se\u00b7raph", "mir", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "PPER", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Ihren ahndenden Wunsch h\u00fcllt' er in Morgenglanz,", "tokens": ["Ih\u00b7ren", "ahn\u00b7den\u00b7den", "Wunsch", "h\u00fcllt'", "er", "in", "Mor\u00b7gen\u00b7glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "APPR", "NE", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Bracht' in D\u00fcften des Schlummers", "tokens": ["Bracht'", "in", "D\u00fcf\u00b7ten", "des", "Schlum\u00b7mers"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Dann die heilige Bildung mir!", "tokens": ["Dann", "die", "hei\u00b7li\u00b7ge", "Bil\u00b7dung", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "PPER", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Uns, zur Liebe geweiht, ach! zu der innigsten", "tokens": ["Uns", ",", "zur", "Lie\u00b7be", "ge\u00b7weiht", ",", "ach", "!", "zu", "der", "in\u00b7nigs\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "APPRART", "NN", "VVPP", "$,", "ITJ", "$.", "APPR", "ART", "ADJA"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Seelenliebe geweiht: warum bestrahlt der Mond,", "tokens": ["See\u00b7len\u00b7lie\u00b7be", "ge\u00b7weiht", ":", "wa\u00b7rum", "be\u00b7strahlt", "der", "Mond", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$.", "PWAV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Still die Wolken durchwandelnd,", "tokens": ["Still", "die", "Wol\u00b7ken", "durch\u00b7wan\u00b7delnd", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVPP", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Uns durch H\u00fcgel und Thal getrennt?", "tokens": ["Uns", "durch", "H\u00fc\u00b7gel", "und", "Thal", "ge\u00b7trennt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Oft beseeltest du uns, Liebe; doch unerkannt", "tokens": ["Oft", "be\u00b7seel\u00b7test", "du", "uns", ",", "Lie\u00b7be", ";", "doch", "un\u00b7er\u00b7kannt"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "NN", "$.", "ADV", "ADJD"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Schien dein Odem uns bald s\u00e4uselnder Fr\u00fchlingshauch,", "tokens": ["Schien", "dein", "O\u00b7dem", "uns", "bald", "s\u00e4u\u00b7seln\u00b7der", "Fr\u00fch\u00b7lings\u00b7hauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Bald ein Nachtigallseufzer,", "tokens": ["Bald", "ein", "Nach\u00b7ti\u00b7gall\u00b7seuf\u00b7zer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Bald Erfrischung der Sommernacht.", "tokens": ["Bald", "Er\u00b7fri\u00b7schung", "der", "Som\u00b7mer\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ART", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.6": {"line.1": {"text": "Liebend pfl\u00fcckten wir oft tauige Rosen uns,", "tokens": ["Lie\u00b7bend", "pfl\u00fcck\u00b7ten", "wir", "oft", "tau\u00b7i\u00b7ge", "Ro\u00b7sen", "uns", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "ADJA", "NN", "PPER", "$,"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Oft Violen zum Strau\u00df, schwebten in Bl\u00fctenduft", "tokens": ["Oft", "Vi\u00b7o\u00b7len", "zum", "Strau\u00df", ",", "schweb\u00b7ten", "in", "Bl\u00fc\u00b7ten\u00b7duft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "NN", "APPRART", "NE", "$,", "VVFIN", "APPR", "NN"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Mit Gesang, wie die V\u00f6gel", "tokens": ["Mit", "Ge\u00b7sang", ",", "wie", "die", "V\u00f6\u00b7gel"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PWAV", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Durch den schimmernden \u00c4ther, hin.", "tokens": ["Durch", "den", "schim\u00b7mern\u00b7den", "\u00c4\u00b7ther", ",", "hin", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.7": {"line.1": {"text": "Liebend h\u00f6rten wir oft murmeln den Erlenbach,", "tokens": ["Lie\u00b7bend", "h\u00f6r\u00b7ten", "wir", "oft", "mur\u00b7meln", "den", "Er\u00b7len\u00b7bach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Sahn aufsteigen den Mond, schwinden das Abendrot,", "tokens": ["Sahn", "auf\u00b7stei\u00b7gen", "den", "Mond", ",", "schwin\u00b7den", "das", "A\u00b7ben\u00b7drot", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "--+--+---+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Voll s\u00fc\u00dfschw\u00e4rmender Wehmut,", "tokens": ["Voll", "s\u00fc\u00df\u00b7schw\u00e4r\u00b7men\u00b7der", "Weh\u00b7mut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Dachten Tod und Unsterblichkeit.", "tokens": ["Dach\u00b7ten", "Tod", "und", "U\u00b7nsterb\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.8": {"line.1": {"text": "Schon im himmlischen Thal, wo wir, noch Seelen nur,", "tokens": ["Schon", "im", "himm\u00b7li\u00b7schen", "Thal", ",", "wo", "wir", ",", "noch", "See\u00b7len", "nur", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,", "PWAV", "PPER", "$,", "ADV", "NN", "ADV", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Tr\u00e4umten, spielten wir stets unter demselben Strauch,", "tokens": ["Tr\u00e4um\u00b7ten", ",", "spiel\u00b7ten", "wir", "stets", "un\u00b7ter", "dem\u00b7sel\u00b7ben", "Strauch", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Pfl\u00fcckten einerlei Blumen,", "tokens": ["Pfl\u00fcck\u00b7ten", "ei\u00b7ner\u00b7lei", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Horchten einerlei Harmonieen.", "tokens": ["Horch\u00b7ten", "ei\u00b7ner\u00b7lei", "Har\u00b7mo\u00b7ni\u00b7e\u00b7en", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Ach! wann d\u00e4mmerst du einst? Eile, gefl\u00fcgelter!", "tokens": ["Ach", "!", "wann", "d\u00e4m\u00b7merst", "du", "einst", "?", "Ei\u00b7le", ",", "ge\u00b7fl\u00fc\u00b7gel\u00b7ter", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "VVFIN", "PPER", "ADV", "$.", "NN", "$,", "ADJD", "$."], "meter": "+-+-+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Selma seufzet dir auch! Eile, du Wonnetag,", "tokens": ["Sel\u00b7ma", "seuf\u00b7zet", "dir", "auch", "!", "Ei\u00b7le", ",", "du", "Won\u00b7ne\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "$.", "NN", "$,", "PPER", "NN", "$,"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Der zu meiner Geliebten", "tokens": ["Der", "zu", "mei\u00b7ner", "Ge\u00b7lieb\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "PPOSAT", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "\u00dcber H\u00fcgel und Thal mich f\u00fchrt!", "tokens": ["\u00dc\u00b7ber", "H\u00fc\u00b7gel", "und", "Thal", "mich", "f\u00fchrt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PPER", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.10": {"line.1": {"text": "Selma, wenn dir alsdann schnelle Vergessenheit", "tokens": ["Sel\u00b7ma", ",", "wenn", "dir", "als\u00b7dann", "schnel\u00b7le", "Ver\u00b7ges\u00b7sen\u00b7heit"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "PPER", "ADV", "ADJA", "NN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Deiner leichteren Tracht, wenn dir der Wangen Glut,", "tokens": ["Dei\u00b7ner", "leich\u00b7te\u00b7ren", "Tracht", ",", "wenn", "dir", "der", "Wan\u00b7gen", "Glut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "KOUS", "PPER", "ART", "NN", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Und des klopfenden Herzens", "tokens": ["Und", "des", "klop\u00b7fen\u00b7den", "Her\u00b7zens"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Ahndung sagte, da\u00df ich es sei!", "tokens": ["Ahn\u00b7dung", "sag\u00b7te", ",", "da\u00df", "ich", "es", "sei", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "KOUS", "PPER", "PPER", "VAFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}