{"dta.poem.15975": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "29.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dein Amt, Gebildeter, und deine Aufgab' ist,", "tokens": ["Dein", "Amt", ",", "Ge\u00b7bil\u00b7de\u00b7ter", ",", "und", "dei\u00b7ne", "Auf\u00b7gab'", "ist", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "KON", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aussprechen was du f\u00fchlst, darstellen was du bist.", "tokens": ["Aus\u00b7spre\u00b7chen", "was", "du", "f\u00fchlst", ",", "dar\u00b7stel\u00b7len", "was", "du", "bist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "$,", "VVIZU", "PWS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Denn Alles in der Welt ringt sich zu stellen dar,", "tokens": ["Denn", "Al\u00b7les", "in", "der", "Welt", "ringt", "sich", "zu", "stel\u00b7len", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "ART", "NN", "VVFIN", "PRF", "PTKZU", "VVINF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und spricht sich unklar aus, du aber sollst es klar.", "tokens": ["Und", "spricht", "sich", "un\u00b7klar", "aus", ",", "du", "a\u00b7ber", "sollst", "es", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "PTKVZ", "$,", "PPER", "ADV", "VMFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Aufkl\u00e4ren sollst du uns dis Dunkel, und erkl\u00e4ren,", "tokens": ["Auf\u00b7kl\u00e4\u00b7ren", "sollst", "du", "uns", "dis", "Dun\u00b7kel", ",", "und", "er\u00b7kl\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PRF", "PDS", "NN", "$,", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie sch\u00f6n die Dinge, wenn wir klar sie s\u00e4hen, w\u00e4ren.", "tokens": ["Wie", "sch\u00f6n", "die", "Din\u00b7ge", ",", "wenn", "wir", "klar", "sie", "s\u00e4\u00b7hen", ",", "w\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "$,", "KOUS", "PPER", "ADJD", "PPER", "VVINF", "$,", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}