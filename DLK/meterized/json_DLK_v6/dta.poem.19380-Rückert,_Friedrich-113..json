{"dta.poem.19380": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "113.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das ist mein Wunsch, da\u00df gut und gl\u00fccklich m\u00f6gen werden,", "tokens": ["Das", "ist", "mein", "Wunsch", ",", "da\u00df", "gut", "und", "gl\u00fcck\u00b7lich", "m\u00f6\u00b7gen", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$,", "KOUS", "ADJD", "KON", "ADJD", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und all mit ihnen ich, die Menschen all auf Erden.", "tokens": ["Und", "all", "mit", "ih\u00b7nen", "ich", ",", "die", "Men\u00b7schen", "all", "auf", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "APPR", "PPER", "PPER", "$,", "ART", "NN", "PIAT", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und wenn ich selbst nicht viel zum allgemeinen Heil", "tokens": ["Und", "wenn", "ich", "selbst", "nicht", "viel", "zum", "all\u00b7ge\u00b7mei\u00b7nen", "Heil"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "PTKNEG", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Beitragen kann, so trag' ich bei mit Lust mein Theil.", "tokens": ["Bei\u00b7tra\u00b7gen", "kann", ",", "so", "trag'", "ich", "bei", "mit", "Lust", "mein", "Theil", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "$,", "ADV", "VVFIN", "PPER", "APPR", "APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die aber nichts dazu bei wollen, k\u00f6nnen tragen,", "tokens": ["Die", "a\u00b7ber", "nichts", "da\u00b7zu", "bei", "wol\u00b7len", ",", "k\u00f6n\u00b7nen", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "PIS", "PAV", "APPR", "VMFIN", "$,", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verklagen kann ich sie darum nicht, doch beklagen.", "tokens": ["Ver\u00b7kla\u00b7gen", "kann", "ich", "sie", "da\u00b7rum", "nicht", ",", "doch", "be\u00b7kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPER", "PAV", "PTKNEG", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wer sieht auf andrem Weg als ich das Heil gelegen,", "tokens": ["Wer", "sieht", "auf", "an\u00b7drem", "Weg", "als", "ich", "das", "Heil", "ge\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "ADJA", "NN", "KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der geh' ihn nur! es geht dahin auf vielen Wegen.", "tokens": ["Der", "geh'", "ihn", "nur", "!", "es", "geht", "da\u00b7hin", "auf", "vie\u00b7len", "We\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "$.", "PPER", "VVFIN", "PAV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Das Streben f\u00fcr die That nimmt Gott vom Menschen an;", "tokens": ["Das", "Stre\u00b7ben", "f\u00fcr", "die", "That", "nimmt", "Gott", "vom", "Men\u00b7schen", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wir haben viel gewollt, zuletzt hat er's gethan.", "tokens": ["Wir", "ha\u00b7ben", "viel", "ge\u00b7wollt", ",", "zu\u00b7letzt", "hat", "er's", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VMPP", "$,", "ADV", "VAFIN", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}