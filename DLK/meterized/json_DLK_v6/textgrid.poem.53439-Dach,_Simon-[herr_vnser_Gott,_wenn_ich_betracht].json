{"textgrid.poem.53439": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "[herr vnser Gott, wenn ich betracht]", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr vnser Gott, wenn ich betracht", "tokens": ["Herr", "vn\u00b7ser", "Gott", ",", "wenn", "ich", "be\u00b7tracht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "$,", "KOUS", "PPER", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein ewiges regieren,", "tokens": ["Dein", "e\u00b7wi\u00b7ges", "re\u00b7gie\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vnd wie durch deine wundermacht", "tokens": ["Vnd", "wie", "durch", "dei\u00b7ne", "wun\u00b7der\u00b7macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du mich pflegst offt zu f\u00fchren,", "tokens": ["Du", "mich", "pflegst", "offt", "zu", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "VVFIN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Verwundert sich mein Hertz' und spricht:", "tokens": ["Ver\u00b7wun\u00b7dert", "sich", "mein", "Hertz'", "und", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Herr, deiner Wei\u00dfheit recht Gericht", "tokens": ["Herr", ",", "dei\u00b7ner", "Wei\u00df\u00b7heit", "recht", "Ge\u00b7richt"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "NN", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ist Sonnenklar zu sp\u00fcren.", "tokens": ["Ist", "Son\u00b7nen\u00b7klar", "zu", "sp\u00fc\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du lessest mich zwar sehr viel Noth", "tokens": ["Du", "les\u00b7sest", "mich", "zwar", "sehr", "viel", "Noth"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit grosser Angst erfahren,", "tokens": ["Mit", "gros\u00b7ser", "Angst", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch giebstu mich nicht in den Todt,", "tokens": ["Doch", "giebs\u00b7tu", "mich", "nicht", "in", "den", "Todt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du kanst mich wol bewahren,", "tokens": ["Du", "kanst", "mich", "wol", "be\u00b7wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Vnd wiederumb zu rechter Zeit", "tokens": ["Vnd", "wie\u00b7de\u00b7rumb", "zu", "rech\u00b7ter", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des Lebens newe Frewdigkeit", "tokens": ["Des", "Le\u00b7bens", "ne\u00b7we", "Frew\u00b7dig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.7": {"text": "Mir gn\u00e4dig offenbahren.", "tokens": ["Mir", "gn\u00e4\u00b7dig", "of\u00b7fen\u00b7bah\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Verst\u00f6ssestu mich gleich von dir", "tokens": ["Ver\u00b7st\u00f6s\u00b7ses\u00b7tu", "mich", "gleich", "von", "dir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Offt in die tieffe Erde;", "tokens": ["Offt", "in", "die", "tief\u00b7fe", "Er\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So bistu wieder bald bey mir", "tokens": ["So", "bis\u00b7tu", "wie\u00b7der", "bald", "bey", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit freundlichem Geberde,", "tokens": ["Mit", "freund\u00b7li\u00b7chem", "Ge\u00b7ber\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Du tr\u00f6stest mich mit deinem Wort,", "tokens": ["Du", "tr\u00f6s\u00b7test", "mich", "mit", "dei\u00b7nem", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vnd holest mich vom finstern Ort,", "tokens": ["Vnd", "ho\u00b7lest", "mich", "vom", "fins\u00b7tern", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Damit ich sehr gro\u00df werde.", "tokens": ["Da\u00b7mit", "ich", "sehr", "gro\u00df", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wie solte dann mein Harffenklang", "tokens": ["Wie", "sol\u00b7te", "dann", "mein", "Harf\u00b7fen\u00b7klang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht Ruhm und Prei\u00df dir geben?", "tokens": ["Nicht", "Ruhm", "und", "Prei\u00df", "dir", "ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "KON", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mein Psalterspiel und Lobgesang", "tokens": ["Mein", "Psal\u00b7ter\u00b7spiel", "und", "Lob\u00b7ge\u00b7sang"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Soll dich mein Gott erheben.", "tokens": ["Soll", "dich", "mein", "Gott", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dich, Heiliger in Israel", "tokens": ["Dich", ",", "Hei\u00b7li\u00b7ger", "in", "Is\u00b7rael"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "NN", "APPR", "NE"], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Preist Mund vnd Seel, die von der Hell", "tokens": ["Preist", "Mund", "vnd", "Seel", ",", "die", "von", "der", "Hell"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NN", "KON", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du hast erl\u00f6st zum Leben.", "tokens": ["Du", "hast", "er\u00b7l\u00f6st", "zum", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Auch tichtet meine Zung' allzeit", "tokens": ["Auch", "tich\u00b7tet", "mei\u00b7ne", "Zung'", "all\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Allein zu deinen Ehren,", "tokens": ["Al\u00b7lein", "zu", "dei\u00b7nen", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df dein Lob der Gerechtigkeit", "tokens": ["Da\u00df", "dein", "Lob", "der", "Ge\u00b7rech\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Sich immer m\u00f6ge mehren.", "tokens": ["Sich", "im\u00b7mer", "m\u00f6\u00b7ge", "meh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die aber la\u00df sich sch\u00e4men sehr,", "tokens": ["Die", "a\u00b7ber", "la\u00df", "sich", "sch\u00e4\u00b7men", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "PRF", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zu schanden mach all jhre Ehr,", "tokens": ["Zu", "schan\u00b7den", "mach", "all", "jhre", "Ehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVFIN", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Die mein Gl\u00fcck wollen st\u00f6ren.", "tokens": ["Die", "mein", "Gl\u00fcck", "wol\u00b7len", "st\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}}}}}