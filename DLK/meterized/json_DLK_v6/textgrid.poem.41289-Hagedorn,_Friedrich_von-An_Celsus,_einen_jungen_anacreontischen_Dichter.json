{"textgrid.poem.41289": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "An Celsus, einen jungen anacreontischen Dichter", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erheb' und zeige dich dem deutschen Vaterlande!", "tokens": ["Er\u00b7heb'", "und", "zei\u00b7ge", "dich", "dem", "deut\u00b7schen", "Va\u00b7ter\u00b7lan\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch, sollen jetzt noch Ku\u00df und Wein", "tokens": ["Doch", ",", "sol\u00b7len", "jetzt", "noch", "Ku\u00df", "und", "Wein"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "VMFIN", "ADV", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Inhalt deiner T\u00f6ne sein,", "tokens": ["Der", "In\u00b7halt", "dei\u00b7ner", "T\u00f6\u00b7ne", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So singe beider Lob nicht zu der Sitten Schande!", "tokens": ["So", "sin\u00b7ge", "bei\u00b7der", "Lob", "nicht", "zu", "der", "Sit\u00b7ten", "Schan\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "PTKNEG", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie dir Anacreon gef\u00e4llt,", "tokens": ["Wie", "dir", "A\u00b7na\u00b7cre\u00b7on", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NE", "VVPP", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "So hei\u00dfe stets der klugen Welt", "tokens": ["So", "hei\u00b7\u00dfe", "stets", "der", "klu\u00b7gen", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein Weiser, wie er hie\u00df, in jeglichem Verstande!", "tokens": ["Ein", "Wei\u00b7ser", ",", "wie", "er", "hie\u00df", ",", "in", "jeg\u00b7li\u00b7chem", "Ver\u00b7stan\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Auch folg' einst einem Rath, der weder eilt noch irrt,", "tokens": ["Auch", "fol\u00b7g'", "einst", "ei\u00b7nem", "Rath", ",", "der", "we\u00b7der", "eilt", "noch", "irrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$,", "PRELS", "KON", "VVFIN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.9": {"text": "Sei nicht der Grille gleich, die bis zum Tode schwirrt!", "tokens": ["Sei", "nicht", "der", "Gril\u00b7le", "gleich", ",", "die", "bis", "zum", "To\u00b7de", "schwirrt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "NN", "ADV", "$,", "PRELS", "APPR", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}