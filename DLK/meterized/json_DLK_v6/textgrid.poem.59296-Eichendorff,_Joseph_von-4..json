{"textgrid.poem.59296": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6rst du die Gr\u00fcnde rufen", "tokens": ["H\u00f6rst", "du", "die", "Gr\u00fcn\u00b7de", "ru\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Tr\u00e4umen halb verwacht?", "tokens": ["In", "Tr\u00e4u\u00b7men", "halb", "ver\u00b7wacht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Oh, von des Schlosses Stufen", "tokens": ["Oh", ",", "von", "des", "Schlos\u00b7ses", "Stu\u00b7fen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Steig nieder in die Nacht! \u2013", "tokens": ["Steig", "nie\u00b7der", "in", "die", "Nacht", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PTKVZ", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Nachtigallen schlagen,", "tokens": ["Die", "Nach\u00b7ti\u00b7gal\u00b7len", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Garten rauschet sacht,", "tokens": ["Der", "Gar\u00b7ten", "rau\u00b7schet", "sacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es will dir Wunder sagen", "tokens": ["Es", "will", "dir", "Wun\u00b7der", "sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die wunderbare Nacht.", "tokens": ["Die", "wun\u00b7der\u00b7ba\u00b7re", "Nacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}