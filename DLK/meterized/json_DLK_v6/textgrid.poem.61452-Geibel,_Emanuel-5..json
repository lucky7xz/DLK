{"textgrid.poem.61452": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das ist das alte Giebelhaus,", "tokens": ["Das", "ist", "das", "al\u00b7te", "Gie\u00b7bel\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wohl kenn' ich Treppen, Flur und Saal!", "tokens": ["Wohl", "kenn'", "ich", "Trep\u00b7pen", ",", "Flur", "und", "Saal", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie stehn wie vormals, da ich hier", "tokens": ["Sie", "stehn", "wie", "vor\u00b7mals", ",", "da", "ich", "hier"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "ADV", "$,", "KOUS", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geliebt zum erstenmal.", "tokens": ["Ge\u00b7liebt", "zum", "ers\u00b7ten\u00b7mal", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dem Mond gleich wechseln Zeit und Herz,", "tokens": ["Dem", "Mond", "gleich", "wech\u00b7seln", "Zeit", "und", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nun wohnen andre Menschen dort,", "tokens": ["Nun", "woh\u00b7nen", "and\u00b7re", "Men\u00b7schen", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und andre Liebe tr\u00e4gt mein Sinn;", "tokens": ["Und", "and\u00b7re", "Lie\u00b7be", "tr\u00e4gt", "mein", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch blieb gefeit der Ort.", "tokens": ["Doch", "blieb", "ge\u00b7feit", "der", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Zum Fest heut ging ich hin im Schwarm,", "tokens": ["Zum", "Fest", "heut", "ging", "ich", "hin", "im", "Schwarm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da kam's auf mich, nicht wei\u00df ich, wie \u2013", "tokens": ["Da", "kam's", "auf", "mich", ",", "nicht", "wei\u00df", "ich", ",", "wie", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "$,", "PTKNEG", "VVFIN", "PPER", "$,", "PWAV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich h\u00f6rte nicht Gesang und Spiel", "tokens": ["Ich", "h\u00f6r\u00b7te", "nicht", "Ge\u00b7sang", "und", "Spiel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und dachte nur an sie;", "tokens": ["Und", "dach\u00b7te", "nur", "an", "sie", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und dacht' an meine junge Zeit,", "tokens": ["Und", "dacht'", "an", "mei\u00b7ne", "jun\u00b7ge", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wie wir's anders gar gemeint,", "tokens": ["Und", "wie", "wir's", "an\u00b7ders", "gar", "ge\u00b7meint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIS", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und an ihr Auge blau und lieb,", "tokens": ["Und", "an", "ihr", "Au\u00b7ge", "blau", "und", "lieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das, ach, um mich geweint.", "tokens": ["Das", ",", "ach", ",", "um", "mich", "ge\u00b7weint", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "ITJ", "$,", "KOUI", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und als ich auf vom Sinnen fuhr,", "tokens": ["Und", "als", "ich", "auf", "vom", "Sin\u00b7nen", "fuhr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Welt umher begriff ich kaum:", "tokens": ["Die", "Welt", "um\u00b7her", "be\u00b7griff", "ich", "kaum", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als sei der Traum mein Leben, war's,", "tokens": ["Als", "sei", "der", "Traum", "mein", "Le\u00b7ben", ",", "wa\u00b7r's", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$,", "VAFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und all mein Leben Traum.", "tokens": ["Und", "all", "mein", "Le\u00b7ben", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}