{"textgrid.poem.58594": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "26.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seel', die du mu\u00dft im dunkeln Glauben gehen,", "tokens": ["Seel'", ",", "die", "du", "mu\u00dft", "im", "dun\u00b7keln", "Glau\u00b7ben", "ge\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "VMFIN", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ei, f\u00fcrchte nicht, wenn Kraft und Trost gebricht,", "tokens": ["Ei", ",", "f\u00fcrch\u00b7te", "nicht", ",", "wenn", "Kraft", "und", "Trost", "ge\u00b7bricht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PTKNEG", "$,", "KOUS", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich bin mit dir, kannst du es gleich nicht sehen,", "tokens": ["Ich", "bin", "mit", "dir", ",", "kannst", "du", "es", "gleich", "nicht", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "$,", "VMFIN", "PPER", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich bin dein Gott, erschrick im Finstern nicht!", "tokens": ["Ich", "bin", "dein", "Gott", ",", "er\u00b7schrick", "im", "Fins\u00b7tern", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "ADJD", "APPRART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich st\u00e4rke dich, wo eigne Kr\u00e4fte fehlen,", "tokens": ["Ich", "st\u00e4r\u00b7ke", "dich", ",", "wo", "eig\u00b7ne", "Kr\u00e4f\u00b7te", "feh\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ich helfe dir im Leiden, Meiden, Ruhn,", "tokens": ["Ich", "hel\u00b7fe", "dir", "im", "Lei\u00b7den", ",", "Mei\u00b7den", ",", "Ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "$,", "NN", "$,", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich halte dich, es soll kein Fall dich qu\u00e4len;", "tokens": ["Ich", "hal\u00b7te", "dich", ",", "es", "soll", "kein", "Fall", "dich", "qu\u00e4\u00b7len", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "VMFIN", "PIAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die rechte Hand des Herren wird es tun.", "tokens": ["Die", "rech\u00b7te", "Hand", "des", "Her\u00b7ren", "wird", "es", "tun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}