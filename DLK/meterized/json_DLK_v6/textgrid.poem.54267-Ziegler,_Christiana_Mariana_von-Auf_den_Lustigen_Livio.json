{"textgrid.poem.54267": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "Auf den Lustigen Livio", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein aufger\u00e4umter Kopf braucht annoch keiner St\u00fctzen.", "tokens": ["Dein", "auf\u00b7ge\u00b7r\u00e4um\u00b7ter", "Kopf", "braucht", "an\u00b7noch", "kei\u00b7ner", "St\u00fct\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Man sagt, der Einfall ist als wie ein altes Hau\u00df;", "tokens": ["Man", "sagt", ",", "der", "Ein\u00b7fall", "ist", "als", "wie", "ein", "al\u00b7tes", "Hau\u00df", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "ART", "NN", "VAFIN", "KOUS", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Allein bey deinem Schertz siehts nicht gebrechlich aus,", "tokens": ["Al\u00b7lein", "bey", "dei\u00b7nem", "Schertz", "siehts", "nicht", "ge\u00b7brech\u00b7lich", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVFIN", "PTKNEG", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mir d\u00fcnckt, er wird gewi\u00df noch viele Jahre n\u00fctzen.", "tokens": ["Mir", "d\u00fcnckt", ",", "er", "wird", "ge\u00b7wi\u00df", "noch", "vie\u00b7le", "Jah\u00b7re", "n\u00fct\u00b7zen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "ADV", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}