{"dta.poem.16131": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "185.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df in der Mitte sei die Wahrheit, ist wol wahr,", "tokens": ["Da\u00df", "in", "der", "Mit\u00b7te", "sei", "die", "Wahr\u00b7heit", ",", "ist", "wol", "wahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "VAFIN", "ART", "NN", "$,", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und, da\u00df beim Aeu\u00dfersten zu irren sei Gefahr.", "tokens": ["Und", ",", "da\u00df", "beim", "A\u00b7e\u00b7u\u00b7\u00dfers\u00b7ten", "zu", "ir\u00b7ren", "sei", "Ge\u00b7fahr", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "APPRART", "NN", "PTKZU", "VVINF", "VAFIN", "NN", "$."], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Doch nicht wird Wahrheit durch zwei Aeu\u00dferste, verbunden,", "tokens": ["Doch", "nicht", "wird", "Wahr\u00b7heit", "durch", "zwei", "A\u00b7e\u00b7u\u00b7\u00dfers\u00b7te", ",", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PTKNEG", "VAFIN", "NN", "APPR", "CARD", "NN", "$,", "VVPP", "$,"], "meter": "-+-+--++--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Noch durch Vermeidung auch der Aeu\u00dfersten gefunden.", "tokens": ["Noch", "durch", "Ver\u00b7mei\u00b7dung", "auch", "der", "A\u00b7e\u00b7u\u00b7\u00dfers\u00b7ten", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Denn nichts ergeben sie, wenn man sie nur verneint,", "tokens": ["Denn", "nichts", "er\u00b7ge\u00b7ben", "sie", ",", "wenn", "man", "sie", "nur", "ver\u00b7neint", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "$,", "KOUS", "PIS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und selbst aufheben sie sich, \u00e4u\u00dferlich vereint.", "tokens": ["Und", "selbst", "auf\u00b7he\u00b7ben", "sie", "sich", ",", "\u00e4u\u00b7\u00dfer\u00b7lich", "ver\u00b7eint", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "$,", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nur wo lebendig zwei sich einen, um das dritte", "tokens": ["Nur", "wo", "le\u00b7ben\u00b7dig", "zwei", "sich", "ei\u00b7nen", ",", "um", "das", "drit\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PWAV", "ADJD", "CARD", "PRF", "ART", "$,", "KOUI", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu zeugen, findet sich die Wahrheit in der Mitte.", "tokens": ["Zu", "zeu\u00b7gen", ",", "fin\u00b7det", "sich", "die", "Wahr\u00b7heit", "in", "der", "Mit\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "VVFIN", "PRF", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}