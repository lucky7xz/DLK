{"textgrid.poem.37619": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Laune", "genre": "verse", "period": "N.A.", "pub_year": 1825, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein k\u00fchner Sinn kann Str\u00f6me hemmen", "tokens": ["Ein", "k\u00fch\u00b7ner", "Sinn", "kann", "Str\u00f6\u00b7me", "hem\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VMFIN", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und bricht durch Felsen seine Bahn,", "tokens": ["Und", "bricht", "durch", "Fel\u00b7sen", "sei\u00b7ne", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch wenn die Nebel ihn beklemmen,", "tokens": ["Doch", "wenn", "die", "Ne\u00b7bel", "ihn", "be\u00b7klem\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da f\u00fchlt er, seine Macht sei Wahn.", "tokens": ["Da", "f\u00fchlt", "er", ",", "sei\u00b7ne", "Macht", "sei", "Wahn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Verh\u00fcllt ist ihm die frohe Ferne,", "tokens": ["Ver\u00b7h\u00fcllt", "ist", "ihm", "die", "fro\u00b7he", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das N\u00e4chste scheint ihm unbekannt;", "tokens": ["Das", "N\u00e4chs\u00b7te", "scheint", "ihm", "un\u00b7be\u00b7kannt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Sonne gleicht dem schw\u00e4chsten Sterne,", "tokens": ["Die", "Son\u00b7ne", "gleicht", "dem", "schw\u00e4chs\u00b7ten", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Er irrt, wohin er sich gewandt! \u2013", "tokens": ["Er", "irrt", ",", "wo\u00b7hin", "er", "sich", "ge\u00b7wandt", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "PRF", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Bald wirken dann die Himmelszeichen,", "tokens": ["Bald", "wir\u00b7ken", "dann", "die", "Him\u00b7mels\u00b7zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die rings um unsre Erde ziehn;", "tokens": ["Die", "rings", "um", "uns\u00b7re", "Er\u00b7de", "ziehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Die heitre Thatkraft mu\u00df entweichen,", "tokens": ["Die", "heit\u00b7re", "That\u00b7kraft", "mu\u00df", "ent\u00b7wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Wenn Scorpionen droben gl\u00fchn. \u2013", "tokens": ["Wenn", "Scor\u00b7pi\u00b7o\u00b7nen", "dro\u00b7ben", "gl\u00fchn", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "NN", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Es reicht kein Arm zum Flammensterne,", "tokens": ["Es", "reicht", "kein", "Arm", "zum", "Flam\u00b7mens\u00b7ter\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Der unerwartet zu uns dringt,", "tokens": ["Der", "un\u00b7er\u00b7war\u00b7tet", "zu", "uns", "dringt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Es ringt kein Arm zum Erdenkerne,", "tokens": ["Es", "ringt", "kein", "Arm", "zum", "Er\u00b7den\u00b7ker\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Der uns der Krankheit Unheil bringt!", "tokens": ["Der", "uns", "der", "Krank\u00b7heit", "Un\u00b7heil", "bringt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Und geht die Welt noch einmal unter,", "tokens": ["Und", "geht", "die", "Welt", "noch", "ein\u00b7mal", "un\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "ADV", "APPR", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "So ist's in b\u00f6ser Laune Spiel;", "tokens": ["So", "ist's", "in", "b\u00f6\u00b7ser", "Lau\u00b7ne", "Spiel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Dem Herrn gefiel sie, als sie munter", "tokens": ["Dem", "Herrn", "ge\u00b7fiel", "sie", ",", "als", "sie", "mun\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Der Traurigen sind ihm zu viel.", "tokens": ["Der", "Trau\u00b7ri\u00b7gen", "sind", "ihm", "zu", "viel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PTKA", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}