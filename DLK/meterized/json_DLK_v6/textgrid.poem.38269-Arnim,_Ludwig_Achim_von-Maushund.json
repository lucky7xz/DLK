{"textgrid.poem.38269": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Maushund", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Mau\u00dfhund kam gegangen,", "tokens": ["Ein", "Mau\u00df\u00b7hund", "kam", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von einem hohen Dach;", "tokens": ["Von", "ei\u00b7nem", "ho\u00b7hen", "Dach", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der K\u00fcrschner wollt ihn fangen,", "tokens": ["Der", "K\u00fcrschner", "wollt", "ihn", "fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Zog ihn bald hinten nach.", "tokens": ["Zog", "ihn", "bald", "hin\u00b7ten", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "That ihn beim Schwanz ergreifen,", "tokens": ["That", "ihn", "beim", "Schwanz", "er\u00b7grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Die Katz fing an zu pfeifen,", "tokens": ["Die", "Katz", "fing", "an", "zu", "pfei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Pfuch, pfuch, pfuch, miau, mau mau.", "tokens": ["Pfuch", ",", "pfuch", ",", "pfuch", ",", "mi\u00b7au", ",", "mau", "mau", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ITJ", "$,", "ITJ", "$,", "ITJ", "$,", "NE", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Da sagt er zu der Katzen: Miau,", "tokens": ["Da", "sagt", "er", "zu", "der", "Kat\u00b7zen", ":", "Mi\u00b7au", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "NE", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Mach kein Geschreien,", "tokens": ["Mach", "kein", "Ge\u00b7schrei\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Magst mich erfreuen;", "tokens": ["Magst", "mich", "er\u00b7freu\u00b7en", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Allein dein Balg", "tokens": ["Al\u00b7lein", "dein", "Balg"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Mir wohl gefallt,", "tokens": ["Mir", "wohl", "ge\u00b7fallt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Den wird es dich jetzt kosten,", "tokens": ["Den", "wird", "es", "dich", "jetzt", "kos\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Denn er ist ziemlich alt.", "tokens": ["Denn", "er", "ist", "ziem\u00b7lich", "alt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "In ihren gro\u00dfen N\u00f6then", "tokens": ["In", "ih\u00b7ren", "gro\u00b7\u00dfen", "N\u00f6\u00b7then"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sprach die Katz: Mau,", "tokens": ["Sprach", "die", "Katz", ":", "Mau", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Der K\u00fcrschner will mich t\u00f6dten,", "tokens": ["Der", "K\u00fcrschner", "will", "mich", "t\u00f6d\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Mau mau, er nahm mir einmal ein Kind,", "tokens": ["Mau", "mau", ",", "er", "nahm", "mir", "ein\u00b7mal", "ein", "Kind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Darzu ein langes Messer, damit er schindt;", "tokens": ["Dar\u00b7zu", "ein", "lan\u00b7ges", "Mes\u00b7ser", ",", "da\u00b7mit", "er", "schindt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Und wenn der K\u00fcrschner will tanzen,", "tokens": ["Und", "wenn", "der", "K\u00fcrschner", "will", "tan\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "So nimmt er die Katz beim Schwanze.", "tokens": ["So", "nimmt", "er", "die", "Katz", "beim", "Schwan\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}