{"dta.poem.3289": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "An Venus.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Og\u00f6ttinn, die in Amathunt", "tokens": ["O\u00b7g\u00f6t\u00b7tinn", ",", "die", "in", "A\u00b7mat\u00b7hunt"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und \u00fcber Paphos herrscht, du Mutter s\u00fcsser Kla-", "tokens": ["Und", "\u00fc\u00b7ber", "Pa\u00b7phos", "herrscht", ",", "du", "Mut\u00b7ter", "s\u00fcs\u00b7ser", "Kla"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NE", "VVFIN", "$,", "PPER", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "gen!", "tokens": ["gen", "!"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Wie lang soll ieder rauher Mund", "tokens": ["Wie", "lang", "soll", "ie\u00b7der", "rau\u00b7her", "Mund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VMFIN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Jm Ton Anakreons dich zu besingen wagen?", "tokens": ["Jm", "Ton", "A\u00b7nak\u00b7re\u00b7ons", "dich", "zu", "be\u00b7sin\u00b7gen", "wa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "PPER", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wenn manche deutsche Muse nun", "tokens": ["Wenn", "man\u00b7che", "deut\u00b7sche", "Mu\u00b7se", "nun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Lieb und K\u00fcssen singt; wie eckelt mir vor K\u00fcssen!", "tokens": ["Von", "Lieb", "und", "K\u00fcs\u00b7sen", "singt", ";", "wie", "ec\u00b7kelt", "mir", "vor", "K\u00fcs\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$.", "PWAV", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gib acht, wie, wann sie artig thun", "tokens": ["Gib", "acht", ",", "wie", ",", "wann", "sie", "ar\u00b7tig", "thun"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "CARD", "$,", "PWAV", "$,", "PWAV", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und schalkhaft t\u00e4ndeln will, die M\u00e4dchen g\u00e4hnen m\u00fcssen!", "tokens": ["Und", "schalk\u00b7haft", "t\u00e4n\u00b7deln", "will", ",", "die", "M\u00e4d\u00b7chen", "g\u00e4h\u00b7nen", "m\u00fcs\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVINF", "VMFIN", "$,", "ART", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Jhr ist Ly\u00e4us unbekannt;", "tokens": ["Ihr", "ist", "Ly\u00b7\u00e4us", "un\u00b7be\u00b7kannt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie sieht so n\u00fcchtern aus, als Wasser, ihr Getr\u00e4nke.", "tokens": ["Sie", "sieht", "so", "n\u00fcch\u00b7tern", "aus", ",", "als", "Was\u00b7ser", ",", "ihr", "Ge\u00b7tr\u00e4n\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "PTKVZ", "$,", "KOUS", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch jauchzt sie, als vom Wein entbrannt,", "tokens": ["Doch", "jauchzt", "sie", ",", "als", "vom", "Wein", "ent\u00b7brannt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und jauchzt, wie ein Student in schwarzberauchter Schen-", "tokens": ["Und", "jauchzt", ",", "wie", "ein", "Stu\u00b7dent", "in", "schwarz\u00b7be\u00b7rauch\u00b7ter", "Schen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PWAV", "ART", "NN", "APPR", "ADJA", "TRUNC"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "ke.", "tokens": ["ke", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}}, "stanza.4": {"line.1": {"text": "Unleidlich str\u00e4ubt sich ieder Ton:", "tokens": ["Un\u00b7leid\u00b7lich", "str\u00e4ubt", "sich", "ie\u00b7der", "Ton", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jhr tr\u00e4ger Witz gebiert nur w\u00f6rterreiche S\u00e4tze.", "tokens": ["Ihr", "tr\u00e4\u00b7ger", "Witz", "ge\u00b7biert", "nur", "w\u00f6r\u00b7ter\u00b7rei\u00b7che", "S\u00e4t\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nie war dein Freund Anakreon", "tokens": ["Nie", "war", "dein", "Freund", "A\u00b7nak\u00b7re\u00b7on"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So schwatzhaft, obgleich \u00e4lt; und Amor hasst Geschw\u00e4tze.", "tokens": ["So", "schwatz\u00b7haft", ",", "ob\u00b7gleich", "\u00e4lt", ";", "und", "A\u00b7mor", "hasst", "Ge\u00b7schw\u00e4t\u00b7ze", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "ADJD", "$.", "KON", "NE", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Die V\u00e4tter dieser Lieder-Brut,", "tokens": ["Die", "V\u00e4t\u00b7ter", "die\u00b7ser", "Lie\u00b7der\u00b7Brut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Affen deines Gleims, o sch\u00f6ne G\u00f6ttinn! strafe.", "tokens": ["Die", "Af\u00b7fen", "dei\u00b7nes", "Gleims", ",", "o", "sch\u00f6\u00b7ne", "G\u00f6t\u00b7tinn", "!", "stra\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "FM", "ADJA", "NN", "$.", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Von Lieb entbrenn\u2019 ihr kaltes Blut!", "tokens": ["Von", "Lieb", "ent\u00b7brenn'", "ihr", "kal\u00b7tes", "Blut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Jhr M\u00e4dchen les\u2019 ihr Lob, ihr frostig Lob und schlafe!", "tokens": ["Ihr", "M\u00e4d\u00b7chen", "les'", "ihr", "Lob", ",", "ihr", "fros\u00b7tig", "Lob", "und", "schla\u00b7fe", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "ADJD", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Nie schall' ihr ungerathnes Lied,", "tokens": ["Nie", "schall'", "ihr", "un\u00b7ge\u00b7rath\u00b7nes", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bey sanftem Saitenspiel, von Lippen kluger Sch\u00f6nen,", "tokens": ["Bey", "sanf\u00b7tem", "Sai\u00b7ten\u00b7spiel", ",", "von", "Lip\u00b7pen", "klu\u00b7ger", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Noch wo der junge Bacchus gl\u00fcht,", "tokens": ["Noch", "wo", "der", "jun\u00b7ge", "Bac\u00b7chus", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "ART", "ADJA", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn ihn die Grazien mit ihren Rosen kr\u00f6nen!", "tokens": ["Wenn", "ihn", "die", "Gra\u00b7zi\u00b7en", "mit", "ih\u00b7ren", "Ro\u00b7sen", "kr\u00f6\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}