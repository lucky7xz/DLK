{"dta.poem.2786": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Liebe zu GOTT.  \n Sechstinne.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was lieb' ich? lieb' ich denn das Hau\u00df der grossen Welt?", "tokens": ["Was", "lieb'", "ich", "?", "lieb'", "ich", "denn", "das", "Hau\u00df", "der", "gros\u00b7sen", "Welt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "VVFIN", "PPER", "ADV", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ergetze mich an Ehr/ und hoher W\u00fcrden-Macht?", "tokens": ["Er\u00b7get\u00b7ze", "mich", "an", "Ehr", "/", "und", "ho\u00b7her", "W\u00fcr\u00b7den\u00b7Macht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "$(", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Verschliesse meine Zeit in Lieblichkeit und Lust?", "tokens": ["Ver\u00b7schlies\u00b7se", "mei\u00b7ne", "Zeit", "in", "Lieb\u00b7lich\u00b7keit", "und", "Lust", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ers\u00e4ttige mein Aug\u2019 an Sch\u00f6nheit der Gestalt?", "tokens": ["Er\u00b7s\u00e4t\u00b7ti\u00b7ge", "mein", "Aug'", "an", "Sch\u00f6n\u00b7heit", "der", "Ge\u00b7stalt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Thu meinem Leibe wohl bey guter Spei\u00df und Tranck?", "tokens": ["Thu", "mei\u00b7nem", "Lei\u00b7be", "wohl", "bey", "gu\u00b7ter", "Spei\u00df", "und", "Tranck", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Kauff Aecker/ Wiesen/ Feld/ und bau auf meinen Schatz?", "tokens": ["Kauff", "A\u00b7e\u00b7cker", "/", "Wie\u00b7sen", "/", "Feld", "/", "und", "bau", "auf", "mei\u00b7nen", "Schatz", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$(", "NN", "$(", "NN", "$(", "KON", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.7": {"text": "Nein; meine Seele scheut die wandelbare Macht/", "tokens": ["Nein", ";", "mei\u00b7ne", "See\u00b7le", "scheut", "die", "wan\u00b7del\u00b7ba\u00b7re", "Macht", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und ha\u00dft was sie befleckt/ die aufgeputzte Lust.", "tokens": ["Und", "ha\u00dft", "was", "sie", "be\u00b7fleckt", "/", "die", "auf\u00b7ge\u00b7putz\u00b7te", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PWS", "PPER", "VVPP", "$(", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sie kennt den Fallstrick wohl der reitzrischen Gestalt/", "tokens": ["Sie", "kennt", "den", "Fall\u00b7strick", "wohl", "der", "reitz\u00b7ri\u00b7schen", "Ge\u00b7stalt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und wei\u00df/ da\u00df sie erstickt in Schwelgerey und Tranck/", "tokens": ["Und", "wei\u00df", "/", "da\u00df", "sie", "er\u00b7stickt", "in", "Schwel\u00b7ge\u00b7rey", "und", "Tranck", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "KOUS", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wie sehr ihr hinderlich ein Kummer-reicher Schatz/", "tokens": ["Wie", "sehr", "ihr", "hin\u00b7der\u00b7lich", "ein", "Kum\u00b7mer\u00b7rei\u00b7cher", "Schatz", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und da\u00df nur Eitelkeit wohnt in dem Hau\u00df der Welt.", "tokens": ["Und", "da\u00df", "nur", "Ei\u00b7tel\u00b7keit", "wohnt", "in", "dem", "Hau\u00df", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Ich liebe meinen GOtt/ den Brunnquell aller Lust/", "tokens": ["Ich", "lie\u00b7be", "mei\u00b7nen", "Gott", "/", "den", "Brunn\u00b7quell", "al\u00b7ler", "Lust", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$(", "ART", "NN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und sehe schon im Geist die himmlische Gestalt.", "tokens": ["Und", "se\u00b7he", "schon", "im", "Geist", "die", "himm\u00b7li\u00b7sche", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Mein Sch\u00f6pffer reichet mir den wahren Himmels-Tranck/", "tokens": ["Mein", "Sch\u00f6pf\u00b7fer", "rei\u00b7chet", "mir", "den", "wah\u00b7ren", "Him\u00b7mels\u00b7Tranck", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Schenckt mir die Seeligkeit/ ach unvergleichlich Schatz.", "tokens": ["Schenckt", "mir", "die", "See\u00b7lig\u00b7keit", "/", "ach", "un\u00b7ver\u00b7gleich\u00b7lich", "Schatz", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$(", "ADV", "ADJD", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.17": {"text": "Die Erde stinckt mich an/ ich hasse Geld und Welt/", "tokens": ["Die", "Er\u00b7de", "stinckt", "mich", "an", "/", "ich", "has\u00b7se", "Geld", "und", "Welt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$(", "PPER", "VVFIN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Weil \u00fcber alles di\u00df noch eine h\u00f6h\u2019re Macht.", "tokens": ["Weil", "\u00fc\u00b7ber", "al\u00b7les", "di\u00df", "noch", "ei\u00b7ne", "h\u00f6h'\u00b7re", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PIS", "PDS", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "O Glantz der Herrlichkeit! O lieblichste Gestalt!", "tokens": ["O", "Glantz", "der", "Herr\u00b7lich\u00b7keit", "!", "O", "lieb\u00b7lichs\u00b7te", "Ge\u00b7stalt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$.", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wie sehn ich mich nach dir! Geu\u00df doch den Lebens-Tranck", "tokens": ["Wie", "sehn", "ich", "mich", "nach", "dir", "!", "Geu\u00df", "doch", "den", "Le\u00b7bens\u00b7Tranck"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "APPR", "PPER", "$.", "NN", "ADV", "ART", "NN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "Den d\u00fcrren Lippen ein/ mein Heyland und mein Schatz!", "tokens": ["Den", "d\u00fcr\u00b7ren", "Lip\u00b7pen", "ein", "/", "mein", "Hey\u00b7land", "und", "mein", "Schatz", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "$(", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Befreye mich doch bald und nimm mich von der Welt/", "tokens": ["Be\u00b7fre\u00b7ye", "mich", "doch", "bald", "und", "nimm", "mich", "von", "der", "Welt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "KON", "VVIMP", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "In der ich schmachten mu\u00df! Dein unumbschriebne Macht/", "tokens": ["In", "der", "ich", "schmach\u00b7ten", "mu\u00df", "!", "Dein", "u\u00b7numbsc\u00b7hrieb\u00b7ne", "Macht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVINF", "VMFIN", "$.", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Vermag ja zu ertheiln die h\u00f6chstgew\u00fcnschte Lust.", "tokens": ["Ver\u00b7mag", "ja", "zu", "er\u00b7theiln", "die", "h\u00f6chst\u00b7ge\u00b7w\u00fcnschte", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKZU", "VVINF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.25": {"text": "Ich habe nie geliebt der Wollust Zauber-Tranck", "tokens": ["Ich", "ha\u00b7be", "nie", "ge\u00b7liebt", "der", "Wol\u00b7lust", "Zau\u00b7ber\u00b7Tranck"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und geitzig nachgestrebt zu haben einen Schatz.", "tokens": ["Und", "geit\u00b7zig", "nach\u00b7ge\u00b7strebt", "zu", "ha\u00b7ben", "ei\u00b7nen", "Schatz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "PTKZU", "VAINF", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ich wuste da\u00df verging die Eitelkeit der Welt/", "tokens": ["Ich", "wus\u00b7te", "da\u00df", "ver\u00b7ging", "die", "Ei\u00b7tel\u00b7keit", "der", "Welt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "VVFIN", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Drumb war ich nicht ihr Freund. Ich lacht\u2019 offt ihrer Macht/", "tokens": ["Drumb", "war", "ich", "nicht", "ihr", "Freund", ".", "Ich", "lacht'", "offt", "ih\u00b7rer", "Macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Und seuffz\u2019te nur zu dir/ mein GOtt/ weil keine Lust", "tokens": ["Und", "seuff\u00b7z'\u00b7te", "nur", "zu", "dir", "/", "mein", "Gott", "/", "weil", "kei\u00b7ne", "Lust"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPER", "$(", "PPOSAT", "NN", "$(", "KOUS", "PIAT", "NN"], "meter": "--+-+-+-+-+-+", "measure": "anapaest.init"}, "line.30": {"text": "Ohn Angst. Und Jahr und Zeit verderben die Gestalt.", "tokens": ["Ohn", "Angst", ".", "Und", "Jahr", "und", "Zeit", "ver\u00b7der\u00b7ben", "die", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$.", "KON", "NN", "KON", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Nun HErr/ weil ich dich lieb/ entschlag ich mich der Welt/", "tokens": ["Nun", "Herr", "/", "weil", "ich", "dich", "lieb", "/", "ent\u00b7schlag", "ich", "mich", "der", "Welt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$(", "KOUS", "PPER", "PRF", "ADJD", "$(", "VVIMP", "PPER", "PRF", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Weil ich dein Allmacht ehr/ verspott ich ird\u2019sche Macht/", "tokens": ["Weil", "ich", "dein", "All\u00b7macht", "ehr", "/", "ver\u00b7spott", "ich", "ird'\u00b7sche", "Macht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "NN", "$(", "VVFIN", "PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Und habe nur an dir und deinem Worte Lust.", "tokens": ["Und", "ha\u00b7be", "nur", "an", "dir", "und", "dei\u00b7nem", "Wor\u00b7te", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "APPR", "PPER", "KON", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Mein geistlich Auge sieht dein\u2019 Anmuth der Gestalt.", "tokens": ["Mein", "geist\u00b7lich", "Au\u00b7ge", "sieht", "dein'", "An\u00b7muth", "der", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Weg/ weil mein Br\u00e4utigam mich tr\u00e4nckt mit seinem Tranck/", "tokens": ["Weg", "/", "weil", "mein", "Br\u00e4u\u00b7ti\u00b7gam", "mich", "tr\u00e4nckt", "mit", "sei\u00b7nem", "Tranck", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Welt/ Macht/ Lust und Gestalt/ Tranck/ Aecker/ Wiesen/ Schatz.", "tokens": ["Welt", "/", "Macht", "/", "Lust", "und", "Ge\u00b7stalt", "/", "Tranck", "/", "A\u00b7e\u00b7cker", "/", "Wie\u00b7sen", "/", "Schatz", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "NN", "KON", "NN", "$(", "NN", "$(", "NN", "$(", "NN", "$(", "NN", "$."], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}}}}}