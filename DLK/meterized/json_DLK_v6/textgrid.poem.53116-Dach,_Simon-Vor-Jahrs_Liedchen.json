{"textgrid.poem.53116": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Vor-Jahrs Liedchen", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Lust hat mich gezwungen", "tokens": ["Die", "Lust", "hat", "mich", "ge\u00b7zwun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu fahren in den Wald,", "tokens": ["Zu", "fah\u00b7ren", "in", "den", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo durch der V\u00f6gel Zungen", "tokens": ["Wo", "durch", "der", "V\u00f6\u00b7gel", "Zun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die gantze Lufft erschallt.", "tokens": ["Die", "gant\u00b7ze", "Lufft", "er\u00b7schallt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Fahrt fort, jhr Frewden Kinder,", "tokens": ["Fahrt", "fort", ",", "jhr", "Frew\u00b7den", "Kin\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr P\u00fcsche-B\u00fcrgerey", "tokens": ["Ihr", "P\u00fc\u00b7sche\u00b7B\u00fcr\u00b7ge\u00b7rey"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vnd Freyheit-volck nicht minder,", "tokens": ["Vnd", "Frey\u00b7heit\u00b7volck", "nicht", "min\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Singt ewre Melodey!", "tokens": ["Singt", "ew\u00b7re", "Me\u00b7lo\u00b7dey", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ihr lebt ohn alle Sorgen", "tokens": ["Ihr", "lebt", "ohn", "al\u00b7le", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vnd lobt die G\u00fct' vnd Macht", "tokens": ["Vnd", "lobt", "die", "G\u00fct'", "vnd", "Macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des Sch\u00f6pffers von dem Morgen", "tokens": ["Des", "Sch\u00f6pf\u00b7fers", "von", "dem", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bi\u00df in die sp\u00e4te Nacht.", "tokens": ["Bi\u00df", "in", "die", "sp\u00e4\u00b7te", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ihr bawt euch artig Neste,", "tokens": ["Ihr", "bawt", "euch", "ar\u00b7tig", "Nes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nur da\u00df Ihr Junge heckt,", "tokens": ["Nur", "da\u00df", "Ihr", "Jun\u00b7ge", "heckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Seyd nirgends Fremd' vnd G\u00e4ste,", "tokens": ["Seyd", "nir\u00b7gends", "Fremd'", "vnd", "G\u00e4s\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Habt ewren Tisch gedeckt.", "tokens": ["Habt", "ew\u00b7ren", "Tisch", "ge\u00b7deckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ihr strebet nicht nach Sch\u00e4tzen", "tokens": ["Ihr", "stre\u00b7bet", "nicht", "nach", "Sch\u00e4t\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch Abgunst M\u00fch' vnd Streit,", "tokens": ["Durch", "Ab\u00b7gunst", "M\u00fch'", "vnd", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Wald ist ewr Ergetzen,", "tokens": ["Der", "Wald", "ist", "ewr", "Er\u00b7get\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Federn ewer Kleidt.", "tokens": ["Die", "Fe\u00b7dern", "e\u00b7wer", "Kleidt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ach wolte Gott, wir lebten", "tokens": ["Ach", "wol\u00b7te", "Gott", ",", "wir", "leb\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VMFIN", "NN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Vnschuld, gleich wie Ihr,", "tokens": ["In", "Vn\u00b7schuld", ",", "gleich", "wie", "Ihr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADV", "KOKOM", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nicht ohn auffh\u00f6ren schwebten", "tokens": ["Nicht", "ohn", "auff\u00b7h\u00f6\u00b7ren", "schweb\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "VVINF", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In sorglicher Begier!", "tokens": ["In", "sorg\u00b7li\u00b7cher", "Be\u00b7gier", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wer ist, der also trawet", "tokens": ["Wer", "ist", ",", "der", "al\u00b7so", "tra\u00b7wet"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VAFIN", "$,", "PRELS", "ADV", "VVFIN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Auff Gott, das h\u00f6chste Gut,", "tokens": ["Auff", "Gott", ",", "das", "h\u00f6chs\u00b7te", "Gut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der diese Welt gebawet,", "tokens": ["Der", "die\u00b7se", "Welt", "ge\u00b7ba\u00b7wet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Vnd allen gutes thut?", "tokens": ["Vnd", "al\u00b7len", "gu\u00b7tes", "thut", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wir sind nicht zu erf\u00fcllen", "tokens": ["Wir", "sind", "nicht", "zu", "er\u00b7f\u00fcl\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit Reichthumb vnd Gewinn,", "tokens": ["Mit", "Reicht\u00b7humb", "vnd", "Ge\u00b7winn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vnd gehn vmb Geldes willen", "tokens": ["Vnd", "gehn", "vmb", "Gel\u00b7des", "wil\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Offt zu der H\u00f6llen hin.", "tokens": ["Offt", "zu", "der", "H\u00f6l\u00b7len", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.9": {"line.1": {"text": "O, da\u00df wir Gott anhiengen,", "tokens": ["O", ",", "da\u00df", "wir", "Gott", "an\u00b7hien\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der vns versorgen kan,", "tokens": ["Der", "vns", "ver\u00b7sor\u00b7gen", "kan", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vnd recht zu leben fiengen", "tokens": ["Vnd", "recht", "zu", "le\u00b7ben", "fi\u00b7en\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "PTKZU", "VVINF", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Von Euch, Ihr V\u00f6gel, an!", "tokens": ["Von", "Euch", ",", "Ihr", "V\u00f6\u00b7gel", ",", "an", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPOSAT", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}