{"textgrid.poem.47280": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Sultan l\u00e4\u00dft den Mewlana", "tokens": ["Der", "Sul\u00b7tan", "l\u00e4\u00dft", "den", "Mew\u00b7la\u00b7na"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Thronsaal f\u00fchren, ihn zu fragen:", "tokens": ["Zum", "Thron\u00b7sa\u00b7al", "f\u00fch\u00b7ren", ",", "ihn", "zu", "fra\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "\u00bbdu r\u00fchmst dich sondrer Weisheit ja,", "tokens": ["\u00bb", "du", "r\u00fchmst", "dich", "sond\u00b7rer", "Weis\u00b7heit", "ja", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So sollst du mir nun Antwort sagen.", "tokens": ["So", "sollst", "du", "mir", "nun", "Ant\u00b7wort", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "In vier verschiedne Sekten teilt", "tokens": ["In", "vier", "ver\u00b7schied\u00b7ne", "Sek\u00b7ten", "teilt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich alles Volk der Muselmanen;", "tokens": ["Sich", "al\u00b7les", "Volk", "der", "Mu\u00b7sel\u00b7ma\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So sage nun mir unverweilt,", "tokens": ["So", "sa\u00b7ge", "nun", "mir", "un\u00b7ver\u00b7weilt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wer geht davon auf rechten Bahnen?", "tokens": ["Wer", "geht", "da\u00b7von", "auf", "rech\u00b7ten", "Bah\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PAV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Auf welchem der vier Pfade mag", "tokens": ["Auf", "wel\u00b7chem", "der", "vier", "Pfa\u00b7de", "mag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "ART", "CARD", "NN", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Staub zum Thron des Herrn gelangen?", "tokens": ["Der", "Staub", "zum", "Thron", "des", "Herrn", "ge\u00b7lan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich zweifelte bis diesen Tag,", "tokens": ["Ich", "zwei\u00b7fel\u00b7te", "bis", "die\u00b7sen", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Nun la\u00df Gewi\u00dfheit mich empfangen.", "tokens": ["Nun", "la\u00df", "Ge\u00b7wi\u00df\u00b7heit", "mich", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Sultan sprach's und harrte stumm;", "tokens": ["Der", "Sul\u00b7tan", "sprach's", "und", "harr\u00b7te", "stumm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Mewlana, erst sah er schweigend", "tokens": ["Der", "Mew\u00b7la\u00b7na", ",", "erst", "sah", "er", "schwei\u00b7gend"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NE", "$,", "ADV", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Im Thronsaal sich des Sultans um,", "tokens": ["Im", "Thron\u00b7sa\u00b7al", "sich", "des", "Sul\u00b7tans", "um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dann sprach er, sich vor ihm verneigend:", "tokens": ["Dann", "sprach", "er", ",", "sich", "vor", "ihm", "ver\u00b7nei\u00b7gend", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PRF", "APPR", "PPER", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "Du, dessen Thron das Ebenbild", "tokens": ["Du", ",", "des\u00b7sen", "Thron", "das", "E\u00b7ben\u00b7bild"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELAT", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Throns der Himmel ist auf Erden,", "tokens": ["Des", "Throns", "der", "Him\u00b7mel", "ist", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mich schirme deiner Gnade Schild;", "tokens": ["Mich", "schir\u00b7me", "dei\u00b7ner", "Gna\u00b7de", "Schild", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So soll dir meine Antwort werden:", "tokens": ["So", "soll", "dir", "mei\u00b7ne", "Ant\u00b7wort", "wer\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Du thronest hier in einem Saal,", "tokens": ["Du", "thro\u00b7nest", "hier", "in", "ei\u00b7nem", "Saal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu dem ge\u00f6ffnet sind vier Th\u00fcren;", "tokens": ["Zu", "dem", "ge\u00b7\u00f6ff\u00b7net", "sind", "vier", "Th\u00fc\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVPP", "VAFIN", "CARD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und deinen Thron sieht allzumal,", "tokens": ["Und", "dei\u00b7nen", "Thron", "sieht", "all\u00b7zu\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wen du durch eine l\u00e4ssest f\u00fchren.", "tokens": ["Wen", "du", "durch", "ei\u00b7ne", "l\u00e4s\u00b7sest", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ART", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Da\u00df ich des Weges nicht geirrt,", "tokens": ["Da\u00df", "ich", "des", "We\u00b7ges", "nicht", "ge\u00b7irrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des mu\u00dfte mir dein Bote frommen;", "tokens": ["Des", "mu\u00df\u00b7te", "mir", "dein", "Bo\u00b7te", "from\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VMFIN", "PPER", "PPOSAT", "NN", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und nun wei\u00df ich, vom Glanz verwirrt,", "tokens": ["Und", "nun", "wei\u00df", "ich", ",", "vom", "Glanz", "ver\u00b7wirrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht, welches Wegs ich bin gekommen.\u00ab", "tokens": ["Nicht", ",", "wel\u00b7ches", "Wegs", "ich", "bin", "ge\u00b7kom\u00b7men", ".", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "$,", "PWAT", "NN", "PPER", "VAFIN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}