{"dta.poem.20131": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Zur Verlobung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Es passt uns nicht die alte Leier", "tokens": ["Es", "passt", "uns", "nicht", "die", "al\u00b7te", "Lei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In unsren jungen Liebesrausch,", "tokens": ["In", "un\u00b7sren", "jun\u00b7gen", "Lie\u00b7bes\u00b7rausch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir denken und wir f\u00fchlen freier,", "tokens": ["Wir", "den\u00b7ken", "und", "wir", "f\u00fch\u00b7len", "frei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wollen\u2019s auch beim Ringetausch;", "tokens": ["Und", "wol\u00b7len's", "auch", "beim", "Rin\u00b7ge\u00b7tausch", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Treue Pfand, zu dieser Stunde", "tokens": ["Der", "Treu\u00b7e", "Pfand", ",", "zu", "die\u00b7ser", "Stun\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Empfang\u2019 es in Champagner-Wein:", "tokens": ["Emp\u00b7fang'", "es", "in", "Cham\u00b7pa\u00b7gner\u00b7Wein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der g\u00fcldne Ring auf Bechers Grunde", "tokens": ["Der", "g\u00fcld\u00b7ne", "Ring", "auf", "Be\u00b7chers", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NE", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Soll Sinnbild meines Lebens sein.", "tokens": ["Soll", "Sinn\u00b7bild", "mei\u00b7nes", "Le\u00b7bens", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "La\u00df \u00fcbersprudeln mich, und freue", "tokens": ["La\u00df", "\u00fc\u00b7bers\u00b7pru\u00b7deln", "mich", ",", "und", "freu\u00b7e"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "VVFIN", "PPER", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Kraft Dich, die da sch\u00e4umt und g\u00e4hrt;", "tokens": ["Der", "Kraft", "Dich", ",", "die", "da", "sch\u00e4umt", "und", "g\u00e4hrt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "$,", "PRELS", "ADV", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Tiefinnen, wie ", "tokens": ["Tie\u00b7fin\u00b7nen", ",", "wie"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "PWAV"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Lebt meine Liebe unversehrt.", "tokens": ["Lebt", "mei\u00b7ne", "Lie\u00b7be", "un\u00b7ver\u00b7sehrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Trink aus! begeistern und erheben", "tokens": ["Trink", "aus", "!", "be\u00b7geis\u00b7tern", "und", "er\u00b7he\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$.", "VVINF", "KON", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "La\u00df Dich zu heil\u2019ger Leidenschaft,", "tokens": ["La\u00df", "Dich", "zu", "heil'\u00b7ger", "Lei\u00b7den\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und trinke dann aus meinem Leben", "tokens": ["Und", "trin\u00b7ke", "dann", "aus", "mei\u00b7nem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dir gleiche Lust und gleiche Kraft.", "tokens": ["Dir", "glei\u00b7che", "Lust", "und", "glei\u00b7che", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}