{"dta.poem.4447": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-200905197778", "language": ["de:0.99"], "booktitle": "Karsch, Anna Luise: Auserlesene Gedichte. Berlin, 1764."}, "poem": {"stanza.1": {"line.1": {"text": "Die Pflanze stirbt, von Wolken unbegossen,", "tokens": ["Die", "Pflan\u00b7ze", "stirbt", ",", "von", "Wol\u00b7ken", "un\u00b7be\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom G\u00e4rtner unbespr\u00fctzt, ", "tokens": ["Vom", "G\u00e4rt\u00b7ner", "un\u00b7be\u00b7spr\u00fctzt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der edle Fruchtkern treibt zum wilden Apfelsprossen,", "tokens": ["Der", "ed\u00b7le", "Frucht\u00b7kern", "treibt", "zum", "wil\u00b7den", "Ap\u00b7fel\u00b7spros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn nicht die Kunst den Baum erzieht.", "tokens": ["Wenn", "nicht", "die", "Kunst", "den", "Baum", "er\u00b7zieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}