{"textgrid.poem.31199": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "An unser Volk!", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Herz entflammt, das rothe Banner schwingend,", "tokens": ["Das", "Herz", "ent\u00b7flammt", ",", "das", "ro\u00b7the", "Ban\u00b7ner", "schwin\u00b7gend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den nackten Flammberg in der nackten Hand,", "tokens": ["Den", "nack\u00b7ten", "Flamm\u00b7berg", "in", "der", "nack\u00b7ten", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So wandern wir, von deiner Zukunft singend,", "tokens": ["So", "wan\u00b7dern", "wir", ",", "von", "dei\u00b7ner", "Zu\u00b7kunft", "sin\u00b7gend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Freiheit S\u00f6hne, durch das Land.", "tokens": ["Der", "Frei\u00b7heit", "S\u00f6h\u00b7ne", ",", "durch", "das", "Land", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nicht deine G\u00f6tter wollen wir erschlagen,", "tokens": ["Nicht", "dei\u00b7ne", "G\u00f6t\u00b7ter", "wol\u00b7len", "wir", "er\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "VMFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die fallen, wenn sie morsch, von selber um;", "tokens": ["Die", "fal\u00b7len", ",", "wenn", "sie", "morsch", ",", "von", "sel\u00b7ber", "um", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "VVINF", "$,", "KOUS", "PPER", "ADJD", "$,", "APPR", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch deine Seele soll sich blutig fragen", "tokens": ["Doch", "dei\u00b7ne", "See\u00b7le", "soll", "sich", "blu\u00b7tig", "fra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "PRF", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "An unserm Aufruhrwort: Warum?", "tokens": ["An", "un\u00b7serm", "Auf\u00b7ruhr\u00b7wort", ":", "Wa\u00b7rum", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "PWAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Warum du hungerst und warum du d\u00fcrstest,", "tokens": ["Wa\u00b7rum", "du", "hun\u00b7gerst", "und", "wa\u00b7rum", "du", "d\u00fcrs\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "KON", "PWAV", "PPER", "VMFIN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Warum du schweisstriefst und warum du frierst,", "tokens": ["Wa\u00b7rum", "du", "schweiss\u00b7triefst", "und", "wa\u00b7rum", "du", "frierst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "KON", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Warum du h\u00fcndisch deine Peinger f\u00fcrstest,", "tokens": ["Wa\u00b7rum", "du", "h\u00fcn\u00b7disch", "dei\u00b7ne", "Pein\u00b7ger", "f\u00fcrs\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Warum du fr\u00f6mmelnd dich verthierst!", "tokens": ["Wa\u00b7rum", "du", "fr\u00f6m\u00b7melnd", "dich", "ver\u00b7thierst", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Weh, dreimal Wehe, wenn am Tag der Jden", "tokens": ["Weh", ",", "drei\u00b7mal", "We\u00b7he", ",", "wenn", "am", "Tag", "der", "Jden"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "NN", "$,", "KOUS", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Kelch des Zorns dann blutig \u00fcbersch\u00e4umt", "tokens": ["Der", "Kelch", "des", "Zorns", "dann", "blu\u00b7tig", "\u00fc\u00b7bersc\u00b7h\u00e4umt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und jener goldne Traum von einem ewgen Frieden", "tokens": ["Und", "je\u00b7ner", "gold\u00b7ne", "Traum", "von", "ei\u00b7nem", "ew\u00b7gen", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "ADJA", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Umsonst getr\u00e4umt!", "tokens": ["Um\u00b7sonst", "ge\u00b7tr\u00e4umt", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}