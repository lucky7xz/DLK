{"textgrid.poem.24696": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Weihnachtsgel\u00e4ute", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weihnachtsgel\u00e4ute", "tokens": ["Weih\u00b7nachts\u00b7ge\u00b7l\u00e4u\u00b7te"], "token_info": ["word"], "pos": ["NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Im n\u00e4chtigen Wind ...", "tokens": ["Im", "n\u00e4ch\u00b7ti\u00b7gen", "Wind", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Wer wei\u00df, wo heute", "tokens": ["Wer", "wei\u00df", ",", "wo", "heu\u00b7te"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PWS", "VVFIN", "$,", "PWAV", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Die Glocken sind,", "tokens": ["Die", "Glo\u00b7cken", "sind", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Die T\u00f6ne von damals sind?", "tokens": ["Die", "T\u00f6\u00b7ne", "von", "da\u00b7mals", "sind", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADV", "VAFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Die lebenden T\u00f6ne", "tokens": ["Die", "le\u00b7ben\u00b7den", "T\u00f6\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Verflogener Jahr'", "tokens": ["Ver\u00b7flo\u00b7ge\u00b7ner", "Jahr'"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Mit kindischer Sch\u00f6ne", "tokens": ["Mit", "kin\u00b7di\u00b7scher", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und duftendem Haar,", "tokens": ["Und", "duf\u00b7ten\u00b7dem", "Haar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Mit tannenduftigem Haar,", "tokens": ["Mit", "tan\u00b7nen\u00b7duf\u00b7ti\u00b7gem", "Haar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Mit Lippen und Locken", "tokens": ["Mit", "Lip\u00b7pen", "und", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Von Tr\u00e4umen schwer? ...", "tokens": ["Von", "Tr\u00e4u\u00b7men", "schwer", "?", "..."], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "ADJD", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und wo kommen die Glocken", "tokens": ["Und", "wo", "kom\u00b7men", "die", "Glo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "VVFIN", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Von heute her,", "tokens": ["Von", "heu\u00b7te", "her", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Die wandernden heute her?", "tokens": ["Die", "wan\u00b7dern\u00b7den", "heu\u00b7te", "her", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Die kommenden Tage,", "tokens": ["Die", "kom\u00b7men\u00b7den", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Die wehn da vorbei.", "tokens": ["Die", "wehn", "da", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Wer h\u00f6rts, ob Klage,", "tokens": ["Wer", "h\u00f6rts", ",", "ob", "Kla\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADV", "$,", "KOUS", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Ob lachender Mai,", "tokens": ["Ob", "la\u00b7chen\u00b7der", "Mai", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Ob bl\u00fchender, gl\u00fchender Mai? ...", "tokens": ["Ob", "bl\u00fc\u00b7hen\u00b7der", ",", "gl\u00fc\u00b7hen\u00b7der", "Mai", "?", "..."], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KOUS", "ADJA", "$,", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}