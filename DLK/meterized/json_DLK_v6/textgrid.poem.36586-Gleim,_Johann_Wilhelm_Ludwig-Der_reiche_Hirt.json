{"textgrid.poem.36586": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der reiche Hirt", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wollte mich Belinde lieben,", "tokens": ["Woll\u00b7te", "mich", "Be\u00b7lin\u00b7de", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "O wie wohl w\u00e4r' ich daran!", "tokens": ["O", "wie", "wohl", "w\u00e4r'", "ich", "da\u00b7ran", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADV", "VAFIN", "PPER", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Viere, f\u00fcnfe, sechse, sieben \u2013 \u2013", "tokens": ["Vie\u00b7re", ",", "f\u00fcn\u00b7fe", ",", "sech\u00b7se", ",", "sie\u00b7ben", "\u2013", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["PIS", "$,", "VVFIN", "$,", "VVFIN", "$,", "CARD", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zwanzig Schafe wend' ich an!", "tokens": ["Zwan\u00b7zig", "Scha\u00b7fe", "wend'", "ich", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Zwanzig? \u2013 hundert wollt' ich sagen!", "tokens": ["Zwan\u00b7zig", "?", "\u2013", "hun\u00b7dert", "wollt'", "ich", "sa\u00b7gen", "!"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "$.", "$(", "CARD", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hundert, ja! g\u00e4b' ich darum.", "tokens": ["Hun\u00b7dert", ",", "ja", "!", "g\u00e4b'", "ich", "da\u00b7rum", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "$,", "ADV", "$.", "VVFIN", "PPER", "PAV", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "Ja! Sie selbst sollt' ich nur fragen,", "tokens": ["Ja", "!", "Sie", "selbst", "sollt'", "ich", "nur", "fra\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPER", "ADV", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Aber immer steh' ich stumm!", "tokens": ["A\u00b7ber", "im\u00b7mer", "steh'", "ich", "stumm", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "An den Bach, der durch die Mitte", "tokens": ["An", "den", "Bach", ",", "der", "durch", "die", "Mit\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meiner fetten Fluren fleu\u00dft,", "tokens": ["Mei\u00b7ner", "fet\u00b7ten", "Flu\u00b7ren", "fleu\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Trat sie einst, mit sanftem Tritte,", "tokens": ["Trat", "sie", "einst", ",", "mit", "sanf\u00b7tem", "Trit\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da erh\u00f6hte sich mein Geist!", "tokens": ["Da", "er\u00b7h\u00f6h\u00b7te", "sich", "mein", "Geist", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da fragt' ich: willst du mich lieben?", "tokens": ["Da", "fragt'", "ich", ":", "willst", "du", "mich", "lie\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Schweigend drehte sie sich um;", "tokens": ["Schwei\u00b7gend", "dreh\u00b7te", "sie", "sich", "um", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "W\u00e4re sie nur stehn geblieben,", "tokens": ["W\u00e4\u00b7re", "sie", "nur", "stehn", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVINF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "O was g\u00e4b' ich nicht darum?", "tokens": ["O", "was", "g\u00e4b'", "ich", "nicht", "da\u00b7rum", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWS", "VVFIN", "PPER", "PTKNEG", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Giebst du deine ganze Herde?", "tokens": ["Giebst", "du", "dei\u00b7ne", "gan\u00b7ze", "Her\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fragte heute mich Dam\u00f6t,", "tokens": ["Frag\u00b7te", "heu\u00b7te", "mich", "Da\u00b7m\u00f6t", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der mit gr\u00e4mlicher Geberde", "tokens": ["Der", "mit", "gr\u00e4m\u00b7li\u00b7cher", "Ge\u00b7ber\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unter seiner Linde steht!", "tokens": ["Un\u00b7ter", "sei\u00b7ner", "Lin\u00b7de", "steht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja! die ganze will ich geben,", "tokens": ["Ja", "!", "die", "gan\u00b7ze", "will", "ich", "ge\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ART", "ADJA", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ja! das schw\u00f6r' ich! morgen fr\u00fch!", "tokens": ["Ja", "!", "das", "schw\u00f6r'", "ich", "!", "mor\u00b7gen", "fr\u00fch", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PDS", "VVFIN", "PPER", "$.", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Herd' und Flur, und Luft und Leben,", "tokens": ["Herd'", "und", "Flur", ",", "und", "Luft", "und", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$,", "KON", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Alles hab' ich, hab' ich sie!", "tokens": ["Al\u00b7les", "hab'", "ich", ",", "hab'", "ich", "sie", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "$,", "VAFIN", "PPER", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}