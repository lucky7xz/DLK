{"textgrid.poem.36303": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An Camoens", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo Indiens Sonne trunknen Duft den Winden", "tokens": ["Wo", "In\u00b7di\u00b7ens", "Son\u00b7ne", "trunk\u00b7nen", "Duft", "den", "Win\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "NN", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Ausstreut, gedachtest du der hohen Kunden,", "tokens": ["Aus\u00b7streut", ",", "ge\u00b7dach\u00b7test", "du", "der", "ho\u00b7hen", "Kun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie Gama einst der Thetis sich verbunden,", "tokens": ["Wie", "Ga\u00b7ma", "einst", "der", "The\u00b7tis", "sich", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "ADV", "ART", "NE", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wolltest der Helden Haupt mit Ruhm umwinden.", "tokens": ["Woll\u00b7test", "der", "Hel\u00b7den", "Haupt", "mit", "Ruhm", "um\u00b7win\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O weh uns Armen, irdisch ewig Blinden;", "tokens": ["O", "weh", "uns", "Ar\u00b7men", ",", "ir\u00b7disch", "e\u00b7wig", "Blin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "NN", "$,", "ADJD", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kaum war dein Lied dem wilden Meer entwunden,", "tokens": ["Kaum", "war", "dein", "Lied", "dem", "wil\u00b7den", "Meer", "ent\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sahst du von Alter, Sorge, Gram gebunden,", "tokens": ["Sahst", "du", "von", "Al\u00b7ter", ",", "Sor\u00b7ge", ",", "Gram", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,", "NN", "$,", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den letzten K\u00f6nig deines Volks verschwinden.", "tokens": ["Den", "letz\u00b7ten", "K\u00f6\u00b7nig", "dei\u00b7nes", "Volks", "ver\u00b7schwin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wollust haucht in dem Liede, Seel' entraubend,", "tokens": ["Wol\u00b7lust", "haucht", "in", "dem", "Lie\u00b7de", ",", "Seel'", "en\u00b7trau\u00b7bend", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,", "NN", "VVPP", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Frohlockend kommt der Helden Schiff geflogen,", "tokens": ["Froh\u00b7lo\u00b7ckend", "kommt", "der", "Hel\u00b7den", "Schiff", "ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Tief unten braust ein Strom verborgner Klagen.", "tokens": ["Tief", "un\u00b7ten", "braust", "ein", "Strom", "ver\u00b7borg\u00b7ner", "Kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sei, Camo\u00ebns, denn mein Vorbild! La\u00df mich's wagen", "tokens": ["Sei", ",", "Ca\u00b7mo\u00ebns", ",", "denn", "mein", "Vor\u00b7bild", "!", "La\u00df", "mich's", "wa\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "$,", "NE", "$,", "KON", "PPOSAT", "NN", "$.", "VVIMP", "PIS", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des deutschen Ruhms Urkunde aus den Wogen", "tokens": ["Des", "deut\u00b7schen", "Ruhms", "Ur\u00b7kun\u00b7de", "aus", "den", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Empor zu halten, an die Rettung glaubend.", "tokens": ["Em\u00b7por", "zu", "hal\u00b7ten", ",", "an", "die", "Ret\u00b7tung", "glau\u00b7bend", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}