{"dta.poem.21200": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dort unten an des Berges Fu\u00df", "tokens": ["Dort", "un\u00b7ten", "an", "des", "Ber\u00b7ges", "Fu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Liegt in dem Felsen eingehauen", "tokens": ["Liegt", "in", "dem", "Fel\u00b7sen", "ein\u00b7ge\u00b7hau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "VVINF"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das unentweihte Grab, des Heilands Ruhestatt.", "tokens": ["Das", "un\u00b7ent\u00b7weih\u00b7te", "Grab", ",", "des", "Hei\u00b7lands", "Ru\u00b7he\u00b7statt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Hain rauscht hier ein heiligs Grauen;", "tokens": ["Der", "Hain", "rauscht", "hier", "ein", "hei\u00b7ligs", "Grau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und oftmals hat", "tokens": ["Und", "oft\u00b7mals", "hat"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "VAFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Die einsame Melancholey", "tokens": ["Die", "ein\u00b7sa\u00b7me", "Me\u00b7lan\u00b7cho\u00b7ley"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Hier Lieder der Unsterblichen geh\u00f6ret,", "tokens": ["Hier", "Lie\u00b7der", "der", "U\u00b7nsterb\u00b7li\u00b7chen", "ge\u00b7h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Die des Erl\u00f6sers Sieg verehret.", "tokens": ["Die", "des", "Er\u00b7l\u00f6\u00b7sers", "Sieg", "ver\u00b7eh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}