{"textgrid.poem.61901": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Versuchung", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Trau' dir selber nicht allzuviel", "tokens": ["Trau'", "dir", "sel\u00b7ber", "nicht", "all\u00b7zu\u00b7viel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "PTKNEG", "PIAT"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Und wend' auf deinem Gange,", "tokens": ["Und", "wend'", "auf", "dei\u00b7nem", "Gan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wende das Haupt auch nicht zum Spiel", "tokens": ["Wen\u00b7de", "das", "Haupt", "auch", "nicht", "zum", "Spiel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADV", "PTKNEG", "APPRART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Nach der S\u00fcnde, der Schlange!", "tokens": ["Nach", "der", "S\u00fcn\u00b7de", ",", "der", "Schlan\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.2": {"line.1": {"text": "Ihr Auge dunkel wie die Nacht", "tokens": ["Ihr", "Au\u00b7ge", "dun\u00b7kel", "wie", "die", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJD", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Versteht so reizend zu blicken;", "tokens": ["Ver\u00b7steht", "so", "rei\u00b7zend", "zu", "bli\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Du wei\u00dft es, da\u00df sie dich elend macht,", "tokens": ["Du", "wei\u00dft", "es", ",", "da\u00df", "sie", "dich", "e\u00b7lend", "macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und l\u00e4ssest dich doch bestricken.", "tokens": ["Und", "l\u00e4s\u00b7sest", "dich", "doch", "be\u00b7stri\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}