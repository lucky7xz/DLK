{"textgrid.poem.49259": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Zweig von sch\u00f6ner Tugend,", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Zweig von sch\u00f6ner Tugend,", "tokens": ["Ein", "Zweig", "von", "sch\u00f6\u00b7ner", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von einer edlen Art,", "tokens": ["Von", "ei\u00b7ner", "ed\u00b7len", "Art", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Cron und Lust der Jugend,", "tokens": ["Die", "Cron", "und", "Lust", "der", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sehr lieblich rein und zart,", "tokens": ["Sehr", "lieb\u00b7lich", "rein", "und", "zart", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Stund pr\u00e4chtig und erhaben,", "tokens": ["Stund", "pr\u00e4ch\u00b7tig", "und", "er\u00b7ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So da\u00df ihm nichts an Gaben", "tokens": ["So", "da\u00df", "ihm", "nichts", "an", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "PIS", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Fast gleich gefunden ward.", "tokens": ["Fast", "gleich", "ge\u00b7fun\u00b7den", "ward", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Was ist doch hier vollkommen?", "tokens": ["Was", "ist", "doch", "hier", "voll\u00b7kom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo herrscht das Gl\u00fccke nicht?", "tokens": ["Wo", "herrscht", "das", "Gl\u00fc\u00b7cke", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Blume ward genommen", "tokens": ["Der", "Blu\u00b7me", "ward", "ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "De\u00df Pflegers Hut und Pflicht.", "tokens": ["De\u00df", "Pfle\u00b7gers", "Hut", "und", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er muste von der Erden,", "tokens": ["Er", "mus\u00b7te", "von", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und sie verweyset werden,", "tokens": ["Und", "sie", "ver\u00b7wey\u00b7set", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "De\u00df Landes Ziehr und Liecht.", "tokens": ["De\u00df", "Lan\u00b7des", "Ziehr", "und", "Liecht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie steht doch und vertrauet", "tokens": ["Sie", "steht", "doch", "und", "ver\u00b7trau\u00b7et"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "De\u00df Himmels Gunst allein,", "tokens": ["De\u00df", "Him\u00b7mels", "Gunst", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der freundlich auff sie schauet", "tokens": ["Der", "freund\u00b7lich", "auff", "sie", "schau\u00b7et"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und selbst ihr Trost wil seyn,", "tokens": ["Und", "selbst", "ihr", "Trost", "wil", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wil den Verlust erg\u00e4ntzen,", "tokens": ["Wil", "den", "Ver\u00b7lust", "er\u00b7g\u00e4nt\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Und fr\u00f6lich sie begl\u00e4ntzen", "tokens": ["Und", "fr\u00f6\u00b7lich", "sie", "be\u00b7gl\u00e4nt\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Durch eines Sternes Schein.", "tokens": ["Durch", "ei\u00b7nes", "Ster\u00b7nes", "Schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er blinckt mit g\u00fcldnen Stralen,", "tokens": ["Er", "blinckt", "mit", "g\u00fcld\u00b7nen", "Stra\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Stern von Mitternacht,", "tokens": ["Der", "Stern", "von", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie Phebus pflegt zu mahlen", "tokens": ["Wie", "Phe\u00b7bus", "pflegt", "zu", "mah\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "VVFIN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Wolcken blaue Tracht,", "tokens": ["Der", "Wol\u00b7cken", "blau\u00b7e", "Tracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie Luna, wann die Flammen", "tokens": ["Wie", "Lu\u00b7na", ",", "wann", "die", "Flam\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "NE", "$,", "PWAV", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "In ihr sind rund beysammen,", "tokens": ["In", "ihr", "sind", "rund", "bey\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Das Firmament belacht.", "tokens": ["Das", "Fir\u00b7ma\u00b7ment", "be\u00b7lacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der kalte Nortstern pfleget", "tokens": ["Der", "kal\u00b7te", "Norts\u00b7tern", "pfle\u00b7get"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Schiffer Trost zu seyn,", "tokens": ["Der", "Schif\u00b7fer", "Trost", "zu", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wann Wind und Sturm sich reget,", "tokens": ["Wann", "Wind", "und", "Sturm", "sich", "re\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Rei\u00dft Mast und Segel ein;", "tokens": ["Rei\u00dft", "Mast", "und", "Se\u00b7gel", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Den Stern, den wir hier meynen,", "tokens": ["Den", "Stern", ",", "den", "wir", "hier", "mey\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der sol und wil bescheinen", "tokens": ["Der", "sol", "und", "wil", "be\u00b7schei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "KON", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die liebste Blum allein.", "tokens": ["Die", "liebs\u00b7te", "Blum", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wer lobt dich nach Gen\u00fcgen,", "tokens": ["Wer", "lobt", "dich", "nach", "Ge\u00b7n\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O du gew\u00fcndschte Nacht,", "tokens": ["O", "du", "ge\u00b7w\u00fcnd\u00b7schte", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die du den Stern solst f\u00fcgen", "tokens": ["Die", "du", "den", "Stern", "solst", "f\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu seiner Blumen Pracht?", "tokens": ["Zu", "sei\u00b7ner", "Blu\u00b7men", "Pracht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hoch bist du, Stern, zu sch\u00e4tzen,", "tokens": ["Hoch", "bist", "du", ",", "Stern", ",", "zu", "sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "NN", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Du, Zweig, kanst dich ergetzen,", "tokens": ["Du", ",", "Zweig", ",", "kanst", "dich", "er\u00b7get\u00b7zen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Dein Treuer Leitstern wacht.", "tokens": ["Dein", "Treu\u00b7er", "Leits\u00b7tern", "wacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Es ist jetzt nicht von N\u00f6hten,", "tokens": ["Es", "ist", "jetzt", "nicht", "von", "N\u00f6h\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df bald der Tag anbricht,", "tokens": ["Da\u00df", "bald", "der", "Tag", "an\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vor tausent Morgenr\u00f6hten", "tokens": ["Vor", "tau\u00b7sent", "Mor\u00b7gen\u00b7r\u00f6h\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gl\u00e4ntzt dieses Sternes Liecht.", "tokens": ["Gl\u00e4ntzt", "die\u00b7ses", "Ster\u00b7nes", "Liecht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du, Blume, wirst nun gr\u00fcnen,", "tokens": ["Du", ",", "Blu\u00b7me", ",", "wirst", "nun", "gr\u00fc\u00b7nen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VAFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wann dich dein Stern beschienen;", "tokens": ["Wann", "dich", "dein", "Stern", "be\u00b7schie\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die Wirckung fehlet nicht.", "tokens": ["Die", "Wir\u00b7ckung", "feh\u00b7let", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Die milte Scho\u00df der Erden", "tokens": ["Die", "mil\u00b7te", "Scho\u00df", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mag reichlich weit und breit", "tokens": ["Mag", "reich\u00b7lich", "weit", "und", "breit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADJD", "ADJD", "KON", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vom Monden tr\u00e4chtig werden", "tokens": ["Vom", "Mon\u00b7den", "tr\u00e4ch\u00b7tig", "wer\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJD", "VAINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bey dieser Friedens-Zeit.", "tokens": ["Bey", "die\u00b7ser", "Frie\u00b7dens\u00b7Zeit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Stern ist nur erschienen,", "tokens": ["Der", "Stern", "ist", "nur", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Di\u00df Bl\u00fcmlein zu bedienen", "tokens": ["Di\u00df", "Bl\u00fcm\u00b7lein", "zu", "be\u00b7die\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Mit Krafft der Fruchtbarkeit.", "tokens": ["Mit", "Krafft", "der", "Frucht\u00b7bar\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Stern, du wirst wachsen machen", "tokens": ["Stern", ",", "du", "wirst", "wach\u00b7sen", "ma\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VAFIN", "VVINF", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Die Blume recht und wol,", "tokens": ["Die", "Blu\u00b7me", "recht", "und", "wol", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dein Stern mu\u00df dich bewachen,", "tokens": ["Dein", "Stern", "mu\u00df", "dich", "be\u00b7wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O Blume, wie er sol,", "tokens": ["O", "Blu\u00b7me", ",", "wie", "er", "sol", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "PPER", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wann dieser Stern so stehet,", "tokens": ["Wann", "die\u00b7ser", "Stern", "so", "ste\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und Thau das Feld durchgehet,", "tokens": ["Und", "Thau", "das", "Feld", "durch\u00b7ge\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "So wird die Rose voll.", "tokens": ["So", "wird", "die", "Ro\u00b7se", "voll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}