{"textgrid.poem.61699": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin gegangen,", "tokens": ["Ich", "bin", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Den Mai empfangen,", "tokens": ["Den", "Mai", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Doch bracht' er keinen Gru\u00df f\u00fcr mich;", "tokens": ["Doch", "bracht'", "er", "kei\u00b7nen", "Gru\u00df", "f\u00fcr", "mich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Wolken zogen,", "tokens": ["Die", "Wol\u00b7ken", "zo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Die Schlo\u00dfen flogen,", "tokens": ["Die", "Schlo\u00b7\u00dfen", "flo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Ein eis'ger Hauch vom Flusse strich.", "tokens": ["Ein", "eis'\u00b7ger", "Hauch", "vom", "Flus\u00b7se", "strich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wer mag der Bl\u00fcten", "tokens": ["Wer", "mag", "der", "Bl\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Im Garten h\u00fcten,", "tokens": ["Im", "Gar\u00b7ten", "h\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wenn also weht der scharfe Wind?", "tokens": ["Wenn", "al\u00b7so", "weht", "der", "schar\u00b7fe", "Wind", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Um den ich bange,", "tokens": ["Um", "den", "ich", "ban\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "PPER", "ADV", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Wie schweigt er lange,", "tokens": ["Wie", "schweigt", "er", "lan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Und r\u00e4t es keiner, was er sinnt!", "tokens": ["Und", "r\u00e4t", "es", "kei\u00b7ner", ",", "was", "er", "sinnt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIS", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wer mag den Segen", "tokens": ["Wer", "mag", "den", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Im Herzen pflegen,", "tokens": ["Im", "Her\u00b7zen", "pfle\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": 