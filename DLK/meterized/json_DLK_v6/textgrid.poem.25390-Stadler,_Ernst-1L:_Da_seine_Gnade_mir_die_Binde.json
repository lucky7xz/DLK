{"textgrid.poem.25390": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da seine Gnade mir die Binde", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da seine Gnade mir die Binde", "tokens": ["Da", "sei\u00b7ne", "Gna\u00b7de", "mir", "die", "Bin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "von den Augen schlo\u00df,", "tokens": ["von", "den", "Au\u00b7gen", "schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Troff Licht wie Regen brennend.", "tokens": ["Troff", "Licht", "wie", "Re\u00b7gen", "bren\u00b7nend", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KOKOM", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Land lag da und bl\u00fchte.", "tokens": ["Land", "lag", "da", "und", "bl\u00fch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "KON", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Ich schritt so wie im Tanz.", "tokens": ["Ich", "schritt", "so", "wie", "im", "Tanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KOKOM", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und was davor mich wie mit Knebeln m\u00fchte,", "tokens": ["Und", "was", "da\u00b7vor", "mich", "wie", "mit", "Kne\u00b7beln", "m\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PAV", "PPER", "KOKOM", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Fiel ab und war von mir getan.", "tokens": ["Fiel", "ab", "und", "war", "von", "mir", "ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mich \u00fcberflo\u00df", "tokens": ["Mich", "\u00fc\u00b7ber\u00b7flo\u00df"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Das Gnadenwunder, unaufh\u00f6rlich quellend \u2013", "tokens": ["Das", "Gna\u00b7den\u00b7wun\u00b7der", ",", "un\u00b7auf\u00b7h\u00f6r\u00b7lich", "quel\u00b7lend", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "so wie junger Wein", "tokens": ["so", "wie", "jun\u00b7ger", "Wein"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Im Herbst,", "tokens": ["Im", "Herbst", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.12": {"text": "wenn sie auf allen goldnen H\u00fcgeln keltern,", "tokens": ["wenn", "sie", "auf", "al\u00b7len", "gold\u00b7nen", "H\u00fc\u00b7geln", "kel\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PIAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Und rings die H\u00e4nge nieder Saft aufspritzt", "tokens": ["Und", "rings", "die", "H\u00e4n\u00b7ge", "nie\u00b7der", "Saft", "auf\u00b7spritzt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "PTKVZ", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "und flammt in den Beh\u00e4ltern,", "tokens": ["und", "flammt", "in", "den", "Be\u00b7h\u00e4l\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Flammte vor mir die Welt", "tokens": ["Flamm\u00b7te", "vor", "mir", "die", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPER", "ART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.16": {"text": "und ward nun ganz erst mein", "tokens": ["und", "ward", "nun", "ganz", "erst", "mein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "ADV", "ADV", "PPOSAT"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Und meines Odems Odem.", "tokens": ["Und", "mei\u00b7nes", "O\u00b7dems", "O\u00b7dem", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Jedes Ding war neu und gieng", "tokens": ["Je\u00b7des", "Ding", "war", "neu", "und", "gieng"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "ADJD", "KON", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "In tiefer Herzenswallung mir entgegen,", "tokens": ["In", "tie\u00b7fer", "Her\u00b7zens\u00b7wal\u00b7lung", "mir", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "sich zu schenken, so wie am Altar,", "tokens": ["sich", "zu", "schen\u00b7ken", ",", "so", "wie", "am", "Al\u00b7tar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKZU", "VVINF", "$,", "ADV", "KOKOM", "APPRART", "NN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.21": {"text": "Des Opfers freudig, ganz in Gl\u00fcck gekleidet.", "tokens": ["Des", "Op\u00b7fers", "freu\u00b7dig", ",", "ganz", "in", "Gl\u00fcck", "ge\u00b7klei\u00b7det", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Und in jedem war", "tokens": ["Und", "in", "je\u00b7dem", "war"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PIS", "VAFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.23": {"text": "Der Gott. Und keines war, darauf nicht seine G\u00fcte", "tokens": ["Der", "Gott", ".", "Und", "kei\u00b7nes", "war", ",", "da\u00b7rauf", "nicht", "sei\u00b7ne", "G\u00fc\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "KON", "PIS", "VAFIN", "$,", "PAV", "PTKNEG", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "so wie Hauch um reife Fr\u00fcchte hieng.", "tokens": ["so", "wie", "Hauch", "um", "rei\u00b7fe", "Fr\u00fcch\u00b7te", "hieng", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.25": {"text": "Mir aber brach die Liebe alle T\u00fcren auf,", "tokens": ["Mir", "a\u00b7ber", "brach", "die", "Lie\u00b7be", "al\u00b7le", "T\u00fc\u00b7ren", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "die Hochmut mir gesperrt:", "tokens": ["die", "Hoch\u00b7mut", "mir", "ge\u00b7sperrt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.27": {"text": "In Not Gescharte, Bettler, S\u00e4ufer,", "tokens": ["In", "Not", "Ge\u00b7schar\u00b7te", ",", "Bett\u00b7ler", ",", "S\u00e4u\u00b7fer", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Dirnen und Verbannte", "tokens": ["Dir\u00b7nen", "und", "Ver\u00b7bann\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.29": {"text": "Wurden mein lieb Geschwister. Meine Demut kniete", "tokens": ["Wur\u00b7den", "mein", "lieb", "Ge\u00b7schwis\u00b7ter", ".", "Mei\u00b7ne", "De\u00b7mut", "knie\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "ADJD", "NN", "$.", "PPOSAT", "NN", "VVFIN"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.30": {"text": "vor dem Licht, das fern in ihren Augen brannte,", "tokens": ["vor", "dem", "Licht", ",", "das", "fern", "in", "ih\u00b7ren", "Au\u00b7gen", "brann\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.31": {"text": "Und ihre rauhen Stimmen", "tokens": ["Und", "ih\u00b7re", "rau\u00b7hen", "Stim\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.32": {"text": "schlossen sich zum himmlischen Konzert.", "tokens": ["schlos\u00b7sen", "sich", "zum", "himm\u00b7li\u00b7schen", "Kon\u00b7zert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.33": {"text": "Ich selbst war dunkel ihrem Leid", "tokens": ["Ich", "selbst", "war", "dun\u00b7kel", "ih\u00b7rem", "Leid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "und ihrer Lust vermengt \u2013 Welle im Chor", "tokens": ["und", "ih\u00b7rer", "Lust", "ver\u00b7mengt", "\u2013", "Wel\u00b7le", "im", "Chor"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "$(", "NN", "APPRART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.35": {"text": "Auffahrender Chor\u00e4le. Meine Seele war die kleine Glocke,", "tokens": ["Auf\u00b7fah\u00b7ren\u00b7der", "Cho\u00b7r\u00e4\u00b7le", ".", "Mei\u00b7ne", "See\u00b7le", "war", "die", "klei\u00b7ne", "Glo\u00b7cke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$.", "PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.36": {"text": "die im Dorfkirchhimmel der Gebete hieng", "tokens": ["die", "im", "Dorf\u00b7kirch\u00b7him\u00b7mel", "der", "Ge\u00b7be\u00b7te", "hieng"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPRART", "NN", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.37": {"text": "Und selig l\u00e4utend in dem \u00dcberschwang der Stimmen", "tokens": ["Und", "se\u00b7lig", "l\u00e4u\u00b7tend", "in", "dem", "\u00dc\u00b7bersc\u00b7hwang", "der", "Stim\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJD", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "sich verlor", "tokens": ["sich", "ver\u00b7lor"], "token_info": ["word", "word"], "pos": ["PRF", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.39": {"text": "Und ausgesch\u00fcttet in dem Tausendfachen untergieng.", "tokens": ["Und", "aus\u00b7ge\u00b7sch\u00fct\u00b7tet", "in", "dem", "Tau\u00b7send\u00b7fa\u00b7chen", "un\u00b7ter\u00b7gieng", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}