{"textgrid.poem.50073": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "29. Auff einen Schul-Fuchs", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dass eine ", "tokens": ["Dass", "ei\u00b7ne"], "token_info": ["word", "word"], "pos": ["KOUS", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Bissweilen grosse Leut' ", "tokens": ["Biss\u00b7wei\u00b7len", "gros\u00b7se", "Leut'"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das glaubt er nicht, und denckt wer bey der Lamp' und Uhr", "tokens": ["Das", "glaubt", "er", "nicht", ",", "und", "denckt", "wer", "bey", "der", "Lamp'", "und", "Uhr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "PTKNEG", "$,", "KON", "VVFIN", "PWS", "APPR", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nicht manche lange N\u00e4chte wachet;", "tokens": ["Nicht", "man\u00b7che", "lan\u00b7ge", "N\u00e4ch\u00b7te", "wa\u00b7chet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wer die ", "tokens": ["Wer", "die"], "token_info": ["word", "word"], "pos": ["PWS", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Versteht, noch voller ", "tokens": ["Ver\u00b7steht", ",", "noch", "vol\u00b7ler"], "token_info": ["word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "ADV", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Der sey ein schlechter Tropff: Kurtz, ", "tokens": ["Der", "sey", "ein", "schlech\u00b7ter", "Tropff", ":", "Kurtz", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$.", "ADJD", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}