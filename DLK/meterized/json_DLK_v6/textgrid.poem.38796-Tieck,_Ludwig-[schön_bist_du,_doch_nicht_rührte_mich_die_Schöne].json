{"textgrid.poem.38796": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "[sch\u00f6n bist du, doch nicht r\u00fchrte mich die Sch\u00f6ne]", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6n bist du, doch nicht r\u00fchrte mich die Sch\u00f6ne,", "tokens": ["Sch\u00f6n", "bist", "du", ",", "doch", "nicht", "r\u00fchr\u00b7te", "mich", "die", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "$,", "ADV", "PTKNEG", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht konnte mich der Augen Licht besiegen,", "tokens": ["Nicht", "konn\u00b7te", "mich", "der", "Au\u00b7gen", "Licht", "be\u00b7sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VMFIN", "PRF", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und nicht der H\u00e4nd' und Arm' holdseelges Schmiegen,", "tokens": ["Und", "nicht", "der", "H\u00e4nd'", "und", "Arm'", "hold\u00b7seel\u00b7ges", "Schmie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "KON", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nicht drang zum Herzen die Gewalt der T\u00f6ne:", "tokens": ["Nicht", "drang", "zum", "Her\u00b7zen", "die", "Ge\u00b7walt", "der", "T\u00f6\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "APPRART", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wohl f\u00fchlt' ich wie sich rings die Welt versch\u00f6ne", "tokens": ["Wohl", "f\u00fchlt'", "ich", "wie", "sich", "rings", "die", "Welt", "ver\u00b7sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PWAV", "PRF", "ADV", "ART", "NN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von deinem Glanz, es m\u00fc\u00dfte jeglich Kriegen", "tokens": ["Von", "dei\u00b7nem", "Glanz", ",", "es", "m\u00fc\u00df\u00b7te", "jeg\u00b7lich", "Krie\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPER", "VMFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mir im vers\u00f6hnten Herzen stille liegen,", "tokens": ["Mir", "im", "ver\u00b7s\u00f6hn\u00b7ten", "Her\u00b7zen", "stil\u00b7le", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df sich der Friedensgeist mit mir vers\u00f6hne.", "tokens": ["Da\u00df", "sich", "der", "Frie\u00b7dens\u00b7geist", "mit", "mir", "ver\u00b7s\u00f6h\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nicht war ich mein und auch noch nicht der Deine,", "tokens": ["Nicht", "war", "ich", "mein", "und", "auch", "noch", "nicht", "der", "Dei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VAFIN", "PPER", "PPOSAT", "KON", "ADV", "ADV", "PTKNEG", "ART", "PPOSAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es kamen, gingen r\u00e4thselvolle Stunden,", "tokens": ["Es", "ka\u00b7men", ",", "gin\u00b7gen", "r\u00e4th\u00b7sel\u00b7vol\u00b7le", "Stun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da schaltst du, Alma, meines Herzens S\u00e4umni\u00df,", "tokens": ["Da", "schaltst", "du", ",", "Al\u00b7ma", ",", "mei\u00b7nes", "Her\u00b7zens", "S\u00e4u\u00b7mni\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NE", "$,", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ein Licht flog aus der Augen hellem Scheine,", "tokens": ["Ein", "Licht", "flog", "aus", "der", "Au\u00b7gen", "hel\u00b7lem", "Schei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da hatt' ich dich Sch\u00f6nheit und Lieb' empfunden", "tokens": ["Da", "hatt'", "ich", "dich", "Sch\u00f6n\u00b7heit", "und", "Lieb'", "emp\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "NN", "KON", "NN", "VVPP"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Im s\u00fc\u00dfesten unnennbarsten Geheimni\u00df.", "tokens": ["Im", "s\u00fc\u00b7\u00dfes\u00b7ten", "un\u00b7nenn\u00b7bars\u00b7ten", "Ge\u00b7heim\u00b7ni\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}