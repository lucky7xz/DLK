{"textgrid.poem.34254": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die poetische Licenz", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es tanzt ein Mann auf einem Seil", "tokens": ["Es", "tanzt", "ein", "Mann", "auf", "ei\u00b7nem", "Seil"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit der Lizenz, den Hals zu brechen,", "tokens": ["Mit", "der", "Li\u00b7zenz", ",", "den", "Hals", "zu", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch der Poet an seinem Theil", "tokens": ["Doch", "der", "Po\u00b7et", "an", "sei\u00b7nem", "Theil"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mu\u00df mir nicht von Lizenzen sprechen;", "tokens": ["Mu\u00df", "mir", "nicht", "von", "Li\u00b7zen\u00b7zen", "spre\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Je schwerer, was er vor sich sieht,", "tokens": ["Je", "schwe\u00b7rer", ",", "was", "er", "vor", "sich", "sieht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWS", "PPER", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Je leichter mu\u00df er es vollbringen,", "tokens": ["Je", "leich\u00b7ter", "mu\u00df", "er", "es", "voll\u00b7brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ein schlechter Reim passirt im Lied,", "tokens": ["Ein", "schlech\u00b7ter", "Reim", "pas\u00b7sirt", "im", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Doch das Sonett mu\u00df rein erklingen:", "tokens": ["Doch", "das", "So\u00b7nett", "mu\u00df", "rein", "er\u00b7klin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Es k\u00f6nnt' ihn ja ein Sch\u00fcler dort", "tokens": ["Es", "k\u00f6nnt'", "ihn", "ja", "ein", "Sch\u00fc\u00b7ler", "dort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Vermeiden, warum mit ihm rechten?", "tokens": ["Ver\u00b7mei\u00b7den", ",", "wa\u00b7rum", "mit", "ihm", "rech\u00b7ten", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "APPR", "PPER", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Allein den Meister braucht's, das Wort", "tokens": ["Al\u00b7lein", "den", "Meis\u00b7ter", "braucht's", ",", "das", "Wort"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Vierfach und dreifach zu verflechten.", "tokens": ["Vier\u00b7fach", "und", "drei\u00b7fach", "zu", "ver\u00b7flech\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.13": {"text": "Nicht, da\u00df ihm die\u00df und das gelang,", "tokens": ["Nicht", ",", "da\u00df", "ihm", "die\u00df", "und", "das", "ge\u00b7lang", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "KOUS", "PPER", "PDS", "KON", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Wird der Gebildete ihm danken,", "tokens": ["Wird", "der", "Ge\u00b7bil\u00b7de\u00b7te", "ihm", "dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Nur, da\u00df sein Geist zur H\u00f6he drang,", "tokens": ["Nur", ",", "da\u00df", "sein", "Geist", "zur", "H\u00f6\u00b7he", "drang", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Wo man nicht k\u00e4mpft, nur spielt mit Schranken;", "tokens": ["Wo", "man", "nicht", "k\u00e4mpft", ",", "nur", "spielt", "mit", "Schran\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PTKNEG", "VVFIN", "$,", "ADV", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Nur, da\u00df er ihm die ganze Kunst,", "tokens": ["Nur", ",", "da\u00df", "er", "ihm", "die", "gan\u00b7ze", "Kunst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Und w\u00e4r's im kleinsten Bilde, zeigte.", "tokens": ["Und", "w\u00e4r's", "im", "kleins\u00b7ten", "Bil\u00b7de", ",", "zeig\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VAFIN", "APPRART", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Der Musen wunderbare Gunst,", "tokens": ["Der", "Mu\u00b7sen", "wun\u00b7der\u00b7ba\u00b7re", "Gunst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Der auch das Spr\u00f6deste sich neigte.", "tokens": ["Der", "auch", "das", "Spr\u00f6\u00b7des\u00b7te", "sich", "neig\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Drum geb' ich denn mit Goethe nicht", "tokens": ["Drum", "geb'", "ich", "denn", "mit", "Goe\u00b7the", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "APPR", "NE", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "F\u00fcr den Gedanken alle Reime,", "tokens": ["F\u00fcr", "den", "Ge\u00b7dan\u00b7ken", "al\u00b7le", "Rei\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PIAT", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.23": {"text": "Ich ford're Beides vom Gedicht,", "tokens": ["Ich", "ford'\u00b7re", "Bei\u00b7des", "vom", "Ge\u00b7dicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Denn Beides w\u00e4chs't aus Einem Keime.", "tokens": ["Denn", "Bei\u00b7des", "w\u00e4chs't", "aus", "Ei\u00b7nem", "Kei\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}