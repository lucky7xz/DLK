{"textgrid.poem.54304": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich w\u00fcnsch euch einen guten Morgen!", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich w\u00fcnsch euch einen guten Morgen!", "tokens": ["Ich", "w\u00fcnsch", "euch", "ei\u00b7nen", "gu\u00b7ten", "Mor\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Habt ihr geschlaffen ohne Sorgen", "tokens": ["Habt", "ihr", "ge\u00b7schlaf\u00b7fen", "oh\u00b7ne", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "VVPP", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und seyd von allen Kummer quitt?", "tokens": ["Und", "seyd", "von", "al\u00b7len", "Kum\u00b7mer", "quitt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was macht der Arm? will er noch lermen?", "tokens": ["Was", "macht", "der", "Arm", "?", "will", "er", "noch", "ler\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$.", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Will noch der Schmertz darinnen schw\u00e4rmen?", "tokens": ["Will", "noch", "der", "Schmertz", "da\u00b7rin\u00b7nen", "schw\u00e4r\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Di\u00df w\u00e4r, ich schw\u00f6r es auf mein Blut,", "tokens": ["Di\u00df", "w\u00e4r", ",", "ich", "schw\u00f6r", "es", "auf", "mein", "Blut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Vor euch und mich zugleich nicht gut.", "tokens": ["Vor", "euch", "und", "mich", "zu\u00b7gleich", "nicht", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "PPER", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "M\u00fc\u00dft ihr ihn in der Binde f\u00fchren?", "tokens": ["M\u00fc\u00dft", "ihr", "ihn", "in", "der", "Bin\u00b7de", "f\u00fch\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Kan unser Lie\u00dfgen nicht curiren?", "tokens": ["Kan", "un\u00b7ser", "Lie\u00df\u00b7gen", "nicht", "cu\u00b7ri\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Ist denn kein Mittel, das man find?", "tokens": ["Ist", "denn", "kein", "Mit\u00b7tel", ",", "das", "man", "find", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "$,", "PRELS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ihr daurt mich, wie mein leiblich Kind.", "tokens": ["Ihr", "daurt", "mich", ",", "wie", "mein", "leib\u00b7lich", "Kind", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOKOM", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Mein Mitleid k\u00f6nt ihr sicher glauben,", "tokens": ["Mein", "Mit\u00b7leid", "k\u00f6nt", "ihr", "si\u00b7cher", "glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Ich such euch warlich nicht zu schrauben,", "tokens": ["Ich", "such", "euch", "war\u00b7lich", "nicht", "zu", "schrau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Der Wohlstand litte solches nicht,", "tokens": ["Der", "Wohl\u00b7stand", "lit\u00b7te", "sol\u00b7ches", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Weil man mit Freunden redlich spricht;", "tokens": ["Weil", "man", "mit", "Freun\u00b7den", "red\u00b7lich", "spricht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Und w\u00fcrd ich anders mich gebehrden,", "tokens": ["Und", "w\u00fcrd", "ich", "an\u00b7ders", "mich", "ge\u00b7behr\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "So d\u00fcrffte Lie\u00dfgen b\u00f6se werden,", "tokens": ["So", "d\u00fcrff\u00b7te", "Lie\u00df\u00b7gen", "b\u00f6\u00b7se", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Die Gunst von ihr ist mir zu lieb,", "tokens": ["Die", "Gunst", "von", "ihr", "ist", "mir", "zu", "lieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VAFIN", "PPER", "PTKA", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.19": {"text": "O w\u00e4r ich nur ein Hertzens-Dieb.", "tokens": ["O", "w\u00e4r", "ich", "nur", "ein", "Hert\u00b7zens\u00b7Dieb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Es solte mir so dann nicht fehlen,", "tokens": ["Es", "sol\u00b7te", "mir", "so", "dann", "nicht", "feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Das gantze Hertz ihr abzustehlen,", "tokens": ["Das", "gant\u00b7ze", "Hertz", "ihr", "ab\u00b7zu\u00b7steh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Warum so viele sich bem\u00fchn,", "tokens": ["Wa\u00b7rum", "so", "vie\u00b7le", "sich", "be\u00b7m\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIS", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Die mich gedencken abzuziehn.", "tokens": ["Die", "mich", "ge\u00b7den\u00b7cken", "ab\u00b7zu\u00b7ziehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NN", "VVIZU", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Da wolt ich mich hinein vergraben,", "tokens": ["Da", "wolt", "ich", "mich", "hin\u00b7ein", "ver\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "APZR", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Und kein vergn\u00fcgter Sitzgen haben;", "tokens": ["Und", "kein", "ver\u00b7gn\u00fcg\u00b7ter", "Sitz\u00b7gen", "ha\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Jedoch verirrter Kiel halt ein,", "tokens": ["Je\u00b7doch", "ver\u00b7irr\u00b7ter", "Kiel", "halt", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Versprochen mu\u00df gehalten seyn.", "tokens": ["Ver\u00b7spro\u00b7chen", "mu\u00df", "ge\u00b7hal\u00b7ten", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Wie h\u00e4lt es um das Componiren?", "tokens": ["Wie", "h\u00e4lt", "es", "um", "das", "Com\u00b7po\u00b7ni\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Was soll ich vor ein Thema f\u00fchren?", "tokens": ["Was", "soll", "ich", "vor", "ein", "The\u00b7ma", "f\u00fch\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Die Muse, die bereits erhitzt,", "tokens": ["Die", "Mu\u00b7se", ",", "die", "be\u00b7reits", "er\u00b7hitzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Hat meine Feder l\u00e4ngst gespitzt.", "tokens": ["Hat", "mei\u00b7ne", "Fe\u00b7der", "l\u00e4ngst", "ge\u00b7spitzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Und also d\u00fcrfft ihr nur befehlen,", "tokens": ["Und", "al\u00b7so", "d\u00fcrfft", "ihr", "nur", "be\u00b7feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Ich will den Ph\u00f6bum brav bestehlen,", "tokens": ["Ich", "will", "den", "Ph\u00f6\u00b7bum", "brav", "be\u00b7steh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "In seinen Hayn und Blumen-Thal,", "tokens": ["In", "sei\u00b7nen", "Hayn", "und", "Blu\u00b7men\u00b7Thal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Es ist ja nicht das erstemahl.", "tokens": ["Es", "ist", "ja", "nicht", "das", "ers\u00b7te\u00b7mahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "So dann will ich ein Versgen machen,", "tokens": ["So", "dann", "will", "ich", "ein", "Ver\u00b7sgen", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.37": {"text": "Und di\u00df zum sch\u00f6nsten Zeitvertreib;", "tokens": ["Und", "di\u00df", "zum", "sch\u00f6ns\u00b7ten", "Zeit\u00b7ver\u00b7treib", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Ich seh euch beyde schon den Leib,", "tokens": ["Ich", "seh", "euch", "bey\u00b7de", "schon", "den", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Mit beyden H\u00e4nden, die sich falten,", "tokens": ["Mit", "bey\u00b7den", "H\u00e4n\u00b7den", ",", "die", "sich", "fal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Voraus im Geist vor Lachen halten,", "tokens": ["Vo\u00b7raus", "im", "Geist", "vor", "La\u00b7chen", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Damit er bey dem Lobesan", "tokens": ["Da\u00b7mit", "er", "bey", "dem", "Lo\u00b7be\u00b7san"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Und Schertze nicht zerspringen kan.", "tokens": ["Und", "Schert\u00b7ze", "nicht", "zer\u00b7sprin\u00b7gen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Wie wird euch nicht mein Meister-Singen", "tokens": ["Wie", "wird", "euch", "nicht", "mein", "Meis\u00b7ter\u00b7Sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "PTKNEG", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "Vortrefflich in den Ohren klingen.", "tokens": ["Vor\u00b7treff\u00b7lich", "in", "den", "Oh\u00b7ren", "klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "Indessen lebt gesund und fein,", "tokens": ["In\u00b7des\u00b7sen", "lebt", "ge\u00b7sund", "und", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Und di\u00df zugleich an Arm und Bein.", "tokens": ["Und", "di\u00df", "zu\u00b7gleich", "an", "Arm", "und", "Bein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Ich kan vor Freuden kaum erwarten,", "tokens": ["Ich", "kan", "vor", "Freu\u00b7den", "kaum", "er\u00b7war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.48": {"text": "Bi\u00df es wird Zeit und Gl\u00fccke karten,", "tokens": ["Bi\u00df", "es", "wird", "Zeit", "und", "Gl\u00fc\u00b7cke", "kar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.49": {"text": "Da\u00df das bewu\u00dfte Fest erscheint,", "tokens": ["Da\u00df", "das", "be\u00b7wu\u00df\u00b7te", "Fest", "er\u00b7scheint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Wornach wir alle l\u00fcstern seynd,", "tokens": ["Wor\u00b7nach", "wir", "al\u00b7le", "l\u00fcs\u00b7tern", "seynd", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Da soll der feste Schlu\u00df verbleiben,", "tokens": ["Da", "soll", "der", "fes\u00b7te", "Schlu\u00df", "ver\u00b7blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.52": {"text": "Das K\u00e4lbgen schwermend auszutreiben.", "tokens": ["Das", "K\u00e4lb\u00b7gen", "schwer\u00b7mend", "aus\u00b7zu\u00b7trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}