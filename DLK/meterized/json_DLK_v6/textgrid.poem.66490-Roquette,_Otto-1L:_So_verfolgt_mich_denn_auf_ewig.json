{"textgrid.poem.66490": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: So verfolgt mich denn auf ewig", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So verfolgt mich denn auf ewig", "tokens": ["So", "ver\u00b7folgt", "mich", "denn", "auf", "e\u00b7wig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieser tolle Klimperd\u00e4mon!", "tokens": ["Die\u00b7ser", "tol\u00b7le", "Klim\u00b7per\u00b7d\u00e4\u00b7mon", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ueber mir vollf\u00fchrt das Fr\u00e4ulein", "tokens": ["Ue\u00b7ber", "mir", "voll\u00b7f\u00fchrt", "das", "Fr\u00e4u\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Auf den Tasten ihre Wirthschaft;", "tokens": ["Auf", "den", "Tas\u00b7ten", "ih\u00b7re", "Wirth\u00b7schaft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Unter mir, in Kr\u00e4nkelt\u00f6nen,", "tokens": ["Un\u00b7ter", "mir", ",", "in", "Kr\u00e4n\u00b7kel\u00b7t\u00f6\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kinder\u00fcbungspr\u00fcgeljammer;", "tokens": ["Kin\u00b7de\u00b7r\u00fc\u00b7bungs\u00b7pr\u00fc\u00b7gel\u00b7jam\u00b7mer", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Gegen\u00fcber gar der Leutnant \u2013", "tokens": ["Ge\u00b7gen\u00b7\u00fc\u00b7ber", "gar", "der", "Leut\u00b7nant", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "NN", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Heilge Musen! Nah und fern", "tokens": ["Heil\u00b7ge", "Mu\u00b7sen", "!", "Nah", "und", "fern"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "NN", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Ein verr\u00fccktes Trommeldr\u00f6hnen", "tokens": ["Ein", "ver\u00b7r\u00fcck\u00b7tes", "Trom\u00b7mel\u00b7dr\u00f6h\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Auf des Hauses Lieblingsm\u00f6bel!", "tokens": ["Auf", "des", "Hau\u00b7ses", "Lieb\u00b7lings\u00b7m\u00f6\u00b7bel", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Bei den Einen wilde L\u00e4rmwuth,", "tokens": ["Bei", "den", "Ei\u00b7nen", "wil\u00b7de", "L\u00e4rm\u00b7wuth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Bei den Andern Zeitvertr\u00f6dlung,", "tokens": ["Bei", "den", "An\u00b7dern", "Zeit\u00b7ver\u00b7tr\u00f6d\u00b7lung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Bei den Meisten r\u00fccksichtslose", "tokens": ["Bei", "den", "Meis\u00b7ten", "r\u00fcck\u00b7sichts\u00b7lo\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.14": {"text": "Stumpfheit und Gedankenmord!", "tokens": ["Stumpf\u00b7heit", "und", "Ge\u00b7dan\u00b7ken\u00b7mord", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "O, wie ist die Zeit verwandelt!", "tokens": ["O", ",", "wie", "ist", "die", "Zeit", "ver\u00b7wan\u00b7delt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fr\u00fcher, wie die V\u00e4ter sagen,", "tokens": ["Fr\u00fc\u00b7her", ",", "wie", "die", "V\u00e4\u00b7ter", "sa\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6rte man in Mittern\u00e4chten", "tokens": ["H\u00f6r\u00b7te", "man", "in", "Mit\u00b7ter\u00b7n\u00e4ch\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur die schw\u00e4rmerische Fl\u00f6te;", "tokens": ["Nur", "die", "schw\u00e4r\u00b7me\u00b7ri\u00b7sche", "Fl\u00f6\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ein bescheidnes Instrument,", "tokens": ["Ein", "be\u00b7scheid\u00b7nes", "Inst\u00b7ru\u00b7ment", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nur ein Ton aus dem Orchester.", "tokens": ["Nur", "ein", "Ton", "aus", "dem", "Or\u00b7ches\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.7": {"text": "Aber heut mu\u00df \u00fcber Massen", "tokens": ["A\u00b7ber", "heut", "mu\u00df", "\u00fc\u00b7ber", "Mas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Jeder Einzle kommandiren,", "tokens": ["Je\u00b7der", "Einz\u00b7le", "kom\u00b7man\u00b7di\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und mit athemloser Arbeit", "tokens": ["Und", "mit", "at\u00b7hem\u00b7lo\u00b7ser", "Ar\u00b7beit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Greift und packt er, was von T\u00f6nen", "tokens": ["Greift", "und", "packt", "er", ",", "was", "von", "T\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "VVFIN", "PPER", "$,", "PRELS", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "H\u00e4ndevoll er kriegen kann,", "tokens": ["H\u00e4n\u00b7de\u00b7voll", "er", "krie\u00b7gen", "kann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Um in ungeheurem Schw\u00e4rmen", "tokens": ["Um", "in", "un\u00b7ge\u00b7heu\u00b7rem", "Schw\u00e4r\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Rasend um sich her zu schleudern", "tokens": ["Ra\u00b7send", "um", "sich", "her", "zu", "schleu\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PRF", "APZR", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Sein verzweifeltes Gef\u00fchl!", "tokens": ["Sein", "ver\u00b7zwei\u00b7fel\u00b7tes", "Ge\u00b7f\u00fchl", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Schicksal, gieb uns armen Eulen,", "tokens": ["Schick\u00b7sal", ",", "gieb", "uns", "ar\u00b7men", "Eu\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die wir deutsche Dichter hei\u00dfen,", "tokens": ["Die", "wir", "deut\u00b7sche", "Dich\u00b7ter", "hei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schaffensruh und Stille brauchen:", "tokens": ["Schaf\u00b7fens\u00b7ruh", "und", "Stil\u00b7le", "brau\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gieb uns Th\u00fcrme, gieb uns B\u00e4ume,", "tokens": ["Gieb", "uns", "Th\u00fcr\u00b7me", ",", "gieb", "uns", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "$,", "VVIMP", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bergesh\u00f6h'n und Einsamkeiten,", "tokens": ["Ber\u00b7ges\u00b7h\u00f6h'n", "und", "Ein\u00b7sam\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Oder schlag' uns gleich mit Taubheit!", "tokens": ["O\u00b7der", "schlag'", "uns", "gleich", "mit", "Taub\u00b7heit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn die H\u00e4user baut man nur noch", "tokens": ["Denn", "die", "H\u00e4u\u00b7ser", "baut", "man", "nur", "noch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PIS", "ADV", "ADV"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Um Claviere drein zu stellen;", "tokens": ["Um", "Cla\u00b7vie\u00b7re", "drein", "zu", "stel\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und die Leute wohnen nur", "tokens": ["Und", "die", "Leu\u00b7te", "woh\u00b7nen", "nur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Drinnen, um darauf zu l\u00e4rmen;", "tokens": ["Drin\u00b7nen", ",", "um", "da\u00b7rauf", "zu", "l\u00e4r\u00b7men", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUI", "PAV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Und sie l\u00e4rmen, und sie klappern,", "tokens": ["Und", "sie", "l\u00e4r\u00b7men", ",", "und", "sie", "klap\u00b7pern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "KON", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Weil sie nichts zu denken haben.", "tokens": ["Weil", "sie", "nichts", "zu", "den\u00b7ken", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ach, ich w\u00fcnschte mir ein eignes", "tokens": ["Ach", ",", "ich", "w\u00fcnschte", "mir", "ein", "eig\u00b7nes"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "PPER", "ART", "ADJA"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Instrument, wo jeder Ton", "tokens": ["Inst\u00b7ru\u00b7ment", ",", "wo", "je\u00b7der", "Ton"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "War' ein Kniff, ein Nasenst\u00fcber,", "tokens": ["Wa\u00b7r'", "ein", "Kniff", ",", "ein", "Na\u00b7sen\u00b7st\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Fu\u00dftritt, Peitschenhieb, Karbatsche!", "tokens": ["Fu\u00df\u00b7tritt", ",", "Peit\u00b7schen\u00b7hieb", ",", "Kar\u00b7bat\u00b7sche", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "O wie himmlisch musikalisch", "tokens": ["O", "wie", "himm\u00b7lisch", "mu\u00b7si\u00b7ka\u00b7lisch"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "KOKOM", "ADJD", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wollt' ich dann noch selber werden", "tokens": ["Wollt'", "ich", "dann", "noch", "sel\u00b7ber", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "ADV", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "F\u00fcr die liebe Nachbarschaft!", "tokens": ["F\u00fcr", "die", "lie\u00b7be", "Nach\u00b7bar\u00b7schaft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}