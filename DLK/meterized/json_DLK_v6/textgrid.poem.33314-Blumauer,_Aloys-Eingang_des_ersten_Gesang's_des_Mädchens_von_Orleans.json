{"textgrid.poem.33314": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Eingang des ersten Gesang's des M\u00e4dchens von Orleans", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr wollt, da\u00df ich die Heiligen euch preise, \u2013", "tokens": ["Ihr", "wollt", ",", "da\u00df", "ich", "die", "Hei\u00b7li\u00b7gen", "euch", "prei\u00b7se", ",", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "PPER", "ART", "NN", "PPER", "VVFIN", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schwach ist mein Ton, und meines Liedes Weise", "tokens": ["Schwach", "ist", "mein", "Ton", ",", "und", "mei\u00b7nes", "Lie\u00b7des", "Wei\u00b7se"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$,", "KON", "PPOSAT", "NN", "NN"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.3": {"text": "Fast zu profan. Doch weil ich folgsam bin,", "tokens": ["Fast", "zu", "pro\u00b7fan", ".", "Doch", "weil", "ich", "folg\u00b7sam", "bin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "$.", "KON", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So sing ich euch die Wunderth\u00e4terin", "tokens": ["So", "sing", "ich", "euch", "die", "Wun\u00b7der\u00b7th\u00e4\u00b7te\u00b7rin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mi\u00df Hanne, die als Jungfer ohne M\u00e4ngel", "tokens": ["Mi\u00df", "Han\u00b7ne", ",", "die", "als", "Jung\u00b7fer", "oh\u00b7ne", "M\u00e4n\u00b7gel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "$,", "PRELS", "KOUS", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Handhabte Frankreichs Lilienstengel,", "tokens": ["Hand\u00b7hab\u00b7te", "Fran\u00b7kreichs", "Li\u00b7li\u00b7ens\u00b7ten\u00b7gel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "$,"], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.7": {"text": "Den K\u00f6nig selbst der Britten Wuth entri\u00df,", "tokens": ["Den", "K\u00f6\u00b7nig", "selbst", "der", "Brit\u00b7ten", "Wuth", "ent\u00b7ri\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und ihn zu Rheims am Altar salben lie\u00df.", "tokens": ["Und", "ihn", "zu", "Rheims", "am", "Al\u00b7tar", "sal\u00b7ben", "lie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NE", "APPRART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Mi\u00df Hanne trug euch unter'm Unterrocke,", "tokens": ["Mi\u00df", "Han\u00b7ne", "trug", "euch", "un\u00b7ter'm", "Un\u00b7ter\u00b7ro\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Wie unter'm Panzerhemd und dem Visier", "tokens": ["Wie", "un\u00b7ter'm", "Pan\u00b7zer\u00b7hemd", "und", "dem", "Vi\u00b7sier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Den ganzen Muth von einer britt'schen Dogge;", "tokens": ["Den", "gan\u00b7zen", "Muth", "von", "ei\u00b7ner", "britt'\u00b7schen", "Dog\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Wiewohl zu meinem Nachtgebrauche mir", "tokens": ["Wie\u00b7wohl", "zu", "mei\u00b7nem", "Nacht\u00b7ge\u00b7brau\u00b7che", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Ein L\u00e4mmchenfrommes M\u00e4dchen lieber w\u00e4re;", "tokens": ["Ein", "L\u00e4mm\u00b7chen\u00b7from\u00b7mes", "M\u00e4d\u00b7chen", "lie\u00b7ber", "w\u00e4\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Doch Hannens L\u00f6wenmuth ist keine M\u00e4hre:", "tokens": ["Doch", "Han\u00b7nens", "L\u00f6\u00b7wen\u00b7muth", "ist", "kei\u00b7ne", "M\u00e4h\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NN", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Ihr werdt's seh'n, les't nur das B\u00fcchlein hier;", "tokens": ["Ihr", "werdt's", "seh'n", ",", "les't", "nur", "das", "B\u00fcch\u00b7lein", "hier", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVINF", "$,", "VVFIN", "ADV", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.16": {"text": "Denn das, womit sie ihrer Thaten Ehre", "tokens": ["Denn", "das", ",", "wo\u00b7mit", "sie", "ih\u00b7rer", "Tha\u00b7ten", "Eh\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "$,", "PWAV", "PPER", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Und ihren Heldenmuth auf's h\u00f6chste trieb,", "tokens": ["Und", "ih\u00b7ren", "Hel\u00b7den\u00b7muth", "auf's", "h\u00f6chs\u00b7te", "trieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPRART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "War, da\u00df sie fast ein Jahr lang \u2013 Jungfer blieb.", "tokens": ["War", ",", "da\u00df", "sie", "fast", "ein", "Jahr", "lang", "\u2013", "Jung\u00b7fer", "blieb", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "ADJD", "$(", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}