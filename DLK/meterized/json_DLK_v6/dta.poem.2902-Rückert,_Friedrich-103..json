{"dta.poem.2902": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "103.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Bruchst\u00fcck immer ist des einzlen Mannes Wissen,", "tokens": ["Ein", "Bruchs\u00b7t\u00fcck", "im\u00b7mer", "ist", "des", "einz\u00b7len", "Man\u00b7nes", "Wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das er als Ganzes darzustellen ist beflissen;", "tokens": ["Das", "er", "als", "Gan\u00b7zes", "dar\u00b7zu\u00b7stel\u00b7len", "ist", "be\u00b7flis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "KOUS", "NN", "VVPP", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Zu loben, wenn er es von innen will erg\u00e4nzen,", "tokens": ["Zu", "lo\u00b7ben", ",", "wenn", "er", "es", "von", "in\u00b7nen", "will", "er\u00b7g\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "KOUS", "PPER", "PPER", "APPR", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu tadeln, wenn mit Schein der Ganzheit \u00fcbergl\u00e4nzen.", "tokens": ["Zu", "ta\u00b7deln", ",", "wenn", "mit", "Schein", "der", "Ganz\u00b7heit", "\u00fc\u00b7ber\u00b7gl\u00e4n\u00b7zen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "KOUS", "APPR", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "In diesem Fall ist doch, wer lehren will und soll,", "tokens": ["In", "die\u00b7sem", "Fall", "ist", "doch", ",", "wer", "leh\u00b7ren", "will", "und", "soll", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "ADV", "$,", "PWS", "VVINF", "VMFIN", "KON", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Eh alle F\u00e4cher noch des Wissens er wei\u00df voll.", "tokens": ["Eh", "al\u00b7le", "F\u00e4\u00b7cher", "noch", "des", "Wis\u00b7sens", "er", "wei\u00df", "voll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "ADV", "ART", "NN", "PPER", "VVFIN", "ADJD", "$."], "meter": "+--+-+-+--++", "measure": "iambic.hexa.invert"}}, "stanza.4": {"line.1": {"text": "Er darf dem Lernenden nicht zeigen seine L\u00fccken,", "tokens": ["Er", "darf", "dem", "Ler\u00b7nen\u00b7den", "nicht", "zei\u00b7gen", "sei\u00b7ne", "L\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit mehr und minder Kunst mu\u00df er denn schlagen Br\u00fccken,", "tokens": ["Mit", "mehr", "und", "min\u00b7der", "Kunst", "mu\u00df", "er", "denn", "schla\u00b7gen", "Br\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "KON", "ADJA", "NN", "VMFIN", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Da\u00df alles scheine nur zusammen fein zu hangen,", "tokens": ["Da\u00df", "al\u00b7les", "schei\u00b7ne", "nur", "zu\u00b7sam\u00b7men", "fein", "zu", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "ADV", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vom einen End der Welt zum andern zu gelangen.", "tokens": ["Vom", "ei\u00b7nen", "End", "der", "Welt", "zum", "an\u00b7dern", "zu", "ge\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ART", "NN", "ART", "NN", "APPRART", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Der arme Mann mu\u00df sich mit fremden Federn schm\u00fccken,", "tokens": ["Der", "ar\u00b7me", "Mann", "mu\u00df", "sich", "mit", "frem\u00b7den", "Fe\u00b7dern", "schm\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PRF", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil er kein Lehrgedicht darf geben in Bruchst\u00fccken.", "tokens": ["Weil", "er", "kein", "Lehr\u00b7ge\u00b7dicht", "darf", "ge\u00b7ben", "in", "Bruchs\u00b7t\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VMFIN", "VVINF", "APPR", "NN", "$."], "meter": "-+-+-+-+--++-", "measure": "iambic.hexa.relaxed"}}}}}