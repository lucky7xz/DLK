{"dta.poem.16031": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "85.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das B\u00f6se hat nicht Macht, die Welt zu Grund zu richten,", "tokens": ["Das", "B\u00f6\u00b7se", "hat", "nicht", "Macht", ",", "die", "Welt", "zu", "Grund", "zu", "rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "NN", "$,", "ART", "NN", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn nichtig ists in sich, und kann nur sich vernichten.", "tokens": ["Denn", "nich\u00b7tig", "ists", "in", "sich", ",", "und", "kann", "nur", "sich", "ver\u00b7nich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "APPR", "PRF", "$,", "KON", "VMFIN", "ADV", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch seine Wirkung kann es mittelbar erstrecken,", "tokens": ["Doch", "sei\u00b7ne", "Wir\u00b7kung", "kann", "es", "mit\u00b7tel\u00b7bar", "er\u00b7stre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der b\u00f6sen Seuche gleich, Gesundes anzustecken.", "tokens": ["Der", "b\u00f6\u00b7sen", "Seu\u00b7che", "gleich", ",", "Ge\u00b7sun\u00b7des", "an\u00b7zu\u00b7ste\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Mittheilen kann sein Gift den Hang der Selbstzerst\u00f6rung;", "tokens": ["Mit\u00b7thei\u00b7len", "kann", "sein", "Gift", "den", "Hang", "der", "Selbst\u00b7zer\u00b7st\u00f6\u00b7rung", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPOSAT", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kein Weiser halte sich gesichert vor Beth\u00f6rung.", "tokens": ["Kein", "Wei\u00b7ser", "hal\u00b7te", "sich", "ge\u00b7si\u00b7chert", "vor", "Be\u00b7th\u00f6\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PRF", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Hier ist die Leidenschaft, die selbst ihr Leiden schafft,", "tokens": ["Hier", "ist", "die", "Lei\u00b7den\u00b7schaft", ",", "die", "selbst", "ihr", "Lei\u00b7den", "schafft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dort der Zweifel, der hin zur Verzweiflung rafft.", "tokens": ["Und", "dort", "der", "Zwei\u00b7fel", ",", "der", "hin", "zur", "Ver\u00b7zwei\u00b7flung", "rafft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "PRELS", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Das ist die Doppelform der Selbstzerst\u00f6rungswuth;", "tokens": ["Das", "ist", "die", "Dop\u00b7pel\u00b7form", "der", "Selbst\u00b7zer\u00b7st\u00f6\u00b7rungs\u00b7wuth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dagegen ist gering, was Welt und Zeit dir thut.", "tokens": ["Da\u00b7ge\u00b7gen", "ist", "ge\u00b7ring", ",", "was", "Welt", "und", "Zeit", "dir", "thut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ADJD", "$,", "PRELS", "NN", "KON", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}