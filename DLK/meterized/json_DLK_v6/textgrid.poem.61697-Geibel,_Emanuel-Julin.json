{"textgrid.poem.61697": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Julin", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es rauscht der Wind, es rinnt die Welle,", "tokens": ["Es", "rauscht", "der", "Wind", ",", "es", "rinnt", "die", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Befl\u00fcgelt schwebt das Schiff dahin;", "tokens": ["Be\u00b7fl\u00fc\u00b7gelt", "schwebt", "das", "Schiff", "da\u00b7hin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "ART", "NN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An jenes Kreidefelsens Schwelle", "tokens": ["An", "je\u00b7nes", "Krei\u00b7de\u00b7fel\u00b7sens", "Schwel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dort, sagt der Schiffer, lag Julin;", "tokens": ["Dort", ",", "sagt", "der", "Schif\u00b7fer", ",", "lag", "Ju\u00b7lin", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "ART", "NN", "$,", "VVFIN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Julin, die hohe Stadt am Sunde,", "tokens": ["Ju\u00b7lin", ",", "die", "ho\u00b7he", "Stadt", "am", "Sun\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die still die Meerflut \u00fcberschwoll;", "tokens": ["Die", "still", "die", "Meer\u00b7flut", "\u00fc\u00b7bersc\u00b7hwoll", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie klingt die fabelhafte Kunde", "tokens": ["Wie", "klingt", "die", "fa\u00b7bel\u00b7haf\u00b7te", "Kun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mir heut ans Herz erinnrungsvoll!", "tokens": ["Mir", "heut", "ans", "Herz", "e\u00b7rinn\u00b7rungs\u00b7voll", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich denk' an meiner Kindheit Tage,", "tokens": ["Ich", "denk'", "an", "mei\u00b7ner", "Kind\u00b7heit", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da mir, von M\u00e4rchenlust beseelt,", "tokens": ["Da", "mir", ",", "von", "M\u00e4r\u00b7chen\u00b7lust", "be\u00b7seelt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Schwester jene Wundersage", "tokens": ["Die", "Schwes\u00b7ter", "je\u00b7ne", "Wun\u00b7der\u00b7sa\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Abends vor der T\u00fcr erz\u00e4hlt.", "tokens": ["Des", "A\u00b7bends", "vor", "der", "T\u00fcr", "er\u00b7z\u00e4hlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Noch steht's mir deutlich im Gem\u00fcte:", "tokens": ["Noch", "steht's", "mir", "deut\u00b7lich", "im", "Ge\u00b7m\u00fc\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wir sa\u00dfen auf der Bank von Stein,", "tokens": ["Wir", "sa\u00b7\u00dfen", "auf", "der", "Bank", "von", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Am Nachbarhaus die Linde bl\u00fchte,", "tokens": ["Am", "Nach\u00b7bar\u00b7haus", "die", "Lin\u00b7de", "bl\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Am Himmel quoll des Mondes Schein.", "tokens": ["Am", "Him\u00b7mel", "quoll", "des", "Mon\u00b7des", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die schlanken Zackengiebel hoben", "tokens": ["Die", "schlan\u00b7ken", "Za\u00b7cken\u00b7gie\u00b7bel", "ho\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So ernst sich, wo der Schatten fiel,", "tokens": ["So", "ernst", "sich", ",", "wo", "der", "Schat\u00b7ten", "fiel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und dann und wann erklang von oben", "tokens": ["Und", "dann", "und", "wann", "er\u00b7klang", "von", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "KON", "PWAV", "VVFIN", "APPR", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von Sankt Marien das Glockenspiel.", "tokens": ["Von", "Sankt", "Ma\u00b7ri\u00b7en", "das", "Glo\u00b7cken\u00b7spiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "NE", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Dann ging's hinein zum Nachtgebete,", "tokens": ["Dann", "ging's", "hin\u00b7ein", "zum", "Nacht\u00b7ge\u00b7be\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und linder Schlaf umfing mich drauf;", "tokens": ["Und", "lin\u00b7der", "Schlaf", "um\u00b7fing", "mich", "drauf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich baute die versunknen St\u00e4dte", "tokens": ["Ich", "bau\u00b7te", "die", "ver\u00b7sun\u00b7knen", "St\u00e4d\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Traume pr\u00e4chtig wieder auf.", "tokens": ["Im", "Trau\u00b7me", "pr\u00e4ch\u00b7tig", "wie\u00b7der", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "O Knabentr\u00e4ume rein und helle,", "tokens": ["O", "Kna\u00b7bent\u00b7r\u00e4u\u00b7me", "rein", "und", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJD", "KON", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O Jugendlust, wo gingt ihr hin! \u2013", "tokens": ["O", "Ju\u00b7gend\u00b7lust", ",", "wo", "gingt", "ihr", "hin", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "VVFIN", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es rauscht der Wind, es rinnt die Welle,", "tokens": ["Es", "rauscht", "der", "Wind", ",", "es", "rinnt", "die", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo sind Vineta und Julin?", "tokens": ["Wo", "sind", "Vi\u00b7ne\u00b7ta", "und", "Ju\u00b7lin", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "NE", "KON", "NE", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}}}}