{"dta.poem.19874": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Das  \n  vergn\u00fcgte Leben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1778", "urn": "urn:nbn:de:kobv:b4-20090519672", "language": ["de:0.99"], "booktitle": "B\u00fcrger, Gottfried August: Gedichte. G\u00f6ttingen, 1778."}, "poem": {"stanza.1": {"line.1": {"text": "Der Geist mus denken. Ohne Denken gleicht               ", "tokens": ["Der", "Geist", "mus", "den\u00b7ken", ".", "Oh\u00b7ne", "Den\u00b7ken", "gleicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$.", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Mensch dem Oechs- und Eselein im Stalle.", "tokens": ["Der", "Mensch", "dem", "O\u00b7echs", "und", "E\u00b7sel\u00b7ein", "im", "Stal\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "TRUNC", "KON", "NN", "APPRART", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sein Herz mus lieben. Ohne Liebe schleicht", "tokens": ["Sein", "Herz", "mus", "lie\u00b7ben", ".", "Oh\u00b7ne", "Lie\u00b7be", "schleicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "VVINF", "$.", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sein Leben mat und lahm, nach Adams Falle.", "tokens": ["Sein", "Le\u00b7ben", "mat", "und", "lahm", ",", "nach", "A\u00b7dams", "Fal\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "KON", "PTKVZ", "$,", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein Kranz umkr\u00e4nz\u2019 ihn, ohne Drang und Zwang,", "tokens": ["Ein", "Kranz", "um\u00b7kr\u00e4nz'", "ihn", ",", "oh\u00b7ne", "Drang", "und", "Zwang", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "KOUI", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Kranz von klugen nur nicht stolzen Leuten,", "tokens": ["Ein", "Kranz", "von", "klu\u00b7gen", "nur", "nicht", "stol\u00b7zen", "Leu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "ADV", "PTKNEG", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die sich auf Wiz verstehn und Schnurrigkeiten;", "tokens": ["Die", "sich", "auf", "Wiz", "ver\u00b7stehn", "und", "Schnur\u00b7rig\u00b7kei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "NN", "VVINF", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Denn sonst w\u00e4hrt mancher Abend gar zu lang.", "tokens": ["Denn", "sonst", "w\u00e4hrt", "man\u00b7cher", "A\u00b7bend", "gar", "zu", "lang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIAT", "NN", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dabei ist\u2019s eine himlisch sch\u00f6ne Sache", "tokens": ["Da\u00b7bei", "ist's", "ei\u00b7ne", "him\u00b7lisch", "sch\u00f6\u00b7ne", "Sa\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Um Einen rechten braven Herzensfreund,", "tokens": ["Um", "Ei\u00b7nen", "rech\u00b7ten", "bra\u00b7ven", "Her\u00b7zens\u00b7freund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der, ist man fr\u00f6lich, wacker mit uns lache,", "tokens": ["Der", ",", "ist", "man", "fr\u00f6\u00b7lich", ",", "wa\u00b7cker", "mit", "uns", "la\u00b7che", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "VAFIN", "PIS", "ADJD", "$,", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und ehrlich weine, so man selber weint.", "tokens": ["Und", "ehr\u00b7lich", "wei\u00b7ne", ",", "so", "man", "sel\u00b7ber", "weint", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$,", "ADV", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der Abend mus ein Leckermal bescheren;", "tokens": ["Der", "A\u00b7bend", "mus", "ein", "Le\u00b7cker\u00b7mal", "be\u00b7sche\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Mal, erheitert durch Gespr\u00e4ch und Wein.", "tokens": ["Ein", "Mal", ",", "er\u00b7hei\u00b7tert", "durch", "Ge\u00b7spr\u00e4ch", "und", "Wein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da mag das Herz vol guter Dinge seyn;", "tokens": ["Da", "mag", "das", "Herz", "vol", "gu\u00b7ter", "Din\u00b7ge", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ADJD", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nur mus der Kopf des Rausches sich erwehren.", "tokens": ["Nur", "mus", "der", "Kopf", "des", "Rau\u00b7sches", "sich", "er\u00b7weh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Was f\u00fcr ein Wunsch zu guter Nacht sich schikt,", "tokens": ["Was", "f\u00fcr", "ein", "Wunsch", "zu", "gu\u00b7ter", "Nacht", "sich", "schikt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das brauch\u2019 ich nicht erst lang und breit zu sagen.", "tokens": ["Das", "brauch'", "ich", "nicht", "erst", "lang", "und", "breit", "zu", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKNEG", "ADV", "ADJD", "KON", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Weibchen mus man mit zu Bette tragen,", "tokens": ["Ein", "Weib\u00b7chen", "mus", "man", "mit", "zu", "Bet\u00b7te", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PIS", "APPR", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das jede Nacht, wie eine Braut, entz\u00fckt.", "tokens": ["Das", "je\u00b7de", "Nacht", ",", "wie", "ei\u00b7ne", "Braut", ",", "ent\u00b7z\u00fckt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$,", "PWAV", "ART", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Sagt, Freunde, schlendert nicht ein solches Leben,", "tokens": ["Sagt", ",", "Freun\u00b7de", ",", "schlen\u00b7dert", "nicht", "ein", "sol\u00b7ches", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "PTKNEG", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gar artig und gem\u00e4chlich seinen Gang?", "tokens": ["Gar", "ar\u00b7tig", "und", "ge\u00b7m\u00e4ch\u00b7lich", "sei\u00b7nen", "Gang", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Seit mir die Lieb\u2019 Amalien gegeben,", "tokens": ["Seit", "mir", "die", "Lieb'", "A\u00b7ma\u00b7li\u00b7en", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Besiz\u2019 ich alles, was ich eben sang.", "tokens": ["Be\u00b7si\u00b7z'", "ich", "al\u00b7les", ",", "was", "ich", "e\u00b7ben", "sang", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PIS", "$,", "PWS", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}