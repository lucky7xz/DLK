{"dta.poem.1709": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Umwechslung aller Dinge.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Drey K\u00f6nge schencken mir auff unterschiedne Weise:", "tokens": ["Drey", "K\u00f6n\u00b7ge", "schen\u00b7cken", "mir", "auff", "un\u00b7ter\u00b7schied\u00b7ne", "Wei\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der aus Hesperien giebt etwas Gold zur Speise/", "tokens": ["Der", "aus", "Hes\u00b7pe\u00b7ri\u00b7en", "giebt", "et\u00b7was", "Gold", "zur", "Spei\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "VVFIN", "PIAT", "NN", "APPRART", "NN", "$("], "meter": "-------+-+-+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Aus Britten-Landes Schatz ist Weyhrauch zu erheben/", "tokens": ["Aus", "Brit\u00b7ten\u00b7Lan\u00b7des", "Schatz", "ist", "Wey\u00b7hrauch", "zu", "er\u00b7he\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VAFIN", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mein Sohn aus Gallien l\u00e4st mir die Myrrhen geben.", "tokens": ["Mein", "Sohn", "aus", "Gal\u00b7li\u00b7en", "l\u00e4st", "mir", "die", "Myr\u00b7rhen", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NE", "VVFIN", "PPER", "ART", "