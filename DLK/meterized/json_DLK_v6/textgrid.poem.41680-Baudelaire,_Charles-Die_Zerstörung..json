{"textgrid.poem.41680": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "Die Zerst\u00f6rung.", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ohn Unterla\u00df sp\u00fcr ich, wie mich der D\u00e4mon dr\u00e4ngt;", "tokens": ["Ohn", "Un\u00b7ter\u00b7la\u00df", "sp\u00fcr", "ich", ",", "wie", "mich", "der", "D\u00e4\u00b7mon", "dr\u00e4ngt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie regungslose Luft h\u00e4lt er mich rings umfangen;", "tokens": ["Wie", "re\u00b7gungs\u00b7lo\u00b7se", "Luft", "h\u00e4lt", "er", "mich", "rings", "um\u00b7fan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVFIN", "PPER", "PRF", "ADV", "VVPP", "$."], "meter": "--+--+---+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Ich f\u00fchl und schlucke ihn, wie er die Lungen sengt,", "tokens": ["Ich", "f\u00fchl", "und", "schlu\u00b7cke", "ihn", ",", "wie", "er", "die", "Lun\u00b7gen", "sengt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Er f\u00fcllt mein schuldig Herz mit ewigem Verlangen.", "tokens": ["Er", "f\u00fcllt", "mein", "schul\u00b7dig", "Herz", "mit", "e\u00b7wi\u00b7gem", "Ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJD", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Oft nimmt er, meiner Glut zur Kunst gar wohl bewu\u00dft,", "tokens": ["Oft", "nimmt", "er", ",", "mei\u00b7ner", "Glut", "zur", "Kunst", "gar", "wohl", "be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "APPRART", "NN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die buhlerische Form der sch\u00f6nsten Frau auf Erden,", "tokens": ["Die", "buh\u00b7le\u00b7ri\u00b7sche", "Form", "der", "sch\u00f6ns\u00b7ten", "Frau", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und heuchlerischen Trugs l\u00e4\u00dft meiner Lippen Lust", "tokens": ["Und", "heuch\u00b7le\u00b7ri\u00b7schen", "Trugs", "l\u00e4\u00dft", "mei\u00b7ner", "Lip\u00b7pen", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Er den verruchten Trank verworfner Schande werden.", "tokens": ["Er", "den", "ver\u00b7ruch\u00b7ten", "Trank", "ver\u00b7worf\u00b7ner", "Schan\u00b7de", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So f\u00fchrt er mich, vom Blick der Gottheit fern gebannt,", "tokens": ["So", "f\u00fchrt", "er", "mich", ",", "vom", "Blick", "der", "Got\u00b7theit", "fern", "ge\u00b7bannt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "APPRART", "NN", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schwerkeuchend und ersch\u00f6pft durchs weite W\u00fcstenland", "tokens": ["Schwer\u00b7keu\u00b7chend", "und", "er\u00b7sch\u00f6pft", "durchs", "wei\u00b7te", "W\u00fcs\u00b7ten\u00b7land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "VVPP", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der toten Leere hin, in endlos-grauen Stunden.", "tokens": ["Der", "to\u00b7ten", "Lee\u00b7re", "hin", ",", "in", "end\u00b7los\u00b7grau\u00b7en", "Stun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Vor meinen Augen, die Verwirrung dunkelt, s\u00e4t", "tokens": ["Vor", "mei\u00b7nen", "Au\u00b7gen", ",", "die", "Ver\u00b7wir\u00b7rung", "dun\u00b7kelt", ",", "s\u00e4t"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ART", "NN", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zerfetzte Kleider er und aufgeri\u00dfne Wunden", "tokens": ["Zer\u00b7fetz\u00b7te", "Klei\u00b7der", "er", "und", "auf\u00b7ge\u00b7ri\u00df\u00b7ne", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "PPER", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und des Zerst\u00f6rungswerks bluttriefend Schlachtger\u00e4t!", "tokens": ["Und", "des", "Zer\u00b7st\u00f6\u00b7rungs\u00b7werks", "blut\u00b7trie\u00b7fend", "Schlacht\u00b7ge\u00b7r\u00e4t", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}}}}}