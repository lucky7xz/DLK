{"dta.poem.13187": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z ehndes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So prangen insgemein der F\u00fcrsten Herrlichkeiten,", "tokens": ["So", "pran\u00b7gen", "ins\u00b7ge\u00b7mein", "der", "F\u00fcrs\u00b7ten", "Herr\u00b7lich\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jedoch kein andrer Thron mit so viel Trefflichkeiten", "tokens": ["Je\u00b7doch", "kein", "an\u00b7drer", "Thron", "mit", "so", "viel", "Treff\u00b7lich\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "ADJA", "NN", "APPR", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "45Als hier, wo Gnad und Huld und Weisheit so regiert,", "tokens": ["hier", ",", "wo", "Gnad", "und", "Huld", "und", "Weis\u00b7heit", "so", "re\u00b7giert", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "NN", "KON", "NN", "KON", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Da\u00df mehr der L\u00e4nder Heil als Schmuck die Krone ziert.", "tokens": ["Da\u00df", "mehr", "der", "L\u00e4n\u00b7der", "Heil", "als", "Schmuck", "die", "Kro\u00b7ne", "ziert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "NN", "KOUS", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa