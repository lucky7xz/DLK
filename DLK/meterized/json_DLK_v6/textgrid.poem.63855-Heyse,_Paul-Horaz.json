{"textgrid.poem.63855": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Horaz", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier am Ufer des Sees, der mir von S\u00fcden her", "tokens": ["Hier", "am", "U\u00b7fer", "des", "Sees", ",", "der", "mir", "von", "S\u00fc\u00b7den", "her"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "$,", "PRELS", "PPER", "APPR", "NN", "APZR"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Unterm Hauche des F\u00f6hn hoch an die Br\u00fcstung spritzt,", "tokens": ["Un\u00b7term", "Hau\u00b7che", "des", "F\u00f6hn", "hoch", "an", "die", "Br\u00fcs\u00b7tung", "spritzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Wandl' ich lesend und sinnend,", "tokens": ["Wandl'", "ich", "le\u00b7send", "und", "sin\u00b7nend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Meinen alten Horaz zur Hand;", "tokens": ["Mei\u00b7nen", "al\u00b7ten", "Ho\u00b7raz", "zur", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Noch das n\u00e4mliche Buch, draus in der Prima schon", "tokens": ["Noch", "das", "n\u00e4m\u00b7li\u00b7che", "Buch", ",", "draus", "in", "der", "Pri\u00b7ma", "schon"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "PAV", "APPR", "ART", "NN", "ADV"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Lang versunkener Welt Zauber mich anger\u00fchrt;", "tokens": ["Lang", "ver\u00b7sun\u00b7ke\u00b7ner", "Welt", "Zau\u00b7ber", "mich", "an\u00b7ge\u00b7r\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "NN", "PPER", "VVPP", "$."], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Denn es kehrt zu der ersten", "tokens": ["Denn", "es", "kehrt", "zu", "der", "ers\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "ADJA"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Lieb' uns immer das Herz zur\u00fcck.", "tokens": ["Lieb'", "uns", "im\u00b7mer", "das", "Herz", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Und ich denke, wie fr\u00fch ernst ich beflissen war,", "tokens": ["Und", "ich", "den\u00b7ke", ",", "wie", "fr\u00fch", "ernst", "ich", "be\u00b7flis\u00b7sen", "war", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PWAV", "ADJD", "VVFIN", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem Venusischen Schwan zagenden Fl\u00fcgelschlags", "tokens": ["Dem", "Ve\u00b7nu\u00b7si\u00b7schen", "Schwan", "za\u00b7gen\u00b7den", "Fl\u00fc\u00b7gel\u00b7schlags"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Mich auf rhythmischen Fl\u00fcgen", "tokens": ["Mich", "auf", "rhyth\u00b7mi\u00b7schen", "Fl\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPR", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Nachzuschwingen begeistrungsvoll;", "tokens": ["Nach\u00b7zu\u00b7schwin\u00b7gen", "be\u00b7geis\u00b7trungs\u00b7voll", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Wie ich gro\u00df mich ged\u00fcnkt, wenn im alk\u00e4ischen", "tokens": ["Wie", "ich", "gro\u00df", "mich", "ge\u00b7d\u00fcnkt", ",", "wenn", "im", "al\u00b7k\u00e4\u00b7i\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADJD", "PPER", "VVPP", "$,", "KOUS", "APPRART", "ADJA"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Oder sapphischen Ma\u00df eine der Oden mir", "tokens": ["O\u00b7der", "sap\u00b7phi\u00b7schen", "Ma\u00df", "ei\u00b7ne", "der", "O\u00b7den", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "ART", "ART", "NN", "PPER"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Nachzustammeln gegl\u00fcckt war", "tokens": ["Nach\u00b7zu\u00b7stam\u00b7meln", "ge\u00b7gl\u00fcckt", "war"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVPP", "VAFIN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "In pedantischem Sch\u00fclerdeutsch.", "tokens": ["In", "pe\u00b7dan\u00b7ti\u00b7schem", "Sch\u00fc\u00b7ler\u00b7deutsch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "Damals zog ich zumeist jene Gedichte vor,", "tokens": ["Da\u00b7mals", "zog", "ich", "zu\u00b7meist", "je\u00b7ne", "Ge\u00b7dich\u00b7te", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "PDAT", "NN", "PTKVZ", "$,"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Drin ein z\u00e4rtlicher Hauch wittert der Leidenschaft", "tokens": ["Drin", "ein", "z\u00e4rt\u00b7li\u00b7cher", "Hauch", "wit\u00b7tert", "der", "Lei\u00b7den\u00b7schaft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und des Leides, mit dem die", "tokens": ["Und", "des", "Lei\u00b7des", ",", "mit", "dem", "die"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "APPR", "PRELS", "ART"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Falschheit immer der Treue lohnt.", "tokens": ["Falschheit", "im\u00b7mer", "der", "Treu\u00b7e", "lohnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Pyrrha glaubt' ich zu sehn, von dem beg\u00fcnstigten", "tokens": ["Pyrr\u00b7ha", "glaubt'", "ich", "zu", "sehn", ",", "von", "dem", "be\u00b7g\u00fcns\u00b7tig\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "PTKZU", "VVINF", "$,", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.2": {"text": "Neuen Liebsten umarmt unter der schattigen", "tokens": ["Neu\u00b7en", "Liebs\u00b7ten", "um\u00b7armt", "un\u00b7ter", "der", "schat\u00b7ti\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Rosenlaube, dieweil sich", "tokens": ["Ro\u00b7sen\u00b7lau\u00b7be", ",", "die\u00b7weil", "sich"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "KOUS", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Stolz der Dichter zur\u00fcckezog;", "tokens": ["Stolz", "der", "Dich\u00b7ter", "zu\u00b7r\u00fc\u00b7ck\u00b7e\u00b7zog", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Und in eigener Brust f\u00fchlt' ich der Eifersucht", "tokens": ["Und", "in", "ei\u00b7ge\u00b7ner", "Brust", "f\u00fchlt'", "ich", "der", "Ei\u00b7fer\u00b7sucht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Brand, wenn Lydia frech r\u00fchmte des Telephus", "tokens": ["Brand", ",", "wenn", "Ly\u00b7dia", "frech", "r\u00fchm\u00b7te", "des", "Te\u00b7le\u00b7phus"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "NE", "ADJD", "VVFIN", "ART", "NN"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Ros'gen Nacken, die sch\u00f6nen", "tokens": ["Ros'\u00b7gen", "Na\u00b7cken", ",", "die", "sch\u00f6\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ART", "ADJA"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Arme, wei\u00df wie aus Wachs geformt.", "tokens": ["Ar\u00b7me", ",", "wei\u00df", "wie", "aus", "Wachs", "ge\u00b7formt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "KOKOM", "APPR", "NN", "VVPP", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.8": {"line.1": {"text": "Gute Jugend! Es liest jeder ein andres sich", "tokens": ["Gu\u00b7te", "Ju\u00b7gend", "!", "Es", "liest", "je\u00b7der", "ein", "and\u00b7res", "sich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "PPER", "VVFIN", "PIS", "ART", "PIS", "PRF"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Aus den Versen heraus eines Poeten, dem", "tokens": ["Aus", "den", "Ver\u00b7sen", "he\u00b7raus", "ei\u00b7nes", "Po\u00b7et\u00b7en", ",", "dem"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "APZR", "ART", "NN", "$,", "PRELS"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Als dem Liebling der G\u00f6tter", "tokens": ["Als", "dem", "Lieb\u00b7ling", "der", "G\u00f6t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Wenig Menschliches ferne blieb.", "tokens": ["We\u00b7nig", "Menschli\u00b7ches", "fer\u00b7ne", "blieb", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Jetzt, da l\u00e4ngst ich wie du hing an der Tempelwand", "tokens": ["Jetzt", ",", "da", "l\u00e4ngst", "ich", "wie", "du", "hing", "an", "der", "Tem\u00b7pel\u00b7wand"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "VVFIN", "PPER", "PWAV", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Auf mein triefendes Kleid, als ein Votivgeschenk", "tokens": ["Auf", "mein", "trie\u00b7fen\u00b7des", "Kleid", ",", "als", "ein", "Vo\u00b7tiv\u00b7ge\u00b7schenk"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "KOUS", "ART", "NN"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Jenem Gott, der im Hafen", "tokens": ["Je\u00b7nem", "Gott", ",", "der", "im", "Ha\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDAT", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Wohlbehalten mich landen lie\u00df,", "tokens": ["Wohl\u00b7be\u00b7hal\u00b7ten", "mich", "lan\u00b7den", "lie\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVINF", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.10": {"line.1": {"text": "Lausch' ich, alter Horaz, lieber verst\u00e4ndnisvoll", "tokens": ["Lausch'", "ich", ",", "al\u00b7ter", "Ho\u00b7raz", ",", "lie\u00b7ber", "ver\u00b7st\u00e4nd\u00b7nis\u00b7voll"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "PPER", "$,", "ADJA", "NN", "$,", "ADV", "ADJD"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Deinem klugen Gespr\u00e4ch, wenn du vom Weltenlauf", "tokens": ["Dei\u00b7nem", "klu\u00b7gen", "Ge\u00b7spr\u00e4ch", ",", "wenn", "du", "vom", "Wel\u00b7ten\u00b7lauf"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "KOUS", "PPER", "APPRART", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Mit gelassner Entsagung", "tokens": ["Mit", "ge\u00b7lass\u00b7ner", "Ent\u00b7sa\u00b7gung"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Sprichst und r\u00fchmst die Gen\u00fcgsamkeit.", "tokens": ["Sprichst", "und", "r\u00fchmst", "die", "Ge\u00b7n\u00fcg\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.11": {"line.1": {"text": "Dein Kollege Katull dr\u00fcben auf Sirmio,", "tokens": ["Dein", "Kol\u00b7le\u00b7ge", "Ka\u00b7tull", "dr\u00fc\u00b7ben", "auf", "Sir\u00b7mio", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "ADV", "APPR", "NE", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ruhlos bis an den Tod brannte das Herz in ihm.", "tokens": ["Ruh\u00b7los", "bis", "an", "den", "Tod", "brann\u00b7te", "das", "Herz", "in", "ihm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "APPR", "PPER", "$."], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Du taugst besser zum weisen", "tokens": ["Du", "taugst", "bes\u00b7ser", "zum", "wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hausfreund einem Gealterten.", "tokens": ["Haus\u00b7freund", "ei\u00b7nem", "Ge\u00b7al\u00b7ter\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}}}}}