{"textgrid.poem.45010": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[anna Kurzrock ins Stammbuch]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kam im frostgen November", "tokens": ["Ich", "kam", "im", "frost\u00b7gen", "No\u00b7vem\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Nach Gr\u00e4z, der traulichen Stadt,", "tokens": ["Nach", "Gr\u00e4z", ",", "der", "trau\u00b7li\u00b7chen", "Stadt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Das Jahr war alt und m\u00fcde,", "tokens": ["Das", "Jahr", "war", "alt", "und", "m\u00fc\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und ich war alt und matt,", "tokens": ["Und", "ich", "war", "alt", "und", "matt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch braucht' es nur zwei Blicke,", "tokens": ["Doch", "braucht'", "es", "nur", "zwei", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Neigung, der Sonne Blick,", "tokens": ["Der", "Nei\u00b7gung", ",", "der", "Son\u00b7ne", "Blick", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Und mir und dem alten November", "tokens": ["Und", "mir", "und", "dem", "al\u00b7ten", "No\u00b7vem\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "KON", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Kam Leben und W\u00e4rme zur\u00fcck.", "tokens": ["Kam", "Le\u00b7ben", "und", "W\u00e4r\u00b7me", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}