{"textgrid.poem.42998": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gleite ins Weite und in die H\u00f6h!", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gleite ins Weite und in die H\u00f6h!", "tokens": ["Glei\u00b7te", "ins", "Wei\u00b7te", "und", "in", "die", "H\u00f6h", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "KON", "APPR", "ART", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Adieu, du zartes Bleu", "tokens": ["A\u00b7die\u00b7u", ",", "du", "zar\u00b7tes", "Bleu"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "PPER", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meines Zigarettenrauches,", "tokens": ["Mei\u00b7nes", "Zi\u00b7ga\u00b7ret\u00b7ten\u00b7rau\u00b7ches", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der du so sanft entfliehst.", "tokens": ["Der", "du", "so", "sanft", "ent\u00b7fliehst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wenn du ein zierliches Nasenloch siehst,", "tokens": ["Wenn", "du", "ein", "zier\u00b7li\u00b7ches", "Na\u00b7sen\u00b7loch", "siehst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "K\u00fc\u00df dem die Haare als Gru\u00df meines Hauches.", "tokens": ["K\u00fc\u00df", "dem", "die", "Haa\u00b7re", "als", "Gru\u00df", "mei\u00b7nes", "Hau\u00b7ches", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ART", "NN", "KOUS", "NN", "PPOSAT", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "Ob dich ein H\u00f6hendruck", "tokens": ["Ob", "dich", "ein", "H\u00f6\u00b7hen\u00b7druck"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Zur Erde zur\u00fcckschl\u00e4gt,", "tokens": ["Zur", "Er\u00b7de", "zu\u00b7r\u00fcck\u00b7schl\u00e4gt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Eine Str\u00f6mung, eines Windes Ruck", "tokens": ["Ei\u00b7ne", "Str\u00f6\u00b7mung", ",", "ei\u00b7nes", "Win\u00b7des", "Ruck"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Dich zu Himmelsgl\u00fcck tr\u00e4gt, \u2013", "tokens": ["Dich", "zu", "Him\u00b7mels\u00b7gl\u00fcck", "tr\u00e4gt", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "APPR", "NN", "VVFIN", "$,", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Finde das, was du erwartetest.", "tokens": ["Fin\u00b7de", "das", ",", "was", "du", "er\u00b7war\u00b7te\u00b7test", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "In dem hold gew\u00fcrzten Augenblick,", "tokens": ["In", "dem", "hold", "ge\u00b7w\u00fcrz\u00b7ten", "Au\u00b7gen\u00b7blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Da du aus mir startetest,", "tokens": ["Da", "du", "aus", "mir", "star\u00b7te\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Spielte Ziehharmonikamusik", "tokens": ["Spiel\u00b7te", "Zieh\u00b7har\u00b7mo\u00b7ni\u00b7ka\u00b7mu\u00b7sik"], "token_info": ["word", "word"], "pos": ["VVFIN", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Ein Lieblingslied von mir: La Paloma,", "tokens": ["Ein", "Lieb\u00b7lings\u00b7lied", "von", "mir", ":", "La", "Pa\u00b7lo\u00b7ma", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "$.", "NE", "NE", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und auf Schwingen dieser Volksweise", "tokens": ["Und", "auf", "Schwin\u00b7gen", "die\u00b7ser", "Volks\u00b7wei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "PDAT", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Steigst du auf. Gl\u00fcckliche Reise!", "tokens": ["Steigst", "du", "auf", ".", "Gl\u00fcck\u00b7li\u00b7che", "Rei\u00b7se", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$.", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "Aus Nikotin ins ewige Aroma.", "tokens": ["Aus", "Ni\u00b7ko\u00b7tin", "ins", "e\u00b7wi\u00b7ge", "A\u00b7ro\u00b7ma", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPRART", "ADJA", "NN", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}}}}}