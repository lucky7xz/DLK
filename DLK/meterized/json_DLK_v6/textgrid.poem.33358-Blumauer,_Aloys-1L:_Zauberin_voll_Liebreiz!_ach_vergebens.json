{"textgrid.poem.33358": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zauberin voll Liebreiz! ach vergebens", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zauberin voll Liebreiz! ach vergebens", "tokens": ["Zau\u00b7be\u00b7rin", "voll", "Lieb\u00b7reiz", "!", "ach", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ADJD", "NN", "$.", "XY", "XY"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Fesseltest du meinen Sohn und mich;", "tokens": ["Fes\u00b7sel\u00b7test", "du", "mei\u00b7nen", "Sohn", "und", "mich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "KON", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ich bin schon am Abhang meines Lebens,", "tokens": ["Ich", "bin", "schon", "am", "Ab\u00b7hang", "mei\u00b7nes", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und mein Sohn ist noch zu jung f\u00fcr dich.", "tokens": ["Und", "mein", "Sohn", "ist", "noch", "zu", "jung", "f\u00fcr", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "APPR", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Wider uns hat sich die Zeit verschworen,", "tokens": ["Wi\u00b7der", "uns", "hat", "sich", "die", "Zeit", "ver\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Mir und meinem Sohn entzog sie dich:", "tokens": ["Mir", "und", "mei\u00b7nem", "Sohn", "ent\u00b7zog", "sie", "dich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "PPOSAT", "NN", "VVFIN", "PPER", "PRF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Viel zu fr\u00fch ward'st du f\u00fcr ihn geboren", "tokens": ["Viel", "zu", "fr\u00fch", "ward'st", "du", "f\u00fcr", "ihn", "ge\u00b7bo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKA", "ADJD", "VAFIN", "PPER", "APPR", "PPER", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ach! und leider viel zu sp\u00e4t f\u00fcr mich.", "tokens": ["Ach", "!", "und", "lei\u00b7der", "viel", "zu", "sp\u00e4t", "f\u00fcr", "mich", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KON", "ADV", "ADV", "PTKA", "ADJD", "APPR", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Die Natur scheint selbst zu widerstreben,", "tokens": ["Die", "Na\u00b7tur", "scheint", "selbst", "zu", "wi\u00b7der\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sie vereitelt sein und mein Bem\u00fch'n:", "tokens": ["Sie", "ver\u00b7ei\u00b7telt", "sein", "und", "mein", "Be\u00b7m\u00fch'n", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "VAINF", "KON", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Was sie nun kaum anf\u00e4ngt, ihm zu geben,", "tokens": ["Was", "sie", "nun", "kaum", "an\u00b7f\u00e4ngt", ",", "ihm", "zu", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "VVFIN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Will die Karge mir jetzt schon entzieh'n.", "tokens": ["Will", "die", "Kar\u00b7ge", "mir", "jetzt", "schon", "ent\u00b7zieh'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["VMFIN", "ART", "NN", "PPER", "ADV", "ADV", "VVFIN", "NE"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "K\u00f6nnt ich ihm so viele Jahre geben,", "tokens": ["K\u00f6nnt", "ich", "ihm", "so", "vie\u00b7le", "Jah\u00b7re", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Als er braucht zur Gunst, nach der er strebt,", "tokens": ["Als", "er", "braucht", "zur", "Gunst", ",", "nach", "der", "er", "strebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPRART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "O, so d\u00fcrft' er sie nicht erst erleben,", "tokens": ["O", ",", "so", "d\u00fcrft'", "er", "sie", "nicht", "erst", "er\u00b7le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VMFIN", "PPER", "PPER", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und ich h\u00e4tte sie nicht \u00fcberlebt.", "tokens": ["Und", "ich", "h\u00e4t\u00b7te", "sie", "nicht", "\u00fc\u00b7ber\u00b7lebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "W\u00fcrde, so durch ein allm\u00e4chtig' Wesen", "tokens": ["W\u00fcr\u00b7de", ",", "so", "durch", "ein", "all\u00b7m\u00e4ch\u00b7tig'", "We\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "$,", "ADV", "APPR", "ART", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Gleich getheilet beider Lebensfrist,", "tokens": ["Gleich", "ge\u00b7thei\u00b7let", "bei\u00b7der", "Le\u00b7bens\u00b7frist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Sieh, ich w\u00fcrde, was ich einst gewesen,", "tokens": ["Sieh", ",", "ich", "w\u00fcr\u00b7de", ",", "was", "ich", "einst", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VAFIN", "$,", "PWS", "PPER", "ADV", "VAPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und er w\u00e4re, was er noch nicht ist.", "tokens": ["Und", "er", "w\u00e4\u00b7re", ",", "was", "er", "noch", "nicht", "ist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "$,", "PWS", "PPER", "ADV", "PTKNEG", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Beide w\u00fcrden wir dann deinen K\u00fcssen", "tokens": ["Bei\u00b7de", "w\u00fcr\u00b7den", "wir", "dann", "dei\u00b7nen", "K\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "PPER", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Voller Zuversicht entgegen geh'n,", "tokens": ["Vol\u00b7ler", "Zu\u00b7ver\u00b7sicht", "ent\u00b7ge\u00b7gen", "geh'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und du w\u00fcrdest nun zu deinen F\u00fcssen", "tokens": ["Und", "du", "w\u00fcr\u00b7dest", "nun", "zu", "dei\u00b7nen", "F\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Zwei gleich br\u00fcnstige Verehrer seh'n.", "tokens": ["Zwei", "gleich", "br\u00fcns\u00b7ti\u00b7ge", "Ver\u00b7eh\u00b7rer", "seh'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["CARD", "ADV", "ADJA", "NN", "VVFIN", "NE"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Doch was w\u00fcnsch' ich? \u2013 Ach, auch dann entzweiten", "tokens": ["Doch", "was", "w\u00fcn\u00b7sch'", "ich", "?", "\u2013", "Ach", ",", "auch", "dann", "ent\u00b7zwei\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWS", "VVFIN", "PPER", "$.", "$(", "ITJ", "$,", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Eifers\u00fcchtig Sohn und Vater sich,", "tokens": ["Ei\u00b7fer\u00b7s\u00fcch\u00b7tig", "Sohn", "und", "Va\u00b7ter", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "PRF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und, best\u00fcrmt von zwei verschied'nen Seiten,", "tokens": ["Und", ",", "be\u00b7st\u00fcrmt", "von", "zwei", "ver\u00b7schie\u00b7d'\u00b7nen", "Sei\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVPP", "APPR", "CARD", "ADJA", "NN", "$,"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "W\u00e4hltest du auch dann vielleicht nicht mich!", "tokens": ["W\u00e4hl\u00b7test", "du", "auch", "dann", "viel\u00b7leicht", "nicht", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADV", "PTKNEG", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Also mag mein Sohn allein dich lieben,", "tokens": ["Al\u00b7so", "mag", "mein", "Sohn", "al\u00b7lein", "dich", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "ADV", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mag noch werden, was ich nicht mehr bin,", "tokens": ["Mag", "noch", "wer\u00b7den", ",", "was", "ich", "nicht", "mehr", "bin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "VAINF", "$,", "PWS", "PPER", "PTKNEG", "ADV", "VAFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Amor gebe Fl\u00fcgel seinen Trieben,", "tokens": ["A\u00b7mor", "ge\u00b7be", "Fl\u00fc\u00b7gel", "sei\u00b7nen", "Trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und du, Theure, harre nur auf ihn.", "tokens": ["Und", "du", ",", "Theu\u00b7re", ",", "har\u00b7re", "nur", "auf", "ihn", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ADJA", "$,", "VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Aber wird dein Herz sich auch entschliessen", "tokens": ["A\u00b7ber", "wird", "dein", "Herz", "sich", "auch", "ent\u00b7schlies\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "PRF", "ADV", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sein zu harren bis er m\u00fcndig ist;", "tokens": ["Sein", "zu", "har\u00b7ren", "bis", "er", "m\u00fcn\u00b7dig", "ist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "PTKZU", "VVINF", "KON", "PPER", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wird es nicht ein Pl\u00e4tzchen haben m\u00fcssen,", "tokens": ["Wird", "es", "nicht", "ein", "Pl\u00e4tz\u00b7chen", "ha\u00b7ben", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ART", "NN", "VAINF", "VMFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wo es sicher aufgehoben ist?", "tokens": ["Wo", "es", "si\u00b7cher", "auf\u00b7ge\u00b7ho\u00b7ben", "ist", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Ja, und wem es in Verwahrung geben", "tokens": ["Ja", ",", "und", "wem", "es", "in", "Ver\u00b7wah\u00b7rung", "ge\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "KON", "PWS", "PPER", "APPR", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dieses Herzchen, das so z\u00e4rtlich liebt?", "tokens": ["Die\u00b7ses", "Herz\u00b7chen", ",", "das", "so", "z\u00e4rt\u00b7lich", "liebt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Jeder, dem du's gibst, l\u00e4\u00dft eh' sein Leben,", "tokens": ["Je\u00b7der", ",", "dem", "du's", "gibst", ",", "l\u00e4\u00dft", "eh'", "sein", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "PIS", "VVFIN", "$,", "VVFIN", "KOUS", "PPOSAT", "NN", "$,"], "meter": "+-+-++--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Eh' er dir den Schatz zur\u00fccke gibt.", "tokens": ["Eh'", "er", "dir", "den", "Schatz", "zu\u00b7r\u00fc\u00b7cke", "gibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ART", "NN", "PTKVZ", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.11": {"line.1": {"text": "Gieb es mir; ich will es treu bewachen,", "tokens": ["Gieb", "es", "mir", ";", "ich", "will", "es", "treu", "be\u00b7wa\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "$.", "PPER", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und so kann es immer unverf\u00fchrt", "tokens": ["Und", "so", "kann", "es", "im\u00b7mer", "un\u00b7ver\u00b7f\u00fchrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ADV", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "An dem Vater erst die Probe machen;", "tokens": ["An", "dem", "Va\u00b7ter", "erst", "die", "Pro\u00b7be", "ma\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wie es seinen Sohn einst lieben wird.", "tokens": ["Wie", "es", "sei\u00b7nen", "Sohn", "einst", "lie\u00b7ben", "wird", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "ADV", "VVINF", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}