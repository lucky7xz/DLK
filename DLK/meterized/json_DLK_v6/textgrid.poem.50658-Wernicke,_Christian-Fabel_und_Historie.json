{"textgrid.poem.50658": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Fabel und Historie", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sucht nach der ", "tokens": ["Sucht", "nach", "der"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Und nach den ", "tokens": ["Und", "nach", "den"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Da\u00df die Gedicht' euch n\u00fctzlich sein", "tokens": ["Da\u00df", "die", "Ge\u00b7dicht'", "euch", "n\u00fctz\u00b7lich", "sein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPER", "ADJD", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und die Geschicht' euch nicht betr\u00fcge;", "tokens": ["Und", "die", "Ge\u00b7schicht'", "euch", "nicht", "be\u00b7tr\u00fc\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Denn jene zeigen uns die ", "tokens": ["Denn", "je\u00b7ne", "zei\u00b7gen", "uns", "die"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VVFIN", "PPER", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}}}}}