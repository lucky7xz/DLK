{"textgrid.poem.56138": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "23.", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O erst ", "tokens": ["O", "erst"], "token_info": ["word", "word"], "pos": ["NE", "ADV"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "nicht mehr um seinetwillen", "tokens": ["nicht", "mehr", "um", "sei\u00b7net\u00b7wil\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "APPR", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "wird in die Himmelstillen", "tokens": ["wird", "in", "die", "Him\u00b7mel\u00b7stil\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "steigen, sich selber genug,", "tokens": ["stei\u00b7gen", ",", "sich", "sel\u00b7ber", "ge\u00b7nug", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRF", "ADV", "ADV", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "um in lichten Profilen,", "tokens": ["um", "in", "lich\u00b7ten", "Pro\u00b7fi\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "als das Ger\u00e4t, das gelang,", "tokens": ["als", "das", "Ge\u00b7r\u00e4t", ",", "das", "ge\u00b7lang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PDS", "VVFIN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Liebling der Winde zu spielen,", "tokens": ["Lieb\u00b7ling", "der", "Win\u00b7de", "zu", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "sicher, schwenkend und schlank, \u2013", "tokens": ["si\u00b7cher", ",", "schwen\u00b7kend", "und", "schlank", ",", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "$,", "ADJD", "KON", "VVFIN", "$,", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "erst, wenn ein reines Wohin", "tokens": ["erst", ",", "wenn", "ein", "rei\u00b7nes", "Wo\u00b7hin"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "wachsender Apparate", "tokens": ["wach\u00b7sen\u00b7der", "Ap\u00b7pa\u00b7ra\u00b7te"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+----", "measure": "unknown.measure.di"}, "line.3": {"text": "Knabenstolz \u00fcberwiegt,", "tokens": ["Kna\u00b7ben\u00b7stolz", "\u00fc\u00b7berw\u00b7iegt", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "wird, \u00fcberst\u00fcrzt von Gewinn,", "tokens": ["wird", ",", "\u00fc\u00b7bers\u00b7t\u00fcrzt", "von", "Ge\u00b7winn", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "jener den Fernen Genahte", "tokens": ["je\u00b7ner", "den", "Fer\u00b7nen", "Ge\u00b7nah\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "ART", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}}}}}