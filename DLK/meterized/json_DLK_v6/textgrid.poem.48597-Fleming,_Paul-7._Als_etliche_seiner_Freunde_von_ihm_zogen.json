{"textgrid.poem.48597": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "7. Als etliche seiner Freunde von ihm zogen", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du, \u00f6der Hain,", "tokens": ["Du", ",", "\u00f6\u00b7der", "Hain", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "du, wilde W\u00fcstenei,", "tokens": ["du", ",", "wil\u00b7de", "W\u00fcs\u00b7te\u00b7nei", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "sagt her, kans sein,", "tokens": ["sagt", "her", ",", "kans", "sein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "VMFIN", "VAINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "sagt her: wo sind sie drei?", "tokens": ["sagt", "her", ":", "wo", "sind", "sie", "drei", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$.", "PWAV", "VAFIN", "PPER", "CARD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und, Zephyr, du,", "tokens": ["Und", ",", "Ze\u00b7phyr", ",", "du", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "$,", "NN", "$,", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "wie denn ihr Andern auch,", "tokens": ["wie", "denn", "ihr", "An\u00b7dern", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "ADJA", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "weht mir noch einen Hauch", "tokens": ["weht", "mir", "noch", "ei\u00b7nen", "Hauch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.8": {"text": "von ihnen zu!", "tokens": ["von", "ih\u00b7nen", "zu", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "So kehrt ihr doch,", "tokens": ["So", "kehrt", "ihr", "doch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Geliebte, zu mir ein", "tokens": ["Ge\u00b7lieb\u00b7te", ",", "zu", "mir", "ein"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "PPER", "ART"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "und wollet noch", "tokens": ["und", "wol\u00b7let", "noch"], "token_info": ["word", "word", "word"], "pos": ["KON", "VMFIN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "von mir umfasset sein?", "tokens": ["von", "mir", "um\u00b7fas\u00b7set", "sein", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wo aber hin,", "tokens": ["Wo", "a\u00b7ber", "hin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "wo nau\u00df fliehn sie mit Flei\u00df?", "tokens": ["wo", "nau\u00df", "fliehn", "sie", "mit", "Flei\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich wei\u00df nicht, was ich wei\u00df,", "tokens": ["Ich", "wei\u00df", "nicht", ",", "was", "ich", "wei\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "wie ich dran bin.", "tokens": ["wie", "ich", "dran", "bin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PAV", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Ich h\u00f6rt', ich sah',", "tokens": ["Ich", "h\u00f6rt'", ",", "ich", "sah'", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "ich griffe sie ja hier.", "tokens": ["ich", "grif\u00b7fe", "sie", "ja", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr seid ja da,", "tokens": ["Ihr", "seid", "ja", "da", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "ihr, meine Liebsten ihr?", "tokens": ["ihr", ",", "mei\u00b7ne", "Liebs\u00b7ten", "ihr", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie ist mir denn?", "tokens": ["Wie", "ist", "mir", "denn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Sie sinds und sinds auch nicht.", "tokens": ["Sie", "sinds", "und", "sinds", "auch", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KON", "VAFIN", "ADV", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich wei\u00df nicht, was geschicht,", "tokens": ["Ich", "wei\u00df", "nicht", ",", "was", "ge\u00b7schicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PRELS", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "wie oder wenn.", "tokens": ["wie", "o\u00b7der", "wenn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "KON", "KOUS", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Steht doch! Ach! nein,", "tokens": ["Steht", "doch", "!", "Ach", "!", "nein", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADV", "$.", "ITJ", "$.", "PTKANT", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "sie sind es leibhaft nicht.", "tokens": ["sie", "sind", "es", "leib\u00b7haft", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es ist ihr Schein,", "tokens": ["Es", "ist", "ihr", "Schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "der mir die Augen bricht.", "tokens": ["der", "mir", "die", "Au\u00b7gen", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ach ich, nur ich", "tokens": ["Ach", "ich", ",", "nur", "ich"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ITJ", "PPER", "$,", "ADV", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "wei\u00df, da\u00df ich gegend bin;", "tokens": ["wei\u00df", ",", "da\u00df", "ich", "ge\u00b7gend", "bin", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "doch zweifelt noch mein Sin", "tokens": ["doch", "zwei\u00b7felt", "noch", "mein", "Sin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "an mir bei sich.", "tokens": ["an", "mir", "bei", "sich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "PRF", "$."], "meter": "-+--", "measure": "dactylic.init"}}, "stanza.5": {"line.1": {"text": "Ach! ach! ach! ach!", "tokens": ["Ach", "!", "ach", "!", "ach", "!", "ach", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ITJ", "$.", "XY", "$.", "XY", "$.", "XY", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Wem ist mehr weh' als mir?", "tokens": ["Wem", "ist", "mehr", "weh'", "als", "mir", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "VVFIN", "KOUS", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer mi\u00dft die Bach,", "tokens": ["Wer", "mi\u00dft", "die", "Bach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "die ich verweine hier?", "tokens": ["die", "ich", "ver\u00b7wei\u00b7ne", "hier", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRELS", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wer z\u00e4hlt die Luft,", "tokens": ["Wer", "z\u00e4hlt", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "die durch die Lippen dringt?", "tokens": ["die", "durch", "die", "Lip\u00b7pen", "dringt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mein wundes Herze springt,", "tokens": ["Mein", "wun\u00b7des", "Her\u00b7ze", "springt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "es klopft, es pufft.", "tokens": ["es", "klopft", ",", "es", "pufft", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Sie, sie sind dort,", "tokens": ["Sie", ",", "sie", "sind", "dort", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "VAFIN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "ohn' die ich nicht bin ich.", "tokens": ["ohn'", "die", "ich", "nicht", "bin", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PTKNEG", "VAFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Macht ihr euch fort,", "tokens": ["Macht", "ihr", "euch", "fort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "ihr Seufzer, nach f\u00fcr mich,", "tokens": ["ihr", "Seuf\u00b7zer", ",", "nach", "f\u00fcr", "mich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "da\u00df, wenn sie sehn,", "tokens": ["da\u00df", ",", "wenn", "sie", "sehn", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PPER", "VVINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "da\u00df sich die ", "tokens": ["da\u00df", "sich", "die"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PRF", "ART"], "meter": "+--", "measure": "dactylic.init"}, "line.7": {"text": "schw\u00f6llt' auf, es m\u00fcss' annoch", "tokens": ["schw\u00f6llt'", "auf", ",", "es", "m\u00fcss'", "an\u00b7noch"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "$,", "PPER", "VMFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "durch euch geschehn!", "tokens": ["durch", "euch", "ge\u00b7schehn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}