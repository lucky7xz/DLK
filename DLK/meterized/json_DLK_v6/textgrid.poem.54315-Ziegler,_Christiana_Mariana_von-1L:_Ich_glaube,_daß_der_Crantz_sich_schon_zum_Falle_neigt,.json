{"textgrid.poem.54315": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich glaube, da\u00df der Crantz sich schon zum Falle neigt,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich glaube, da\u00df der Crantz sich schon zum Falle neigt,", "tokens": ["Ich", "glau\u00b7be", ",", "da\u00df", "der", "Crantz", "sich", "schon", "zum", "Fal\u00b7le", "neigt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "PRF", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und da sie, liebste Braut, nun aus dem Bette steigt,", "tokens": ["Und", "da", "sie", ",", "liebs\u00b7te", "Braut", ",", "nun", "aus", "dem", "Bet\u00b7te", "steigt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "$,", "ADJA", "NN", "$,", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So wird sie doch der Klang der Klapper nicht erschrecken,", "tokens": ["So", "wird", "sie", "doch", "der", "Klang", "der", "Klap\u00b7per", "nicht", "er\u00b7schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wormit ich sie von Schlaff gesuchet aufzuwecken.", "tokens": ["Wor\u00b7mit", "ich", "sie", "von", "Schlaff", "ge\u00b7su\u00b7chet", "auf\u00b7zu\u00b7we\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "APPR", "NN", "VVPP", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dergleichen Hau\u00dfrath wird ihr billig \u00fcberbracht,", "tokens": ["Derg\u00b7lei\u00b7chen", "Hau\u00df\u00b7rath", "wird", "ihr", "bil\u00b7lig", "\u00fc\u00b7berb\u00b7racht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "NN", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wann k\u00fcnfftig im August ein kleiner Engel lacht,", "tokens": ["Wann", "k\u00fcnff\u00b7tig", "im", "Au\u00b7gust", "ein", "klei\u00b7ner", "En\u00b7gel", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Und der Herr Br\u00e4utigam h\u00f6rt auch nun auf zu klagen,", "tokens": ["Und", "der", "Herr", "Br\u00e4u\u00b7ti\u00b7gam", "h\u00f6rt", "auch", "nun", "auf", "zu", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NE", "VVFIN", "ADV", "ADV", "APPR", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Das Hertze klopffte sonst, und tausend andre Plagen", "tokens": ["Das", "Hert\u00b7ze", "klopff\u00b7te", "sonst", ",", "und", "tau\u00b7send", "and\u00b7re", "Pla\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "VVFIN", "ADV", "$,", "KON", "CARD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Beschwerten seinen Leib, wie sa\u00df er nicht betr\u00fcbt;", "tokens": ["Be\u00b7schwer\u00b7ten", "sei\u00b7nen", "Leib", ",", "wie", "sa\u00df", "er", "nicht", "be\u00b7tr\u00fcbt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "PWAV", "VVFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Nun aber, da er das erh\u00e4lt, was ihm geliebt,", "tokens": ["Nun", "a\u00b7ber", ",", "da", "er", "das", "er\u00b7h\u00e4lt", ",", "was", "ihm", "ge\u00b7liebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "PDS", "VVFIN", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So wei\u00df er gar nichts mehr von Kranckheit, Pein und Schmertzen,", "tokens": ["So", "wei\u00df", "er", "gar", "nichts", "mehr", "von", "Kran\u00b7ck\u00b7heit", ",", "Pein", "und", "Schmert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PIS", "ADV", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+---+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Man sieht ihn mit der Braut gesund und freudig schertzen.", "tokens": ["Man", "sieht", "ihn", "mit", "der", "Braut", "ge\u00b7sund", "und", "freu\u00b7dig", "schert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "ART", "NN", "ADJD", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die nun vor seinen Leib der beste Medicus,", "tokens": ["Die", "nun", "vor", "sei\u00b7nen", "Leib", "der", "bes\u00b7te", "Me\u00b7di\u00b7cus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wie wei\u00dflich hat er nicht gewehlet, heissen mu\u00df.", "tokens": ["Wie", "wei\u00df\u00b7lich", "hat", "er", "nicht", "ge\u00b7weh\u00b7let", ",", "heis\u00b7sen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und wann sie t\u00e4glich gleich vors Bette kommt gegangen,", "tokens": ["Und", "wann", "sie", "t\u00e4g\u00b7lich", "gleich", "vors", "Bet\u00b7te", "kommt", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADJD", "ADV", "APPRART", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Deswegen doch von ihm kein Jahr-Geld wird verlangen", "tokens": ["Des\u00b7we\u00b7gen", "doch", "von", "ihm", "kein", "Jahr\u00b7Geld", "wird", "ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "APPR", "PPER", "PIAT", "NN", "VAFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Der Himmel, welcher euch das Band der Ehe weyht,", "tokens": ["Der", "Him\u00b7mel", ",", "wel\u00b7cher", "euch", "das", "Band", "der", "E\u00b7he", "weyht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "G\u00f6nn euch bey Bett und Tisch nichts als Vergn\u00fcglichkeit!", "tokens": ["G\u00f6nn", "euch", "bey", "Bett", "und", "Tisch", "nichts", "als", "Ver\u00b7gn\u00fcg\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "NN", "KON", "NN", "PIS", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So wird, wenn j\u00e4hrlich was soll in der Wiegen liegen,", "tokens": ["So", "wird", ",", "wenn", "j\u00e4hr\u00b7lich", "was", "soll", "in", "der", "Wie\u00b7gen", "lie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "KOUS", "ADJD", "PWS", "VMFIN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die Kinder-Mutter einst von euch viel Sportuln kriegen.", "tokens": ["Die", "Kin\u00b7der\u00b7Mut\u00b7ter", "einst", "von", "euch", "viel", "Spor\u00b7tuln", "krie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}