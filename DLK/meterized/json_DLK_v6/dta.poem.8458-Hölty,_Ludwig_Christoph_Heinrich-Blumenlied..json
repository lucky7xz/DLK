{"dta.poem.8458": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Blumenlied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn, Paradiesesblumen gleich,", "tokens": ["Wenn", ",", "Pa\u00b7ra\u00b7die\u00b7ses\u00b7blu\u00b7men", "gleich", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["KOUS", "$,", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus Klee die Blumen dringen;", "tokens": ["Aus", "Klee", "die", "Blu\u00b7men", "drin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wenn die V\u00f6gel silberhell", "tokens": ["Und", "wenn", "die", "V\u00f6\u00b7gel", "sil\u00b7ber\u00b7hell"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Garten hier, und dort am Quell,", "tokens": ["Im", "Gar\u00b7ten", "hier", ",", "und", "dort", "am", "Quell", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$,", "KON", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auf Bl\u00fctenb\u00e4umen singen.", "tokens": ["Auf", "Bl\u00fc\u00b7ten\u00b7b\u00e4u\u00b7men", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Doch holder bl\u00fcht ein edles Weib,", "tokens": ["Doch", "hol\u00b7der", "bl\u00fcht", "ein", "ed\u00b7les", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Seele gut, und sch\u00f6n von Leib,", "tokens": ["Von", "See\u00b7le", "gut", ",", "und", "sch\u00f6n", "von", "Leib", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$,", "KON", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In frischer Jugendbl\u00fcte.", "tokens": ["In", "fri\u00b7scher", "Ju\u00b7gend\u00b7bl\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wir lassen alle Blumen stehn,", "tokens": ["Wir", "las\u00b7sen", "al\u00b7le", "Blu\u00b7men", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das liebe Weibchen anzusehn,", "tokens": ["Das", "lie\u00b7be", "Weib\u00b7chen", "an\u00b7zu\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und freun uns ihrer G\u00fcte.", "tokens": ["Und", "freun", "uns", "ih\u00b7rer", "G\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}