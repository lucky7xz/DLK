{"textgrid.poem.67621": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "42.", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbda lacht die wilde Blume,", "tokens": ["\u00bb", "da", "lacht", "die", "wil\u00b7de", "Blu\u00b7me", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und was ich mir zum Ruhme", "tokens": ["Und", "was", "ich", "mir", "zum", "Ruh\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PPER", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hineinges\u00e4t,", "tokens": ["Hin\u00b7ein\u00b7ge\u00b7s\u00e4t", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Vergeht.\u00ab", "tokens": ["Ver\u00b7geht", ".", "\u00ab"], "token_info": ["word", "punct", "punct"], "pos": ["VVFIN", "$.", "$("], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Freund, was die ", "tokens": ["Freund", ",", "was", "die"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "PRELS", "ART"], "meter": "+--", "measure": "dactylic.init"}, "line.6": {"text": "Das bl\u00fcht;", "tokens": ["Das", "bl\u00fcht", ";"], "token_info": ["word", "word", "punct"], "pos": ["PDS", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Was Du der ", "tokens": ["Was", "Du", "der"], "token_info": ["word", "word", "word"], "pos": ["PWS", "PPER", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Geht drauf.", "tokens": ["Geht", "drauf", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "Ihr Regelndichter", "tokens": ["Ihr", "Re\u00b7geln\u00b7dich\u00b7ter"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Und K\u00fcnsterichter,", "tokens": ["Und", "K\u00fcns\u00b7te\u00b7rich\u00b7ter", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Seit Ihr seid,", "tokens": ["Seit", "Ihr", "seid", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Hat nichts Freud';", "tokens": ["Hat", "nichts", "Freud'", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Verdorrt ist alles Land", "tokens": ["Ver\u00b7dorrt", "ist", "al\u00b7les", "Land"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Durch Eure Hand.", "tokens": ["Durch", "Eu\u00b7re", "Hand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}