{"dta.poem.2717": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Trost-Zeilen/  \n  Bey Beerdigung Fr. A. v. F. g. K. den  25.  \n Aug. 1678.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wen red' ich erstlich an? die Mutter schwim\u0303t in Thr\u00e4nen/", "tokens": ["Wen", "red'", "ich", "erst\u00b7lich", "an", "?", "die", "Mut\u00b7ter", "schwim\u0303t", "in", "Thr\u00e4\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADJD", "PTKVZ", "$.", "ART", "NN", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und klagt/ wie Hecuba/ der liebsten Tochter Tod:", "tokens": ["Und", "klagt", "/", "wie", "He\u00b7cu\u00b7ba", "/", "der", "liebs\u00b7ten", "Toch\u00b7ter", "Tod", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "KOKOM", "NE", "$(", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du aber/ werther Freund/ st\u00f6st\u2019 Seufftzer aus und", "tokens": ["Du", "a\u00b7ber", "/", "wert\u00b7her", "Freund", "/", "st\u00f6st'", "Seufft\u00b7zer", "aus", "und"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "$(", "ADJA", "NN", "$(", "VVFIN", "NN", "PTKVZ", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und gleiches Seelen-Weh\u2019macht dir die Augen roth.", "tokens": ["Und", "glei\u00b7ches", "See\u00b7len\u00b7Weh'\u00b7macht", "dir", "die", "Au\u00b7gen", "roth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PPER", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Hier theilt das Trauren sich in unterschiedne Flutten/", "tokens": ["Hier", "theilt", "das", "Trau\u00b7ren", "sich", "in", "un\u00b7ter\u00b7schied\u00b7ne", "Flut\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Weib/ Tochter/ Schwester/ Muhm und Mutter wird ver-", "tokens": ["Weib", "/", "Toch\u00b7ter", "/", "Schwes\u00b7ter", "/", "Muhm", "und", "Mut\u00b7ter", "wird", "ver"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$(", "NN", "$(", "NN", "$(", "NN", "KON", "NN", "VAFIN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ach Fall! der vieler Hertz auff einmal heisset bluten/", "tokens": ["Ach", "Fall", "!", "der", "vie\u00b7ler", "Hertz", "auff", "ein\u00b7mal", "heis\u00b7set", "blu\u00b7ten", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "ART", "PIAT", "NN", "APPR", "ADV", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "(mi\u00dft.", "tokens": ["(", "mi\u00dft", "."], "token_info": ["punct", "word", "punct"], "pos": ["$(", "VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.9": {"text": "Leid! dessen Bitterkeit nicht leicht ein Trost vers\u00fc\u00dft.", "tokens": ["Leid", "!", "des\u00b7sen", "Bit\u00b7ter\u00b7keit", "nicht", "leicht", "ein", "Trost", "ver\u00b7s\u00fc\u00dft", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PRELAT", "NN", "PTKNEG", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Di\u00df ist der letzte Sto\u00df/ ", "tokens": ["Di\u00df", "ist", "der", "letz\u00b7te", "Sto\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Der Hintrit rafft zugleich die Lebens-Geister hin/", "tokens": ["Der", "Hin\u00b7trit", "rafft", "zu\u00b7gleich", "die", "Le\u00b7bens\u00b7Geis\u00b7ter", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Di\u00df ist der Kern der Noth/ der Auszug von den Schmertzen/", "tokens": ["Di\u00df", "ist", "der", "Kern", "der", "Noth", "/", "der", "Aus\u00b7zug", "von", "den", "Schmert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "$(", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der bi\u00df ins schwartze Grab beugt den gekr\u00e4nckten Sinn.", "tokens": ["Der", "bi\u00df", "ins", "schwart\u00b7ze", "Grab", "beugt", "den", "ge\u00b7kr\u00e4nck\u00b7ten", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Bey ihres Alters Schnee/ bey den verlebten Tagen", "tokens": ["Bey", "ih\u00b7res", "Al\u00b7ters", "Schnee", "/", "bey", "den", "ver\u00b7leb\u00b7ten", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$(", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "War ja die ", "tokens": ["War", "ja", "die"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ADV", "ART"], "meter": "+--", "measure": "dactylic.init"}, "line.16": {"text": "Bey steter Einsamkeit/ bey vieler Kranctheit Plagen/", "tokens": ["Bey", "ste\u00b7ter", "Ein\u00b7sam\u00b7keit", "/", "bey", "vie\u00b7ler", "Kranc\u00b7theit", "Pla\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$(", "APPR", "PIAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Hat ihre Pfleg und Treu sie viemals auffgericht.", "tokens": ["Hat", "ih\u00b7re", "Pfleg", "und", "Treu", "sie", "vie\u00b7mals", "auff\u00b7ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "KON", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Jhr Alles das hie\u00df sie/ ihr dencken/ und ihr hoffen", "tokens": ["Ihr", "Al\u00b7les", "das", "hie\u00df", "sie", "/", "ihr", "den\u00b7cken", "/", "und", "ihr", "hof\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "PIS", "PDS", "VVFIN", "PPER", "$(", "PPER", "VVINF", "$(", "KON", "PPER", "VVINF"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.19": {"text": "War blo\u00df der T\u00f6chter Gl\u00fcck und Wolfarth anzuschaun:", "tokens": ["War", "blo\u00df", "der", "T\u00f6ch\u00b7ter", "Gl\u00fcck", "und", "Wolf\u00b7arth", "an\u00b7zu\u00b7schaun", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "NN", "KON", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Der treue Hertzens-Wunsch hat auch den Zweck getroffen/", "tokens": ["Der", "treu\u00b7e", "Hert\u00b7zens\u00b7Wunsch", "hat", "auch", "den", "Zweck", "ge\u00b7trof\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wenn sie den Bienen gleich sie sah die Nahrung baun.", "tokens": ["Wenn", "sie", "den", "Bie\u00b7nen", "gleich", "sie", "sah", "die", "Nah\u00b7rung", "baun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "PPER", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Zerschmoltze nicht ihr Hertz in unerme\u00dfnen Freuden.", "tokens": ["Zer\u00b7schmolt\u00b7ze", "nicht", "ihr", "Hertz", "in", "un\u00b7er\u00b7me\u00df\u00b7nen", "Freu\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Als sie zum ersten mal ein Enckelein erblickt/", "tokens": ["Als", "sie", "zum", "ers\u00b7ten", "mal", "ein", "En\u00b7cke\u00b7lein", "er\u00b7blickt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und konte sie wol satt ihr Aug\u2019 und Seele weiden", "tokens": ["Und", "kon\u00b7te", "sie", "wol", "satt", "ihr", "Aug'", "und", "See\u00b7le", "wei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Als GOttes Seegen mehr der Enckel hat geschickt.", "tokens": ["Als", "Got\u00b7tes", "See\u00b7gen", "mehr", "der", "En\u00b7ckel", "hat", "ge\u00b7schickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "ADV", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Sie sah\u2019 nun ihr Geschlecht in Leibes-Erben leben/", "tokens": ["Sie", "sah'", "nun", "ihr", "Ge\u00b7schlecht", "in", "Lei\u00b7bes\u00b7Er\u00b7ben", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und wie der Tochter Haus ein fruchtbar Weinstock war/", "tokens": ["Und", "wie", "der", "Toch\u00b7ter", "Haus", "ein", "frucht\u00b7bar", "Wein\u00b7stock", "war", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "NN", "ART", "ADJD", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Sie sahe Gl\u00fcck und Heil ob ihren Kindern schweben/", "tokens": ["Sie", "sa\u00b7he", "Gl\u00fcck", "und", "Heil", "ob", "ih\u00b7ren", "Kin\u00b7dern", "schwe\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "KOUS", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Und baute Gottes G\u00fct\u2019 ein Lob und Danck-Altar.", "tokens": ["Und", "bau\u00b7te", "Got\u00b7tes", "G\u00fct'", "ein", "Lob", "und", "Dan\u00b7ck\u00b7Al\u00b7tar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "NN", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.30": {"text": "Denckt sie an jene Lust/ wenn gleich des Oelbaums Zweigen", "tokens": ["Denckt", "sie", "an", "je\u00b7ne", "Lust", "/", "wenn", "gleich", "des", "O\u00b7el\u00b7baums", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PDAT", "NN", "$(", "KOUS", "ADV", "ART", "NN", "NN"], "meter": "+--+-+-+----+-", "measure": "iambic.penta.invert"}, "line.31": {"text": "Die Blumen aus der Eh\u2019 so Bett als Tisch bekr\u00f6nt/", "tokens": ["Die", "Blu\u00b7men", "aus", "der", "Eh'", "so", "Bett", "als", "Tisch", "be\u00b7kr\u00f6nt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ADV", "NN", "KOUS", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und konten hocherfreut die grosse Mutter zeigen/", "tokens": ["Und", "kon\u00b7ten", "ho\u00b7cher\u00b7freut", "die", "gros\u00b7se", "Mut\u00b7ter", "zei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADJD", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Wenn ihr noch lallend Mund di\u00df s\u00fcsse Wort erth\u00f6nt.", "tokens": ["Wenn", "ihr", "noch", "lal\u00b7lend", "Mund", "di\u00df", "s\u00fcs\u00b7se", "Wort", "er\u00b7th\u00f6nt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "NN", "PDS", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Wer hat sie werthste Frau/ gesegnet nicht gepriesen?", "tokens": ["Wer", "hat", "sie", "werths\u00b7te", "Frau", "/", "ge\u00b7seg\u00b7net", "nicht", "ge\u00b7prie\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADJA", "NN", "$(", "VVFIN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Schien nit des Himmels-Gunst verschwendrisch ihr geneigt?", "tokens": ["Schien", "nit", "des", "Him\u00b7mels\u00b7Gunst", "ver\u00b7schwend\u00b7risch", "ihr", "ge\u00b7neigt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ART", "NN", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Der Tochter Ehstand war gleich einer vollen Wiesen", "tokens": ["Der", "Toch\u00b7ter", "Eh\u00b7stand", "war", "gleich", "ei\u00b7ner", "vol\u00b7len", "Wie\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VAFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Wo Florens milde Hand die sch\u00f6nsten Blumen zeigt.", "tokens": ["Wo", "Flo\u00b7rens", "mil\u00b7de", "Hand", "die", "sch\u00f6ns\u00b7ten", "Blu\u00b7men", "zeigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Ja di\u00df war auch ihr Wunsch/ wenn sie nun satt am Leben", "tokens": ["Ja", "di\u00df", "war", "auch", "ihr", "Wunsch", "/", "wenn", "sie", "nun", "satt", "am", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "PDS", "VAFIN", "ADV", "PPOSAT", "NN", "$(", "KOUS", "PPER", "ADV", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Nach Gottes hohen Rath verliesse dieses Rund/", "tokens": ["Nach", "Got\u00b7tes", "ho\u00b7hen", "Rath", "ver\u00b7lies\u00b7se", "die\u00b7ses", "Rund", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "VVFIN", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Die liebste Tochter solt ihr das Geleite geben/", "tokens": ["Die", "liebs\u00b7te", "Toch\u00b7ter", "solt", "ihr", "das", "Ge\u00b7lei\u00b7te", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Und mit getreuer Hand zudr\u00fccken Aug und Mund.", "tokens": ["Und", "mit", "ge\u00b7treu\u00b7er", "Hand", "zu\u00b7dr\u00fc\u00b7cken", "Aug", "und", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVINF", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Wie strauchelt doch der Mensch mit Sinnen und Gedencken?", "tokens": ["Wie", "strau\u00b7chelt", "doch", "der", "Mensch", "mit", "Sin\u00b7nen", "und", "Ge\u00b7den\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ART", "NN", "APPR", "NN",