{"textgrid.poem.62361": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es weht um mich Narzissenduft,", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es weht um mich Narzissenduft,", "tokens": ["Es", "weht", "um", "mich", "Nar\u00b7zis\u00b7sen\u00b7duft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es spricht zu mir die Fr\u00fchlingsluft:", "tokens": ["Es", "spricht", "zu", "mir", "die", "Fr\u00fch\u00b7lings\u00b7luft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Geliebter,", "tokens": ["Ge\u00b7lieb\u00b7ter", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Erwach' im rothen Morgenglanz,", "tokens": ["Er\u00b7wach'", "im", "ro\u00b7then", "Mor\u00b7gen\u00b7glanz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dein harrt ein bl\u00fctenreicher Kranz,", "tokens": ["Dein", "harrt", "ein", "bl\u00fc\u00b7ten\u00b7rei\u00b7cher", "Kranz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Betr\u00fcbter!", "tokens": ["Be\u00b7tr\u00fcb\u00b7ter", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Nur mu\u00dft du k\u00e4mpfen drum und thun", "tokens": ["Nur", "mu\u00dft", "du", "k\u00e4mp\u00b7fen", "drum", "und", "thun"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "PAV", "KON", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und l\u00e4nger nicht in Tr\u00e4umen ruhn;", "tokens": ["Und", "l\u00e4n\u00b7ger", "nicht", "in", "Tr\u00e4u\u00b7men", "ruhn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKNEG", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00df schwinden!", "tokens": ["La\u00df", "schwin\u00b7den", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Komm, Lieber, komm aufs Feld hinaus,", "tokens": ["Komm", ",", "Lie\u00b7ber", ",", "komm", "aufs", "Feld", "hin\u00b7aus", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJD", "$,", "VVFIN", "APPRART", "NN", "APZR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du wirst im gr\u00fcnen Bl\u00e4tterhaus", "tokens": ["Du", "wirst", "im", "gr\u00fc\u00b7nen", "Bl\u00e4t\u00b7ter\u00b7haus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ihn finden.", "tokens": ["Ihn", "fin\u00b7den", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Wir sind dir Alle wohlgesinnt,", "tokens": ["Wir", "sind", "dir", "Al\u00b7le", "wohl\u00b7ge\u00b7sinnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du armes, liebebanges Kind,", "tokens": ["Du", "ar\u00b7mes", ",", "lie\u00b7be\u00b7ban\u00b7ges", "Kind", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir D\u00fcfte.", "tokens": ["Wir", "D\u00fcf\u00b7te", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Warst immer treu uns Spielgesell,", "tokens": ["Warst", "im\u00b7mer", "treu", "uns", "Spiel\u00b7ge\u00b7sell", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Drum dienen willig dir und schnell", "tokens": ["Drum", "die\u00b7nen", "wil\u00b7lig", "dir", "und", "schnell"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVINF", "VMFIN", "PPER", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die L\u00fcfte.", "tokens": ["Die", "L\u00fcf\u00b7te", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "Zur Liebsten tragen wir dein Ach,", "tokens": ["Zur", "Liebs\u00b7ten", "tra\u00b7gen", "wir", "dein", "Ach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und kr\u00e4nzen ihr das Schlafgemach", "tokens": ["Und", "kr\u00e4n\u00b7zen", "ihr", "das", "Schlaf\u00b7ge\u00b7mach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Bl\u00fcten.", "tokens": ["Mit", "Bl\u00fc\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Wir wollen, wenn du von ihr gehst,", "tokens": ["Wir", "wol\u00b7len", ",", "wenn", "du", "von", "ihr", "gehst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und einsam dann und traurig stehst,", "tokens": ["Und", "ein\u00b7sam", "dann", "und", "trau\u00b7rig", "stehst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie h\u00fcten.", "tokens": ["Sie", "h\u00fc\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.5": {"line.1": {"text": "Erwach' im morgenrothen Glanz,", "tokens": ["Er\u00b7wach'", "im", "mor\u00b7gen\u00b7ro\u00b7then", "Glanz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schon harret dein der Myrthenkranz,", "tokens": ["Schon", "har\u00b7ret", "dein", "der", "Myr\u00b7then\u00b7kranz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Geliebter!", "tokens": ["Ge\u00b7lieb\u00b7ter", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Der Fr\u00fchling k\u00fcndet gute M\u00e4hr", "tokens": ["Der", "Fr\u00fch\u00b7ling", "k\u00fcn\u00b7det", "gu\u00b7te", "M\u00e4hr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und nun kein Ach, kein Weinen mehr,", "tokens": ["Und", "nun", "kein", "Ach", ",", "kein", "Wei\u00b7nen", "mehr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "$,", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Betr\u00fcbter!", "tokens": ["Be\u00b7tr\u00fcb\u00b7ter", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}