{"textgrid.poem.50782": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ha! was ist das? Die Sehnen zucken wieder,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ha! was ist das? Die Sehnen zucken wieder,", "tokens": ["Ha", "!", "was", "ist", "das", "?", "Die", "Seh\u00b7nen", "zu\u00b7cken", "wie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWS", "VAFIN", "PDS", "$.", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Wie Fr\u00fchlingsbronn quillt auferweckt das Blut,", "tokens": ["Wie", "Fr\u00fch\u00b7lings\u00b7bronn", "quillt", "au\u00b7fer\u00b7weckt", "das", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es dehnen sich die aufgetauten Glieder,", "tokens": ["Es", "deh\u00b7nen", "sich", "die", "auf\u00b7ge\u00b7tau\u00b7ten", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und in der Brust schwillt junger Lebensmut!", "tokens": ["Und", "in", "der", "Brust", "schwillt", "jun\u00b7ger", "Le\u00b7bens\u00b7mut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nun ist's geschehn \u2013 nun bricht herein der Jammer!", "tokens": ["Nun", "ist's", "ge\u00b7schehn", "\u2013", "nun", "bricht", "her\u00b7ein", "der", "Jam\u00b7mer", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "$(", "ADV", "VVFIN", "PTKVZ", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich \u00fcberschau mein grausiges Geschick!", "tokens": ["Ich", "\u00fc\u00b7bersc\u00b7hau", "mein", "grau\u00b7si\u00b7ges", "Ge\u00b7schick", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich messe tappend meine Totenkammer,", "tokens": ["Ich", "mes\u00b7se", "tap\u00b7pend", "mei\u00b7ne", "To\u00b7ten\u00b7kam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Sp\u00e4ne knirschen unter dem Genick.", "tokens": ["Die", "Sp\u00e4\u00b7ne", "knir\u00b7schen", "un\u00b7ter", "dem", "Ge\u00b7nick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Halt an, o Wahnsinn; denn ", "tokens": ["Halt", "an", ",", "o", "Wahn\u00b7sinn", ";", "denn"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VVIMP", "PTKVZ", "$,", "FM", "NN", "$.", "KON"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und bleib es bis zum letzten Odemzug!", "tokens": ["Und", "bleib", "es", "bis", "zum", "letz\u00b7ten", "O\u00b7dem\u00b7zug", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So scharet euch, ihr, meine Lebensgeister,", "tokens": ["So", "scha\u00b7ret", "euch", ",", "ihr", ",", "mei\u00b7ne", "Le\u00b7bens\u00b7geis\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu k\u00e4mpfen mit dem wilden Sinnentrug!", "tokens": ["Zu", "k\u00e4mp\u00b7fen", "mit", "dem", "wil\u00b7den", "Sin\u00b7nen\u00b7trug", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So \u00f6ffnet euch, krampfhaft geballte F\u00e4uste,", "tokens": ["So", "\u00f6ff\u00b7net", "euch", ",", "krampf\u00b7haft", "ge\u00b7ball\u00b7te", "F\u00e4us\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und faltet euch ergeben auf der Brust!", "tokens": ["Und", "fal\u00b7tet", "euch", "er\u00b7ge\u00b7ben", "auf", "der", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn zehnfach mir die Qual die Stirn umkreiste:", "tokens": ["Wenn", "zehn\u00b7fach", "mir", "die", "Qual", "die", "Stirn", "um\u00b7kreis\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVIMP", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Fest will ich bleiben und mir selbst bewu\u00dft!", "tokens": ["Fest", "will", "ich", "blei\u00b7ben", "und", "mir", "selbst", "be\u00b7wu\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VVINF", "KON", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Von Erdenduldern ein verlorner Posten,", "tokens": ["Von", "Er\u00b7den\u00b7dul\u00b7dern", "ein", "ver\u00b7lor\u00b7ner", "Pos\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Will ich hier streiten an der H\u00f6lle Tor!", "tokens": ["Will", "ich", "hier", "strei\u00b7ten", "an", "der", "H\u00f6l\u00b7le", "Tor", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVFIN", "APPR", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den herbsten Kelch des Leidens will ich kosten:", "tokens": ["Den", "herbs\u00b7ten", "Kelch", "des", "Lei\u00b7dens", "will", "ich", "kos\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Halt mir den Becher, g\u00f6ttlicher Humor!", "tokens": ["Halt", "mir", "den", "Be\u00b7cher", ",", "g\u00f6tt\u00b7li\u00b7cher", "Hu\u00b7mor", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,", "ADJA", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}}}}