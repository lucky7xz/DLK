{"textgrid.poem.47285": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Einen Haushalt klein und fein", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einen Haushalt klein und fein", "tokens": ["Ei\u00b7nen", "Haus\u00b7halt", "klein", "und", "fein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hab' ich angestellt;", "tokens": ["Hab'", "ich", "an\u00b7ge\u00b7stellt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Der soll mein Freund sein,", "tokens": ["Der", "soll", "mein", "Freund", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Dem er wohlgef\u00e4llt.", "tokens": ["Dem", "er", "wohl\u00b7ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Der Specht, der Holz mit dem Schnabel haut,", "tokens": ["Der", "Specht", ",", "der", "Holz", "mit", "dem", "Schna\u00b7bel", "haut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Hat das Haus mir aufgebaut;", "tokens": ["Hat", "das", "Haus", "mir", "auf\u00b7ge\u00b7baut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df das Haus beworfen sei,", "tokens": ["Da\u00df", "das", "Haus", "be\u00b7wor\u00b7fen", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Trug die Schwalbe M\u00f6rtel bei,", "tokens": ["Trug", "die", "Schwal\u00b7be", "M\u00f6r\u00b7tel", "bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und als Dach hat sich zuletzt", "tokens": ["Und", "als", "Dach", "hat", "sich", "zu\u00b7letzt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "NN", "VAFIN", "PRF", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Obendrauf ein Schwamm gesetzt.", "tokens": ["O\u00b7ben\u00b7drauf", "ein", "Schwamm", "ge\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Drinnen die Kammern", "tokens": ["Drin\u00b7nen", "die", "Kam\u00b7mern"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Und die Gem\u00e4cher,", "tokens": ["Und", "die", "Ge\u00b7m\u00e4\u00b7cher", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Schr\u00e4nke und F\u00e4cher", "tokens": ["Schr\u00e4n\u00b7ke", "und", "F\u00e4\u00b7cher"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Flimmern und flammern;", "tokens": ["Flim\u00b7mern", "und", "flam\u00b7mern", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Alles hat mir unbezahlt", "tokens": ["Al\u00b7les", "hat", "mir", "un\u00b7be\u00b7zahlt"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "PPER", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Schmetterling mit Duft bemalt.", "tokens": ["Schmet\u00b7ter\u00b7ling", "mit", "Duft", "be\u00b7malt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "O wie r\u00fcstig in dem Haus", "tokens": ["O", "wie", "r\u00fcs\u00b7tig", "in", "dem", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Geht die Wirtschaft ein und aus.", "tokens": ["Geht", "die", "Wirt\u00b7schaft", "ein", "und", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wasserj\u00fcngferchen, das flinke,", "tokens": ["Was\u00b7ser\u00b7j\u00fcng\u00b7fer\u00b7chen", ",", "das", "flin\u00b7ke", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PDS", "VVFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Holt mir Wasser, das ich trinke;", "tokens": ["Holt", "mir", "Was\u00b7ser", ",", "das", "ich", "trin\u00b7ke", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Biene mu\u00df mir Essen holen,", "tokens": ["Bie\u00b7ne", "mu\u00df", "mir", "Es\u00b7sen", "ho\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Frage nicht, wo sie's gestohlen.", "tokens": ["Fra\u00b7ge", "nicht", ",", "wo", "sie's", "ge\u00b7stoh\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "PWAV", "PIS", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Sch\u00fcsseln sind die Eicheln\u00e4pfchen,", "tokens": ["Sch\u00fcs\u00b7seln", "sind", "die", "Ei\u00b7chel\u00b7n\u00e4pf\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Kr\u00fcge Tannenz\u00e4pfchen,", "tokens": ["Und", "die", "Kr\u00fc\u00b7ge", "Tan\u00b7nen\u00b7z\u00e4pf\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Messer, Gabel,", "tokens": ["Mes\u00b7ser", ",", "Ga\u00b7bel", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Rosendorn und Vogelschnabel.", "tokens": ["Ro\u00b7sen\u00b7dorn", "und", "Vo\u00b7gel\u00b7schna\u00b7bel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Storch im Haus ist Kinderw\u00e4rter,", "tokens": ["Storch", "im", "Haus", "ist", "Kin\u00b7der\u00b7w\u00e4r\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VAFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Maulwurf G\u00e4rtner,", "tokens": ["Maul\u00b7wurf", "G\u00e4rt\u00b7ner", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Und Beschlie\u00dferin im H\u00e4uslein", "tokens": ["Und", "Be\u00b7schlie\u00b7\u00dfe\u00b7rin", "im", "H\u00e4us\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist das M\u00e4uslein.", "tokens": ["Ist", "das", "M\u00e4us\u00b7lein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.8": {"line.1": {"text": "Aber die Grille", "tokens": ["A\u00b7ber", "die", "Gril\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Singt in der Stille,", "tokens": ["Singt", "in", "der", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Sie ist das Heimchen, ist immer daheim", "tokens": ["Sie", "ist", "das", "Heim\u00b7chen", ",", "ist", "im\u00b7mer", "da\u00b7heim"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "VAFIN", "ADV", "ADV"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und wei\u00df nichts als den einen Reim.", "tokens": ["Und", "wei\u00df", "nichts", "als", "den", "ei\u00b7nen", "Reim", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "KOKOM", "ART", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Doch im ganzen Haus das Beste", "tokens": ["Doch", "im", "gan\u00b7zen", "Haus", "das", "Bes\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schl\u00e4ft noch feste.", "tokens": ["Schl\u00e4ft", "noch", "fes\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJA", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.10": {"line.1": {"text": "In dem Winkel, in dem Bettchen,", "tokens": ["In", "dem", "Win\u00b7kel", ",", "in", "dem", "Bett\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zwischen zweien Rosenbl\u00e4ttchen,", "tokens": ["Zwi\u00b7schen", "zwei\u00b7en", "Ro\u00b7sen\u00b7bl\u00e4tt\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schl\u00e4ft das Sch\u00e4tzchen Tausendsch\u00f6nchen,", "tokens": ["Schl\u00e4ft", "das", "Sch\u00e4tz\u00b7chen", "Tau\u00b7send\u00b7sch\u00f6n\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihr zu Fu\u00df ein Kaiserkr\u00f6nchen.", "tokens": ["Ihr", "zu", "Fu\u00df", "ein", "Kai\u00b7ser\u00b7kr\u00f6n\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "H\u00fcter ist Vergi\u00dfmeinnicht,", "tokens": ["H\u00fc\u00b7ter", "ist", "Ver\u00b7gi\u00df\u00b7mein\u00b7nicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Der vom Bette wanket nicht;", "tokens": ["Der", "vom", "Bet\u00b7te", "wan\u00b7ket", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Gl\u00fchwurm mit dem Kerzenschimmer", "tokens": ["Gl\u00fch\u00b7wurm", "mit", "dem", "Ker\u00b7zen\u00b7schim\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.8": {"text": "Hellt das Zimmer.", "tokens": ["Hellt", "das", "Zim\u00b7mer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.11": {"line.1": {"text": "Die Wachtel wacht", "tokens": ["Die", "Wach\u00b7tel", "wacht"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Die ganze Nacht,", "tokens": ["Die", "gan\u00b7ze", "Nacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und wenn der Tag beginnt,", "tokens": ["Und", "wenn", "der", "Tag", "be\u00b7ginnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ruft sie: \u00bbKind! Kind!", "tokens": ["Ruft", "sie", ":", "\u00bb", "Kind", "!", "Kind", "!"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "$(", "NN", "$.", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Wach' auf geschwind.\u00ab", "tokens": ["Wach'", "auf", "ge\u00b7schwind", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "ADJD", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.12": {"line.1": {"text": "Wenn die Liebe wachet auf,", "tokens": ["Wenn", "die", "Lie\u00b7be", "wa\u00b7chet", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Geht das Leben raschen Lauf.", "tokens": ["Geht", "das", "Le\u00b7ben", "ra\u00b7schen", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "In seidnen Gew\u00e4ndern,", "tokens": ["In", "seid\u00b7nen", "Ge\u00b7w\u00e4n\u00b7dern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Gewebt aus Sommerfaden,", "tokens": ["Ge\u00b7webt", "aus", "Som\u00b7mer\u00b7fa\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In flatternden B\u00e4ndern,", "tokens": ["In", "flat\u00b7tern\u00b7den", "B\u00e4n\u00b7dern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Von Sorgen unbeladen,", "tokens": ["Von", "Sor\u00b7gen", "un\u00b7be\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Lustig aus dem engen Haus", "tokens": ["Lus\u00b7tig", "aus", "dem", "en\u00b7gen", "Haus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Die Flur hinaus.", "tokens": ["Die", "Flur", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.14": {"line.1": {"text": "Sch\u00f6nen Wagen", "tokens": ["Sch\u00f6\u00b7nen", "Wa\u00b7gen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Hab' ich bestellt,", "tokens": ["Hab'", "ich", "be\u00b7stellt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Uns zu tragen", "tokens": ["Uns", "zu", "tra\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PPER", "PTKZU", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Durch die Welt.", "tokens": ["Durch", "die", "Welt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.15": {"line.1": {"text": "Vier Heupferdchen sollen ihn", "tokens": ["Vier", "Heu\u00b7pferd\u00b7chen", "sol\u00b7len", "ihn"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "NN", "VMFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Als vier Apfelschimmel ziehn;", "tokens": ["Als", "vier", "Ap\u00b7fel\u00b7schim\u00b7mel", "ziehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "CARD", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie sind wohl ein gut Gespann,", "tokens": ["Sie", "sind", "wohl", "ein", "gut", "Ge\u00b7spann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Das mit Rossen sich messen kann;", "tokens": ["Das", 