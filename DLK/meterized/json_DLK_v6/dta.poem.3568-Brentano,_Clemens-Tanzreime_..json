{"dta.poem.3568": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Tanzreime .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Sch\u00e4tzle ist Nunn,               ", "tokens": ["Mein", "Sch\u00e4tz\u00b7le", "ist", "Nunn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Mach mich nit lachun;", "tokens": ["Mach", "mich", "nit", "la\u00b7chun", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Die Lieb ist brochun,", "tokens": ["Die", "Lieb", "ist", "bro\u00b7ch\u00b7un", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Kanns nimmer machun.", "tokens": ["Kanns", "nim\u00b7mer", "ma\u00b7chun", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Schatzlein freu dich, juchze,", "tokens": ["Schatz\u00b7lein", "freu", "dich", ",", "juch\u00b7ze", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Das Abscheiden thut weh;", "tokens": ["Das", "Ab\u00b7schei\u00b7den", "thut", "weh", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die Liebe thut wanken,", "tokens": ["Die", "Lie\u00b7be", "thut", "wan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Wie ein Schiff auf der See.", "tokens": ["Wie", "ein", "Schiff", "auf", "der", "See", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Da\u00df im Wald finster ist,", "tokens": ["Da\u00df", "im", "Wald", "fins\u00b7ter", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "ADJD", "VAFIN", "$,"], "meter": "--++-+", "measure": "anapaest.init"}, "line.2": {"text": "Das machen die Birken;", "tokens": ["Das", "ma\u00b7chen", "die", "Bir\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Da\u00df mich mein Schatz nicht mag,", "tokens": ["Da\u00df", "mich", "mein", "Schatz", "nicht", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "PTKNEG", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das kann ich merken.", "tokens": ["Das", "kann", "ich", "mer\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Da\u00df im Wald finster ist,", "tokens": ["Da\u00df", "im", "Wald", "fins\u00b7ter", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "ADJD", "VAFIN", "$,"], "meter": "--++-+", "measure": "anapaest.init"}, "line.2": {"text": "Das machen die Aest;", "tokens": ["Das", "ma\u00b7chen", "die", "A\u00b7est", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Da\u00df mich mein Schatz nit mag,", "tokens": ["Da\u00df", "mich", "mein", "Schatz", "nit", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "PTKNEG", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das glaub ich fest.", "tokens": ["Das", "glaub", "ich", "fest", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Ich hab geheurat, ich hab gehau\u00dft,", "tokens": ["Ich", "hab", "ge\u00b7heu\u00b7rat", ",", "ich", "hab", "ge\u00b7hau\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Hab einen Mann wie eine Faust;", "tokens": ["Hab", "ei\u00b7nen", "Mann", "wie", "ei\u00b7ne", "Faust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hab ein Herz wie eine Nu\u00df,", "tokens": ["Hab", "ein", "Herz", "wie", "ei\u00b7ne", "Nu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist keine Freud und keine Lust.", "tokens": ["Ist", "kei\u00b7ne", "Freud", "und", "kei\u00b7ne", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}