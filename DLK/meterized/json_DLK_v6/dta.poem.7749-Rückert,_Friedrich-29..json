{"dta.poem.7749": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "29.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sieh, wie unm\u00e4chtig sind, die nun im Lande walten,", "tokens": ["Sieh", ",", "wie", "un\u00b7m\u00e4ch\u00b7tig", "sind", ",", "die", "nun", "im", "Lan\u00b7de", "wal\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ADJD", "VAFIN", "$,", "PRELS", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die neuen F\u00fcrsten, wenn man sie vergleicht den alten.", "tokens": ["Die", "neu\u00b7en", "F\u00fcrs\u00b7ten", ",", "wenn", "man", "sie", "ver\u00b7gleicht", "den", "al\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "PIS", "PPER", "VVFIN", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der F\u00fcrst trat staunend an des Riesenbaues Rand,", "tokens": ["Der", "F\u00fcrst", "trat", "stau\u00b7nend", "an", "des", "Rie\u00b7sen\u00b7bau\u00b7es", "Rand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo er in alter Schrift geschrieben dieses fand:", "tokens": ["Wo", "er", "in", "al\u00b7ter", "Schrift", "ge\u00b7schrie\u00b7ben", "die\u00b7ses", "fand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "NN", "VVPP", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "\u201eich baute, wer darf einzurei\u00dfen sich getraun,", "tokens": ["\u201e", "ich", "bau\u00b7te", ",", "wer", "darf", "ein\u00b7zu\u00b7rei\u00b7\u00dfen", "sich", "ge\u00b7traun", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "PWS", "VMFIN", "VVFIN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der thu's, weil leichter doch Einrei\u00dfen ist als Baun.\u201c", "tokens": ["Der", "thu's", ",", "weil", "leich\u00b7ter", "doch", "Ein\u00b7rei\u00b7\u00dfen", "ist", "als", "Baun", ".", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NE", "$,", "KOUS", "ADJD", "ADV", "ADV", "VAFIN", "KOKOM", "NE", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Berechnen lie\u00df der F\u00fcrst die Schreiber alle Posten,", "tokens": ["Be\u00b7rech\u00b7nen", "lie\u00df", "der", "F\u00fcrst", "die", "Schrei\u00b7ber", "al\u00b7le", "Pos\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was ihm der Riesenbau m\u00f6cht' einzurei\u00dfen kosten.", "tokens": ["Was", "ihm", "der", "Rie\u00b7sen\u00b7bau", "m\u00f6cht'", "ein\u00b7zu\u00b7rei\u00b7\u00dfen", "kos\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "VMFIN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Doch weil die Kosten weit die Eink\u00fcnft' \u00fcberstiegen,", "tokens": ["Doch", "weil", "die", "Kos\u00b7ten", "weit", "die", "Ein\u00b7k\u00fcnft'", "\u00fc\u00b7bers\u00b7tie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Lie\u00df er die Tr\u00fcmmer stehn, bis sie der Zeit erliegen.", "tokens": ["Lie\u00df", "er", "die", "Tr\u00fcm\u00b7mer", "stehn", ",", "bis", "sie", "der", "Zeit", "er\u00b7lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$,", "KOUS", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}