{"textgrid.poem.67280": {"metadata": {"author": {"name": "Stolberg, Christian Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Turtelt\u00e4ubchen girrte, die s\u00fc\u00dfeste", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Turtelt\u00e4ubchen girrte, die s\u00fc\u00dfeste", "tokens": ["Mein", "Tur\u00b7tel\u00b7t\u00e4ub\u00b7chen", "girr\u00b7te", ",", "die", "s\u00fc\u00b7\u00dfes\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "ART", "ADJA"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Wehmuthsstimmen athmete Liebeshauch", "tokens": ["Der", "Weh\u00b7muths\u00b7stim\u00b7men", "ath\u00b7me\u00b7te", "Lie\u00b7bes\u00b7hauch"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Aus Unschuldskehle \u2013 ach, und doch mir", "tokens": ["Aus", "Un\u00b7schulds\u00b7keh\u00b7le", "\u2013", "ach", ",", "und", "doch", "mir"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$(", "XY", "$,", "KON", "ADV", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dr\u00f6hnt' es in Mark und Gebein ein Donner!", "tokens": ["Dr\u00f6hnt'", "es", "in", "Mark", "und", "Ge\u00b7bein", "ein", "Don\u00b7ner", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Sangst, Sehervogel, warnende T\u00f6ne du?", "tokens": ["Sangst", ",", "Se\u00b7her\u00b7vo\u00b7gel", ",", "war\u00b7nen\u00b7de", "T\u00f6\u00b7ne", "du", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "So deutet's Reue. \u2013 Brausender schnob empor", "tokens": ["So", "deu\u00b7tet's", "Reu\u00b7e", ".", "\u2013", "Brau\u00b7sen\u00b7der", "schnob", "em\u00b7por"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "$.", "$(", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mir Grolles Lohe, hadernd mit den", "tokens": ["Mir", "Grol\u00b7les", "Lo\u00b7he", ",", "ha\u00b7dernd", "mit", "den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "NE", "$,", "ADJD", "APPR", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gr\u00e4uelgeburten der tr\u00fcben Stunde.", "tokens": ["Gr\u00e4u\u00b7el\u00b7ge\u00b7bur\u00b7ten", "der", "tr\u00fc\u00b7ben", "Stun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Da girrt' es! Hauchte, Turtelchen, dir allein", "tokens": ["Da", "girrt'", "es", "!", "Hauch\u00b7te", ",", "Tur\u00b7tel\u00b7chen", ",", "dir", "al\u00b7lein"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "NN", "$,", "NN", "$,", "PPER", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In's fromme Herzchen schmachtendes Sanftgef\u00fchl", "tokens": ["In's", "from\u00b7me", "Herz\u00b7chen", "schmach\u00b7ten\u00b7des", "Sanft\u00b7ge\u00b7f\u00fchl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Allvater, ach und linde, wie dein", "tokens": ["All\u00b7va\u00b7ter", ",", "ach", "und", "lin\u00b7de", ",", "wie", "dein"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "XY", "KON", "ADJA", "$,", "PWAV", "PPOSAT"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Busengefieder, die Sehnsuchtsseufzer?", "tokens": ["Bu\u00b7sen\u00b7ge\u00b7fie\u00b7der", ",", "die", "Sehn\u00b7suchts\u00b7seuf\u00b7zer", "?"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Da schwieg die Windsbraut, l\u00f6schte den Flammenblick", "tokens": ["Da", "schwieg", "die", "Winds\u00b7braut", ",", "l\u00f6schte", "den", "Flam\u00b7men\u00b7blick"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die milde Z\u00e4hre. \u2013 Zwar das Gefilde lag", "tokens": ["Die", "mil\u00b7de", "Z\u00e4h\u00b7re", ".", "\u2013", "Zwar", "das", "Ge\u00b7fil\u00b7de", "lag"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "$(", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Noch vor mir schwer an Jammerssaaten,", "tokens": ["Noch", "vor", "mir", "schwer", "an", "Jam\u00b7mers\u00b7saa\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Saaten des Drangsals und schn\u00f6den Frevels;", "tokens": ["Saa\u00b7ten", "des", "Drangs\u00b7als", "und", "schn\u00f6\u00b7den", "Fre\u00b7vels", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Doch still und stiller ward's, es ums\u00e4uselten", "tokens": ["Doch", "still", "und", "stil\u00b7ler", "ward's", ",", "es", "um\u00b7s\u00e4u\u00b7sel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJD", "KON", "ADJD", "VAFIN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+--++-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mich leise Lispel, fl\u00fcsterten, deuteten", "tokens": ["Mich", "lei\u00b7se", "Lis\u00b7pel", ",", "fl\u00fcs\u00b7ter\u00b7ten", ",", "deu\u00b7te\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "ADJD", "NE", "$,", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Auf jene Vaterhand, die Sonnen", "tokens": ["Auf", "je\u00b7ne", "Va\u00b7ter\u00b7hand", ",", "die", "Son\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "G\u00e4ngelt, und sch\u00fctzt in dem Nest den Sperlig.", "tokens": ["G\u00e4n\u00b7gelt", ",", "und", "sch\u00fctzt", "in", "dem", "Nest", "den", "Sper\u00b7lig", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Ich hub das Aug', es schimmerte Hesperus", "tokens": ["Ich", "hub", "das", "Aug'", ",", "es", "schim\u00b7mer\u00b7te", "Hes\u00b7pe\u00b7rus"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "NE"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und Luna's Nachen \u2013 Ewig unwandelbar", "tokens": ["Und", "Lu\u00b7na's", "Na\u00b7chen", "\u2013", "E\u00b7wig", "un\u00b7wan\u00b7del\u00b7bar"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "NE", "NN", "$(", "ADJD", "ADJD"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dort oben wallt der Sterne Reigen,", "tokens": ["Dort", "o\u00b7ben", "wallt", "der", "Ster\u00b7ne", "Rei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Knospet hienieden und bl\u00fcht die Rose,", "tokens": ["Knos\u00b7pet", "hien\u00b7ie\u00b7den", "und", "bl\u00fcht", "die", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Drehn Freud' und Kummer wechselnd im Ringe sich,", "tokens": ["Drehn", "Freud'", "und", "Kum\u00b7mer", "wech\u00b7selnd", "im", "Rin\u00b7ge", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "KON", "NN", "ADJD", "APPRART", "NN", "PRF", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Brautku\u00df und Thr\u00e4ne, sie die den Aschenkrug", "tokens": ["Braut\u00b7ku\u00df", "und", "Thr\u00e4\u00b7ne", ",", "sie", "die", "den", "A\u00b7schen\u00b7krug"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "PPER", "ART", "ART", "NN"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Der Unsern netzt \u2013 Kartaunen br\u00fcll'n, es", "tokens": ["Der", "Un\u00b7sern", "netzt", "\u2013", "Kar\u00b7tau\u00b7nen", "br\u00fcll'n", ",", "es"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VVFIN", "$(", "NN", "VVINF", "$,", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schlummert der S\u00e4ugling im Schoo\u00df der Mutter.", "tokens": ["Schlum\u00b7mert", "der", "S\u00e4ug\u00b7ling", "im", "Schoo\u00df", "der", "Mut\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Getrost! Vom Himmel steiget zu uns herab,", "tokens": ["Ge\u00b7trost", "!", "Vom", "Him\u00b7mel", "stei\u00b7get", "zu", "uns", "her\u00b7ab", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "APPRART", "NN", "VVFIN", "APPR", "PPER", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wie Edens Rosen bl\u00fchend, und flammender", "tokens": ["Wie", "E\u00b7dens", "Ro\u00b7sen", "bl\u00fc\u00b7hend", ",", "und", "flam\u00b7men\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "NE", "NN", "VVPP", "$,", "KON", "NN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Als Sonnen, ", "tokens": ["Als", "Son\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["KOUS", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Leitende Knaul in des Lebens Irrgang.", "tokens": ["Lei\u00b7ten\u00b7de", "Knaul", "in", "des", "Le\u00b7bens", "Irr\u00b7gang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}