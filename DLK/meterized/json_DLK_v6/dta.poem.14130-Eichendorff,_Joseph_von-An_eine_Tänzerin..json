{"dta.poem.14130": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "An eine T\u00e4nzerin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wecke nicht die Zauberlieder", "tokens": ["We\u00b7cke", "nicht", "die", "Zau\u00b7berl\u00b7ie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der dunklen Tiefe Schoo\u00df,", "tokens": ["In", "der", "dunk\u00b7len", "Tie\u00b7fe", "Schoo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Selbst verzaubert sinkst Du nieder,", "tokens": ["Selbst", "ver\u00b7zau\u00b7bert", "sinkst", "Du", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sie lassen Dich nicht los.", "tokens": ["Und", "sie", "las\u00b7sen", "Dich", "nicht", "los", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "T\u00f6dtlich schlingt sich um die Glieder", "tokens": ["T\u00f6dt\u00b7lich", "schlingt", "sich", "um", "die", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "S\u00fcndlich Gl\u00fch'n,", "tokens": ["S\u00fcnd\u00b7lich", "Gl\u00fch'n", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Und verbl\u00fch'n", "tokens": ["Und", "ver\u00b7bl\u00fch'n"], "token_info": ["word", "word"], "pos": ["KON", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "M\u00fcssen Sch\u00f6nheit, Tanz und Lieder,", "tokens": ["M\u00fcs\u00b7sen", "Sch\u00f6n\u00b7heit", ",", "Tanz", "und", "Lie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Ach, ich kenne Dich nicht wieder!", "tokens": ["Ach", ",", "ich", "ken\u00b7ne", "Dich", "nicht", "wie\u00b7der", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}