{"textgrid.poem.61527": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Herakles auf dem \u00d6ta", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Halt aus! Und ob's wie fressend Feuer auch", "tokens": ["Halt", "aus", "!", "Und", "ob's", "wie", "fres\u00b7send", "Feu\u00b7er", "auch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PTKVZ", "$.", "KON", "KOUS", "KOKOM", "ADJD", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bis ans Gebein dir zehrt: dies ist das letzte,", "tokens": ["Bis", "ans", "Ge\u00b7bein", "dir", "zehrt", ":", "dies", "ist", "das", "letz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "PPER", "VVFIN", "$.", "PDS", "VAFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was du zu dulden hast, halt aus, mein Herz!", "tokens": ["Was", "du", "zu", "dul\u00b7den", "hast", ",", "halt", "aus", ",", "mein", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKZU", "VVINF", "VAFIN", "$,", "VVFIN", "PTKVZ", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "In Qualen noch des Todes preis' ich dich,", "tokens": ["In", "Qua\u00b7len", "noch", "des", "To\u00b7des", "preis'", "ich", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ART", "NN", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "O Vater Zeus, Erhabner; denn ich wei\u00df,", "tokens": ["O", "Va\u00b7ter", "Zeus", ",", "Er\u00b7hab\u00b7ner", ";", "denn", "ich", "wei\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "NE", "$,", "NN", "$.", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du hast dem Sohne, dem in Sterblichkeit", "tokens": ["Du", "hast", "dem", "Soh\u00b7ne", ",", "dem", "in", "Sterb\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Geborenen, auch dies zum Heil verordnet", "tokens": ["Ge\u00b7bo\u00b7re\u00b7nen", ",", "auch", "dies", "zum", "Heil", "ver\u00b7ord\u00b7net"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "PDS", "APPRART", "NN", "VVPP"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.5": {"text": "Und ziehst durch Leid und Hitze, den du liebst,", "tokens": ["Und", "ziehst", "durch", "Leid", "und", "Hit\u00b7ze", ",", "den", "du", "liebst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Weil er dich sucht, in deine Klarheit nach.", "tokens": ["Weil", "er", "dich", "sucht", ",", "in", "dei\u00b7ne", "Klar\u00b7heit", "nach", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Aus eitel Kampf und M\u00fchsal webtest du", "tokens": ["Aus", "ei\u00b7tel", "Kampf", "und", "M\u00fch\u00b7sal", "web\u00b7test", "du"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "NN", "KON", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mein irdisch Los, und wie des Ringers Stunde", "tokens": ["Mein", "ir\u00b7disch", "Los", ",", "und", "wie", "des", "Rin\u00b7gers", "Stun\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "KON", "PWAV", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Am Tag der Spiele ging mein Leben hin.", "tokens": ["Am", "Tag", "der", "Spie\u00b7le", "ging", "mein", "Le\u00b7ben", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hab' ich vom Aufgang bis zum Niedergang", "tokens": ["Hab'", "ich", "vom", "Auf\u00b7gang", "bis", "zum", "Nie\u00b7der\u00b7gang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPRART", "NN", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Den Erdkreis nicht bewandert? Hab' ich nicht,", "tokens": ["Den", "Erd\u00b7kreis", "nicht", "be\u00b7wan\u00b7dert", "?", "Hab'", "ich", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVPP", "$.", "NN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der nackte Mann, gerungen bis aufs Blut", "tokens": ["Der", "nack\u00b7te", "Mann", ",", "ge\u00b7run\u00b7gen", "bis", "aufs", "Blut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "VVPP", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Mit all der Riesenbrut der schwangern Wildnis,", "tokens": ["Mit", "all", "der", "Rie\u00b7sen\u00b7brut", "der", "schwan\u00b7gern", "Wild\u00b7nis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die, aufgequollen aus dem Element,", "tokens": ["Die", ",", "auf\u00b7ge\u00b7quol\u00b7len", "aus", "dem", "E\u00b7le\u00b7ment", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "In trotz'ger Urkraft jeder S\u00fchnung lachte,", "tokens": ["In", "trotz'\u00b7ger", "Ur\u00b7kraft", "je\u00b7der", "S\u00fch\u00b7nung", "lach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Bis diese Sehnen ihre Wut erdr\u00fcckt?", "tokens": ["Bis", "die\u00b7se", "Seh\u00b7nen", "ih\u00b7re", "Wut", "er\u00b7dr\u00fcckt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Hab' ich nicht deines Himmels stolz Gew\u00f6lb'", "tokens": ["Hab'", "ich", "nicht", "dei\u00b7nes", "Him\u00b7mels", "stolz", "Ge\u00b7w\u00f6lb'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PTKNEG", "PPOSAT", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Getragen auf den Schultern hier und bin", "tokens": ["Ge\u00b7tra\u00b7gen", "auf", "den", "Schul\u00b7tern", "hier", "und", "bin"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "ADV", "KON", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Hinabgestiegen zu den Pforten drunten", "tokens": ["Hin\u00b7ab\u00b7ge\u00b7stie\u00b7gen", "zu", "den", "Pfor\u00b7ten", "drun\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Der ew'gen Nacht, da\u00df ich den W\u00e4chter dort", "tokens": ["Der", "ew'\u00b7gen", "Nacht", ",", "da\u00df", "ich", "den", "W\u00e4ch\u00b7ter", "dort"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "PPER", "ART", "NE", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Mit meiner Hand, den grimmen, b\u00e4ndigte?", "tokens": ["Mit", "mei\u00b7ner", "Hand", ",", "den", "grim\u00b7men", ",", "b\u00e4n\u00b7dig\u00b7te", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nicht reut der Arbeit mich. Im Schwei\u00df des Kampfes", "tokens": ["Nicht", "reut", "der", "Ar\u00b7beit", "mich", ".", "Im", "Schwei\u00df", "des", "Kamp\u00b7fes"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "ART", "NN", "PPER", "$.", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wuchs in der Brust der K\u00fchnheit Bl\u00fcte mir,", "tokens": ["Wuchs", "in", "der", "Brust", "der", "K\u00fchn\u00b7heit", "Bl\u00fc\u00b7te", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Des Harrens Mut, und meiner Glieder Kraft", "tokens": ["Des", "Har\u00b7rens", "Mut", ",", "und", "mei\u00b7ner", "Glie\u00b7der", "Kraft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ward wie geschmiedet Erz. Doch preis' ich dich", "tokens": ["Ward", "wie", "ge\u00b7schmie\u00b7det", "Erz", ".", "Doch", "preis'", "ich", "dich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "KOKOM", "VVPP", "NN", "$.", "KON", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Um Gr\u00f6\u00dferes. Denn wo die Br\u00fcder mir", "tokens": ["Um", "Gr\u00f6\u00b7\u00dfe\u00b7res", ".", "Denn", "wo", "die", "Br\u00fc\u00b7der", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUI", "NN", "$.", "KON", "PWAV", "ART", "NN", "PPER"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Trostlos verzagten oder eingeh\u00fcllt", "tokens": ["Trost\u00b7los", "ver\u00b7zag\u00b7ten", "o\u00b7der", "ein\u00b7ge\u00b7h\u00fcllt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVINF", "KON", "VVPP"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "In dumpfen Trotz unwillig nur dem Schicksal", "tokens": ["In", "dum\u00b7pfen", "Trotz", "un\u00b7wil\u00b7lig", "nur", "dem", "Schick\u00b7sal"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJD", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wie einer ma\u00dflos fremden Macht sich beugten,", "tokens": ["Wie", "ei\u00b7ner", "ma\u00df\u00b7los", "frem\u00b7den", "Macht", "sich", "beug\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJD", "ADJA", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Da gabst du mir's, durch alles Irrsals Graus", "tokens": ["Da", "gabst", "du", "mir's", ",", "durch", "al\u00b7les", "Irr\u00b7sals", "Graus"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NE", "$,", "APPR", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Das Walten deiner Segenshand zu ahnen;", "tokens": ["Das", "Wal\u00b7ten", "dei\u00b7ner", "Se\u00b7gens\u00b7hand", "zu", "ah\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Und immer, wenn ich der gewalt'gen Not,", "tokens": ["Und", "im\u00b7mer", ",", "wenn", "ich", "der", "ge\u00b7walt'\u00b7gen", "Not", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Der unbeugsamen, fest ins Auge blickte,", "tokens": ["Der", "un\u00b7beug\u00b7sa\u00b7men", ",", "fest", "ins", "Au\u00b7ge", "blick\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Zuletzt erkannt' ich in den strengen Z\u00fcgen", "tokens": ["Zu\u00b7letzt", "er\u00b7kannt'", "ich", "in", "den", "stren\u00b7gen", "Z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Auch so Verhei\u00dfung l\u00e4chelnd niedersah.", "tokens": ["Auch", "so", "Ver\u00b7hei\u00b7\u00dfung", "l\u00e4\u00b7chelnd", "nie\u00b7der\u00b7sah", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Heil mir! Denn wieder wie durch Schleier seh' ich's", "tokens": ["Heil", "mir", "!", "Denn", "wie\u00b7der", "wie", "durch", "Schlei\u00b7er", "seh'", "ich's"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$.", "KON", "ADV", "KOKOM", "APPR", "NN", "VVFIN", "PIS"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu dieser Stunde. Horch, schon rollt, schon rollt", "tokens": ["Zu", "die\u00b7ser", "Stun\u00b7de", ".", "Horch", ",", "schon", "rollt", ",", "schon", "rollt"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$.", "NE", "$,", "ADV", "VVFIN", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Um \u00d6tas Gipfel aus entw\u00f6lktem Blau", "tokens": ["Um", "\u00d6\u00b7tas", "Gip\u00b7fel", "aus", "ent\u00b7w\u00f6lk\u00b7tem", "Blau"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dein naher Donner Gnade k\u00fcndend her,", "tokens": ["Dein", "na\u00b7her", "Don\u00b7ner", "Gna\u00b7de", "k\u00fcn\u00b7dend", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und winkend zuckt wie Adlerfl\u00fcgelschlag", "tokens": ["Und", "win\u00b7kend", "zuckt", "wie", "Ad\u00b7ler\u00b7fl\u00fc\u00b7gel\u00b7schlag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "KOKOM", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Dein Blitz herab. Hab' Dank, hab' Dank, es lodern", "tokens": ["Dein", "Blitz", "her\u00b7ab", ".", "Hab'", "Dank", ",", "hab'", "Dank", ",", "es", "lo\u00b7dern"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "$.", "NN", "NN", "$,", "VAFIN", "NN", "$,", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Um mich die Scheiter; \u00fcber, unter mir", "tokens": ["Um", "mich", "die", "Schei\u00b7ter", ";", "\u00fc\u00b7ber", ",", "un\u00b7ter", "mir"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KOUI", "PRF", "ART", "NN", "$.", "APPR", "$,", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Schlagen der L\u00f6sung Flammen jauchzend auf,", "tokens": ["Schla\u00b7gen", "der", "L\u00f6\u00b7sung", "Flam\u00b7men", "jauch\u00b7zend", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "ADJD", "PTKVZ", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.9": {"text": "Und wie das Staubgeborne endlich, endlich", "tokens": ["Und", "wie", "das", "Staub\u00b7ge\u00b7bor\u00b7ne", "end\u00b7lich", ",", "end\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "PWAV", "ART", "NN", "ADV", "$,", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Gleich wie ein m\u00fcrb Gewand herniederflockt,", "tokens": ["Gleich", "wie", "ein", "m\u00fcrb", "