{"textgrid.poem.57593": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Soldatentod", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gequ\u00e4lt, geschlagen, maltr\u00e4tiert,", "tokens": ["Ge\u00b7qu\u00e4lt", ",", "ge\u00b7schla\u00b7gen", ",", "malt\u00b7r\u00e4\u00b7tiert", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von fr\u00fch bis in die sp\u00e4te Nacht,", "tokens": ["Von", "fr\u00fch", "bis", "in", "die", "sp\u00e4\u00b7te", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit rohen Worten kujoniert,", "tokens": ["Mit", "ro\u00b7hen", "Wor\u00b7ten", "ku\u00b7jo\u00b7niert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Um Lachen und um Lust gebracht,", "tokens": ["Um", "La\u00b7chen", "und", "um", "Lust", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "KON", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es trat ihn wie ein schmutzig Tier", "tokens": ["Es", "trat", "ihn", "wie", "ein", "schmut\u00b7zig", "Tier"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "KOKOM", "ART", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der rohe Unteroffizier \u2013", "tokens": ["Der", "ro\u00b7he", "Un\u00b7ter\u00b7of\u00b7fi\u00b7zier", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da ri\u00df vom Koppel er das Erz", "tokens": ["Da", "ri\u00df", "vom", "Kop\u00b7pel", "er", "das", "Erz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und stie\u00df es ihm ins Herz.", "tokens": ["Und", "stie\u00df", "es", "ihm", "ins", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Tags drauf im Milit\u00e4rgericht", "tokens": ["Tags", "drauf", "im", "Mi\u00b7li\u00b7t\u00e4r\u00b7ge\u00b7richt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PAV", "APPRART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Sprach man ihm Leib und Leben ab,", "tokens": ["Sprach", "man", "ihm", "Leib", "und", "Le\u00b7ben", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "PPER", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df brav er sonst, das z\u00e4hlte nicht,", "tokens": ["Da\u00df", "brav", "er", "sonst", ",", "das", "z\u00e4hl\u00b7te", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "ADV", "$,", "PDS", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr Wahnsinnstat ein schnelles Grab.", "tokens": ["F\u00fcr", "Wahn\u00b7sinns\u00b7tat", "ein", "schnel\u00b7les", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u00bbkein Mitleid darf dem Hunde bl\u00fchn,", "tokens": ["\u00bb", "kein", "Mit\u00b7leid", "darf", "dem", "Hun\u00b7de", "bl\u00fchn", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIAT", "NN", "VMFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wo bliebe sonst die Disziplin?\u00ab", "tokens": ["Wo", "blie\u00b7be", "sonst", "die", "Dis\u00b7zip\u00b7lin", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sie schossen tot ihn in der Fr\u00fch \u2013", "tokens": ["Sie", "schos\u00b7sen", "tot", "ihn", "in", "der", "Fr\u00fch", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das Blut vertrocknet nie.", "tokens": ["Das", "Blut", "ver\u00b7trock\u00b7net", "nie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}