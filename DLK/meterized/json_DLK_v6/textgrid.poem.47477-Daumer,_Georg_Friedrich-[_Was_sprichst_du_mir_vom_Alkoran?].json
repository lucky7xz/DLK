{"textgrid.poem.47477": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[ Was sprichst du mir vom Alkoran?]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was sprichst du mir vom Alkoran?", "tokens": ["Was", "sprichst", "du", "mir", "vom", "Al\u00b7ko\u00b7ran", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Studir' ich ihn so eben nicht?", "tokens": ["Stu\u00b7dir'", "ich", "ihn", "so", "e\u00b7ben", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich starre nach der Liebsten Aug'", "tokens": ["Ich", "star\u00b7re", "nach", "der", "Liebs\u00b7ten", "Aug'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und les' in ihrem Angesicht;", "tokens": ["Und", "les'", "in", "ih\u00b7rem", "An\u00b7ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Sure \u00bb", "tokens": ["Die", "Su\u00b7re", "\u00bb"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Ich lese hier die Sure \u00bb", "tokens": ["Ich", "le\u00b7se", "hier", "die", "Su\u00b7re", "\u00bb"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}