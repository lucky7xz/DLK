{"textgrid.poem.45136": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Der kleinen Hohenwart", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was du zu viel hast, macht mich nicht betr\u00fcbt,", "tokens": ["Was", "du", "zu", "viel", "hast", ",", "macht", "mich", "nicht", "be\u00b7tr\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKA", "PIS", "VAFIN", "$,", "VVFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Zeit nimmt leider mehr uns, als sie gibt,", "tokens": ["Die", "Zeit", "nimmt", "lei\u00b7der", "mehr", "uns", ",", "als", "sie", "gibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und die Lebendigkeit, wie bunt sies treibe,", "tokens": ["Und", "die", "Le\u00b7ben\u00b7dig\u00b7keit", ",", "wie", "bunt", "sies", "trei\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PWAV", "ADJD", "PIS", "VVFIN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Sch\u00fctzt vor dem Totsein bei lebendgem Leibe.", "tokens": ["Sch\u00fctzt", "vor", "dem", "Tot\u00b7sein", "bei", "le\u00b7bend\u00b7gem", "Lei\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}