{"textgrid.poem.55568": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "1L: Veilchen bring ich getragen,", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Veilchen bring ich getragen,", "tokens": ["Veil\u00b7chen", "bring", "ich", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "VVPP", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Junge Bl\u00fcten zu dir,", "tokens": ["Jun\u00b7ge", "Bl\u00fc\u00b7ten", "zu", "dir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPER", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Da\u00df ich dein moosig Haupt", "tokens": ["Da\u00df", "ich", "dein", "moo\u00b7sig", "Haupt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ringsum bekr\u00e4nze,", "tokens": ["Ring\u00b7sum", "be\u00b7kr\u00e4n\u00b7ze", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Ringsum dich weihe,", "tokens": ["Ring\u00b7sum", "dich", "wei\u00b7he", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Felsen des Tals.", "tokens": ["Fel\u00b7sen", "des", "Tals", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Sei du mir heilig!", "tokens": ["Sei", "du", "mir", "hei\u00b7lig", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADJD", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Sei den Geliebten", "tokens": ["Sei", "den", "Ge\u00b7lieb\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Lieber als andre", "tokens": ["Lie\u00b7ber", "als", "and\u00b7re"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "KOKOM", "PIS"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Felsen des Tals!", "tokens": ["Fel\u00b7sen", "des", "Tals", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Ich sah von dir", "tokens": ["Ich", "sah", "von", "dir"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Der Freunde Seligkeit,", "tokens": ["Der", "Freun\u00b7de", "Se\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verbunden Edle", "tokens": ["Ver\u00b7bun\u00b7den", "Ed\u00b7le"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Mit ew'gem Band.", "tokens": ["Mit", "ew'\u00b7gem", "Band", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Ich irrer Wandrer", "tokens": ["Ich", "ir\u00b7rer", "Wand\u00b7rer"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "F\u00fchlt erst auf dir", "tokens": ["F\u00fchlt", "erst", "auf", "dir"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Besitztumsfreuden", "tokens": ["Be\u00b7sitz\u00b7tums\u00b7freu\u00b7den"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Und Heimatsgl\u00fcck.", "tokens": ["Und", "Hei\u00b7mats\u00b7gl\u00fcck", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Da, wo wir lieben,", "tokens": ["Da", ",", "wo", "wir", "lie\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Ist Vaterland;", "tokens": ["Ist", "Va\u00b7ter\u00b7land", ";"], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wo wir genie\u00dfen,", "tokens": ["Wo", "wir", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ist Hof und Haus.", "tokens": ["Ist", "Hof", "und", "Haus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Schrieb meinen Namen", "tokens": ["Schrieb", "mei\u00b7nen", "Na\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "An deine Stirn;", "tokens": ["An", "dei\u00b7ne", "Stirn", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Du bist mir eigen,", "tokens": ["Du", "bist", "mir", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Mir Ruhesitz.", "tokens": ["Mir", "Ru\u00b7he\u00b7sitz", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Und aus dem fernen", "tokens": ["Und", "aus", "dem", "fer\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Unlieben Land", "tokens": ["Un\u00b7lie\u00b7ben", "Land"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Mein Geist wird wandern", "tokens": ["Mein", "Geist", "wird", "wan\u00b7dern"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Und ruhn auf dir.", "tokens": ["Und", "ruhn", "auf", "dir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Sei du mir heilig,", "tokens": ["Sei", "du", "mir", "hei\u00b7lig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADJD", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Sei den Geliebten", "tokens": ["Sei", "den", "Ge\u00b7lieb\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Lieber als andre", "tokens": ["Lie\u00b7ber", "als", "and\u00b7re"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "KOKOM", "PIS"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Felsen des Tals!", "tokens": ["Fel\u00b7sen", "des", "Tals", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.9": {"line.1": {"text": "Ich sehe sie versammelt", "tokens": ["Ich", "se\u00b7he", "sie", "ver\u00b7sam\u00b7melt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dort unten um den Teich;", "tokens": ["Dort", "un\u00b7ten", "um", "den", "Teich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie tanzen einen Reihen", "tokens": ["Sie", "tan\u00b7zen", "ei\u00b7nen", "Rei\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Sommerabendrot;", "tokens": ["Im", "Som\u00b7mer\u00b7a\u00b7ben\u00b7drot", ";"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und warme Jugendfreude", "tokens": ["Und", "war\u00b7me", "Ju\u00b7gend\u00b7freu\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Webt in dem Abendrot.", "tokens": ["Webt", "in", "dem", "A\u00b7ben\u00b7drot", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.10": {"line.1": {"text": "Sie dr\u00fccken sich die H\u00e4nde", "tokens": ["Sie", "dr\u00fc\u00b7cken", "sich", "die", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und gl\u00fchn einander an.", "tokens": ["Und", "gl\u00fchn", "ein\u00b7an\u00b7der", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und aus den Reihn verlieret", "tokens": ["Und", "aus", "den", "Reihn", "ver\u00b7lie\u00b7ret"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sich Psyche zwischen Felsen", "tokens": ["Sich", "Psy\u00b7che", "zwi\u00b7schen", "Fel\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und Str\u00e4uchen weg, und traurend", "tokens": ["Und", "Str\u00e4u\u00b7chen", "weg", ",", "und", "trau\u00b7rend"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "NN", "PTKVZ", "$,", "KON", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Um den Abwesenden,", "tokens": ["Um", "den", "Ab\u00b7we\u00b7sen\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Lehnt sie sich \u00fcber den Fels.", "tokens": ["Lehnt", "sie", "sich", "\u00fc\u00b7ber", "den", "Fels", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.8": {"text": "Wo meine Brust hier ruht,", "tokens": ["Wo", "mei\u00b7ne", "Brust", "hier", "ruht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "An das Moos mit innigem", "tokens": ["An", "das", "Moos", "mit", "in\u00b7ni\u00b7gem"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.10": {"text": "Liebesgef\u00fchl sich", "tokens": ["Lie\u00b7bes\u00b7ge\u00b7f\u00fchl", "sich"], "token_info": ["word", "word"], "pos": ["NN", "PRF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Atmend dr\u00e4ngt,", "tokens": ["At\u00b7mend", "dr\u00e4ngt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.12": {"text": "Ruhst du vielleicht dann, Psyche.", "tokens": ["Ruhst", "du", "viel\u00b7leicht", "dann", ",", "Psy\u00b7che", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$,", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.13": {"text": "Tr\u00fcbe blickt dein Aug", "tokens": ["Tr\u00fc\u00b7be", "blickt", "dein", "Aug"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.14": {"text": "In den Bach hinab,", "tokens": ["In", "den", "Bach", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.15": {"text": "Und eine Tr\u00e4ne quillt", "tokens": ["Und", "ei\u00b7ne", "Tr\u00e4\u00b7ne", "quillt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Vorbeigequollnen Freuden nach;", "tokens": ["Vor\u00b7bei\u00b7ge\u00b7quoll\u00b7nen", "Freu\u00b7den", "nach", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Hebst dann zum Himmel", "tokens": ["Hebst", "dann", "zum", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPRART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.18": {"text": "Dein bittend Aug,", "tokens": ["Dein", "bit\u00b7tend", "Aug", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.19": {"text": "Erblickest \u00fcber dir", "tokens": ["Er\u00b7bli\u00b7ckest", "\u00fc\u00b7ber", "dir"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.20": {"text": "Da meinen Namen.", "tokens": ["Da", "mei\u00b7nen", "Na\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.21": {"text": "\u2013 Auch der \u2013", "tokens": ["\u2013", "Auch", "der", "\u2013"], "token_info": ["punct", "word", "word", "punct"], "pos": ["$(", "ADV", "ART", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.22": {"text": "Nimm des verlebten Tages Zier,", "tokens": ["Nimm", "des", "ver\u00b7leb\u00b7ten", "Ta\u00b7ges", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Die bald welke Rose, von deinem Busen,", "tokens": ["Die", "bald", "wel\u00b7ke", "Ro\u00b7se", ",", "von", "dei\u00b7nem", "Bu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.24": {"text": "Streu die freundlichen Bl\u00e4tter", "tokens": ["Streu", "die", "freund\u00b7li\u00b7chen", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.25": {"text": "\u00dcbers d\u00fcstre Moos,", "tokens": ["\u00dc\u00b7bers", "d\u00fcst\u00b7re", "Moos", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.26": {"text": "Ein Opfer der Zukunft!", "tokens": ["Ein", "Op\u00b7fer", "der", "Zu\u00b7kunft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}