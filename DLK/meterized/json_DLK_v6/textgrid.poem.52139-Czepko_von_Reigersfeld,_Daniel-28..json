{"textgrid.poem.52139": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "28.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Wort, das aller Welt den Frieden saget an,", "tokens": ["Das", "Wort", ",", "das", "al\u00b7ler", "Welt", "den", "Frie\u00b7den", "sa\u00b7get", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIAT", "NN", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Gnade heist und uns aus Gnade wird gegeben,", "tokens": ["Und", "Gna\u00b7de", "heist", "und", "uns", "aus", "Gna\u00b7de", "wird", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "KON", "PPER", "APPR", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das soll (Ach Schand! ach Spott!) der wilde Krieg erheben,", "tokens": ["Das", "soll", "(", "Ach", "Schand", "!", "ach", "Spott", "!", ")", "der", "wil\u00b7de", "Krieg", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "$(", "ITJ", "NN", "$.", "XY", "NN", "$.", "$(", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und durch verbotne Dienst, ihm \u00f6ffnen Th\u00fcr und Bahn.", "tokens": ["Und", "durch", "ver\u00b7bot\u00b7ne", "Dienst", ",", "ihm", "\u00f6ff\u00b7nen", "Th\u00fcr", "und", "Bahn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,", "PPER", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie arg ist doch die Welt, wir wollen Christen seyn,", "tokens": ["Wie", "arg", "ist", "doch", "die", "Welt", ",", "wir", "wol\u00b7len", "Chris\u00b7ten", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ADV", "ART", "NN", "$,", "PPER", "VMFIN", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wann wir den Mantel dann vom Gottes Dienst entdecken,", "tokens": ["Wann", "wir", "den", "Man\u00b7tel", "dann", "vom", "Got\u00b7tes", "Dienst", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ADV", "APPRART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So sieht man Raub und Brand und Mort darunter stecken,", "tokens": ["So", "sieht", "man", "Raub", "und", "Brand", "und", "Mort", "da\u00b7run\u00b7ter", "ste\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "NN", "KON", "NN", "KON", "NN", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und h\u00fcllen nichts als Schuld und S\u00fcnd in Glauben ein.", "tokens": ["Und", "h\u00fcl\u00b7len", "nichts", "als", "Schuld", "und", "S\u00fcnd", "in", "Glau\u00b7ben", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "KOKOM", "NN", "KON", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wollt den Bedr\u00e4ngten ihr also zu H\u00fclffe gehn,", "tokens": ["Wollt", "den", "Be\u00b7dr\u00e4ng\u00b7ten", "ihr", "al\u00b7so", "zu", "H\u00fclf\u00b7fe", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PPER", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Da\u00df ihr sie h\u00e4rter k\u00f6nnt durch falschen Schein bedr\u00e4ngen,", "tokens": ["Da\u00df", "ihr", "sie", "h\u00e4r\u00b7ter", "k\u00f6nnt", "durch", "fal\u00b7schen", "Schein", "be\u00b7dr\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VVFIN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie heilge Titul ihr wollt um die Waffen h\u00e4ngen,", "tokens": ["Wie", "heil\u00b7ge", "Ti\u00b7tul", "ihr", "wollt", "um", "die", "Waf\u00b7fen", "h\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "PPER", "VMFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Sieht man doch \u00fcberall die Kr\u00e4le fornen stehn.", "tokens": ["Sieht", "man", "doch", "\u00fc\u00b7be\u00b7rall", "die", "Kr\u00e4\u00b7le", "for\u00b7nen", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ADV", "ART", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Das liebe Gottes Wort kommt uns sehr theuer an,", "tokens": ["Das", "lie\u00b7be", "Got\u00b7tes", "Wort", "kommt", "uns", "sehr", "theu\u00b7er", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "PPER", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr predigt dergestalt (bedenckt, ihr seyd ja Christen)", "tokens": ["Ihr", "pre\u00b7digt", "der\u00b7ge\u00b7stalt", "(", "be\u00b7denckt", ",", "ihr", "seyd", "ja", "Chris\u00b7ten", ")"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$(", "VVFIN", "$,", "PPER", "VAFIN", "ADV", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das Vieh aus St\u00e4llen hin, die Heller aus den Kisten:", "tokens": ["Das", "Vieh", "aus", "St\u00e4l\u00b7len", "hin", ",", "die", "Hel\u00b7ler", "aus", "den", "Kis\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "PTKVZ", "$,", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und endlich denn aus Land und St\u00e4dten iedermann.", "tokens": ["Und", "end\u00b7lich", "denn", "aus", "Land", "und", "St\u00e4d\u00b7ten", "ie\u00b7der\u00b7mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPR", "NN", "KON", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}