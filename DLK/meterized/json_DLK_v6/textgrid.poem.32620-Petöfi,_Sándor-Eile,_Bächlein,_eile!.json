{"textgrid.poem.32620": {"metadata": {"author": {"name": "Pet\u00f6fi, S\u00e1ndor", "birth": "N.A.", "death": "N.A."}, "title": "Eile, B\u00e4chlein, eile!", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eile, B\u00e4chlein, eile,", "tokens": ["Ei\u00b7le", ",", "B\u00e4ch\u00b7lein", ",", "ei\u00b7le", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ohne Rast und Weile,", "tokens": ["Oh\u00b7ne", "Rast", "und", "Wei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df dir bald im Meere", "tokens": ["Da\u00df", "dir", "bald", "im", "Mee\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "NN"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Ruh' beschieden w\u00e4re!", "tokens": ["Ruh'", "be\u00b7schie\u00b7den", "w\u00e4\u00b7re", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVPP", "VAFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Flie\u00dfet, flie\u00dft, ihr Tr\u00e4nen,", "tokens": ["Flie\u00b7\u00dfet", ",", "flie\u00dft", ",", "ihr", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Bis der Tod, mein Sehnen", "tokens": ["Bis", "der", "Tod", ",", "mein", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Kennt, und ich im Grabe", "tokens": ["Kennt", ",", "und", "ich", "im", "Gra\u00b7be"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KON", "PPER", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ruh' gefunden habe!", "tokens": ["Ruh'", "ge\u00b7fun\u00b7den", "ha\u00b7be", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVPP", "VAFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}