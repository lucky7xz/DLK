{"textgrid.poem.60327": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Niagara", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Klar und wie die Jugend heiter,", "tokens": ["Klar", "und", "wie", "die", "Ju\u00b7gend", "hei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "PWAV", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und wie murmelnd s\u00fc\u00dfen Traum,", "tokens": ["Und", "wie", "mur\u00b7melnd", "s\u00fc\u00b7\u00dfen", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADJD", "ADJA", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Zieht der Niagara weiter", "tokens": ["Zieht", "der", "Nia\u00b7ga\u00b7ra", "wei\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NE", "ADV"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "An des Urwalds gr\u00fcnem Saum;", "tokens": ["An", "des", "Ur\u00b7walds", "gr\u00fc\u00b7nem", "Saum", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Zieht dahin im sanften Flusse,", "tokens": ["Zieht", "da\u00b7hin", "im", "sanf\u00b7ten", "Flus\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df er noch des Waldes Pracht", "tokens": ["Da\u00df", "er", "noch", "des", "Wal\u00b7des", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Widerstrahlt mit froher Mu\u00dfe", "tokens": ["Wi\u00b7der\u00b7strahlt", "mit", "fro\u00b7her", "Mu\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Sterne stiller Nacht.", "tokens": ["Und", "die", "Ster\u00b7ne", "stil\u00b7ler", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Also sanft die Wellen gleiten,", "tokens": ["Al\u00b7so", "sanft", "die", "Wel\u00b7len", "glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df der Wandrer ungest\u00f6rt", "tokens": ["Da\u00df", "der", "Wand\u00b7rer", "un\u00b7ge\u00b7st\u00f6rt"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und erstaunt die meilenweiten", "tokens": ["Und", "er\u00b7staunt", "die", "mei\u00b7len\u00b7wei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Katarakte rauschen h\u00f6rt.", "tokens": ["Ka\u00b7ta\u00b7rak\u00b7te", "rau\u00b7schen", "h\u00f6rt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wo des Niagara Bahnen", "tokens": ["Wo", "des", "Nia\u00b7ga\u00b7ra", "Bah\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NE", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "N\u00e4her ziehn dem Katarakt,", "tokens": ["N\u00e4\u00b7her", "ziehn", "dem", "Ka\u00b7ta\u00b7rakt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat den Strom ein wildes Ahnen", "tokens": ["Hat", "den", "Strom", "ein", "wil\u00b7des", "Ah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Pl\u00f6tzlich seines Falls gepackt.", "tokens": ["Pl\u00f6tz\u00b7lich", "sei\u00b7nes", "Falls", "ge\u00b7packt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Erd und Himmels unbek\u00fcmmert", "tokens": ["Erd", "und", "Him\u00b7mels", "un\u00b7be\u00b7k\u00fcm\u00b7mert"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eilt er jetzt im tollen Zug,", "tokens": ["Eilt", "er", "jetzt", "im", "tol\u00b7len", "Zug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat ihr sch\u00f6nes Bild zertr\u00fcmmert,", "tokens": ["Hat", "ihr", "sch\u00f6\u00b7nes", "Bild", "zer\u00b7tr\u00fcm\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das er erst so freundlich trug.", "tokens": ["Das", "er", "erst", "so", "freund\u00b7lich", "trug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Die Stromschnellen st\u00fcrzen, schie\u00dfen,", "tokens": ["Die", "Strom\u00b7schnel\u00b7len", "st\u00fcr\u00b7zen", ",", "schie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Donnern fort im wilden Drang,", "tokens": ["Don\u00b7nern", "fort", "im", "wil\u00b7den", "Drang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie von Sehnsucht hingerissen", "tokens": ["Wie", "von", "Sehn\u00b7sucht", "hin\u00b7ge\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nach dem gro\u00dfen Untergang.", "tokens": ["Nach", "dem", "gro\u00b7\u00dfen", "Un\u00b7ter\u00b7gang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Den der Wandrer fern vernommen,", "tokens": ["Den", "der", "Wand\u00b7rer", "fern", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Niagaras tiefen Fall", "tokens": ["Nia\u00b7ga\u00b7ras", "tie\u00b7fen", "Fall"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "H\u00f6rt er nicht, herangekommen,", "tokens": ["H\u00f6rt", "er", "nicht", ",", "her\u00b7an\u00b7ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil zu laut der Wogenschall.", "tokens": ["Weil", "zu", "laut", "der", "Wo\u00b7gen\u00b7schall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Und so mag vergebens lauschen,", "tokens": ["Und", "so", "mag", "ver\u00b7ge\u00b7bens", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wer dem Sturze n\u00e4her geht;", "tokens": ["Wer", "dem", "Stur\u00b7ze", "n\u00e4\u00b7her", "geht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch die Zukunft h\u00f6rte rauschen", "tokens": ["Doch", "die", "Zu\u00b7kunft", "h\u00f6r\u00b7te", "rau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In der Ferne der Prophet.", "tokens": ["In", "der", "Fer\u00b7ne", "der", "Pro\u00b7phet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}