{"textgrid.poem.52602": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "13. (Faust)", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach! die vergangenen Freuden", "tokens": ["Ach", "!", "die", "ver\u00b7gan\u00b7ge\u00b7nen", "Freu\u00b7den"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die waren wohl so sch\u00f6n!", "tokens": ["Die", "wa\u00b7ren", "wohl", "so", "sch\u00f6n", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Jetzt will mir Alles verleiden", "tokens": ["Jetzt", "will", "mir", "Al\u00b7les", "ver\u00b7lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PIS", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Verwehen und vergehn.", "tokens": ["Ver\u00b7we\u00b7hen", "und", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es bricht mir \u00fcberm Haupte", "tokens": ["Es", "bricht", "mir", "\u00fc\u00b7berm", "Haup\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die finstre Zeit herein,", "tokens": ["Die", "finst\u00b7re", "Zeit", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und was ich ", "tokens": ["Und", "was", "ich"], "token_info": ["word", "word", "word"], "pos": ["KON", "PWS", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Das trifft allm\u00e4lig ein.", "tokens": ["Das", "trifft", "all\u00b7m\u00e4\u00b7lig", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}