{"textgrid.poem.48205": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Aber es bleibt auf dem alten Fleck", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwie konnt' ich das tun, wie konnt' ich ", "tokens": ["\u00bb", "wie", "konnt'", "ich", "das", "tun", ",", "wie", "konnt'", "ich"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PWAV", "VMFIN", "PPER", "PDS", "VVINF", "$,", "PWAV", "VMFIN", "PPER"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "So h\u00f6rt man nicht auf, sich anzuklagen,", "tokens": ["So", "h\u00f6rt", "man", "nicht", "auf", ",", "sich", "an\u00b7zu\u00b7kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PTKNEG", "PTKVZ", "$,", "PRF", "VVIZU", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Bei jeder Dummheit, bei jedem Verlieren", "tokens": ["Bei", "je\u00b7der", "Dumm\u00b7heit", ",", "bei", "je\u00b7dem", "Ver\u00b7lie\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "APPR", "PIAT", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Hei\u00dft es: \u00bbDas soll dir nicht wieder passieren.\u00ab", "tokens": ["Hei\u00dft", "es", ":", "\u00bb", "Das", "soll", "dir", "nicht", "wie\u00b7der", "pas\u00b7sie\u00b7ren", ".", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "$.", "$(", "PDS", "VMFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+++-+-", "measure": "unknown.measure.hexa"}}, "stanza.2": {"line.1": {"text": "Irrtum! Heut traf es blo\u00df Kunzen und Hinzen,", "tokens": ["Irr\u00b7tum", "!", "Heut", "traf", "es", "blo\u00df", "Kun\u00b7zen", "und", "Hin\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VVFIN", "PPER", "ADV", "NN", "KON", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Morgen trifft es schon ganze Provinzen,", "tokens": ["Mor\u00b7gen", "trifft", "es", "schon", "gan\u00b7ze", "Pro\u00b7vin\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Am dritten Tag ganze Konfessionen,", "tokens": ["Am", "drit\u00b7ten", "Tag", "gan\u00b7ze", "Kon\u00b7fes\u00b7si\u00b7o\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Oder die \u00bbRassen, die zwischen uns wohnen\u00ab,", "tokens": ["O\u00b7der", "die", "\u00bb", "Ras\u00b7sen", ",", "die", "zwi\u00b7schen", "uns", "woh\u00b7nen", "\u00ab", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "$(", "NN", "$,", "PRELS", "APPR", "PPER", "VVINF", "$(", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Immer kriegt man einen Schreck,", "tokens": ["Im\u00b7mer", "kriegt", "man", "ei\u00b7nen", "Schreck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Aber es bleibt auf dem alten Fleck.", "tokens": ["A\u00b7ber", "es", "bleibt", "auf", "dem", "al\u00b7ten", "Fleck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}}}}