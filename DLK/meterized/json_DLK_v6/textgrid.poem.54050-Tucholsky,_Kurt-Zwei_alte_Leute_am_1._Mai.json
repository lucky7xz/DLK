{"textgrid.poem.54050": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Zwei alte Leute am 1. Mai", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u2013 \u00bbWei\u00dft du noch, Alter, vor dem Kriege?", "tokens": ["\u2013", "\u00bb", "Wei\u00dft", "du", "noch", ",", "Al\u00b7ter", ",", "vor", "dem", "Krie\u00b7ge", "?"], "token_info": ["punct", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "$(", "VVFIN", "PPER", "ADV", "$,", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wir haben manchen Mai erlebt.", "tokens": ["Wir", "ha\u00b7ben", "man\u00b7chen", "Mai", "er\u00b7lebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir glaubten an die schnellen Siege \u2013", "tokens": ["Wir", "glaub\u00b7ten", "an", "die", "schnel\u00b7len", "Sie\u00b7ge", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "du hast das Streikplakat geklebt . . . \u00ab", "tokens": ["du", "hast", "das", "Streik\u00b7pla\u00b7kat", "ge\u00b7klebt", ".", ".", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$.", "$.", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u2013 \u00bbJa, Alte, das waren sch\u00f6ne Zeiten . . .", "tokens": ["\u2013", "\u00bb", "Ja", ",", "Al\u00b7te", ",", "das", "wa\u00b7ren", "sch\u00f6\u00b7ne", "Zei\u00b7ten", ".", ".", "."], "token_info": ["punct", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "$(", "PTKANT", "$,", "NN", "$,", "PDS", "VAFIN", "ADJA", "NN", "$.", "$.", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wir waren allemal dabei \u2013", "tokens": ["Wir", "wa\u00b7ren", "al\u00b7le\u00b7mal", "da\u00b7bei", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PAV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich seh uns noch im Zuge schreiten", "tokens": ["Ich", "seh", "uns", "noch", "im", "Zu\u00b7ge", "schrei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "am 1. Mai.\u00ab", "tokens": ["am", "1.", "Mai", ".", "\u00ab"], "token_info": ["word", "ordinal", "word", "punct", "punct"], "pos": ["APPRART", "ADJA", "NN", "$.", "$("], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "\u2013 \u00bbUnd unser J\u00fcngster war noch klein. Den lie\u00df ich", "tokens": ["\u2013", "\u00bb", "Und", "un\u00b7ser", "J\u00fcngs\u00b7ter", "war", "noch", "klein", ".", "Den", "lie\u00df", "ich"], "token_info": ["punct", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "$(", "KON", "PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$.", "NE", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "zu Haus . . . wir gingen los mit Hans.", "tokens": ["zu", "Haus", ".", ".", ".", "wir", "gin\u00b7gen", "los", "mit", "Hans", "."], "token_info": ["word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$.", "$.", "$.", "PPER", "VVFIN", "PTKVZ", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mitunter wars ja etwas spie\u00dfig \u2013", "tokens": ["Mi\u00b7tun\u00b7ter", "wars", "ja", "et\u00b7was", "spie\u00b7\u00dfig", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "so . . . Kriegerverein mit Kaffeekranz.\u00ab", "tokens": ["so", ".", ".", ".", "Krie\u00b7ger\u00b7ver\u00b7ein", "mit", "Kaf\u00b7fee\u00b7kranz", ".", "\u00ab"], "token_info": ["word", "punct", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "$.", "$.", "$.", "NN", "APPR", "NN", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "\u2013 \u00bbNa, la\u00df man \u2013 du warst doch die Nettste!", "tokens": ["\u2013", "\u00bb", "Na", ",", "la\u00df", "man", "\u2013", "du", "warst", "doch", "die", "Netts\u00b7te", "!"], "token_info": ["punct", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "ITJ", "$,", "VVIMP", "PIS", "$(", "PPER", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Mir wars blo\u00df zu viel Dudelei . . .", "tokens": ["Mir", "wars", "blo\u00df", "zu", "viel", "Du\u00b7de\u00b7lei", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PIAT", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und anno 14 wars denn auch der letzte \u2013", "tokens": ["Und", "an\u00b7no", "14", "wars", "denn", "auch", "der", "letz\u00b7te", "\u2013"], "token_info": ["word", "word", "number", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "CARD", "VAFIN", "ADV", "ADV", "ART", "ADJA", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "der 1. Mai.\u00ab", "tokens": ["der", "1.", "Mai", ".", "\u00ab"], "token_info": ["word", "ordinal", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$("], "meter": "-+", "measure": "iambic.single"}}, "stanza.3": {"line.1": {"text": "\u2013 \u00bbKein Wunder. Mu\u00dft mal denken, Alter:", "tokens": ["\u2013", "\u00bb", "Kein", "Wun\u00b7der", ".", "Mu\u00dft", "mal", "den\u00b7ken", ",", "Al\u00b7ter", ":"], "token_info": ["punct", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "$(", "PIAT", "NN", "$.", "VMFIN", "ADV", "VVINF", "$,", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wer ist uns da voraufmarschiert!", "tokens": ["Wer", "ist", "uns", "da", "vor\u00b7auf\u00b7mar\u00b7schiert", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Wels als roter Fahnenhalter,", "tokens": ["Der", "Wels", "als", "ro\u00b7ter", "Fah\u00b7nen\u00b7hal\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOUS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "der L\u00f6be, pr\u00e4chtig ausstaffiert . . . \u00ab", "tokens": ["der", "L\u00f6\u00b7be", ",", "pr\u00e4ch\u00b7tig", "aus\u00b7staf\u00b7fiert", ".", ".", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "VVPP", "$.", "$.", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u2013 \u00bbJa solche haben glatte H\u00e4nde . . .", "tokens": ["\u2013", "\u00bb", "Ja", "sol\u00b7che", "ha\u00b7ben", "glat\u00b7te", "H\u00e4n\u00b7de", ".", ".", "."], "token_info": ["punct", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "$(", "PTKANT", "PIAT", "VAFIN", "ADJA", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "F\u00fcr die ist frisch, fromm, frech und frei", "tokens": ["F\u00fcr", "die", "ist", "frisch", ",", "fromm", ",", "frech", "und", "frei"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "VAFIN", "ADJD", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "der Klassenkampf schon l\u00e4ngst zu Ende \u2013", "tokens": ["der", "Klas\u00b7sen\u00b7kampf", "schon", "l\u00e4ngst", "zu", "En\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "APPR", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die und der 1. Mai!", "tokens": ["Die", "und", "der", "1.", "Mai", "!"], "token_info": ["word", "word", "word", "ordinal", "word", "punct"], "pos": ["ART", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Was wissen die vom Klassenkrieg . . . !", "tokens": ["Was", "wis\u00b7sen", "die", "vom", "Klas\u00b7sen\u00b7krieg", ".", ".", ".", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["PWS", "VVFIN", "ART", "APPRART", "NN", "$.", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Die sch\u00fctzen sich vor ihrer eigenen Republik \u2013!\u00ab", "tokens": ["Die", "sch\u00fct\u00b7zen", "sich", "vor", "ih\u00b7rer", "ei\u00b7ge\u00b7nen", "Re\u00b7pub\u00b7lik", "\u2013", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$(", "$.", "$("], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "\u2013 \u00bbNa, la\u00df man, Alter, die Beschwerde.", "tokens": ["\u2013", "\u00bb", "Na", ",", "la\u00df", "man", ",", "Al\u00b7ter", ",", "die", "Be\u00b7schwer\u00b7de", "."], "token_info": ["punct", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "$(", "ITJ", "$,", "VVIMP", "PIS", "$,", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich wei\u00df, da\u00df etwas in uns singt:", "tokens": ["Ich", "wei\u00df", ",", "da\u00df", "et\u00b7was", "in", "uns", "singt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PIS", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wacht auf, Verdammte dieser Erde,", "tokens": ["Wacht", "auf", ",", "Ver\u00b7damm\u00b7te", "die\u00b7ser", "Er\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "die stets man noch zum Hungern zwingt!\u00ab", "tokens": ["die", "stets", "man", "noch", "zum", "Hun\u00b7gern", "zwingt", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADV", "PIS", "ADV", "APPRART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u2013 \u00bbWir wissen, Alte, was wir lieben:", "tokens": ["\u2013", "\u00bb", "Wir", "wis\u00b7sen", ",", "Al\u00b7te", ",", "was", "wir", "lie\u00b7ben", ":"], "token_info": ["punct", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "$(", "PPER", "VVFIN", "$,", "NN", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "den Klassenkampf und die Partei!", "tokens": ["den", "Klas\u00b7sen\u00b7kampf", "und", "die", "Par\u00b7tei", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wir sind ja doch die Alten geblieben", "tokens": ["Wir", "sind", "ja", "doch", "die", "Al\u00b7ten", "ge\u00b7blie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "NN", "VVPP"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "am 1. Mai! Am 1. Mai!\u00ab", "tokens": ["am", "1.", "Mai", "!", "Am", "1.", "Mai", "!", "\u00ab"], "token_info": ["word", "ordinal", "word", "punct", "word", "ordinal", "word", "punct", "punct"], "pos": ["APPRART", "ADJA", "NN", "$.", "APPRART", "ADJA", "NN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}}}}