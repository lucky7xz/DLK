{"textgrid.poem.64047": {"metadata": {"author": {"name": "Ahlefeld, Charlotte von", "birth": "N.A.", "death": "N.A."}, "title": "Sehnsucht", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.85", "no:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn Philomelens bange Liebesklage", "tokens": ["Wenn", "Phi\u00b7lo\u00b7me\u00b7lens", "ban\u00b7ge", "Lie\u00b7bes\u00b7kla\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NE", "ADV", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mir neu ert\u00f6nt im leisen Pappelhain,", "tokens": ["Mir", "neu", "er\u00b7t\u00f6nt", "im", "lei\u00b7sen", "Pap\u00b7pel\u00b7hain", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da denk' ich sehnend der vergangnen Tage,", "tokens": ["Da", "denk'", "ich", "seh\u00b7nend", "der", "ver\u00b7gang\u00b7nen", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und seufze schmerzlich: ach, ich bin allein!", "tokens": ["Und", "seuf\u00b7ze", "schmerz\u00b7lich", ":", "ach", ",", "ich", "bin", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$.", "XY", "$,", "PPER", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O f\u00fchltest ", "tokens": ["O", "f\u00fchl\u00b7test"], "token_info": ["word", "word"], "pos": ["NE", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Das neu erwacht, rings um mich her sich regt,", "tokens": ["Das", "neu", "er\u00b7wacht", ",", "rings", "um", "mich", "her", "sich", "regt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVPP", "$,", "ADV", "APPR", "PRF", "APZR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das Leben der Natur, die mit dem ew'gen Streben", "tokens": ["Das", "Le\u00b7ben", "der", "Na\u00b7tur", ",", "die", "mit", "dem", "ew'\u00b7gen", "Stre\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Im Jugendglanz sich jetzt empor bewegt.", "tokens": ["Im", "Ju\u00b7gend\u00b7glanz", "sich", "jetzt", "em\u00b7por", "be\u00b7wegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "ADV", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Denn zwiefach sch\u00f6n war mir des Jahres Morgen", "tokens": ["Denn", "zwie\u00b7fach", "sch\u00f6n", "war", "mir", "des", "Jah\u00b7res", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit seinem holden L\u00e4cheln neben Dir.", "tokens": ["Mit", "sei\u00b7nem", "hol\u00b7den", "L\u00e4\u00b7cheln", "ne\u00b7ben", "Dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O banne schnell der Liebe leise Sorgen,", "tokens": ["O", "ban\u00b7ne", "schnell", "der", "Lie\u00b7be", "lei\u00b7se", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und eil' auf ihren Fl\u00fcgeln her zu mir.", "tokens": ["Und", "eil'", "auf", "ih\u00b7ren", "Fl\u00fc\u00b7geln", "her", "zu", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "APZR", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dann will ich Dir die sch\u00f6nsten Kr\u00e4nze binden,", "tokens": ["Dann", "will", "ich", "Dir", "die", "sch\u00f6ns\u00b7ten", "Kr\u00e4n\u00b7ze", "bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die mir des Fr\u00fchlings bunter Segen beut.", "tokens": ["Die", "mir", "des", "Fr\u00fch\u00b7lings", "bun\u00b7ter", "Se\u00b7gen", "beut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gesellig soll sich Epheu um sie winden,", "tokens": ["Ge\u00b7sel\u00b7lig", "soll", "sich", "E\u00b7pheu", "um", "sie", "win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PRF", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das als der Treue Sinnbild Dich erfreut.", "tokens": ["Das", "als", "der", "Treu\u00b7e", "Sinn\u00b7bild", "Dich", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "KOUS", "ART", "NN", "NE", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Nur dann, wenn ich Dich freudig wiedersehe,", "tokens": ["Nur", "dann", ",", "wenn", "ich", "Dich", "freu\u00b7dig", "wie\u00b7der\u00b7se\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Entschlummert sanft in mir der Sehnsucht Schmerz,", "tokens": ["Ent\u00b7schlum\u00b7mert", "sanft", "in", "mir", "der", "Sehn\u00b7sucht", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er flieht mich nur in Deiner theuern N\u00e4he,", "tokens": ["Er", "flieht", "mich", "nur", "in", "Dei\u00b7ner", "theu\u00b7ern", "N\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Denn ", "tokens": ["Denn"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}}}}}