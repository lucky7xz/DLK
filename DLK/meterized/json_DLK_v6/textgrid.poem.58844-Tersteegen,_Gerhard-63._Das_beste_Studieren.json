{"textgrid.poem.58844": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "63. Das beste Studieren", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es bringt das viele H\u00f6r'n und Lesen", "tokens": ["Es", "bringt", "das", "vie\u00b7le", "H\u00f6r'n", "und", "Le\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Viel Bilder, aber wenig Wesen;", "tokens": ["Viel", "Bil\u00b7der", ",", "a\u00b7ber", "we\u00b7nig", "We\u00b7sen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Durch Meiden, Leiden, Stillesein,", "tokens": ["Durch", "Mei\u00b7den", ",", "Lei\u00b7den", ",", "Stil\u00b7le\u00b7sein", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wird man gelehrt und Gott gemein.", "tokens": ["Wird", "man", "ge\u00b7lehrt", "und", "Gott", "ge\u00b7mein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "VVPP", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}