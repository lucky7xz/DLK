{"textgrid.poem.64252": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "An Deutsch-\u00d6sterreich", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf, mein Deutsch-\u00d6sterreich,", "tokens": ["Auf", ",", "mein", "Deut\u00b7sch\u00d6s\u00b7ter\u00b7reich", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["APPR", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "R\u00fcste walk\u00fcrengleich", "tokens": ["R\u00fcs\u00b7te", "wal\u00b7k\u00fc\u00b7ren\u00b7gleich"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Helm, Schild und Speer.", "tokens": ["Helm", ",", "Schild", "und", "Speer", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Wie an des Rheines Strand", "tokens": ["Wie", "an", "des", "Rhei\u00b7nes", "Strand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Glorreich die Wache stand,", "tokens": ["Glor\u00b7reich", "die", "Wa\u00b7che", "stand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "So f\u00fcr dein Donauland", "tokens": ["So", "f\u00fcr", "dein", "Do\u00b7nau\u00b7land"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.7": {"text": "Schwinge die Wehr!", "tokens": ["Schwin\u00b7ge", "die", "Wehr", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Treu deinem Kaiserhaus", "tokens": ["Treu", "dei\u00b7nem", "Kai\u00b7ser\u00b7haus"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Breite die Schwingen aus,", "tokens": ["Brei\u00b7te", "die", "Schwin\u00b7gen", "aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Zweik\u00f6pf'ger Aar.", "tokens": ["Zwei\u00b7k\u00f6pf'\u00b7ger", "Aar", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Was dir die St\u00e4rke schafft,", "tokens": ["Was", "dir", "die", "St\u00e4r\u00b7ke", "schafft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was dich macht adlerhaft,", "tokens": ["Was", "dich", "macht", "ad\u00b7ler\u00b7haft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Das ist die deutsche Kraft,", "tokens": ["Das", "ist", "die", "deut\u00b7sche", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Frei, k\u00fchn und wahr!", "tokens": ["Frei", ",", "k\u00fchn", "und", "wahr", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Du hast jahrhundertlang", "tokens": ["Du", "hast", "jahr\u00b7hun\u00b7dert\u00b7lang"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "T\u00fcrken- und Slawendrang", "tokens": ["T\u00fcr\u00b7ken", "und", "Sla\u00b7wen\u00b7drang"], "token_info": ["word", "word", "word"], "pos": ["TRUNC", "KON", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Sieghaft entschart.", "tokens": ["Sieg\u00b7haft", "ent\u00b7schart", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Was du mit Heldenmut,", "tokens": ["Was", "du", "mit", "Hel\u00b7den\u00b7mut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was du mit edlem Blut", "tokens": ["Was", "du", "mit", "ed\u00b7lem", "Blut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Schufest zu deutschem Gut,", "tokens": ["Schu\u00b7fest", "zu", "deut\u00b7schem", "Gut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Halt' es bewahrt!", "tokens": ["Halt'", "es", "be\u00b7wahrt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVPP", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Ob dich, Deutsch-\u00d6sterreich,", "tokens": ["Ob", "dich", ",", "Deut\u00b7sch\u00d6s\u00b7ter\u00b7reich", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Rudeln von W\u00f6lfen gleich,", "tokens": ["Ru\u00b7deln", "von", "W\u00f6l\u00b7fen", "gleich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ADV", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Feind auch umkreist, \u2013", "tokens": ["Feind", "auch", "um\u00b7kreist", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "ADV", "VVFIN", "$,", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Vorw\u00e4rts! durch Wolken dicht", "tokens": ["Vor\u00b7w\u00e4rts", "!", "durch", "Wol\u00b7ken", "dicht"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$.", "APPR", "NN", "ADJD"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Strahlend die Sonne bricht,", "tokens": ["Strah\u00b7lend", "die", "Son\u00b7ne", "bricht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Sieg gibt die Menge nicht:", "tokens": ["Sieg", "gibt", "die", "Men\u00b7ge", "nicht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sieg gibt der Geist!", "tokens": ["Sieg", "gibt", "der", "Geist", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Und nicht in Einsamkeit", "tokens": ["Und", "nicht", "in", "Ein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "K\u00e4mpfst du den harten Streit; \u2013", "tokens": ["K\u00e4mpfst", "du", "den", "har\u00b7ten", "Streit", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Treu dir gesellt,", "tokens": ["Treu", "dir", "ge\u00b7sellt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVPP", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Schild an Schild, Tritt an Tritt", "tokens": ["Schild", "an", "Schild", ",", "Tritt", "an", "Tritt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "$,", "NN", "APPR", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Streitet dein Deutschland mit:", "tokens": ["Strei\u00b7tet", "dein", "Deutschland", "mit", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Schwestern, \u2013 im Siegesschritt", "tokens": ["Schwes\u00b7tern", ",", "\u2013", "im", "Sie\u00b7ges\u00b7schritt"], "token_info": ["word", "punct", "punct", "word", "word"], "pos": ["NN", "$,", "$(", "APPRART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "St\u00fcrmt ihr die Welt!", "tokens": ["St\u00fcrmt", "ihr", "die", "Welt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}