{"textgrid.poem.56606": {"metadata": {"author": {"name": "Groth, Klaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Still as \u00fcnnern warme D\u0119k", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.71", "af:0.28"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Still as \u00fcnnern warme D\u0119k", "tokens": ["Still", "as", "\u00fcn\u00b7nern", "war\u00b7me", "D\u0119k"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Liggt dat D\u00f6rp in witten Snee,", "tokens": ["Liggt", "dat", "D\u00f6rp", "in", "wit\u00b7ten", "Snee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Mank de Ellern sl\u00f6ppt de B\u0119k,", "tokens": ["Mank", "de", "El\u00b7lern", "sl\u00f6ppt", "de", "B\u0119k", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00dcnnert Is de blanke See.", "tokens": ["\u00dcn\u00b7nert", "Is", "de", "blan\u00b7ke", "See", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wicheln stat in witte Haar,", "tokens": ["Wi\u00b7cheln", "stat", "in", "wit\u00b7te", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Spegelt slapri all de K\u00f6pp,", "tokens": ["Spe\u00b7gelt", "sla\u00b7pri", "all", "de", "K\u00f6pp", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PIAT", "NE", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "All is ruhi, kold un klar,", "tokens": ["All", "is", "ru\u00b7hi", ",", "kold", "un", "klar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "FM", "FM", "$,", "ADJD", "FM", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "As de Dod, de ewi sl\u00f6ppt.", "tokens": ["As", "de", "Dod", ",", "de", "e\u00b7wi", "sl\u00f6ppt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$,", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wit, so wit de Ogen reckt,", "tokens": ["Wit", ",", "so", "wit", "de", "O\u00b7gen", "reckt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "APPR", "NE", "NE", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nich en L\u0119ben, nich en Lut;", "tokens": ["Nich", "en", "L\u0119\u00b7ben", ",", "nich", "en", "Lut", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NE", "NE", "$,", "PTKNEG", "FM", "FM", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Blau na'n blauen H\u0119ben treckt", "tokens": ["Blau", "na'n", "blau\u00b7en", "H\u0119\u00b7ben", "treckt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sach de Rok nan Snee herut.", "tokens": ["Sach", "de", "Rok", "nan", "Snee", "he\u00b7rut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "NE", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ik much slapen, as de Bom,", "tokens": ["Ik", "much", "sla\u00b7pen", ",", "as", "de", "Bom", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVINF", "$,", "NE", "NE", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "S\u00fcnner Weh un s\u00fcnner Lust,", "tokens": ["S\u00fcn\u00b7ner", "Weh", "un", "s\u00fcn\u00b7ner", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "FM", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch dar treckt mi as in Drom", "tokens": ["Doch", "dar", "treckt", "mi", "as", "in", "Drom"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKVZ", "VVFIN", "FM", "FM", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Still de Rok to Hus.", "tokens": ["Still", "de", "Rok", "to", "Hus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "FM", "FM", "FM", "FM", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}