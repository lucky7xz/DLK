{"textgrid.poem.33044": {"metadata": {"author": {"name": "B\u00fcchner, Luise", "birth": "N.A.", "death": "N.A."}, "title": "Guter Rath", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Still mu\u00dft du werden, pochend Herz,", "tokens": ["Still", "mu\u00dft", "du", "wer\u00b7den", ",", "po\u00b7chend", "Herz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "VAINF", "$,", "VVPP", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Still wie der Stern am Himmelszelt,", "tokens": ["Still", "wie", "der", "Stern", "am", "Him\u00b7mels\u00b7zelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie er, mu\u00dft unber\u00fchrt du steh'n", "tokens": ["Wie", "er", ",", "mu\u00dft", "un\u00b7be\u00b7r\u00fchrt", "du", "steh'n"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "$,", "VMFIN", "ADJD", "PPER", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom nicht'gen Treiben dieser Welt.", "tokens": ["Vom", "nicht'\u00b7gen", "Trei\u00b7ben", "die\u00b7ser", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Still mu\u00dft du werden wie der Fels,", "tokens": ["Still", "mu\u00dft", "du", "wer\u00b7den", "wie", "der", "Fels", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "VAINF", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An dem sich wild die Brandung bricht;", "tokens": ["An", "dem", "sich", "wild", "die", "Bran\u00b7dung", "bricht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PRF", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ob auch ein Schifflein jach zerschellt", "tokens": ["Ob", "auch", "ein", "Schif\u00b7flein", "jach", "zer\u00b7schellt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An seinem Fu\u00df, er f\u00fchlt es nicht.", "tokens": ["An", "sei\u00b7nem", "Fu\u00df", ",", "er", "f\u00fchlt", "es", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Still mu\u00dft du werden wie der Schwan,", "tokens": ["Still", "mu\u00dft", "du", "wer\u00b7den", "wie", "der", "Schwan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "VAINF", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der lautlos schwimmt den See dahin,", "tokens": ["Der", "laut\u00b7los", "schwimmt", "den", "See", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "ART", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie einsam er die Fluth zertheilt,", "tokens": ["Wie", "ein\u00b7sam", "er", "die", "Fluth", "zer\u00b7theilt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mu\u00dft du des Lebens Kreise zieh'n.", "tokens": ["Mu\u00dft", "du", "des", "Le\u00b7bens", "Krei\u00b7se", "zieh'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["VMFIN", "PPER", "ART", "NN", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "So stolz mu\u00dft steh'n du, so allein,", "tokens": ["So", "stolz", "mu\u00dft", "steh'n", "du", ",", "so", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VMFIN", "VVFIN", "PPER", "$,", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dann wirst du froh und gl\u00fccklich sein./", "tokens": ["Dann", "wirst", "du", "froh", "und", "gl\u00fcck\u00b7lich", "sein", ".", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "VAINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch ach! du seufzest leise: nein,", "tokens": ["Doch", "ach", "!", "du", "seuf\u00b7zest", "lei\u00b7se", ":", "nein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "XY", "$.", "PPER", "VVFIN", "ADJD", "$.", "PTKANT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht froh, nicht gl\u00fccklich werd' ich sein!", "tokens": ["Nicht", "froh", ",", "nicht", "gl\u00fcck\u00b7lich", "werd'", "ich", "sein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "PTKNEG", "ADJD", "VAFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "O, ich versteh' dich, gl\u00fchend Herz,", "tokens": ["O", ",", "ich", "ver\u00b7steh'", "dich", ",", "gl\u00fc\u00b7hend", "Herz", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "PPER", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu hei\u00df liebst du das Leben noch,", "tokens": ["Zu", "hei\u00df", "liebst", "du", "das", "Le\u00b7ben", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VVFIN", "PPER", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Trotz seinen Schmerzen, seiner Qual,", "tokens": ["Trotz", "sei\u00b7nen", "Schmer\u00b7zen", ",", "sei\u00b7ner", "Qual", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Trotz seiner Noth liebst du es doch.", "tokens": ["Trotz", "sei\u00b7ner", "Noth", "liebst", "du", "es", "doch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "So schlag' in Menschenleid und Lust,", "tokens": ["So", "schlag'", "in", "Men\u00b7schen\u00b7leid", "und", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So dulde denn und klage nicht,", "tokens": ["So", "dul\u00b7de", "denn", "und", "kla\u00b7ge", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "KON", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sei einsam eher nicht und kalt,", "tokens": ["Sei", "ein\u00b7sam", "e\u00b7her", "nicht", "und", "kalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "ADV", "PTKNEG", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht still, als bis der Tod dich bricht!", "tokens": ["Nicht", "still", ",", "als", "bis", "der", "Tod", "dich", "bricht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PTKVZ", "$,", "KOUS", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}