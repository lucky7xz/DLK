{"dta.poem.1345": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "An den edlen Pfingst-Wind/ GOtt den  \n heiligen Geist.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Komm/ linder West/ la\u00df deinen Athem sp\u00fcren/", "tokens": ["Komm", "/", "lin\u00b7der", "West", "/", "la\u00df", "dei\u00b7nen", "A\u00b7them", "sp\u00fc\u00b7ren", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "ADJA", "NN", "$(", "VVIMP", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Sulamith verlangt und seuffzt nach dir/", "tokens": ["Die", "Su\u00b7la\u00b7mith", "ver\u00b7langt", "und", "seuffzt", "nach", "dir", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "KON", "VVFIN", "APPR", "PPER", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihr Garten will Geruch und Schmuck verlieren/", "tokens": ["Ihr", "Gar\u00b7ten", "will", "Ge\u00b7ruch", "und", "Schmuck", "ver\u00b7lie\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Drum finde dich mit Hold und Trost zu ihr.", "tokens": ["Drum", "fin\u00b7de", "dich", "mit", "Hold", "und", "Trost", "zu", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PRF", "APPR", "NE", "KON", "NN", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der strenge Nord best\u00fcrmet ihre Sinnen/", "tokens": ["Der", "stren\u00b7ge", "Nord", "be\u00b7st\u00fcr\u00b7met", "ih\u00b7re", "Sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Das Hertze wird als ein gefrornes Ei\u00df/", "tokens": ["Das", "Hert\u00b7ze", "wird", "als", "ein", "ge\u00b7fror\u00b7nes", "Ei\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VAFIN", "KOKOM", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die Hoffnung will nicht Bl\u00fctt und Stock gewinnen/", "tokens": ["Die", "Hoff\u00b7nung", "will", "nicht", "Bl\u00fctt", "und", "Stock", "ge\u00b7win\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die Andacht starrt/ die keine Flamme wei\u00df.", "tokens": ["Die", "An\u00b7dacht", "starrt", "/", "die", "kei\u00b7ne", "Flam\u00b7me", "wei\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Ein d\u00fcrrer Ost entzieht die welcken Kr\u00e4ffte/", "tokens": ["Ein", "d\u00fcr\u00b7rer", "Ost", "ent\u00b7zieht", "die", "wel\u00b7cken", "Kr\u00e4ff\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Sein Blasen hemmt den frischen Perlen-Thau/", "tokens": ["Sein", "Bla\u00b7sen", "hemmt", "den", "fri\u00b7schen", "Per\u00b7len\u00b7\u00b7T\u00b7hau", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.11": {"text": "Der trockne Staub verzehret Marck und S\u00e4ffte/", "tokens": ["Der", "trock\u00b7ne", "Staub", "ver\u00b7zeh\u00b7ret", "Marck", "und", "S\u00e4ff\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Manch Tugend-Blat erstirbt auff matter Au:", "tokens": ["Manch", "Tu\u00b7gen\u00b7dBlat", "er\u00b7stirbt", "auff", "mat\u00b7ter", "Au", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Offt mu\u00df sie auch den heissen S\u00fcd empfinden/", "tokens": ["Offt", "mu\u00df", "sie", "auch", "den", "heis\u00b7sen", "S\u00fcd", "emp\u00b7fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Wenn Creutzes-Brand in Blutt und Adern wallt/", "tokens": ["Wenn", "Creut\u00b7zes\u00b7Brand", "in", "Blutt", "und", "A\u00b7dern", "wallt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ADJD", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Bey Mittags-Glutt will aller Schatten schwinden/", "tokens": ["Bey", "Mit\u00b7tags\u00b7Glutt", "will", "al\u00b7ler", "Schat\u00b7ten", "schwin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Und sie verliert Mutt/ Anmutt und Gestalt.", "tokens": ["Und", "sie", "ver\u00b7liert", "Mutt", "/", "An\u00b7mutt", "und", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "NE", "$(", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Komm/ linder West/ la\u00df deinen Athem sp\u00fcren/", "tokens": ["Komm", "/", "lin\u00b7der", "West", "/", "la\u00df", "dei\u00b7nen", "A\u00b7them", "sp\u00fc\u00b7ren", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "ADJA", "NN", "$(", "VVIMP", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Da\u00df Sulamith die schwache wird erg\u00f6zt/", "tokens": ["Da\u00df", "Su\u00b7la\u00b7mith", "die", "schwa\u00b7che", "wird", "er\u00b7g\u00f6zt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ART", "ADJA", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Ihr Garten wird viel neue Fr\u00fcchte f\u00fchren/", "tokens": ["Ihr", "Gar\u00b7ten", "wird", "viel", "neu\u00b7e", "Fr\u00fcch\u00b7te", "f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PIAT", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Wenn ihn durch dich ein Gnaden-Regen nezt.", "tokens": ["Wenn", "ihn", "durch", "dich", "ein", "Gna\u00b7den\u00b7Re\u00b7gen", "nezt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Dein sanffter Hauch/ dein angenehmes Wehen", "tokens": ["Dein", "sanff\u00b7ter", "Hauch", "/", "dein", "an\u00b7ge\u00b7neh\u00b7mes", "We\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Bes\u00e4met ihn mit Gott-beliebter Frucht/", "tokens": ["Be\u00b7s\u00e4\u00b7met", "ihn", "mit", "Got\u00b7tbe\u00b7lieb\u00b7ter", "Frucht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Das wilde Land wird sich verbessert sehen/", "tokens": ["Das", "wil\u00b7de", "Land", "wird", "sich", "ver\u00b7bes\u00b7sert", "se\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PRF", "VVPP", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Durch deinen Geist und Trieb anheimgesucht.", "tokens": ["Durch", "dei\u00b7nen", "Geist", "und", "Trieb", "an\u00b7heim\u00b7ge\u00b7sucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Komm/ linder West/ la\u00df deinen Athem spielen/", "tokens": ["Komm", "/", "lin\u00b7der", "West", "/", "la\u00df", "dei\u00b7nen", "A\u00b7them", "spie\u00b7len", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "ADJA", "NN", "$(", "VVIMP", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Da\u00df sich bey uns der Glaub\u2019 an GOtt entz\u00fcnd.", "tokens": ["Da\u00df", "sich", "bey", "uns", "der", "Glaub'", "an", "Gott", "ent\u00b7z\u00fcnd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "APPR", "PPER", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.27": {"text": "Komm/ reiner Geist/ la\u00df deine Regung f\u00fchlen/", "tokens": ["Komm", "/", "rei\u00b7ner", "Geist", "/", "la\u00df", "dei\u00b7ne", "Re\u00b7gung", "f\u00fch\u00b7len", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "ADJA", "NN", "$(", "VVIMP", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Da\u00df sich die Brust von Liebe hei\u00df befind.", "tokens": ["Da\u00df", "sich", "die", "Brust", "von", "Lie\u00b7be", "hei\u00df", "be\u00b7find", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "APPR", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "O werther Gast/ zeuch ein mit deinem Worte/", "tokens": ["O", "wert\u00b7her", "Gast", "/", "zeuch", "ein", "mit", "dei\u00b7nem", "Wor\u00b7te", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$(", "VVIMP", "ART", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Komm/ finde dich mit deinen Gaben ein/", "tokens": ["Komm", "/", "fin\u00b7de", "dich", "mit", "dei\u00b7nen", "Ga\u00b7ben", "ein", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "ART", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Wir \u00f6ffnen dir mit Lust des Hertzens Pforte/", "tokens": ["Wir", "\u00f6ff\u00b7nen", "dir", "mit", "Lust", "des", "Hert\u00b7zens", "Pfor\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Und Seel und Geist soll deine Wohnung seyn.", "tokens": ["Und", "Seel", "und", "Geist", "soll", "dei\u00b7ne", "Woh\u00b7nung", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VMFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}