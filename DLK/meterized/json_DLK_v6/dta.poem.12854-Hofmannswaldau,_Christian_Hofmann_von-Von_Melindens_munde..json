{"dta.poem.12854": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Von Melindens munde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Allzugeringe faust! unw\u00fcrdiges papier!", "tokens": ["All\u00b7zu\u00b7ge\u00b7rin\u00b7ge", "faust", "!", "un\u00b7w\u00fcr\u00b7di\u00b7ges", "pa\u00b7pier", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich wei\u00df Melindens mund nicht lebhafft abzuschildern:", "tokens": ["Ich", "wei\u00df", "Me\u00b7lin\u00b7dens", "mund", "nicht", "leb\u00b7hafft", "ab\u00b7zu\u00b7schil\u00b7dern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "NN", "PTKNEG", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es schickt mein pinsel sich nicht zu dergleichen bildern,", "tokens": ["Es", "schickt", "mein", "pin\u00b7sel", "sich", "nicht", "zu", "derg\u00b7lei\u00b7chen", "bil\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PRF", "PTKNEG", "PTKA", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und ein nur blasses blat fa\u00dft keine solche zier.", "tokens": ["Und", "ein", "nur", "blas\u00b7ses", "blat", "fa\u00dft", "kei\u00b7ne", "sol\u00b7che", "zier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "ADJA", "NN", "VVFIN", "PIAT", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es starrt, o sch\u00f6nster mund! so mund als hand vor dir,", "tokens": ["Es", "starrt", ",", "o", "sch\u00f6ns\u00b7ter", "mund", "!", "so", "mund", "als", "hand", "vor", "dir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "FM", "ADJA", "NN", "$.", "ADV", "ADJD", "KOKOM", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Denn meine poesie will allbereits verwildern;", "tokens": ["Denn", "mei\u00b7ne", "po\u00b7e\u00b7sie", "will", "all\u00b7be\u00b7reits", "ver\u00b7wil\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch dein gelinder ku\u00df kan alle h\u00e4rte mildern:", "tokens": ["Doch", "dein", "ge\u00b7lin\u00b7der", "ku\u00df", "kan", "al\u00b7le", "h\u00e4r\u00b7te", "mil\u00b7dern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VMFIN", "PIS", "VVFIN", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und wen dein othem r\u00fchrt, der tritt getrost herf\u00fcr.", "tokens": ["Und", "wen", "dein", "ot\u00b7hem", "r\u00fchrt", ",", "der", "tritt", "ge\u00b7trost", "her\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPOSAT", "ADJA", "VVFIN", "$,", "PRELS", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wohlan! so sey es denn: Die farbe deiner lippen", "tokens": ["Wo\u00b7hlan", "!", "so", "sey", "es", "denn", ":", "Die", "far\u00b7be", "dei\u00b7ner", "lip\u00b7pen"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$.", "ADV", "VAFIN", "PPER", "ADV", "$.", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Steigt h\u00f6her, als die pracht von den corallen-klippen,", "tokens": ["Steigt", "h\u00f6\u00b7her", ",", "als", "die", "pracht", "von", "den", "coral\u00b7len\u00b7klip\u00b7pen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "KOUS", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.11": {"text": "Und wer im tode liegt, den macht ihr feuer frisch.", "tokens": ["Und", "wer", "im", "to\u00b7de", "liegt", ",", "den", "macht", "ihr", "feu\u00b7er", "frisch", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPRART", "NN", "VVFIN", "$,", "ART", "VVFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Zwar andern schl\u00e4gt die brust weit eine s\u00fc\u00dfre wunde;", "tokens": ["Zwar", "an\u00b7dern", "schl\u00e4gt", "die", "brust", "weit", "ei\u00b7ne", "s\u00fc\u00df\u00b7re", "wun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "ART", "NN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Allein ich bin vergn\u00fcgt, hab ich auf deinem munde,", "tokens": ["Al\u00b7lein", "ich", "bin", "ver\u00b7gn\u00fcgt", ",", "hab", "ich", "auf", "dei\u00b7nem", "mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "VVPP", "$,", "VAFIN", "PPER", "APPR", "PPOSAT", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Du andre Helena! nur einen freyen tisch.", "tokens": ["Du", "and\u00b7re", "He\u00b7le\u00b7na", "!", "nur", "ei\u00b7nen", "frey\u00b7en", "tisch", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NE", "$.", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}