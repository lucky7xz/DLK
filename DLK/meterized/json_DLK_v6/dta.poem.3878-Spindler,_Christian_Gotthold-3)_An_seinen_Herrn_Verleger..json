{"dta.poem.3878": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "3) An seinen Herrn Verleger.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Ge\u00dfner!", "tokens": ["Mein", "Ge\u00df\u00b7ner", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "dir zu Ehren dichten,", "tokens": ["dir", "zu", "Eh\u00b7ren", "dich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und seine Schuldigkeit entrichten,", "tokens": ["Und", "sei\u00b7ne", "Schul\u00b7dig\u00b7keit", "ent\u00b7rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vermag kein schwaches Sayten-Spiel,", "tokens": ["Ver\u00b7mag", "kein", "schwa\u00b7ches", "Say\u00b7ten\u00b7Spiel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dich, ", "tokens": ["Dich", ","], "token_info": ["word", "punct"], "pos": ["PPER", "$,"], "meter": "-", "measure": "single.down"}, "line.6": {"text": "Nach W\u00fcrden und Verdienst zu loben,", "tokens": ["Nach", "W\u00fcr\u00b7den", "und", "Ver\u00b7dienst", "zu", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Vermag kein schwacher Dichter-Kiel.", "tokens": ["Ver\u00b7mag", "kein", "schwa\u00b7cher", "Dich\u00b7ter\u00b7Kiel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ich will dir alles gern verschweigen,", "tokens": ["Ich", "will", "dir", "al\u00b7les", "gern", "ver\u00b7schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PIS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Nur aber eines mu\u00df ich zeigen:", "tokens": ["Nur", "a\u00b7ber", "ei\u00b7nes", "mu\u00df", "ich", "zei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Verdient nicht deine Schreibe-Kunst,", "tokens": ["Ver\u00b7di\u00b7ent", "nicht", "dei\u00b7ne", "Schrei\u00b7be\u00b7Kunst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "So wohl im erst als andern Theile,", "tokens": ["So", "wohl", "im", "erst", "als", "an\u00b7dern", "Thei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "ADV", "KOUS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Bey jeder nur gedruckten Zeile", "tokens": ["Bey", "je\u00b7der", "nur", "ge\u00b7druck\u00b7ten", "Zei\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Und ihrer Kupffer; Huld und Gunst?", "tokens": ["Und", "ih\u00b7rer", "Kupf\u00b7fer", ";", "Huld", "und", "Gunst", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$.", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Wie zeigen die verlegten Wercke", "tokens": ["Wie", "zei\u00b7gen", "die", "ver\u00b7leg\u00b7ten", "Wer\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Nicht alle eine feine St\u00e4rcke", "tokens": ["Nicht", "al\u00b7le", "ei\u00b7ne", "fei\u00b7ne", "St\u00e4r\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "PIS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Von Einsicht, Wissenschafft und Witz", "tokens": ["Von", "Ein\u00b7sicht", ",", "Wis\u00b7sen\u00b7schafft", "und", "Witz"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Du selbst besorgest alle Sachen,", "tokens": ["Du", "selbst", "be\u00b7sor\u00b7gest", "al\u00b7le", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Du selbsten kanst ", "tokens": ["Du", "selbs\u00b7ten", "kanst"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.19": {"text": "Die sonderlich der Jugend n\u00fctz.", "tokens": ["Die", "son\u00b7der\u00b7lich", "der", "Ju\u00b7gend", "n\u00fctz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Die von dir viel verlegten Schrifften,", "tokens": ["Die", "von", "dir", "viel", "ver\u00b7leg\u00b7ten", "Schriff\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Die werden dir ein Denckmahl stifften,", "tokens": ["Die", "wer\u00b7den", "dir", "ein", "Denck\u00b7mahl", "stiff\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Da\u00df du so manchen Tag als Nacht", "tokens": ["Da\u00df", "du", "so", "man\u00b7chen", "Tag", "als", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PIAT", "NN", "KOUS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Mit Dichten, Lesen, Schreiben, Sinnen,", "tokens": ["Mit", "Dich\u00b7ten", ",", "Le\u00b7sen", ",", "Schrei\u00b7ben", ",", "Sin\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Und mit viel lehrsamen Beginnen", "tokens": ["Und", "mit", "viel", "lehr\u00b7sa\u00b7men", "Be\u00b7gin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.25": {"text": "Mit gr\u00f6ster M\u00fche zugebracht.", "tokens": ["Mit", "gr\u00f6s\u00b7ter", "M\u00fc\u00b7he", "zu\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Drum fahre fort, o ", "tokens": ["Drum", "fah\u00b7re", "fort", ",", "o"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PAV", "VVFIN", "PTKVZ", "$,", "FM"], "meter": "-+---", "measure": "dactylic.init"}, "line.27": {"text": "Zur Presse mit noch manchen Theile,", "tokens": ["Zur", "Pres\u00b7se", "mit", "noch", "man\u00b7chen", "Thei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Du weist, man liebet deine Schrifft;", "tokens": ["Du", "weist", ",", "man", "lie\u00b7bet", "dei\u00b7ne", "Schrifft", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PIS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Und dieses mu\u00df dich auch bewegen,", "tokens": ["Und", "die\u00b7ses", "mu\u00df", "dich", "auch", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Weil dein so g\u00fctiges Verlegen", "tokens": ["Weil", "dein", "so", "g\u00fc\u00b7ti\u00b7ges", "Ver\u00b7le\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Dir deiner Ehre Denckmahl stifft.", "tokens": ["Dir", "dei\u00b7ner", "Eh\u00b7re", "Denck\u00b7mahl", "stifft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Letzt gr\u00fcsse mir die ", "tokens": ["Letzt", "gr\u00fcs\u00b7se", "mir", "die"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.33": {"text": "La\u00df bald die Antworts-Schrifft erscheinen,", "tokens": ["La\u00df", "bald", "die", "Ant\u00b7worts\u00b7Schrifft", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Sey fernerhin mein ", "tokens": ["Sey", "fer\u00b7ner\u00b7hin", "mein"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ADV", "PPOSAT"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.35": {"text": "Dir wird so Danck-als Freundschaffts-Pflichten", "tokens": ["Dir", "wird", "so", "Dan\u00b7ck\u00b7als", "Freund\u00b7schaffts\u00b7Pflich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.36": {"text": "Bis in die Todes-Grufft entrichten,", "tokens": ["Bis", "in", "die", "To\u00b7des\u00b7G\u00b7rufft", "ent\u00b7rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Dein Spindler, der es redlich meint.", "tokens": ["Dein", "Spind\u00b7ler", ",", "der", "es", "red\u00b7lich", "meint", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}