{"textgrid.poem.42881": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "W\u00e4sche", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4sche ist von des Menschen Um\u00e4u\u00dferung", "tokens": ["W\u00e4\u00b7sche", "ist", "von", "des", "Men\u00b7schen", "Um\u00b7\u00e4u\u00b7\u00dfe\u00b7rung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "APPR", "ART", "NN", "NN"], "meter": "+-+--+--+--", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Das Innerste, also das Feinste,", "tokens": ["Das", "In\u00b7ners\u00b7te", ",", "al\u00b7so", "das", "Feins\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und sollte immer das Reinste", "tokens": ["Und", "soll\u00b7te", "im\u00b7mer", "das", "Reins\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ADV", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sein, wie im Menschen selber die Seele.", "tokens": ["Sein", ",", "wie", "im", "Men\u00b7schen", "sel\u00b7ber", "die", "See\u00b7le", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "$,", "PWAV", "APPRART", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Was immer ihr fehle,", "tokens": ["Was", "im\u00b7mer", "ihr", "feh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Die Sauberkeit fehle ihr nie.", "tokens": ["Die", "Sau\u00b7ber\u00b7keit", "feh\u00b7le", "ihr", "nie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und sch\u00f6n und sch\u00f6ner, wenn au\u00dferdem sie", "tokens": ["Und", "sch\u00f6n", "und", "sch\u00f6\u00b7ner", ",", "wenn", "au\u00b7\u00dfer\u00b7dem", "sie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "ADJA", "$,", "KOUS", "PAV", "PPER"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Noch Wohlgeschmack, einen freien Geist", "tokens": ["Noch", "Wohl\u00b7ge\u00b7schmack", ",", "ei\u00b7nen", "frei\u00b7en", "Geist"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und das Verst\u00e4ndnis f\u00fcr neueste Zeit", "tokens": ["Und", "das", "Ver\u00b7st\u00e4nd\u00b7nis", "f\u00fcr", "neu\u00b7es\u00b7te", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.6": {"text": "Und f\u00fcr die Gesetze der Ewigkeit", "tokens": ["Und", "f\u00fcr", "die", "Ge\u00b7set\u00b7ze", "der", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Beweist. \u2013", "tokens": ["Be\u00b7weist", ".", "\u2013"], "token_info": ["word", "punct", "punct"], "pos": ["NN", "$.", "$("], "meter": "-+", "measure": "iambic.single"}}, "stanza.3": {"line.1": {"text": "Wie doch die innersten Bl\u00e4ttchen der Bl\u00fcten", "tokens": ["Wie", "doch", "die", "in\u00b7ners\u00b7ten", "Bl\u00e4tt\u00b7chen", "der", "Bl\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die innigsten sind. \u2013", "tokens": ["Die", "in\u00b7nigs\u00b7ten", "sind", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "VAFIN", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "W\u00e4sche sollst du wie dein Gewissen", "tokens": ["W\u00e4\u00b7sche", "sollst", "du", "wie", "dein", "Ge\u00b7wis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "KOKOM", "PPOSAT", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Und wie dein Kind", "tokens": ["Und", "wie", "dein", "Kind"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Peinlich pflegen und z\u00e4rtlich beh\u00fcten.", "tokens": ["Pein\u00b7lich", "pfle\u00b7gen", "und", "z\u00e4rt\u00b7lich", "be\u00b7h\u00fc\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "KON", "ADJD", "VVINF", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}}}}