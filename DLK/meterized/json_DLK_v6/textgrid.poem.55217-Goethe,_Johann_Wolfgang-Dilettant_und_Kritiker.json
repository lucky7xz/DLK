{"textgrid.poem.55217": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Dilettant und Kritiker", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es hatt ein Knab eine Taube zart,", "tokens": ["Es", "hatt", "ein", "Knab", "ei\u00b7ne", "Tau\u00b7be", "zart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Gar sch\u00f6n von Farben und bunt,", "tokens": ["Gar", "sch\u00f6n", "von", "Far\u00b7ben", "und", "bunt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "KON", "ADJD", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Gar herzlich lieb, nach Knabenart,", "tokens": ["Gar", "herz\u00b7lich", "lieb", ",", "nach", "Kna\u00b7be\u00b7nart", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "$,", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ge\u00e4tzet aus seinem Mund", "tokens": ["Ge\u00e4t\u00b7zet", "aus", "sei\u00b7nem", "Mund"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Und hatte so Freud am T\u00e4ubchen sein,", "tokens": ["Und", "hat\u00b7te", "so", "Freud", "am", "T\u00e4ub\u00b7chen", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "NN", "APPRART", "NN", "VAINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Da\u00df er nicht konnte sich freuen allein.", "tokens": ["Da\u00df", "er", "nicht", "konn\u00b7te", "sich", "freu\u00b7en", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VMFIN", "PRF", "VVFIN", "ADV", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Da lebte nicht weit ein Alt-Fuchs herum,", "tokens": ["Da", "leb\u00b7te", "nicht", "weit", "ein", "Al\u00b7tFuchs", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Erfahren und lehrreich und schw\u00e4tzig darum;", "tokens": ["Er\u00b7fah\u00b7ren", "und", "lehr\u00b7reich", "und", "schw\u00e4t\u00b7zig", "da\u00b7rum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "KON", "ADJD", "PAV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Der hatte den Knaben manch St\u00fcndlein ergetzt,", "tokens": ["Der", "hat\u00b7te", "den", "Kna\u00b7ben", "manch", "St\u00fcnd\u00b7lein", "er\u00b7getzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Mit Wundern und L\u00fcgen verprahlt und verschw\u00e4tzt.", "tokens": ["Mit", "Wun\u00b7dern", "und", "L\u00fc\u00b7gen", "ver\u00b7prahlt", "und", "ver\u00b7schw\u00e4tzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "KON", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbmu\u00df meinem Fuchs doch mein T\u00e4ubelein zeigen!\u00ab", "tokens": ["\u00bb", "mu\u00df", "mei\u00b7nem", "Fuchs", "doch", "mein", "T\u00e4u\u00b7be\u00b7lein", "zei\u00b7gen", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VMFIN", "PPOSAT", "NE", "ADV", "PPOSAT", "NN", "VVINF", "$.", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Er lief und fand ihn strecken in Str\u00e4uchen.", "tokens": ["Er", "lief", "und", "fand", "ihn", "stre\u00b7cken", "in", "Str\u00e4u\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "\u00bbsieh, Fuchs, mein lieb T\u00e4ublein, mein T\u00e4ubchen so sch\u00f6n!", "tokens": ["\u00bb", "sieh", ",", "Fuchs", ",", "mein", "lieb", "T\u00e4ub\u00b7lein", ",", "mein", "T\u00e4ub\u00b7chen", "so", "sch\u00f6n", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "$,", "NE", "$,", "PPOSAT", "ADJD", "NN", "$,", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Hast du dein' Tag' so ein T\u00e4ubchen gesehn?\u00ab", "tokens": ["Hast", "du", "dein'", "Tag'", "so", "ein", "T\u00e4ub\u00b7chen", "ge\u00b7sehn", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "ADV", "ART", "NN", "VVPP", "$.", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbzeig her!\u00ab \u2013 Der Knabe reicht's. \u2013 \u00bbGeht wohl an;", "tokens": ["\u00bb", "zeig", "her", "!", "\u00ab", "\u2013", "Der", "Kna\u00b7be", "reicht'", "s.", "\u2013", "\u00bb", "Geht", "wohl", "an", ";"], "token_info": ["punct", "word", "word", "punct", "punct", "punct", "word", "word", "word", "abbreviation", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "$.", "$(", "$(", "ART", "NN", "VVFIN", "NE", "$(", "$(", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Aber es fehlt noch, manches dran.", "tokens": ["A\u00b7ber", "es", "fehlt", "noch", ",", "man\u00b7ches", "dran", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$,", "PIS", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Die Federn, zum Exempel, sind zu kurz geraten.\u00ab", "tokens": ["Die", "Fe\u00b7dern", ",", "zum", "Ex\u00b7em\u00b7pel", ",", "sind", "zu", "kurz", "ge\u00b7ra\u00b7ten", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "APPRART", "NN", "$,", "VAFIN", "PTKA", "ADJD", "VVPP", "$.", "$("], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Da fing er an, rupft' sich den Braten.", "tokens": ["Da", "fing", "er", "an", ",", "rupft'", "sich", "den", "Bra\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Knabe schrie. \u2013 \u00bbDu mu\u00dft st\u00e4rkre einsetzen,", "tokens": ["Der", "Kna\u00b7be", "schrie", ".", "\u2013", "\u00bb", "Du", "mu\u00dft", "st\u00e4r\u00b7kre", "ein\u00b7set\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "$(", "PPER", "VMFIN", "VVFIN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sonst ziert's nicht, schwinget nicht.\u00ab", "tokens": ["Sonst", "ziert's", "nicht", ",", "schwin\u00b7get", "nicht", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "$,", "VVFIN", "PTKNEG", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da war's nackt \u2013 Mi\u00dfgeburt! \u2013 und in Fetzen.", "tokens": ["Da", "wa\u00b7r's", "nackt", "\u2013", "Mi\u00df\u00b7ge\u00b7burt", "!", "\u2013", "und", "in", "Fet\u00b7zen", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "$(", "NE", "$.", "$(", "KON", "APPR", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Dem Knaben das Herze bricht.", "tokens": ["Dem", "Kna\u00b7ben", "das", "Her\u00b7ze", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Wer sich erkennt im Knaben gut,", "tokens": ["Wer", "sich", "er\u00b7kennt", "im", "Kna\u00b7ben", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "VVFIN", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der sei vor F\u00fcchsen auf seiner Hut.", "tokens": ["Der", "sei", "vor", "F\u00fcch\u00b7sen", "auf", "sei\u00b7ner", "Hut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}