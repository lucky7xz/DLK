{"textgrid.poem.43262": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zwei Schl\u00e4ger, Trinkhorn und Cerevis", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwei Schl\u00e4ger, Trinkhorn und Cerevis", "tokens": ["Zwei", "Schl\u00e4\u00b7ger", ",", "Trink\u00b7horn", "und", "Ce\u00b7re\u00b7vis"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["CARD", "NN", "$,", "NN", "KON", "NE"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Schm\u00fccken die hellen W\u00e4nd'", "tokens": ["Schm\u00fc\u00b7cken", "die", "hel\u00b7len", "W\u00e4nd'"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "In voller Wichs noch \u00fcberdies", "tokens": ["In", "vol\u00b7ler", "Wichs", "noch", "\u00fc\u00b7ber\u00b7dies"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00e4ngt mitten ein Corpsstudent.", "tokens": ["H\u00e4ngt", "mit\u00b7ten", "ein", "Corps\u00b7stu\u00b7dent", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Es gleicht das flotte Conterfei", "tokens": ["Es", "gleicht", "das", "flot\u00b7te", "Con\u00b7ter\u00b7fei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nimmer dem Original,", "tokens": ["Nim\u00b7mer", "dem", "O\u00b7rig\u00b7i\u00b7nal", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "+--+---", "measure": "dactylic.di.plus"}, "line.3": {"text": "Die Nase theilt ein Schmi\u00df entzwei,", "tokens": ["Die", "Na\u00b7se", "theilt", "ein", "Sch\u00b7mi\u00df", "ent\u00b7zwei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Der sch\u00f6ne Kopf ist jetzt kahl.", "tokens": ["Der", "sch\u00f6\u00b7ne", "Kopf", "ist", "jetzt", "kahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Und eben klagt ihm ein Bauer breit:", "tokens": ["Und", "e\u00b7ben", "klagt", "ihm", "ein", "Bau\u00b7er", "breit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u00bbhoh-le ... Z\u00e4h-n-e!! ... Theures Bier!\u00ab", "tokens": ["\u00bb", "hoh\u00b7le", "...", "Z\u00e4h\u00b7n\u00b7e", "!!", "...", "Theu\u00b7res", "Bier", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "ADJA", "$(", "NN", "$.", "$(", "ADJA", "NN", "$.", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Er summt: \u00bbOh Burschenherrlichkeit,", "tokens": ["Er", "summt", ":", "\u00bb", "Oh", "Bur\u00b7schen\u00b7herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "ITJ", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was wurde aus Dir und mir!'", "tokens": ["Was", "wur\u00b7de", "aus", "Dir", "und", "mir", "!", "'"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWS", "VAFIN", "APPR", "PPER", "KON", "PPER", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}