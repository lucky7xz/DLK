{"textgrid.poem.45543": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[man spricht jetzt viel von dem Glauben]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man spricht jetzt viel von dem Glauben:", "tokens": ["Man", "spricht", "jetzt", "viel", "von", "dem", "Glau\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der eine w\u00fcnscht zu glauben,", "tokens": ["Der", "ei\u00b7ne", "w\u00fcnscht", "zu", "glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der andre glaubt zu glauben,", "tokens": ["Der", "and\u00b7re", "glaubt", "zu", "glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der dritte hat den Glauben.", "tokens": ["Der", "drit\u00b7te", "hat", "den", "Glau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Allein der Glaube hat keinen.", "tokens": ["Al\u00b7lein", "der", "Glau\u00b7be", "hat", "kei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "PIAT", "$."], "meter": "---+--+-", "measure": "iambic.di.relaxed"}, "line.6": {"text": "Was mein ist, ist nur Meinen.", "tokens": ["Was", "mein", "ist", ",", "ist", "nur", "Mei\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "VAFIN", "$,", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}