{"textgrid.poem.59614": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was klingt mir f\u00fcr ein s\u00fc\u00dfer Wunderschall", "tokens": ["Was", "klingt", "mir", "f\u00fcr", "ein", "s\u00fc\u00b7\u00dfer", "Wun\u00b7der\u00b7schall"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit Himmelst\u00f6nen tief im tiefsten Herzen,", "tokens": ["Mit", "Him\u00b7mel\u00b7st\u00f6\u00b7nen", "tief", "im", "tiefs\u00b7ten", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gleichwie die Stimme klingt der hohen Schmerzen,", "tokens": ["Gleich\u00b7wie", "die", "Stim\u00b7me", "klingt", "der", "ho\u00b7hen", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die ewig liebekranke Nachtigall?", "tokens": ["Die", "e\u00b7wig", "lie\u00b7be\u00b7kran\u00b7ke", "Nach\u00b7ti\u00b7gall", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Was bl\u00fcht ihr, l\u00e4ngstvergangne Wonnen, all", "tokens": ["Was", "bl\u00fcht", "ihr", ",", "l\u00e4ngst\u00b7ver\u00b7gang\u00b7ne", "Won\u00b7nen", ",", "all"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PWS", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,", "PIAT"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und z\u00fcndet mir die Brust mit Himmelsschmerzen?", "tokens": ["Und", "z\u00fcn\u00b7det", "mir", "die", "Brust", "mit", "Him\u00b7mels\u00b7schmer\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und la\u00dft die finstern Geister in mir scherzen?", "tokens": ["Und", "la\u00dft", "die", "fins\u00b7tern", "Geis\u00b7ter", "in", "mir", "scher\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "ADJA", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "O das ist Liebe, das ist Liebesschall!", "tokens": ["O", "das", "ist", "Lie\u00b7be", ",", "das", "ist", "Lie\u00b7bes\u00b7schall", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PDS", "VAFIN", "NN", "$,", "PDS", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O bliebst du ewig, s\u00fc\u00dfer Wunderschall!", "tokens": ["O", "bliebst", "du", "e\u00b7wig", ",", "s\u00fc\u00b7\u00dfer", "Wun\u00b7der\u00b7schall", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "O w\u00fcrd' ich selber ganz zur Philomele", "tokens": ["O", "w\u00fcrd'", "ich", "sel\u00b7ber", "ganz", "zur", "Phi\u00b7lo\u00b7me\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "ADV", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und kl\u00e4nge mich in Liebesklagen tot!", "tokens": ["Und", "kl\u00e4n\u00b7ge", "mich", "in", "Lie\u00b7bes\u00b7kla\u00b7gen", "tot", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Denn wer die Liebe hat, der hat das All,", "tokens": ["Denn", "wer", "die", "Lie\u00b7be", "hat", ",", "der", "hat", "das", "All", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VAFIN", "$,", "PRELS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Liebe ist der Seelen gro\u00dfe Seele,", "tokens": ["Die", "Lie\u00b7be", "ist", "der", "See\u00b7len", "gro\u00b7\u00dfe", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der G\u00f6tter Leben und der G\u00f6tter Tod.", "tokens": ["Der", "G\u00f6t\u00b7ter", "Le\u00b7ben", "und", "der", "G\u00f6t\u00b7ter", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}