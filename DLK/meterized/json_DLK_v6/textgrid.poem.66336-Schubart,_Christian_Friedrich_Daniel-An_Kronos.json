{"textgrid.poem.66336": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "An Kronos", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie die Engel durch die Himmel fliegen,", "tokens": ["Wie", "die", "En\u00b7gel", "durch", "die", "Him\u00b7mel", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Fliegst du, ", "tokens": ["Fliegst", "du", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Um die Stirne flattert wonniglich Vergn\u00fcgen;", "tokens": ["Um", "die", "Stir\u00b7ne", "flat\u00b7tert", "won\u00b7nig\u00b7lich", "Ver\u00b7gn\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VVFIN", "ADJD", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Deine locken thauen s\u00fc\u00dfen Duft.", "tokens": ["Dei\u00b7ne", "lo\u00b7cken", "thau\u00b7en", "s\u00fc\u00b7\u00dfen", "Duft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVINF", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Weile, ", "tokens": ["Wei\u00b7le", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Wohlger\u00fcche schickt dir die Natur.", "tokens": ["Wohl\u00b7ge\u00b7r\u00fc\u00b7che", "schickt", "dir", "die", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Deiner Kinder buntes, schw\u00e4rmendes Gewimmel", "tokens": ["Dei\u00b7ner", "Kin\u00b7der", "bun\u00b7tes", ",", "schw\u00e4r\u00b7men\u00b7des", "Ge\u00b7wim\u00b7mel"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Sonnt und w\u00e4lzt sich auf der jungen Flur.", "tokens": ["Sonnt", "und", "w\u00e4lzt", "sich", "auf", "der", "jun\u00b7gen", "Flur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Nachtigallen mit der sanften Seele", "tokens": ["Nach\u00b7ti\u00b7gal\u00b7len", "mit", "der", "sanf\u00b7ten", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Fl\u00f6ten dir ein Fr\u00fchlingsliedchen f\u00fcr!", "tokens": ["Fl\u00f6\u00b7ten", "dir", "ein", "Fr\u00fch\u00b7lings\u00b7lied\u00b7chen", "f\u00fcr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Schwalben zwitschern mit der kleinen Kehle!", "tokens": ["Schwal\u00b7ben", "zwit\u00b7schern", "mit", "der", "klei\u00b7nen", "Keh\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Braune Lerchen hangen \u00fcber dir!", "tokens": ["Brau\u00b7ne", "Ler\u00b7chen", "han\u00b7gen", "\u00fc\u00b7ber", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Milch, wie Nektar, spritzt f\u00fcr dich das volle", "tokens": ["Milch", ",", "wie", "Nek\u00b7tar", ",", "spritzt", "f\u00fcr", "dich", "das", "vol\u00b7le"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "NN", "$,", "VVFIN", "APPR", "PPER", "ART", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Euter ins krystallne Glas hinein:", "tokens": ["Eu\u00b7ter", "ins", "krys\u00b7tall\u00b7ne", "Glas", "hin\u00b7ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Weile doch, o ", "tokens": ["Wei\u00b7le", "doch", ",", "o"], "token_info": ["word", "word", "punct", "word"], "pos": ["NN", "ADV", "$,", "FM"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Wei\u00df gewaschen, soll dein Lager sein.", "tokens": ["Wei\u00df", "ge\u00b7wa\u00b7schen", ",", "soll", "dein", "La\u00b7ger", "sein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "VMFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Deinen Schlummer f\u00f6rdern Sch\u00e4ferlieder,", "tokens": ["Dei\u00b7nen", "Schlum\u00b7mer", "f\u00f6r\u00b7dern", "Sch\u00e4\u00b7fer\u00b7lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und das Murmeln der wohlth\u00e4t'gen Blau;", "tokens": ["Und", "das", "Mur\u00b7meln", "der", "wohlt\u00b7h\u00e4t'\u00b7gen", "Blau", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "--+--++-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Maienbl\u00fcthen tanzen auf dich nieder,", "tokens": ["Mai\u00b7en\u00b7bl\u00fc\u00b7then", "tan\u00b7zen", "auf", "dich", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und ein M\u00e4dchen bl\u00fchend wie die Au'", "tokens": ["Und", "ein", "M\u00e4d\u00b7chen", "bl\u00fc\u00b7hend", "wie", "die", "Au'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVPP", "KOKOM", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Bei\u00dft ins Fingerchen, und auf den Zehen", "tokens": ["Bei\u00dft", "ins", "Fin\u00b7ge\u00b7rchen", ",", "und", "auf", "den", "Ze\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "$,", "KON", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schleicht sie hin zu dir und b\u00fccket sich,", "tokens": ["Schleicht", "sie", "hin", "zu", "dir", "und", "b\u00fc\u00b7cket", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPER", "KON", "VVFIN", "PRF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Um den gro\u00dfen Schlummernden zu sehen,", "tokens": ["Um", "den", "gro\u00b7\u00dfen", "Schlum\u00b7mern\u00b7den", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Mit dem Silberbarte! \u2013 Ha, sie k\u00fcsset dich!", "tokens": ["Mit", "dem", "Sil\u00b7ber\u00b7bar\u00b7te", "!", "\u2013", "Ha", ",", "sie", "k\u00fcs\u00b7set", "dich", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "$(", "ITJ", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.7": {"line.1": {"text": "Weile, ", "tokens": ["Wei\u00b7le", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Zeig uns doch dein wonnesam Gesicht!", "tokens": ["Zeig", "uns", "doch", "dein", "won\u00b7ne\u00b7sam", "Ge\u00b7sicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "PPOSAT", "ADJD", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Doch du eilst in Fr\u00fchlingsduft vor\u00fcber,", "tokens": ["Doch", "du", "eilst", "in", "Fr\u00fch\u00b7lings\u00b7duft", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wie in St\u00fcrmen, und du weilest nicht!", "tokens": ["Wie", "in", "St\u00fcr\u00b7men", ",", "und", "du", "wei\u00b7lest", "nicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "$,", "KON", "PPER", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Ach, so nimm mich mit auf deinen Wagen,", "tokens": ["Ach", ",", "so", "nimm", "mich", "mit", "auf", "dei\u00b7nen", "Wa\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVIMP", "PPER", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Nimm mein M\u00e4dchen mit; denn ohne sie", "tokens": ["Nimm", "mein", "M\u00e4d\u00b7chen", "mit", ";", "denn", "oh\u00b7ne", "sie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "PTKVZ", "$.", "KON", "APPR", "PPER"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "F\u00e4nd' ich dort in ew'gen Sommertagen", "tokens": ["F\u00e4nd'", "ich", "dort", "in", "ew'\u00b7gen", "Som\u00b7mer\u00b7ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Meinen Fr\u00fchling, meinen Himmel nie!", "tokens": ["Mei\u00b7nen", "Fr\u00fch\u00b7ling", ",", "mei\u00b7nen", "Him\u00b7mel", "nie", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}