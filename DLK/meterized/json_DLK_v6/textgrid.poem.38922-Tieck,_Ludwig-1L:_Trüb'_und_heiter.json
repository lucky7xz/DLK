{"textgrid.poem.38922": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Tr\u00fcb' und heiter", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tr\u00fcb' und heiter", "tokens": ["Tr\u00fcb'", "und", "hei\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "ADJD"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Fliegt die Welt vor uns vorbey,", "tokens": ["Fliegt", "die", "Welt", "vor", "uns", "vor\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wir wandeln weiter", "tokens": ["Wir", "wan\u00b7deln", "wei\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Bald tr\u00fcb' und heiter", "tokens": ["Bald", "tr\u00fcb'", "und", "hei\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Und wissen nicht, wie es uns sey:", "tokens": ["Und", "wis\u00b7sen", "nicht", ",", "wie", "es", "uns", "sey", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Himmlische Poesie", "tokens": ["Himm\u00b7li\u00b7sche", "Poe\u00b7sie"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Lehrst uns wie.", "tokens": ["Lehrst", "uns", "wie", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPER", "KOKOM", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Aber sie vernehmen dich nicht,", "tokens": ["A\u00b7ber", "sie", "ver\u00b7neh\u00b7men", "dich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Sie wenden sich hinweg vom Licht,", "tokens": ["Sie", "wen\u00b7den", "sich", "hin\u00b7weg", "vom", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APZR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Sie leben weiter", "tokens": ["Sie", "le\u00b7ben", "wei\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Immer tr\u00fcber, wen'ger heiter,", "tokens": ["Im\u00b7mer", "tr\u00fc\u00b7ber", ",", "wen'\u00b7ger", "hei\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Merken nicht da\u00df alles Tr\u00fcbe", "tokens": ["Mer\u00b7ken", "nicht", "da\u00df", "al\u00b7les", "Tr\u00fc\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "KOUS", "PIS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Durch der K\u00fcnste G\u00f6ttermacht", "tokens": ["Durch", "der", "K\u00fcns\u00b7te", "G\u00f6t\u00b7ter\u00b7macht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "In der heitern Milde lacht,", "tokens": ["In", "der", "hei\u00b7tern", "Mil\u00b7de", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Selbst der Ha\u00df wird lichte Liebe. \u2013", "tokens": ["Selbst", "der", "Ha\u00df", "wird", "lich\u00b7te", "Lie\u00b7be", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}