{"textgrid.poem.60515": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "An einen Tadler", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn gegen falschen Schmerz du dich ereiferst", "tokens": ["Wenn", "ge\u00b7gen", "fal\u00b7schen", "Schmerz", "du", "dich", "er\u00b7ei\u00b7ferst"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN", "PPER", "PRF", "VVPP"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Und Tr\u00e4nenk\u00fcnstelei, so hast du recht;", "tokens": ["Und", "Tr\u00e4\u00b7nen\u00b7k\u00fcns\u00b7te\u00b7lei", ",", "so", "hast", "du", "recht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch hast du was von einem Henkersknecht,", "tokens": ["Doch", "hast", "du", "was", "von", "ei\u00b7nem", "Hen\u00b7kers\u00b7knecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wenn du mit Spott den wahren Schmerz begeiferst.", "tokens": ["Wenn", "du", "mit", "Spott", "den", "wah\u00b7ren", "Schmerz", "be\u00b7gei\u00b7ferst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+---", "measure": "unknown.measure.tetra"}}, "stanza.2": {"line.1": {"text": "Verfolge r\u00fcstig, wo du kannst, die L\u00fcgen;", "tokens": ["Ver\u00b7fol\u00b7ge", "r\u00fcs\u00b7tig", ",", "wo", "du", "kannst", ",", "die", "L\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "PWAV", "PPER", "VMFIN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Wahrheit ehre; ist dir wohl zumut,", "tokens": ["Die", "Wahr\u00b7heit", "eh\u00b7re", ";", "ist", "dir", "wohl", "zu\u00b7mut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So sollst du z\u00fcgeln dein vergn\u00fcgtes Blut", "tokens": ["So", "sollst", "du", "z\u00fc\u00b7geln", "dein", "ver\u00b7gn\u00fcg\u00b7tes", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und zur Gesundheit nicht die Roheit f\u00fcgen.", "tokens": ["Und", "zur", "Ge\u00b7sund\u00b7heit", "nicht", "die", "Ro\u00b7heit", "f\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Auch Freuden gibt es, die nur Freuden scheinen,", "tokens": ["Auch", "Freu\u00b7den", "gibt", "es", ",", "die", "nur", "Freu\u00b7den", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "PPER", "$,", "PRELS", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und mehr vielleicht als Schmerzen, die nicht wahr;", "tokens": ["Und", "mehr", "viel\u00b7leicht", "als", "Schmer\u00b7zen", ",", "die", "nicht", "wahr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "KOUS", "NN", "$,", "PRELS", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wem Lust bl\u00fcht, lache; traure, wem sie gar;", "tokens": ["Wem", "Lust", "bl\u00fcht", ",", "la\u00b7che", ";", "trau\u00b7re", ",", "wem", "sie", "gar", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "$,", "VVFIN", "$.", "VVFIN", "$,", "PWS", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und ists ein Dichter, mag sein Lied auch weinen.", "tokens": ["Und", "ists", "ein", "Dich\u00b7ter", ",", "mag", "sein", "Lied", "auch", "wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "$,", "VMFIN", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}