{"textgrid.poem.59122": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "341. Fort! Fort!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den Eigenheiten nachzusp\u00fcren", "tokens": ["Den", "Ei\u00b7gen\u00b7hei\u00b7ten", "nach\u00b7zu\u00b7sp\u00fc\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und sie lang h\u00f6r'n, ist Zeit verlieren;", "tokens": ["Und", "sie", "lang", "h\u00f6r'n", ",", "ist", "Zeit", "ver\u00b7lie\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "VVINF", "$,", "VAFIN", "NN", "VVINF", "$."], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Es f\u00f6rdert, wer auch hinkend geht,", "tokens": ["Es", "f\u00f6r\u00b7dert", ",", "wer", "auch", "hin\u00b7kend", "geht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "ADV", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch mehr, als der gar stille steht.", "tokens": ["Noch", "mehr", ",", "als", "der", "gar", "stil\u00b7le", "steht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "ART", "ADV", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}