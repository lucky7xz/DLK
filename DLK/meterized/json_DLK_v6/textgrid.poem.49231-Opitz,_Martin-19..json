{"textgrid.poem.49231": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "19.", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es streite wer da wil, ich bleibe doch darbei,", "tokens": ["Es", "strei\u00b7te", "wer", "da", "wil", ",", "ich", "blei\u00b7be", "doch", "dar\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PWS", "ADV", "VMFIN", "$,", "PPER", "VVFIN", "ADV", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df der Poeten F\u00fcrst, Homerus, blind gewesen;", "tokens": ["Da\u00df", "der", "Po\u00b7et\u00b7en", "F\u00fcrst", ",", "Ho\u00b7me\u00b7rus", ",", "blind", "ge\u00b7we\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "$,", "NE", "$,", "ADJD", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kan man von Venus nicht in seinem Buche lesen,", "tokens": ["Kan", "man", "von", "Ve\u00b7nus", "nicht", "in", "sei\u00b7nem", "Bu\u00b7che", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "NE", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df ihr die Faust verletzt durch Diomeden sei?", "tokens": ["Da\u00df", "ihr", "die", "Faust", "ver\u00b7letzt", "durch", "Dio\u00b7me\u00b7den", "sei", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "APPR", "NE", "VAFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.2": {"line.1": {"text": "Er hette ja viel mehr sie sonsten treffen sollen,", "tokens": ["Er", "het\u00b7te", "ja", "viel", "mehr", "sie", "sons\u00b7ten", "tref\u00b7fen", "sol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADV", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nicht die zarte Hand, so viel zu edel war;", "tokens": ["Und", "nicht", "die", "zar\u00b7te", "Hand", ",", "so", "viel", "zu", "e\u00b7del", "war", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "ADJA", "NN", "$,", "ADV", "ADV", "PTKA", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er hett' auch ohne Schwert ged\u00fcrft in die Gefahr;", "tokens": ["Er", "hett'", "auch", "oh\u00b7ne", "Schwert", "ge\u00b7d\u00fcrft", "in", "die", "Ge\u00b7fahr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "NN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Homerus ist doch blind, sie sagen was sie wollen.", "tokens": ["Ho\u00b7me\u00b7rus", "ist", "doch", "blind", ",", "sie", "sa\u00b7gen", "was", "sie", "wol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "ADJD", "$,", "PPER", "VVINF", "PWS", "PPER", "VMFIN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}}}}