{"textgrid.poem.62516": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach, das ist ein s\u00fc\u00dfes Bild!", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, das ist ein s\u00fc\u00dfes Bild!", "tokens": ["Ach", ",", "das", "ist", "ein", "s\u00fc\u00b7\u00dfes", "Bild", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie es meine Seele f\u00fcllt!", "tokens": ["Wie", "es", "mei\u00b7ne", "See\u00b7le", "f\u00fcllt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00e4chtig will mich's an sich ziehen,", "tokens": ["M\u00e4ch\u00b7tig", "will", "mich's", "an", "sich", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PIS", "APPR", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lieber stets, je mehr ich's schau'.", "tokens": ["Lie\u00b7ber", "stets", ",", "je", "mehr", "ich's", "schau'", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "$,", "ADV", "ADV", "PIS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gerne m\u00f6cht' ich vor der Frau,", "tokens": ["Ger\u00b7ne", "m\u00f6cht'", "ich", "vor", "der", "Frau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Bei dem holden M\u00e4gdlein knieen.", "tokens": ["Bei", "dem", "hol\u00b7den", "M\u00e4gd\u00b7lein", "kni\u00b7e\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}}, "stanza.2": {"line.1": {"text": "Nach dem Buche unverwandt,", "tokens": ["Nach", "dem", "Bu\u00b7che", "un\u00b7ver\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach der theuern Mutter Hand", "tokens": ["Nach", "der", "theu\u00b7ern", "Mut\u00b7ter", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schaut sie, welche zeigt die Zeilen.", "tokens": ["Schaut", "sie", ",", "wel\u00b7che", "zeigt", "die", "Zei\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PRELS", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gotteswort mu\u00df das wol sein,", "tokens": ["Got\u00b7tes\u00b7wort", "mu\u00df", "das", "wol", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PDS", "ADV", "VAINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Was ein Kindlein so erfreun", "tokens": ["Was", "ein", "Kin\u00b7dlein", "so", "er\u00b7freun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "ADV", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und es zwingen kann zu weilen.", "tokens": ["Und", "es", "zwin\u00b7gen", "kann", "zu", "wei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "VMFIN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Engel schauen froh hinein,", "tokens": ["En\u00b7gel", "schau\u00b7en", "froh", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00f6chten gerne Zeugen sein", "tokens": ["M\u00f6ch\u00b7ten", "ger\u00b7ne", "Zeu\u00b7gen", "sein"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "NN", "PPOSAT"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus bescheidner heil'ger Ferne;", "tokens": ["Aus", "be\u00b7scheid\u00b7ner", "heil'\u00b7ger", "Fer\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch das Fenster, durch die Th\u00fcr", "tokens": ["Durch", "das", "Fens\u00b7ter", ",", "durch", "die", "Th\u00fcr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schauen Engel stets nach ihr,", "tokens": ["Schau\u00b7en", "En\u00b7gel", "stets", "nach", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie nach einem sch\u00f6nen Sterne.", "tokens": ["Wie", "nach", "ei\u00b7nem", "sch\u00f6\u00b7nen", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sagt, wer ist die Sch\u00fclerin?", "tokens": ["Sagt", ",", "wer", "ist", "die", "Sch\u00fc\u00b7le\u00b7rin", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist es nicht die K\u00f6nigin", "tokens": ["Ist", "es", "nicht", "die", "K\u00f6\u00b7ni\u00b7gin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PTKNEG", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aller Heiligen und Frommen?", "tokens": ["Al\u00b7ler", "Hei\u00b7li\u00b7gen", "und", "From\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "O Maria reich und mild", "tokens": ["O", "Ma\u00b7ria", "reich", "und", "mild"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NE", "ADJD", "KON", "ADJD"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "La\u00df, o Sch\u00fclerin, dies Bild", "tokens": ["La\u00df", ",", "o", "Sch\u00fc\u00b7le\u00b7rin", ",", "dies", "Bild"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "$,", "FM", "NN", "$,", "PDS", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nie aus meiner Seele kommen!", "tokens": ["Nie", "aus", "mei\u00b7ner", "See\u00b7le", "kom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Die den Herrn geb\u00e4ren soll,", "tokens": ["Die", "den", "Herrn", "ge\u00b7b\u00e4\u00b7ren", "soll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Knieet still und andachtsvoll,", "tokens": ["Kni\u00b7e\u00b7et", "still", "und", "an\u00b7dachts\u00b7voll", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Scheint nur ", "tokens": ["Scheint", "nur"], "token_info": ["word", "word"], "pos": ["VVFIN", "ADV"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Jedes Wort und jedes Ding", "tokens": ["Je\u00b7des", "Wort", "und", "je\u00b7des", "Ding"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sei auch mir ein Himmelswink,", "tokens": ["Sei", "auch", "mir", "ein", "Him\u00b7mels\u00b7wink", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Flei\u00dfig auf mein Heil zu achten.", "tokens": ["Flei\u00b7\u00dfig", "auf", "mein", "Heil", "zu", "ach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}