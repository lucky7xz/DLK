{"textgrid.poem.58364": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "402. Der beste F\u00fchrer", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Herren Auge leitet mich,", "tokens": ["Des", "Her\u00b7ren", "Au\u00b7ge", "lei\u00b7tet", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich mu\u00df ihn stets in stillem Geist ansehen;", "tokens": ["Ich", "mu\u00df", "ihn", "stets", "in", "stil\u00b7lem", "Geist", "an\u00b7se\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sein Wink, der zeigt mir innerlich,", "tokens": ["Sein", "Wink", ",", "der", "zeigt", "mir", "in\u00b7ner\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie ich soll recht und rein und sicher gehen.", "tokens": ["Wie", "ich", "soll", "recht", "und", "rein", "und", "si\u00b7cher", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VMFIN", "ADJD", "KON", "ADJD", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}