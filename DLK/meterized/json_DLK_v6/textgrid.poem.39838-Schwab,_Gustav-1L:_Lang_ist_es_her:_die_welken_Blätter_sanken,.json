{"textgrid.poem.39838": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Lang ist es her: die welken Bl\u00e4tter sanken,", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lang ist es her: die welken Bl\u00e4tter sanken,", "tokens": ["Lang", "ist", "es", "her", ":", "die", "wel\u00b7ken", "Bl\u00e4t\u00b7ter", "san\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PTKVZ", "$.", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Nordwind brauste durch die N\u00e4chte hin,", "tokens": ["Der", "Nord\u00b7wind", "braus\u00b7te", "durch", "die", "N\u00e4ch\u00b7te", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein \u00f6des Mondlicht tr\u00fcb aus Wolken schien,", "tokens": ["Ein", "\u00f6\u00b7des", "Mond\u00b7licht", "tr\u00fcb", "aus", "Wol\u00b7ken", "schien", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hinschleichend durch der Reb' entlaubte Ranken.", "tokens": ["Hin\u00b7schlei\u00b7chend", "durch", "der", "Reb'", "ent\u00b7laub\u00b7te", "Ran\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mir schweiften hell und sonnig die Gedanken,", "tokens": ["Mir", "schweif\u00b7ten", "hell", "und", "son\u00b7nig", "die", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Bild der S\u00fc\u00dfen hie\u00df den Winter ziehn,", "tokens": ["Das", "Bild", "der", "S\u00fc\u00b7\u00dfen", "hie\u00df", "den", "Win\u00b7ter", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und um mich sah ich Alles leuchten, bl\u00fchn,", "tokens": ["Und", "um", "mich", "sah", "ich", "Al\u00b7les", "leuch\u00b7ten", ",", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "PPER", "PIS", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gebrochen war die Zeit mit ihren Schranken.", "tokens": ["Ge\u00b7bro\u00b7chen", "war", "die", "Zeit", "mit", "ih\u00b7ren", "Schran\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Seit nun die Gute hin ist, kam geschienen", "tokens": ["Seit", "nun", "die", "Gu\u00b7te", "hin", "ist", ",", "kam", "ge\u00b7schie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADV", "ART", "NN", "ADV", "VAFIN", "$,", "VVFIN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Manch Fr\u00fchlingsmorgenrot, manch Sommerlicht,", "tokens": ["Manch", "Fr\u00fch\u00b7lings\u00b7mor\u00b7gen\u00b7rot", ",", "manch", "Som\u00b7mer\u00b7licht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch mocht' es mir zu keiner Freude dienen;", "tokens": ["Doch", "mocht'", "es", "mir", "zu", "kei\u00b7ner", "Freu\u00b7de", "die\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Jetzt rauscht durch's kahle Feld der Herbstwind wieder,", "tokens": ["Jetzt", "rauscht", "durch's", "kah\u00b7le", "Feld", "der", "Herbst\u00b7wind", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Darum erl\u00f6scht auch meine Hoffnung nicht:", "tokens": ["Da\u00b7rum", "er\u00b7l\u00f6scht", "auch", "mei\u00b7ne", "Hoff\u00b7nung", "nicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vielleicht er weht den alten Lenz mir nieder.", "tokens": ["Viel\u00b7leicht", "er", "weht", "den", "al\u00b7ten", "Lenz", "mir", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ART", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}