{"textgrid.poem.42474": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "1L: In einem der ersten Hererogefechte", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In einem der ersten Hererogefechte", "tokens": ["In", "ei\u00b7nem", "der", "ers\u00b7ten", "He\u00b7re\u00b7ro\u00b7ge\u00b7fech\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "ADJA", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Steht eine Seitendeckung im Dorn.", "tokens": ["Steht", "ei\u00b7ne", "Sei\u00b7ten\u00b7de\u00b7ckung", "im", "Dorn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Kaffern dr\u00e4ngen in gro\u00dfen Massen", "tokens": ["Die", "Kaf\u00b7fern", "dr\u00e4n\u00b7gen", "in", "gro\u00b7\u00dfen", "Mas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Auf die Abteilung mit Geschrei und Zorn.", "tokens": ["Auf", "die", "Ab\u00b7tei\u00b7lung", "mit", "Ge\u00b7schrei", "und", "Zorn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-++-+-+-+", "measure": "zehnsilber"}}, "stanza.2": {"line.1": {"text": "Schon kommen sie in den Busch gelaufen,", "tokens": ["Schon", "kom\u00b7men", "sie", "in", "den", "Busch", "ge\u00b7lau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da springt der Leutnant alleine vor.", "tokens": ["Da", "springt", "der", "Leut\u00b7nant", "al\u00b7lei\u00b7ne", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ein Schu\u00df trifft sein Knie, er sinkt zusammen,", "tokens": ["Ein", "Schu\u00df", "trifft", "sein", "Knie", ",", "er", "sinkt", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Doch beh\u00e4lt er die Leitung wie zuvor.", "tokens": ["Doch", "be\u00b7h\u00e4lt", "er", "die", "Lei\u00b7tung", "wie", "zu\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "KOKOM", "ADV", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Unverbunden liegt er im Sande,", "tokens": ["Un\u00b7ver\u00b7bun\u00b7den", "liegt", "er", "im", "San\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Sein Kommando t\u00f6nt hell, der Feind mu\u00df zur\u00fcck.", "tokens": ["Sein", "Kom\u00b7man\u00b7do", "t\u00f6nt", "hell", ",", "der", "Feind", "mu\u00df", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "$,", "ART", "NN", "VMFIN", "PTKVZ", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Bis zum Abend dauert das Ringen,", "tokens": ["Bis", "zum", "A\u00b7bend", "dau\u00b7ert", "das", "Rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da gl\u00e4nzt dem Leutnant das Siegesgl\u00fcck.", "tokens": ["Da", "gl\u00e4nzt", "dem", "Leut\u00b7nant", "das", "Sie\u00b7ges\u00b7gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Nun wird er vorsichtig aufgehoben;", "tokens": ["Nun", "wird", "er", "vor\u00b7sich\u00b7tig", "auf\u00b7ge\u00b7ho\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein Heilschnitt dort ist unm\u00f6glich, o Not.", "tokens": ["Ein", "Heil\u00b7schnitt", "dort", "ist", "un\u00b7m\u00f6g\u00b7lich", ",", "o", "Not", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "ADJD", "$,", "FM", "NN", "$."], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Aber nirgends ist ein Ochsenwagen,", "tokens": ["A\u00b7ber", "nir\u00b7gends", "ist", "ein", "Och\u00b7sen\u00b7wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Er mu\u00df hier warten auf den Tod.", "tokens": ["Er", "mu\u00df", "hier", "war\u00b7ten", "auf", "den", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ein junger Unteroffizier will helfen,", "tokens": ["Ein", "jun\u00b7ger", "Un\u00b7ter\u00b7of\u00b7fi\u00b7zier", "will", "hel\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er tr\u00e4gt mit drei andern den Leutnant fort;", "tokens": ["Er", "tr\u00e4gt", "mit", "drei", "an\u00b7dern", "den", "Leut\u00b7nant", "fort", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "CARD", "ADJA", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Durch hundertunddrei\u00dfig Kilometer", "tokens": ["Durch", "hun\u00b7der\u00b7tund\u00b7drei\u00b7\u00dfig", "Ki\u00b7lo\u00b7me\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["APPR", "CARD", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Tragen sie ihn bis zum sichern Ort.", "tokens": ["Tra\u00b7gen", "sie", "ihn", "bis", "zum", "si\u00b7chern", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Durch Busch und W\u00fcste, durch Dorn und Dickicht,", "tokens": ["Durch", "Busch", "und", "W\u00fcs\u00b7te", ",", "durch", "Dorn", "und", "Di\u00b7ckicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u00dcber holprichte Wege auf und ab,", "tokens": ["\u00dc\u00b7ber", "hol\u00b7prich\u00b7te", "We\u00b7ge", "auf", "und", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Langsam, langsam kommen sie vorw\u00e4rts,", "tokens": ["Lang\u00b7sam", ",", "lang\u00b7sam", "kom\u00b7men", "sie", "vor\u00b7w\u00e4rts", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Wie m\u00fcde Greise am Pilgerstab.", "tokens": ["Wie", "m\u00fc\u00b7de", "Grei\u00b7se", "am", "Pil\u00b7ger\u00b7stab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Schon will ihnen manchmal die Kraft versagen,", "tokens": ["Schon", "will", "ih\u00b7nen", "manch\u00b7mal", "die", "Kraft", "ver\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "----+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Schon sind sie alle dem Umfallen nah.", "tokens": ["Schon", "sind", "sie", "al\u00b7le", "dem", "Um\u00b7fal\u00b7len", "nah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch sie haben ihn Schritt f\u00fcr Schritt weiter getragen,", "tokens": ["Doch", "sie", "ha\u00b7ben", "ihn", "Schritt", "f\u00fcr", "Schritt", "wei\u00b7ter", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "NN", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "--+--+-++--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und endlich, endlich sind sie da.", "tokens": ["Und", "end\u00b7lich", ",", "end\u00b7lich", "sind", "sie", "da", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADV", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Der Leutnant bat oft, ihn liegen zu lassen,", "tokens": ["Der", "Leut\u00b7nant", "bat", "oft", ",", "ihn", "lie\u00b7gen", "zu", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "PPER", "VVINF", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Aber stets blieb ihre M\u00fche bereit,", "tokens": ["A\u00b7ber", "stets", "blieb", "ih\u00b7re", "M\u00fc\u00b7he", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Bis sie mit z\u00e4hstem Herzschlag am Ziel sind,", "tokens": ["Bis", "sie", "mit", "z\u00e4hs\u00b7tem", "Herz\u00b7schlag", "am", "Ziel", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "APPRART", "NN", "VAFIN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Noch grade zur letzten und rechten Zeit.", "tokens": ["Noch", "gra\u00b7de", "zur", "letz\u00b7ten", "und", "rech\u00b7ten", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "ADJA", "KON", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}