{"textgrid.poem.24960": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sei du mir treu, bis ich von hinnen mu\u00df,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sei du mir treu, bis ich von hinnen mu\u00df,", "tokens": ["Sei", "du", "mir", "treu", ",", "bis", "ich", "von", "hin\u00b7nen", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADJD", "$,", "KOUS", "PPER", "APPR", "ADV", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der durch die Welt du mich bisher geleitet!", "tokens": ["Der", "durch", "die", "Welt", "du", "mich", "bis\u00b7her", "ge\u00b7lei\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PPER", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie f\u00fcr die Wonnen, die du mir bereitet,", "tokens": ["Wie", "f\u00fcr", "die", "Won\u00b7nen", ",", "die", "du", "mir", "be\u00b7rei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Soll ich dir danken, hoher Genius?", "tokens": ["Soll", "ich", "dir", "dan\u00b7ken", ",", "ho\u00b7her", "Ge\u00b7nius", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVINF", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Arm w\u00e4r' ich ohne das, was du gegeben,", "tokens": ["Arm", "w\u00e4r'", "ich", "oh\u00b7ne", "das", ",", "was", "du", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "APPR", "ART", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und, fl\u00f6hest du, was g\u00f6lte mir dies Leben?", "tokens": ["Und", ",", "fl\u00f6\u00b7hest", "du", ",", "was", "g\u00f6l\u00b7te", "mir", "dies", "Le\u00b7ben", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PPER", "$,", "PWS", "VVFIN", "PPER", "PDS", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Als Knabe schon, wenn ich von den Genossen,", "tokens": ["Als", "Kna\u00b7be", "schon", ",", "wenn", "ich", "von", "den", "Ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den l\u00e4rmenden, zur Einsamkeit entfloh,", "tokens": ["Den", "l\u00e4r\u00b7men\u00b7den", ",", "zur", "Ein\u00b7sam\u00b7keit", "ent\u00b7floh", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In meiner Seele, allen sonst verschlossen,", "tokens": ["In", "mei\u00b7ner", "See\u00b7le", ",", "al\u00b7len", "sonst", "ver\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PIAT", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Empfand ich deinen Odem stolz und froh,", "tokens": ["Emp\u00b7fand", "ich", "dei\u00b7nen", "O\u00b7dem", "stolz", "und", "froh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und leicht ward in der Jugend goldner Fr\u00fche", "tokens": ["Und", "leicht", "ward", "in", "der", "Ju\u00b7gend", "gold\u00b7ner", "Fr\u00fc\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Durch dich mir jede Pein und jede M\u00fche.", "tokens": ["Durch", "dich", "mir", "je\u00b7de", "Pein", "und", "je\u00b7de", "M\u00fc\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPER", "PIAT", "NN", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Tief der Natur ins heil'ge Auge schauen,", "tokens": ["Tief", "der", "Na\u00b7tur", "ins", "heil'\u00b7ge", "Au\u00b7ge", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Ihr in des Herzens Allgeheimstes sp\u00e4hn", "tokens": ["Ihr", "in", "des", "Her\u00b7zens", "All\u00b7ge\u00b7heims\u00b7tes", "sp\u00e4hn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "ADJA", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mich lehrtest du, und im Gewittergrauen", "tokens": ["Mich", "lehr\u00b7test", "du", ",", "und", "im", "Ge\u00b7wit\u00b7ter\u00b7grau\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KON", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Donners ernste Rede zu verstehn,", "tokens": ["Des", "Don\u00b7ners", "erns\u00b7te", "Re\u00b7de", "zu", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und in der Bergschlucht, wo die Wasser rauschen,", "tokens": ["Und", "in", "der", "Berg\u00b7schlucht", ",", "wo", "die", "Was\u00b7ser", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PWAV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der gro\u00dfen Mutter Worte zu belauschen.", "tokens": ["Der", "gro\u00b7\u00dfen", "Mut\u00b7ter", "Wor\u00b7te", "zu", "be\u00b7lau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Mit Wesen, die sich selber mein Gedanke", "tokens": ["Mit", "We\u00b7sen", ",", "die", "sich", "sel\u00b7ber", "mein", "Ge\u00b7dan\u00b7ke"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "PRF", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erschuf, den luft'gen Kindern meines Traums,", "tokens": ["Er\u00b7schuf", ",", "den", "luft'\u00b7gen", "Kin\u00b7dern", "mei\u00b7nes", "Traums", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "War mein ein hohes Leben sonder Schranke", "tokens": ["War", "mein", "ein", "ho\u00b7hes", "Le\u00b7ben", "son\u00b7der", "Schran\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "ART", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In einer Welt jenseits des Raums,", "tokens": ["In", "ei\u00b7ner", "Welt", "jen\u00b7seits", "des", "Raums", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und fort und fort mich n\u00e4hrtest du mit hehren", "tokens": ["Und", "fort", "und", "fort", "mich", "n\u00e4hr\u00b7test", "du", "mit", "heh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKVZ", "KON", "PTKVZ", "PPER", "VVFIN", "PPER", "APPR", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Traumbildern und der alten Weisheit Lehren.", "tokens": ["Traum\u00b7bil\u00b7dern", "und", "der", "al\u00b7ten", "Weis\u00b7heit", "Leh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Die durst'gen Lippen labte mir der Quell,", "tokens": ["Die", "dur\u00b7st'\u00b7gen", "Lip\u00b7pen", "lab\u00b7te", "mir", "der", "Quell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der nie versiegende, von Kunst und Dichtung,", "tokens": ["Der", "nie", "ver\u00b7sie\u00b7gen\u00b7de", ",", "von", "Kunst", "und", "Dich\u00b7tung", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und an den Geistern, welche aus Vernichtung", "tokens": ["Und", "an", "den", "Geis\u00b7tern", ",", "wel\u00b7che", "aus", "Ver\u00b7nich\u00b7tung"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Tr\u00fcmmern ihrer Welt zu uns noch hell", "tokens": ["Und", "Tr\u00fcm\u00b7mern", "ih\u00b7rer", "Welt", "zu", "uns", "noch", "hell"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "PPOSAT", "NN", "APPR", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Her\u00fcberstrahlen durch der Zeiten Nacht,", "tokens": ["Her\u00b7\u00fc\u00b7bers\u00b7trah\u00b7len", "durch", "der", "Zei\u00b7ten", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Hab' ich des eignen Geistes Licht entfacht.", "tokens": ["Hab'", "ich", "des", "eig\u00b7nen", "Geis\u00b7tes", "Licht", "ent\u00b7facht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Mit Indiens Weisen in den Siedelein,", "tokens": ["Mit", "In\u00b7di\u00b7ens", "Wei\u00b7sen", "in", "den", "Sie\u00b7de\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Wo Ganga rauscht an Wasserlilienbeeten,", "tokens": ["Wo", "Gan\u00b7ga", "rauscht", "an", "Was\u00b7ser\u00b7li\u00b7li\u00b7en\u00b7bee\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mit Zoroaster bei des Feuers Schein,", "tokens": ["Mit", "Zo\u00b7roas\u00b7ter", "bei", "des", "Feu\u00b7ers", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Des heiligen, zu dem die Parsen beten,", "tokens": ["Des", "hei\u00b7li\u00b7gen", ",", "zu", "dem", "die", "Par\u00b7sen", "be\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "APPR", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wie mit Arabiens k\u00fchnen W\u00fcstens\u00f6hnen", "tokens": ["Wie", "mit", "A\u00b7ra\u00b7biens", "k\u00fch\u00b7nen", "W\u00fcs\u00b7ten\u00b7s\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NE", "ADJA", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Sprach ich vertraut in ihrer Sprache T\u00f6nen.", "tokens": ["Sprach", "ich", "ver\u00b7traut", "in", "ih\u00b7rer", "Spra\u00b7che", "T\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Und gleich dem Geist, nicht haftend an der Scholle,", "tokens": ["Und", "gleich", "dem", "Geist", ",", "nicht", "haf\u00b7tend", "an", "der", "Schol\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "PTKNEG", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schritt pilgernd auch mein Fu\u00df von Land zu Land;", "tokens": ["Schritt", "pil\u00b7gernd", "auch", "mein", "Fu\u00df", "von", "Land", "zu", "Land", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "ADV", "PPOSAT", "NN", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Erde breitete wie eine Rolle", "tokens": ["Die", "Er\u00b7de", "brei\u00b7te\u00b7te", "wie", "ei\u00b7ne", "Rol\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihr Sch\u00f6nstes vor mir aus; bald hoch vom Rand", "tokens": ["Ihr", "Sch\u00f6ns\u00b7tes", "vor", "mir", "aus", ";", "bald", "hoch", "vom", "Rand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "PTKVZ", "$.", "ADV", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Des Schiffs, bald von der Alpen steilstem Pik", "tokens": ["Des", "Schiffs", ",", "bald", "von", "der", "Al\u00b7pen", "steils\u00b7tem", "Pik"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "In ihren Wundern schwelgte mir der Blick.", "tokens": ["In", "ih\u00b7ren", "Wun\u00b7dern", "schwelg\u00b7te", "mir", "der", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "F\u00fcr alles, was erhaben ist und gro\u00df,", "tokens": ["F\u00fcr", "al\u00b7les", ",", "was", "er\u00b7ha\u00b7ben", "ist", "und", "gro\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "ADJD", "VAFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Lie\u00df mir Italien die Seele flammen,", "tokens": ["Lie\u00df", "mir", "I\u00b7ta\u00b7li\u00b7en", "die", "See\u00b7le", "flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NE", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "An ihrer Brust erzogen, hehre Ammen,", "tokens": ["An", "ih\u00b7rer", "Brust", "er\u00b7zo\u00b7gen", ",", "heh\u00b7re", "Am\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie die Sibyllen Michel Angelos,", "tokens": ["Sie", "die", "Si\u00b7byl\u00b7len", "Mi\u00b7chel", "An\u00b7ge\u00b7los", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "NE", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und in des Tabor himmlischem Gesicht", "tokens": ["Und", "in", "des", "Ta\u00b7bor", "himm\u00b7li\u00b7schem", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Trug Raffael sie auf zum ew'gen Licht.", "tokens": ["Trug", "Raf\u00b7fael", "sie", "auf", "zum", "ew'\u00b7gen", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "wo