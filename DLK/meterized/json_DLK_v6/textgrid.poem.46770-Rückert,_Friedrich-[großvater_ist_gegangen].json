{"textgrid.poem.46770": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[gro\u00dfvater ist gegangen]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gro\u00dfvater ist gegangen,", "tokens": ["Gro\u00df\u00b7va\u00b7ter", "ist", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Eh' er dich konnt' umfangen.", "tokens": ["Eh'", "er", "dich", "konnt'", "um\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihr waret, um zu wandern", "tokens": ["Ihr", "wa\u00b7ret", ",", "um", "zu", "wan\u00b7dern"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUI", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das eine hier zum andern,", "tokens": ["Das", "ei\u00b7ne", "hier", "zum", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "ADV", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zu jung, zu alt von Jahren.", "tokens": ["Zu", "jung", ",", "zu", "alt", "von", "Jah\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "$,", "PTKA", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da ist er hingefahren,", "tokens": ["Da", "ist", "er", "hin\u00b7ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wo N\u00e4h und Ferne schwindet,", "tokens": ["Wo", "N\u00e4h", "und", "Fer\u00b7ne", "schwin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und sich Getrenntes findet;", "tokens": ["Und", "sich", "Ge\u00b7trenn\u00b7tes", "fin\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Da seid ihr zwei vereinigt,", "tokens": ["Da", "seid", "ihr", "zwei", "ver\u00b7ei\u00b7nigt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "CARD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Von Erdenweh gereinigt.", "tokens": ["Von", "Er\u00b7den\u00b7weh", "ge\u00b7rei\u00b7nigt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Von ihm hier abgefodert,", "tokens": ["Von", "ihm", "hier", "ab\u00b7ge\u00b7fo\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Bist du emporgelodert;", "tokens": ["Bist", "du", "em\u00b7por\u00b7ge\u00b7lo\u00b7dert", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Konnt' ich dich vorenthalten", "tokens": ["Konnt'", "ich", "dich", "vor\u00b7ent\u00b7hal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Den Fordrungen des Alten?", "tokens": ["Den", "Ford\u00b7run\u00b7gen", "des", "Al\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+---+-", "measure": "dactylic.init"}, "line.15": {"text": "Du magst an ihn dich schmiegen,", "tokens": ["Du", "magst", "an", "ihn", "dich", "schmie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Ihm auf dem Kniee dich wiegen,", "tokens": ["Ihm", "auf", "dem", "Kni\u00b7ee", "dich", "wie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.17": {"text": "Und von dem Sohn ihn gr\u00fc\u00dfen,", "tokens": ["Und", "von", "dem", "Sohn", "ihn", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Der hier hat missen m\u00fcssen", "tokens": ["Der", "hier", "hat", "mis\u00b7sen", "m\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "VAFIN", "VVINF", "VMFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Die Tochter auf der Erde,", "tokens": ["Die", "Toch\u00b7ter", "auf", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Da\u00df jenem droben werde", "tokens": ["Da\u00df", "je\u00b7nem", "dro\u00b7ben", "wer\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "ADV", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Die Enkelin, die Krone", "tokens": ["Die", "En\u00b7ke\u00b7lin", ",", "die", "Kro\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Von seinem \u00e4ltsten Sohne.", "tokens": ["Von", "sei\u00b7nem", "\u00e4lts\u00b7ten", "Soh\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Mag er mit dir sich kr\u00e4nzen,", "tokens": ["Mag", "er", "mit", "dir", "sich", "kr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "Und seine Augen gl\u00e4nzen", "tokens": ["Und", "sei\u00b7ne", "Au\u00b7gen", "gl\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Vor Lust, dich zu erkennen;", "tokens": ["Vor", "Lust", ",", "dich", "zu", "er\u00b7ken\u00b7nen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Du brauchst dich nicht zu nennen,", "tokens": ["Du", "brauchst", "dich", "nicht", "zu", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "Er sieht in dir das treue", "tokens": ["Er", "sieht", "in", "dir", "das", "treu\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "Abbild, das jugendneue", "tokens": ["Ab\u00b7bild", ",", "das", "ju\u00b7gend\u00b7neu\u00b7e"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ART", "ADJA"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.29": {"text": "Von der, die, ihm erkoren", "tokens": ["Von", "der", ",", "die", ",", "ihm", "er\u00b7ko\u00b7ren"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "$,", "PRELS", "$,", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.30": {"text": "Zur Braut einst', mich geboren;", "tokens": ["Zur", "Braut", "einst'", ",", "mich", "ge\u00b7bo\u00b7ren", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": "Der Gattin Jugendglieder", "tokens": ["Der", "Gat\u00b7tin", "Ju\u00b7gend\u00b7glie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.32": {"text": "Bringt ihm die Enklin wieder.", "tokens": ["Bringt", "ihm", "die", "En\u00b7klin", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.33": {"text": "So sei mir aufbehalten,", "tokens": ["So", "sei", "mir", "auf\u00b7be\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "Mein J\u00fcngstes bei dem Alten!", "tokens": ["Mein", "J\u00fcngs\u00b7tes", "bei", "dem", "Al\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.35": {"text": "Und ordnet vor die Kreise,", "tokens": ["Und", "ord\u00b7net", "vor", "die", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.36": {"text": "Bis wir familienweise", "tokens": ["Bis", "wir", "fa\u00b7mi\u00b7li\u00b7en\u00b7wei\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Uns finden all zusammen", "tokens": ["Uns", "fin\u00b7den", "all", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.38": {"text": "Dorthin, woher wir stammen,", "tokens": ["Dor\u00b7thin", ",", "wo\u00b7her", "wir", "stam\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.39": {"text": "Gereint von allen Makeln,", "tokens": ["Ge\u00b7reint", "von", "al\u00b7len", "Ma\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.40": {"text": "In sel'gen Tabernakeln.", "tokens": ["In", "sel'\u00b7gen", "Ta\u00b7ber\u00b7na\u00b7keln", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}