{"textgrid.poem.33301": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Die neue Pfr\u00fcnde", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als j\u00fcngst der junge Pfarrer Kant", "tokens": ["Als", "j\u00fcngst", "der", "jun\u00b7ge", "Pfar\u00b7rer", "Kant"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit seiner Dulcinea schmollte,", "tokens": ["Mit", "sei\u00b7ner", "Dul\u00b7ci\u00b7nea", "schmoll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "In deren Liebesold er stand,", "tokens": ["In", "de\u00b7ren", "Lie\u00b7be\u00b7sold", "er", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sie sogar verlassen wollte,", "tokens": ["Und", "sie", "so\u00b7gar", "ver\u00b7las\u00b7sen", "woll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Schrieb ihm Madam mit eig'ner Hand:", "tokens": ["Schrieb", "ihm", "Ma\u00b7dam", "mit", "eig'\u00b7ner", "Hand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mein Herr! Ein junges Weib ist eine Pfr\u00fcnde,", "tokens": ["Mein", "Herr", "!", "Ein", "jun\u00b7ges", "Weib", "ist", "ei\u00b7ne", "Pfr\u00fcn\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "ART", "ADJA", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die unter einer schweren S\u00fcnde", "tokens": ["Die", "un\u00b7ter", "ei\u00b7ner", "schwe\u00b7ren", "S\u00fcn\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Den Mann, dem sie ertheilet wird,", "tokens": ["Den", "Mann", ",", "dem", "sie", "er\u00b7thei\u00b7let", "wird", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Zum residiren obligirt.", "tokens": ["Zum", "re\u00b7si\u00b7di\u00b7ren", "ob\u00b7li\u00b7girt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}