{"textgrid.poem.25714": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Komm herab, du m\u00e4chtiger Pokal!", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Komm herab, du m\u00e4chtiger Pokal!", "tokens": ["Komm", "her\u00b7ab", ",", "du", "m\u00e4ch\u00b7ti\u00b7ger", "Po\u00b7kal", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Blind vor Alter, der das Hochzeitsmahl", "tokens": ["Blind", "vor", "Al\u00b7ter", ",", "der", "das", "Hoch\u00b7zeits\u00b7mahl"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Unsers Eltervaters mit gefeiert!", "tokens": ["Un\u00b7sers", "El\u00b7ter\u00b7va\u00b7ters", "mit", "ge\u00b7fei\u00b7ert", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "F\u00fcllen will ich dich mit s\u00fc\u00dfem Wein',", "tokens": ["F\u00fcl\u00b7len", "will", "ich", "dich", "mit", "s\u00fc\u00b7\u00dfem", "Wein'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Und der Mundschenk meiner Freunde seyn,", "tokens": ["Und", "der", "Mund\u00b7schenk", "mei\u00b7ner", "Freun\u00b7de", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN", "VAINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Bis Aurora diese Nacht entschleiert.", "tokens": ["Bis", "Au\u00b7ro\u00b7ra", "die\u00b7se", "Nacht", "ent\u00b7schlei\u00b7ert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PDAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Geh, du \u00fcberschweppernder Pokal,", "tokens": ["Geh", ",", "du", "\u00fc\u00b7bersc\u00b7hwep\u00b7pern\u00b7der", "Po\u00b7kal", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "---+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "L\u00f6se nun die Zungen, bis der Saal", "tokens": ["L\u00f6\u00b7se", "nun", "die", "Zun\u00b7gen", ",", "bis", "der", "Saal"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Von Gesang und Scherz und Lachen sch\u00fcttert!", "tokens": ["Von", "Ge\u00b7sang", "und", "Scherz", "und", "La\u00b7chen", "sch\u00fct\u00b7tert", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Keiner setze voll dich wieder hin!", "tokens": ["Kei\u00b7ner", "set\u00b7ze", "voll", "dich", "wie\u00b7der", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADJD", "PPER", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Sag' ihm, da\u00df er sonst der Enkelin", "tokens": ["Sag'", "ihm", ",", "da\u00df", "er", "sonst", "der", "En\u00b7ke\u00b7lin"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "KOUS", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.12": {"text": "Deines Herrn, den Hochzeitwein verbittert!", "tokens": ["Dei\u00b7nes", "Herrn", ",", "den", "Hoch\u00b7zeit\u00b7wein", "ver\u00b7bit\u00b7tert", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Denn, verschonend fristete die Zeit", "tokens": ["Denn", ",", "ver\u00b7scho\u00b7nend", "fris\u00b7te\u00b7te", "die", "Zeit"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "VVPP", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.14": {"text": "Dein zerbrechlich Leben, eingeweiht", "tokens": ["Dein", "zer\u00b7brech\u00b7lich", "Le\u00b7ben", ",", "ein\u00b7ge\u00b7weiht"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "ADJD", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.15": {"text": "Vor dem Traualtar' auch sie zu sehen,", "tokens": ["Vor", "dem", "Trau\u00b7al\u00b7ta\u00b7r'", "auch", "sie", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "PPER", "PTKZU", "VVINF", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.16": {"text": "Und mit W\u00fcnschen f\u00fcr den wackern Mann,", "tokens": ["Und", "mit", "W\u00fcn\u00b7schen", "f\u00fcr", "den", "wa\u00b7ckern", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.17": {"text": "Der in ihr ein Engelherz gewann,", "tokens": ["Der", "in", "ihr", "ein", "En\u00b7gel\u00b7herz", "ge\u00b7wann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.18": {"text": "Dreimal in die Rund' herum zu gehen.", "tokens": ["Drei\u00b7mal", "in", "die", "Rund'", "he\u00b7rum", "zu", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "APZR", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "Dreimal auch mit W\u00fcnschen f\u00fcr den Arm,", "tokens": ["Drei\u00b7mal", "auch", "mit", "W\u00fcn\u00b7schen", "f\u00fcr", "den", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.20": {"text": "Der zur Schlacht der Feinde gr\u00f6\u00dfern Schwarm", "tokens": ["Der", "zur", "Schlacht", "der", "Fein\u00b7de", "gr\u00f6\u00b7\u00dfern", "Schwarm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPRART", "NN", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.21": {"text": "Lange schon, auf ihrem Boden, winkte!", "tokens": ["Lan\u00b7ge", "schon", ",", "auf", "ih\u00b7rem", "Bo\u00b7den", ",", "wink\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "$,", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.22": {"text": "Dreimal f\u00fcr das Schwert in ", "tokens": ["Drei\u00b7mal", "f\u00fcr", "das", "Schwert", "in"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "APPR"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.23": {"text": "Das so oft ein Lorbeerkranz umwand,", "tokens": ["Das", "so", "oft", "ein", "Lor\u00b7beer\u00b7kranz", "um\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.24": {"text": "Als es vor dem Heer' im Schlachtfeld' blinkte.", "tokens": ["Als", "es", "vor", "dem", "Heer'", "im", "Schlacht\u00b7feld'", "blink\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.25": {"text": "Aber wenn zuletzt sich m\u00e4uschenstill,", "tokens": ["A\u00b7ber", "wenn", "zu\u00b7letzt", "sich", "m\u00e4u\u00b7schen\u00b7still", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "PRF", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.26": {"text": "Grade wenn man recht ihn fangen will,", "tokens": ["Gra\u00b7de", "wenn", "man", "recht", "ihn", "fan\u00b7gen", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PIS", "ADJD", "PPER", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.27": {"text": "Unser Br\u00e4utigam davon gestohlen,", "tokens": ["Un\u00b7ser", "Br\u00e4u\u00b7ti\u00b7gam", "da\u00b7von", "ge\u00b7stoh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PAV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.28": {"text": "Und der Saal, so fest er immer steht,", "tokens": ["Und", "der", "Saal", ",", "so", "fest", "er", "im\u00b7mer", "steht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ADV", "ADJD", "PPER", "ADV", "VVFIN", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.29": {"text": "Um die G\u00e4ste sich im Kreise dreht,", "tokens": ["Um", "die", "G\u00e4s\u00b7te", "sich", "im", "Krei\u00b7se", "dreht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.30": {"text": "Bis sie Abschied stammelnd sich empfohlen:", "tokens": ["Bis", "sie", "Ab\u00b7schied", "stam\u00b7melnd", "sich", "emp\u00b7foh\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "PRF", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.31": {"text": "Dann, du freudenreicher Erbpokal,", "tokens": ["Dann", ",", "du", "freu\u00b7den\u00b7rei\u00b7cher", "Erb\u00b7po\u00b7kal", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.32": {"text": "F\u00fclle dich mein Weibchen noch einmal,", "tokens": ["F\u00fcl\u00b7le", "dich", "mein", "Weib\u00b7chen", "noch", "ein\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "ADV", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.33": {"text": "Steh' ich anders noch auf gleichen F\u00fc\u00dfen:", "tokens": ["Steh'", "ich", "an\u00b7ders", "noch", "auf", "glei\u00b7chen", "F\u00fc\u00b7\u00dfen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.34": {"text": "So, dich leer in H\u00e4nden haltend, soll", "tokens": ["So", ",", "dich", "leer", "in", "H\u00e4n\u00b7den", "hal\u00b7tend", ",", "soll"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "$,", "PRF", "ADJD", "APPR", "NN", "VVPP", "$,", "VMFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.35": {"text": "Mich Auror' als Ueberwinder wohl", "tokens": ["Mich", "Au\u00b7ror'", "als", "Ue\u00b7ber\u00b7win\u00b7der", "wohl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "NE", "KOUS", "NN", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.36": {"text": "Auf dem scherbenvollen Wahlplatz' gr\u00fc\u00dfen.", "tokens": ["Auf", "dem", "scher\u00b7ben\u00b7vol\u00b7len", "Wahl\u00b7platz'", "gr\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}