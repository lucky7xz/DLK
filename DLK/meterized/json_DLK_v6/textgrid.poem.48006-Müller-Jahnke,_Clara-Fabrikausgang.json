{"textgrid.poem.48006": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Fabrikausgang", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bleigraue Schatten zittern durch die Luft,", "tokens": ["Blei\u00b7grau\u00b7e", "Schat\u00b7ten", "zit\u00b7tern", "durch", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "aus hohen Essen quillt ein blauer Duft.", "tokens": ["aus", "ho\u00b7hen", "Es\u00b7sen", "quillt", "ein", "blau\u00b7er", "Duft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Durch Steingef\u00fcge dr\u00f6hnt der H\u00e4mmer Ton,", "tokens": ["Durch", "Stein\u00b7ge\u00b7f\u00fc\u00b7ge", "dr\u00f6hnt", "der", "H\u00e4m\u00b7mer", "Ton", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "um Erzge\u00e4st schwirrt dumpf die Transmission,", "tokens": ["um", "Erz\u00b7ge\u00b7\u00e4st", "schwirrt", "dumpf", "die", "Trans\u00b7mis\u00b7si\u00b7on", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "schwirrt stumpf und dumpf, noch eh' die Sonne kam", "tokens": ["schwirrt", "stumpf", "und", "dumpf", ",", "noch", "eh'", "die", "Son\u00b7ne", "kam"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "$,", "ADV", "KOUS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "bis da\u00df der Tag vergl\u00fcht in Zorn und Scham,", "tokens": ["bis", "da\u00df", "der", "Tag", "ver\u00b7gl\u00fcht", "in", "Zorn", "und", "Scham", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "bis da\u00df die Nacht barmherzig deckt die Qual \u2013", "tokens": ["bis", "da\u00df", "die", "Nacht", "barm\u00b7her\u00b7zig", "deckt", "die", "Qual", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "NN", "ADJD", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein Glockenzeichen gellt im Arbeitssaal.", "tokens": ["Ein", "Glo\u00b7cken\u00b7zei\u00b7chen", "gellt", "im", "Ar\u00b7beits\u00b7saal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da stockt der L\u00e4rm \u2013 und kreischend geht das Tor:", "tokens": ["Da", "stockt", "der", "L\u00e4rm", "\u2013", "und", "krei\u00b7schend", "geht", "das", "Tor", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$(", "KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ein J\u00fcngling st\u00fcrmt, ein Knabe fast, hervor;", "tokens": ["Ein", "J\u00fcng\u00b7ling", "st\u00fcrmt", ",", "ein", "Kna\u00b7be", "fast", ",", "her\u00b7vor", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "ADV", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "im staubigen Rock, die M\u00fctze im Genick,", "tokens": ["im", "stau\u00b7bi\u00b7gen", "Rock", ",", "die", "M\u00fct\u00b7ze", "im", "Ge\u00b7nick", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "ein frohes Leuchten noch im Kinderblick,", "tokens": ["ein", "fro\u00b7hes", "Leuch\u00b7ten", "noch", "im", "Kin\u00b7der\u00b7blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "staunt er die Welt wie neugeboren an \u2013", "tokens": ["staunt", "er", "die", "Welt", "wie", "neu\u00b7ge\u00b7bo\u00b7ren", "an", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "KOKOM", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "da schiebt ihn seitw\u00e4rts schon sein Nebenmann.", "tokens": ["da", "schiebt", "ihn", "seit\u00b7w\u00e4rts", "schon", "sein", "Ne\u00b7ben\u00b7mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Da dr\u00e4ngt's hervor wie fl\u00fcgellahme Brut,", "tokens": ["Da", "dr\u00e4ngt's", "her\u00b7vor", "wie", "fl\u00fc\u00b7gel\u00b7lah\u00b7me", "Brut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "da w\u00e4chst und wogt des Elends graue Flut:", "tokens": ["da", "w\u00e4chst", "und", "wogt", "des", "E\u00b7lends", "grau\u00b7e", "Flut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Mit bangem Blick die blasse Mutter hier, \u2013", "tokens": ["Mit", "ban\u00b7gem", "Blick", "die", "blas\u00b7se", "Mut\u00b7ter", "hier", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "ADV", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "zu Hause weint der S\u00e4ugling schon nach ihr.", "tokens": ["zu", "Hau\u00b7se", "weint", "der", "S\u00e4ug\u00b7ling", "schon", "nach", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das M\u00e4del dort, Chrysanthemum am Hut,", "tokens": ["Das", "M\u00e4\u00b7del", "dort", ",", "Chry\u00b7san\u00b7the\u00b7mum", "am", "Hut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "NE", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "\u2013 in flacher Brust erlogne Liebesglut, \u2013", "tokens": ["\u2013", "in", "fla\u00b7cher", "Brust", "er\u00b7log\u00b7ne", "Lie\u00b7bes\u00b7glut", ",", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "ADJA", "NN", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "das frech vertraut dem n\u00e4chsten Burschen nickt, \u2013", "tokens": ["das", "frech", "ver\u00b7traut", "dem", "n\u00e4chs\u00b7ten", "Bur\u00b7schen", "nickt", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJD", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "der Mann, der stieren Auges vor sich blickt, \u2013", "tokens": ["der", "Mann", ",", "der", "stie\u00b7ren", "Au\u00b7ges", "vor", "sich", "blickt", ",", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "APPR", "PRF", "VVFIN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "und nun der Greis, der matt nach Hause wankt", "tokens": ["und", "nun", "der", "Greis", ",", "der", "matt", "nach", "Hau\u00b7se", "wankt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "$,", "PRELS", "ADJD", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "und f\u00fcr den Hungerlohn dem Sch\u00f6pfer dankt . . .", "tokens": ["und", "f\u00fcr", "den", "Hun\u00b7ger\u00b7lohn", "dem", "Sch\u00f6p\u00b7fer", "dankt", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$.", "$.", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Des Landes Mark, der Gro\u00dfstadt Kraft und Glut", "tokens": ["Des", "Lan\u00b7des", "Mark", ",", "der", "Gro\u00df\u00b7stadt", "Kraft", "und", "Glut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "verschlingt des Elends uferlose Flut.", "tokens": ["ver\u00b7schlingt", "des", "E\u00b7lends", "u\u00b7fer\u00b7lo\u00b7se", "Flut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Mit m\u00fcdem Schritt, die Stirn gesenkt und schwer,", "tokens": ["Mit", "m\u00fc\u00b7dem", "Schritt", ",", "die", "Stirn", "ge\u00b7senkt", "und", "schwer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ART", "NN", "VVPP", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "zur Heimstatt zieht der Arbeit Sklavenheer,", "tokens": ["zur", "Heims\u00b7tatt", "zieht", "der", "Ar\u00b7beit", "Skla\u00b7ven\u00b7heer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "zu kurzer Rast, da\u00df schlafgest\u00e4rkt die Kraft", "tokens": ["zu", "kur\u00b7zer", "Rast", ",", "da\u00df", "schlaf\u00b7ge\u00b7st\u00e4rkt", "die", "Kraft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "KOUS", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "beim n\u00e4chsten Morgengraun aufs neue schafft.", "tokens": ["beim", "n\u00e4chs\u00b7ten", "Mor\u00b7gen\u00b7graun", "aufs", "neu\u00b7e", "schafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPRART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mit frischer Gier, mit niegestillter Wut", "tokens": ["Mit", "fri\u00b7scher", "Gier", ",", "mit", "nie\u00b7ge\u00b7still\u00b7ter", "Wut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "trinkt die Maschine ihres Herzens Blut.", "tokens": ["trinkt", "die", "Ma\u00b7schi\u00b7ne", "ih\u00b7res", "Her\u00b7zens", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.9": {"line.1": {"text": "Vor\u00fcberziehn, in seltsam scheuer Hast,", "tokens": ["Vor\u00b7\u00fc\u00b7ber\u00b7ziehn", ",", "in", "selt\u00b7sam", "scheu\u00b7er", "Hast", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "sie an der Arbeitsherren Prunkpalast:", "tokens": ["sie", "an", "der", "Ar\u00b7beits\u00b7her\u00b7ren", "Prunk\u00b7pa\u00b7last", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "den Tisch, der dort vor Ueberf\u00fclle bricht,", "tokens": ["den", "Tisch", ",", "der", "dort", "vor", "Ue\u00b7ber\u00b7f\u00fcl\u00b7le", "bricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Zwei M\u00e4nner nur, den Hammer in der Hand,", "tokens": ["Zwei", "M\u00e4n\u00b7ner", "nur", ",", "den", "Ham\u00b7mer", "in", "der", "Hand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADV", "$,", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "hemmen den Blick und starren unverwandt", "tokens": ["hem\u00b7men", "den", "Blick", "und", "star\u00b7ren", "un\u00b7ver\u00b7wandt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN", "ADJD"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "in all den Glast, der Freude goldenen Sitz;", "tokens": ["in", "all", "den", "Glast", ",", "der", "Freu\u00b7de", "gol\u00b7de\u00b7nen", "Sitz", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "NN", "$,", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "aus ihren Augen zuckt des Hasses Blitz.", "tokens": ["aus", "ih\u00b7ren", "Au\u00b7gen", "zuckt", "des", "Has\u00b7ses", "Blitz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "\u2013 So blickt der Leu, wenn sich die Schlange regt. \u2013", "tokens": ["\u2013", "So", "blickt", "der", "Leu", ",", "wenn", "sich", "die", "Schlan\u00b7ge", "reg\u00b7t.", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "abbreviation", "punct"], "pos": ["$(", "ADV", "VVFIN", "ART", "NN", "$,", "KOUS", "PRF", "ART", "NN", "NE", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "sie wissen ", "tokens": ["sie", "wis\u00b7sen"], "token_info": ["word", "word"], "pos": ["PPER", "VVINF"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "sie schaun ihr Ziel, so sternenlicht und weit . . .", "tokens": ["sie", "schaun", "ihr", "Ziel", ",", "so", "ster\u00b7nen\u00b7licht", "und", "weit", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$.", "$.", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und um sie braut die gro\u00dfe Einsamkeit,", "tokens": ["Und", "um", "sie", "braut", "die", "gro\u00b7\u00dfe", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "die schwere Ruh. \u2013", "tokens": ["die", "schwe\u00b7re", "Ruh", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Vom Himmel dichtgedr\u00e4ngt", "tokens": ["Vom", "Him\u00b7mel", "dicht\u00b7ge\u00b7dr\u00e4ngt"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "die schwarze Wolkenmasse niederh\u00e4ngt,", "tokens": ["die", "schwar\u00b7ze", "Wol\u00b7ken\u00b7mas\u00b7se", "nie\u00b7der\u00b7h\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "indes am freien Horizont verloht", "tokens": ["in\u00b7des", "am", "frei\u00b7en", "Ho\u00b7ri\u00b7zont", "ver\u00b7loht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "sturmdunklen Blicks ein blutig Abendrot.", "tokens": ["sturm\u00b7dun\u00b7klen", "Blicks", "ein", "blu\u00b7tig", "A\u00b7ben\u00b7drot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}