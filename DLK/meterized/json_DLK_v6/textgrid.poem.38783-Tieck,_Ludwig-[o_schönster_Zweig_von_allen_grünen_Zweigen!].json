{"textgrid.poem.38783": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "[o sch\u00f6nster Zweig von allen gr\u00fcnen Zweigen!]", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O sch\u00f6nster Zweig von allen gr\u00fcnen Zweigen!", "tokens": ["O", "sch\u00f6ns\u00b7ter", "Zweig", "von", "al\u00b7len", "gr\u00fc\u00b7nen", "Zwei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du Myrthenreis, wie ich dich vor mir sehe,", "tokens": ["Du", "Myr\u00b7then\u00b7reis", ",", "wie", "ich", "dich", "vor", "mir", "se\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PWAV", "PPER", "PRF", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Flieht vor dem stillen Gr\u00fcn mein Leid und Wehe,", "tokens": ["Flieht", "vor", "dem", "stil\u00b7len", "Gr\u00fcn", "mein", "Leid", "und", "We\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Wonneschau'r in meine Seele steigen.", "tokens": ["Und", "Won\u00b7ne\u00b7schau'r", "in", "mei\u00b7ne", "See\u00b7le", "stei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mein bist du jetzt; als du noch dorten eigen", "tokens": ["Mein", "bist", "du", "jetzt", ";", "als", "du", "noch", "dor\u00b7ten", "ei\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "VAFIN", "PPER", "ADV", "$.", "KOUS", "PPER", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der reinsten Brust, war mir der Fittig wehn", "tokens": ["Der", "reins\u00b7ten", "Brust", ",", "war", "mir", "der", "Fit\u00b7tig", "wehn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "VAFIN", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Des Lenzes schon, es schwebt in deiner N\u00e4he", "tokens": ["Des", "Len\u00b7zes", "schon", ",", "es", "schwebt", "in", "dei\u00b7ner", "N\u00e4\u00b7he"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Herz, es will ein Kreuz sich dunkel zeigen.", "tokens": ["Ein", "Herz", ",", "es", "will", "ein", "Kreuz", "sich", "dun\u00b7kel", "zei\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VMFIN", "ART", "NN", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie schlang das kleine Herz sich an die Myrthe,", "tokens": ["Wie", "schlang", "das", "klei\u00b7ne", "Herz", "sich", "an", "die", "Myr\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als wollten sie liebkosen sich in K\u00fcssen,", "tokens": ["Als", "woll\u00b7ten", "sie", "lieb\u00b7ko\u00b7sen", "sich", "in", "K\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als wenn ein T\u00e4ubchen um ein Bl\u00fcmlein girrte;", "tokens": ["Als", "wenn", "ein", "T\u00e4ub\u00b7chen", "um", "ein", "Bl\u00fcm\u00b7lein", "girr\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ach, Kreuz, gedr\u00fcckt von dir wurden zerrissen", "tokens": ["Ach", ",", "Kreuz", ",", "ge\u00b7dr\u00fcckt", "von", "dir", "wur\u00b7den", "zer\u00b7ris\u00b7sen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "NN", "$,", "VVPP", "APPR", "PPER", "VAFIN", "VVPP"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die zarten Bl\u00e4tter: jetzges, k\u00fcnftges Weinen,", "tokens": ["Die", "zar\u00b7ten", "Bl\u00e4t\u00b7ter", ":", "jetz\u00b7ges", ",", "k\u00fcnft\u00b7ges", "Wei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mein Gl\u00fcck, mein Leid sah' ich mir so erscheinen.", "tokens": ["Mein", "Gl\u00fcck", ",", "mein", "Leid", "sah'", "ich", "mir", "so", "er\u00b7schei\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "VVFIN", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}