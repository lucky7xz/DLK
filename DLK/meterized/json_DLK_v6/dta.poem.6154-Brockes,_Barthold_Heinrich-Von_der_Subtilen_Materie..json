{"dta.poem.6154": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Subtilen Materie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Er nimmt, indem er sich da, wo es leer ist, sencket;", "tokens": ["Er", "nimmt", ",", "in\u00b7dem", "er", "sich", "da", ",", "wo", "es", "leer", "ist", ",", "sen\u00b7cket", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PRF", "ADV", "$,", "PWAV", "PPER", "ADJD", "VAFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Stets allerley Gestalten an,", "tokens": ["Stets", "al\u00b7ler\u00b7ley", "Ge\u00b7stal\u00b7ten", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durchdringt das festeste, weil nichts ihn hemmen kan.", "tokens": ["Durch\u00b7dringt", "das", "fes\u00b7tes\u00b7te", ",", "weil", "nichts", "ihn", "hem\u00b7men", "kan", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "$,", "KOUS", "PIS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Lufft, der Himmels-Lufft geheime Federn lencket,", "tokens": ["Der", "Lufft", ",", "der", "Him\u00b7mels\u00b7Lufft", "ge\u00b7hei\u00b7me", "Fe\u00b7dern", "len\u00b7cket", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sein\u2019 immerfl\u00fc\u00dfige Beschaffenheit,", "tokens": ["Sein'", "im\u00b7mer\u00b7fl\u00fc\u00b7\u00dfi\u00b7ge", "Be\u00b7schaf\u00b7fen\u00b7heit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die sich so leicht, so fertig reget:", "tokens": ["Die", "sich", "so", "leicht", ",", "so", "fer\u00b7tig", "re\u00b7get", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "ADJD", "$,", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sein Druck der w\u00fcrckenden Geschwindigkeit", "tokens": ["Sein", "Druck", "der", "w\u00fcr\u00b7cken\u00b7den", "Ge\u00b7schwin\u00b7dig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "F\u00e4ngt die Bewegung an, die alle Welt beweget.", "tokens": ["F\u00e4ngt", "die", "Be\u00b7we\u00b7gung", "an", ",", "die", "al\u00b7le", "Welt", "be\u00b7we\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}