{"textgrid.poem.57276": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Willkommen, o silberner Mond,", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willkommen, o silberner Mond,", "tokens": ["Will\u00b7kom\u00b7men", ",", "o", "sil\u00b7ber\u00b7ner", "Mond", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Sch\u00f6ner, stiller Gef\u00e4hrt der Nacht!", "tokens": ["Sch\u00f6\u00b7ner", ",", "stil\u00b7ler", "Ge\u00b7f\u00e4hrt", "der", "Nacht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Du entfliehst? Eile nicht, bleib, Gedankenfreund!", "tokens": ["Du", "ent\u00b7fliehst", "?", "Ei\u00b7le", "nicht", ",", "bleib", ",", "Ge\u00b7dan\u00b7ken\u00b7freund", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NN", "PTKNEG", "$,", "VVFIN", "$,", "NN", "$."], "meter": "--+--++-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Sehet, er bleibt, das Gew\u00f6lk wallte nur hin.", "tokens": ["Se\u00b7het", ",", "er", "bleibt", ",", "das", "Ge\u00b7w\u00f6lk", "wall\u00b7te", "nur", "hin", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "$,", "ART", "NN", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "+--+--+---+", "measure": "dactylic.tri.plus"}}, "stanza.2": {"line.1": {"text": "Des Mayes Erwachen ist nur", "tokens": ["Des", "Ma\u00b7yes", "Er\u00b7wa\u00b7chen", "ist", "nur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Sch\u00f6ner noch wie die Sommernacht,", "tokens": ["Sch\u00f6\u00b7ner", "noch", "wie", "die", "Som\u00b7mer\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KOKOM", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Wenn ihm Thau, hell wie Licht, aus der Locke tr\u00e4uft,", "tokens": ["Wenn", "ihm", "Thau", ",", "hell", "wie", "Licht", ",", "aus", "der", "Lo\u00b7cke", "tr\u00e4uft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "$,", "ADJD", "KOKOM", "NN", "$,", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Und zu dem H\u00fcgel herauf r\u00f6thlich er k\u00f6mt.", "tokens": ["Und", "zu", "dem", "H\u00fc\u00b7gel", "her\u00b7auf", "r\u00f6th\u00b7lich", "er", "k\u00f6mt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "ADJD", "PPER", "VVFIN", "$."], "meter": "---+---+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Ihr Edleren, ach es bew\u00e4chst", "tokens": ["Ihr", "Ed\u00b7le\u00b7ren", ",", "ach", "es", "be\u00b7w\u00e4chst"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "XY", "PPER", "VVFIN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Eure Maale schon ernstes Moos!", "tokens": ["Eu\u00b7re", "Maa\u00b7le", "schon", "erns\u00b7tes", "Moos", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "O wie war gl\u00fccklich ich, als ich noch mit euch", "tokens": ["O", "wie", "war", "gl\u00fcck\u00b7lich", "ich", ",", "als", "ich", "noch", "mit", "euch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "VAFIN", "ADJD", "PPER", "$,", "KOUS", "PPER", "ADV", "APPR", "PPER"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "S\u00e4he sich r\u00f6then den Tag, schimmern die Nacht.", "tokens": ["S\u00e4\u00b7he", "sich", "r\u00f6\u00b7then", "den", "Tag", ",", "schim\u00b7mern", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+--+--++--+", "measure": "dactylic.di.plus"}}}}}