{"textgrid.poem.64036": {"metadata": {"author": {"name": "Ahlefeld, Charlotte von", "birth": "N.A.", "death": "N.A."}, "title": "Beim Abschied", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wirst Du in der Ferne mein gedenken,", "tokens": ["Wirst", "Du", "in", "der", "Fer\u00b7ne", "mein", "ge\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "PPOSAT", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wenn die Welt ger\u00e4uschvoll Dich zerstreut?", "tokens": ["Wenn", "die", "Welt", "ge\u00b7r\u00e4uschvoll", "Dich", "zer\u00b7streut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "PPER", "VVPP", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Wirst Du oft mir stille Stunden schenken,", "tokens": ["Wirst", "Du", "oft", "mir", "stil\u00b7le", "Stun\u00b7den", "schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Der Erinnrung unsres Gl\u00fccks geweiht?", "tokens": ["Der", "E\u00b7rinn\u00b7rung", "uns\u00b7res", "Gl\u00fccks", "ge\u00b7weiht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Wird kein neues Band mir Dein Vertrauen,", "tokens": ["Wird", "kein", "neu\u00b7es", "Band", "mir", "Dein", "Ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ADJA", "NN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Keines Deine Liebe mir entziehn?", "tokens": ["Kei\u00b7nes", "Dei\u00b7ne", "Lie\u00b7be", "mir", "ent\u00b7ziehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Kann ich ganz auf Deine Treue bauen,", "tokens": ["Kann", "ich", "ganz", "auf", "Dei\u00b7ne", "Treu\u00b7e", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "O so nimm mein Herz auf ewig hin!", "tokens": ["O", "so", "nimm", "mein", "Herz", "auf", "e\u00b7wig", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVIMP", "PPOSAT", "NN", "APPR", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Immer bleibt es z\u00e4rtlich Dir ergeben,", "tokens": ["Im\u00b7mer", "bleibt", "es", "z\u00e4rt\u00b7lich", "Dir", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Auch wenn nie mein Blick Dich wiedersieht.", "tokens": ["Auch", "wenn", "nie", "mein", "Blick", "Dich", "wie\u00b7der\u00b7sieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ADV", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wenn getrennt von Dir mein tr\u00fcbes Leben", "tokens": ["Wenn", "ge\u00b7trennt", "von", "Dir", "mein", "tr\u00fc\u00b7bes", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VVPP", "APPR", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wie ein Seufzerhauch vor\u00fcber flieht.", "tokens": ["Wie", "ein", "Seuf\u00b7zer\u00b7hauch", "vor\u00b7\u00fc\u00b7ber", "flieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Ach so viele heucheln nur Gef\u00fchle", "tokens": ["Ach", "so", "vie\u00b7le", "heu\u00b7cheln", "nur", "Ge\u00b7f\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "PIAT", "NN", "ADV", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Einer nie gekannten Innigkeit;", "tokens": ["Ei\u00b7ner", "nie", "ge\u00b7kann\u00b7ten", "In\u00b7nig\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und in dem zerstreuenden Gew\u00fchle", "tokens": ["Und", "in", "dem", "zer\u00b7streu\u00b7en\u00b7den", "Ge\u00b7w\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Endet schnell der Schwur der Ewigkeit.", "tokens": ["En\u00b7det", "schnell", "der", "Schwur", "der", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Darum will ich nicht Dir Treue schw\u00f6ren,", "tokens": ["Da\u00b7rum", "will", "ich", "nicht", "Dir", "Treu\u00b7e", "schw\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "PTKNEG", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Aber fest und liebend halt' ich sie,", "tokens": ["A\u00b7ber", "fest", "und", "lie\u00b7bend", "halt'", "ich", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "VVFIN", "PPER", "PPER", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und die Zukunft soll Dir ewig lehren", "tokens": ["Und", "die", "Zu\u00b7kunft", "soll", "Dir", "e\u00b7wig", "leh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Deiner Freundinn Herz vergisst Dich nie.", "tokens": ["Dei\u00b7ner", "Freun\u00b7dinn", "Herz", "ver\u00b7gisst", "Dich", "nie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}