{"textgrid.poem.63135": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "29", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die sich meinethalb entbl\u00f6\u00dften,", "tokens": ["Die", "sich", "mei\u00b7net\u00b7halb", "ent\u00b7bl\u00f6\u00df\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wegen mir wie G\u00e4nse r\u00f6sten", "tokens": ["We\u00b7gen", "mir", "wie", "G\u00e4n\u00b7se", "r\u00f6s\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "KOKOM", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In der allertiefsten H\u00f6lle,", "tokens": ["In", "der", "al\u00b7ler\u00b7tiefs\u00b7ten", "H\u00f6l\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denen ich Gel\u00e4ut und Schelle", "tokens": ["De\u00b7nen", "ich", "Ge\u00b7l\u00e4ut", "und", "Schel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "PPER", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Um die Narrenh\u00e4lse hing.", "tokens": ["Um", "die", "Nar\u00b7ren\u00b7h\u00e4l\u00b7se", "hing", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Alle Jungfraun, die ich fing,", "tokens": ["Al\u00b7le", "Jung\u00b7fraun", ",", "die", "ich", "fing", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "(frug nicht erst um Eh und Freiung)", "tokens": ["(", "frug", "nicht", "erst", "um", "Eh", "und", "Frei\u00b7ung", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKNEG", "ADV", "APPR", "NN", "KON", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Villon bittet um Verzeihung.", "tokens": ["Vil\u00b7lon", "bit\u00b7tet", "um", "Ver\u00b7zei\u00b7hung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Jene braven Polizisten,", "tokens": ["Je\u00b7ne", "bra\u00b7ven", "Po\u00b7li\u00b7zis\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die mit plumpen Schergen-Listen", "tokens": ["Die", "mit", "plum\u00b7pen", "Scher\u00b7gen\u00b7Lis\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hinter mir und meiner Bande", "tokens": ["Hin\u00b7ter", "mir", "und", "mei\u00b7ner", "Ban\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jagten kreuz und quer im Lande,", "tokens": ["Jag\u00b7ten", "kreuz", "und", "quer", "im", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Meine M\u00f6rder, meine R\u00e4uber,", "tokens": ["Mei\u00b7ne", "M\u00f6r\u00b7der", ",", "mei\u00b7ne", "R\u00e4u\u00b7ber", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Meine Ruh- und Zeitvertreiber,", "tokens": ["Mei\u00b7ne", "Ruh", "und", "Zeit\u00b7ver\u00b7trei\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "TRUNC", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Die ich brauchte zur Belebung -", "tokens": ["Die", "ich", "brauch\u00b7te", "zur", "Be\u00b7le\u00b7bung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "APPRART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Villon bittet um Vergebung.", "tokens": ["Vil\u00b7lon", "bit\u00b7tet", "um", "Ver\u00b7ge\u00b7bung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Meine W\u00fcnsche sind wie Algen:", "tokens": ["Mei\u00b7ne", "W\u00fcn\u00b7sche", "sind", "wie", "Al\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "KOKOM", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Baut eintausend feste Galgen", "tokens": ["Baut", "ein\u00b7tau\u00b7send", "fes\u00b7te", "Gal\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle meine guten Freunde,", "tokens": ["Al\u00b7le", "mei\u00b7ne", "gu\u00b7ten", "Freun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine herzliche Gemeinde,", "tokens": ["Mei\u00b7ne", "herz\u00b7li\u00b7che", "Ge\u00b7mein\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "H\u00e4ngt sie auf in langer Reihe -", "tokens": ["H\u00e4ngt", "sie", "auf", "in", "lan\u00b7ger", "Rei\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df ich ihnen gern verzeihe \u2013", "tokens": ["Da\u00df", "ich", "ih\u00b7nen", "gern", "ver\u00b7zei\u00b7he", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Von Paris bis Roussillon,", "tokens": ["Von", "Pa\u00b7ris", "bis", "Rous\u00b7sil\u00b7lon", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "NE", "$,"], "meter": "--+-++-", "measure": "anapaest.init"}, "line.8": {"text": "Villon bittet um Pardon...", "tokens": ["Vil\u00b7lon", "bit\u00b7tet", "um", "Par\u00b7don", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}