n hast die nicht gemeine kunst,", "tokens": ["du", "f\u00fcr", "dich", "ei\u00b7gen", "hast", "die", "nicht", "ge\u00b7mei\u00b7ne", "kunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "ADJD", "VAFIN", "ART", "PTKNEG", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "da\u00df weder des hofs glanz, davon du lang gebeizet,", "tokens": ["da\u00df", "we\u00b7der", "des", "hofs", "glanz", ",", "da\u00b7von", "du", "lang", "ge\u00b7bei\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KON", "ART", "ADJA", "NN", "$,", "PAV", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.46": {"text": "noch ehr, gewin und gl\u00fcck dich von dir abgereizet;", "tokens": ["noch", "ehr", ",", "ge\u00b7win", "und", "gl\u00fcck", "dich", "von", "dir", "ab\u00b7ge\u00b7rei\u00b7zet", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "NE", "KON", "VVIMP", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "also, gl\u00fcckselig du, allzeit zu bleiben reich", "tokens": ["al\u00b7so", ",", "gl\u00fcck\u00b7se\u00b7lig", "du", ",", "all\u00b7zeit", "zu", "blei\u00b7ben", "reich"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADJD", "PPER", "$,", "ADV", "PTKZU", "VVINF", "ADJD"], "meter": "+----+-+-+-+", "measure": "dactylic.init"}, "line.48": {"text": "bist stets geduldig, weis und (k\u00fcrzlich) dir selbs gleich.", "tokens": ["bist", "stets", "ge\u00b7dul\u00b7dig", ",", "weis", "und", "(", "k\u00fcrz\u00b7lich", ")", "dir", "selbs", "gleich", "."], "token_info": ["word", "word", "word", 