{"textgrid.poem.46168": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Zu ehren meines lieben freinds Dieterich Haaken", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der menschen wohn ist falsch, betr\u00fcglich die verj\u00e4hung,", "tokens": ["Der", "men\u00b7schen", "wohn", "ist", "falsch", ",", "be\u00b7tr\u00fcg\u00b7lich", "die", "ver\u00b7j\u00e4\u00b7hung", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "$,", "ADJD", "ART", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "als ob des gl\u00fccks allmacht, der ewigkeit versehung", "tokens": ["als", "ob", "des", "gl\u00fccks", "all\u00b7macht", ",", "der", "e\u00b7wig\u00b7keit", "ver\u00b7se\u00b7hung"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOKOM", "KOUS", "ART", "ADJA", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und auch des himmels will (mit zwang der g\u00f6tter hand", "tokens": ["und", "auch", "des", "him\u00b7mels", "will", "(", "mit", "zwang", "der", "g\u00f6t\u00b7ter", "hand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "VMFIN", "$(", "APPR", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "verk\u00fcrzend) ohn ihr schuld verhinderten den stand", "tokens": ["ver\u00b7k\u00fcr\u00b7zend", ")", "ohn", "ihr", "schuld", "ver\u00b7hin\u00b7der\u00b7ten", "den", "stand"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$(", "APPR", "PPOSAT", "ADJD", "VVFIN", "ART", "VVFIN"], "meter": "-+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "der menschen und der welt. Das werk recht zu besehen,", "tokens": ["der", "men\u00b7schen", "und", "der", "welt", ".", "Das", "werk", "recht", "zu", "be\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ART", "NN", "$.", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "so mu\u00df der mensch, da\u00df er die ursach selbs, gestehen.", "tokens": ["so", "mu\u00df", "der", "mensch", ",", "da\u00df", "er", "die", "ur\u00b7sach", "selbs", ",", "ge\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "$,", "KOUS", "PPER", "ART", "NN", "ADV", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dan ja ein jeder mensch, dem gr\u00f6sten k\u00f6nig gleich,", "tokens": ["Dan", "ja", "ein", "je\u00b7der", "mensch", ",", "dem", "gr\u00f6s\u00b7ten", "k\u00f6\u00b7nig", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "PIAT", "NN", "$,", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "hat der anmutungen und der begirden reich,", "tokens": ["hat", "der", "an\u00b7mu\u00b7tun\u00b7gen", "und", "der", "be\u00b7gir\u00b7den", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "KON", "ART", "ADJA", "ADJD", "$,"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "die seine vernunft stets soll meistern, zu regieren,", "tokens": ["die", "sei\u00b7ne", "ver\u00b7nunft", "stets", "soll", "meis\u00b7tern", ",", "zu", "re\u00b7gie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "VMFIN", "VVINF", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "und sie, was farb und schein sie auch in dem schild f\u00fchren,", "tokens": ["und", "sie", ",", "was", "farb", "und", "schein", "sie", "auch", "in", "dem", "schild", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PWS", "VVFIN", "KON", "ADJD", "PPER", "ADV", "APPR", "ART", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "zu b\u00fc\u00dfen ihren lust (als schmeichler) ihres theils", "tokens": ["zu", "b\u00fc\u00b7\u00dfen", "ih\u00b7ren", "lust", "(", "als", "schmeich\u00b7ler", ")", "ih\u00b7res", "theils"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PTKZU", "VVINF", "PPOSAT", "NN", "$(", "KOUS", "ADJA", "$(", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "vergessen oft des reichs und ihres f\u00fcrstenheils.", "tokens": ["ver\u00b7ges\u00b7sen", "oft", "des", "reichs", "und", "ih\u00b7res", "f\u00fcrs\u00b7ten\u00b7heils", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Da wil der menschen herz der sch\u00f6nheit sich ergeben,", "tokens": ["Da", "wil", "der", "men\u00b7schen", "herz", "der", "sch\u00f6n\u00b7heit", "sich", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "dort ein krauslechtes haar kan seine seel verweben,", "tokens": ["dort", "ein", "kraus\u00b7lech\u00b7tes", "haar", "kan", "sei\u00b7ne", "seel", "ver\u00b7we\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VMFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "hie eines augs anblick, mehr dan ein scharfer blitz,", "tokens": ["hie", "ei\u00b7nes", "augs", "an\u00b7blick", ",", "mehr", "dan", "ein", "schar\u00b7fer", "blitz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NE", "$,", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "dort eine wei\u00dfe hand beraubet ihn der witz;", "tokens": ["dort", "ei\u00b7ne", "wei\u00b7\u00dfe", "hand", "be\u00b7rau\u00b7bet", "ihn", "der", "witz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "ja, ein geschm\u00f6ll, ein wort, ein seufz kan nach gefallen", "tokens": ["ja", ",", "ein", "ge\u00b7schm\u00f6ll", ",", "ein", "wort", ",", "ein", "seufz", "kan", "nach", "ge\u00b7fal\u00b7len"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ART", "ADJD", "$,", "ART", "NN", "$,", "ART", "NN", "VMFIN", "APPR", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "der vernunft mayestet zu fu\u00df ihm machen fallen.", "tokens": ["der", "ver\u00b7nunft", "ma\u00b7ye\u00b7stet", "zu", "fu\u00df", "ihm", "ma\u00b7chen", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PTKVZ", "PPER", "VVINF", "VVINF", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.19": {"text": "Bald hochmut, hofnung, lust, freud, ehrgeiz, schimpf und schmach,", "tokens": ["Bald", "hoch\u00b7mut", ",", "hof\u00b7nung", ",", "lust", ",", "freud", ",", "ehr\u00b7geiz", ",", "schimpf", "und", "schmach", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "NN", "$,", "VVFIN", "$,", "VVFIN", "$,", "KOUS", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "bald kleinmut, forcht, neid, ha\u00df, verdru\u00df, leid, zorn und rach", "tokens": ["bald", "klein\u00b7mut", ",", "forcht", ",", "neid", ",", "ha\u00df", ",", "ver\u00b7dru\u00df", ",", "leid", ",", "zorn", "und", "rach"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "NN", "$,", "ADJD", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "verfinstern seinen tag, als ihres herrens meister", "tokens": ["ver\u00b7fins\u00b7tern", "sei\u00b7nen", "tag", ",", "als", "ih\u00b7res", "her\u00b7rens", "meis\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "und feuren seine nacht als ungeheure geister,", "tokens": ["und", "feu\u00b7ren", "sei\u00b7ne", "nacht", "als", "un\u00b7ge\u00b7heu\u00b7re", "geis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "KOUS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "so da\u00df der arme mensch, torecht und ungerecht,", "tokens": ["so", "da\u00df", "der", "ar\u00b7me", "mensch", ",", "to\u00b7recht", "und", "un\u00b7ge\u00b7recht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "ein k\u00f6nig von geburt, wird seiner knechten knecht.", "tokens": ["ein", "k\u00f6\u00b7nig", "von", "ge\u00b7burt", ",", "wird", "sei\u00b7ner", "knech\u00b7ten", "knecht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "NE", "$,", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Und wie er auch sein lob vermeinet zu verbl\u00fcmen,", "tokens": ["Und", "wie", "er", "auch", "sein", "lob", "ver\u00b7mei\u00b7net", "zu", "ver\u00b7bl\u00fc\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "so hat doch billich er vollkommen nichts zu r\u00fchmen.", "tokens": ["so", "hat", "doch", "bil\u00b7lich", "er", "voll\u00b7kom\u00b7men", "nichts", "zu", "r\u00fch\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADJD", "PPER", "ADJD", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "ein hagel, ein sturmwind, ein wogen in dem meer,", "tokens": ["ein", "ha\u00b7gel", ",", "ein", "sturm\u00b7wind", ",", "ein", "wo\u00b7gen", "in", "dem", "meer", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJD", "$,", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.28": {"text": "ein schu\u00df, streich, stich, fall, thier, so leichtlich als ein heer,", "tokens": ["ein", "schu\u00df", ",", "streich", ",", "stich", ",", "fall", ",", "thier", ",", "so", "leicht\u00b7lich", "als", "ein", "heer", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "$,", "ADJD", "$,", "NN", "$,", "ADJD", "$,", "ADV", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "ja des hofs \u00fcberflu\u00df, der st\u00e4t und d\u00f6rfer s\u00fcnden", "tokens": ["ja", "des", "hofs", "\u00fc\u00b7berf\u00b7lu\u00df", ",", "der", "st\u00e4t", "und", "d\u00f6r\u00b7fer", "s\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "PTKVZ", "$,", "PRELS", "ADJD", "KON", "ADJD", "VVINF"], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.30": {"text": "die nach lust wider uns bald einen vortheil finden,", "tokens": ["die", "nach", "lust", "wi\u00b7der", "uns", "bald", "ei\u00b7nen", "vor\u00b7theil", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "VVFIN", "APPR", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "dem ackerman die ernt, dem kaufman all sein gut,", "tokens": ["dem", "ac\u00b7ker\u00b7man", "die", "ernt", ",", "dem", "kauf\u00b7man", "all", "sein", "gut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ART", "ADJD", "$,", "PRELS", "ADV", "PIAT", "PPOSAT", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "dem hofman seinen pracht, dem kriegsman seinen mut,", "tokens": ["dem", "hof\u00b7man", "sei\u00b7nen", "pracht", ",", "dem", "kriegs\u00b7man", "sei\u00b7nen", "mut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "dem b\u00fcrger seine ruh und jedem noch das leben", "tokens": ["dem", "b\u00fcr\u00b7ger", "sei\u00b7ne", "ruh", "und", "je\u00b7dem", "noch", "das", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "PPOSAT", "NN", "KON", "PIS", "ADV", "PDS", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "beraubend, sollen uns und jedem die lehr geben,", "tokens": ["be\u00b7rau\u00b7bend", ",", "sol\u00b7len", "uns", "und", "je\u00b7dem", "die", "lehr", "ge\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VMFIN", "PPER", "KON", "PIS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.35": {"text": "da\u00df wer von gro\u00dfem leid, von sorg, anfechtung, pein,", "tokens": ["da\u00df", "wer", "von", "gro\u00b7\u00dfem", "leid", ",", "von", "sorg", ",", "an\u00b7fech\u00b7tung", ",", "pein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "PWS", "APPR", "ADJA", "ADJD", "$,", "APPR", "NE", "$,", "NN", "$,", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "begehret, wa nicht frei, doch etwas los zu sein,", "tokens": ["be\u00b7ge\u00b7hret", ",", "wa", "nicht", "frei", ",", "doch", "et\u00b7was", "los", "zu", "sein", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PTKNEG", "ADJD", "$,", "ADV", "ADV", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.37": {"text": "das beste mittel ist, sich zu dem kreuz zu biegen", "tokens": ["das", "bes\u00b7te", "mit\u00b7tel", "ist", ",", "sich", "zu", "dem", "kreuz", "zu", "bie\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,", "PRF", "APPR", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "und mit der armut sich verbindend zu vern\u00fcgen.", "tokens": ["und", "mit", "der", "ar\u00b7mut", "sich", "ver\u00b7bin\u00b7dend", "zu", "ver\u00b7n\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PRF", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Nu dises lieds inhalt, als welches ich von dir,", "tokens": ["Nu", "di\u00b7ses", "lieds", "in\u00b7halt", ",", "als", "wel\u00b7ches", "ich", "von", "dir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word",