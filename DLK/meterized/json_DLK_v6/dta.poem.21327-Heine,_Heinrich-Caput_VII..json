{"dta.poem.21327": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput VII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Und jedesmal wenn ich ein Haus", "tokens": ["Und", "je\u00b7des\u00b7mal", "wenn", "ich", "ein", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bezeichnet in solcher Weise,", "tokens": ["Be\u00b7zeich\u00b7net", "in", "sol\u00b7cher", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ein Sterbegl\u00f6ckchen erscholl fernher,", "tokens": ["Ein", "Ster\u00b7be\u00b7gl\u00f6ck\u00b7chen", "er\u00b7scholl", "fern\u00b7her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wehm\u00fcthig wimmernd und leise.", "tokens": ["Weh\u00b7m\u00fct\u00b7hig", "wim\u00b7mernd", "und", "lei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "KON", "ADJD", "$."], "meter": "++-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Am Himmel aber erblich der Mond,", "tokens": ["Am", "Him\u00b7mel", "a\u00b7ber", "er\u00b7blich", "der", "Mond", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er wurde immer tr\u00fcber;", "tokens": ["Er", "wur\u00b7de", "im\u00b7mer", "tr\u00fc\u00b7ber", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gleich schwarzen Rossen jagten an ihm", "tokens": ["Gleich", "schwar\u00b7zen", "Ros\u00b7sen", "jag\u00b7ten", "an", "ihm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "APPR", "PPER"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Die wilden Wolken vor\u00fcber.", "tokens": ["Die", "wil\u00b7den", "Wol\u00b7ken", "vor\u00b7\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Und immer ging hinter mir einher", "tokens": ["Und", "im\u00b7mer", "ging", "hin\u00b7ter", "mir", "ein\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPR", "PPER", "PTKVZ"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit seinem verborgenen Beile", "tokens": ["Mit", "sei\u00b7nem", "ver\u00b7bor\u00b7ge\u00b7nen", "Bei\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Die dunkle Gestalt \u2013 so wanderten wir", "tokens": ["Die", "dunk\u00b7le", "Ge\u00b7stalt", "\u2013", "so", "wan\u00b7der\u00b7ten", "wir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "ADV", "VVFIN", "PPER"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wohl eine gute Weile.", "tokens": ["Wohl", "ei\u00b7ne", "gu\u00b7te", "Wei\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wir gehen und gehen, bis wir zuletzt", "tokens": ["Wir", "ge\u00b7hen", "und", "ge\u00b7hen", ",", "bis", "wir", "zu\u00b7letzt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVINF", "$,", "KOUS", "PPER", "ADV"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wieder zum Domplatz gelangen;", "tokens": ["Wie\u00b7der", "zum", "Dom\u00b7platz", "ge\u00b7lan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Weit offen standen die Pforten dort,", "tokens": ["Weit", "of\u00b7fen", "stan\u00b7den", "die", "Pfor\u00b7ten", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wir sind hineingegangen.", "tokens": ["Wir", "sind", "hin\u00b7ein\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}