{"textgrid.poem.61954": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "Drei Schlagworte", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie hei\u00dft das Wort, das in der halben Welt", "tokens": ["Wie", "hei\u00dft", "das", "Wort", ",", "das", "in", "der", "hal\u00b7ben", "Welt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$,", "PRELS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Man gleichbedeutend mit dem Gelde h\u00e4lt,", "tokens": ["Man", "gleich\u00b7be\u00b7deu\u00b7tend", "mit", "dem", "Gel\u00b7de", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch mit dem Geld, das stets im S\u00e4ckel bleibt,", "tokens": ["Doch", "mit", "dem", "Geld", ",", "das", "stets", "im", "S\u00e4\u00b7ckel", "bleibt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und schon von selbst die besten Zinsen treibt?", "tokens": ["Und", "schon", "von", "selbst", "die", "bes\u00b7ten", "Zin\u00b7sen", "treibt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es ist, es hei\u00dft die, die, die, die,", "tokens": ["Es", "ist", ",", "es", "hei\u00dft", "die", ",", "die", ",", "die", ",", "die", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PPER", "VVFIN", "ART", "$,", "PRELS", "$,", "PRELS", "$,", "PRELS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die teure Bourgeoisie!", "tokens": ["Die", "teu\u00b7re", "Bour\u00b7ge\u00b7o\u00b7i\u00b7sie", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie hei\u00dft das Wort, das in der halben Welt", "tokens": ["Wie", "hei\u00dft", "das", "Wort", ",", "das", "in", "der", "hal\u00b7ben", "Welt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$,", "PRELS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Man gleichbedeutend mit dem Elend h\u00e4lt,", "tokens": ["Man", "gleich\u00b7be\u00b7deu\u00b7tend", "mit", "dem", "E\u00b7lend", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch mit dem Elend, \u2013 das mit wack'rem Mut", "tokens": ["Doch", "mit", "dem", "E\u00b7lend", ",", "\u2013", "das", "mit", "wack'\u00b7rem", "Mut"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "$,", "$(", "PDS", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Die schwere, gro\u00dfe Arbeit tut?", "tokens": ["Die", "schwe\u00b7re", ",", "gro\u00b7\u00dfe", "Ar\u00b7beit", "tut", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es ist, es hei\u00dft: der, der, der, der,", "tokens": ["Es", "ist", ",", "es", "hei\u00dft", ":", "der", ",", "der", ",", "der", ",", "der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PPER", "VVFIN", "$.", "ART", "$,", "PRELS", "$,", "PRELS", "$,", "PRELS", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Es hei\u00dfet: Proletarier!", "tokens": ["Es", "hei\u00b7\u00dfet", ":", "Pro\u00b7le\u00b7ta\u00b7rier", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Wie hei\u00dft das Wort, das in der halben Welt", "tokens": ["Wie", "hei\u00dft", "das", "Wort", ",", "das", "in", "der", "hal\u00b7ben", "Welt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$,", "PRELS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Man gleichbedeutend mit Utopien h\u00e4lt,", "tokens": ["Man", "gleich\u00b7be\u00b7deu\u00b7tend", "mit", "U\u00b7to\u00b7pi\u00b7en", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Doch mit Utopien, \u00e4hnlich Morgenlicht,", "tokens": ["Doch", "mit", "U\u00b7to\u00b7pi\u00b7en", ",", "\u00e4hn\u00b7lich", "Mor\u00b7gen\u00b7licht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "$,", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Das hell und warm zu jedem Herzen spricht?", "tokens": ["Das", "hell", "und", "warm", "zu", "je\u00b7dem", "Her\u00b7zen", "spricht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "KON", "ADJD", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es ist, es ist mein Ideal,", "tokens": ["Es", "ist", ",", "es", "ist", "mein", "I\u00b7deal", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das gro\u00dfe Wort, es hei\u00dft: sozial.", "tokens": ["Das", "gro\u00b7\u00dfe", "Wort", ",", "es", "hei\u00dft", ":", "so\u00b7zi\u00b7al", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "$.", "ADV", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}}}}