{"dta.poem.5333": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich kam mir nunmehr vor, auf eine neue Weise,", "tokens": ["Ich", "kam", "mir", "nun\u00b7mehr", "vor", ",", "auf", "ei\u00b7ne", "neu\u00b7e", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von einem unumschr\u00e4nckten Kreise", "tokens": ["Von", "ei\u00b7nem", "un\u00b7um\u00b7schr\u00e4nck\u00b7ten", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Jm Mittel-Punct zu stehen,", "tokens": ["Jm", "Mit\u00b7tel\u00b7Punct", "zu", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und ein aus meiner Seel entsprungues Dencken", "tokens": ["Und", "ein", "aus", "mei\u00b7ner", "Seel", "ent\u00b7sprun\u00b7gues", "Den\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "In eine runde Tieff\u2019 ohn Um-Kreis zu versencken:", "tokens": ["In", "ei\u00b7ne", "run\u00b7de", "Tief\u00b7f'", "ohn", "Um\u00b7Kreis", "zu", "ver\u00b7sen\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Mein GOtt, ach la\u00df der Fluthen glattes Na\u00df;", "tokens": ["Mein", "Gott", ",", "ach", "la\u00df", "der", "Flut\u00b7hen", "glat\u00b7tes", "Na\u00df", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ITJ", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Des sch\u00f6nen Himmels Spiegel-Gla\u00df,", "tokens": ["Des", "sch\u00f6\u00b7nen", "Him\u00b7mels", "Spie\u00b7gel\u00b7Gla\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Des C\u00f6rpers Augen oft, doch nicht dem Aug\u2019 allein,", "tokens": ["Des", "C\u00f6r\u00b7pers", "Au\u00b7gen", "oft", ",", "doch", "nicht", "dem", "Aug'", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,", "ADV", "PTKNEG", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Auch meinem Geist auf diese Weise,", "tokens": ["Auch", "mei\u00b7nem", "Geist", "auf", "die\u00b7se", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "In den Erwegungen von diesem grossen Kreise,", "tokens": ["In", "den", "Er\u00b7we\u00b7gun\u00b7gen", "von", "die\u00b7sem", "gros\u00b7sen", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Dir, aller Sternen HErrn, zum Preise,", "tokens": ["Dir", ",", "al\u00b7ler", "Ster\u00b7nen", "Herrn", ",", "zum", "Prei\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PIAT", "NN", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Auch einen Himmels-Spiegel seyn.", "tokens": ["Auch", "ei\u00b7nen", "Him\u00b7mels\u00b7Spie\u00b7gel", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}