{"dta.poem.820": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "ViI.  Auf den Sontag des Felsens des Aufste-  \n hens vnd \u00e4rgernus/ oder nach der Geburt  \n  JESU.  Luc. 2.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Owunder! Gott ist Mensch/ die Mutter hat gebohren ", "tokens": ["O\u00b7wun\u00b7der", "!", "Gott", "ist", "Mensch", "/", "die", "Mut\u00b7ter", "hat", "ge\u00b7boh\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "NN", "VAFIN", "NN", "$(", "ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Jungfraw war vnd blieb/ der aller kr\u00e4ffte bind\u2019t", "tokens": ["Die", "Jung\u00b7fraw", "war", "vnd", "blieb", "/", "der", "al\u00b7ler", "kr\u00e4ff\u00b7te", "bind't"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "KON", "VVFIN", "$(", "ART", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Durch seiner wortte krafft/ lig\u2019t als ein schwaches Kindt", "tokens": ["Durch", "sei\u00b7ner", "wort\u00b7te", "krafft", "/", "lig't", "als", "ein", "schwa\u00b7ches", "Kindt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$(", "VVFIN", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gewindelt in ein Band/ vnd hat doch nicht verlohren", "tokens": ["Ge\u00b7win\u00b7delt", "in", "ein", "Band", "/", "vnd", "hat", "doch", "nicht", "ver\u00b7loh\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "$(", "KON", "VAFIN", "ADV", "PTKNEG", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was Gro\u00df v\u00f1 G\u00f6ttlich heist. der Held vorl\u00e4ngst verschwohr\u1ebd", "tokens": ["Was", "Gro\u00df", "v\u00f1", "G\u00f6tt\u00b7lich", "heist", ".", "der", "Held", "vor\u00b7l\u00e4ngst", "ver\u00b7schwohr\u1ebd"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "NE", "NE", "NE", "VVFIN", "$.", "ART", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem/ d\u1ebd der Starcke zwang/ kom\u2019t an/ vnd tilg\u2019t die S\u00fcnd/", "tokens": ["Dem", "/", "d\u1ebd", "der", "Star\u00b7cke", "zwang", "/", "kom't", "an", "/", "vnd", "til\u00b7g't", "die", "S\u00fcnd", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "NE", "ART", "NN", "VVFIN", "$(", "VVFIN", "PTKVZ", "$(", "KON", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "Er bawet was zust\u00f6rt vnd was er d\u00fcrfftig find\u2019t", "tokens": ["Er", "ba\u00b7wet", "was", "zu\u00b7st\u00f6rt", "vnd", "was", "er", "d\u00fcr\u00b7ff\u00b7tig", "find't"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "VVFIN", "KON", "PWS", "PPER", "ADJD", "VVFIN"], "meter": "-+---+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Das hat Er zu dem sitz der Herrligkeit erkohren.", "tokens": ["Das", "hat", "Er", "zu", "dem", "sitz", "der", "Herr\u00b7lig\u00b7keit", "er\u00b7koh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wol dem/ so bey jhm h\u00e4lt/ ob schon das blo\u00dfe Schwerdt", "tokens": ["Wol", "dem", "/", "so", "bey", "jhm", "h\u00e4lt", "/", "ob", "schon", "das", "blo\u00b7\u00dfe", "Schwerdt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "$(", "ADV", "APPR", "PPER", "VVFIN", "$(", "KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Scharff durch das zarte Fleisch/ vnd liebe Seele f\u00e4hrt!", "tokens": ["Scharff", "durch", "das", "zar\u00b7te", "Fleisch", "/", "vnd", "lie\u00b7be", "See\u00b7le", "f\u00e4hrt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "$(", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er ist der Fel\u00df an dem ein jeder auff-mag-stehen.", "tokens": ["Er", "ist", "der", "Fel\u00df", "an", "dem", "ein", "je\u00b7der", "auf\u00b7fmag\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PRELS", "ART", "PIS", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Weh/ weh/ vnd ewig weh dem der jhm wiederspricht!", "tokens": ["Weh", "/", "weh", "/", "vnd", "e\u00b7wig", "weh", "dem", "der", "jhm", "wie\u00b7der\u00b7spricht", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PTKVZ", "$(", "KON", "ADJD", "ADV", "ART", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dis ist der Fel\u00df an dem er Haupt vnd Hertz zu bricht/", "tokens": ["Dis", "ist", "der", "Fel\u00df", "an", "dem", "er", "Haupt", "vnd", "Hertz", "zu", "bricht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "PRELS", "PPER", "NN", "KON", "NN", "PTKZU", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer an den Stein anst\u00f6\u00dft/ mu\u00df sch\u00e4ndlich vntergehen.", "tokens": ["Wer", "an", "den", "Stein", "an\u00b7st\u00f6\u00dft", "/", "mu\u00df", "sch\u00e4nd\u00b7lich", "vn\u00b7ter\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "$(", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}