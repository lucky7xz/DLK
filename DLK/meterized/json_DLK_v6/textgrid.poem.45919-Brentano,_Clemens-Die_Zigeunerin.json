{"textgrid.poem.45919": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Die Zigeunerin", "genre": "verse", "period": "N.A.", "pub_year": 1808, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liebe Frau, da\u00df Gott dich segne,", "tokens": ["Lie\u00b7be", "Frau", ",", "da\u00df", "Gott", "dich", "seg\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KOUS", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und da\u00df dir sein Gl\u00fcck begegne!", "tokens": ["Und", "da\u00df", "dir", "sein", "Gl\u00fcck", "be\u00b7geg\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sei willkommen altes M\u00e4nnlein!", "tokens": ["Sei", "will\u00b7kom\u00b7men", "al\u00b7tes", "M\u00e4nn\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da mit deinem sch\u00f6nen Kindlein!", "tokens": ["Da", "mit", "dei\u00b7nem", "sch\u00f6\u00b7nen", "Kin\u00b7dlein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}