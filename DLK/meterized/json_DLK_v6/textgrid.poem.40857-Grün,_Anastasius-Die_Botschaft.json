{"textgrid.poem.40857": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "Die Botschaft", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu Maxen, der einsam sinnend in stiller Kammer sa\u00df,", "tokens": ["Zu", "Ma\u00b7xen", ",", "der", "ein\u00b7sam", "sin\u00b7nend", "in", "stil\u00b7ler", "Kam\u00b7mer", "sa\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "PRELS", "ADJD", "ADJD", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "An Worte der Weisen dachte, von Thaten der Helden las,", "tokens": ["An", "Wor\u00b7te", "der", "Wei\u00b7sen", "dach\u00b7te", ",", "von", "Tha\u00b7ten", "der", "Hel\u00b7den", "las", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVFIN", "$,", "APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Still gr\u00fc\u00dfend trat ein Bote gar selt'ner Art heran;", "tokens": ["Still", "gr\u00fc\u00b7\u00dfend", "trat", "ein", "Bo\u00b7te", "gar", "selt'\u00b7ner", "Art", "he\u00b7ran", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VVFIN", "ART", "NN", "ADV", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Was sagt der wohl f\u00fcr Kunde dem Kaiserj\u00fcngling an?", "tokens": ["Was", "sagt", "der", "wohl", "f\u00fcr", "Kun\u00b7de", "dem", "Kai\u00b7ser\u00b7j\u00fcng\u00b7ling", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADV", "APPR", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Ob Frohes er, ob Schlimmes darbring', errieth man nicht,", "tokens": ["Ob", "Fro\u00b7hes", "er", ",", "ob", "Schlim\u00b7mes", "dar\u00b7bring'", ",", "er\u00b7rieth", "man", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "PPER", "$,", "KOUS", "NN", "PAV", "$,", "VVFIN", "PIS", "PTKNEG", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Er trug der Trauer Farbe, doch l\u00e4chelt sein Angesicht,", "tokens": ["Er", "trug", "der", "Trau\u00b7er", "Far\u00b7be", ",", "doch", "l\u00e4\u00b7chelt", "sein", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Er glich dem Todesengel, der schwarz und d\u00fcster naht,", "tokens": ["Er", "glich", "dem", "To\u00b7de\u00b7sen\u00b7gel", ",", "der", "schwarz", "und", "d\u00fcs\u00b7ter", "naht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Doch l\u00e4chelnd den Dulder k\u00fcsset und f\u00fchrt zu ros'gem Pfad.", "tokens": ["Doch", "l\u00e4\u00b7chelnd", "den", "Dul\u00b7der", "k\u00fcs\u00b7set", "und", "f\u00fchrt", "zu", "ros'\u00b7gem", "Pfad", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "VVFIN", "KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "\u00bbvon einem M\u00e4dchen bring' ich viellieben, s\u00fc\u00dfen Gru\u00df,", "tokens": ["\u00bb", "von", "ei\u00b7nem", "M\u00e4d\u00b7chen", "bring'", "ich", "viel\u00b7lie\u00b7ben", ",", "s\u00fc\u00b7\u00dfen", "Gru\u00df", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "VVFIN", "PPER", "VVINF", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Von einem todten Freunde den letzten Scheideku\u00df:", "tokens": ["Von", "ei\u00b7nem", "tod\u00b7ten", "Freun\u00b7de", "den", "letz\u00b7ten", "Schei\u00b7de\u00b7ku\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Von Nancy bin ich gesendet, manch Ro\u00df zu Tod' ich ritt,", "tokens": ["Von", "Nan\u00b7cy", "bin", "ich", "ge\u00b7sen\u00b7det", ",", "manch", "Ro\u00df", "zu", "Tod'", "ich", "ritt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "PPER", "VVPP", "$,", "PIAT", "NN", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+---+-+-+", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Die\u00df wei\u00dfe Brieflein bracht' ich, mein theurer Herr, euch mit.\u00ab", "tokens": ["Die\u00df", "wei\u00b7\u00dfe", "Brie\u00b7flein", "bracht'", "ich", ",", "mein", "theu\u00b7rer", "Herr", ",", "euch", "mit", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PDS", "ADJA", "NN", "VVFIN", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "So sprach der Bote zu Maxen. Der lispelt in sich hinein:", "tokens": ["So", "sprach", "der", "Bo\u00b7te", "zu", "Ma\u00b7xen", ".", "Der", "lis\u00b7pelt", "in", "sich", "hin\u00b7ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "$.", "ART", "VVFIN", "APPR", "PRF", "PTKVZ", "$."], "meter": "-+-+--+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "\u00bbo k\u00e4mst du von der Einen, sie denk' ich, lieb' ich allein,", "tokens": ["\u00bb", "o", "k\u00e4mst", "du", "von", "der", "Ei\u00b7nen", ",", "sie", "denk'", "ich", ",", "lieb'", "ich", "al\u00b7lein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "An sie nur glaub' ich sehnend, sah gleich mein Aug' sie nicht,", "tokens": ["An", "sie", "nur", "glaub'", "ich", "seh\u00b7nend", ",", "sah", "gleich", "mein", "Aug'", "sie", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VVFIN", "PPER", "VVPP", "$,", "VVFIN", "ADV", "PPOSAT", "NN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wie Christen, was sie nie schauten, an Himmel, Gott und Gericht.\u00ab", "tokens": ["Wie", "Chris\u00b7ten", ",", "was", "sie", "nie", "schau\u00b7ten", ",", "an", "Him\u00b7mel", ",", "Gott", "und", "Ge\u00b7richt", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,", "APPR", "NN", "$,", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-++--+-+--+", "measure": "iambic.septa.relaxed"}}, "stanza.5": {"line.1": {"text": "Er \u00f6ffnet still das Briefchen, drin lag ein goldner Ring,", "tokens": ["Er", "\u00f6ff\u00b7net", "still", "das", "Brief\u00b7chen", ",", "drin", "lag", "ein", "gold\u00b7ner", "Ring", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Saphir' und Demant inmitten, die blank der Reif umfing,", "tokens": ["Sa\u00b7phir'", "und", "De\u00b7mant", "in\u00b7mit\u00b7ten", ",", "die", "blank", "der", "Reif", "um\u00b7fing", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VVFIN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und eine graue Locke, von Blute roth manch Haar,", "tokens": ["Und", "ei\u00b7ne", "grau\u00b7e", "Lo\u00b7cke", ",", "von", "Blu\u00b7te", "roth", "manch", "Haar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "APPR", "NN", "ADJD", "PIAT", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Viel Arges und viel Liebes im Brief zu lesen war:", "tokens": ["Viel", "Ar\u00b7ges", "und", "viel", "Lie\u00b7bes", "im", "Brief", "zu", "le\u00b7sen", "war", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "APPRART", "NN", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+---+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "\u00bbes weint auf dessen Grabstein mein Aug' in tiefem Leide,", "tokens": ["\u00bb", "es", "weint", "auf", "des\u00b7sen", "Grab\u00b7stein", "mein", "Aug'", "in", "tie\u00b7fem", "Lei\u00b7de", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "PRELAT", "NN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der Freund uns war und Vater, und den wir liebten Beide;", "tokens": ["Der", "Freund", "uns", "war", "und", "Va\u00b7ter", ",", "und", "den", "wir", "lieb\u00b7ten", "Bei\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VAFIN", "KON", "NN", "$,", "KON", "ART", "PPER", "VVFIN", "PIS", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Nimm hin die graue Locke und leg' sie an dein Herz,", "tokens": ["Nimm", "hin", "die", "grau\u00b7e", "Lo\u00b7cke", "und", "leg'", "sie", "an", "dein", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "ADJA", "NN", "KON", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und denke sein, wie deiner er dacht' in Lust und Schmerz.", "tokens": ["Und", "den\u00b7ke", "sein", ",", "wie", "dei\u00b7ner", "er", "dacht'", "in", "Lust", "und", "Schmerz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VAINF", "$,", "PWAV", "PPOSAT", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Dein bin ich durch Wahl des Vaters, durch Wahl des Herzens dein!", "tokens": ["Dein", "bin", "ich", "durch", "Wahl", "des", "Va\u00b7ters", ",", "durch", "Wahl", "des", "Her\u00b7zens", "dein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "PPER", "APPR", "NN", "ART", "NN", "$,", "APPR", "NN", "ART", "NN", "PPOSAT", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Nimm hin die\u00df goldne Reiflein, gar werthlos zwar und klein,", "tokens": ["Nimm", "hin", "die\u00df", "gold\u00b7ne", "Rei\u00b7flein", ",", "gar", "wert\u00b7hlos", "zwar", "und", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PDS", "ADJA", "NN", "$,", "ADV", "ADJD", "ADV", "KON", "ADJD", "$,"], "meter": "-+-+-+--+---+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Doch will ich dich ", "tokens": ["Doch", "will", "ich", "dich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "PRF"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Am Glanz des goldnen ", "tokens": ["Am", "Glanz", "des", "gold\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Und Max k\u00fc\u00dft Ring und Locke, er f\u00fchlt sein Herz so wund:", "tokens": ["Und", "Max", "k\u00fc\u00dft", "Ring", "und", "Lo\u00b7cke", ",", "er", "f\u00fchlt", "sein", "Herz", "so", "wund", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "NN", "KON", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "\u00bbo Karl und o Maria!\u00ab so lispelt still sein Mund,", "tokens": ["\u00bb", "o", "Karl", "und", "o", "Ma\u00b7ria", "!", "\u00ab", "so", "lis\u00b7pelt", "still", "sein", "Mund", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NE", "KON", "FM", "NE", "$.", "$(", "ADV", "VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u00bbo Stern der Freundschaft, wie endet so blutigroth dein Lauf!", "tokens": ["\u00bb", "o", "Stern", "der", "Freund\u00b7schaft", ",", "wie", "en\u00b7det", "so", "blu\u00b7ti\u00b7groth", "dein", "Lauf", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "ART", "NN", "$,", "PWAV", "VVFIN", "ADV", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "O goldner Stern der Liebe, wie steigst du so herrlich auf!\u00ab", "tokens": ["O", "gold\u00b7ner", "Stern", "der", "Lie\u00b7be", ",", "wie", "steigst", "du", "so", "herr\u00b7lich", "auf", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "ADJA", "NN", "ART", "NN", "$,", "PWAV", "VVFIN", "PPER", "ADV", "ADJD", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+++-+-+", "measure": "unknown.measure.octa.plus"}}, "stanza.9": {"line.1": {"text": "Und eine gro\u00dfe Thr\u00e4ne, die seinem Aug' entschlich,", "tokens": ["Und", "ei\u00b7ne", "gro\u00b7\u00dfe", "Thr\u00e4\u00b7ne", ",", "die", "sei\u00b7nem", "Aug'", "ent\u00b7schlich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Senkt auf die graue Locke und auf das Ringlein sich;", "tokens": ["Senkt", "auf", "die", "grau\u00b7e", "Lo\u00b7cke", "und", "auf", "das", "Rin\u00b7glein", "sich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "KON", "APPR", "ART", "NN", "PRF", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Doch was die Thr\u00e4ne geboren, ob Freude oder Schmerz?", "tokens": ["Doch", "was", "die", "Thr\u00e4\u00b7ne", "ge\u00b7bo\u00b7ren", ",", "ob", "Freu\u00b7de", "o\u00b7der", "Schmerz", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVPP", "$,", "KOUS", "NN", "KON", "NN", "$."], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Nicht darf der Dichter richten, nur ahnen mag's das Herz.", "tokens": ["Nicht", "darf", "der", "Dich\u00b7ter", "rich\u00b7ten", ",", "nur", "ah\u00b7nen", "mag's", "das", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VMFIN", "ART", "NN", "VVINF", "$,", "ADV", "VVINF", "VMFIN", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}