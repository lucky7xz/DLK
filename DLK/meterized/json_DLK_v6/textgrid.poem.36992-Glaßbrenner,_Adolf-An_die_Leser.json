{"textgrid.poem.36992": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "An die Leser", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn Ihr, Du mein hochverehrter", "tokens": ["Wenn", "Ihr", ",", "Du", "mein", "hoch\u00b7ver\u00b7ehr\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "PPER", "PPOSAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Leser, und Du (leider nicht mein)", "tokens": ["Le\u00b7ser", ",", "und", "Du", "(", "lei\u00b7der", "nicht", "mein", ")"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "PPER", "$(", "ADV", "PTKNEG", "PPOSAT", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Holde, reizende und kluge", "tokens": ["Hol\u00b7de", ",", "rei\u00b7zen\u00b7de", "und", "klu\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADJA", "KON", "ADJA"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Leserin, in diesem Buche", "tokens": ["Le\u00b7se\u00b7rin", ",", "in", "die\u00b7sem", "Bu\u00b7che"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Selbst zuweilen solchen baaren", "tokens": ["Selbst", "zu\u00b7wei\u00b7len", "sol\u00b7chen", "baa\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Unsinn findet, der nicht Sinn hat,", "tokens": ["Un\u00b7sinn", "fin\u00b7det", ",", "der", "nicht", "Sinn", "hat", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PRELS", "PTKNEG", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und bei welchem der Verstand uns,", "tokens": ["Und", "bei", "wel\u00b7chem", "der", "Ver\u00b7stand", "uns", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PRELS", "ART", "NN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie der Deutsche pflegt zu sagen,", "tokens": ["Wie", "der", "Deut\u00b7sche", "pflegt", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Stillsteht: so erw\u00e4gt zuv\u00f6rderst,", "tokens": ["Still\u00b7steht", ":", "so", "er\u00b7w\u00e4gt", "zu\u00b7v\u00f6r\u00b7derst", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "ADV", "VVFIN", "ADV", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Eh' Ihr mein Gedicht in's Feuer", "tokens": ["Eh'", "Ihr", "mein", "Ge\u00b7dicht", "in's", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Werft und mir, was noch viel h\u00e4rter,", "tokens": ["Werft", "und", "mir", ",", "was", "noch", "viel", "h\u00e4r\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PPER", "$,", "PRELS", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Eure Gunst entzieht, erw\u00e4gt erst", "tokens": ["Eu\u00b7re", "Gunst", "ent\u00b7zieht", ",", "er\u00b7w\u00e4gt", "erst"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "ADV"], "meter": "+-+-+---", "measure": "unknown.measure.tri"}, "line.13": {"text": "Meine drei Rechtfert'gungsgr\u00fcnde,", "tokens": ["Mei\u00b7ne", "drei", "Recht\u00b7fert'\u00b7gungs\u00b7gr\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Die ich Euch hier unten k\u00fcnde.", "tokens": ["Die", "ich", "Euch", "hier", "un\u00b7ten", "k\u00fcn\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Es ist n\u00e4mlich sehr wohl m\u00f6glich", "tokens": ["Es", "ist", "n\u00e4m\u00b7lich", "sehr", "wohl", "m\u00f6g\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df, selbst im verkehrten Sinne", "tokens": ["Da\u00df", ",", "selbst", "im", "ver\u00b7kehr\u00b7ten", "Sin\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ungeeignet, hier zum Beispiel:", "tokens": ["Un\u00b7ge\u00b7eig\u00b7net", ",", "hier", "zum", "Bei\u00b7spiel", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcr ", "tokens": ["F\u00fcr"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Statt ", "tokens": ["Statt"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Statt ", "tokens": ["Statt"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Und dann umgekehrt dies wieder", "tokens": ["Und", "dann", "um\u00b7ge\u00b7kehrt", "dies", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "PDS", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Alles. Wie gesagt, sehr m\u00f6glich!", "tokens": ["Al\u00b7les", ".", "Wie", "ge\u00b7sagt", ",", "sehr", "m\u00f6g\u00b7lich", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "$.", "PWAV", "VVPP", "$,", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Aber erstens soll sich jeder", "tokens": ["A\u00b7ber", "ers\u00b7tens", "soll", "sich", "je\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PRF", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gutgesinnte \u2013 und wahrhaftig,", "tokens": ["Gut\u00b7ge\u00b7sinn\u00b7te", "\u2013", "und", "wahr\u00b7haf\u00b7tig", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$(", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ja, das bin ich! \u2013 gegenw\u00e4rtig", "tokens": ["Ja", ",", "das", "bin", "ich", "!", "\u2013", "ge\u00b7gen\u00b7w\u00e4r\u00b7tig"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct", "word"], "pos": ["PTKANT", "$,", "PDS", "VAFIN", "PPER", "$.", "$(", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf das Eifrigste bem\u00fchn, da\u00df", "tokens": ["Auf", "das", "Eif\u00b7rigs\u00b7te", "be\u00b7m\u00fchn", ",", "da\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "VVINF", "$,", "KOUS"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.5": {"text": "Einem der Verstand, und nicht nur", "tokens": ["Ei\u00b7nem", "der", "Ver\u00b7stand", ",", "und", "nicht", "nur"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "ART", "NN", "$,", "KON", "PTKNEG", "ADV"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Und mit ihm das Wissen, stillsteht", "tokens": ["Und", "mit", "ihm", "das", "Wis\u00b7sen", ",", "still\u00b7steht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "APPR", "PPER", "ART", "NN", "$,", "VVFIN"], "meter": "----+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Oder, besser noch, zur\u00fcckgeht.", "tokens": ["O\u00b7der", ",", "bes\u00b7ser", "noch", ",", "zu\u00b7r\u00fcck\u00b7geht", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "ADJD", "ADV", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Zweitens gab ein Linguist hier \u2013", "tokens": ["Zwei\u00b7tens", "gab", "ein", "Lin\u00b7gu\u00b7ist", "hier", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Den als Freund ich grade deshalb", "tokens": ["Den", "als", "Freund", "ich", "gra\u00b7de", "des\u00b7halb"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "KOUS", "NN", "PPER", "ADV", "PAV"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Sch\u00e4tzen lernte, weil er nie mich", "tokens": ["Sch\u00e4t\u00b7zen", "lern\u00b7te", ",", "weil", "er", "nie", "mich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "KOUS", "PPER", "ADV", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu taxiren ihn veranla\u00dft,", "tokens": ["Zu", "ta\u00b7xi\u00b7ren", "ihn", "ver\u00b7an\u00b7la\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und mir deshalb just war theuer,", "tokens": ["Und", "mir", "des\u00b7halb", "just", "war", "theu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PAV", "ADV", "VAFIN", "ADJD", "$,"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Weil er sich bewies so billig \u2013", "tokens": ["Weil", "er", "sich", "be\u00b7wies", "so", "bil\u00b7lig", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "ADV", "ADJD", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Mir den Rath: beim Uebersetzen", "tokens": ["Mir", "den", "Rath", ":", "beim", "Ue\u00b7ber\u00b7set\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "ART", "NN", "$.", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Aus der Sprache dieses Sternes", "tokens": ["Aus", "der", "Spra\u00b7che", "die\u00b7ses", "Ster\u00b7nes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "In das Deutsche je mitunter \u2013", "tokens": ["In", "das", "Deut\u00b7sche", "je", "mi\u00b7tun\u00b7ter", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ADV", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Um, worauf es doch alleine", "tokens": ["Um", ",", "wo\u00b7rauf", "es", "doch", "al\u00b7lei\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["KOUI", "$,", "PWAV", "PPER", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Ankommt, meinem deutschen Volke", "tokens": ["An\u00b7kommt", ",", "mei\u00b7nem", "deut\u00b7schen", "Vol\u00b7ke"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Das Verst\u00e4ndni\u00df zu erleichtern \u2013", "tokens": ["Das", "Ver\u00b7st\u00e4nd\u00b7ni\u00df", "zu", "er\u00b7leich\u00b7tern", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Auch ", "tokens": ["Auch"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.14": {"text": "Was nach unserm Erdbegriffe", "tokens": ["Was", "nach", "un\u00b7serm", "Erd\u00b7be\u00b7grif\u00b7fe"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Wirklich dumm ist, nach dem hies'gen", "tokens": ["Wirk\u00b7lich", "dumm", "ist", ",", "nach", "dem", "hies'\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "ADJD", "VAFIN", "$,", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Aber klug und weise und just", "tokens": ["A\u00b7ber", "klug", "und", "wei\u00b7se", "und", "just"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "VVFIN", "KON", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.17": {"text": "De\u00dfhalb wird durch \u00bbdumm\u00ab gepriesen:", "tokens": ["De\u00df\u00b7halb", "wird", "durch", "\u00bb", "dumm", "\u00ab", "ge\u00b7prie\u00b7sen", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PAV", "VAFIN", "APPR", "$(", "ADJD", "$(", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Auch im Deutschen so zu geben;", "tokens": ["Auch", "im", "Deut\u00b7schen", "so", "zu", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Im gewohnten Styl zu bleiben,", "tokens": ["Im", "ge\u00b7wohn\u00b7ten", "Styl", "zu", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Und zuweilen ", "tokens": ["Und", "zu\u00b7wei\u00b7len"], "token_info": ["word", "word"], "pos": ["KON", "ADV"], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Drittens endlich wird der Leser", "tokens": ["Drit\u00b7tens", "end\u00b7lich", "wird", "der", "Le\u00b7ser"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die sch\u00f6ne Leserin wohl", "tokens": ["Und", "die", "sch\u00f6\u00b7ne", "Le\u00b7se\u00b7rin", "wohl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ADV"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Nicht Unm\u00f6gliches verlangen,", "tokens": ["Nicht", "Un\u00b7m\u00f6g\u00b7li\u00b7ches", "ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht verlangen, da\u00df ich, Mitglied", "tokens": ["Nicht", "ver\u00b7lan\u00b7gen", ",", "da\u00df", "ich", ",", "Mit\u00b7glied"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PTKNEG", "VVINF", "$,", "KOUS", "PPER", "$,", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Zweier Welten, sel'ges einer", "tokens": ["Zwei\u00b7er", "Wel\u00b7ten", ",", "sel'\u00b7ges", "ei\u00b7ner"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["CARD", "NN", "$,", "ADJA", "ART"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ganz vern\u00fcnft'gen und gelehrten,", "tokens": ["Ganz", "ver\u00b7n\u00fcnft'\u00b7gen", "und", "ge\u00b7lehr\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "KON", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und lebend'ges einer tollen", "tokens": ["Und", "le\u00b7ben\u00b7d'\u00b7ges", "ei\u00b7ner", "tol\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Unvern\u00fcnft'gen und verkehrten,", "tokens": ["Un\u00b7ver\u00b7n\u00fcnft'\u00b7gen", "und", "ver\u00b7kehr\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Alle Tage unter Wesen,", "tokens": ["Al\u00b7le", "Ta\u00b7ge", "un\u00b7ter", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Deren keins vollkommen Mensch,", "tokens": ["De\u00b7ren", "keins", "voll\u00b7kom\u00b7men", "Mensch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Jedes Mensch und Thier zugleich ist;", "tokens": ["Je\u00b7des", "Mensch", "und", "Thier", "zu\u00b7gleich", "ist", ";"], "token_info": ["word", "word", "word", "word", "word",