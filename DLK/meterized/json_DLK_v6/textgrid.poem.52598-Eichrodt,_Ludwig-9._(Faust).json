{"textgrid.poem.52598": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "9. (Faust)", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du glaubest nicht an meine Liebe,", "tokens": ["Du", "glau\u00b7best", "nicht", "an", "mei\u00b7ne", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und kannst mich weinend gehen sehn!", "tokens": ["Und", "kannst", "mich", "wei\u00b7nend", "ge\u00b7hen", "sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "VVPP", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wenn ichs mit dem Herzblut schriebe,", "tokens": ["Und", "wenn", "ichs", "mit", "dem", "Herz\u00b7blut", "schrie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du willst mich nimmer mehr verstehn.", "tokens": ["Du", "willst", "mich", "nim\u00b7mer", "mehr", "ver\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So lasse dir noch Eines sagen \u2013", "tokens": ["So", "las\u00b7se", "dir", "noch", "Ei\u00b7nes", "sa\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So schmieg an meine Brust dein Haupt,", "tokens": ["So", "schmieg", "an", "mei\u00b7ne", "Brust", "dein", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "H\u00f6r dir mein Herz entgegenschlagen,", "tokens": ["H\u00f6r", "dir", "mein", "Herz", "ent\u00b7ge\u00b7gen\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und f\u00fchle, was du nicht geglaubt.", "tokens": ["Und", "f\u00fch\u00b7le", ",", "was", "du", "nicht", "ge\u00b7glaubt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWS", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dann lasse mich die Lippen dr\u00fccken,", "tokens": ["Dann", "las\u00b7se", "mich", "die", "Lip\u00b7pen", "dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die d\u00fcrstenden, auf deinen Mund,", "tokens": ["Die", "d\u00fcrs\u00b7ten\u00b7den", ",", "auf", "dei\u00b7nen", "Mund", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und seine S\u00fc\u00dfigkeiten pfl\u00fccken,", "tokens": ["Und", "sei\u00b7ne", "S\u00fc\u00b7\u00dfig\u00b7kei\u00b7ten", "pfl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So wird dir meine Liebe kund!", "tokens": ["So", "wird", "dir", "mei\u00b7ne", "Lie\u00b7be", "kund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "La\u00df dich umschlingen, la\u00df umfangen", "tokens": ["La\u00df", "dich", "um\u00b7schlin\u00b7gen", ",", "la\u00df", "um\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "PPER", "VVINF", "$,", "VVIMP", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dich, die nicht h\u00e4\u00dflich wie der Zwang,", "tokens": ["Dich", ",", "die", "nicht", "h\u00e4\u00df\u00b7lich", "wie", "der", "Zwang", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PTKNEG", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und b\u00fc\u00dfen mein gerecht Verlangen", "tokens": ["Und", "b\u00fc\u00b7\u00dfen", "mein", "ge\u00b7recht", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In deinen Armen hei\u00df und lang!", "tokens": ["In", "dei\u00b7nen", "Ar\u00b7men", "hei\u00df", "und", "lang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Dich lieb ich \u2013 und der Erdeng\u00f6tter", "tokens": ["Dich", "lieb", "ich", "\u2013", "und", "der", "Er\u00b7den\u00b7g\u00f6t\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$(", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sei \u00fcber uns nicht Einer Herr!", "tokens": ["Sei", "\u00fc\u00b7ber", "uns", "nicht", "Ei\u00b7ner", "Herr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ach, um Zeloten oder Sp\u00f6tter", "tokens": ["Ach", ",", "um", "Ze\u00b7lo\u00b7ten", "o\u00b7der", "Sp\u00f6t\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KOUI", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bek\u00fcmmert sich kein Gl\u00fccklicher.", "tokens": ["Be\u00b7k\u00fcm\u00b7mert", "sich", "kein", "Gl\u00fcck\u00b7li\u00b7cher", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PIAT", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.6": {"line.1": {"text": "Du mu\u00dft den Zweifel \u00fcberwinden,", "tokens": ["Du", "mu\u00dft", "den", "Zwei\u00b7fel", "\u00fc\u00b7berw\u00b7in\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vertrauen ist Entschlu\u00df und That,", "tokens": ["Ver\u00b7trau\u00b7en", "ist", "Ent\u00b7schlu\u00df", "und", "That", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wirst du kein ander Herz doch finden,", "tokens": ["Wirst", "du", "kein", "an\u00b7der", "Herz", "doch", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "ADJD", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der solche Kraft des Liebens hat!", "tokens": ["Der", "sol\u00b7che", "Kraft", "des", "Lie\u00b7bens", "hat", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}