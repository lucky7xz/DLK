{"dta.poem.4194": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Bluhmen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Liebste Fr\u00fchlings-Kinder, ihr,", "tokens": ["Liebs\u00b7te", "Fr\u00fch\u00b7lings\u00b7Kin\u00b7der", ",", "ihr", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Eurer Form- und Farben Zier,", "tokens": ["Eu\u00b7rer", "For\u00b7m", "und", "Far\u00b7ben", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "TRUNC", "KON", "NN", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "R\u00fchrt mein Aug\u2019 und mein Gem\u00fchte.", "tokens": ["R\u00fchrt", "mein", "Aug'", "und", "mein", "Ge\u00b7m\u00fch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich bewundr\u2019 in eurer Pracht", "tokens": ["Ich", "be\u00b7wundr'", "in", "eu\u00b7rer", "Pracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dessen Weisheit, Macht und G\u00fcte,", "tokens": ["Des\u00b7sen", "Weis\u00b7heit", ",", "Macht", "und", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Welcher euch hervorgebracht.", "tokens": ["Wel\u00b7cher", "euch", "her\u00b7vor\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAT", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Billig sollt\u2019 euch niemand sehen,", "tokens": ["Bil\u00b7lig", "sollt'", "euch", "nie\u00b7mand", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ohne Gottes Huld und Macht", "tokens": ["Oh\u00b7ne", "Got\u00b7tes", "Huld", "und", "Macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Froh und dankbar zu erh\u00f6hen.", "tokens": ["Froh", "und", "dank\u00b7bar", "zu", "er\u00b7h\u00f6\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Denn was kann vom bunten Schein,", "tokens": ["Denn", "was", "kann", "vom", "bun\u00b7ten", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VMFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Welcher euer Wesen zieret,", "tokens": ["Wel\u00b7cher", "eu\u00b7er", "We\u00b7sen", "zie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Doch der Zweck gewesen seyn", "tokens": ["Doch", "der", "Zweck", "ge\u00b7we\u00b7sen", "seyn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAPP", "VAINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "De\u00df, Der euch so sch\u00f6n formieret,", "tokens": ["De\u00df", ",", "Der", "euch", "so", "sch\u00f6n", "for\u00b7mie\u00b7ret", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ART", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Als da\u00df man an ihren Sch\u00e4tzen,", "tokens": ["Als", "da\u00df", "man", "an", "ih\u00b7ren", "Sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PIS", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Jhm zum Ruhm, sich soll ergetzen?", "tokens": ["Jhm", "zum", "Ruhm", ",", "sich", "soll", "er\u00b7get\u00b7zen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "$,", "PRF", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Diese Absicht zu erlangen,", "tokens": ["Die\u00b7se", "Ab\u00b7sicht", "zu", "er\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Haben wir die Sch\u00f6nheit nicht", "tokens": ["Ha\u00b7ben", "wir", "die", "Sch\u00f6n\u00b7heit", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Nur allein, wir haben\u2019s Licht,", "tokens": ["Nur", "al\u00b7lein", ",", "wir", "ha\u00b7ben's", "Licht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PPER", "VAFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Auch das herrliche Gesicht", "tokens": ["Auch", "das", "herr\u00b7li\u00b7che", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "(welche Wunder-Ding\u2019!) empfangen.", "tokens": ["(", "wel\u00b7che", "Wun\u00b7der\u00b7Ding'", "!", ")", "emp\u00b7fan\u00b7gen", "."], "token_info": ["punct", "word", "word", "punct", "punct", "word", "punct"], "pos": ["$(", "PWAT", "NN", "$.", "$(", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Was hat nicht dazu geh\u00f6rt,", "tokens": ["Was", "hat", "nicht", "da\u00b7zu", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PTKNEG", "PAV", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.22": {"text": "Da\u00df sie, und uns sichtbar, w\u00e4ren?", "tokens": ["Da\u00df", "sie", ",", "und", "uns", "sicht\u00b7bar", ",", "w\u00e4\u00b7ren", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KON", "PPER", "ADJD", "$,", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Wunder, die nicht zu erkl\u00e4ren!", "tokens": ["Wun\u00b7der", ",", "die", "nicht", "zu", "er\u00b7kl\u00e4\u00b7ren", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ist denn alles die\u00df nicht wehrt,", "tokens": ["Ist", "denn", "al\u00b7les", "die\u00df", "nicht", "wehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIS", "PDS", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df man Den, Der sie beschehrt,", "tokens": ["Da\u00df", "man", "Den", ",", "Der", "sie", "be\u00b7schehrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "$,", "ART", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn man sie betrachtet, ehrt?", "tokens": ["Wenn", "man", "sie", "be\u00b7trach\u00b7tet", ",", "ehrt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVPP", "$,", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da, zumahl wenn wir sie sehen,", "tokens": ["Da", ",", "zu\u00b7mahl", "wenn", "wir", "sie", "se\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "KOUS", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und in ihnen Jhn erh\u00f6hen,", "tokens": ["Und", "in", "ih\u00b7nen", "Jhn", "er\u00b7h\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dieses Sehn von jedermann", "tokens": ["Die\u00b7ses", "Sehn", "von", "je\u00b7der\u00b7mann"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "NN", "APPR", "PIS"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sonder Anmuht nicht geschehen,", "tokens": ["Son\u00b7der", "An\u00b7muht", "nicht", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Noch betrachtet werden kann.", "tokens": ["Noch", "be\u00b7trach\u00b7tet", "wer\u00b7den", "kann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wei\u00df man aber etwan nicht,", "tokens": ["Wei\u00df", "man", "a\u00b7ber", "et\u00b7wan", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ADV", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn wir was betrachten wollen,", "tokens": ["Wenn", "wir", "was", "be\u00b7trach\u00b7ten", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VVINF", "VMINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie es eigentlich geschicht,", "tokens": ["Wie", "es", "ei\u00b7gent\u00b7lich", "ge\u00b7schicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und wie wir uns freuen sollen;", "tokens": ["Und", "wie", "wir", "uns", "freu\u00b7en", "sol\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PRF", "VVINF", "VMFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "So vernehmt den Unterricht:", "tokens": ["So", "ver\u00b7nehmt", "den", "Un\u00b7ter\u00b7richt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Machet ihr den Anfang nur;", "tokens": ["Ma\u00b7chet", "ihr", "den", "An\u00b7fang", "nur", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Seht die Werke der Natur,", "tokens": ["Seht", "die", "Wer\u00b7ke", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+---+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Mehr als sonst, bedachtsam an;", "tokens": ["Mehr", "als", "sonst", ",", "be\u00b7dacht\u00b7sam", "an", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "ADV", "$,", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn bem\u00fcht euch, euer Denken", "tokens": ["Denn", "be\u00b7m\u00fcht", "euch", ",", "eu\u00b7er", "Den\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Auf den Sch\u00f6pfer hinzulenken,", "tokens": ["Auf", "den", "Sch\u00f6p\u00b7fer", "hin\u00b7zu\u00b7len\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der, um euch mit solchen Sch\u00e4tzen", "tokens": ["Der", ",", "um", "euch", "mit", "sol\u00b7chen", "Sch\u00e4t\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "$,", "KOUI", "PPER", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Zu vergn\u00fcgen, zu ergetzen,", "tokens": ["Zu", "ver\u00b7gn\u00fc\u00b7gen", ",", "zu", "er\u00b7get\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sie so lieblich bilden kann.", "tokens": ["Sie", "so", "lieb\u00b7lich", "bil\u00b7den", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Werdet ihr auf diese Weise", "tokens": ["Wer\u00b7det", "ihr", "auf", "die\u00b7se", "Wei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Jhm, zu Seinem Ruhm und Preise,", "tokens": ["Jhm", ",", "zu", "Sei\u00b7nem", "Ruhm", "und", "Prei\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Blo\u00df nur die Bewundrung schenken;", "tokens": ["Blo\u00df", "nur", "die", "Be\u00b7wund\u00b7rung", "schen\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "So wird, durch Sein Werk ger\u00fchrt,", "tokens": ["So", "wird", ",", "durch", "Sein", "Werk", "ge\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "Euer Geist, zu Jhm gef\u00fchrt,", "tokens": ["Eu\u00b7er", "Geist", ",", "zu", "Jhm", "ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Sich im Sch\u00f6pfer selbst versenken.", "tokens": ["Sich", "im", "Sch\u00f6p\u00b7fer", "selbst", "ver\u00b7sen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}