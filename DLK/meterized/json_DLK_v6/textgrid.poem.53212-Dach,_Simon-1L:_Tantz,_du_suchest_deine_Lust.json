{"textgrid.poem.53212": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Tantz, du suchest deine Lust", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tantz, du suchest deine Lust", "tokens": ["Tantz", ",", "du", "su\u00b7chest", "dei\u00b7ne", "Lust"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In der Jugend frischen Brust,", "tokens": ["In", "der", "Ju\u00b7gend", "fri\u00b7schen", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In den leichten F\u00fcssen,", "tokens": ["In", "den", "leich\u00b7ten", "F\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "In den Seiten, in dem Wein", "tokens": ["In", "den", "Sei\u00b7ten", ",", "in", "dem", "Wein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Vnd was mehr f\u00fcr Sachen seyn,", "tokens": ["Vnd", "was", "mehr", "f\u00fcr", "Sa\u00b7chen", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "APPR", "NN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "So dir dienen m\u00fcssen.", "tokens": ["So", "dir", "die\u00b7nen", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVINF", "VMINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Da\u00df du dein Gewerb und Werck", "tokens": ["Da\u00df", "du", "dein", "Ge\u00b7werb", "und", "Werck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bi\u00df anher zu K\u00f6nigsberg", "tokens": ["Bi\u00df", "an\u00b7her", "zu", "K\u00f6\u00b7nigs\u00b7berg"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "APPR", "NE"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dennoch hast getrieben,", "tokens": ["Den\u00b7noch", "hast", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Das war furchtsam und verzagt.", "tokens": ["Das", "war", "furcht\u00b7sam", "und", "ver\u00b7zagt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "KON", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mars hat dir auch nachgejagt,", "tokens": ["Mars", "hat", "dir", "auch", "nach\u00b7ge\u00b7jagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Du bist schier geblieben.", "tokens": ["Du", "bist", "schier", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Nu man sagt, da\u00df Fried und Rhu", "tokens": ["Nu", "man", "sagt", ",", "da\u00df", "Fried", "und", "Rhu"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "VVFIN", "$,", "KOUS", "NN", "KON", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Vns umbgeben, hast auch du", "tokens": ["Vns", "umb\u00b7ge\u00b7ben", ",", "hast", "auch", "du"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVPP", "$,", "VAFIN", "ADV", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wieder Lufft bekommen,", "tokens": ["Wie\u00b7der", "Lufft", "be\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Darumb schwingst du dich empor", "tokens": ["Da\u00b7rumb", "schwingst", "du", "dich", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "PTKVZ"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Vnd wirst besser als zuvor", "tokens": ["Vnd", "wirst", "bes\u00b7ser", "als", "zu\u00b7vor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADJD", "KOKOM", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Hier auch angenommen.", "tokens": ["Hier", "auch", "an\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Greiff dich jetzt, du Frewden Mann,", "tokens": ["Greiff", "dich", "jetzt", ",", "du", "Frew\u00b7den", "Mann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Diesem Paar zu Ehren an,", "tokens": ["Die\u00b7sem", "Paar", "zu", "Eh\u00b7ren", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das sich heut vertrawet:", "tokens": ["Das", "sich", "heut", "ver\u00b7tra\u00b7wet", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "ADV", "VVFIN", "$."], "meter": "--+-+-", "measure": "anapaest.init"}, "line.4": {"text": "W\u00fcrtz das s\u00fcsse Hochzeitmal,", "tokens": ["W\u00fcrtz", "das", "s\u00fcs\u00b7se", "Hoch\u00b7zeit\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Werd umbher im gantzen Saal", "tokens": ["Werd", "um\u00b7bher", "im", "gant\u00b7zen", "Saal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nimmer still geschawet.", "tokens": ["Nim\u00b7mer", "still", "ge\u00b7scha\u00b7wet", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Gib den Sitten Geist und Brand,", "tokens": ["Gib", "den", "Sit\u00b7ten", "Geist", "und", "Brand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Fa\u00df Cupido bey der Hand,", "tokens": ["Fa\u00df", "Cu\u00b7pi\u00b7do", "bey", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fliegt umbher zusammen,", "tokens": ["Fliegt", "um\u00b7bher", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Nichts befinde sich so alt,", "tokens": ["Nichts", "be\u00b7fin\u00b7de", "sich", "so", "alt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df sich nicht lieb' alsobald,", "tokens": ["Da\u00df", "sich", "nicht", "lieb'", "al\u00b7so\u00b7bald", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PTKNEG", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "F\u00fchlt es ewre Flammen.", "tokens": ["F\u00fchlt", "es", "ew\u00b7re", "Flam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Wenn du alles hast gethan,", "tokens": ["Wenn", "du", "al\u00b7les", "hast", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dann sieh Braut und Br\u00e4utgam an,", "tokens": ["Dann", "sieh", "Braut", "und", "Br\u00e4ut\u00b7gam", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "NN", "KON", "NE", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihre sch\u00f6ne Jugend,", "tokens": ["Ih\u00b7re", "sch\u00f6\u00b7ne", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ihrer keuschen Vnschuld Kleid,", "tokens": ["Ih\u00b7rer", "keu\u00b7schen", "Vn\u00b7schuld", "Kleid", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alle dein Verdienst steht weit", "tokens": ["Al\u00b7le", "dein", "Ver\u00b7dienst", "steht", "weit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN", "VVFIN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Vnter ihrer Tugend.", "tokens": ["Vn\u00b7ter", "ih\u00b7rer", "Tu\u00b7gend", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}