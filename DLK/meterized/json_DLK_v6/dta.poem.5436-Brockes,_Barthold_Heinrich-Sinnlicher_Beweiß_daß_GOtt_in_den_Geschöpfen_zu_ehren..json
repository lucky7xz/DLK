{"dta.poem.5436": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Sinnlicher Bewei\u00df da\u00df GOtt in den  \n Gesch\u00f6pfen zu ehren.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da wir in der Religion und unsrer heil'gen Bibel Lehren", "tokens": ["Da", "wir", "in", "der", "Re\u00b7li\u00b7gi\u00b7on", "und", "uns\u00b7rer", "heil'\u00b7gen", "Bi\u00b7bel", "Leh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "KON", "PPOSAT", "ADJA", "NN", "NN"], "meter": "-+--+--+-+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Schon k\u00f6nnten unterwiesen werden, durch einen", "tokens": ["Schon", "k\u00f6nn\u00b7ten", "un\u00b7ter\u00b7wie\u00b7sen", "wer\u00b7den", ",", "durch", "ei\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VMFIN", "VVPP", "VAINF", "$,", "APPR", "ART"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sinn allein: durchs H\u00f6ren;", "tokens": ["Sinn", "al\u00b7lein", ":", "durchs", "H\u00f6\u00b7ren", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$.", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "So sage mir, zu welcher Absicht der Sch\u00f6pfer doch in diesem", "tokens": ["So", "sa\u00b7ge", "mir", ",", "zu", "wel\u00b7cher", "Ab\u00b7sicht", "der", "Sch\u00f6p\u00b7fer", "doch", "in", "die\u00b7sem"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "PWAT", "NN", "ART", "NN", "ADV", "APPR", "PDAT"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.5": {"text": "Leben", "tokens": ["Le\u00b7ben"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Der andern Sinnen Wunder-Gaben und tausend Vor-", "tokens": ["Der", "an\u00b7dern", "Sin\u00b7nen", "Wun\u00b7der\u00b7Ga\u00b7ben", "und", "tau\u00b7send", "Vor"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "KON", "CARD", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "w\u00fcrff' uns gegeben,", "tokens": ["w\u00fcrff'", "uns", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Als seine Weisheit, Macht und Liebe, in seinen wun-", "tokens": ["Als", "sei\u00b7ne", "Weis\u00b7heit", ",", "Macht", "und", "Lie\u00b7be", ",", "in", "sei\u00b7nen", "wun"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "NN", "KON", "NN", "$,", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "derbaren Wercken,", "tokens": ["der\u00b7ba\u00b7ren", "Wer\u00b7cken", ","], "token_info": ["word", "word", "punct"], "pos": ["PRELAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Mit Lust und mit Bewunderung, in heil\u2019ger Andacht zu", "tokens": ["Mit", "Lust", "und", "mit", "Be\u00b7wun\u00b7de\u00b7rung", ",", "in", "heil'\u00b7ger", "An\u00b7dacht", "zu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,", "APPR", "ADJA", "NN", "PTKZU"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.11": {"text": "bemercken.", "tokens": ["be\u00b7mer\u00b7cken", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.12": {"text": "Sein herrlich Regiment in allem zu sp\u00fchren, schmecken,", "tokens": ["Sein", "herr\u00b7lich", "Re\u00b7gi\u00b7ment", "in", "al\u00b7lem", "zu", "sp\u00fch\u00b7ren", ",", "schme\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "APPR", "PIS", "PTKZU", "VVINF", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "und zu sehn,", "tokens": ["und", "zu", "sehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PTKZU", "VVINF", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.14": {"text": "Und dergestalt mit Leib und Seele sein G\u00f6ttlich Wesen zu", "tokens": ["Und", "der\u00b7ge\u00b7stalt", "mit", "Leib", "und", "See\u00b7le", "sein", "G\u00f6tt\u00b7lich", "We\u00b7sen", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "NN", "KON", "NN", "PPOSAT", "NN", "NN", "PTKZU"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.15": {"text": "erh\u00f6hn?", "tokens": ["er\u00b7h\u00f6hn", "?"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-+", "measure": "iambic.single"}}}}}