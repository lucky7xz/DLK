{"textgrid.poem.28211": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "26.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir haben ein freyes und grosses Gem\u00fcte,", "tokens": ["Wir", "ha\u00b7ben", "ein", "frey\u00b7es", "und", "gros\u00b7ses", "Ge\u00b7m\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "KON", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ein edle, rechtschaffene, l\u00f6bliche G\u00fcte.", "tokens": ["Ein", "ed\u00b7le", ",", "recht\u00b7schaf\u00b7fe\u00b7ne", ",", "l\u00f6b\u00b7li\u00b7che", "G\u00fc\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ein Krieger, hat dieser was, das er verrichte,", "tokens": ["Ein", "Krie\u00b7ger", ",", "hat", "die\u00b7ser", "was", ",", "das", "er", "ver\u00b7rich\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VAFIN", "PDS", "PIS", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Der geht wol nicht traurig von unsrem Gesichte.", "tokens": ["Der", "geht", "wol", "nicht", "trau\u00b7rig", "von", "uns\u00b7rem", "Ge\u00b7sich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PTKNEG", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}}}}