"word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVFIN", "$,", "PWAV", "ART", "NN", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gleit' ich \u00fcber den Strom, der sanfter aufschwillt;", "tokens": ["Gleit'", "ich", "\u00fc\u00b7ber", "den", "Strom", ",", "der", "sanf\u00b7ter", "auf\u00b7schwillt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,", "PRELS", "ADJD", "VVPP", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Denn, der mich begleitet, der Gott gebots ihm!", "tokens": ["Denn", ",", "der", "mich", "be\u00b7glei\u00b7tet", ",", "der", "Gott", "ge\u00b7bots", "ihm", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PRELS", "PPER", "VVPP", "$,", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Weine nicht, Cidli.", "tokens": ["Wei\u00b7ne", "nicht", ",", "Cid\u00b7li", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "XY", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}