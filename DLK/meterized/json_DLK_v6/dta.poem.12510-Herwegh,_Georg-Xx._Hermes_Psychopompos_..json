{"dta.poem.12510": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Xx.  \n  Hermes   Psychopompos   .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1843", "urn": "urn:nbn:de:kobv:b4-200905192440", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hermes, Hermes Schattenf\u00fchrer,", "tokens": ["Her\u00b7mes", ",", "Her\u00b7mes", "Schat\u00b7ten\u00b7f\u00fch\u00b7rer", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "gro\u00dfer Todten-General!", "tokens": ["gro\u00b7\u00dfer", "Tod\u00b7ten\u00b7Ge\u00b7ne\u00b7ral", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gott der Diebe, Gott der Kr\u00e4mer,", "tokens": ["Gott", "der", "Die\u00b7be", ",", "Gott", "der", "Kr\u00e4\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gott der Deutschen allzumal;", "tokens": ["Gott", "der", "Deut\u00b7schen", "all\u00b7zu\u00b7mal", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Immer bist du noch befl\u00fcgelt", "tokens": ["Im\u00b7mer", "bist", "du", "noch", "be\u00b7fl\u00fc\u00b7gelt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "wie in der antiken Welt:", "tokens": ["wie", "in", "der", "an\u00b7ti\u00b7ken", "Welt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Doch die Schwingen an den F\u00fc\u00dfen", "tokens": ["Doch", "die", "Schwin\u00b7gen", "an", "den", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "deuten ", "tokens": ["deu\u00b7ten"], "token_info": ["word"], "pos": ["VVINF"], "meter": "+-", "measure": "trochaic.single"}}}}}