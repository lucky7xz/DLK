{"textgrid.poem.31736": {"metadata": {"author": {"name": "Weerth, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Arbeite", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Mann im schlechten blauen Kittel,", "tokens": ["Du", "Mann", "im", "schlech\u00b7ten", "blau\u00b7en", "Kit\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPRART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Arbeite! Schaffe Salz und Brot!", "tokens": ["Ar\u00b7bei\u00b7te", "!", "Schaf\u00b7fe", "Salz", "und", "Brot", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Arbeite! Arbeit ist ein Mittel,", "tokens": ["Ar\u00b7bei\u00b7te", "!", "Ar\u00b7beit", "ist", "ein", "Mit\u00b7tel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Probat f\u00fcr Pestilenz und Not.", "tokens": ["Pro\u00b7bat", "f\u00fcr", "Pes\u00b7ti\u00b7lenz", "und", "Not", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Arbeite! R\u00fchre deine Arme!", "tokens": ["Ar\u00b7bei\u00b7te", "!", "R\u00fch\u00b7re", "dei\u00b7ne", "Ar\u00b7me", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Arbeite sechzehn Stunden so!", "tokens": ["Ar\u00b7bei\u00b7te", "sech\u00b7zehn", "Stun\u00b7den", "so", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Arbeite! Nachts ja lacht das warme,", "tokens": ["Ar\u00b7bei\u00b7te", "!", "Nachts", "ja", "lacht", "das", "war\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "ADV", "VVFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Lager dir von faulem Stroh.", "tokens": ["Das", "La\u00b7ger", "dir", "von", "fau\u00b7lem", "Stroh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Arbeite! Hast ja straffe Sehnen.", "tokens": ["Ar\u00b7bei\u00b7te", "!", "Hast", "ja", "straf\u00b7fe", "Seh\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VAFIN", "ADV", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Arbeite! Denk, mit schwangerem Leib", "tokens": ["Ar\u00b7bei\u00b7te", "!", "Denk", ",", "mit", "schwan\u00b7ge\u00b7rem", "Leib"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Harrt in der H\u00fctte dein mit Tr\u00e4nen", "tokens": ["Harrt", "in", "der", "H\u00fct\u00b7te", "dein", "mit", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPOSAT", "APPR", "NN"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein sch\u00f6nes leichenbleiches Weib.", "tokens": ["Ein", "sch\u00f6\u00b7nes", "lei\u00b7chen\u00b7blei\u00b7ches", "Weib", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Arbeite! Gleich der Stirn der Rinder", "tokens": ["Ar\u00b7bei\u00b7te", "!", "Gleich", "der", "Stirn", "der", "Rin\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist ja die deine breit und dick.", "tokens": ["Ist", "ja", "die", "dei\u00b7ne", "breit", "und", "dick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "PPOSAT", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Arbeite! Deine nackten Kinder,", "tokens": ["Ar\u00b7bei\u00b7te", "!", "Dei\u00b7ne", "nack\u00b7ten", "Kin\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die k\u00fcssen dich, kehrst du zur\u00fcck.", "tokens": ["Die", "k\u00fcs\u00b7sen", "dich", ",", "kehrst", "du", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+++-+", "measure": "unknown.measure.penta"}}, "stanza.5": {"line.1": {"text": "Arbeite bis die Adern klopfen!", "tokens": ["Ar\u00b7bei\u00b7te", "bis", "die", "A\u00b7dern", "klop\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Arbeite bis die Rippe kracht!", "tokens": ["Ar\u00b7bei\u00b7te", "bis", "die", "Rip\u00b7pe", "kracht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Arbeite bis die Schl\u00e4fen tropfen \u2013", "tokens": ["Ar\u00b7bei\u00b7te", "bis", "die", "Schl\u00e4\u00b7fen", "trop\u00b7fen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du bist zur Arbeit ja gemacht!", "tokens": ["Du", "bist", "zur", "Ar\u00b7beit", "ja", "ge\u00b7macht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Arbeite bis die Sinne schwinden!", "tokens": ["Ar\u00b7bei\u00b7te", "bis", "die", "Sin\u00b7ne", "schwin\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Arbeite bis die Kraft versiegt!", "tokens": ["Ar\u00b7bei\u00b7te", "bis", "die", "Kraft", "ver\u00b7siegt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Arbeite! \u2013 Wirst ja Ruhe finden,", "tokens": ["Ar\u00b7bei\u00b7te", "!", "\u2013", "Wirst", "ja", "Ru\u00b7he", "fin\u00b7den", ","], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "VAFIN", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn dein Gebein im Grabe liegt.", "tokens": ["Wenn", "dein", "Ge\u00b7bein", "im", "Gra\u00b7be", "liegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}