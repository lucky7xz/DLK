{"textgrid.poem.55968": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Spanische T\u00e4nzerin", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie in der Hand ein Schwefelz\u00fcndholz, wei\u00df,", "tokens": ["Wie", "in", "der", "Hand", "ein", "Schwe\u00b7fel\u00b7z\u00fcnd\u00b7holz", ",", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "eh es zur Flamme kommt, nach allen Seiten", "tokens": ["eh", "es", "zur", "Flam\u00b7me", "kommt", ",", "nach", "al\u00b7len", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "zuckende Zungen streckt \u2013: beginnt im Kreis", "tokens": ["zu\u00b7cken\u00b7de", "Zun\u00b7gen", "streckt", "\u2013", ":", "be\u00b7ginnt", "im", "Kreis"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "$(", "$.", "VVFIN", "APPRART", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "naher Beschauer hastig, hell und hei\u00df", "tokens": ["na\u00b7her", "Be\u00b7schau\u00b7er", "has\u00b7tig", ",", "hell", "und", "hei\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJD", "$,", "ADJD", "KON", "ADJD"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "ihr runder Tanz sich zuckend auszubreiten.", "tokens": ["ihr", "run\u00b7der", "Tanz", "sich", "zu\u00b7ckend", "aus\u00b7zu\u00b7brei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PRF", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und pl\u00f6tzlich ist er Flamme, ganz und gar.", "tokens": ["Und", "pl\u00f6tz\u00b7lich", "ist", "er", "Flam\u00b7me", ",", "ganz", "und", "gar", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "NN", "$,", "ADV", "KON", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Mit einem Blick entz\u00fcndet sie ihr Haar", "tokens": ["Mit", "ei\u00b7nem", "Blick", "ent\u00b7z\u00fcn\u00b7det", "sie", "ihr", "Haar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und dreht auf einmal mit gewagter Kunst", "tokens": ["und", "dreht", "auf", "ein\u00b7mal", "mit", "ge\u00b7wag\u00b7ter", "Kunst"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "ihr ganzes Kleid in diese Feuersbrunst,", "tokens": ["ihr", "gan\u00b7zes", "Kleid", "in", "die\u00b7se", "Feu\u00b7ers\u00b7brunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "aus welcher sich, wie Schlangen die erschrecken,", "tokens": ["aus", "wel\u00b7cher", "sich", ",", "wie", "Schlan\u00b7gen", "die", "er\u00b7schre\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PRF", "$,", "PWAV", "NN", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "die nackten Arme wach und klappernd strecken.", "tokens": ["die", "nack\u00b7ten", "Ar\u00b7me", "wach", "und", "klap\u00b7pernd", "stre\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und dann: als w\u00fcrde ihr das Feuer knapp,", "tokens": ["Und", "dann", ":", "als", "w\u00fcr\u00b7de", "ihr", "das", "Feu\u00b7er", "knapp", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "KOKOM", "VAFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "nimmt sie es ganz zusamm und wirft es ab", "tokens": ["nimmt", "sie", "es", "ganz", "zu\u00b7samm", "und", "wirft", "es", "ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "PTKVZ", "KON", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "sehr herrisch, mit hochm\u00fctiger Geb\u00e4rde", "tokens": ["sehr", "her\u00b7risch", ",", "mit", "hoch\u00b7m\u00fc\u00b7ti\u00b7ger", "Ge\u00b7b\u00e4r\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und schaut: da liegt es rasend auf der Erde", "tokens": ["und", "schaut", ":", "da", "liegt", "es", "ra\u00b7send", "auf", "der", "Er\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "und flammt noch immer und ergiebt sich nicht \u2013.", "tokens": ["und", "flammt", "noch", "im\u00b7mer", "und", "er\u00b7giebt", "sich", "nicht", "\u2013", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "KON", "VVFIN", "PRF", "PTKNEG", "$(", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Doch sieghaft, sicher und mit einem s\u00fc\u00dfen", "tokens": ["Doch", "sieg\u00b7haft", ",", "si\u00b7cher", "und", "mit", "ei\u00b7nem", "s\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "ADJD", "KON", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "gr\u00fc\u00dfenden L\u00e4cheln hebt sie ihr Gesicht", "tokens": ["gr\u00fc\u00b7\u00dfen\u00b7den", "L\u00e4\u00b7cheln", "hebt", "sie", "ihr", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "und stampft es aus mit kleinen festen F\u00fc\u00dfen.", "tokens": ["und", "stampft", "es", "aus", "mit", "klei\u00b7nen", "fes\u00b7ten", "F\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}