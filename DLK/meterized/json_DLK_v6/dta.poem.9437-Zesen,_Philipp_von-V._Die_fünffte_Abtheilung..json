{"dta.poem.9437": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "V.  \n Die f\u00fcnffte Abtheilung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ja Liebeste Schwester/ dein Bruder ist kommen/", "tokens": ["Ja", "Lie\u00b7bes\u00b7te", "Schwes\u00b7ter", "/", "dein", "Bru\u00b7der", "ist", "kom\u00b7men", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADJA", "NN", "$(", "PPOSAT", "NN", "VAFIN", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "und gehet im Garten und siehet nach Dier/", "tokens": ["und", "ge\u00b7het", "im", "Gar\u00b7ten", "und", "sie\u00b7het", "nach", "Dier", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "KON", "VVFIN", "APPR", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ich habe mir Myrrhen und W\u00fcrtze genommen/", "tokens": ["Ich", "ha\u00b7be", "mir", "Myr\u00b7rhen", "und", "W\u00fcrt\u00b7ze", "ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ich habe den Honig gekostet allhier.", "tokens": ["Ich", "ha\u00b7be", "den", "Ho\u00b7nig", "ge\u00b7kos\u00b7tet", "all\u00b7hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "ADV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Ich habe von Reben", "tokens": ["Ich", "ha\u00b7be", "von", "Re\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Gerruncken/ mein Leben/", "tokens": ["Ger\u00b7run\u00b7cken", "/", "mein", "Le\u00b7ben", "/"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$(", "PPOSAT", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Den s\u00fc\u00dfesten Wein.", "tokens": ["Den", "s\u00fc\u00b7\u00dfes\u00b7ten", "Wein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Kommt nehmet und esset", "tokens": ["Kommt", "neh\u00b7met", "und", "es\u00b7set"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Des traurens vergesset/", "tokens": ["Des", "trau\u00b7rens", "ver\u00b7ges\u00b7set", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Jhr Lieben/ Ey! trincket und schencket vollein!", "tokens": ["Ihr", "Lie\u00b7ben", "/", "Ey", "!", "trin\u00b7cket", "und", "schen\u00b7cket", "voll\u00b7ein", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$(", "NN", "$.", "VVFIN", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}