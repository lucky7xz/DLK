{"dta.poem.19423": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "156.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer \u00fcber Gr\u00e4ber geht, und denket nicht an sich,", "tokens": ["Wer", "\u00fc\u00b7ber", "Gr\u00e4\u00b7ber", "geht", ",", "und", "den\u00b7ket", "nicht", "an", "sich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "VVFIN", "$,", "KON", "VVFIN", "PTKNEG", "APPR", "PRF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und spricht nicht ein Gebet, thut doppelt freventlich.", "tokens": ["Und", "spricht", "nicht", "ein", "Ge\u00b7bet", ",", "thut", "dop\u00b7pelt", "fre\u00b7vent\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ART", "NN", "$,", "VVFIN", "ADJD", "ADJD", "$."], "meter": "-+---+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Er hat vergessen, da\u00df im Herrn die Todten leben,", "tokens": ["Er", "hat", "ver\u00b7ges\u00b7sen", ",", "da\u00df", "im", "Herrn", "die", "Tod\u00b7ten", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "KOUS", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und hat vergessen, da\u00df er selbst soll sterben eben.", "tokens": ["Und", "hat", "ver\u00b7ges\u00b7sen", ",", "da\u00df", "er", "selbst", "soll", "ster\u00b7ben", "e\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "