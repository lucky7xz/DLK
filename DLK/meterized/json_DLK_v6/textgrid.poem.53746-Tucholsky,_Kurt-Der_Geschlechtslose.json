{"textgrid.poem.53746": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Der Geschlechtslose", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe keine Zeugungsglieder.", "tokens": ["Ich", "ha\u00b7be", "kei\u00b7ne", "Zeu\u00b7gungs\u00b7glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich bin kein Mann \u2013 das steht mal fest.", "tokens": ["Ich", "bin", "kein", "Mann", "\u2013", "das", "steht", "mal", "fest", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$(", "PDS", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mir ist der Umsturz sehr zuwider \u2013", "tokens": ["Mir", "ist", "der", "Um\u00b7sturz", "sehr", "zu\u00b7wi\u00b7der", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "ich hasse Lenin wie die Pest.", "tokens": ["ich", "has\u00b7se", "Le\u00b7nin", "wie", "die", "Pest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Was auch geschieht, ich respektiere", "tokens": ["Was", "auch", "ge\u00b7schieht", ",", "ich", "res\u00b7pek\u00b7tie\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "ADV", "VVFIN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "die Uniform voll B\u00fcrgersinn.", "tokens": ["die", "U\u00b7ni\u00b7form", "voll", "B\u00fcr\u00b7ger\u00b7sinn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und treten mich die Untroffziere,", "tokens": ["Und", "tre\u00b7ten", "mich", "die", "Un\u00b7troff\u00b7zie\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "so schmerzt mich nur, da\u00df ich es bin.", "tokens": ["so", "schmerzt", "mich", "nur", ",", "da\u00df", "ich", "es", "bin", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mich zieren keine runden Br\u00fcste.", "tokens": ["Mich", "zie\u00b7ren", "kei\u00b7ne", "run\u00b7den", "Br\u00fcs\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich bin kein Weib \u2013 das ist mal klar.", "tokens": ["Ich", "bin", "kein", "Weib", "\u2013", "das", "ist", "mal", "klar", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$(", "PDS", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer mich im Kompromi\u00df auch k\u00fc\u00dfte:", "tokens": ["Wer", "mich", "im", "Kom\u00b7pro\u00b7mi\u00df", "auch", "k\u00fc\u00df\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "noch nie geschahs, da\u00df ich gebar.", "tokens": ["noch", "nie", "ge\u00b7schahs", ",", "da\u00df", "ich", "ge\u00b7bar", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$,", "KOUS", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "An alle hab ich mich verloren,", "tokens": ["An", "al\u00b7le", "hab", "ich", "mich", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VAFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "ich gab mich allen einmal hin.", "tokens": ["ich", "gab", "mich", "al\u00b7len", "ein\u00b7mal", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie kommts, da\u00df die zum Sieg erkoren,", "tokens": ["Wie", "kommts", ",", "da\u00df", "die", "zum", "Sieg", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "$,", "KOUS", "ART", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "und da\u00df ich stets der Dumme bin?", "tokens": ["und", "da\u00df", "ich", "stets", "der", "Dum\u00b7me", "bin", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Was ist es nur \u2013?", "tokens": ["Was", "ist", "es", "nur", "\u2013", "?"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "$(", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Ich seh mein Leibchen", "tokens": ["Ich", "seh", "mein", "Leib\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "im Spiegel an, und in der Tat:", "tokens": ["im", "Spie\u00b7gel", "an", ",", "und", "in", "der", "Tat", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$,", "KON", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich bin kein M\u00e4nnchen und kein Weibchen \u2013", "tokens": ["Ich", "bin", "kein", "M\u00e4nn\u00b7chen", "und", "kein", "Weib\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "KON", "PIAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "ich bin ein deutscher Demokrat.", "tokens": ["ich", "bin", "ein", "deut\u00b7scher", "De\u00b7mo\u00b7krat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}