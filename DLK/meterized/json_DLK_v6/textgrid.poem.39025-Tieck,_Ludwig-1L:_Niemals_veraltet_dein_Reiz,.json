{"textgrid.poem.39025": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Niemals veraltet dein Reiz,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Niemals veraltet dein Reiz,", "tokens": ["Nie\u00b7mals", "ver\u00b7al\u00b7tet", "dein", "Reiz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "So oft ich hier wandle.", "tokens": ["So", "oft", "ich", "hier", "wand\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Dank dem edlen Geiste,", "tokens": ["Dank", "dem", "ed\u00b7len", "Geis\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Der das s\u00fcsse Labyrinth erschuf", "tokens": ["Der", "das", "s\u00fcs\u00b7se", "La\u00b7by\u00b7rinth", "er\u00b7schuf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Und uns verg\u00f6nnte,", "tokens": ["Und", "uns", "ver\u00b7g\u00f6nn\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Hier, wo aus gr\u00fcnen B\u00e4umen", "tokens": ["Hier", ",", "wo", "aus", "gr\u00fc\u00b7nen", "B\u00e4u\u00b7men"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Bilder uns gr\u00fc\u00dfen,", "tokens": ["Bil\u00b7der", "uns", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Wo Blumenpracht den Fr\u00fchling ausgie\u00dft,", "tokens": ["Wo", "Blu\u00b7men\u00b7pracht", "den", "Fr\u00fch\u00b7ling", "aus\u00b7gie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Und Duft und Farben spendend", "tokens": ["Und", "Duft", "und", "Far\u00b7ben", "spen\u00b7dend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Alle Sinne mit Zauber umstrickt,", "tokens": ["Al\u00b7le", "Sin\u00b7ne", "mit", "Zau\u00b7ber", "um\u00b7strickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.11": {"text": "Gl\u00fccklich zu seyn.", "tokens": ["Gl\u00fcck\u00b7lich", "zu", "seyn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VAINF", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Dort das sprudelnde Wasser,", "tokens": ["Dort", "das", "spru\u00b7deln\u00b7de", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.13": {"text": "Und in dem einsamen Raum", "tokens": ["Und", "in", "dem", "ein\u00b7sa\u00b7men", "Raum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.14": {"text": "Unter Eppich und Ulmen versteckt,", "tokens": ["Un\u00b7ter", "Ep\u00b7pich", "und", "Ul\u00b7men", "ver\u00b7steckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "VVPP", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.15": {"text": "Die niederperlenden Tropfen Kristall's,", "tokens": ["Die", "nie\u00b7der\u00b7per\u00b7len\u00b7den", "Trop\u00b7fen", "Kris\u00b7tall's", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Die in Marmorbecken", "tokens": ["Die", "in", "Mar\u00b7mor\u00b7be\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["ART", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.17": {"text": "Melodisch fallen und klingen:", "tokens": ["Me\u00b7lo\u00b7disch", "fal\u00b7len", "und", "klin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.18": {"text": "Dazu der Turteltaube Liebesklage", "tokens": ["Da\u00b7zu", "der", "Tur\u00b7tel\u00b7tau\u00b7be", "Lie\u00b7bes\u00b7kla\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Aus dichterem Geb\u00fcsch,", "tokens": ["Aus", "dich\u00b7te\u00b7rem", "Ge\u00b7b\u00fcsch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.20": {"text": "Den wilden Waldruf", "tokens": ["Den", "wil\u00b7den", "Wald\u00b7ruf"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.21": {"text": "Fremden Gefl\u00fcgels.", "tokens": ["Frem\u00b7den", "Ge\u00b7fl\u00fc\u00b7gels", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.22": {"text": "Wie oft schon trank ich hier das s\u00fc\u00dfeste", "tokens": ["Wie", "oft", "schon", "trank", "ich", "hier", "das", "s\u00fc\u00b7\u00dfes\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Innigste Leben entz\u00fcckt. \u2013", "tokens": ["In\u00b7nigs\u00b7te", "Le\u00b7ben", "ent\u00b7z\u00fcckt", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "VVPP", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Hier auch bist du gewandelt,", "tokens": ["Hier", "auch", "bist", "du", "ge\u00b7wan\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Edelster Genius,", "tokens": ["E\u00b7dels\u00b7ter", "Ge\u00b7nius", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Unsers Vaterlands Zier und Lust,", "tokens": ["Un\u00b7sers", "Va\u00b7ter\u00b7lands", "Zier", "und", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "G\u00f6the, deutscher herrlicher S\u00e4nger.", "tokens": ["G\u00f6\u00b7the", ",", "deut\u00b7scher", "herr\u00b7li\u00b7cher", "S\u00e4n\u00b7ger", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Hier, so verk\u00fcndet die Sage,", "tokens": ["Hier", ",", "so", "ver\u00b7k\u00fcn\u00b7det", "die", "Sa\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-++-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Ward dein Lied vom Tasso gedichtet,", "tokens": ["Ward", "dein", "Lied", "vom", "Tas\u00b7so", "ge\u00b7dich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Und jedes lispelnde Blatt,", "tokens": ["Und", "je\u00b7des", "lis\u00b7peln\u00b7de", "Blatt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Der Lorbeer rauscht deinen Namen,", "tokens": ["Der", "Lor\u00b7beer", "rauscht", "dei\u00b7nen", "Na\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Die Springquellen reden von dir,", "tokens": ["Die", "Spring\u00b7quel\u00b7len", "re\u00b7den", "von", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "APPR", "PPER", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "Und ein Geisterschauer", "tokens": ["Und", "ein", "Geis\u00b7ter\u00b7schau\u00b7er"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.11": {"text": "Fliegt \u00fcber mir hinweg", "tokens": ["Fliegt", "\u00fc\u00b7ber", "mir", "hin\u00b7weg"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPER", "APZR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Und s\u00e4uselt noch heilig in den fernen Pinien.", "tokens": ["Und", "s\u00e4u\u00b7selt", "noch", "hei\u00b7lig", "in", "den", "fer\u00b7nen", "Pi\u00b7ni\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "So les' ich t\u00e4glich die alte Welt,", "tokens": ["So", "les'", "ich", "t\u00e4g\u00b7lich", "die", "al\u00b7te", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Stein und Boden und Flu\u00df,", "tokens": ["Stein", "und", "Bo\u00b7den", "und", "Flu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Himmelsbl\u00e4ue und Baum", "tokens": ["Him\u00b7mels\u00b7bl\u00e4u\u00b7e", "und", "Baum"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Reden von ihr.", "tokens": ["Re\u00b7den", "von", "ihr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Des Mittelalters Wunder,", "tokens": ["Des", "Mit\u00b7tel\u00b7al\u00b7ters", "Wun\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Kraft der Religion,", "tokens": ["Die", "Kraft", "der", "Re\u00b7li\u00b7gi\u00b7on", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die Helden der Vorzeit,", "tokens": ["Die", "Hel\u00b7den", "der", "Vor\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Treten sichtlich vor mich hin,", "tokens": ["Tre\u00b7ten", "sicht\u00b7lich", "vor", "mich", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Mit Glanz umflossen.", "tokens": ["Mit", "Glanz", "um\u00b7flos\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Schwebt mir Rafaels Schatten", "tokens": ["Schwebt", "mir", "Ra\u00b7faels", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.11": {"text": "Gr\u00fcssend vor\u00fcber,", "tokens": ["Gr\u00fcs\u00b7send", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Er inmitten der Schaar", "tokens": ["Er", "in\u00b7mit\u00b7ten", "der", "Schaar"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.13": {"text": "Der begeisterten Dichter und Bildner,", "tokens": ["Der", "be\u00b7geis\u00b7ter\u00b7ten", "Dich\u00b7ter", "und", "Bild\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.14": {"text": "Erwiedr' ich mit Thr\u00e4nen den Gru\u00df.", "tokens": ["Er\u00b7wiedr'", "ich", "mit", "Thr\u00e4\u00b7nen", "den", "Gru\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "NN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.15": {"text": "Und nun noch mu\u00df mir die s\u00fc\u00dfeste, lieblichste", "tokens": ["Und", "nun", "noch", "mu\u00df", "mir", "die", "s\u00fc\u00b7\u00dfes\u00b7te", ",", "lieb\u00b7lichs\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADV", "ADV", "VMFIN", "PPER", "ART", "ADJA", "$,", "ADJA"], "meter": "---+--+--+-+", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Sch\u00f6nste Erinnrung begegnen,", "tokens": ["Sch\u00f6ns\u00b7te", "E\u00b7rinn\u00b7rung", "be\u00b7geg\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.17": {"text": "Deine hohe Gestalt,", "tokens": ["Dei\u00b7ne", "ho\u00b7he", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.18": {"text": "Du mir von Kindheit befreundet,", "tokens": ["Du", "mir", "von", "Kind\u00b7heit", "be\u00b7freun\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "APPR", "NN", "VVPP", "$,"], "meter": "---+--+-", "measure": "iambic.di.relaxed"}, "line.19": {"text": "Vorbild und Muster,", "tokens": ["Vor\u00b7bild", "und", "Mus\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.20": {"text": "In dessen Lied mir der trunknen", "tokens": ["In", "des\u00b7sen", "Lied", "mir", "der", "trunk\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "PPER", "ART", "ADJA"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.21": {"text": "Begeistrung Quelle rauscht,", "tokens": ["Be\u00b7geis\u00b7trung", "Quel\u00b7le", "rauscht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.22": {"text": "Du, der den Muth der Brust mir weckst,", "tokens": ["Du", ",", "der", "den", "Muth", "der", "Brust", "mir", "weckst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Und, Unerreichbarer, im Kampf der Liebe", "tokens": ["Und", ",", "Un\u00b7er\u00b7reich\u00b7ba\u00b7rer", ",", "im", "Kampf", "der", "Lie\u00b7be"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "NN", "$,", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Das frohe Gef\u00fchl mir wieder", "tokens": ["Das", "fro\u00b7he", "Ge\u00b7f\u00fchl", "mir", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPER", "ADV"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.25": {"text": "In Besch\u00e4mung wandelst.", "tokens": ["In", "Be\u00b7sch\u00e4\u00b7mung", "wan\u00b7delst", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}