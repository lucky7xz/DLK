{"dta.poem.12677": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Das verliebte m\u00e4gdgen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es bleibt dabey,", "tokens": ["Es", "bleibt", "da\u00b7bey", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Da\u00df jeder mensch verliebet sey:", "tokens": ["Da\u00df", "je\u00b7der", "mensch", "ver\u00b7lie\u00b7bet", "sey", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VVFIN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Will di\u00df das frauen-volck gleich offters nicht gestehn,", "tokens": ["Will", "di\u00df", "das", "frau\u00b7en\u00b7volck", "gleich", "off\u00b7ters", "nicht", "ge\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDS", "ART", "NN", "ADV", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und schweret stein und bein,", "tokens": ["Und", "schwe\u00b7ret", "stein", "und", "bein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es treffe di\u00df bey ihnen doch nicht ein,", "tokens": ["Es", "tref\u00b7fe", "di\u00df", "bey", "ih\u00b7nen", "doch", "nicht", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "APPR", "PPER", "ADV", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "So zeigt sichs doch denn in der that,", "tokens": ["So", "zeigt", "sichs", "doch", "denn", "in", "der", "that", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "ADV", "APPR", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ob di\u00df ihr wort viel auf sich hat,", "tokens": ["Ob", "di\u00df", "ihr", "wort", "viel", "auf", "sich", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "PPOSAT", "NN", "ADV", "APPR", "PRF", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn ihrer zehen sich um einen mann offt schlagen,", "tokens": ["Wenn", "ih\u00b7rer", "ze\u00b7hen", "sich", "um", "ei\u00b7nen", "mann", "offt", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "CARD", "PRF", "APPR", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der noch dazu sich mu\u00df mit einer kr\u00fccke tragen.", "tokens": ["Der", "noch", "da\u00b7zu", "sich", "mu\u00df", "mit", "ei\u00b7ner", "kr\u00fc\u00b7cke", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PAV", "PRF", "VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Drum bleibt es bibel-wahr,", "tokens": ["Drum", "bleibt", "es", "bi\u00b7bel\u00b7wahr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Da\u00df uns die m\u00e4gdgen trefflich lieben,", "tokens": ["Da\u00df", "uns", "die", "m\u00e4gd\u00b7gen", "treff\u00b7lich", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und wenn wir st\u00fcrben, uns wohl gar", "tokens": ["Und", "wenn", "wir", "st\u00fcr\u00b7ben", ",", "uns", "wohl", "gar"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Mit ihren nadeln bald aus unsern gr\u00e4bern gr\u00fcben.", "tokens": ["Mit", "ih\u00b7ren", "na\u00b7deln", "bald", "aus", "un\u00b7sern", "gr\u00e4\u00b7bern", "gr\u00fc\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}