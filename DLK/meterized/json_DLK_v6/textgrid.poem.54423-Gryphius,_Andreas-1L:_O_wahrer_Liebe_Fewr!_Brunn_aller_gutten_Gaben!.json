{"textgrid.poem.54423": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "1L: O wahrer Liebe Fewr! Brunn aller gutten Gaben!", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O wahrer Liebe Fewr! Brunn aller gutten Gaben!", "tokens": ["O", "wah\u00b7rer", "Lie\u00b7be", "Fewr", "!", "Brunn", "al\u00b7ler", "gut\u00b7ten", "Ga\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "NN", "$.", "NN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O dreymal grosser Gott/ O h\u00f6chste Heyligkeit!", "tokens": ["O", "drey\u00b7mal", "gros\u00b7ser", "Gott", "/", "O", "h\u00f6chs\u00b7te", "Hey\u00b7lig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJA", "NN", "$(", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "O Meister aller Kunst/ O Frewd/ die alles Leid", "tokens": ["O", "Meis\u00b7ter", "al\u00b7ler", "Kunst", "/", "O", "Frewd", "/", "die", "al\u00b7les", "Leid"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "PIAT", "NN", "$(", "NE", "NN", "$(", "ART", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vertreibt/ O keusche Taub/ vor der die Hellen-Raben", "tokens": ["Ver\u00b7treibt", "/", "O", "keu\u00b7sche", "Taub", "/", "vor", "der", "die", "Hel\u00b7len\u00b7Ra\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$(", "NE", "ADJA", "NN", "$(", "APPR", "ART", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Erzittern! welche noch/ eh denn die Berg erhaben/", "tokens": ["Er\u00b7zit\u00b7tern", "!", "wel\u00b7che", "noch", "/", "eh", "denn", "die", "Berg", "er\u00b7ha\u00b7ben", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PRELS", "ADV", "$(", "KOUS", "ADV", "ART", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vnd eh die Welt gegr\u00fcndt; eh das gestirnte Kleid/", "tokens": ["Vnd", "eh", "die", "Welt", "ge\u00b7gr\u00fcndt", ";", "eh", "das", "ge\u00b7stirn\u00b7te", "Kleid", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVPP", "$.", "KOUS", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dem Himmel angelegt/ ja schon vor Ewigkeit/", "tokens": ["Dem", "Him\u00b7mel", "an\u00b7ge\u00b7legt", "/", "ja", "schon", "vor", "E\u00b7wig\u00b7keit", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$(", "ADV", "ADV", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die zwey die dir gantz gleich/ von Sich gelassen haben!", "tokens": ["Die", "zwey", "die", "dir", "gantz", "gleich", "/", "von", "Sich", "ge\u00b7las\u00b7sen", "ha\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "ART", "PPER", "ADV", "ADV", "$(", "APPR", "PRF", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "O wei\u00dfheit ohne Maa\u00df! O Gast der reinen Seel.", "tokens": ["O", "wei\u00df\u00b7heit", "oh\u00b7ne", "Maa\u00df", "!", "O", "Gast", "der", "rei\u00b7nen", "Seel", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "NN", "$.", "NE", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "O wesentliches Liecht! O tewre Gnaden-Quell", "tokens": ["O", "we\u00b7sent\u00b7li\u00b7ches", "Liecht", "!", "O", "tew\u00b7re", "Gna\u00b7den\u00b7Quell"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$.", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die du den zarten Leib Mariens hast befeuchtet/", "tokens": ["Die", "du", "den", "zar\u00b7ten", "Leib", "Ma\u00b7ri\u00b7ens", "hast", "be\u00b7feuch\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "NE", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ach la\u00df ein Tr\u00f6pfflin nur/ von deinem Lebenstaw", "tokens": ["Ach", "la\u00df", "ein", "Tr\u00f6pf\u00b7flin", "nur", "/", "von", "dei\u00b7nem", "Le\u00b7ben\u00b7staw"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "VVIMP", "ART", "NN", "ADV", "$(", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Erfrischen meinen Geist! hilff da\u00df Ich doch nur schaw", "tokens": ["Er\u00b7fri\u00b7schen", "mei\u00b7nen", "Geist", "!", "hilff", "da\u00df", "Ich", "doch", "nur", "schaw"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$.", "NN", "KOUS", "PPER", "ADV", "ADV", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ein F\u00fcncklin deiner Flam/ so bin Ich recht erleuchtet.", "tokens": ["Ein", "F\u00fcnck\u00b7lin", "dei\u00b7ner", "Flam", "/", "so", "bin", "Ich", "recht", "er\u00b7leuch\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$(", "ADV", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}