{"textgrid.poem.49544": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "An gewisse Frauenzimmer", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr habt mir lange schon, ich sey nicht sch\u00f6n, gesagt.", "tokens": ["Ihr", "habt", "mir", "lan\u00b7ge", "schon", ",", "ich", "sey", "nicht", "sch\u00f6n", ",", "ge\u00b7sagt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "$,", "PPER", "VAFIN", "PTKNEG", "ADJD", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie kommt es, da\u00df ihr mich noch immer damit plagt?", "tokens": ["Wie", "kommt", "es", ",", "da\u00df", "ihr", "mich", "noch", "im\u00b7mer", "da\u00b7mit", "plagt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PRF", "ADV", "ADV", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bem\u00fchet' ich mich dann, die Sache zu bestreiten?", "tokens": ["Be\u00b7m\u00fc\u00b7het'", "ich", "mich", "dann", ",", "die", "Sa\u00b7che", "zu", "be\u00b7strei\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ach! Sch\u00f6nen! qu\u00e4lt mich nicht mit alten Neuigkeiten.", "tokens": ["Ach", "!", "Sch\u00f6\u00b7nen", "!", "qu\u00e4lt", "mich", "nicht", "mit", "al\u00b7ten", "Neu\u00b7ig\u00b7kei\u00b7ten", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "NN", "$.", "VVFIN", "PPER", "PTKNEG", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ihr f\u00fchlt ja selbst, wie euch das Ding die Seele nagt,", "tokens": ["Ihr", "f\u00fchlt", "ja", "selbst", ",", "wie", "euch", "das", "Ding", "die", "See\u00b7le", "nagt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "PWAV", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df euch der Spiegel stets, was ihr mir saget, sagt.", "tokens": ["Da\u00df", "euch", "der", "Spie\u00b7gel", "stets", ",", "was", "ihr", "mir", "sa\u00b7get", ",", "sagt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "$,", "PWS", "PPER", "PPER", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}