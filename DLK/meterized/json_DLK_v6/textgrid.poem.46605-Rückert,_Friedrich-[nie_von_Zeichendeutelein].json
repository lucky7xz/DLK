{"textgrid.poem.46605": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[nie von Zeichendeutelein]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nie von Zeichendeutelein", "tokens": ["Nie", "von", "Zei\u00b7chen\u00b7deu\u00b7tel\u00b7ein"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df dich mehr betr\u00fcgen!", "tokens": ["La\u00df", "dich", "mehr", "be\u00b7tr\u00fc\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Denn die b\u00f6sen treffen ein,", "tokens": ["Denn", "die", "b\u00f6\u00b7sen", "tref\u00b7fen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die guten l\u00fcgen.", "tokens": ["Und", "die", "gu\u00b7ten", "l\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Meines Kindes Herze brach,", "tokens": ["Mei\u00b7nes", "Kin\u00b7des", "Her\u00b7ze", "brach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie's der Traum gesprochen;", "tokens": ["Wie's", "der", "Traum", "ge\u00b7spro\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und de\u00df Leben er versprach,", "tokens": ["Und", "de\u00df", "Le\u00b7ben", "er", "ver\u00b7sprach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dem ist's auch gebrochen.", "tokens": ["Dem", "ist's", "auch", "ge\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}