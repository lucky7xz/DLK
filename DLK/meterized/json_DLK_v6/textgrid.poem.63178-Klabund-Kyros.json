{"textgrid.poem.63178": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Kyros", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man sagt, dass Kyros, der Perser, die Griechen bekriege,", "tokens": ["Man", "sagt", ",", "dass", "Ky\u00b7ros", ",", "der", "Per\u00b7ser", ",", "die", "Grie\u00b7chen", "be\u00b7krie\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "NE", "$,", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Weil er die Griechenknaben liebe.", "tokens": ["Weil", "er", "die", "Grie\u00b7chen\u00b7kna\u00b7ben", "lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In silberne Fesseln schl\u00e4gt er die Gefangenen.", "tokens": ["In", "sil\u00b7ber\u00b7ne", "Fes\u00b7seln", "schl\u00e4gt", "er", "die", "Ge\u00b7fan\u00b7ge\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+-+-+-+--", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ihrer hundert ziehen seinen Sichelwagen", "tokens": ["Ih\u00b7rer", "hun\u00b7dert", "zie\u00b7hen", "sei\u00b7nen", "Si\u00b7chel\u00b7wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "CARD", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Nackt und nur gefl\u00fcgelte Sandalen an den F\u00fcssen", "tokens": ["Nackt", "und", "nur", "ge\u00b7fl\u00fc\u00b7gel\u00b7te", "San\u00b7da\u00b7len", "an", "den", "F\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "KON", "ADV", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.6": {"text": "Wie Hermes.", "tokens": ["Wie", "Her\u00b7mes", "."], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "NE", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Ihrer f\u00fcnfzig bedienen den Herrn bei der Tafel,", "tokens": ["Ih\u00b7rer", "f\u00fcnf\u00b7zig", "be\u00b7die\u00b7nen", "den", "Herrn", "bei", "der", "Ta\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "CARD", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Ihrer dreissig spielen mit ihm Diskos.", "tokens": ["Ih\u00b7rer", "dreis\u00b7sig", "spie\u00b7len", "mit", "ihm", "Dis\u00b7kos", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "CARD", "VVFIN", "APPR", "PPER", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Vor ihrer zehn deklamiert er persische Oden.", "tokens": ["Vor", "ih\u00b7rer", "zehn", "de\u00b7kla\u00b7miert", "er", "per\u00b7si\u00b7sche", "O\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "CARD", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Die also beginnen:", "tokens": ["Die", "al\u00b7so", "be\u00b7gin\u00b7nen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Griechenknaben, G\u00f6tters\u00f6hne ...", "tokens": ["Grie\u00b7chen\u00b7kna\u00b7ben", ",", "G\u00f6t\u00b7ter\u00b7s\u00f6h\u00b7ne", "..."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Aber zur Nacht", "tokens": ["A\u00b7ber", "zur", "Nacht"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPRART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.13": {"text": "L\u00e4sst er die weissen Knaben mit jungen schwarzen Sklavinnen spielen.", "tokens": ["L\u00e4sst", "er", "die", "weis\u00b7sen", "Kna\u00b7ben", "mit", "jun\u00b7gen", "schwar\u00b7zen", "Skla\u00b7vin\u00b7nen", "spie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "APPR", "ADJA", "ADJA", "NN", "VVINF", "$."], "meter": "+--+-+--+-+--+-+-", "measure": "iambic.septa.invert"}, "line.14": {"text": "Sie spielen Hund und H\u00fcndin.", "tokens": ["Sie", "spie\u00b7len", "Hund", "und", "H\u00fcn\u00b7din", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Der K\u00f6nig seufzt aus seinen Kissen", "tokens": ["Der", "K\u00f6\u00b7nig", "seufzt", "aus", "sei\u00b7nen", "Kis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Und zieht den sch\u00f6nsten der Knaben,", "tokens": ["Und", "zieht", "den", "sch\u00f6ns\u00b7ten", "der", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.17": {"text": "Die sch\u00f6nste Sklavin", "tokens": ["Die", "sch\u00f6ns\u00b7te", "Skla\u00b7vin"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.18": {"text": "An seine Seite,", "tokens": ["An", "sei\u00b7ne", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.19": {"text": "Entschl\u00e4ft in ihren Armen", "tokens": ["Ent\u00b7schl\u00e4ft", "in", "ih\u00b7ren", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Liebend, geliebt.", "tokens": ["Lie\u00b7bend", ",", "ge\u00b7liebt", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "VVPP", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}