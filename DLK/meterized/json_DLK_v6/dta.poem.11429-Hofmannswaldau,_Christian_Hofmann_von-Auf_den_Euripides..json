{"dta.poem.11429": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf den Euripides.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dein ruhm, Euripides! taurt bi\u00df auf diese stunde,", "tokens": ["Dein", "ruhm", ",", "Eu\u00b7ri\u00b7pi\u00b7des", "!", "taurt", "bi\u00df", "auf", "die\u00b7se", "stun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NE", "$.", "VVFIN", "ADV", "APPR", "PDAT", "ADJA", "$,"], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Du hast manch trauer-spiel beweglich aufgef\u00fchrt;", "tokens": ["Du", "hast", "manch", "trau\u00b7e\u00b7rspiel", "be\u00b7weg\u00b7lich", "auf\u00b7ge\u00b7f\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das aber, da die wuth der aufgehetzten hunde", "tokens": ["Das", "a\u00b7ber", ",", "da", "die", "wuth", "der", "auf\u00b7ge\u00b7hetz\u00b7ten", "hun\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "$,", "KOUS", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dich sterbend vorgestellt, hat uns weit mehr ger\u00fchrt.", "tokens": ["Dich", "ster\u00b7bend", "vor\u00b7ge\u00b7stellt", ",", "hat", "uns", "weit", "mehr", "ge\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVPP", "$,", "VAFIN", "PPER", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}}}}}