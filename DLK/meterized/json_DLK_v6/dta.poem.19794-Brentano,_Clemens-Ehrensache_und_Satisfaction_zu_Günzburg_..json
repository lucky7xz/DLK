{"dta.poem.19794": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Ehrensache und Satisfaction zu G\u00fcnzburg .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zu G\u00fcnzburg in der werthen Stadt,               ", "tokens": ["Zu", "G\u00fcnz\u00b7burg", "in", "der", "wert\u00b7hen", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als ihre Zunft den Jahrstag hat,", "tokens": ["Als", "ih\u00b7re", "Zunft", "den", "Jahr\u00b7stag", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Schneider alle kamen,", "tokens": ["Die", "Schnei\u00b7der", "al\u00b7le", "ka\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Meister s\u00e4mmtlich jung und alt,", "tokens": ["Die", "Meis\u00b7ter", "s\u00e4mmt\u00b7lich", "jung", "und", "alt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Gesellen auch in schiefer Gestalt", "tokens": ["Die", "Ge\u00b7sel\u00b7len", "auch", "in", "schie\u00b7fer", "Ge\u00b7stalt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPR", "ADJA", "NN"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Da in der Kirch zusammen.", "tokens": ["Da", "in", "der", "Kirch", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Teufel aber hat kein Ruh,", "tokens": ["Der", "Teu\u00b7fel", "a\u00b7ber", "hat", "kein", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Baut sein Capelle auch dazu,", "tokens": ["Baut", "sein", "Ca\u00b7pel\u00b7le", "auch", "da\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als sie zum Opfer gehen,", "tokens": ["Als", "sie", "zum", "Op\u00b7fer", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da hat man mitten in der Schaar", "tokens": ["Da", "hat", "man", "mit\u00b7ten", "in", "der", "Schaar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein gro\u00dfen Gei\u00dfbock offenbar", "tokens": ["Ein", "gro\u00b7\u00dfen", "Gei\u00df\u00b7bock", "of\u00b7fen\u00b7bar"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In ihrer Mitt gesehen.", "tokens": ["In", "ih\u00b7rer", "Mitt", "ge\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der gieng ganz sittsam neben her", "tokens": ["Der", "gieng", "ganz", "sitt\u00b7sam", "ne\u00b7ben", "her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "APPR", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Opfer zu in aller Ehr,", "tokens": ["Dem", "Op\u00b7fer", "zu", "in", "al\u00b7ler", "Ehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und th\u00e4t sich doch nit b\u00fccken,", "tokens": ["Und", "th\u00e4t", "sich", "doch", "nit", "b\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein alter Meister hochgeschorn", "tokens": ["Ein", "al\u00b7ter", "Meis\u00b7ter", "hoch\u00b7ge\u00b7schorn"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der fa\u00dft da einen grimmen Zorn,", "tokens": ["Der", "fa\u00dft", "da", "ei\u00b7nen", "grim\u00b7men", "Zorn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und wollt dar\u00fcber z\u00fccken.", "tokens": ["Und", "wollt", "da\u00b7r\u00fc\u00b7ber", "z\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PAV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wo f\u00fchrt der Teufel den Bock daher,", "tokens": ["Wo", "f\u00fchrt", "der", "Teu\u00b7fel", "den", "Bock", "da\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ART", "NE", "PAV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Potz Elle, Fingerhut und Scheer,", "tokens": ["Potz", "El\u00b7le", ",", "Fin\u00b7ger\u00b7hut", "und", "Scheer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er k\u00f6mmt mir recht und eben,", "tokens": ["Er", "k\u00f6mmt", "mir", "recht", "und", "e\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "KON", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gieng er nur besser her zu mir,", "tokens": ["Gieng", "er", "nur", "bes\u00b7ser", "her", "zu", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich w\u00fcsste schon ein Kunst daf\u00fcr,", "tokens": ["Ich", "w\u00fcss\u00b7te", "schon", "ein", "Kunst", "da\u00b7f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wollt ihm ein Maultasch geben.", "tokens": ["Wollt", "ihm", "ein", "Maul\u00b7tasch", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}