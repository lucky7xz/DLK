{"textgrid.poem.62593": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Madrigal", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das gl\u00e4nzende und helle Nacht-Gew\u00fcrm,", "tokens": ["Das", "gl\u00e4n\u00b7zen\u00b7de", "und", "hel\u00b7le", "Nacht\u00b7Ge\u00b7w\u00fcrm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Scheint nur zur warmen Sommer-Zeit.", "tokens": ["Scheint", "nur", "zur", "war\u00b7men", "Som\u00b7mer\u00b7Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So auch die Heuchel-Christen,", "tokens": ["So", "auch", "die", "Heu\u00b7chel\u00b7Chris\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sich bey dem Gl\u00fcck mit ihrer Fr\u00f6mmigkeit", "tokens": ["Sich", "bey", "dem", "Gl\u00fcck", "mit", "ih\u00b7rer", "Fr\u00f6m\u00b7mig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und Liebe Gottes br\u00fcsten.", "tokens": ["Und", "Lie\u00b7be", "Got\u00b7tes", "br\u00fcs\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Frommen aber sind nicht so gesinnt:", "tokens": ["Die", "From\u00b7men", "a\u00b7ber", "sind", "nicht", "so", "ge\u00b7sinnt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Bey rauher Zeit, bey Tr\u00fcbsal, Sturm und Wind;", "tokens": ["Bey", "rau\u00b7her", "Zeit", ",", "bey", "Tr\u00fcb\u00b7sal", ",", "Sturm", "und", "Wind", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Bey Jammer, Elend, Weinen", "tokens": ["Bey", "Jam\u00b7mer", ",", "E\u00b7lend", ",", "Wei\u00b7nen"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Sie nach der Sterne Art bey kalten N\u00e4chten funkelnd scheinen.", "tokens": ["Sie", "nach", "der", "Ster\u00b7ne", "Art", "bey", "kal\u00b7ten", "N\u00e4ch\u00b7ten", "fun\u00b7kelnd", "schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "NN", "APPR", "ADJA", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}}}}