{"textgrid.poem.25223": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Durch silberne Halme", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch silberne Halme", "tokens": ["Durch", "sil\u00b7ber\u00b7ne", "Hal\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Eisiger Scheiben", "tokens": ["Ei\u00b7si\u00b7ger", "Schei\u00b7ben"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "D\u00e4mmert zu mir", "tokens": ["D\u00e4m\u00b7mert", "zu", "mir"], "token_info": ["word", "word", "word"], "pos": ["NE", "APPR", "PPER"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Ins Dunkel der Mond./", "tokens": ["Ins", "Dun\u00b7kel", "der", "Mond", ".", "/"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$.", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Ich bin ein See,", "tokens": ["Ich", "bin", "ein", "See", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Erstarrt zu Eise,", "tokens": ["Er\u00b7starrt", "zu", "Ei\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Darin sich spiegelt", "tokens": ["Da\u00b7rin", "sich", "spie\u00b7gelt"], "token_info": ["word", "word", "word"], "pos": ["PAV", "PRF", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Der traurige Mond;", "tokens": ["Der", "trau\u00b7ri\u00b7ge", "Mond", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "D\u00fcrres Schilf", "tokens": ["D\u00fcr\u00b7res", "Schilf"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Zittert und fl\u00fcstert ...", "tokens": ["Zit\u00b7tert", "und", "fl\u00fcs\u00b7tert", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Ich h\u00f6re dich weinen", "tokens": ["Ich", "h\u00f6\u00b7re", "dich", "wei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und schluchzen/ wie einst.", "tokens": ["Und", "schluch\u00b7zen", "/", "wie", "einst", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVINF", "$(", "KOKOM", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Einst f\u00fcllt' ich achtlos", "tokens": ["Einst", "f\u00fcllt'", "ich", "acht\u00b7los"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Dir Tage mit Leide,", "tokens": ["Dir", "Ta\u00b7ge", "mit", "Lei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Bis da\u00df du weintest", "tokens": ["Bis", "da\u00df", "du", "wein\u00b7test"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Aus schluchzender Brust.", "tokens": ["Aus", "schluch\u00b7zen\u00b7der", "Brust", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Wohl hab ich flehend", "tokens": ["Wohl", "hab", "ich", "fle\u00b7hend"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Gek\u00fc\u00dft die Tr\u00e4nen,", "tokens": ["Ge\u00b7k\u00fc\u00dft", "die", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Doch war's geschehen,", "tokens": ["Doch", "wa\u00b7r's", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Da\u00df du geweint.", "tokens": ["Da\u00df", "du", "ge\u00b7weint", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Jetzt ist dein Auge", "tokens": ["Jetzt", "ist", "dein", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "L\u00e4ngst getrocknet ...", "tokens": ["L\u00e4ngst", "ge\u00b7trock\u00b7net", "..."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Doch weinst du ewig", "tokens": ["Doch", "weinst", "du", "e\u00b7wig"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "In meiner Seele.", "tokens": ["In", "mei\u00b7ner", "See\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Und ich mu\u00df weinen", "tokens": ["Und", "ich", "mu\u00df", "wei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "All deine Tr\u00e4nen,", "tokens": ["All", "dei\u00b7ne", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Geliebtes Antlitz/", "tokens": ["Ge\u00b7lieb\u00b7tes", "Ant\u00b7litz", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Und noch viel mehr.", "tokens": ["Und", "noch", "viel", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}