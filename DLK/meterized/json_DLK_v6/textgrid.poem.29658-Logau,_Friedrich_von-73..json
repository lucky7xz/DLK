{"textgrid.poem.29658": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "73.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rumrich ist ein Tausend-K\u00fcnstler; was er wil, mu\u00df ihm gelingen;", "tokens": ["Rum\u00b7rich", "ist", "ein", "Tau\u00b7sen\u00b7dK\u00fcnst\u00b7ler", ";", "was", "er", "wil", ",", "mu\u00df", "ihm", "ge\u00b7lin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$.", "PWS", "PPER", "VMFIN", "$,", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Kan er eines, glaub ich alles: \u00fcber seinen Schaten springen", "tokens": ["Kan", "er", "ei\u00b7nes", ",", "glaub", "ich", "al\u00b7les", ":", "\u00fc\u00b7ber", "sei\u00b7nen", "Scha\u00b7ten", "sprin\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PIS", "$,", "VVFIN", "PPER", "PIS", "$.", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Oder (ist ihm dieses lieber) pfeiffen und zu gleiche singen.", "tokens": ["O\u00b7der", "(", "ist", "ihm", "die\u00b7ses", "lie\u00b7ber", ")", "pfeif\u00b7fen", "und", "zu", "glei\u00b7che", "sin\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "VAFIN", "PPER", "PDAT", "ADV", "$(", "VVFIN", "KON", "APPR", "ADJA", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}