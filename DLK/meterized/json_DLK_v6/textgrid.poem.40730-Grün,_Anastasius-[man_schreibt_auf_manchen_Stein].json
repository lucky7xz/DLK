{"textgrid.poem.40730": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "[man schreibt auf manchen Stein]", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man schreibt auf manchen Stein:", "tokens": ["Man", "schreibt", "auf", "man\u00b7chen", "Stein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "\u00bber hatte keinen Feind!\u00ab", "tokens": ["\u00bb", "er", "hat\u00b7te", "kei\u00b7nen", "Feind", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als Lobspruch ist's gemeint,", "tokens": ["Als", "Lob\u00b7spruch", "ist's", "ge\u00b7meint", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Doch schlie\u00dft's viel Schlimmes ein:", "tokens": ["Doch", "schlie\u00dft's", "viel", "Schlim\u00b7mes", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es kl\u00e4nge just so gut:", "tokens": ["Es", "kl\u00e4n\u00b7ge", "just", "so", "gut", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ihm fehlte Herz und Blut,", "tokens": ["Ihm", "fehl\u00b7te", "Herz", "und", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Er lie\u00df wie Kies sich treten,", "tokens": ["Er", "lie\u00df", "wie", "Kies", "sich", "tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "NE", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Er lie\u00df wie Thon sich kneten,", "tokens": ["Er", "lie\u00df", "wie", "Thon", "sich", "kne\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Sein Aug' war blind dem Lichte,", "tokens": ["Sein", "Aug'", "war", "blind", "dem", "Lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Sein Mund war stumm f\u00fcr Wichte.", "tokens": ["Sein", "Mund", "war", "stumm", "f\u00fcr", "Wich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}