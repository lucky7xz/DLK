{"textgrid.poem.66773": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Am Br\u00fcckenrande", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Grauwei\u00dfer Nebel lastet kalt und feucht,", "tokens": ["Grau\u00b7wei\u00b7\u00dfer", "Ne\u00b7bel", "las\u00b7tet", "kalt", "und", "feucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Tr\u00fcb flimmert's von den schwarzen Schifferk\u00e4hnen,", "tokens": ["Tr\u00fcb", "flim\u00b7mert's", "von", "den", "schwar\u00b7zen", "Schif\u00b7fer\u00b7k\u00e4h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Am Br\u00fcckenrande la\u00dft mich einsam lehnen,", "tokens": ["Am", "Br\u00fc\u00b7cken\u00b7ran\u00b7de", "la\u00dft", "mich", "ein\u00b7sam", "leh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Arm gest\u00fctzt, das Haupt hinausgebeugt!", "tokens": ["Im", "Arm", "ge\u00b7st\u00fctzt", ",", "das", "Haupt", "hin\u00b7aus\u00b7ge\u00b7beugt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Weltstadt Wirbel braust an mir vor\u00fcber,", "tokens": ["Der", "Welt\u00b7stadt", "Wir\u00b7bel", "braust", "an", "mir", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Laut donnernd rollt's vor meinem Ohre hin,", "tokens": ["Laut", "don\u00b7nernd", "rollt's", "vor", "mei\u00b7nem", "Oh\u00b7re", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die Schifferlampen flimmern tr\u00fcb und tr\u00fcber,", "tokens": ["Die", "Schif\u00b7fer\u00b7lam\u00b7pen", "flim\u00b7mern", "tr\u00fcb", "und", "tr\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "In Nacht und Nebel weht mein Sinn.", "tokens": ["In", "Nacht", "und", "Ne\u00b7bel", "weht", "mein", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O stolzes Herz, weltsch\u00f6pfender Titan,", "tokens": ["O", "stol\u00b7zes", "Herz", ",", "welt\u00b7sch\u00f6p\u00b7fen\u00b7der", "Ti\u00b7tan", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PWAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Quellfrischer Mut, du machtlos m\u00e4chtig Wollen,", "tokens": ["Quell\u00b7fri\u00b7scher", "Mut", ",", "du", "macht\u00b7los", "m\u00e4ch\u00b7tig", "Wol\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "ADJD", "ADJD", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "H\u00f6rst du des Lebens Speichen dr\u00f6hnend rollen?", "tokens": ["H\u00f6rst", "du", "des", "Le\u00b7bens", "Spei\u00b7chen", "dr\u00f6h\u00b7nend", "rol\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du bist ein Nichts in deinem Schw\u00e4rmerwahn.", "tokens": ["Du", "bist", "ein", "Nichts", "in", "dei\u00b7nem", "Schw\u00e4r\u00b7mer\u00b7wahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "PIS", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nach Atzung schreit, nach Atzung rennt das Leben,", "tokens": ["Nach", "At\u00b7zung", "schreit", ",", "nach", "At\u00b7zung", "rennt", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Magen herrscht, der hungrige Despot,", "tokens": ["Der", "Ma\u00b7gen", "herrscht", ",", "der", "hung\u00b7ri\u00b7ge", "Des\u00b7pot", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Du Tor, du Narr, la\u00df ab von deinem Streben", "tokens": ["Du", "Tor", ",", "du", "Narr", ",", "la\u00df", "ab", "von", "dei\u00b7nem", "Stre\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "$,", "NE", "NE", "$,", "VVIMP", "PTKVZ", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und sei wie sie und such dein Brot!", "tokens": ["Und", "sei", "wie", "sie", "und", "such", "dein", "Brot", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "KOKOM", "PPER", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "S\u00e4nftigt dein Wort der Millionen Weh?", "tokens": ["S\u00e4nf\u00b7tigt", "dein", "Wort", "der", "Mil\u00b7lion\u00b7en", "Weh", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "NN", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Ach nein, sie schwitzen Blut, indes du dichtest!", "tokens": ["Ach", "nein", ",", "sie", "schwit\u00b7zen", "Blut", ",", "in\u00b7des", "du", "dich\u00b7test", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKANT", "$,", "PPER", "ADJA", "NN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was frommt es, da\u00df du segnest oder richtest?", "tokens": ["Was", "frommt", "es", ",", "da\u00df", "du", "seg\u00b7nest", "o\u00b7der", "rich\u00b7test", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was ist ein Tr\u00f6pflein Trost im Tr\u00e4nensee?", "tokens": ["Was", "ist", "ein", "Tr\u00f6pf\u00b7lein", "Trost", "im", "Tr\u00e4\u00b7nen\u00b7see", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Geh, greif zum Hammer und zur Linnenbluse", "tokens": ["Geh", ",", "greif", "zum", "Ham\u00b7mer", "und", "zur", "Lin\u00b7nen\u00b7blu\u00b7se"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "ADJD", "APPRART", "NN", "KON", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und leg den schwachen Griffel aus der Hand,", "tokens": ["Und", "leg", "den", "schwa\u00b7chen", "Grif\u00b7fel", "aus", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Es ist kein Raum auf Erden f\u00fcr die Muse,", "tokens": ["Es", "ist", "kein", "Raum", "auf", "Er\u00b7den", "f\u00fcr", "die", "Mu\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dort oben ist ihr Heimatland.", "tokens": ["Dort", "o\u00b7ben", "ist", "ihr", "Hei\u00b7mat\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du willst Prophet, willst Menschheitsf\u00fchrer sein,", "tokens": ["Du", "willst", "Pro\u00b7phet", ",", "willst", "Menschheits\u00b7f\u00fch\u00b7rer", "sein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "$,", "VMFIN", "NN", "VAINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nicht, wie das Laffenpack, in Versen l\u00fcgen,", "tokens": ["Nicht", ",", "wie", "das", "Laf\u00b7fen\u00b7pack", ",", "in", "Ver\u00b7sen", "l\u00fc\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "PWAV", "ART", "NN", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Willst mit der Wahrheit eisig-ernsten Z\u00fcgen", "tokens": ["Willst", "mit", "der", "Wahr\u00b7heit", "ei\u00b7sig\u00b7erns\u00b7ten", "Z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Die Welt vom Schleier ihres Trugs befrein?", "tokens": ["Die", "Welt", "vom", "Schlei\u00b7er", "ih\u00b7res", "Trugs", "be\u00b7fr\u00b7ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Glaubst du, der P\u00f6bel kommt zu deinem Spiegel,", "tokens": ["Glaubst", "du", ",", "der", "P\u00f6\u00b7bel", "kommt", "zu", "dei\u00b7nem", "Spie\u00b7gel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Erbebt vor Schreck und eilt hindann bekehrt?", "tokens": ["Er\u00b7bebt", "vor", "Schreck", "und", "eilt", "hin\u00b7dann", "be\u00b7kehrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "VVFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Glaubst du, aus deiner Dichtung L\u00e4utrungstiegel", "tokens": ["Glaubst", "du", ",", "aus", "dei\u00b7ner", "Dich\u00b7tung", "L\u00e4ut\u00b7rungs\u00b7tie\u00b7gel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ersteht die Menschheit goldgekl\u00e4rt?", "tokens": ["Er\u00b7steht", "die", "Menschheit", "gold\u00b7ge\u00b7kl\u00e4rt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "O dumpfe Qual! der Nebel ballt sich dicht,", "tokens": ["O", "dump\u00b7fe", "Qual", "!", "der", "Ne\u00b7bel", "ballt", "sich", "dicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "ART", "NN", "VVFIN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gestaltenlos umflort er meine Augen,", "tokens": ["Ge\u00b7stal\u00b7ten\u00b7los", "um\u00b7flort", "er", "mei\u00b7ne", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gebt Luft und Licht, sie br\u00fcnstig einzusaugen!", "tokens": ["Gebt", "Luft", "und", "Licht", ",", "sie", "br\u00fcns\u00b7tig", "ein\u00b7zu\u00b7sau\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "KON", "NN", "$,", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dies \u00f6de Nebelchaos trag ich nicht.", "tokens": ["Dies", "\u00f6\u00b7de", "Ne\u00b7belc\u00b7haos", "trag", "ich", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Schwer, wie der Brockenriese, pre\u00dft's mich nieder,", "tokens": ["Schwer", ",", "wie", "der", "Bro\u00b7cken\u00b7rie\u00b7se", ",", "pre\u00dft's", "mich", "nie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "Ich ringe, bete, flehe \u2013 la\u00dft mich frei!", "tokens": ["Ich", "rin\u00b7ge", ",", "be\u00b7te", ",", "fle\u00b7he", "\u2013", "la\u00dft", "mich", "frei", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$(", "VVIMP", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Fr\u00f6stelnd erschauern meine m\u00fcden Glieder \u2013", "tokens": ["Fr\u00f6s\u00b7telnd", "er\u00b7schau\u00b7ern", "mei\u00b7ne", "m\u00fc\u00b7den", "Glie\u00b7der", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.8": {"text": "O, w\u00e4r's auf immerdar vorbei! ...", "tokens": ["O", ",", "w\u00e4r's", "auf", "im\u00b7mer\u00b7dar", "vor\u00b7bei", "!", "..."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "VAFIN", "APPR", "ADV", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und doch! Wach auf aus deiner Seelenpein,", "tokens": ["Und", "doch", "!", "Wach", "auf", "aus", "dei\u00b7ner", "See\u00b7len\u00b7pein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "NN", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zieh stolz und freudig f\u00fcrder deine Bahnen,", "tokens": ["Zieh", "stolz", "und", "freu\u00b7dig", "f\u00fcr\u00b7der", "dei\u00b7ne", "Bah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und zweifelst du, la\u00df eines dich gemahnen:", "tokens": ["Und", "zwei\u00b7felst", "du", ",", "la\u00df", "ei\u00b7nes", "dich", "ge\u00b7mah\u00b7nen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "VVIMP", "ART", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nicht L\u00fcgnerin kann Mutter Erde sein.", "tokens": ["Nicht", "L\u00fcg\u00b7ne\u00b7rin", "kann", "Mut\u00b7ter", "Er\u00b7de", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "VMFIN", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Aus ihrem Scho\u00df ist auch dein Leib entsprungen,", "tokens": ["Aus", "ih\u00b7rem", "Scho\u00df", "ist", "auch", "dein", "Leib", "ent\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ihr hei\u00dfer Ku\u00df dr\u00fcckt' ein des Dichters Mal,", "tokens": ["Ihr", "hei\u00b7\u00dfer", "Ku\u00df", "dr\u00fcckt'", "ein", "des", "Dich\u00b7ters", "Mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ART", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wenn, ihrer wert, du einst dein Lied gesungen,", "tokens": ["Wenn", ",", "ih\u00b7rer", "wert"