{"textgrid.poem.65404": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Wassermann", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn das Wasser drau\u00dfen", "tokens": ["Wenn", "das", "Was\u00b7ser", "drau\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Von den Scheiben rinnt,", "tokens": ["Von", "den", "Schei\u00b7ben", "rinnt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Gie\u00df' mir Wein hier innen", "tokens": ["Gie\u00df'", "mir", "Wein", "hier", "in\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "NN", "ADV", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "In das Glas geschwind!", "tokens": ["In", "das", "Glas", "ge\u00b7schwind", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Ist das Wetter tr\u00fcbe,", "tokens": ["Ist", "das", "Wet\u00b7ter", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Hell ist doch der Wein,", "tokens": ["Hell", "ist", "doch", "der", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Hell des M\u00e4dchens Auge,", "tokens": ["Hell", "des", "M\u00e4d\u00b7chens", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Das ihn schenket ein.", "tokens": ["Das", "ihn", "schen\u00b7ket", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Herrschet denn am Himmel", "tokens": ["Herr\u00b7schet", "denn", "am", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Heut' der Wassermann?", "tokens": ["Heut'", "der", "Was\u00b7ser\u00b7mann", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Kellnerin, so lege", "tokens": ["Kell\u00b7ne\u00b7rin", ",", "so", "le\u00b7ge"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADV", "VVFIN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Gleich die Laden an.", "tokens": ["Gleich", "die", "La\u00b7den", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Gar zu griesegr\u00e4mlich", "tokens": ["Gar", "zu", "grie\u00b7se\u00b7gr\u00e4m\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PTKA", "ADJD"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Schauet er herein,", "tokens": ["Schau\u00b7et", "er", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "M\u00f6chte seinen Regen", "tokens": ["M\u00f6ch\u00b7te", "sei\u00b7nen", "Re\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Gie\u00dfen in den Wein.", "tokens": ["Gie\u00b7\u00dfen", "in", "den", "Wein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Sieh nur nach dem Kruge,", "tokens": ["Sieh", "nur", "nach", "dem", "Kru\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Sch\u00f6ne Kellnerin,", "tokens": ["Sch\u00f6\u00b7ne", "Kell\u00b7ne\u00b7rin", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df er nicht f\u00fcr deinen", "tokens": ["Da\u00df", "er", "nicht", "f\u00fcr", "dei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "APPR", "PPOSAT"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Seinen stelle hin!", "tokens": ["Sei\u00b7nen", "stel\u00b7le", "hin", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}