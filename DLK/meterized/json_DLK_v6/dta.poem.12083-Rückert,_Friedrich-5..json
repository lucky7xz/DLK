{"dta.poem.12083": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der letzte Stral von Gold um Berges Haupt zerrann,", "tokens": ["Der", "letz\u00b7te", "Stral", "von", "Gold", "um", "Ber\u00b7ges", "Haupt", "zer\u00b7rann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und von der Arbeit kehrt nachhaus der m\u00fcde Mann.", "tokens": ["Und", "von", "der", "Ar\u00b7beit", "kehrt", "nach\u00b7haus", "der", "m\u00fc\u00b7de", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Frau steht in der Th\u00fcr, reicht ihm das Kind entgegen;", "tokens": ["Die", "Frau", "steht", "in", "der", "Th\u00fcr", ",", "reicht", "ihm", "das", "Kind", "ent\u00b7ge\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das H\u00fcndlein l\u00e4uft herf\u00fcr und wedelt angelegen.", "tokens": ["Das", "H\u00fcnd\u00b7lein", "l\u00e4uft", "her\u00b7f\u00fcr", "und", "we\u00b7delt", "an\u00b7ge\u00b7le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "KON", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Verdrie\u00dfen l\u00e4\u00dft sichs nicht, da\u00df seine Liebkosungen", "tokens": ["Ver\u00b7drie\u00b7\u00dfen", "l\u00e4\u00dft", "sichs", "nicht", ",", "da\u00df", "sei\u00b7ne", "Lieb\u00b7ko\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "PIS", "PTKNEG", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Mann nicht weiter merkt, der liebkost seinem Jungen.", "tokens": ["Der", "Mann", "nicht", "wei\u00b7ter", "merkt", ",", "der", "lieb\u00b7kost", "sei\u00b7nem", "Jun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ADV", "VVFIN", "$,", "PRELS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Mit vollem Euter kommt die Geis; sein Kind zu nehmen,", "tokens": ["Mit", "vol\u00b7lem", "Eu\u00b7ter", "kommt", "die", "Geis", ";", "sein", "Kind", "zu", "neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$.", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil melken will die Frau, mu\u00df sich der Mann bequemen.", "tokens": ["Weil", "mel\u00b7ken", "will", "die", "Frau", ",", "mu\u00df", "sich", "der", "Mann", "be\u00b7que\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVINF", "VMFIN", "ART", "NN", "$,", "VMFIN", "PRF", "ART", "NN", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Die Milch am Feuer kocht, das Kindlein wird gewiegt,", "tokens": ["Die", "Milch", "am", "Feu\u00b7er", "kocht", ",", "das", "Kin\u00b7dlein", "wird", "ge\u00b7wiegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$,", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das stille Paar genie\u00dft, ihr Gl\u00fcck im Schlummer liegt.", "tokens": ["Das", "stil\u00b7le", "Paar", "ge\u00b7nie\u00dft", ",", "ihr", "Gl\u00fcck", "im", "Schlum\u00b7mer", "liegt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "O stilles Gl\u00fcck! daheim k\u00f6nnt' ich ein gleiches haben,", "tokens": ["O", "stil\u00b7les", "Gl\u00fcck", "!", "da\u00b7heim", "k\u00f6nnt'", "ich", "ein", "glei\u00b7ches", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "ADV", "VMFIN", "PPER", "ART", "ADJA", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und mu\u00df mich in der Fremd' am fremden Anblick laben.", "tokens": ["Und", "mu\u00df", "mich", "in", "der", "Fremd'", "am", "frem\u00b7den", "An\u00b7blick", "la\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "APPR", "ART", "NN", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}