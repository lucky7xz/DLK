{"textgrid.poem.54650": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "64. Gedancken \u00fcber meine Geburt", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der du mich an das Licht hast bey besternter Nacht/", "tokens": ["Der", "du", "mich", "an", "das", "Licht", "hast", "bey", "bes\u00b7tern\u00b7ter", "Nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "APPR", "ART", "NN", "VAFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Aus meiner Mutter Leib' als einem Kercker bracht;", "tokens": ["Aus", "mei\u00b7ner", "Mut\u00b7ter", "Leib'", "als", "ei\u00b7nem", "Ker\u00b7cker", "bracht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "La\u00df fern von Sternen mich/ doch Sternen gleich auffgehen/", "tokens": ["La\u00df", "fern", "von", "Ster\u00b7nen", "mich", "/", "doch", "Ster\u00b7nen", "gleich", "auff\u00b7ge\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJD", "APPR", "NN", "PPER", "$(", "ADV", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn hir ich untergeh/ dort \u00fcber Sternen stehen.", "tokens": ["Wenn", "hir", "ich", "un\u00b7ter\u00b7geh", "/", "dort", "\u00fc\u00b7ber", "Ster\u00b7nen", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$(", "ADV