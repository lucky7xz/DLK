{"dta.poem.7370": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Der Graue.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Es war um die Septemberzeit,", "tokens": ["Es", "war", "um", "die", "Sep\u00b7tem\u00b7ber\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Als, sch\u00fcrend des Kamines Brand,", "tokens": ["Als", ",", "sch\u00fc\u00b7rend", "des", "Ka\u00b7mi\u00b7nes", "Brand", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Geb\u00fcckt, in regenfeuchtem Kleid,", "tokens": ["Ge\u00b7b\u00fcckt", ",", "in", "re\u00b7gen\u00b7feuch\u00b7tem", "Kleid", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Hausherr in der Halle stand,", "tokens": ["Der", "Haus\u00b7herr", "in", "der", "Hal\u00b7le", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Er und die G\u00e4ste, All' im Rauch;", "tokens": ["Er", "und", "die", "G\u00e4s\u00b7te", ",", "All'", "im", "Rauch", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "ART", "NN", "$,", "PIS", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Van Neelen, Redel, Verney, Dahm,", "tokens": ["Van", "Nee\u00b7len", ",", "Re\u00b7del", ",", "Ver\u00b7ney", ",", "Dahm", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "$,", "NN", "$,", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und dann der blonde Waller auch,", "tokens": ["Und", "dann", "der", "blon\u00b7de", "Wal\u00b7ler", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der eben erst aus Smyrna kam.", "tokens": ["Der", "e\u00b7ben", "erst", "aus", "Smyr\u00b7na", "kam", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Im Schlote schnob der Wind, es go\u00df", "tokens": ["Im", "Schlo\u00b7te", "schnob", "der", "Wind", ",", "es", "go\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Regen sprudelnd sich vom Dach,", "tokens": ["Der", "Re\u00b7gen", "spru\u00b7delnd", "sich", "vom", "Dach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wenn am Brand ein Fl\u00e4mmchen scho\u00df,", "tokens": ["Und", "wenn", "am", "Brand", "ein", "Fl\u00e4mm\u00b7chen", "scho\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schien doppelt \u00f6de das Gemach.", "tokens": ["Schien", "dop\u00b7pelt", "\u00f6\u00b7de", "das", "Ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die G\u00e4ste waren all' zur Hand,", "tokens": ["Die", "G\u00e4s\u00b7te", "wa\u00b7ren", "all'", "zur", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIS", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Erleichternd ihres Wirthes M\u00fch';", "tokens": ["Er\u00b7leich\u00b7ternd", "ih\u00b7res", "Wirt\u00b7hes", "M\u00fch'", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Van Neelen nur am Fenster stand,", "tokens": ["Van", "Nee\u00b7len", "nur", "am", "Fens\u00b7ter", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und schimpfte auf die Landparthie.", "tokens": ["Und", "schimpf\u00b7te", "auf", "die", "Land\u00b7par\u00b7thie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch nach und nach mag's besser gehn,", "tokens": ["Doch", "nach", "und", "nach", "mag's", "bes\u00b7ser", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "KON", "APPR", "NE", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schon hat der Wind die Glut gefacht,", "tokens": ["Schon", "hat", "der", "Wind", "die", "Glut", "ge\u00b7facht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Regen l\u00e4\u00dft man drau\u00dfen stehn,", "tokens": ["Den", "Re\u00b7gen", "l\u00e4\u00dft", "man", "drau\u00b7\u00dfen", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Champagnerflaschen sind gebracht.", "tokens": ["Cham\u00b7pag\u00b7ner\u00b7fla\u00b7schen", "sind", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Leuchter hatten wenig Werth,", "tokens": ["Die", "Leuch\u00b7ter", "hat\u00b7ten", "we\u00b7nig", "Werth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es gieng wie beim Studentenfest:", "tokens": ["Es", "gieng", "wie", "beim", "Stu\u00b7den\u00b7ten\u00b7fest", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Sobald die Flasche ist geleert,", "tokens": ["So\u00b7bald", "die", "Fla\u00b7sche", "ist", "ge\u00b7leert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wird eine Kerze drauf gepre\u00dft.", "tokens": ["Wird", "ei\u00b7ne", "Ker\u00b7ze", "drauf", "ge\u00b7pre\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PAV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Je mehr es fehlt, so mehr man lacht,", "tokens": ["Je", "mehr", "es", "fehlt", ",", "so", "mehr", "man", "lacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "$,", "ADV", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Wein ist hei\u00df, die Kost gew\u00e4hlt,", "tokens": ["Der", "Wein", "ist", "hei\u00df", ",", "die", "Kost", "ge\u00b7w\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Manch' derbes Sp\u00e4\u00dfchen wird gemacht,", "tokens": ["Man\u00b7ch'", "der\u00b7bes", "Sp\u00e4\u00df\u00b7chen", "wird", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und mancher feine Streich erz\u00e4hlt.", "tokens": ["Und", "man\u00b7cher", "fei\u00b7ne", "Streich", "er\u00b7z\u00e4hlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zuletzt von Wein und Reden gl\u00fch,", "tokens": ["Zu\u00b7letzt", "von", "Wein", "und", "Re\u00b7den", "gl\u00fch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "R\u00fcckt seinen Stuhl der Herr vom Haus:", "tokens": ["R\u00fcckt", "sei\u00b7nen", "Stuhl", "der", "Herr", "vom", "Haus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "\u201eich lud Euch zu 'ner Landparthie,", "tokens": ["\u201e", "ich", "lud", "Euch", "zu", "'ner", "Land\u00b7par\u00b7thie", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Es ward 'ne Wasserfahrt daraus.\u201c", "tokens": ["Es", "ward", "'ne", "Was\u00b7ser\u00b7fahrt", "da\u00b7raus", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PAV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "\u201edoch da die allersch\u00f6nste Fracht", "tokens": ["\u201e", "doch", "da", "die", "al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "Fracht"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am Ende nach dem Hafen schifft,", "tokens": ["Am", "En\u00b7de", "nach", "dem", "Ha\u00b7fen", "schifft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So, meine Herren, gute Nacht!", "tokens": ["So", ",", "mei\u00b7ne", "Her\u00b7ren", ",", "gu\u00b7te", "Nacht", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "PPOSAT", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und nehmt vorlieb, wie es sich trifft.\u201c", "tokens": ["Und", "nehmt", "vor\u00b7lieb", ",", "wie", "es", "sich", "trifft", ".", "\u201c"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$,", "PWAV", "PPER", "PRF", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da lachend nach den Flaschen greift", "tokens": ["Da", "la\u00b7chend", "nach", "den", "Fla\u00b7schen", "greift"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Jeder. \u2014 Th\u00fcren auf und zu. \u2014", "tokens": ["Ein", "Je\u00b7der", ".", "Th\u00fc\u00b7ren", "auf", "und", "zu", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PIS", "$.", "$(", "NN", "PTKVZ", "KON", "PTKVZ", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Und Waller, noch im Gehen, streift", "tokens": ["Und", "Wal\u00b7ler", ",", "noch", "im", "Ge\u00b7hen", ",", "streift"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "NN", "$,", "ADV", "APPRART", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Aus seinem Frack den Ivanhoe.", "tokens": ["Aus", "sei\u00b7nem", "Frack", "den", "I\u00b7van\u00b7hoe", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}