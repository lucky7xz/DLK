{"textgrid.poem.47994": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Weihe-Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein leises Rauschen durch die Tannenzweige \u2013", "tokens": ["Ein", "lei\u00b7ses", "Rau\u00b7schen", "durch", "die", "Tan\u00b7nen\u00b7zwei\u00b7ge", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "des kurzen Tages Zwielicht geht zur Neige.", "tokens": ["des", "kur\u00b7zen", "Ta\u00b7ges", "Zwie\u00b7licht", "geht", "zur", "Nei\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Im Westen glimmt ein matter Rosenstreif,", "tokens": ["Im", "Wes\u00b7ten", "glimmt", "ein", "mat\u00b7ter", "Ro\u00b7sen\u00b7streif", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "auf stille Fluren f\u00e4llt der wei\u00dfe Reif.", "tokens": ["auf", "stil\u00b7le", "Flu\u00b7ren", "f\u00e4llt", "der", "wei\u00b7\u00dfe", "Reif", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der wei\u00dfe Reif, der rings das Feierkleid", "tokens": ["Der", "wei\u00b7\u00dfe", "Reif", ",", "der", "rings", "das", "Fei\u00b7er\u00b7kleid"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "der Erde stickt mit flimmerndem Geschmeid.", "tokens": ["der", "Er\u00b7de", "stickt", "mit", "flim\u00b7mern\u00b7dem", "Ge\u00b7schmeid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der Abend kommt. Es kommt die heilige Nacht,", "tokens": ["Der", "A\u00b7bend", "kommt", ".", "Es", "kommt", "die", "hei\u00b7li\u00b7ge", "Nacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "die aus den Menschen selige Kinder macht,", "tokens": ["die", "aus", "den", "Men\u00b7schen", "se\u00b7li\u00b7ge", "Kin\u00b7der", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "die Weihe-Nacht, da trost- und wundersam", "tokens": ["die", "Wei\u00b7he\u00b7Nacht", ",", "da", "trost", "und", "wun\u00b7der\u00b7sam"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "TRUNC", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "ein M\u00e4rchentraum zur dunklen Erde kam:", "tokens": ["ein", "M\u00e4r\u00b7chen\u00b7traum", "zur", "dunk\u00b7len", "Er\u00b7de", "kam", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Der Friedensk\u00f6nig, den die Welt verstie\u00df,", "tokens": ["Der", "Frie\u00b7dens\u00b7k\u00f6\u00b7nig", ",", "den", "die", "Welt", "ver\u00b7stie\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "weil er die Armen Gottes Kinder hie\u00df.", "tokens": ["weil", "er", "die", "Ar\u00b7men", "Got\u00b7tes", "Kin\u00b7der", "hie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Weil er den Sanften, der den Frieden liebt,", "tokens": ["Weil", "er", "den", "Sanf\u00b7ten", ",", "der", "den", "Frie\u00b7den", "liebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "den Liebenden, der seine Seele gibt,", "tokens": ["den", "Lie\u00b7ben\u00b7den", ",", "der", "sei\u00b7ne", "See\u00b7le", "gibt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "weit \u00fcber alle Reichen dieser Welt,", "tokens": ["weit", "\u00fc\u00b7ber", "al\u00b7le", "Rei\u00b7chen", "die\u00b7ser", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PIAT", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "hoch \u00fcber alle Herrschenden gestellt.", "tokens": ["hoch", "\u00fc\u00b7ber", "al\u00b7le", "Herr\u00b7schen\u00b7den", "ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Du Weiser, seit die Engelharfen klangen,", "tokens": ["Du", "Wei\u00b7ser", ",", "seit", "die", "En\u00b7gel\u00b7har\u00b7fen", "klan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "sind nun Jahrtausende dahingegangen,", "tokens": ["sind", "nun", "Jahr\u00b7tau\u00b7sen\u00b7de", "da\u00b7hin\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "PAV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "die deinen Namen auf den Fahnen trugen", "tokens": ["die", "dei\u00b7nen", "Na\u00b7men", "auf", "den", "Fah\u00b7nen", "tru\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und zu den fernsten L\u00e4ndern Br\u00fccken schlugen,", "tokens": ["und", "zu", "den", "ferns\u00b7ten", "L\u00e4n\u00b7dern", "Br\u00fc\u00b7cken", "schlu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Millionen Kirchen prangen dir zum Ruhme,", "tokens": ["Mil\u00b7lion\u00b7en", "Kir\u00b7chen", "pran\u00b7gen", "dir", "zum", "Ruh\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "die ewige Flamme brennt im Heiligtume . . . .", "tokens": ["die", "e\u00b7wi\u00b7ge", "Flam\u00b7me", "brennt", "im", "Hei\u00b7lig\u00b7tu\u00b7me", ".", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$.", "$.", "$.", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.12": {"line.1": {"text": "Und dennoch, du, der Sklaven Heil gespendet,", "tokens": ["Und", "den\u00b7noch", ",", "du", ",", "der", "Skla\u00b7ven", "Heil", "ge\u00b7spen\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PPER", "$,", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "du w\u00e4rst noch heut in tiefe Nacht gesendet,", "tokens": ["du", "w\u00e4rst", "noch", "heut", "in", "tie\u00b7fe", "Nacht", "ge\u00b7sen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "du schienst auch heut in unser finstres Tal", "tokens": ["du", "schienst", "auch", "heut", "in", "un\u00b7ser", "finst\u00b7res", "Tal"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "aus fernen Himmeln, ein verirrter Strahl;", "tokens": ["aus", "fer\u00b7nen", "Him\u00b7meln", ",", "ein", "ver\u00b7irr\u00b7ter", "Strahl", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.14": {"line.1": {"text": "und gingest du im schlichten Arbeitskleid", "tokens": ["und", "gin\u00b7gest", "du", "im", "schlich\u00b7ten", "Ar\u00b7beits\u00b7kleid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "durch deine Menschheit, deine Christenheit,", "tokens": ["durch", "dei\u00b7ne", "Menschheit", ",", "dei\u00b7ne", "Chris\u00b7ten\u00b7heit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.15": {"line.1": {"text": "sie h\u00e4tten heute dir das Kreuz errichtet", "tokens": ["sie", "h\u00e4t\u00b7ten", "heu\u00b7te", "dir", "das", "Kreuz", "er\u00b7rich\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PPER", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und morgen dir den Holzsto\u00df aufgeschichtet!", "tokens": ["und", "mor\u00b7gen", "dir", "den", "Holz\u00b7sto\u00df", "auf\u00b7ge\u00b7schich\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.16": {"line.1": {"text": "Hoch auf dem Grunde, den dein Blick gesucht,", "tokens": ["Hoch", "auf", "dem", "Grun\u00b7de", ",", "den", "dein", "Blick", "ge\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "dar\u00fcber hin rast laut der Zeiten Flucht,", "tokens": ["da\u00b7r\u00fc\u00b7ber", "hin", "rast", "laut", "der", "Zei\u00b7ten", "Flucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APZR", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.17": {"line.1": {"text": "da regt sich's dumpf, und aus der Erde Scho\u00df", "tokens": ["da", "regt", "sich's", "dumpf", ",", "und", "aus", "der", "Er\u00b7de", "Scho\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ADJD", "$,", "KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "ringt sich der Urquell aller Sehnsucht los.", "tokens": ["ringt", "sich", "der", "Ur\u00b7quell", "al\u00b7ler", "Sehn\u00b7sucht", "los", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "PIAT", "NN", "PTKVZ", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.18": {"line.1": {"text": "Die Welt durchhallt ein Schrei nach Luft und Licht:", "tokens": ["Die", "Welt", "durch\u00b7hallt", "ein", "Schrei", "nach", "Luft", "und", "Licht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wann braust du, Strom, der Wall und Schranke bricht?", "tokens": ["Wann", "braust", "du", ",", "Strom", ",", "der", "Wall", "und", "Schran\u00b7ke", "bricht", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "NN", "$,", "ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.19": {"line.1": {"text": "Wann kommst du, Tag, da hell die Sonne steigt,", "tokens": ["Wann", "kommst", "du", ",", "Tag", ",", "da", "hell", "die", "Son\u00b7ne", "steigt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "NN", "$,", "KOUS", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "vor deren Glanz der tiefste Schatten weicht?", "tokens": ["vor", "de\u00b7ren", "Glanz", "der", "tiefs\u00b7te", "Schat\u00b7ten", "weicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "---+-+-+-+", "measure": "zehnsilber"}}, "stanza.20": {"line.1": {"text": "Ich sah dein dunkles Angesicht", "tokens": ["Ich", "sah", "dein", "dunk\u00b7les", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "ergl\u00fchn in einem Strom von Licht. \u2013", "tokens": ["er\u00b7gl\u00fchn", "in", "ei\u00b7nem", "Strom", "von", "Licht", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich sah dein Aug, das sonst so tr\u00fcbe,", "tokens": ["Ich", "sah", "dein", "Aug", ",", "das", "sonst", "so", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PRELS", "ADV", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "verkl\u00e4rt von einem Strahl der Liebe.", "tokens": ["ver\u00b7kl\u00e4rt", "von", "ei\u00b7nem", "Strahl", "der", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.21": {"line.1": {"text": "Da ward mir traumhaft wunderbar", "tokens": ["Da", "ward", "mir", "traum\u00b7haft", "wun\u00b7der\u00b7bar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "zumut; in tiefster Seele war", "tokens": ["zu\u00b7mut", ";", "in", "tiefs\u00b7ter", "See\u00b7le", "war"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "APPR", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "mir's fast, als k\u00f6nnt der lichte Schein", "tokens": ["mir's", "fast", ",", "als", "k\u00f6nnt", "der", "lich\u00b7te", "Schein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "$,", "KOUS", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "ein Abglanz meiner Liebe sein.", "tokens": ["ein", "Ab\u00b7glanz", "mei\u00b7ner", "Lie\u00b7be", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}