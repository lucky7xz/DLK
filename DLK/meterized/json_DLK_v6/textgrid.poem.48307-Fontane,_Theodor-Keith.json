{"textgrid.poem.48307": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Keith", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.85", "da:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da, wo der Strom der Schotten,", "tokens": ["Da", ",", "wo", "der", "Strom", "der", "Schot\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Tay vom Felsen springt,", "tokens": ["Der", "Tay", "vom", "Fel\u00b7sen", "springt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo's noch in Schlucht und Grotten", "tokens": ["Wo's", "noch", "in", "Schlucht", "und", "Grot\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von Bruce und Wallace klingt,", "tokens": ["Von", "Bru\u00b7ce", "und", "Wal\u00b7la\u00b7ce", "klingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Am Tay, wo blut'ge Siege", "tokens": ["Am", "Tay", ",", "wo", "blut'\u00b7ge", "Sie\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PWAV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Jedweden Fleck geweiht,", "tokens": ["Jed\u00b7we\u00b7den", "Fleck", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Dort stand auch deine Wiege,", "tokens": ["Dort", "stand", "auch", "dei\u00b7ne", "Wie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Feldmarschall ", "tokens": ["Feld\u00b7mar\u00b7schall"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Es sang die Hochlandsamme", "tokens": ["Es", "sang", "die", "Hoch\u00b7land\u00b7sam\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit Schlachten dich in Ruh',", "tokens": ["Mit", "Schlach\u00b7ten", "dich", "in", "Ruh'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Aus ihrem Clan und Stamme", "tokens": ["Aus", "ih\u00b7rem", "Clan", "und", "Stam\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Pries sie die Helden dazu;", "tokens": ["Pries", "sie", "die", "Hel\u00b7den", "da\u00b7zu", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PAV", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Drum, ehe der Bart am Kinne", "tokens": ["Drum", ",", "e\u00b7he", "der", "Bart", "am", "Kin\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "$,", "KOUS", "ART", "NN", "APPRART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Dir spro\u00dfte noch hervor,", "tokens": ["Dir", "spro\u00df\u00b7te", "noch", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Standst du, voll Mannessinne,", "tokens": ["Standst", "du", ",", "voll", "Man\u00b7nes\u00b7sin\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Schon mit bei Sherifmoor.", "tokens": ["Schon", "mit", "bei", "She\u00b7rif\u00b7mo\u00b7or", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "APPR", "NE", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Du standest bei den Schwachen,", "tokens": ["Du", "stan\u00b7dest", "bei", "den", "Schwa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Stuarts mu\u00dften fliehn,", "tokens": ["Die", "Stu\u00b7arts", "mu\u00df\u00b7ten", "fliehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es trug auch dich ein Nachen", "tokens": ["Es", "trug", "auch", "dich", "ein", "Na\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gen Frankreichs K\u00fcste hin;", "tokens": ["Gen", "Fran\u00b7kreichs", "K\u00fcs\u00b7te", "hin", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein Kunst- und Wanderleben", "tokens": ["Ein", "Kunst", "und", "Wan\u00b7der\u00b7le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "TRUNC", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hob an, von Land zu Land:", "tokens": ["Hob", "an", ",", "von", "Land", "zu", "Land", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Den Degen in der Hand.", "tokens": ["Den", "De\u00b7gen", "in", "der", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Du spieltest alle Rollen,", "tokens": ["Du", "spiel\u00b7test", "al\u00b7le", "Rol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den H\u00f6fling selbst, mit Gl\u00fcck,", "tokens": ["Den", "H\u00f6f\u00b7ling", "selbst", ",", "mit", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch sch\u00f6pfen aus dem vollen", "tokens": ["Doch", "sch\u00f6p\u00b7fen", "aus", "dem", "vol\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Lie\u00df dich das Ritterst\u00fcck;", "tokens": ["Lie\u00df", "dich", "das", "Rit\u00b7ter\u00b7st\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das war dein Fach, das K\u00fchne,", "tokens": ["Das", "war", "dein", "Fach", ",", "das", "K\u00fch\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Mut bis in den Tod,", "tokens": ["Der", "Mut", "bis", "in", "den", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und mancher schlechten B\u00fchne", "tokens": ["Und", "man\u00b7cher", "schlech\u00b7ten", "B\u00fch\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Halfst du aus arger Not. \u2013", "tokens": ["Halfst", "du", "aus", "ar\u00b7ger", "Not", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Es gab nur eine Truppe", "tokens": ["Es", "gab", "nur", "ei\u00b7ne", "Trup\u00b7pe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Damals von gutem Ruf,", "tokens": ["Da\u00b7mals", "von", "gu\u00b7tem", "Ruf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Das war die gl\u00e4nzende Gruppe,", "tokens": ["Das", "war", "die", "gl\u00e4n\u00b7zen\u00b7de", "Grup\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Es suchte sein Theater", "tokens": ["Es", "such\u00b7te", "sein", "The\u00b7a\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Talente weit und breit,", "tokens": ["Ta\u00b7len\u00b7te", "weit", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und siehe, gewinnen tat er", "tokens": ["Und", "sie\u00b7he", ",", "ge\u00b7win\u00b7nen", "tat", "er"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVIMP", "$,", "VVINF", "VVFIN", "PPER"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Auch dich auf Lebenszeit.", "tokens": ["Auch", "dich", "auf", "Le\u00b7bens\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Nur immer Musterdramen", "tokens": ["Nur", "im\u00b7mer", "Mus\u00b7ter\u00b7dra\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gab's da, mal hier, mal dort:", "tokens": ["Gab's", "da", ",", "mal", "hier", ",", "mal", "dort", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "ADV", "ADV", "$,", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vor lauter Handlung kamen", "tokens": ["Vor", "lau\u00b7ter", "Hand\u00b7lung", "ka\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Spieler kaum zu Wort;", "tokens": ["Die", "Spie\u00b7ler", "kaum", "zu", "Wort", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Abwechselnd zu Fu\u00df und zu Rosse", "tokens": ["Ab\u00b7wech\u00b7selnd", "zu", "Fu\u00df", "und", "zu", "Ros\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "APPR", "NN", "KON", "APPR", "NN"], "meter": "++--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Gab's Lust- und Trauerspiel,", "tokens": ["Gab's", "Lust", "und", "Trau\u00b7er\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Bei Ro\u00dfbach, jene Posse", "tokens": ["Bei", "Ro\u00df\u00b7bach", ",", "je\u00b7ne", "Pos\u00b7se"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NE", "$,", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vor allen wohlgefiel.", "tokens": ["Vor", "al\u00b7len", "wohl\u00b7ge\u00b7fiel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Da kam, voll Tod und Wetter,", "tokens": ["Da", "kam", ",", "voll", "Tod", "und", "Wet\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von Hochkirch jene Nacht,", "tokens": ["Von", "Hoch\u00b7kirch", "je\u00b7ne", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du mu\u00dftest auf die Bretter,", "tokens": ["Du", "mu\u00df\u00b7test", "auf", "die", "Bret\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O ", "tokens": ["O"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Das gab kein sichres Spielen,", "tokens": ["Das", "gab", "kein", "sich\u00b7res", "Spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nur Wirrwarr und Geschrei,", "tokens": ["Nur", "Wirr\u00b7warr", "und", "Ge\u00b7schrei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und wenn Stichworte fielen,", "tokens": ["Und", "wenn", "Stich\u00b7wor\u00b7te", "fie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NN", "VVFIN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "War's vollends erst vorbei.", "tokens": ["Wa\u00b7r's", "vol\u00b7lends", "erst", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Der Vorhang sollte fallen,", "tokens": ["Der", "Vor\u00b7hang", "soll\u00b7te", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du aber, rings bedroht,", "tokens": ["Du", "a\u00b7ber", ",", "rings", "be\u00b7droht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Riefst: \u00bbBestes St\u00fcck von allen", "tokens": ["Riefst", ":", "\u00bb", "Bes\u00b7tes", "St\u00fcck", "von", "al\u00b7len"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "$(", "ADJA", "NN", "APPR", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bleibt ehrenvoller Tod!\u00ab", "tokens": ["Bleibt", "eh\u00b7ren\u00b7vol\u00b7ler", "Tod", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und so, im Kugelregen,", "tokens": ["Und", "so", ",", "im", "Ku\u00b7gel\u00b7re\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Tratst du vom Schauplatz ab \u2013", "tokens": ["Tratst", "du", "vom", "Schau\u00b7platz", "ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "La\u00df auf dein Grab mich legen", "tokens": ["La\u00df", "auf", "dein", "Grab", "mich", "le\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "APPR", "PPOSAT", "NN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dies Lied zum Feldherrnstab.", "tokens": ["Dies", "Lied", "zum", "Feld\u00b7herrns\u00b7tab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}