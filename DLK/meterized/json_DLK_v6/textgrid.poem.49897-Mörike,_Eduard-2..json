{"textgrid.poem.49897": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Winterboden schl\u00e4ft, ein Blumenkeim,", "tokens": ["Im", "Win\u00b7ter\u00b7bo\u00b7den", "schl\u00e4ft", ",", "ein", "Blu\u00b7men\u00b7keim", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Schmetterling, der einst um Busch und H\u00fcgel", "tokens": ["Der", "Schmet\u00b7ter\u00b7ling", ",", "der", "einst", "um", "Busch", "und", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In Fr\u00fchlingsn\u00e4chten wiegt den samtnen Fl\u00fcgel;", "tokens": ["In", "Fr\u00fch\u00b7lings\u00b7n\u00e4ch\u00b7ten", "wiegt", "den", "samt\u00b7nen", "Fl\u00fc\u00b7gel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nie soll er kosten deinen Honigseim.", "tokens": ["Nie", "soll", "er", "kos\u00b7ten", "dei\u00b7nen", "Ho\u00b7ni\u00b7gseim", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wer aber wei\u00df, ob nicht sein zarter Geist,", "tokens": ["Wer", "a\u00b7ber", "wei\u00df", ",", "ob", "nicht", "sein", "zar\u00b7ter", "Geist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$,", "KOUS", "PTKNEG", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wenn jede Zier des Sommers hingesunken,", "tokens": ["Wenn", "je\u00b7de", "Zier", "des", "Som\u00b7mers", "hin\u00b7ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dereinst, von deinem leisen Dufte trunken,", "tokens": ["De\u00b7reinst", ",", "von", "dei\u00b7nem", "lei\u00b7sen", "Duf\u00b7te", "trun\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "PPOSAT", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mir unsichtbar, dich Bl\u00fchende umkreist?", "tokens": ["Mir", "un\u00b7sicht\u00b7bar", ",", "dich", "Bl\u00fc\u00b7hen\u00b7de", "um\u00b7kreist", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$,", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}