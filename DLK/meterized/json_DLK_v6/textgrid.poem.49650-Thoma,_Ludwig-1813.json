{"textgrid.poem.49650": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1813", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun wei\u00df es doch das Volk der Denker!", "tokens": ["Nun", "wei\u00df", "es", "doch", "das", "Volk", "der", "Den\u00b7ker", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der liebe Gott als Schlachtenlenker", "tokens": ["Der", "lie\u00b7be", "Gott", "als", "Schlach\u00b7ten\u00b7len\u00b7ker"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KOUS", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Verlor uns Jena. Sp\u00e4ter dann", "tokens": ["Ver\u00b7lor", "uns", "Je\u00b7na", ".", "Sp\u00e4\u00b7ter", "dann"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "NE", "$.", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "War Er's, der Leipzig uns gewann.", "tokens": ["War", "Er's", ",", "der", "Leip\u00b7zig", "uns", "ge\u00b7wann", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "$,", "ART", "NE", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er will von allen Weltbezirken", "tokens": ["Er", "will", "von", "al\u00b7len", "Welt\u00b7be\u00b7zir\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Besonders stark in Preu\u00dfen wirken.", "tokens": ["Be\u00b7son\u00b7ders", "stark", "in", "Preu\u00b7\u00dfen", "wir\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sein ganzes Trachten lenkt ihn hin", "tokens": ["Sein", "gan\u00b7zes", "Trach\u00b7ten", "lenkt", "ihn", "hin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Weltennabel, nach Berlin.", "tokens": ["Zum", "Wel\u00b7ten\u00b7na\u00b7bel", ",", "nach", "Ber\u00b7lin", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nach Friedrich, der so fromm gewesen,", "tokens": ["Nach", "Fried\u00b7rich", ",", "der", "so", "fromm", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "PRELS", "ADV", "ADJD", "VAPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie wir in allen B\u00fcchern lesen,", "tokens": ["Wie", "wir", "in", "al\u00b7len", "B\u00fc\u00b7chern", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zog eine freiere Idee", "tokens": ["Zog", "ei\u00b7ne", "frei\u00b7e\u00b7re", "I\u00b7dee"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sich an und um den Strand der Spree.", "tokens": ["Sich", "an", "und", "um", "den", "Strand", "der", "Spree", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKVZ", "KON", "APPR", "ART", "NN", "ART", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da half nun Gott den Ohnehosen.", "tokens": ["Da", "half", "nun", "Gott", "den", "Oh\u00b7ne\u00b7ho\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ja \u2013 ausgerechnet den Franzosen!", "tokens": ["Ja", "\u2013", "aus\u00b7ge\u00b7rech\u00b7net", "den", "Fran\u00b7zo\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie hatten ihn zwar abgesetzt,", "tokens": ["Sie", "hat\u00b7ten", "ihn", "zwar", "ab\u00b7ge\u00b7setzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch schien er nicht dadurch verletzt.", "tokens": ["Doch", "schien", "er", "nicht", "da\u00b7durch", "ver\u00b7letzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "PAV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "So einfach ist es zu erkl\u00e4ren!", "tokens": ["So", "ein\u00b7fach", "ist", "es", "zu", "er\u00b7kl\u00e4\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es thront in seinen lichten Sph\u00e4ren", "tokens": ["Es", "thront", "in", "sei\u00b7nen", "lich\u00b7ten", "Sph\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Weltensch\u00f6pfer und gibt acht,", "tokens": ["Der", "Wel\u00b7ten\u00b7sch\u00f6p\u00b7fer", "und", "gibt", "acht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVFIN", "CARD", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Was man spezjell in Preu\u00dfen macht!", "tokens": ["Was", "man", "spez\u00b7jell", "in", "Preu\u00b7\u00dfen", "macht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADJD", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}