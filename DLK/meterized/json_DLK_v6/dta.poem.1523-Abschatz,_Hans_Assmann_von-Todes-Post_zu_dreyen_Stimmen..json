{"dta.poem.1523": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Todes-Post  \n zu dreyen Stimmen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "1.\nBeschicke dein Hau\u00df/ die Boten erscheinen/ ", "tokens": ["Be\u00b7schi\u00b7cke", "dein", "Hau\u00df", "/", "die", "Bo\u00b7ten", "er\u00b7schei\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$(", "ART", "NN", "VVINF", "$("], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Welche dir nach und nach k\u00fcndigen an/", "tokens": ["Wel\u00b7che", "dir", "nach", "und", "nach", "k\u00fcn\u00b7di\u00b7gen", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "KON", "APPR", "VVFIN", "PTKVZ", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Da\u00df dein Abscheiden nicht ferne seyn kan:", "tokens": ["Da\u00df", "dein", "Ab\u00b7schei\u00b7den", "nicht", "fer\u00b7ne", "seyn", "kan", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PTKNEG", "ADV", "VAINF", "VMFIN", "$."], "meter": "+-++--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Bestelle dein Grab/ versorge die Deinen.", "tokens": ["Be\u00b7stel\u00b7le", "dein", "Grab", "/", "ver\u00b7sor\u00b7ge", "die", "Dei\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$(", "VVFIN", "ART", "PPOSAT", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "2.\nBeschicke dein Hau\u00df! die bleichenden Haare/ ", "tokens": ["Be\u00b7schi\u00b7cke", "dein", "Hau\u00df", "!", "die", "blei\u00b7chen\u00b7den", "Haa\u00b7re", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$.", "ART", "ADJA", "NN", "$("], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die streichenden Jahre sind Leiter zur Bahre/", "tokens": ["Die", "strei\u00b7chen\u00b7den", "Jah\u00b7re", "sind", "Lei\u00b7ter", "zur", "Bah\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "NN", "APPRART", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Schwindel im Haubte/ Beklemmung und Schmertzen", "tokens": ["Schwin\u00b7del", "im", "Haub\u00b7te", "/", "Be\u00b7klem\u00b7mung", "und", "Schmert\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "$(", "NN", "KON", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Drohen mit t\u00f6dlichen St\u00f6ssen dem Hertzen.", "tokens": ["Dro\u00b7hen", "mit", "t\u00f6d\u00b7li\u00b7chen", "St\u00f6s\u00b7sen", "dem", "Hert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "3.\nBestelle dein Hau\u00df/ berathe die Seele! ", "tokens": ["Be\u00b7stel\u00b7le", "dein", "Hau\u00df", "/", "be\u00b7ra\u00b7the", "die", "See\u00b7le", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$(", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Stunden entf\u00fchren/ verlieren die Zeit:", "tokens": ["Stun\u00b7den", "ent\u00b7f\u00fch\u00b7ren", "/", "ver\u00b7lie\u00b7ren", "die", "Zeit", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$(", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Wer sich zur Reise nicht wei\u00dflich bereit/", "tokens": ["Wer", "sich", "zur", "Rei\u00b7se", "nicht", "wei\u00df\u00b7lich", "be\u00b7reit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPRART", "NN", "PTKNEG", "ADJD", "ADJD", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Geht irre/ verf\u00e4llt zur h\u00f6llischen H\u00f6le.", "tokens": ["Geht", "ir\u00b7re", "/", "ver\u00b7f\u00e4llt", "zur", "h\u00f6l\u00b7li\u00b7schen", "H\u00f6\u00b7le", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$(", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "4.\nBethr\u00e4ne mit Sehnen was Ubels ver\u00fcbet/ ", "tokens": ["Be\u00b7th\u00b7r\u00e4\u00b7ne", "mit", "Seh\u00b7nen", "was", "U\u00b7bels", "ver\u00b7\u00fc\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PWS", "NN", "VVFIN", "$("], "meter": "-++--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Fall deme zu F\u00fcssen der S\u00fcnde vergiebet!", "tokens": ["Fall", "de\u00b7me", "zu", "F\u00fcs\u00b7sen", "der", "S\u00fcn\u00b7de", "ver\u00b7gie\u00b7bet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "APPR", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Eyfriges Ruffen und gl\u00e4ubiges Hoffen", "tokens": ["Ey\u00b7fri\u00b7ges", "Ruf\u00b7fen", "und", "gl\u00e4u\u00b7bi\u00b7ges", "Hof\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "H\u00e4lt dir die Pforte der Himmels-Burg offen.", "tokens": ["H\u00e4lt", "dir", "die", "Pfor\u00b7te", "der", "Him\u00b7mels\u00b7Burg", "of\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "1. ", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Traurige Stimme/ bek\u00fcmmerte Post/ ", "tokens": ["Trau\u00b7ri\u00b7ge", "Stim\u00b7me", "/", "be\u00b7k\u00fcm\u00b7mer\u00b7te", "Post", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "ADJA", "NN", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Die zagende Seele vom Leibe zu scheiden/", "tokens": ["Die", "za\u00b7gen\u00b7de", "See\u00b7le", "vom", "Lei\u00b7be", "zu", "schei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Behagende Freuden und Freunde vermeiden/", "tokens": ["Be\u00b7ha\u00b7gen\u00b7de", "Freu\u00b7den", "und", "Freun\u00b7de", "ver\u00b7mei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "W\u00fcrmern im Grabe gedeyhen zur Kost!", "tokens": ["W\u00fcr\u00b7mern", "im", "Gra\u00b7be", "ge\u00b7dey\u00b7hen", "zur", "Kost", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVPP", "APPRART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.6": {"line.1": {"text": "2. ", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Sollen die Glieder so zeitlich erkalten?", "tokens": ["Sol\u00b7len", "die", "Glie\u00b7der", "so", "zeit\u00b7lich", "er\u00b7kal\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADV", "ADJD", "VVINF", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Zeigt sich kein Mittel die Seele zu halten?", "tokens": ["Zeigt", "sich", "kein", "Mit\u00b7tel", "die", "See\u00b7le", "zu", "hal\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PIAT", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Wissen denn keine ge\u00fcbte Chymisten/", "tokens": ["Wis\u00b7sen", "denn", "kei\u00b7ne", "ge\u00b7\u00fcb\u00b7te", "Chy\u00b7mis\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIAT", "ADJA", "NN", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.5": {"text": "L\u00e4nger den fliehenden Athem zu fristen.", "tokens": ["L\u00e4n\u00b7ger", "den", "flie\u00b7hen\u00b7den", "A\u00b7them", "zu", "fris\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.7": {"line.1": {"text": "3. ", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Begeben das Leben/ verlassen das Gutt/", "tokens": ["Be\u00b7ge\u00b7ben", "das", "Le\u00b7ben", "/", "ver\u00b7las\u00b7sen", "das", "Gutt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$(", "VVFIN", "ART", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Aechzen und lechzen nach Labsal und Trost/", "tokens": ["A\u00b7ech\u00b7zen", "und", "lech\u00b7zen", "nach", "Lab\u00b7sal", "und", "Trost", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "APPR", "NN", "KON", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Wenn sich die Wellen der H\u00f6llen erbost/", "tokens": ["Wenn", "sich", "die", "Wel\u00b7len", "der", "H\u00f6l\u00b7len", "er\u00b7bost", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Verblassen/ erstarren betr\u00fcbet den Mutt.", "tokens": ["Ver\u00b7blas\u00b7sen", "/", "er\u00b7star\u00b7ren", "be\u00b7tr\u00fc\u00b7bet", "den", "Mutt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "VVINF", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.8": {"line.1": {"text": "4. ", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Man dencket bekr\u00e4ncket was Jugend begangen/", "tokens": ["Man", "den\u00b7cket", "be\u00b7kr\u00e4n\u00b7cket", "was", "Ju\u00b7gend", "be\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "VVFIN", "PIS", "NN", "VVPP", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Wie reiffere Jahre mit Schulden umfangen!", "tokens": ["Wie", "reif\u00b7fe\u00b7re", "Jah\u00b7re", "mit", "Schul\u00b7den", "um\u00b7fan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Find sich kein Retter/ kein B\u00fcrge zu zahlen/", "tokens": ["Find", "sich", "kein", "Ret\u00b7ter", "/", "kein", "B\u00fcr\u00b7ge", "zu", "zah\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PIAT", "NN", "$(", "PIAT", "NN", "PTKZU", "VVINF", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.5": {"text": "Bleibet nichts \u00fcbrig als Kercker und Qualen.", "tokens": ["Blei\u00b7bet", "nichts", "\u00fcb\u00b7rig", "als", "Ker\u00b7cker", "und", "Qua\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "KOKOM", "NN", "KON", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.9": {"line.1": {"text": "1. ", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Fr\u00f6liche Zeitung/ erfreuende Post/ ", "tokens": ["Fr\u00f6\u00b7li\u00b7che", "Zei\u00b7tung", "/", "er\u00b7freu\u00b7en\u00b7de", "Post", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "ADJA", "NN", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Von Leyden und Hertze-durch schneiden sich scheiden/", "tokens": ["Von", "Ley\u00b7den", "und", "Hert\u00b7ze\u00b7durch", "schnei\u00b7den", "sich", "schei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PRF", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Beschwerden der Erden in Ruhe vermeiden/", "tokens": ["Be\u00b7schwer\u00b7den", "der", "Er\u00b7den", "in", "Ru\u00b7he", "ver\u00b7mei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "NN", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Ewig geniessen die Englische Kost!", "tokens": ["E\u00b7wig", "ge\u00b7nies\u00b7sen", "die", "Eng\u00b7li\u00b7sche", "Kost", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.10": {"line.1": {"text": "2. ", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Lasset die siechenden Glieder erkalten/", "tokens": ["Las\u00b7set", "die", "sie\u00b7chen\u00b7den", "Glie\u00b7der", "er\u00b7kal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Wei\u00df sich die Seele nur sicher zu halten!", "tokens": ["Wei\u00df", "sich", "die", "See\u00b7le", "nur", "si\u00b7cher", "zu", "hal\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Wenn Erd und Himmel zerschmeltzen in Flammen/", "tokens": ["Wenn", "Erd", "und", "Him\u00b7mel", "zer\u00b7schmelt\u00b7zen", "in", "Flam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Kommen sie beyderseits wieder zusammen.", "tokens": ["Kom\u00b7men", "sie", "bey\u00b7der\u00b7seits", "wie\u00b7der", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "PTKVZ", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.11": {"line.1": {"text": "3. ", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Wohl sterben giebt Erben zum himmlischen Gutt/", "tokens": ["Wohl", "ster\u00b7ben", "giebt", "Er\u00b7ben", "zum", "himm\u00b7li\u00b7schen", "Gutt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VVFIN", "NN", "APPRART", "ADJA", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Unser Erl\u00f6ser geht selber voran/", "tokens": ["Un\u00b7ser", "Er\u00b7l\u00f6\u00b7ser", "geht", "sel\u00b7ber", "vo\u00b7ran", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "PTKVZ", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Machet uns eben die stachlichte Bahn:", "tokens": ["Ma\u00b7chet", "uns", "e\u00b7ben", "die", "stach\u00b7lich\u00b7te", "Bahn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Drum folget demselben mit freudigem Mutt.", "tokens": ["Drum", "fol\u00b7get", "dem\u00b7sel\u00b7ben", "mit", "freu\u00b7di\u00b7gem", "Mutt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PDAT", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.12": {"line.1": {"text": "4. ", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Wie will uns ver\u00fcbete Missethat dr\u00fccken?", "tokens": ["Wie", "will", "uns", "ver\u00b7\u00fc\u00b7be\u00b7te", "Mis\u00b7se\u00b7that", "dr\u00fc\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Sie lieget auff JEsu geduldigem R\u00fccken.", "tokens": ["Sie", "lie\u00b7get", "auff", "Je\u00b7su", "ge\u00b7dul\u00b7di\u00b7gem", "R\u00fc\u00b7cken"