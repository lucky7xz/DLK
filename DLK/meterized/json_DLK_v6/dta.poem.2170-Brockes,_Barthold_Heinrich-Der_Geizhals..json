{"dta.poem.2170": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Geizhals.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mir ist ein alter Mann bekannt, dem Ansehn nach,", "tokens": ["Mir", "ist", "ein", "al\u00b7ter", "Mann", "be\u00b7kannt", ",", "dem", "An\u00b7sehn", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "PTKVZ", "$,", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ein guter Christ,", "tokens": ["ein", "gu\u00b7ter", "Christ", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Der Geld von Jugend auf gesammlet, und selbst nicht", "tokens": ["Der", "Geld", "von", "Ju\u00b7gend", "auf", "ge\u00b7samm\u00b7let", ",", "und", "selbst", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "APPR", "VVPP", "$,", "KON", "ADV", "PTKNEG"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "weis, wie reich er ist.", "tokens": ["weis", ",", "wie", "reich", "er", "ist", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "So bald er Beutel voll gemacht, verschlie\u00dft er sie in gro\u00dfe", "tokens": ["So", "bald", "er", "Beu\u00b7tel", "voll", "ge\u00b7macht", ",", "ver\u00b7schlie\u00dft", "er", "sie", "in", "gro\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "NN", "ADJD", "VVPP", "$,", "VVFIN", "PPER", "PPER", "APPR", "ADJA"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Schranken,", "tokens": ["Schran\u00b7ken", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Und macht, wenn die\u00df geschehn, von ihnen sich keine wei-", "tokens": ["Und", "macht", ",", "wenn", "die\u00df", "ge\u00b7schehn", ",", "von", "ih\u00b7nen", "sich", "kei\u00b7ne", "wei"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PDS", "VVINF", "$,", "APPR", "PPER", "PRF", "PIAT", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "tere Gedanken.", "tokens": ["te\u00b7re", "Ge\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Inzwischen hat er einen Knecht, der seines Herren Weise", "tokens": ["I\u00b7nzwi\u00b7schen", "hat", "er", "ei\u00b7nen", "Knecht", ",", "der", "sei\u00b7nes", "Her\u00b7ren", "Wei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.10": {"text": "kennt,", "tokens": ["kennt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.11": {"text": "Und der die\u00df ungebrauchte Geld sich lieber selbst, als nie-", "tokens": ["Und", "der", "die\u00df", "un\u00b7ge\u00b7brauch\u00b7te", "Geld", "sich", "lie\u00b7ber", "selbst", ",", "als", "nie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "PDS", "ADJA", "NN", "PRF", "ADV", "ADV", "$,", "KOUS", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.12": {"text": "mand, g\u00f6nnt;", "tokens": ["mand", ",", "g\u00f6nnt", ";"], "token_info": ["word", "punct", "word", "punct"], "pos": ["PIS", "$,", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.13": {"text": "Der \u00f6ffnet, wenn es ihm beliebt, den Schranken am ver-", "tokens": ["Der", "\u00f6ff\u00b7net", ",", "wenn", "es", "ihm", "be\u00b7liebt", ",", "den", "Schran\u00b7ken", "am", "ver"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "KOUS", "PPER", "PPER", "ADJD", "$,", "ART", "NN", "APPRART", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "borgnen Ort,", "tokens": ["borg\u00b7nen", "Ort", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.15": {"text": "Und nimmt nach eigenem Gefallen die gr\u00f6\u00dften Beutel", "tokens": ["Und", "nimmt", "nach", "ei\u00b7ge\u00b7nem", "Ge\u00b7fal\u00b7len", "die", "gr\u00f6\u00df\u00b7ten", "Beu\u00b7tel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+---+--+-+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "mit sich fort,", "tokens": ["mit", "sich", "fort", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PRF", "PTKVZ", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.17": {"text": "Ohn da\u00df der Alte solches merkt. Der sammlet immer", "tokens": ["Ohn", "da\u00df", "der", "Al\u00b7te", "sol\u00b7ches", "merkt", ".", "Der", "samm\u00b7let", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "KOUS", "ART", "NN", "PIS", "VVFIN", "$.", "ART", "VVFIN", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "ohn Erm\u00fcden,", "tokens": ["ohn", "Er\u00b7m\u00fc\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.19": {"text": "Und wenn er sein Gesammletes verschlossen hat, ist er zu-", "tokens": ["Und", "wenn", "er", "sein", "Ge\u00b7samm\u00b7le\u00b7tes", "ver\u00b7schlos\u00b7sen", "hat", ",", "ist", "er", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "NN", "VVPP", "VAFIN", "$,", "VAFIN", "PPER", "TRUNC"], "meter": "-+-+-+-+-+--+-+", "measure": "iambic.septa.relaxed"}, "line.20": {"text": "frieden.", "tokens": ["frie\u00b7den", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.21": {"text": "Er geizet, karget, schindet, plaget die Armen, thut sich", "tokens": ["Er", "gei\u00b7zet", ",", "kar\u00b7get", ",", "schin\u00b7det", ",", "pla\u00b7get", "die", "Ar\u00b7men", ",", "thut", "sich"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "ART", "NN", "$,", "VVFIN", "PRF"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "nichts zu Gut,", "tokens": ["nichts", "zu", "Gut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "PTKA", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.23": {"text": "Zuletzt stockt endlich dieses Schinders und ewgen Samm-", "tokens": ["Zu\u00b7letzt", "stockt", "end\u00b7lich", "die\u00b7ses", "Schin\u00b7ders", "und", "ew\u00b7gen", "Sam\u00b7m"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PDAT", "NN", "KON", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "lers altes Blut,", "tokens": ["lers", "al\u00b7tes", "Blut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.25": {"text": "Er stirbt, und man begr\u00e4bet ihn. Nun sprecht, was soll", "tokens": ["Er", "stirbt", ",", "und", "man", "be\u00b7gr\u00e4\u00b7bet", "ihn", ".", "Nun", "sprecht", ",", "was", "soll"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "PIS", "VVFIN", "PPER", "$.", "ADV", "VVFIN", "$,", "PWS", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "man solchem Leben", "tokens": ["man", "sol\u00b7chem", "Le\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["PIS", "PIAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.27": {"text": "Und solchem alten Mammonsknecht f\u00fcr einen w\u00fcrdgen", "tokens": ["Und", "sol\u00b7chem", "al\u00b7ten", "Mam\u00b7mons\u00b7knecht", "f\u00fcr", "ei\u00b7nen", "w\u00fcrd\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Namen geben?", "tokens": ["Na\u00b7men", "ge\u00b7ben", "?"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.29": {"text": "Mich deucht, er h\u00f6rt zwar in der Narren, doch mehr", "tokens": ["Mich", "deucht", ",", "er", "h\u00f6rt", "zwar", "in", "der", "Nar\u00b7ren", ",", "doch", "mehr"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$,", "ADV", "ADV"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.30": {"text": "noch in der Diebe Zahl,", "tokens": ["noch", "in", "der", "Die\u00b7be", "Zahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.31": {"text": "Da er, durch sein liebloses Geizen, der ganzen Menschheit", "tokens": ["Da", "er", ",", "durch", "sein", "lieb\u00b7lo\u00b7ses", "Gei\u00b7zen", ",", "der", "gan\u00b7zen", "Menschheit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.32": {"text": "etwas stahl,", "tokens": ["et\u00b7was", "stahl", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.33": {"text": "Was allen, blo\u00df nur ihm nichts, n\u00fctzte. Der wahre", "tokens": ["Was", "al\u00b7len", ",", "blo\u00df", "nur", "ihm", "nichts", ",", "n\u00fctz\u00b7te", ".", "Der", "wah\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PWS", "PIS", "$,", "ADV", "ADV", "PPER", "PIS", "$,", "VVFIN", "$.", "ART", "ADJA"], "meter": "-+-+---+--+-", "measure": "iambic.tetra.relaxed"}, "line.34": {"text": "Nutzen der Metallen", "tokens": ["Nut\u00b7zen", "der", "Me\u00b7tal\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.35": {"text": "Ist, da\u00df sie immer circuliren. Wer diesen n\u00f6thgen Kreis-", "tokens": ["Ist", ",", "da\u00df", "sie", "im\u00b7mer", "cir\u00b7cu\u00b7li\u00b7ren", ".", "Wer", "die\u00b7sen", "n\u00f6th\u00b7gen", "Kreis"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "$,", "KOUS", "PPER", "ADV", "VVINF", "$.", "PWS", "PDAT", "ADJA", "TRUNC"], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.36": {"text": "lauf hemmt", "tokens": ["lauf", "hemmt"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.37": {"text": "Und in dem eingesperrten Gelde den Flu\u00df des irdschen", "tokens": ["Und", "in", "dem", "ein\u00b7ge\u00b7sperr\u00b7ten", "Gel\u00b7de", "den", "Flu\u00df", "des", "ird\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "NN", "ART", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.38": {"text": "Heils verd\u00e4mmt,", "tokens": ["Heils", "ver\u00b7d\u00e4mmt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.39": {"text": "Es sich und allen Menschen stielt, dem w\u00fcnsch ich, solchen", "tokens": ["Es", "sich", "und", "al\u00b7len", "Men\u00b7schen", "stielt", ",", "dem", "w\u00fcnsch", "ich", ",", "sol\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPER", "PRF", "KON", "PIAT", "NN", "VVFIN", "$,", "PRELS", "VVFIN", "PPER", "$,", "PIAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "schlauen Knecht!", "tokens": ["schlau\u00b7en", "Knecht", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.41": {"text": "Doch auch dabey: da\u00df, eh\u2019 er stirbt, er mit Entsetzen,", "tokens": ["Doch", "auch", "da\u00b7bey", ":", "da\u00df", ",", "eh'", "er", "stirbt", ",", "er", "mit", "Ent\u00b7set\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PAV", "$.", "KOUS", "$,", "KOUS", "PPER", "VVFIN", "$,", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Zagen, Schrecken", "tokens": ["Za\u00b7gen", ",", "Schre\u00b7cken"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.43": {"text": "Den unersetzlichen Verlust, zu seiner Strafe, mag ent-", "tokens": ["Den", "un\u00b7er\u00b7setz\u00b7li\u00b7chen", "Ver\u00b7lust", ",", "zu", "sei\u00b7ner", "Stra\u00b7fe", ",", "mag", "ent"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "$,", "VMFIN", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.44": {"text": "decken!", "tokens": ["de\u00b7cken", "!"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}