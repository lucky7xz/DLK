{"textgrid.poem.37415": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "So und so", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zur Schenke lenkt mit Wohlbehagen", "tokens": ["Zur", "Schen\u00b7ke", "lenkt", "mit", "Wohl\u00b7be\u00b7ha\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er jeden Abend seinen Schritt", "tokens": ["Er", "je\u00b7den", "A\u00b7bend", "sei\u00b7nen", "Schritt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "PIAT", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und bleibt, bis da\u00df die Lerchen schlagen.", "tokens": ["Und", "bleibt", ",", "bis", "da\u00df", "die", "Ler\u00b7chen", "schla\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KON", "KOUS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er singt die letzte Strophe mit.", "tokens": ["Er", "singt", "die", "letz\u00b7te", "Stro\u00b7phe", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dagegen ist es zu beklagen,", "tokens": ["Da\u00b7ge\u00b7gen", "ist", "es", "zu", "be\u00b7kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df er die Kirche nie betritt.", "tokens": ["Da\u00df", "er", "die", "Kir\u00b7che", "nie", "be\u00b7tritt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hier, leider, kann man niemals sagen:", "tokens": ["Hier", ",", "lei\u00b7der", ",", "kann", "man", "nie\u00b7mals", "sa\u00b7gen", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "$,", "VMFIN", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er singt die letzte Strophe mit.", "tokens": ["Er", "singt", "die", "letz\u00b7te", "Stro\u00b7phe", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}