{"textgrid.poem.61152": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Glaubt mir, dafern in Deutschlands Eingeweide", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Glaubt mir, dafern in Deutschlands Eingeweide", "tokens": ["Glaubt", "mir", ",", "da\u00b7fern", "in", "Deutschlands", "Ein\u00b7ge\u00b7wei\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "APPR", "NE", "NN"], "meter": "+--+-++-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Das Schwert ihr kehrt und sch\u00fcrt des Kriegs Verderben:", "tokens": ["Das", "Schwert", "ihr", "kehrt", "und", "sch\u00fcrt", "des", "Kriegs", "Ver\u00b7der\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht Freiheit werden eure Kinder erben;", "tokens": ["Nicht", "Frei\u00b7heit", "wer\u00b7den", "eu\u00b7re", "Kin\u00b7der", "er\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "VAFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zum Baume tragt ihr selbst des Beiles Schneide.", "tokens": ["Zum", "Bau\u00b7me", "tragt", "ihr", "selbst", "des", "Bei\u00b7les", "Schnei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es wird ein Kampf von unerme\u00dfnem Leide,", "tokens": ["Es", "wird", "ein", "Kampf", "von", "un\u00b7er\u00b7me\u00df\u00b7nem", "Lei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Darin die Besten auf der Walstatt sterben;", "tokens": ["Da\u00b7rin", "die", "Bes\u00b7ten", "auf", "der", "Wals\u00b7tatt", "ster\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Slawe wird zuletzt das Reich erwerben,", "tokens": ["Der", "Sla\u00b7we", "wird", "zu\u00b7letzt", "das", "Reich", "er\u00b7wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df er auf Gr\u00e4bern seine Rosse weide.", "tokens": ["Da\u00df", "er", "auf", "Gr\u00e4\u00b7bern", "sei\u00b7ne", "Ros\u00b7se", "wei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Schon h\u00f6r' ich als der Knechtschaft Siegesreigen", "tokens": ["Schon", "h\u00f6r'", "ich", "als", "der", "Knecht\u00b7schaft", "Sie\u00b7ges\u00b7rei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KOUS", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Prophet'schen Ohrs den Klang von seinen Hufen -", "tokens": ["Pro\u00b7phet'\u00b7schen", "Ohrs", "den", "Klang", "von", "sei\u00b7nen", "Hu\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr aber glaubt es nicht, und ich mu\u00df schweigen.", "tokens": ["Ihr", "a\u00b7ber", "glaubt", "es", "nicht", ",", "und", "ich", "mu\u00df", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "PTKNEG", "$,", "KON", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So schwieg Kassandra auf des Tempels Stufen,", "tokens": ["So", "schwieg", "Kas\u00b7sand\u00b7ra", "auf", "des", "Tem\u00b7pels", "Stu\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da sie im Geist sah Trojas Flamme steigen,", "tokens": ["Da", "sie", "im", "Geist", "sah", "Tro\u00b7jas", "Flam\u00b7me", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "NE", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und niemand h\u00f6rt' es, da\u00df sie Weh gerufen.", "tokens": ["Und", "nie\u00b7mand", "h\u00f6rt'", "es", ",", "da\u00df", "sie", "Weh", "ge\u00b7ru\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "$,", "KOUS", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}