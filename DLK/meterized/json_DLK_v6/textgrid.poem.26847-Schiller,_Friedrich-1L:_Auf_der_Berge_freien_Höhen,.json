{"textgrid.poem.26847": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf der Berge freien H\u00f6hen,", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf der Berge freien H\u00f6hen,", "tokens": ["Auf", "der", "Ber\u00b7ge", "frei\u00b7en", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der Mittagsonne Schein,", "tokens": ["In", "der", "Mit\u00b7ta\u00b7gson\u00b7ne", "Schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "An des warmen Strahles Kr\u00e4ften", "tokens": ["An", "des", "war\u00b7men", "Strah\u00b7les", "Kr\u00e4f\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zeugt Natur den goldnen Wein.", "tokens": ["Zeugt", "Na\u00b7tur", "den", "gold\u00b7nen", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und noch niemand hats erkundet,", "tokens": ["Und", "noch", "nie\u00b7mand", "hats", "er\u00b7kun\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIS", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie die gro\u00dfe Mutter schafft;", "tokens": ["Wie", "die", "gro\u00b7\u00dfe", "Mut\u00b7ter", "schafft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unergr\u00fcndlich ist das Wirken,", "tokens": ["Un\u00b7er\u00b7gr\u00fcnd\u00b7lich", "ist", "das", "Wir\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unerforschlich ist die Kraft.", "tokens": ["Un\u00b7er\u00b7for\u00b7schlich", "ist", "die", "Kraft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Funkelnd wie ein Sohn der Sonne,", "tokens": ["Fun\u00b7kelnd", "wie", "ein", "Sohn", "der", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie des Lichtes Feuerquell,", "tokens": ["Wie", "des", "Lich\u00b7tes", "Feu\u00b7er\u00b7quell", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Springt er perlend aus der Tonne", "tokens": ["Springt", "er", "per\u00b7lend", "aus", "der", "Ton\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Purpurn und kristallenhell.", "tokens": ["Pur\u00b7purn", "und", "kris\u00b7tal\u00b7len\u00b7hell", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Und erfreuet alle Sinnen,", "tokens": ["Und", "er\u00b7freu\u00b7et", "al\u00b7le", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und in jede bange Brust", "tokens": ["Und", "in", "je\u00b7de", "ban\u00b7ge", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gie\u00dft er ein balsamisch Hoffen", "tokens": ["Gie\u00dft", "er", "ein", "bal\u00b7sa\u00b7misch", "Hof\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und des Lebens neue Lust.", "tokens": ["Und", "des", "Le\u00b7bens", "neu\u00b7e", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Aber matt auf unsre Zonen", "tokens": ["A\u00b7ber", "matt", "auf", "uns\u00b7re", "Zo\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00e4llt der Sonne schr\u00e4ges Licht,", "tokens": ["F\u00e4llt", "der", "Son\u00b7ne", "schr\u00e4\u00b7ges", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur die Bl\u00e4tter kann sie f\u00e4rben,", "tokens": ["Nur", "die", "Bl\u00e4t\u00b7ter", "kann", "sie", "f\u00e4r\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber Fr\u00fcchte reift sie nicht.", "tokens": ["A\u00b7ber", "Fr\u00fcch\u00b7te", "reift", "sie", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Doch der Norden auch will leben,", "tokens": ["Doch", "der", "Nor\u00b7den", "auch", "will", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und was lebt, will sich erfreun;", "tokens": ["Und", "was", "lebt", ",", "will", "sich", "er\u00b7freun", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "$,", "VMFIN", "PRF", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Darum schaffen wir erfindend", "tokens": ["Da\u00b7rum", "schaf\u00b7fen", "wir", "er\u00b7fin\u00b7dend"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ohne Weinstock uns den Wein.", "tokens": ["Oh\u00b7ne", "Wein\u00b7stock", "uns", "den", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Bleich nur ists, was wir bereiten", "tokens": ["Bleich", "nur", "ists", ",", "was", "wir", "be\u00b7rei\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "ADV", "VAFIN", "$,", "PRELS", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf dem h\u00e4uslichen Altar;", "tokens": ["Auf", "dem", "h\u00e4us\u00b7li\u00b7chen", "Al\u00b7tar", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was Natur lebendig bildet,", "tokens": ["Was", "Na\u00b7tur", "le\u00b7ben\u00b7dig", "bil\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gl\u00e4nzend ists und ewig klar.", "tokens": ["Gl\u00e4n\u00b7zend", "ists", "und", "e\u00b7wig", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "KON", "ADJD", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Aber freudig aus der Schale", "tokens": ["A\u00b7ber", "freu\u00b7dig", "aus", "der", "Scha\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6pfen wir die tr\u00fcbe Flut,", "tokens": ["Sch\u00f6p\u00b7fen", "wir", "die", "tr\u00fc\u00b7be", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auch die ", "tokens": ["Auch", "die"], "token_info": ["word", "word"], "pos": ["ADV", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Borgt sie gleich von irdscher Glut.", "tokens": ["Borgt", "sie", "gleich", "von", "ird\u00b7scher", "Glut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Ihrem Wirken freigegeben", "tokens": ["Ih\u00b7rem", "Wir\u00b7ken", "frei\u00b7ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist der Kr\u00e4fte gro\u00dfes Reich;", "tokens": ["Ist", "der", "Kr\u00e4f\u00b7te", "gro\u00b7\u00dfes", "Reich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Neues bildend aus dem Alten,", "tokens": ["Neu\u00b7es", "bil\u00b7dend", "aus", "dem", "Al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stellt sie sich dem Sch\u00f6pfer gleich.", "tokens": ["Stellt", "sie", "sich", "dem", "Sch\u00f6p\u00b7fer", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Selbst das Band der Elemente", "tokens": ["Selbst", "das", "Band", "der", "E\u00b7le\u00b7men\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ART", "NN"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Trennt ihr herrschendes Gebot,", "tokens": ["Trennt", "ihr", "herr\u00b7schen\u00b7des", "Ge\u00b7bot", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und sie ahmt mit Herdes Flammen", "tokens": ["Und", "sie", "ahmt", "mit", "Her\u00b7des", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nach den hohen Sonnengott.", "tokens": ["Nach", "den", "ho\u00b7hen", "Son\u00b7nen\u00b7gott", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Fernhin zu den selgen Inseln", "tokens": ["Fern\u00b7hin", "zu", "den", "sel\u00b7gen", "In\u00b7seln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Richtet sie der Schiffe Lauf,", "tokens": ["Rich\u00b7tet", "sie", "der", "Schif\u00b7fe", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und des S\u00fcdens goldne Fr\u00fcchte", "tokens": ["Und", "des", "S\u00fc\u00b7dens", "gold\u00b7ne", "Fr\u00fcch\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sch\u00fcttet sie im Norden auf.", "tokens": ["Sch\u00fct\u00b7tet", "sie", "im", "Nor\u00b7den", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Drum ein Sinnbild und ein Zeichen", "tokens": ["Drum", "ein", "Sinn\u00b7bild", "und", "ein", "Zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sei uns dieser Feuersaft,", "tokens": ["Sei", "uns", "die\u00b7ser", "Feu\u00b7er\u00b7saft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was der Mensch sich kann erlangen", "tokens": ["Was", "der", "Mensch", "sich", "kann", "er\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "PRF", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit dem Willen und der Kraft.", "tokens": ["Mit", "dem", "Wil\u00b7len", "und", "der", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}