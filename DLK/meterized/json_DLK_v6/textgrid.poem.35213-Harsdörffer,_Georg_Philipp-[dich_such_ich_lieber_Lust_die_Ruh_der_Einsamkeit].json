{"textgrid.poem.35213": {"metadata": {"author": {"name": "Harsd\u00f6rffer, Georg Philipp", "birth": "N.A.", "death": "N.A."}, "title": "[dich/ such ich lieber Lust/ die Ruh der Einsamkeit]", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dich/ such ich lieber Lust/ die Ruh der Einsamkeit/", "tokens": ["Dich", "/", "such", "ich", "lie\u00b7ber", "Lust", "/", "die", "Ruh", "der", "Ein\u00b7sam\u00b7keit", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "VVIMP", "PPER", "ADV", "NN", "$(", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "zu meines Traurens Trost vers\u00fcsend meine Zeit.", "tokens": ["zu", "mei\u00b7nes", "Trau\u00b7rens", "Trost", "ver\u00b7s\u00fc\u00b7send", "mei\u00b7ne", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVPP", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hier hat kein Wandfu\u00df den seltnen Weg belastet/", "tokens": ["Hier", "hat", "kein", "Wand\u00b7fu\u00df", "den", "selt\u00b7nen", "Weg", "be\u00b7las\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Das stumme Wasservolk hat angelfrey gemastet/", "tokens": ["Das", "stum\u00b7me", "Was\u00b7ser\u00b7volk", "hat", "an\u00b7gel\u00b7frey", "ge\u00b7mas\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der m\u00fcde J\u00e4gersmann in seinem Durst entbrandt/", "tokens": ["Der", "m\u00fc\u00b7de", "J\u00e4\u00b7gers\u00b7mann", "in", "sei\u00b7nem", "Durst", "ent\u00b7brandt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Hat niemahls aufgesch\u00f6pft hier Wasser mit der Hand/", "tokens": ["Hat", "nie\u00b7mahls", "auf\u00b7ge\u00b7sch\u00f6pft", "hier", "Was\u00b7ser", "mit", "der", "Hand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "ADV", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "kein windgeschwindes Wild hat man hier m\u00f6gen f\u00e4llen/", "tokens": ["kein", "wind\u00b7ge\u00b7schwin\u00b7des", "Wild", "hat", "man", "hier", "m\u00f6\u00b7gen", "f\u00e4l\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "PIS", "ADV", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Noch mit dem hohen Zeug nach schwartz\u0113 Wildpret stell\u0113/", "tokens": ["Noch", "mit", "dem", "ho\u00b7hen", "Zeug", "nach", "schwartz\u0113", "Wild\u00b7pret", "stell\u0113", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.9": {"text": "Hier birgt vor niemand nicht der Igel seine Zucht/", "tokens": ["Hier", "birgt", "vor", "nie\u00b7mand", "nicht", "der", "I\u00b7gel", "sei\u00b7ne", "Zucht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PIS", "PTKNEG", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die Eul in dem Gem\u00e4ur ihr hole Wohnung sucht.", "tokens": ["Die", "Eul", "in", "dem", "Ge\u00b7m\u00e4ur", "ihr", "ho\u00b7le", "Woh\u00b7nung", "sucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Das langstverfallne Haus zeigt der gew\u00f6lbte Bogen/", "tokens": ["Das", "langst\u00b7ver\u00b7fall\u00b7ne", "Haus", "zeigt", "der", "ge\u00b7w\u00f6lb\u00b7te", "Bo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ihn hat der Zeiten Zeit vorlangsten \u00fcberwogen/", "tokens": ["Ihn", "hat", "der", "Zei\u00b7ten", "Zeit", "vor\u00b7langs\u00b7ten", "\u00fc\u00b7ber\u00b7wo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der Wiesel und der Luchs sind wohnhaft hier zu Land/", "tokens": ["Der", "Wie\u00b7sel", "und", "der", "Luchs", "sind", "wohn\u00b7haft", "hier", "zu", "Land", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VAFIN", "ADJD", "ADV", "APPR", "NN", "$("], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.14": {"text": "Wie manche Schlang bef\u00fcllt mit Jungen diesen Sand/", "tokens": ["Wie", "man\u00b7che", "Schlang", "be\u00b7f\u00fcllt", "mit", "Jun\u00b7gen", "die\u00b7sen", "Sand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVFIN", "APPR", "NN", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Lebt auch noch unverfault ein Bret vom dritten Gaden/", "tokens": ["Lebt", "auch", "noch", "un\u00b7ver\u00b7fault", "ein", "Bret", "vom", "drit\u00b7ten", "Ga\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADJD", "ART", "NN", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Hier mag dz Giftgeschmeis in Otterleiche baden.", "tokens": ["Hier", "mag", "dz", "Gift\u00b7ge\u00b7schmeis", "in", "Ot\u00b7ter\u00b7lei\u00b7che", "ba\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "KOUS", "NN", "APPR", "NE", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ein alte Birken-St\u00e4tt n\u00e4chst der verfallnen T\u00fcr/", "tokens": ["Ein", "al\u00b7te", "Bir\u00b7ken\u00b7St\u00e4tt", "n\u00e4chst", "der", "ver\u00b7fall\u00b7nen", "T\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Dekkt die bem\u00f6sten Stein und gr\u00fcnet hoch herf\u00fcr/", "tokens": ["De\u00b7kkt", "die", "be\u00b7m\u00f6s\u00b7ten", "Stein", "und", "gr\u00fc\u00b7net", "hoch", "her\u00b7f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "KON", "VVFIN", "ADJD", "ADV", "$("], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Der Fall begr\u00e4bt das Grab/ \u00fcmzirkt den \u00f6den Rangen/", "tokens": ["Der", "Fall", "be\u00b7gr\u00e4bt", "das", "Grab", "/", "\u00fcm\u00b7zirkt", "den", "\u00f6\u00b7den", "Ran\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$(", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Es trauren n\u00e4chst d\u0113 Schlo\u00df die dikkbeb\u00e4umten Hangen.", "tokens": ["Es", "trau\u00b7ren", "n\u00e4chst", "d\u0113", "Schlo\u00df", "die", "dikk\u00b7be\u00b7b\u00e4um\u00b7ten", "Han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wie nennt man diesen Bau/ der keinen Namen hat?", "tokens": ["Wie", "nennt", "man", "die\u00b7sen", "Bau", "/", "der", "kei\u00b7nen", "Na\u00b7men", "hat", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PIS", "PDAT", "NN", "$(", "ART", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Sein Abweg dient dem Weg jetzt an Begleiters stat.", "tokens": ["Sein", "Ab\u00b7weg", "dient", "dem", "Weg", "jetzt", "an", "Be\u00b7glei\u00b7ters", "stat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Hat sich dann Holtz und Laub in diesen Deich gest\u00fcrtzet?", "tokens": ["Hat", "sich", "dann", "Holtz", "und", "Laub", "in", "die\u00b7sen", "Deich", "ge\u00b7st\u00fcrt\u00b7zet", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ADV", "NN", "KON", "NN", "APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Das fast ein jeder Baum ist Mahlerrecht gek\u00fcrtzet.", "tokens": ["Das", "fast", "ein", "je\u00b7der", "Baum", "ist", "Mah\u00b7ler\u00b7recht", "ge\u00b7k\u00fcrt\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "PIAT", "NN", "VAFIN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Sag/ Nymfe dieser Gruft/ bin ich hier gantz allein?", "tokens": ["Sag", "/", "Nym\u00b7fe", "die\u00b7ser", "Gruft", "/", "bin", "ich", "hier", "gantz", "al\u00b7lein", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "PDAT", "NN", "$(", "VAFIN", "PPER", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Die gelbe Lachenbrut quakkt aus der Pf\u00fctzen/ nein.", "tokens": ["Die", "gel\u00b7be", "La\u00b7chen\u00b7brut", "quakkt", "aus", "der", "Pf\u00fct\u00b7zen", "/", "nein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$(", "PTKANT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "An diesem wilden Ort solt Echo Lieb erfrieren/", "tokens": ["An", "die\u00b7sem", "wil\u00b7den", "Ort", "solt", "E\u00b7cho", "Lieb", "er\u00b7frie\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "VMFIN", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Die pfleget meine Laut und mein Gesang zu zieren.", "tokens": ["Die", "pfle\u00b7get", "mei\u00b7ne", "Laut", "und", "mein", "Ge\u00b7sang", "zu", "zie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ich liebe diesen Wald/ der ferne von Geschrey", "tokens": ["Ich", "lie\u00b7be", "die\u00b7sen", "Wald", "/", "der", "fer\u00b7ne", "von", "Ge\u00b7schrey"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "$(", "ART", "ADJA", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Mich auf so w\u00fcstem Weg f\u00fcrt aller Sorgen frey.", "tokens": ["Mich", "auf", "so", "w\u00fcs\u00b7tem", "Weg", "f\u00fcrt", "al\u00b7ler", "Sor\u00b7gen", "frey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADV", "ADJA", "NN", "VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Hier schirmen meine Ruh der Felsen k\u00fcle Schatten/", "tokens": ["Hier", "schir\u00b7men", "mei\u00b7ne", "Ruh", "der", "Fel\u00b7sen", "k\u00fc\u00b7le", "Schat\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Wo sich in rauher N\u00e4h die Nachtigallen gatten.", "tokens": ["Wo", "sich", "in", "rau\u00b7her", "N\u00e4h", "die", "Nach\u00b7ti\u00b7gal\u00b7len", "gat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "APPR", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Von welcher Liebes-Klag erschallt der sch\u00f6ne Schall/", "tokens": ["Von", "wel\u00b7cher", "Lie\u00b7bes\u00b7Klag", "er\u00b7schallt", "der", "sch\u00f6\u00b7ne", "Schall", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Der durch den Thal ert\u00f6nt/ der hold und helle Hall.", "tokens": ["Der", "durch", "den", "Thal", "er\u00b7t\u00f6nt", "/", "der", "hold", "und", "hel\u00b7le", "Hall", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$(", "ART", "ADJD", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Singt dann der Felsenstein? so will ich gleichfalls singen/", "tokens": ["Singt", "dann", "der", "Fel\u00b7sen\u00b7stein", "?", "so", "will", "ich", "gleich\u00b7falls", "sin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$.", "ADV", "VMFIN", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Da\u00df meiner Lauten Laut soll in der Luft erklingen:", "tokens": ["Da\u00df", "mei\u00b7ner", "Lau\u00b7ten", "Laut", "soll", "in", "der", "Luft", "er\u00b7klin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Einsame dichten die lieblichsten Lieder/", "tokens": ["Ein\u00b7sa\u00b7me", "dich\u00b7ten", "die", "lieb\u00b7lichs\u00b7ten", "Lie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ART", "ADJA", "NN", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.38": {"text": "Lieblich erschallend in Felsen herwieder.", "tokens": ["Lieb\u00b7lich", "er\u00b7schal\u00b7lend", "in", "Fel\u00b7sen", "her\u00b7wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "NN", "PTKVZ", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.39": {"text": "Nimmermer werd ich die Wildnisse hassen", "tokens": ["Nim\u00b7mer\u00b7mer", "werd", "ich", "die", "Wild\u00b7nis\u00b7se", "has\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "ART", "NN", "VVINF"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.40": {"text": "Welche reitzt H\u00fcrden die Sch\u00e4fer zu lassen.", "tokens": ["Wel\u00b7che", "reitzt", "H\u00fcr\u00b7den", "die", "Sch\u00e4\u00b7fer", "zu", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.41": {"text": "Einsamkeit kr\u00f6net die Anger und Auen/", "tokens": ["Ein\u00b7sam\u00b7keit", "kr\u00f6\u00b7net", "die", "An\u00b7ger", "und", "Au\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "KON", "NN", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.42": {"text": "Mahnet die Hirten und Herden zu schauen.", "tokens": ["Mah\u00b7net", "die", "Hir\u00b7ten", "und", "Her\u00b7den", "zu", "schau\u00b7en", "."], "token