{"dta.poem.14848": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der verirrte J\u00e4ger.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ewie rauscht schon abendlich der Wald,", "tokens": ["\u201e", "wie", "rauscht", "schon", "a\u00b7bend\u00b7lich", "der", "Wald", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VVFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Brust mir schaurig schwellt!", "tokens": ["Die", "Brust", "mir", "schau\u00b7rig", "schwellt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Freunde fern, der Wind so kalt,", "tokens": ["Die", "Freun\u00b7de", "fern", ",", "der", "Wind", "so", "kalt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So tief und weit die Welt!\u201c", "tokens": ["So", "tief", "und", "weit", "die", "Welt", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}