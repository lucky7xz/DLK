{"textgrid.poem.57657": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "In knospenden Zweigen ...", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In knospenden Zweigen schmettern die Finken,", "tokens": ["In", "knos\u00b7pen\u00b7den", "Zwei\u00b7gen", "schmet\u00b7tern", "die", "Fin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es trommelt der Specht sein Liebeslied,", "tokens": ["Es", "trom\u00b7melt", "der", "Specht", "sein", "Lie\u00b7bes\u00b7lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der Tauber ruckst im Eichenwipfel,", "tokens": ["Der", "Tau\u00b7ber", "ruckst", "im", "Ei\u00b7chen\u00b7wip\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Zittern und Beben den Wald durchzieht.", "tokens": ["Ein", "Zit\u00b7tern", "und", "Be\u00b7ben", "den", "Wald", "durch\u00b7zieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Im Fallaub leuchten einzelne Bl\u00fcten,", "tokens": ["Im", "Fal\u00b7laub", "leuch\u00b7ten", "ein\u00b7zel\u00b7ne", "Bl\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Wald wird morgen voll Blumen sein,", "tokens": ["Der", "Wald", "wird", "mor\u00b7gen", "voll", "Blu\u00b7men", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "NN", "VAINF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Es hat erweckt sein Singen und Bl\u00fchen", "tokens": ["Es", "hat", "er\u00b7weckt", "sein", "Sin\u00b7gen", "und", "Bl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Aus unseren Augen der Sonnenschein.", "tokens": ["Aus", "un\u00b7se\u00b7ren", "Au\u00b7gen", "der", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}