{"textgrid.poem.28698": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da\u00df alle Menschen L\u00fcgner seyn, ist mit Bescheid zu nemen;", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df alle Menschen L\u00fcgner seyn, ist mit Bescheid zu nemen;", "tokens": ["Da\u00df", "al\u00b7le", "Men\u00b7schen", "L\u00fcg\u00b7ner", "seyn", ",", "ist", "mit", "Be\u00b7scheid", "zu", "ne\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "NN", "VAINF", "$,", "VAFIN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Die Schrifft, die siht auff unsre Zeit, da l\u00fcgen heist bequemen;", "tokens": ["Die", "Schrifft", ",", "die", "siht", "auff", "uns\u00b7re", "Zeit", ",", "da", "l\u00fc\u00b7gen", "heist", "be\u00b7que\u00b7men", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "ADJD", "ADJA", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Bequemen heist politisch seyn. Wer wil sich dessen sch\u00e4men?", "tokens": ["Be\u00b7que\u00b7men", "heist", "po\u00b7li\u00b7tisch", "seyn", ".", "Wer", "wil", "sich", "des\u00b7sen", "sch\u00e4\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "VAINF", "$.", "PWS", "VMFIN", "PRF", "PDS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}}}}