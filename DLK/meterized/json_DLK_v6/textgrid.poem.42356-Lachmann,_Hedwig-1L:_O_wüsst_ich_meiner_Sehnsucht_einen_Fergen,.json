{"textgrid.poem.42356": {"metadata": {"author": {"name": "Lachmann, Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: O w\u00fcsst ich meiner Sehnsucht einen Fergen,", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O w\u00fcsst ich meiner Sehnsucht einen Fergen,", "tokens": ["O", "w\u00fcsst", "ich", "mei\u00b7ner", "Sehn\u00b7sucht", "ei\u00b7nen", "Fer\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dass er ihr eine sanfte F\u00e4hrte weise!", "tokens": ["Dass", "er", "ihr", "ei\u00b7ne", "sanf\u00b7te", "F\u00e4hr\u00b7te", "wei\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So kehrt sie mir zur\u00fcck aus hohen Bergen,", "tokens": ["So", "kehrt", "sie", "mir", "zu\u00b7r\u00fcck", "aus", "ho\u00b7hen", "Ber\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Todmatt vom Flug und fast erstarrt im Eise.", "tokens": ["Tod\u00b7matt", "vom", "Flug", "und", "fast", "er\u00b7starrt", "im", "Ei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "KON", "ADV", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich wollte, dass ein leichter Kahn mich f\u00fchre", "tokens": ["Ich", "woll\u00b7te", ",", "dass", "ein", "leich\u00b7ter", "Kahn", "mich", "f\u00fch\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "ART", "ADJA", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Strom entlang in ebene Gel\u00e4nde,", "tokens": ["Den", "Strom", "ent\u00b7lang", "in", "e\u00b7be\u00b7ne", "Ge\u00b7l\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPO", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Und dass ich dort durch eine niedre T\u00fcre", "tokens": ["Und", "dass", "ich", "dort", "durch", "ei\u00b7ne", "nied\u00b7re", "T\u00fc\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In einem stillen Hause Eingang f\u00e4nde.", "tokens": ["In", "ei\u00b7nem", "stil\u00b7len", "Hau\u00b7se", "Ein\u00b7gang", "f\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und drinnen nur von abendlichen Kerzen", "tokens": ["Und", "drin\u00b7nen", "nur", "von", "a\u00b7bend\u00b7li\u00b7chen", "Ker\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein mildes D\u00e4mmerlicht am eignen Herde.", "tokens": ["Ein", "mil\u00b7des", "D\u00e4m\u00b7mer\u00b7licht", "am", "eig\u00b7nen", "Her\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein warmer Raum, ein Kind an meinem Herzen,", "tokens": ["Ein", "war\u00b7mer", "Raum", ",", "ein", "Kind", "an", "mei\u00b7nem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und eine Seele mein auf dieser Erde.", "tokens": ["Und", "ei\u00b7ne", "See\u00b7le", "mein", "auf", "die\u00b7ser", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPOSAT", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}