{"textgrid.poem.47894": {"metadata": {"author": {"name": "Klaj, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Zueignungs-Schrifft an Jhr Hoch F\u00fcrstl. Durchl. Poet", "genre": "verse", "period": "N.A.", "pub_year": 1636, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sonne", "tokens": ["Die", "Son\u00b7ne"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Man sah sich Lust mit Lust/ mit Freude Freude jagen/", "tokens": ["Man", "sah", "sich", "Lust", "mit", "Lust", "/", "mit", "Freu\u00b7de", "Freu\u00b7de", "ja\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "NN", "APPR", "NN", "$(", "APPR", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das frohe Volck der Welt schnidt/ f\u00fchrte Garben ein/", "tokens": ["Das", "fro\u00b7he", "Volck", "der", "Welt", "schnidt", "/", "f\u00fchr\u00b7te", "Gar\u00b7ben", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$(", "VVFIN", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "gieng Wild und Vogeln nach/ brach Aepffel/ lase Wein/", "tokens": ["gieng", "Wild", "und", "Vo\u00b7geln", "nach", "/", "brach", "A\u00b7e\u00b7pf\u00b7fel", "/", "la\u00b7se", "Wein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NE", "KON", "NN", "APPR", "$(", "VVFIN", "NN", "$(", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "und was der Feldlust mehr; Als nechst de\u00df Meeres Schaume/", "tokens": ["und", "was", "der", "Feld\u00b7lust", "mehr", ";", "Als", "nechst", "de\u00df", "Mee\u00b7res", "Schau\u00b7me", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "ADV", "$.", "KOUS", "ADV", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "im fremden Norden Land/ schlug au\u00df Rupertens", "tokens": ["im", "frem\u00b7den", "Nor\u00b7den", "Land", "/", "schlug", "au\u00df", "Ru\u00b7per\u00b7tens"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "NN", "$(", "VVFIN", "APPR", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "ein neubear\u00fcnter Zweig; Ein Zweig von Helden art/", "tokens": ["ein", "neu\u00b7bea\u00b7r\u00fcn\u00b7ter", "Zweig", ";", "Ein", "Zweig", "von", "Hel\u00b7den", "art", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ART", "NN", "APPR", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "dadurch das Himmel Hau\u00df in Freud gesetzet ward", "tokens": ["da\u00b7durch", "das", "Him\u00b7mel", "Hau\u00df", "in", "Freud", "ge\u00b7set\u00b7zet", "ward"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "NN", "APPR", "NN", "VVPP", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "und auch das Erdenrund. Es war im Sternenhimmel", "tokens": ["und", "auch", "das", "Er\u00b7den\u00b7rund", ".", "Es", "war", "im", "Ster\u00b7nen\u00b7him\u00b7mel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "$.", "PPER", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "ein neues Freuden Fest/ ein neues Festget\u00fcmmel/", "tokens": ["ein", "neu\u00b7es", "Freu\u00b7den", "Fest", "/", "ein", "neu\u00b7es", "Fest\u00b7ge\u00b7t\u00fcm\u00b7mel", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "der Erdbau folgte nach und stimmte lustig ein/", "tokens": ["der", "Erd\u00b7bau", "folg\u00b7te", "nach", "und", "stimm\u00b7te", "lus\u00b7tig", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "KON", "VVFIN", "ADJD", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "es musten Land und Sand/ und Wasser fr\u00f6lich seyn/", "tokens": ["es", "mus\u00b7ten", "Land", "und", "Sand", "/", "und", "Was\u00b7ser", "fr\u00f6\u00b7lich", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "KON", "NN", "$(", "KON", "NN", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "da sonst alls frolich ist. Der grosse Bundes-Engel", "tokens": ["da", "sonst", "alls", "fro\u00b7lich", "ist", ".", "Der", "gros\u00b7se", "Bun\u00b7des\u00b7En\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "ADJD", "VAFIN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "beschrieb das Engelvolck/ das Volck/ das sonder M\u00e4ngel", "tokens": ["be\u00b7schrieb", "das", "En\u00b7gel\u00b7volck", "/", "das", "Volck", "/", "das", "son\u00b7der", "M\u00e4n\u00b7gel"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$(", "ART", "NN", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "der Hoffart standhafft blieb; es funde sich im Saal", "tokens": ["der", "Hof\u00b7fart", "stand\u00b7hafft", "blieb", ";", "es", "fun\u00b7de", "sich", "im", "Saal"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "$.", "PPER", "VVFIN", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "der Fl\u00fcgeltr\u00e4ger Schar in ungezehlter Zahl.", "tokens": ["der", "Fl\u00fc\u00b7gel\u00b7tr\u00e4\u00b7ger", "Schar", "in", "un\u00b7ge\u00b7zehl\u00b7ter", "Zahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "De\u00df Ober-Feldmarschalcks Michaels Partisane/", "tokens": ["De\u00df", "O\u00b7ber\u00b7Feld\u00b7mar\u00b7schalcks", "Mic\u00b7haels", "Par\u00b7ti\u00b7sa\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "NE", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "war Lorbergr\u00fcn \u00fcmlaubt; in Gabrielens Fahne", "tokens": ["war", "Lor\u00b7ber\u00b7gr\u00fcn", "\u00fcm\u00b7laubt", ";", "in", "Ga\u00b7bri\u00b7e\u00b7lens", "Fah\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "NN", "VVPP", "$.", "APPR", "NE", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "war trefflich eingew\u00fcrckt mit Gold die Drachen-Schlacht/", "tokens": ["war", "treff\u00b7lich", "ein\u00b7ge\u00b7w\u00fcrckt", "mit", "Gold", "die", "Dra\u00b7chen\u00b7Schlacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVPP", "APPR", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "F\u00fcrst Vriel bracht Liecht/ das au\u00df Nacht Tagliecht macht.", "tokens": ["F\u00fcrst", "Vriel", "bracht", "Liecht", "/", "das", "au\u00df", "Nacht", "Ta\u00b7gliecht", "macht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "NN", "$(", "ART", "APPR", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "F\u00fcrst Raphael stund da mit starcken Spetzereyen", "tokens": ["F\u00fcrst", "Ra\u00b7phael", "stund", "da", "mit", "star\u00b7cken", "Spet\u00b7ze\u00b7re\u00b7yen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NE", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.22": {"text": "die t\u00f6dten Noth vnd Tod. Der Engel Reitereyen", "tokens": ["die", "t\u00f6d\u00b7ten", "Noth", "vnd", "Tod", ".", "Der", "En\u00b7gel", "Rei\u00b7te\u00b7re\u00b7yen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$.", "ART", "NN", "NN"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.23": {"text": "Sich schwungen ab vnd auff; Zebaoth/ St! fieng an", "tokens": ["Sich", "schwun\u00b7gen", "ab", "vnd", "auff", ";", "Ze\u00b7bao\u00b7th", "/", "St", "!", "fi\u00b7eng", "an"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["PRF", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$.", "NE", "$(", "NN", "$.", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.24": {"text": "Still/ es w\u00fcrd alles still im blauen Himmelplan:", "tokens": ["Still", "/", "es", "w\u00fcrd", "al\u00b7les", "still", "im", "blau\u00b7en", "Him\u00b7mel\u00b7plan", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$(", "PPER", "VAFIN", "PIS", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}