{"dta.poem.18557": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "A ls ihn  S elimene und  A ma-  \n lia zu ihren Vertrauten annah-  \n men.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Du wunder sch\u00f6nes Gl\u00fcck! komm la\u00df dich einmahl k\u00fcssen/", "tokens": ["Du", "wun\u00b7der", "sch\u00f6\u00b7nes", "Gl\u00fcck", "!", "komm", "la\u00df", "dich", "ein\u00b7mahl", "k\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "ADJA", "NN", "$.", "VVFIN", "VVIMP", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil zweyer Engel Gunst mich k\u00fcssens w\u00fcrdig h\u00e4lt.", "tokens": ["Weil", "zwey\u00b7er", "En\u00b7gel", "Gunst", "mich", "k\u00fcs\u00b7sens", "w\u00fcr\u00b7dig", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "NN", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch k\u00fcsse sie dabey mit lauter Lust Narcissen/", "tokens": ["Doch", "k\u00fcs\u00b7se", "sie", "da\u00b7bey", "mit", "lau\u00b7ter", "Lust", "Nar\u00b7cis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PAV", "APPR", "PIAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil ihr Vergn\u00fcgen auch auff einen Diener f\u00e4llt.", "tokens": ["Weil", "ihr", "Ver\u00b7gn\u00fc\u00b7gen", "auch", "auff", "ei\u00b7nen", "Die\u00b7ner", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und da\u00df der K\u00fcsse Krafft", "tokens": ["Und", "da\u00df", "der", "K\u00fcs\u00b7se", "Krafft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Den Auszug aller Freuden/", "tokens": ["Den", "Aus\u00b7zug", "al\u00b7ler", "Freu\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und selbst den Himmel schafft/", "tokens": ["Und", "selbst", "den", "Him\u00b7mel", "schafft", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "So lasse mich auff ihren Lippen weiden.", "tokens": ["So", "las\u00b7se", "mich", "auff", "ih\u00b7ren", "Lip\u00b7pen", "wei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Verstattet mir die Gunst/ ihr Cronen Teutscher Sch\u00f6nen!", "tokens": ["Ver\u00b7stat\u00b7tet", "mir", "die", "Gunst", "/", "ihr", "Cro\u00b7nen", "Teut\u00b7scher", "Sch\u00f6\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$(", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und lasset einen Ku\u00df der Treue Siegel seyn.", "tokens": ["Und", "las\u00b7set", "ei\u00b7nen", "Ku\u00df", "der", "Treu\u00b7e", "Sie\u00b7gel", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn also pflegt man sich vertraulich zu bekr\u00f6nen/", "tokens": ["Denn", "al\u00b7so", "pflegt", "man", "sich", "ver\u00b7trau\u00b7lich", "zu", "be\u00b7kr\u00f6\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "PRF", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So schreibt man sich ins Buch getreuer Seelen ein.", "tokens": ["So", "schreibt", "man", "sich", "ins", "Buch", "ge\u00b7treu\u00b7er", "See\u00b7len", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PRF", "APPRART", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So lacht die g\u00fcldne Zeit/", "tokens": ["So", "lacht", "die", "g\u00fcld\u00b7ne", "Zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Aus lauter edlen Hertzen/", "tokens": ["Aus", "lau\u00b7ter", "ed\u00b7len", "Hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und so wird Ewigkeit/", "tokens": ["Und", "so", "wird", "E\u00b7wig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Das Ende seyn/ vergn\u00fcgt und treu zu schertzen.", "tokens": ["Das", "En\u00b7de", "seyn", "/", "ver\u00b7gn\u00fcgt", "und", "treu", "zu", "schert\u00b7zen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAINF", "$(", "VVPP", "KON", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Kommt \u00f6ffnet mir vertraut den Purpur eurer Lippen/", "tokens": ["Kommt", "\u00f6ff\u00b7net", "mir", "ver\u00b7traut", "den", "Pur\u00b7pur", "eu\u00b7rer", "Lip\u00b7pen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "PPER", "ADJD", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ja schliesset nicht vor mir das sch\u00f6ne Rosen-Thor.", "tokens": ["Ja", "schlies\u00b7set", "nicht", "vor", "mir", "das", "sch\u00f6\u00b7ne", "Ro\u00b7sen\u00b7Thor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "PTKNEG", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kein unbelebter Fel\u00df und stumme Marmol-Klippen/", "tokens": ["Kein", "un\u00b7be\u00b7leb\u00b7ter", "Fel\u00df", "und", "stum\u00b7me", "Mar\u00b7mol\u00b7Klip\u00b7pen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gehn an Verschwiegenheit Rosanders Munde vor.", "tokens": ["Gehn", "an", "Ver\u00b7schwie\u00b7gen\u00b7heit", "Ro\u00b7san\u00b7ders", "Mun\u00b7de", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "NE", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Legt alle Sorgen ab/", "tokens": ["Legt", "al\u00b7le", "Sor\u00b7gen", "ab", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Sagt die geheimsten Sachen.", "tokens": ["Sagt", "die", "ge\u00b7heims\u00b7ten", "Sa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sie sollen sich ihr Grab/", "tokens": ["Sie", "sol\u00b7len", "sich", "ihr", "Grab", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Zur Sicherheit in meinen Hertzen machen.", "tokens": ["Zur", "Si\u00b7cher\u00b7heit", "in", "mei\u00b7nen", "Hert\u00b7zen", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ein neid'sches L\u00e4stermaul wird hier verstummen m\u00fcssen/", "tokens": ["Ein", "nei\u00b7d'\u00b7sches", "L\u00e4s\u00b7ter\u00b7maul", "wird", "hier", "ver\u00b7stum\u00b7men", "m\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "VVINF", "VMFIN", "$("], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wir lachen in geheim/ und schweigen \u00f6ffentlich.", "tokens": ["Wir", "la\u00b7chen", "in", "ge\u00b7heim", "/", "und", "schwei\u00b7gen", "\u00f6f\u00b7fent\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJD", "$(", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wir tr\u00e4ncken unsern Mund mit lauter Nectar Fl\u00fcssen/", "tokens": ["Wir", "tr\u00e4n\u00b7cken", "un\u00b7sern", "Mund", "mit", "lau\u00b7ter", "Nec\u00b7tar", "Fl\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PIAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das Gl\u00fccke dienet uns/ die Seelen k\u00fcssen sich.", "tokens": ["Das", "Gl\u00fc\u00b7cke", "die\u00b7net", "uns", "/", "die", "See\u00b7len", "k\u00fcs\u00b7sen", "sich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$(", "ART", "NN", "VVFIN", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dr\u00fcm mu\u00df die Welt gestehn/", "tokens": ["Dr\u00fcm", "mu\u00df", "die", "Welt", "ge\u00b7stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Es sey kein s\u00fcsser Schertzen/", "tokens": ["Es", "sey", "kein", "s\u00fcs\u00b7ser", "Schert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und nichts so wundersch\u00f6n", "tokens": ["Und", "nichts", "so", "wun\u00b7der\u00b7sch\u00f6n"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIS", "ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Als nur allein das Kleeblat treuer Hertzen.", "tokens": ["Als", "nur", "al\u00b7lein", "das", "Klee\u00b7blat", "treu\u00b7er", "Hert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}