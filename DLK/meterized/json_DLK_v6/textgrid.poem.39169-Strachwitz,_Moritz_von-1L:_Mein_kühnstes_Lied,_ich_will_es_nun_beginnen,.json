{"textgrid.poem.39169": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein k\u00fchnstes Lied, ich will es nun beginnen,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein k\u00fchnstes Lied, ich will es nun beginnen,", "tokens": ["Mein", "k\u00fchns\u00b7tes", "Lied", ",", "ich", "will", "es", "nun", "be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es braust hinan, im \u00c4therduft zu baden,", "tokens": ["Es", "braust", "hi\u00b7nan", ",", "im", "\u00c4t\u00b7her\u00b7duft", "zu", "ba\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es quillt empor in sprudelnden Kaskaden,", "tokens": ["Es", "quillt", "em\u00b7por", "in", "spru\u00b7deln\u00b7den", "Kas\u00b7ka\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Melodie ist seiner Welle Rinnen.", "tokens": ["Und", "Me\u00b7lo\u00b7die", "ist", "sei\u00b7ner", "Wel\u00b7le", "Rin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es dreht als Turm um schroffe Wolkenzinnen", "tokens": ["Es", "dreht", "als", "Turm", "um", "schrof\u00b7fe", "Wol\u00b7ken\u00b7zin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOUS", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Wirbelreigen sich der Oreaden,", "tokens": ["Im", "Wir\u00b7bel\u00b7rei\u00b7gen", "sich", "der", "O\u00b7rea\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Es hallt als Glocke in des \u00c4thers Pfaden,", "tokens": ["Es", "hallt", "als", "Glo\u00b7cke", "in", "des", "\u00c4\u00b7thers", "Pfa\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Weit auszul\u00e4uten mein gewalt'ges Minnen.", "tokens": ["Weit", "aus\u00b7zu\u00b7l\u00e4u\u00b7ten", "mein", "ge\u00b7walt'\u00b7ges", "Min\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich bin geliebt! Dir, Meer im Wogensunde,", "tokens": ["Ich", "bin", "ge\u00b7liebt", "!", "Dir", ",", "Meer", "im", "Wo\u00b7gen\u00b7sun\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$.", "PPER", "$,", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Euch blauen L\u00fcften, tobenden Orkanen!", "tokens": ["Euch", "blau\u00b7en", "L\u00fcf\u00b7ten", ",", "to\u00b7ben\u00b7den", "Or\u00b7ka\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Euch ruf' ich's zu mit klanggewalt'gem Munde.", "tokens": ["Euch", "ruf'", "ich's", "zu", "mit", "klang\u00b7ge\u00b7walt'\u00b7gem", "Mun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PTKZU", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Es taucht das All in Liebesozeanen", "tokens": ["Es", "taucht", "das", "All", "in", "Lie\u00b7be\u00b7so\u00b7ze\u00b7a\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und sprengt den Tau auf meine Liebeswunde,", "tokens": ["Und", "sprengt", "den", "Tau", "auf", "mei\u00b7ne", "Lie\u00b7bes\u00b7wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gerissen ist die Fessel des Titanen.", "tokens": ["Ge\u00b7ris\u00b7sen", "ist", "die", "Fes\u00b7sel", "des", "Ti\u00b7ta\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}