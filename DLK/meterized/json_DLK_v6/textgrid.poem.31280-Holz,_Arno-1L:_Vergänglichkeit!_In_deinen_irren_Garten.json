{"textgrid.poem.31280": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Verg\u00e4nglichkeit! In deinen irren Garten", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verg\u00e4nglichkeit! In deinen irren Garten", "tokens": ["Ver\u00b7g\u00e4ng\u00b7lich\u00b7keit", "!", "In", "dei\u00b7nen", "ir\u00b7ren", "Gar\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "verlihrt sich \u00fcmmer tieffer mir mein Fuh\u00df/", "tokens": ["ver\u00b7lihrt", "sich", "\u00fcm\u00b7mer", "tief\u00b7fer", "mir", "mein", "Fuh\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ADJD", "PPER", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "lengst starb des lezzten Fehder-Singers Gruh\u00df/", "tokens": ["lengst", "starb", "des", "lezz\u00b7ten", "Feh\u00b7der\u00b7Sin\u00b7gers", "Gruh\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "der Eppich traumt au\u00df duncklen Mauer-Scharten.", "tokens": ["der", "Ep\u00b7pich", "traumt", "au\u00df", "dunck\u00b7len", "Mau\u00b7e\u00b7rSchar\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Das sonst so zahrte Gra\u00df", "tokens": ["Das", "sonst", "so", "zahr\u00b7te", "Gra\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "ADV", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "h\u00e4ngkt schwehr und Tropffen-na\u00df/", "tokens": ["h\u00e4ngkt", "schwehr", "und", "Tropf\u00b7fen\u00b7na\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "NE", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "gr\u00fcn-fahle Creutze mohdern weit und breit \u2013", "tokens": ["gr\u00fcn\u00b7fah\u00b7le", "Creut\u00b7ze", "moh\u00b7dern", "weit", "und", "breit", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "ADJD", "KON", "ADJD", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.8": {"text": "Verg\u00e4nglichkeit!", "tokens": ["Ver\u00b7g\u00e4ng\u00b7lich\u00b7keit", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Verg\u00e4nglichkeit! W\u00f6lbt sich im Lentz die Linde", "tokens": ["Ver\u00b7g\u00e4ng\u00b7lich\u00b7keit", "!", "W\u00f6lbt", "sich", "im", "Lentz", "die", "Lin\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "VVFIN", "PRF", "APPRART", "NN", "ART", "NE"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "noch \u00fcmmer \u00fcber meines Vatters Tach?", "tokens": ["noch", "\u00fcm\u00b7mer", "\u00fc\u00b7ber", "mei\u00b7nes", "Vat\u00b7ters", "Tach", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Durch Schaum-Kraut klukkerte der kleine Bach/", "tokens": ["Durch", "Schaum\u00b7Kraut", "kluk\u00b7ker\u00b7te", "der", "klei\u00b7ne", "Bach", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "ich schnizzte Schiffgens mir au\u00df Kn\u00fcppel-Rinde.", "tokens": ["ich", "schnizz\u00b7te", "Schiff\u00b7gens", "mir", "au\u00df", "Kn\u00fcp\u00b7pel\u00b7Rin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Do schry mir r\u00e4cht ins Hertz", "tokens": ["Do", "schry", "mir", "r\u00e4cht", "ins", "Hertz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "der Gukguk seine Tertz.", "tokens": ["der", "Guk\u00b7guk", "sei\u00b7ne", "Tertz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich horchte zu; das war die G\u00f6ldne Zeit \u2013", "tokens": ["Ich", "horch\u00b7te", "zu", ";", "das", "war", "die", "G\u00f6ld\u00b7ne", "Zeit", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "PDS", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Verg\u00e4nglichkeit!", "tokens": ["Ver\u00b7g\u00e4ng\u00b7lich\u00b7keit", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Verg\u00e4nglichkeit! Wo blieb die s\u00fcsse Stunde/", "tokens": ["Ver\u00b7g\u00e4ng\u00b7lich\u00b7keit", "!", "Wo", "blieb", "die", "s\u00fcs\u00b7se", "Stun\u00b7de", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWAV", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "do ich mit Fillis unter Bluhmen sa\u00df?", "tokens": ["do", "ich", "mit", "Fil\u00b7lis", "un\u00b7ter", "Bluh\u00b7men", "sa\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NE", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "do ich zuerst der ersten Lust gena\u00df?", "tokens": ["do", "ich", "zu\u00b7erst", "der", "ers\u00b7ten", "Lust", "ge\u00b7na\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich hingk verzukkt an ihrem rohten Munde!", "tokens": ["Ich", "hingk", "ver\u00b7zukkt", "an", "ih\u00b7rem", "roh\u00b7ten", "Mun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ihr Hahr/ gantz auff-gerollt/", "tokens": ["Ihr", "Hahr", "/", "gantz", "auf\u00b7fge\u00b7rollt", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "ADV", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "war wie au\u00df Serafs-Gold/", "tokens": ["war", "wie", "au\u00df", "Se\u00b7rafs\u00b7Gold", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "APPR", "NE", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "wei\u00df wie au\u00df L\u00e4mmer-W\u00f6lckgen war ihr Kleid \u2013", "tokens": ["wei\u00df", "wie", "au\u00df", "L\u00e4m\u00b7mer\u00b7W\u00f6lck\u00b7gen", "war", "ihr", "Kleid", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "APPR", "NN", "VAFIN", "PPOSAT", "NN", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.8": {"text": "Verg\u00e4nglichkeit!", "tokens": ["Ver\u00b7g\u00e4ng\u00b7lich\u00b7keit", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Verg\u00e4nglichkeit! Wie kreisste froh der Becher", "tokens": ["Ver\u00b7g\u00e4ng\u00b7lich\u00b7keit", "!", "Wie", "kreiss\u00b7te", "froh", "der", "Be\u00b7cher"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "PWAV", "VVFIN", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "in drauter Br\u00fcder Wein-\u00fcmblaubtem Rund?", "tokens": ["in", "drau\u00b7ter", "Br\u00fc\u00b7der", "Wein\u00b7\u00fcm\u00b7blaub\u00b7tem", "Rund", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Do schien das Leben mir noch Eins so bundt/", "tokens": ["Do", "schien", "das", "Le\u00b7ben", "mir", "noch", "Eins", "so", "bundt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "ADV", "NN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Printz ", "tokens": ["Printz"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Hier Schellendaus! ", "tokens": ["Hier", "Schel\u00b7len\u00b7daus", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Kriescht alle Gikkrikri!", "tokens": ["Kriescht", "al\u00b7le", "Gik\u00b7krik\u00b7ri", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Wer wei\u00df/ schon morgen ligen wir gemeyt \u2013", "tokens": ["Wer", "wei\u00df", "/", "schon", "mor\u00b7gen", "li\u00b7gen", "wir", "ge\u00b7meyt", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$(", "ADV", "ADV", "VVFIN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Verg\u00e4nglichkeit!", "tokens": ["Ver\u00b7g\u00e4ng\u00b7lich\u00b7keit", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Verg\u00e4nglichkeit! Ich sehe noch das St\u00fcbgen/", "tokens": ["Ver\u00b7g\u00e4ng\u00b7lich\u00b7keit", "!", "Ich", "se\u00b7he", "noch", "das", "St\u00fcb\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "die Lampe br\u00e4nnt/ ans Fenster st\u00fcrmt der Nord/", "tokens": ["die", "Lam\u00b7pe", "br\u00e4nnt", "/", "ans", "Fens\u00b7ter", "st\u00fcrmt", "der", "Nord", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "APPRART", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "du spihlst mir f\u00fcr auff unsrem ", "tokens": ["du", "spihlst", "mir", "f\u00fcr", "auff", "uns\u00b7rem"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "APPR", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "im Traum noch lallt und l\u00e4chelt unser B\u00fcbgen.", "tokens": ["im", "Traum", "noch", "lallt", "und", "l\u00e4\u00b7chelt", "un\u00b7ser", "B\u00fcb\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Itzt lehnstu dich zur\u00fckk/", "tokens": ["Itzt", "lehns\u00b7tu", "dich", "zu\u00b7r\u00fckk", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "so sah mich an das Gl\u00fckk!", "tokens": ["so", "sah", "mich", "an", "das", "Gl\u00fckk", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Im Ofen knallte lustig Scheit \u00fcmb Scheit \u2013", "tokens": ["Im", "O\u00b7fen", "knall\u00b7te", "lus\u00b7tig", "Scheit", "\u00fcmb", "Scheit", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADJD", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Verg\u00e4nglichkeit!", "tokens": ["Ver\u00b7g\u00e4ng\u00b7lich\u00b7keit", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Verg\u00e4nglichkeit! Ein Grauen sonder Gleichen", "tokens": ["Ver\u00b7g\u00e4ng\u00b7lich\u00b7keit", "!", "Ein", "Grau\u00b7en", "son\u00b7der", "Glei\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "durchgrieselt mich; so war ich nie allein.", "tokens": ["durch\u00b7grie\u00b7selt", "mich", ";", "so", "war", "ich", "nie", "al\u00b7lein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "ADV", "VAFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Welt ist nichts al\u00df Schatten-Werck und Schein/", "tokens": ["Die", "Welt", "ist", "nichts", "al\u00df", "Schat\u00b7ten\u00b7\u00b7Werck", "und", "Schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIS", "KOKOM", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "der Grund/ drauffd\u00e4m ich dantzte/ g\u00e4hrt von Leichen!", "tokens": ["der", "Grund", "/", "drauff\u00b7d\u00e4m", "ich", "dantz\u00b7te", "/", "g\u00e4hrt", "von", "Lei\u00b7chen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "VVFIN", "PPER", "VVFIN", "$(", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sie ligen hin-gestr\u00e4kkt/", "tokens": ["Sie", "li\u00b7gen", "hin\u00b7ge\u00b7str\u00e4kkt", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "kaum/ da\u00df der Sand sie d\u00e4kkt/", "tokens": ["kaum", "/", "da\u00df", "der", "Sand", "sie", "d\u00e4kkt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "KOUS", "ART", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "ihr Abseyn s\u00e4lber predigt stumm mein Leid \u2013", "tokens": ["ihr", "Ab\u00b7seyn", "s\u00e4l\u00b7ber", "pre\u00b7digt", "stumm", "mein", "Leid", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "ADJD", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Verg\u00e4nglichkeit!", "tokens": ["Ver\u00b7g\u00e4ng\u00b7lich\u00b7keit", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Verg\u00e4nglichkeit! Du scheussliches Gerippe/", "tokens": ["Ver\u00b7g\u00e4ng\u00b7lich\u00b7keit", "!", "Du", "scheuss\u00b7li\u00b7ches", "Ge\u00b7rip\u00b7pe", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "f\u00fcr dem noch jeder schaudrend sich entsezzt/", "tokens": ["f\u00fcr", "dem", "noch", "je\u00b7der", "schaud\u00b7rend", "sich", "ent\u00b7sezzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "PIS", "ADJD", "PRF", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "du hast mir alle Mitleids-loh\u00df gemezzt/", "tokens": ["du", "hast", "mir", "al\u00b7le", "Mit\u00b7leids\u00b7loh\u00df", "ge\u00b7mezzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "von ihrem Mord-Bluht dr\u00e4uffelt deine Hippe.", "tokens": ["von", "ih\u00b7rem", "Mord\u00b7Bluht", "dr\u00e4uf\u00b7felt", "dei\u00b7ne", "Hip\u00b7pe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Nun schafft mir nur noch Grau\u00df/", "tokens": ["Nun", "schafft", "mir", "nur", "noch", "Grau\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "mein Leib/ dih\u00df Erden-Hau\u00df.", "tokens": ["mein", "Leib", "/", "dih\u00df", "Er\u00b7den\u00b7Hau\u0