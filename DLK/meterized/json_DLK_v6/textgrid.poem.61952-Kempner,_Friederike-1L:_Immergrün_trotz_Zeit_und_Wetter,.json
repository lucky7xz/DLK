{"textgrid.poem.61952": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "1L: Immergr\u00fcn trotz Zeit und Wetter,", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Immergr\u00fcn trotz Zeit und Wetter,", "tokens": ["Im\u00b7mer\u00b7gr\u00fcn", "trotz", "Zeit", "und", "Wet\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Pfl\u00e4nzchen, zart und fest und sch\u00f6n,", "tokens": ["Pfl\u00e4nz\u00b7chen", ",", "zart", "und", "fest", "und", "sch\u00f6n", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Smaragdfarben Deine Bl\u00e4tter,", "tokens": ["Sma\u00b7ragd\u00b7far\u00b7ben", "Dei\u00b7ne", "Bl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00f6nntest bei den sch\u00f6nsten steh'n.", "tokens": ["K\u00f6nn\u00b7test", "bei", "den", "sch\u00f6ns\u00b7ten", "steh'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["VMFIN", "APPR", "ART", "ADJA", "VVFIN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Denn der Freieste von Allen,", "tokens": ["Denn", "der", "Frei\u00b7es\u00b7te", "von", "Al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dessen Blick man nie bestach,", "tokens": ["Des\u00b7sen", "Blick", "man", "nie", "be\u00b7stach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PIS", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rousseau fand an Dir Gefallen,", "tokens": ["Rous\u00b7se\u00b7au", "fand", "an", "Dir", "Ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPER", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "War ger\u00fchrt, wenn er Dich brach.", "tokens": ["War", "ge\u00b7r\u00fchrt", ",", "wenn", "er", "Dich", "brach", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn er Deinen zarten Stengel", "tokens": ["Wenn", "er", "Dei\u00b7nen", "zar\u00b7ten", "Sten\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Selten froh in H\u00e4nden nahm,", "tokens": ["Sel\u00b7ten", "froh", "in", "H\u00e4n\u00b7den", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zagend, forschend, suchend M\u00e4ngel,", "tokens": ["Za\u00b7gend", ",", "for\u00b7schend", ",", "su\u00b7chend", "M\u00e4n\u00b7gel", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "$,", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und zum Vorschein keiner kam. \u2013", "tokens": ["Und", "zum", "Vor\u00b7schein", "kei\u00b7ner", "kam", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "APPRART", "NN", "PIS", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Pfl\u00e4nzchen, liebstes mir von allen,", "tokens": ["Pfl\u00e4nz\u00b7chen", ",", "liebs\u00b7tes", "mir", "von", "al\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "PPER", "APPR", "PIAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ewig bleibst Du teuer mir,", "tokens": ["E\u00b7wig", "bleibst", "Du", "teu\u00b7er", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rousseau konntest Du gefallen:", "tokens": ["Rous\u00b7se\u00b7au", "konn\u00b7test", "Du", "ge\u00b7fal\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "VVPP", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Dank f\u00fcr seine Freuden Dir!", "tokens": ["Dank", "f\u00fcr", "sei\u00b7ne", "Freu\u00b7den", "Dir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PPOSAT", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}