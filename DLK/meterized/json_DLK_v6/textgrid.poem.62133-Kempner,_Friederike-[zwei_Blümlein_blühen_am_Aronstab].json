{"textgrid.poem.62133": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[zwei Bl\u00fcmlein bl\u00fchen am Aronstab]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwei Bl\u00fcmlein bl\u00fchen am Aronstab,", "tokens": ["Zwei", "Bl\u00fcm\u00b7lein", "bl\u00fc\u00b7hen", "am", "A\u00b7ronst\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ach, beide \u00fcberdauern das Grab;", "tokens": ["Ach", ",", "bei\u00b7de", "\u00fc\u00b7berd\u00b7au\u00b7ern", "das", "Grab", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PIS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Das wei\u00dfe liebliche Bl\u00fcmelein,", "tokens": ["Das", "wei\u00b7\u00dfe", "lieb\u00b7li\u00b7che", "Bl\u00fc\u00b7me\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das schenkte mir mein M\u00fctterlein.", "tokens": ["Das", "schenk\u00b7te", "mir", "mein", "M\u00fct\u00b7ter\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mein M\u00fctterlein, so hold und rein,", "tokens": ["Mein", "M\u00fct\u00b7ter\u00b7lein", ",", "so", "hold", "und", "rein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie dieses lichte Bl\u00fcmelein:", "tokens": ["Wie", "die\u00b7ses", "lich\u00b7te", "Bl\u00fc\u00b7me\u00b7lein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr Bl\u00fcmlein \u00fcberdauert das Grab,", "tokens": ["Ihr", "Bl\u00fcm\u00b7lein", "\u00fc\u00b7berd\u00b7au\u00b7ert", "das", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Im Jenseits gr\u00fcnet der Aronstab!", "tokens": ["Im", "Jen\u00b7seits", "gr\u00fc\u00b7net", "der", "A\u00b7ronst\u00b7ab", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}