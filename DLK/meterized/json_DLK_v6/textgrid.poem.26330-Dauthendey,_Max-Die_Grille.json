{"textgrid.poem.26330": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Die Grille", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durchdringend heftig ruft die Grille,", "tokens": ["Durch\u00b7drin\u00b7gend", "hef\u00b7tig", "ruft", "die", "Gril\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "N\u00e4chtlich im Garten leidenschaftlich singend.", "tokens": ["N\u00e4cht\u00b7lich", "im", "Gar\u00b7ten", "lei\u00b7den\u00b7schaft\u00b7lich", "sin\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ADJD", "VVPP", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Im Hintergrund der B\u00e4ume volle Stille,", "tokens": ["Im", "Hin\u00b7ter\u00b7grund", "der", "B\u00e4u\u00b7me", "vol\u00b7le", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und \u00c4ste, hochgereckt wie mit dem Finstern ringend.", "tokens": ["Und", "\u00c4s\u00b7te", ",", "hoch\u00b7ge\u00b7reckt", "wie", "mit", "dem", "Fins\u00b7tern", "rin\u00b7gend", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ADJD", "KOKOM", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und jemand sitzt im Gartengrund versteckt.", "tokens": ["Und", "je\u00b7mand", "sitzt", "im", "Gar\u00b7ten\u00b7grund", "ver\u00b7steckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und jemand pre\u00dft die H\u00e4nde fest zum Mund,", "tokens": ["Und", "je\u00b7mand", "pre\u00dft", "die", "H\u00e4n\u00b7de", "fest", "zum", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vom schrillen Grillenrufe aufgeweckt,", "tokens": ["Vom", "schril\u00b7len", "Gril\u00b7len\u00b7ru\u00b7fe", "auf\u00b7ge\u00b7weckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit einem harten Heimwehschrei im Schlund.", "tokens": ["Mit", "ei\u00b7nem", "har\u00b7ten", "Heim\u00b7weh\u00b7schrei", "im", "Schlund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}