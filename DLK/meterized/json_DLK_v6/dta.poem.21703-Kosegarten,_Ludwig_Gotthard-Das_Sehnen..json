{"dta.poem.21703": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Das Sehnen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1802", "urn": "urn:nbn:de:kobv:b4-200905193310", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wehmuth, die mich h&#252;llt,", "tokens": ["Weh\u00b7muth", ",", "die", "mich", "h", "&#252;", "llt", ","], "token_info": ["word", "punct", "word", "word", "word", "XML_entity", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "VVFIN", "$(", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Welche Gottheit stillt", "tokens": ["Wel\u00b7che", "Got\u00b7theit", "stillt"], "token_info": ["word", "word", "word"], "pos": ["PWAT", "NN", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Mein unendlich Sehnen?", "tokens": ["Mein", "un\u00b7end\u00b7lich", "Seh\u00b7nen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Die ihr meine Wimper n\u00e4sst,", "tokens": ["Die", "ihr", "mei\u00b7ne", "Wim\u00b7per", "n\u00e4sst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Namenlosen Gram entpresst,", "tokens": ["Na\u00b7men\u00b7lo\u00b7sen", "Gram", "ent\u00b7presst", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Fliesset, fliesset Thr\u00e4nen!", "tokens": ["Flies\u00b7set", ",", "flies\u00b7set", "Thr\u00e4\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Mond, der lieb und traut", "tokens": ["Mond", ",", "der", "lieb", "und", "traut"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "ADJD", "KON", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "In mein Fenster schaut,", "tokens": ["In", "mein", "Fens\u00b7ter", "schaut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Sage, was mir fehle?", "tokens": ["Sa\u00b7ge", ",", "was", "mir", "feh\u00b7le", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Sterne, die ihr droben blinkt,", "tokens": ["Ster\u00b7ne", ",", "die", "ihr", "dro\u00b7ben", "blinkt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Holden Gruss mir freundlich winkt,", "tokens": ["Hol\u00b7den", "Gruss", "mir", "freund\u00b7lich", "winkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nennt mir, was mich qu\u00e4le.", "tokens": ["Nennt", "mir", ",", "was", "mich", "qu\u00e4\u00b7le", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Leise Schauer wehn,", "tokens": ["Lei\u00b7se", "Schau\u00b7er", "wehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NE", "VVINF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "S\u00fcsses Liebeflehn", "tokens": ["S\u00fcs\u00b7ses", "Lie\u00b7be\u00b7flehn"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Girrt um mich im D\u00fcstern.", "tokens": ["Girrt", "um", "mich", "im", "D\u00fcs\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PRF", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Rosen und Violenduft", "tokens": ["Ro\u00b7sen", "und", "Vi\u00b7o\u00b7len\u00b7duft"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "W\u00fcrzen rings die Zauberluft.", "tokens": ["W\u00fcr\u00b7zen", "rings", "die", "Zau\u00b7ber\u00b7luft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Holde Stimmen fl\u00fcstern.", "tokens": ["Hol\u00b7de", "Stim\u00b7men", "fl\u00fcs\u00b7tern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "In die Ferne strebt,", "tokens": ["In", "die", "Fer\u00b7ne", "strebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Wie auf Fl\u00fcgeln schwebt", "tokens": ["Wie", "auf", "Fl\u00fc\u00b7geln", "schwebt"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Mein erh\u00f6htes Wesen.", "tokens": ["Mein", "er\u00b7h\u00f6h\u00b7tes", "We\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Fremder Zug, geheime Kraft,", "tokens": ["Frem\u00b7der", "Zug", ",", "ge\u00b7hei\u00b7me", "Kraft", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Namenlose Leidenschaft,", "tokens": ["Na\u00b7men\u00b7lo\u00b7se", "Lei\u00b7den\u00b7schaft", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Lass, ach lass genesen!", "tokens": ["Lass", ",", "ach", "lass", "ge\u00b7ne\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ITJ", "VVFIN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "\u00c4ngstender beklemmt", "tokens": ["\u00c4ngs\u00b7ten\u00b7der", "be\u00b7klemmt"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Mich die Wehmuth, hemmt", "tokens": ["Mich", "die", "Weh\u00b7muth", ",", "hemmt"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPER", "ART", "NN", "$,", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Athem mir und Rede.", "tokens": ["A\u00b7them", "mir", "und", "Re\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KON", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Einsam schmachten, o der Pein!", "tokens": ["Ein\u00b7sam", "schmach\u00b7ten", ",", "o", "der", "Pein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "FM", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "O des Grams, allein zu seyn", "tokens": ["O", "des", "Grams", ",", "al\u00b7lein", "zu", "seyn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ART", "NN", "$,", "ADV", "PTKZU", "VAINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "In des Lebens \u00d6de.", "tokens": ["In", "des", "Le\u00b7bens", "\u00d6\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Ist denn ach kein Arm,", "tokens": ["Ist", "denn", "ach", "kein", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Der in Freud' und Harm", "tokens": ["Der", "in", "Freud'", "und", "Harm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "KON", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Liebend mich umschl\u00f6sse?", "tokens": ["Lie\u00b7bend", "mich", "um\u00b7schl\u00f6s\u00b7se", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ist denn ach kein f\u00fchlend Herz,", "tokens": ["Ist", "denn", "ach", "kein", "f\u00fch\u00b7lend", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PIAT", "CARD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Keines, drinn in Lust und Schmerz", "tokens": ["Kei\u00b7nes", ",", "drinn", "in", "Lust", "und", "Schmerz"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "$,", "ADV", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Meines sich erg\u00f6sse?", "tokens": ["Mei\u00b7nes", "sich", "er\u00b7g\u00f6s\u00b7se", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "PRF", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Die ihr einsam klagt,", "tokens": ["Die", "ihr", "ein\u00b7sam", "klagt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Einsam, wenn es tagt,", "tokens": ["Ein\u00b7sam", ",", "wenn", "es", "tagt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Einsam wenn es nachtet,", "tokens": ["Ein\u00b7sam", "wenn", "es", "nach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOUS", "PPER", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ungetr\u00f6stet ach ver\u00e4chzt", "tokens": ["Un\u00b7ge\u00b7tr\u00f6s\u00b7tet", "ach", "ver\u00b7\u00e4chzt"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADV", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ihr das holde Daseyn, lechzt,", "tokens": ["Ihr", "das", "hol\u00b7de", "Da\u00b7seyn", ",", "lechzt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Schmachtet und verschmachtet.", "tokens": ["Schmach\u00b7tet", "und", "ver\u00b7schmach\u00b7tet", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}