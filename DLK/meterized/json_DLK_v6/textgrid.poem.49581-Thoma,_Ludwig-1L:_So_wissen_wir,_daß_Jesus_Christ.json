{"textgrid.poem.49581": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: So wissen wir, da\u00df Jesus Christ", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So wissen wir, da\u00df Jesus Christ", "tokens": ["So", "wis\u00b7sen", "wir", ",", "da\u00df", "Je\u00b7sus", "Christ"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In einem Stall geboren ist", "tokens": ["In", "ei\u00b7nem", "Stall", "ge\u00b7bo\u00b7ren", "ist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVPP", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu Bethlehem bei kalter Nacht.", "tokens": ["Zu", "Beth\u00b7le\u00b7hem", "bei", "kal\u00b7ter", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kein Reicher hat nicht aufgemacht.", "tokens": ["Kein", "Rei\u00b7cher", "hat", "nicht", "auf\u00b7ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die lagen all im weichen Bett.", "tokens": ["Die", "la\u00b7gen", "all", "im", "wei\u00b7chen", "Bett", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df auf der harten Liegerst\u00e4tt'", "tokens": ["Da\u00df", "auf", "der", "har\u00b7ten", "Lie\u00b7ger\u00b7st\u00e4tt'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Kindlein in der Krippe fror,", "tokens": ["Das", "Kin\u00b7dlein", "in", "der", "Krip\u00b7pe", "fror", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kam ihnen nicht betr\u00fcbsam vor.", "tokens": ["Kam", "ih\u00b7nen", "nicht", "be\u00b7tr\u00fcb\u00b7sam", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PTKNEG", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie hielten es f\u00fcr gar gering,", "tokens": ["Sie", "hiel\u00b7ten", "es", "f\u00fcr", "gar", "ge\u00b7ring", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie da\u00df es kleinen Leuten ging.", "tokens": ["Wie", "da\u00df", "es", "klei\u00b7nen", "Leu\u00b7ten", "ging", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was geht sie ", "tokens": ["Was", "geht", "sie"], "token_info": ["word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Nur Armen ward es kundgetan.", "tokens": ["Nur", "Ar\u00b7men", "ward", "es", "kund\u00b7ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}