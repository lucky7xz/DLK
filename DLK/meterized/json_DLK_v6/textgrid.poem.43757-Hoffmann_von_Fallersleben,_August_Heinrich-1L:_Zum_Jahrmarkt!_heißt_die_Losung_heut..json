{"textgrid.poem.43757": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zum Jahrmarkt! hei\u00dft die Losung heut.", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zum Jahrmarkt! hei\u00dft die Losung heut.", "tokens": ["Zum", "Jahr\u00b7markt", "!", "hei\u00dft", "die", "Lo\u00b7sung", "heut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Eisele, beisele, bum!", "tokens": ["Ei\u00b7se\u00b7le", ",", "bei\u00b7se\u00b7le", ",", "bum", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "XY", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Drauf haben wir uns lang gefreut.", "tokens": ["Drauf", "ha\u00b7ben", "wir", "uns", "lang", "ge\u00b7freut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "PRF", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da kann man kaufen, h\u00f6ren, seh'n,", "tokens": ["Da", "kann", "man", "kau\u00b7fen", ",", "h\u00f6\u00b7ren", ",", "seh'n", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "VVINF", "$,", "VVINF", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und ohne Geld nach Hause geh'n.", "tokens": ["Und", "oh\u00b7ne", "Geld", "nach", "Hau\u00b7se", "geh'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "APPR", "NN", "APPR", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Krambuden hier in langen Reih'n!", "tokens": ["Kram\u00b7bu\u00b7den", "hier", "in", "lan\u00b7gen", "Reih'n", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Kr\u00e4mer schrei'n: kauft ein! kauft ein!", "tokens": ["Die", "Kr\u00e4\u00b7mer", "schrei'n", ":", "kauft", "ein", "!", "kauft", "ein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$.", "VVFIN", "PTKVZ", "$.", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gedr\u00e4ng und L\u00e4rm, Musik und Sang \u2013", "tokens": ["Ge\u00b7dr\u00e4ng", "und", "L\u00e4rm", ",", "Mu\u00b7sik", "und", "Sang", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sollt Einem werden angst und bang.", "tokens": ["Sollt", "Ei\u00b7nem", "wer\u00b7den", "angst", "und", "bang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "VAFIN", "VVPP", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Im Adler zapft man neuen Wein,", "tokens": ["Im", "Ad\u00b7ler", "zapft", "man", "neu\u00b7en", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PIS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Tanz soll im wilden Manne sein \u2013", "tokens": ["Tanz", "soll", "im", "wil\u00b7den", "Man\u00b7ne", "sein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "APPRART", "ADJA", "NN", "VAINF", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Was hat es weiter denn f\u00fcr Noth?", "tokens": ["Was", "hat", "es", "wei\u00b7ter", "denn", "f\u00fcr", "Noth", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im wei\u00dfen Ro\u00df ist Table d'hote.", "tokens": ["Im", "wei\u00b7\u00dfen", "Ro\u00df", "ist", "Tab\u00b7le", "d'\u00b7ho\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "NE", "NE", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Kunstreiter auf dem Rasen dort!", "tokens": ["Kuns\u00b7trei\u00b7ter", "auf", "dem", "Ra\u00b7sen", "dort", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "H\u00f6rt, wie sie blasen immerfort!", "tokens": ["H\u00f6rt", ",", "wie", "sie", "bla\u00b7sen", "im\u00b7mer\u00b7fort", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hanswurst macht seine Possen auch", "tokens": ["Hans\u00b7wurst", "macht", "sei\u00b7ne", "Pos\u00b7sen", "auch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und h\u00e4lt vor Lachen sich den Bauch.", "tokens": ["Und", "h\u00e4lt", "vor", "La\u00b7chen", "sich", "den", "Bauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Zum Lottotisch heran! heran!", "tokens": ["Zum", "Lot\u00b7to\u00b7tisch", "he\u00b7ran", "!", "he\u00b7ran", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$.", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Setzt ihr nur Einen Batzen dran,", "tokens": ["Setzt", "ihr", "nur", "Ei\u00b7nen", "Bat\u00b7zen", "dran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gewinnt ihr Porzellan und Glas", "tokens": ["Ge\u00b7winnt", "ihr", "Por\u00b7zel\u00b7lan", "und", "Glas"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und habt f\u00fcr einen Gulden Spa\u00df.", "tokens": ["Und", "habt", "f\u00fcr", "ei\u00b7nen", "Gul\u00b7den", "Spa\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Hier ist zu seh'n, ganz wundernett!", "tokens": ["Hier", "ist", "zu", "seh'n", ",", "ganz", "wun\u00b7der\u00b7nett", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKZU", "VVINF", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Wachsfigurenkabinet:", "tokens": ["Ein", "Wachs\u00b7fi\u00b7gu\u00b7ren\u00b7ka\u00b7bi\u00b7net", ":"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Potentaten aller Welt", "tokens": ["Die", "Po\u00b7ten\u00b7ta\u00b7ten", "al\u00b7ler", "Welt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und heut ums halbe Eintrittsgeld!", "tokens": ["Und", "heut", "ums", "hal\u00b7be", "Ein\u00b7tritts\u00b7geld", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "O, welch Vergn\u00fcgen aller Art!", "tokens": ["O", ",", "welch", "Ver\u00b7gn\u00fc\u00b7gen", "al\u00b7ler", "Art", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAT", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wer sein Geld noch hat gespart,", "tokens": ["Und", "wer", "sein", "Geld", "noch", "hat", "ge\u00b7spart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPOSAT", "NN", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der kann's verreiten hier gar schnell:", "tokens": ["Der", "kann's", "ver\u00b7rei\u00b7ten", "hier", "gar", "schnell", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er setzt sich auf das Carroussel.", "tokens": ["Er", "setzt", "sich", "auf", "das", "Car\u00b7rous\u00b7sel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}