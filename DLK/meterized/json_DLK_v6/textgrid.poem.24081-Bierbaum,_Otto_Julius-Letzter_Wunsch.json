{"textgrid.poem.24081": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Letzter Wunsch", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df deine Hand auf meiner Stirne liegt,", "tokens": ["Da\u00df", "dei\u00b7ne", "Hand", "auf", "mei\u00b7ner", "Stir\u00b7ne", "liegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wenn mich das Sterben in der Wiege wiegt,", "tokens": ["Wenn", "mich", "das", "Ster\u00b7ben", "in", "der", "Wie\u00b7ge", "wiegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die leis hin\u00fcber ins Vergessen schaukelt,", "tokens": ["Die", "leis", "hin\u00b7\u00fc\u00b7ber", "ins", "Ver\u00b7ges\u00b7sen", "schau\u00b7kelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von schwarzen Schmetterlingen schwer umgaukelt.", "tokens": ["Von", "schwar\u00b7zen", "Schmet\u00b7ter\u00b7lin\u00b7gen", "schwer", "um\u00b7gau\u00b7kelt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein letzter Blick in deine braunen Sonnen:", "tokens": ["Ein", "letz\u00b7ter", "Blick", "in", "dei\u00b7ne", "brau\u00b7nen", "Son\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Vor\u00fcber str\u00f6men alle unsre Wonnen", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "str\u00f6\u00b7men", "al\u00b7le", "uns\u00b7re", "Won\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "In einer bitter-s\u00fc\u00dfen Letztsekunde;", "tokens": ["In", "ei\u00b7ner", "bit\u00b7ter\u00b7s\u00fc\u00b7\u00dfen", "Letz\u00b7tse\u00b7kun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ein letzter Ku\u00df von deinem warmen Munde,", "tokens": ["Ein", "letz\u00b7ter", "Ku\u00df", "von", "dei\u00b7nem", "war\u00b7men", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ein letztes Wort von dir, so liebeweich:", "tokens": ["Ein", "letz\u00b7tes", "Wort", "von", "dir", ",", "so", "lie\u00b7be\u00b7weich", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Dann hab ich, eh ich tot, das Himmelreich,", "tokens": ["Dann", "hab", "ich", ",", "eh", "ich", "tot", ",", "das", "Him\u00b7mel\u00b7reich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "KOUS", "PPER", "ADJD", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und tauche selig in den gro\u00dfen Frieden:", "tokens": ["Und", "tau\u00b7che", "se\u00b7lig", "in", "den", "gro\u00b7\u00dfen", "Frie\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Der Erde Holdestes war mir beschieden.", "tokens": ["Der", "Er\u00b7de", "Hol\u00b7des\u00b7tes", "war", "mir", "be\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}}}}}