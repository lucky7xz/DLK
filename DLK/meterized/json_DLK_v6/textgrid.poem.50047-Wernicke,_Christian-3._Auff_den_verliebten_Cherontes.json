{"textgrid.poem.50047": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "3. Auff den verliebten Cherontes", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Cherontes ", "tokens": ["Cher\u00b7on\u00b7tes"], "token_info": ["word"], "pos": ["NE"], "meter": "+--", "measure": "dactylic.init"}, "line.2": {"text": "Hegt solch ein Feuer nicht, als ihm die Adern f\u00fcllet.", "tokens": ["Hegt", "solch", "ein", "Feu\u00b7er", "nicht", ",", "als", "ihm", "die", "A\u00b7dern", "f\u00fcl\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ART", "NN", "PTKNEG", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er brennt! weil in der ", "tokens": ["Er", "brennt", "!", "weil", "in", "der"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "KOUS", "APPR", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Gleich als im ", "tokens": ["Gleich", "als", "im"], "token_info": ["word", "word", "word"], "pos": ["ADV", "KOKOM", "APPRART"], "meter": "+--", "measure": "dactylic.init"}, "line.5": {"text": "Als w\u00e4r's ", "tokens": ["Als", "w\u00e4r's"], "token_info": ["word", "word"], "pos": ["KOUS", "NE"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Kurtz: denn was dienet mir zu meinem Kleinen Zweck", "tokens": ["Kurtz", ":", "denn", "was", "die\u00b7net", "mir", "zu", "mei\u00b7nem", "Klei\u00b7nen", "Zweck"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$.", "KON", "PWS", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So vieler Worte Pracht: Cherontes ist ", "tokens": ["So", "vie\u00b7ler", "Wor\u00b7te", "Pracht", ":", "Cher\u00b7on\u00b7tes", "ist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PIAT", "NN", "NN", "$.", "NE", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}