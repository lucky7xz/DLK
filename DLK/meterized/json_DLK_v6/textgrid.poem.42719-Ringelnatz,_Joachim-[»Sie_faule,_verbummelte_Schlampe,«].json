{"textgrid.poem.42719": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "[\u00bbSie faule, verbummelte Schlampe,\u00ab]", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbsie faule, verbummelte Schlampe,\u00ab", "tokens": ["\u00bb", "sie", "fau\u00b7le", ",", "ver\u00b7bum\u00b7mel\u00b7te", "Schlam\u00b7pe", ",", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "ADJA", "NN", "$,", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Sagte der Spiegel zur Lampe.", "tokens": ["Sag\u00b7te", "der", "Spie\u00b7gel", "zur", "Lam\u00b7pe", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "\u00bbsie altes, schmieriges Scherbenst\u00fcck,\u00ab", "tokens": ["\u00bb", "sie", "al\u00b7tes", ",", "schmie\u00b7ri\u00b7ges", "Scher\u00b7ben\u00b7st\u00fcck", ",", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "ADJA", "$,", "ADJA", "NN", "$,", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Gab die Lampe dem Spiegel zur\u00fcck.", "tokens": ["Gab", "die", "Lam\u00b7pe", "dem", "Spie\u00b7gel", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Der Spiegel in seiner Erbitterung", "tokens": ["Der", "Spie\u00b7gel", "in", "sei\u00b7ner", "Er\u00b7bit\u00b7te\u00b7rung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Bekam einen ganz gewaltigen Sprung.", "tokens": ["Be\u00b7kam", "ei\u00b7nen", "ganz", "ge\u00b7wal\u00b7ti\u00b7gen", "Sprung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADV", "ADJA", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Der zornigen Lampe verging die Puste.", "tokens": ["Der", "zor\u00b7ni\u00b7gen", "Lam\u00b7pe", "ver\u00b7ging", "die", "Pus\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Sie fauchte, rauchte, schwelte und ru\u00dfte.", "tokens": ["Sie", "fauch\u00b7te", ",", "rauch\u00b7te", ",", "schwel\u00b7te", "und", "ru\u00df\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Das Stubenm\u00e4dchen lie\u00df beide in Ruhe", "tokens": ["Das", "Stu\u00b7ben\u00b7m\u00e4d\u00b7chen", "lie\u00df", "bei\u00b7de", "in", "Ru\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PIS", "APPR", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Und doch: Ihr schob man die Schuld in die Schuhe.", "tokens": ["Und", "doch", ":", "Ihr", "schob", "man", "die", "Schuld", "in", "die", "Schu\u00b7he", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "PPER", "VVFIN", "PIS", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}}}}