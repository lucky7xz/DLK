{"textgrid.poem.65124": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Sehnsucht, die gen Himmel weis't,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sehnsucht, die gen Himmel weis't,", "tokens": ["Die", "Sehn\u00b7sucht", ",", "die", "gen", "Him\u00b7mel", "weis't", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Umrauscht mein Haupt mit dunkeln Schwingen,", "tokens": ["Um\u00b7rauscht", "mein", "Haupt", "mit", "dun\u00b7keln", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Seh' ich auf deinem Bild den Geist", "tokens": ["Seh'", "ich", "auf", "dei\u00b7nem", "Bild", "den", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So schmerzlich mit dem Stoffe ringen.", "tokens": ["So", "schmerz\u00b7lich", "mit", "dem", "Stof\u00b7fe", "rin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Sch\u00f6nheit ewig reinen Strahl", "tokens": ["Der", "Sch\u00f6n\u00b7heit", "e\u00b7wig", "rei\u00b7nen", "Strahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein inn'res Aug' hatt' ihn getrunken,", "tokens": ["Dein", "inn'\u00b7res", "Aug'", "hatt'", "ihn", "ge\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vor einem heil'gen Ideal", "tokens": ["Vor", "ei\u00b7nem", "heil'\u00b7gen", "I\u00b7deal"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Warst auf die Kniee du gesunken!", "tokens": ["Warst", "auf", "die", "Kni\u00b7ee", "du", "ge\u00b7sun\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und hei\u00dfe Inbrunst trieb dich an", "tokens": ["Und", "hei\u00b7\u00dfe", "In\u00b7brunst", "trieb", "dich", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die himmlisch herrlichen Gestalten", "tokens": ["Die", "himm\u00b7lisch", "herr\u00b7li\u00b7chen", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit deines Geistes starkem Bann", "tokens": ["Mit", "dei\u00b7nes", "Geis\u00b7tes", "star\u00b7kem", "Bann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr alle Zeiten festzuhalten.", "tokens": ["F\u00fcr", "al\u00b7le", "Zei\u00b7ten", "fest\u00b7zu\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du sahst ihn wohl den ew'gen Kranz,", "tokens": ["Du", "sahst", "ihn", "wohl", "den", "ew'\u00b7gen", "Kranz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es war kein Wahn, der dich beseelte,", "tokens": ["Es", "war", "kein", "Wahn", ",", "der", "dich", "be\u00b7seel\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Wille war, die Kraft war ganz,", "tokens": ["Der", "Wil\u00b7le", "war", ",", "die", "Kraft", "war", "ganz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "ART", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Allein, das ird'sche Werkzeug fehlte!", "tokens": ["Al\u00b7lein", ",", "das", "ird'\u00b7sche", "Werk\u00b7zeug", "fehl\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Versagt blieb dir's, in G\u00f6tterruh'", "tokens": ["Ver\u00b7sagt", "blieb", "dir's", ",", "in", "G\u00f6t\u00b7ter\u00b7ruh'"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVPP", "VVFIN", "PIS", "$,", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und G\u00f6tterglanz dein Bild zu tauchen,", "tokens": ["Und", "G\u00f6t\u00b7ter\u00b7glanz", "dein", "Bild", "zu", "tau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nur deine Sehnsucht wu\u00dftest du,", "tokens": ["Nur", "dei\u00b7ne", "Sehn\u00b7sucht", "wu\u00df\u00b7test", "du", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur deinen Schmerz ihm einzuhauchen.", "tokens": ["Nur", "dei\u00b7nen", "Schmerz", "ihm", "ein\u00b7zu\u00b7hau\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und ob die Welt sich dann erbaut,", "tokens": ["Und", "ob", "die", "Welt", "sich", "dann", "er\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nie konnte dir dein Werk gen\u00fcgen,", "tokens": ["Nie", "konn\u00b7te", "dir", "dein", "Werk", "ge\u00b7n\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Denn H\u00f6h'res hattest du geschaut", "tokens": ["Denn", "H\u00f6h'\u00b7res", "hat\u00b7test", "du", "ge\u00b7schaut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf deines Geistes Wanderz\u00fcgen!", "tokens": ["Auf", "dei\u00b7nes", "Geis\u00b7tes", "Wan\u00b7der\u00b7z\u00fc\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "O du gemahnst mich wie ein Kind,", "tokens": ["O", "du", "ge\u00b7mahnst", "mich", "wie", "ein", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "PPER", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das, von Erwachens Weh umd\u00fcstert,", "tokens": ["Das", ",", "von", "Er\u00b7wa\u00b7chens", "Weh", "um\u00b7d\u00fcs\u00b7tert", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "APPR", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vergeblich sich darauf besinnt,", "tokens": ["Ver\u00b7geb\u00b7lich", "sich", "da\u00b7rauf", "be\u00b7sinnt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was ihm sein Schutzgeist zugefl\u00fcstert.", "tokens": ["Was", "ihm", "sein", "Schutz\u00b7geist", "zu\u00b7ge\u00b7fl\u00fcs\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}