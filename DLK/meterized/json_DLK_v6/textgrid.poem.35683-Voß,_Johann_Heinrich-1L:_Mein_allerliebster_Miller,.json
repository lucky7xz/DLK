{"textgrid.poem.35683": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein allerliebster Miller,", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein allerliebster Miller,", "tokens": ["Mein", "al\u00b7ler\u00b7liebs\u00b7ter", "Mil\u00b7ler", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wer hat dich Ton und Triller", "tokens": ["Wer", "hat", "dich", "Ton", "und", "Tril\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So silberrein gelehrt,", "tokens": ["So", "sil\u00b7ber\u00b7rein", "ge\u00b7lehrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df nur auf dich die Sch\u00f6ne,", "tokens": ["Da\u00df", "nur", "auf", "dich", "die", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und nimmer auf die T\u00f6ne", "tokens": ["Und", "nim\u00b7mer", "auf", "die", "T\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Des armen Bruders h\u00f6rt?", "tokens": ["Des", "ar\u00b7men", "Bru\u00b7ders", "h\u00f6rt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Singst du nur ganz gew\u00f6hnlich;", "tokens": ["Singst", "du", "nur", "ganz", "ge\u00b7w\u00f6hn\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie zauberst du! Wie sehnlich", "tokens": ["Wie", "zau\u00b7berst", "du", "!", "Wie", "sehn\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "$.", "PWAV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Err\u00f6tet dir das Kind!", "tokens": ["Er\u00b7r\u00f6\u00b7tet", "dir", "das", "Kind", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sie f\u00e4ngt sich an zu f\u00e4cheln,", "tokens": ["Sie", "f\u00e4ngt", "sich", "an", "zu", "f\u00e4\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APZR", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und spricht, mit scheuem L\u00e4cheln:", "tokens": ["Und", "spricht", ",", "mit", "scheu\u00b7em", "L\u00e4\u00b7cheln", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ei! wie Sie lose sind!", "tokens": ["Ei", "!", "wie", "Sie", "lo\u00b7se", "sind", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWAV", "PPER", "VVFIN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch wenn der Obotrite", "tokens": ["Doch", "wenn", "der", "O\u00b7bot\u00b7ri\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sich noch so sehr bem\u00fchte,", "tokens": ["Sich", "noch", "so", "sehr", "be\u00b7m\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Mienchen zu erflehn;", "tokens": ["Ein", "Mien\u00b7chen", "zu", "er\u00b7flehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So spricht das M\u00e4dchen schimpfend,", "tokens": ["So", "spricht", "das", "M\u00e4d\u00b7chen", "schimp\u00b7fend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die kleine Nase r\u00fcmpfend:", "tokens": ["Die", "klei\u00b7ne", "Na\u00b7se", "r\u00fcmp\u00b7fend", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das kann kein Mensch verstehn!", "tokens": ["Das", "kann", "kein", "Mensch", "ver\u00b7stehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "O lehre mich Selinden", "tokens": ["O", "leh\u00b7re", "mich", "Se\u00b7lin\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Doch endlich \u00fcberwinden,", "tokens": ["Doch", "end\u00b7lich", "\u00fc\u00b7berw\u00b7in\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die unerbittlich ist!", "tokens": ["Die", "un\u00b7er\u00b7bitt\u00b7lich", "ist", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sie soll mit zwanzig K\u00fcssen", "tokens": ["Sie", "soll", "mit", "zwan\u00b7zig", "K\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dich einst bezahlen m\u00fcssen!", "tokens": ["Dich", "einst", "be\u00b7zah\u00b7len", "m\u00fcs\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "VMINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Doch wenn du sittsam bist.", "tokens": ["Doch", "wenn", "du", "sitt\u00b7sam", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}