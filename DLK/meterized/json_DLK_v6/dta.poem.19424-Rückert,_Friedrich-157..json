{"dta.poem.19424": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "157.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Unsterblichkeit ist nicht der Zukunft aufgespart,", "tokens": ["U\u00b7nsterb\u00b7lich\u00b7keit", "ist", "nicht", "der", "Zu\u00b7kunft", "auf\u00b7ge\u00b7spart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PTKNEG", "ART", "NN", "VVPP", "$,"], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Unsterblichkeit ist im Gef\u00fchl der Gegenwart.", "tokens": ["U\u00b7nsterb\u00b7lich\u00b7keit", "ist", "im", "Ge\u00b7f\u00fchl", "der", "Ge\u00b7gen\u00b7wart", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}}, "stanza.2": {"line.1": {"text": "Du w\u00e4rst nicht, der du bist, in diesem Nu der Zeit,", "tokens": ["Du", "w\u00e4rst", "nicht", ",", "der", "du", "bist", ",", "in", "die\u00b7sem", "Nu", "der", "Zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "$,", "PRELS", "PPER", "VAFIN", "$,", "APPR", "PDAT", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn du derselbige nicht w\u00e4rst in Ewigkeit.", "tokens": ["Wenn", "du", "der\u00b7sel\u00b7bi\u00b7ge", "nicht", "w\u00e4rst", "in", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "PTKNEG", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sobald du denken willst, du w\u00e4rest nicht mehr einst,", "tokens": ["So\u00b7bald", "du", "den\u00b7ken", "willst", ",", "du", "w\u00e4\u00b7rest", "nicht", "mehr", "einst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "$,", "PPER", "VAFIN", "PTKNEG", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So f\u00fchlst du, da\u00df du dich insoweit selbst verneinst.", "tokens": ["So", "f\u00fchlst", "du", ",", "da\u00df", "du", "dich", "in\u00b7so\u00b7weit", "selbst", "ver\u00b7neinst", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PRF", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Verneine nur dis Nein! dazu hast du empfahen", "tokens": ["Ver\u00b7nei\u00b7ne", "nur", "dis", "Nein", "!", "da\u00b7zu", "hast", "du", "em\u00b7pfa\u00b7hen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PDS", "PTKANT", "$.", "PAV", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Geistes Kraft allein, dich ewig zu bejahen.", "tokens": ["Des", "Geis\u00b7tes", "Kraft", "al\u00b7lein", ",", "dich", "e\u00b7wig", "zu", "be\u00b7ja\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "$,", "PRF", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}