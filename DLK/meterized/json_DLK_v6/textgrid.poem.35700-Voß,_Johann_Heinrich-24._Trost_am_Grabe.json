{"textgrid.poem.35700": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "24. Trost am Grabe", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Trockne deines Jammers Thr\u00e4nen,", "tokens": ["Trock\u00b7ne", "dei\u00b7nes", "Jam\u00b7mers", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heitre deinen Blick;", "tokens": ["Heit\u00b7re", "dei\u00b7nen", "Blick", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Denn es bringt kein banges Sehnen", "tokens": ["Denn", "es", "bringt", "kein", "ban\u00b7ges", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihn, der starb, zur\u00fcck.", "tokens": ["Ihn", ",", "der", "starb", ",", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "VVFIN", "$,", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Ach, die holde Stimm' und Rede,", "tokens": ["Ach", ",", "die", "hol\u00b7de", "Stimm'", "und", "Re\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und der Lieblichkeiten jede,", "tokens": ["Und", "der", "Lieb\u00b7lich\u00b7kei\u00b7ten", "je\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PIAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und sein freundliches Gesicht", "tokens": ["Und", "sein", "freund\u00b7li\u00b7ches", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ruht im Grab', und kehret nicht.", "tokens": ["Ruht", "im", "Grab'", ",", "und", "keh\u00b7ret", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,", "KON", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Gleich des Feldes Blumen schwindet", "tokens": ["Gleich", "des", "Fel\u00b7des", "Blu\u00b7men", "schwin\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alles Fleisch umher;", "tokens": ["Al\u00b7les", "Fleisch", "um\u00b7her", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Traurend sucht der Freund, und findet", "tokens": ["Trau\u00b7rend", "sucht", "der", "Freund", ",", "und", "fin\u00b7det"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seinen Freund nicht mehr:", "tokens": ["Sei\u00b7nen", "Freund", "nicht", "mehr", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Vor dem welken Greis' am Stabe", "tokens": ["Vor", "dem", "wel\u00b7ken", "Greis'", "am", "Sta\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NE", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sinkt der J\u00fcngling und der Knabe,", "tokens": ["Sinkt", "der", "J\u00fcng\u00b7ling", "und", "der", "Kna\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Vor der Mutter sinkt ins Grab", "tokens": ["Vor", "der", "Mut\u00b7ter", "sinkt", "ins", "Grab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Oft die junge Braut hinab.", "tokens": ["Oft", "die", "jun\u00b7ge", "Braut", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Gleich des Feldes Blumen werde", "tokens": ["Gleich", "des", "Fel\u00b7des", "Blu\u00b7men", "wer\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "NN", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alles Fleisch verst\u00e4ubt!", "tokens": ["Al\u00b7les", "Fleisch", "ver\u00b7st\u00e4ubt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Nur der Erdenleib wird Erde;", "tokens": ["Nur", "der", "Er\u00b7den\u00b7leib", "wird", "Er\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sein Bewohner bleibt!", "tokens": ["Sein", "Be\u00b7woh\u00b7ner", "bleibt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Ja du lebst, Geliebter, lebest", "tokens": ["Ja", "du", "lebst", ",", "Ge\u00b7lieb\u00b7ter", ",", "le\u00b7best"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PTKANT", "PPER", "VVFIN", "$,", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "\u00dcber Sternen, oder schwebest", "tokens": ["\u00dc\u00b7ber", "Ster\u00b7nen", ",", "o\u00b7der", "schwe\u00b7best"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Mitleidsvoll um deinen Freund,", "tokens": ["Mit\u00b7le\u00b7ids\u00b7voll", "um", "dei\u00b7nen", "Freund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Der an deinem Grabe weint!", "tokens": ["Der", "an", "dei\u00b7nem", "Gra\u00b7be", "weint", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Diese Kr\u00e4fte, dieses Trachten", "tokens": ["Die\u00b7se", "Kr\u00e4f\u00b7te", ",", "die\u00b7ses", "Trach\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PDAT", "NN", "$,", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zur Vollkommenheit,", "tokens": ["Zur", "Voll\u00b7kom\u00b7men\u00b7heit", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Dieses Vorgef\u00fchl, dies Schmachten", "tokens": ["Die\u00b7ses", "Vor\u00b7ge\u00b7f\u00fchl", ",", "dies", "Schmach\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PDAT", "NN", "$,", "PDS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nach Unsterblichkeit:", "tokens": ["Nach", "U\u00b7nsterb\u00b7lich\u00b7keit", ":"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Dieser Geist, der Welten denket,", "tokens": ["Die\u00b7ser", "Geist", ",", "der", "Wel\u00b7ten", "den\u00b7ket", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "W\u00fcrde mit ins Grab gesenket?", "tokens": ["W\u00fcr\u00b7de", "mit", "ins", "Grab", "ge\u00b7sen\u00b7ket", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und geschaffen h\u00e4tte Gott", "tokens": ["Und", "ge\u00b7schaf\u00b7fen", "h\u00e4t\u00b7te", "Gott"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVPP", "VAFIN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Dieses alles nur zum Spott?", "tokens": ["Die\u00b7ses", "al\u00b7les", "nur", "zum", "Spott", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Nein, nicht spottend, nicht vergebens", "tokens": ["Nein", ",", "nicht", "spot\u00b7tend", ",", "nicht", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PTKANT", "$,", "PTKNEG", "ADJD", "$,", "PTKNEG", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schufst du, Gott, dein Bild;", "tokens": ["Schufst", "du", ",", "Gott", ",", "dein", "Bild", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Lieb' und Weisheit hat des Lebens", "tokens": ["Lieb'", "und", "Weis\u00b7heit", "hat", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Geist in Staub geh\u00fcllt.", "tokens": ["Geist", "in", "Staub", "ge\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Diese H\u00fclle wird zertr\u00fcmmert,", "tokens": ["Die\u00b7se", "H\u00fcl\u00b7le", "wird", "zer\u00b7tr\u00fcm\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und die freie Seele schimmert", "tokens": ["Und", "die", "frei\u00b7e", "See\u00b7le", "schim\u00b7mert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Zu der h\u00f6hern Geister Chor", "tokens": ["Zu", "der", "h\u00f6\u00b7hern", "Geis\u00b7ter", "Chor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Immer herrlicher empor.", "tokens": ["Im\u00b7mer", "herr\u00b7li\u00b7cher", "em\u00b7por", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Auf! von Moder und Verwesung,", "tokens": ["Auf", "!", "von", "Mo\u00b7der", "und", "Ver\u00b7we\u00b7sung", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blick' hinauf, mein Geist,", "tokens": ["Blick'", "hin\u00b7auf", ",", "mein", "Geist", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wo im Friedensthal Genesung", "tokens": ["Wo", "im", "Frie\u00b7denst\u00b7hal", "Ge\u00b7ne\u00b7sung"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles Jammers fleu\u00dft:", "tokens": ["Al\u00b7les", "Jam\u00b7mers", "fleu\u00dft", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Wo nicht Krieg, Erdbeben, Fluten,", "tokens": ["Wo", "nicht", "Krieg", ",", "Erd\u00b7be\u00b7ben", ",", "Flu\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "PTKNEG", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Hunger, Pest und wilde Gluten,", "tokens": ["Hun\u00b7ger", ",", "Pest", "und", "wil\u00b7de", "Glu\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wo nicht Trennung mehr noch Tod", "tokens": ["Wo", "nicht", "Tren\u00b7nung", "mehr", "noch", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PTKNEG", "NN", "ADV", "ADV", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Liebenden Geliebten droht!", "tokens": ["Lie\u00b7ben\u00b7den", "Ge\u00b7lieb\u00b7ten", "droht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Ach des Wonnetags, der wieder", "tokens": ["Ach", "des", "Won\u00b7ne\u00b7tags", ",", "der", "wie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ewig Freund und Freund,", "tokens": ["E\u00b7wig", "Freund", "und", "Freund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Eltern, Kinder, Schwestern, Br\u00fcder,", "tokens": ["El\u00b7tern", ",", "Kin\u00b7der", ",", "Schwes\u00b7tern", ",", "Br\u00fc\u00b7der", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mann und Weib vereint:", "tokens": ["Mann", "und", "Weib", "ver\u00b7eint", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Wann, gelehrt von Himmelsweisen,", "tokens": ["Wann", ",", "ge\u00b7lehrt", "von", "Him\u00b7mels\u00b7wei\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "VVPP", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wir des Vaters Liebe preisen,", "tokens": ["Wir", "des", "Va\u00b7ters", "Lie\u00b7be", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Der aus Irrtum, Schmach und Gram", "tokens": ["Der", "aus", "Irr\u00b7tum", ",", "Schmach", "und", "Gram"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "APPR", "NE", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Uns in seine Ruhe nahm!", "tokens": ["Uns", "in", "sei\u00b7ne", "Ru\u00b7he", "nahm", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Bald vielleicht, ach bald verschwunden", "tokens": ["Bald", "viel\u00b7leicht", ",", "ach", "bald", "ver\u00b7schwun\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "ITJ", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist auch meine Zeit,", "tokens": ["Ist", "auch", "mei\u00b7ne", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und die letzte meiner Stunden", "tokens": ["Und", "die", "letz\u00b7te", "mei\u00b7ner", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00f6mmt vielleicht schon heut'!", "tokens": ["K\u00f6mmt", "viel\u00b7leicht", "schon", "heut'", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "O la\u00dft Gottes Weg' uns wandeln,", "tokens": ["O", "la\u00dft", "Got\u00b7tes", "Weg'", "uns", "wan\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NN", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Immer gut und redlich handeln:", "tokens": ["Im\u00b7mer", "gut", "und", "red\u00b7lich", "han\u00b7deln", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df wir, wenn der Vater ruft,", "tokens": ["Da\u00df", "wir", ",", "wenn", "der", "Va\u00b7ter", "ruft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Freudig sinken in die Gruft!", "tokens": ["Freu\u00b7dig", "sin\u00b7ken", "in", "die", "Gruft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}