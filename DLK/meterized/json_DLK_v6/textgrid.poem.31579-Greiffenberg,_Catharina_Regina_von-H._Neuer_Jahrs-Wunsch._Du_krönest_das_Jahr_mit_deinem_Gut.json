{"textgrid.poem.31579": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "H. Neuer Jahrs-Wunsch. Du kr\u00f6nest das Jahr mit deinem Gut", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr Jesu / kr\u00f6ne du mit Fr\u00fcchten dieses Jahr!", "tokens": ["Herr", "Je\u00b7su", "/", "kr\u00f6\u00b7ne", "du", "mit", "Fr\u00fcch\u00b7ten", "die\u00b7ses", "Jahr", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$(", "VVFIN", "PPER", "APPR", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "der edle Geist-Geschmack die ganze Welt erg\u00f6tzet /", "tokens": ["der", "ed\u00b7le", "Geist\u00b7Ge\u00b7schmack", "die", "gan\u00b7ze", "Welt", "er\u00b7g\u00f6t\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "sie aus der irdisch-in die Ewigkeit versetzet /", "tokens": ["sie", "aus", "der", "ir\u00b7dischin", "die", "E\u00b7wig\u00b7keit", "ver\u00b7set\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "in der die S\u00fc\u00dfheit erst vollkommen wird und klar.", "tokens": ["in", "der", "die", "S\u00fc\u00df\u00b7heit", "erst", "voll\u00b7kom\u00b7men", "wird", "und", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "ADV", "ADJD", "VAFIN", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ach schicke zeitig Zeit / weil bi\u00dfher Fr\u00fcling war.", "tokens": ["Ach", "schi\u00b7cke", "zei\u00b7tig", "Zeit", "/", "weil", "bi\u00df\u00b7her", "Fr\u00fc\u00b7ling", "war", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ADJD", "NN", "$(", "KOUS", "ADV", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dieweil der Diamant mit deinem Blut genetzet /", "tokens": ["Die\u00b7weil", "der", "Di\u00b7a\u00b7mant", "mit", "dei\u00b7nem", "Blut", "ge\u00b7net\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "du reines Opfferlamm / so gib da\u00df er ge\u00e4tzet", "tokens": ["du", "rei\u00b7nes", "Opf\u00b7fer\u00b7lamm", "/", "so", "gib", "da\u00df", "er", "ge\u00e4t\u00b7zet"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$(", "ADV", "VVIMP", "KOUS", "PPER", "VVPP"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "und einmal werd' erweicht / dein Klarheit offenbar?", "tokens": ["und", "ein\u00b7mal", "werd'", "er\u00b7weicht", "/", "dein", "Klar\u00b7heit", "of\u00b7fen\u00b7bar", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "VVPP", "$(", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Du dunkle Wolkenseul / werd' auch einmal zu Feur.", "tokens": ["Du", "dunk\u00b7le", "Wol\u00b7ken\u00b7seul", "/", "werd'", "auch", "ein\u00b7mal", "zu", "Feur", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$(", "VAFIN", "ADV", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Zeig deine Herrlichkeit / in Werken / wie in Sinnen.", "tokens": ["Zeig", "dei\u00b7ne", "Herr\u00b7lich\u00b7keit", "/", "in", "Wer\u00b7ken", "/", "wie", "in", "Sin\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$(", "APPR", "NN", "$(", "KOKOM", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "La\u00df / was du l\u00e4ngst vertr\u00f6st / vollzogen werden heur.", "tokens": ["La\u00df", "/", "was", "du", "l\u00e4ngst", "ver\u00b7tr\u00f6st", "/", "voll\u00b7zo\u00b7gen", "wer\u00b7den", "heur", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$(", "PWS", "PPER", "ADV", "VVPP", "$(", "VVPP", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "La\u00df die empfangne Frucht heur Tages-Liecht gewinnen.", "tokens": ["La\u00df", "die", "emp\u00b7fang\u00b7ne", "Frucht", "heur", "Ta\u00b7ges\u00b7Liecht", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Gib / da\u00df / wie jener Knab / zum siebenden ich geh /", "tokens": ["Gib", "/", "da\u00df", "/", "wie", "je\u00b7ner", "Knab", "/", "zum", "sie\u00b7ben\u00b7den", "ich", "geh", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$(", "KOUS", "$(", "KOKOM", "PDAT", "NN", "$(", "APPRART", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "und ja einmal nach Wunsch ein H\u00fclffes-W\u00f6lklein seh.", "tokens": ["und", "ja", "ein\u00b7mal", "nach", "Wunsch", "ein", "H\u00fclf\u00b7fes\u00b7W\u00f6lk\u00b7lein", "seh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPR", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}