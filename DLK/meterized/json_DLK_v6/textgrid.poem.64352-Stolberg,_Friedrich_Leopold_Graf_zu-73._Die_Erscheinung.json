{"textgrid.poem.64352": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "73. Die Erscheinung", "genre": "verse", "period": "N.A.", "pub_year": 1781, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hingesunken am See, unter den D\u00fcften des", "tokens": ["Hin\u00b7ge\u00b7sun\u00b7ken", "am", "See", ",", "un\u00b7ter", "den", "D\u00fcf\u00b7ten", "des"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "$,", "APPR", "ART", "NN", "ART"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Lenzes, dacht' ich nur sie, f\u00fchlte nur sie allein,", "tokens": ["Len\u00b7zes", ",", "dacht'", "ich", "nur", "sie", ",", "f\u00fchl\u00b7te", "nur", "sie", "al\u00b7lein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "ADV", "PPER", "$,", "VVFIN", "ADV", "PPER", "ADV", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Die des Tags mir die Seele,", "tokens": ["Die", "des", "Tags", "mir", "die", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Die des Nachts mir die Seele f\u00fcllt.", "tokens": ["Die", "des", "Nachts", "mir", "die", "See\u00b7le", "f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Bl\u00fcten fielen, und Tau fiel auf die Wimper mir,", "tokens": ["Bl\u00fc\u00b7ten", "fie\u00b7len", ",", "und", "Tau", "fiel", "auf", "die", "Wim\u00b7per", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "KON", "NN", "VVFIN", "APPR", "ART", "NN", "PPER", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Weste wiegten mich ein: eh ich die Augen schlo\u00df,", "tokens": ["Wes\u00b7te", "wieg\u00b7ten", "mich", "ein", ":", "eh", "ich", "die", "Au\u00b7gen", "schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "$.", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Sah ich funkeln den Abend,", "tokens": ["Sah", "ich", "fun\u00b7keln", "den", "A\u00b7bend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "H\u00f6rt' einschlafend die Nachtigall.", "tokens": ["H\u00f6rt'", "ein\u00b7schla\u00b7fend", "die", "Nach\u00b7ti\u00b7gall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVPP", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Da erschien mir im Traum eine der Himmlischen:", "tokens": ["Da", "er\u00b7schien", "mir", "im", "Traum", "ei\u00b7ne", "der", "Himm\u00b7li\u00b7schen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "ART", "ART", "NN", "$."], "meter": "--+--+---+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Gleich dem Abendgestirn flammten die Augen der", "tokens": ["Gleich", "dem", "A\u00b7bend\u00b7ges\u00b7tirn", "flamm\u00b7ten", "die", "Au\u00b7gen", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "ART", "NN", "ART"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "G\u00f6ttin, Seligkeit schwebte", "tokens": ["G\u00f6t\u00b7tin", ",", "Se\u00b7lig\u00b7keit", "schweb\u00b7te"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$,", "NN", "VVFIN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Auf den Lippen der L\u00e4chelnden.", "tokens": ["Auf", "den", "Lip\u00b7pen", "der", "L\u00e4\u00b7cheln\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+--", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Wie der Abend des Bachs Wellen mit Golde deckt,", "tokens": ["Wie", "der", "A\u00b7bend", "des", "Bachs", "Wel\u00b7len", "mit", "Gol\u00b7de", "deckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Flo\u00df ihr Rosengewand \u00fcber die g\u00f6ttlichen", "tokens": ["Flo\u00df", "ihr", "Ro\u00b7sen\u00b7ge\u00b7wand", "\u00fc\u00b7ber", "die", "g\u00f6tt\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "APPR", "ART", "ADJA"], "meter": "+-+--++--+--", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Leicht hinschwebenden Glieder", "tokens": ["Leicht", "hin\u00b7schwe\u00b7ben\u00b7den", "Glie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "In Ged\u00fcft von Ambrosia.", "tokens": ["In", "Ge\u00b7d\u00fcft", "von", "A\u00b7mbro\u00b7sia", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ehmals kanntest du mich, sprach sie, und l\u00e4chelte;", "tokens": ["Eh\u00b7mals", "kann\u00b7test", "du", "mich", ",", "sprach", "sie", ",", "und", "l\u00e4\u00b7chel\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "VVFIN", "PPER", "$,", "KON", "VVFIN", "$."], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Ehmals kanntest du mich, sprach sie, und tr\u00e4ufelte", "tokens": ["Eh\u00b7mals", "kann\u00b7test", "du", "mich", ",", "sprach", "sie", ",", "und", "tr\u00e4u\u00b7fel\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "VVFIN", "PPER", "$,", "KON", "VVFIN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Ihres himmlischen Nektars", "tokens": ["Ih\u00b7res", "himm\u00b7li\u00b7schen", "Nek\u00b7tars"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In die bebenden Lippen mir:", "tokens": ["In", "die", "be\u00b7ben\u00b7den", "Lip\u00b7pen", "mir", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPER", "$."], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Freude hei\u00df' ich; es bl\u00fcht bei den Unsterblichen", "tokens": ["Freu\u00b7de", "hei\u00df'", "ich", ";", "es", "bl\u00fcht", "bei", "den", "U\u00b7nsterb\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+--+--+---", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Meine Laube, doch senkt auch zu den Sterblichen", "tokens": ["Mei\u00b7ne", "Lau\u00b7be", ",", "doch", "senkt", "auch", "zu", "den", "Sterb\u00b7li\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ADV", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Sich mein Fittich herunter,", "tokens": ["Sich", "mein", "Fit\u00b7tich", "her\u00b7un\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und ich tr\u00e4nke mit Nektar sie.", "tokens": ["Und", "ich", "tr\u00e4n\u00b7ke", "mit", "Nek\u00b7tar", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "PPER", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.7": {"line.1": {"text": "Komm, ich liebe dich, komm, weihe die Leier mir,", "tokens": ["Komm", ",", "ich", "lie\u00b7be", "dich", ",", "komm", ",", "wei\u00b7he", "die", "Lei\u00b7er", "mir", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "PPER", "$,", "VVFIN", "$,", "VVFIN", "ART", "NN", "PPER", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Mir dies klopfende Herz! komm, und entsage der,", "tokens": ["Mir", "dies", "klop\u00b7fen\u00b7de", "Herz", "!", "komm", ",", "und", "ent\u00b7sa\u00b7ge", "der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PDS", "ADJA", "NN", "$.", "VVFIN", "$,", "KON", "VVFIN", "ART", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Die des Tags dir die Seele,", "tokens": ["Die", "des", "Tags", "dir", "die", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Die des Nachts dir die Seele f\u00fcllt.", "tokens": ["Die", "des", "Nachts", "dir", "die", "See\u00b7le", "f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.8": {"line.1": {"text": "G\u00f6ttin, sprach ich, f\u00fcr dich seufzen die Sterblichen,", "tokens": ["G\u00f6t\u00b7tin", ",", "sprach", "ich", ",", "f\u00fcr", "dich", "seuf\u00b7zen", "die", "Sterb\u00b7li\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "$,", "APPR", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Selig preisen durch dich sich die Unsterblichen;", "tokens": ["Se\u00b7lig", "prei\u00b7sen", "durch", "dich", "sich", "die", "U\u00b7nsterb\u00b7li\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPER", "PRF", "ART", "NN", "$."], "meter": "+-+-+++-+---", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Ach ich liebe dich, G\u00f6ttin:", "tokens": ["Ach", "ich", "lie\u00b7be", "dich", ",", "G\u00f6t\u00b7tin", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ITJ", "PPER", "VVFIN", "PPER", "$,", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Aber, Himmlische, z\u00fcrne nicht!", "tokens": ["A\u00b7ber", ",", "Himm\u00b7li\u00b7sche", ",", "z\u00fcr\u00b7ne", "nicht", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$,", "VVFIN", "PTKNEG", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.9": {"line.1": {"text": "Sieh ich folge dir nicht! z\u00fcrne der Sterblichen,", "tokens": ["Sieh", "ich", "fol\u00b7ge", "dir", "nicht", "!", "z\u00fcr\u00b7ne", "der", "Sterb\u00b7li\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVFIN", "PPER", "PTKNEG", "$.", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Z\u00fcrne Lyda nur nicht! Kann ich entsagen der,", "tokens": ["Z\u00fcr\u00b7ne", "Ly\u00b7da", "nur", "nicht", "!", "Kann", "ich", "ent\u00b7sa\u00b7gen", "der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "PTKNEG", "$.", "VMFIN", "PPER", "VVFIN", "ART", "$,"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Die des Tags mir die Seele,", "tokens": ["Die", "des", "Tags", "mir", "die", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Die des Nachts mir die Seele f\u00fcllt?", "tokens": ["Die", "des", "Nachts", "mir", "die", "See\u00b7le", "f\u00fcllt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.10": {"line.1": {"text": "Sie verschwand, wie ein Blitz; und ich erwachte schnell:", "tokens": ["Sie", "ver\u00b7schwand", ",", "wie", "ein", "Blitz", ";", "und", "ich", "er\u00b7wach\u00b7te", "schnell", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ART", "NN", "$.", "KON", "PPER", "VVFIN", "ADJD", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Hochauf klopfte mein Herz, aber es klopfte der,", "tokens": ["Hoch\u00b7auf", "klopf\u00b7te", "mein", "Herz", ",", "a\u00b7ber", "es", "klopf\u00b7te", "der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,", "KON", "PPER", "VVFIN", "ART", "$,"], "meter": "-+--+++--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die des Tags mir die Seele,", "tokens": ["Die", "des", "Tags", "mir", "die", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Die des Nachts mir die Seele f\u00fcllt.", "tokens": ["Die", "des", "Nachts", "mir", "die", "See\u00b7le", "f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}