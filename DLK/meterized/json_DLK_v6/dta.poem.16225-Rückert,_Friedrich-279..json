{"dta.poem.16225": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "279.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gekommen in die Nacht der Welt ist Gottes Licht;", "tokens": ["Ge\u00b7kom\u00b7men", "in", "die", "Nacht", "der", "Welt", "ist", "Got\u00b7tes", "Licht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "VAFIN", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wir sind daran erwacht, und schlummern f\u00fcrder nicht.", "tokens": ["Wir", "sind", "da\u00b7ran", "er\u00b7wacht", ",", "und", "schlum\u00b7mern", "f\u00fcr\u00b7der", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "VVPP", "$,", "KON", "VVFIN", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wir schlummern f\u00fcrder nicht den Weltbet\u00e4ubungschlummer,", "tokens": ["Wir", "schlum\u00b7mern", "f\u00fcr\u00b7der", "nicht", "den", "Welt\u00b7be\u00b7t\u00e4u\u00b7bung\u00b7schlum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wir blicken, wach im Licht, aufs Nachtgraun ohne Kummer.", "tokens": ["Wir", "bli\u00b7cken", ",", "wach", "im", "Licht", ",", "aufs", "Nacht\u00b7graun", "oh\u00b7ne", "Kum\u00b7mer", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJD", "APPRART", "NN", "$,", "APPRART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wo ist der N\u00e4chte Graun? es ist vom Licht bezwungen;", "tokens": ["Wo", "ist", "der", "N\u00e4ch\u00b7te", "Graun", "?", "es", "ist", "vom", "Licht", "be\u00b7zwun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "ADJA", "NN", "$.", "PPER", "VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wir blicken mit Vertraun ins Licht, vom Licht durchdrungen.", "tokens": ["Wir", "bli\u00b7cken", "mit", "Ver\u00b7traun", "ins", "Licht", ",", "vom", "Licht", "durch\u00b7drun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPRART", "NN", "$,", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Da\u00df wir durchdrungen sind vom Lichte, dem wir dienen,", "tokens": ["Da\u00df", "wir", "durch\u00b7drun\u00b7gen", "sind", "vom", "Lich\u00b7te", ",", "dem", "wir", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "APPRART", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wir zeigens dem Gesind der Nacht in unsern Mienen.", "tokens": ["Wir", "zei\u00b7gens", "dem", "Ge\u00b7sind", "der", "Nacht", "in", "un\u00b7sern", "Mie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "In hellen Mienen macht sich kund die Kraft des Herrn,", "tokens": ["In", "hel\u00b7len", "Mie\u00b7nen", "macht", "sich", "kund", "die", "Kraft", "des", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PRF", "PTKVZ", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wer nicht in der Nacht kann leuchten, ist kein Stern.", "tokens": ["Und", "wer", "nicht", "in", "der", "Nacht", "kann", "leuch\u00b7ten", ",", "ist", "kein", "Stern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PTKNEG", "APPR", "ART", "NN", "VMFIN", "VVINF", "$,", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}