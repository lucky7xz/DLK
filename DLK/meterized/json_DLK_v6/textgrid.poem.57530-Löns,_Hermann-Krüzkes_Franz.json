{"textgrid.poem.57530": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Kr\u00fczkes Franz", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Siehst du den Menschen? so glasig und \u00f6de", "tokens": ["Siehst", "du", "den", "Men\u00b7schen", "?", "so", "gla\u00b7sig", "und", "\u00f6\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "$.", "ADV", "ADJD", "KON", "ADJD"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Starret sein Blick nach den Steinen,", "tokens": ["Star\u00b7ret", "sein", "Blick", "nach", "den", "Stei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "In seinem Angesicht, geistlos und bl\u00f6de", "tokens": ["In", "sei\u00b7nem", "An\u00b7ge\u00b7sicht", ",", "geist\u00b7los", "und", "bl\u00f6\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADJD", "KON", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Seh ich nie Lachen, noch Weinen.", "tokens": ["Seh", "ich", "nie", "La\u00b7chen", ",", "noch", "Wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "$,", "ADV", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Wie er dahinwankt mit schlotternden Knieen,", "tokens": ["Wie", "er", "da\u00b7hin\u00b7wankt", "mit", "schlot\u00b7tern\u00b7den", "Kni\u00b7e\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "--+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Aschgrau die Jammergeb\u00e4rde,", "tokens": ["Aschgrau", "die", "Jam\u00b7mer\u00b7ge\u00b7b\u00e4r\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Zitternd die mageren Finger ziehen", "tokens": ["Zit\u00b7ternd", "die", "ma\u00b7ge\u00b7ren", "Fin\u00b7ger", "zie\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "ART", "ADJA", "NN", "VVINF"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Kreuzzeichen \u00fcber die Erde.", "tokens": ["Kreuz\u00b7zei\u00b7chen", "\u00fc\u00b7ber", "die", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "\u00bbkr\u00fczkes Franz, Kr\u00fczkes Franz!\u00ab necken und h\u00f6hnen", "tokens": ["\u00bb", "kr\u00fcz\u00b7kes", "Franz", ",", "Kr\u00fcz\u00b7kes", "Franz", "!", "\u00ab", "ne\u00b7cken", "und", "h\u00f6h\u00b7nen"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["$(", "NE", "NE", "$,", "NN", "NE", "$.", "$(", "VVINF", "KON", "ADJA"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Heulend und br\u00fcllend die Knaben,", "tokens": ["Heu\u00b7lend", "und", "br\u00fcl\u00b7lend", "die", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "\u00bbfr\u00e4nzchen, kiek hier\u00ab \u2013 so h\u00f6r' ich es t\u00f6nen,", "tokens": ["\u00bb", "fr\u00e4nz\u00b7chen", ",", "kiek", "hier", "\u00ab", "\u2013", "so", "h\u00f6r'", "ich", "es", "t\u00f6\u00b7nen", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVINF", "$,", "VVIMP", "ADV", "$(", "$(", "ADV", "VVFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "\u00bbdor ligg din Moder begraben!\u00ab", "tokens": ["\u00bb", "dor", "ligg", "din", "Mo\u00b7der", "be\u00b7gra\u00b7ben", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPOSAT", "NN", "VVPP", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Gierig mit scheuen und sch\u00fcchternen Blicken", "tokens": ["Gie\u00b7rig", "mit", "scheu\u00b7en", "und", "sch\u00fcch\u00b7ter\u00b7nen", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "VVINF", "KON", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Folgt er den h\u00f6hnenden Rufen,", "tokens": ["Folgt", "er", "den", "h\u00f6h\u00b7nen\u00b7den", "Ru\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Aber vergebens versucht er zu dr\u00fccken", "tokens": ["A\u00b7ber", "ver\u00b7ge\u00b7bens", "ver\u00b7sucht", "er", "zu", "dr\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PTKZU", "VVINF"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Kreuze auf steinernen Stufen.", "tokens": ["Kreu\u00b7ze", "auf", "stei\u00b7ner\u00b7nen", "Stu\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.5": {"line.1": {"text": "War er ein froher und munterer Knabe,", "tokens": ["War", "er", "ein", "fro\u00b7her", "und", "mun\u00b7te\u00b7rer", "Kna\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "KON", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Als seine Mutter noch lebte,", "tokens": ["Als", "sei\u00b7ne", "Mut\u00b7ter", "noch", "leb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Seit man sie barg in dem hungrigen Grabe,", "tokens": ["Seit", "man", "sie", "barg", "in", "dem", "hung\u00b7ri\u00b7gen", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Wahnsinn den Geist ihm umwebte.", "tokens": ["Wahn\u00b7sinn", "den", "Geist", "ihm", "um\u00b7web\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.6": {"line.1": {"text": "Als man der Mutter den Grabh\u00fcgel h\u00e4ufte,", "tokens": ["Als", "man", "der", "Mut\u00b7ter", "den", "Grab\u00b7h\u00fc\u00b7gel", "h\u00e4uf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Glaubt er, vergessen man habe,", "tokens": ["Glaubt", "er", ",", "ver\u00b7ges\u00b7sen", "man", "ha\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PIS", "VAFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Da\u00df man ein Kreuz in die Lehmscholle streifte,", "tokens": ["Da\u00df", "man", "ein", "Kreuz", "in", "die", "Lehm\u00b7schol\u00b7le", "streif\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ruhe ihr g\u00e4be im Grabe.", "tokens": ["Ru\u00b7he", "ihr", "g\u00e4\u00b7be", "im", "Gra\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.7": {"line.1": {"text": "Nimmer nun, w\u00e4hnt er, k\u00f6nne entweichen", "tokens": ["Nim\u00b7mer", "nun", ",", "w\u00e4hnt", "er", ",", "k\u00f6n\u00b7ne", "ent\u00b7wei\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "$,", "VVFIN", "PPER", "$,", "VMFIN", "VVINF"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Jene des Fegfeuers Qualen,", "tokens": ["Je\u00b7ne", "des", "Feg\u00b7feu\u00b7ers", "Qua\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis man das leidenerl\u00f6sende Zeichen", "tokens": ["Bis", "man", "das", "lei\u00b7de\u00b7ner\u00b7l\u00f6\u00b7sen\u00b7de", "Zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "ART", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Ihr auf das Grab w\u00fcrde malen.", "tokens": ["Ihr", "auf", "das", "Grab", "w\u00fcr\u00b7de", "ma\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Sucht er die Stelle, wo sie begraben,", "tokens": ["Sucht", "er", "die", "Stel\u00b7le", ",", "wo", "sie", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PWAV", "PPER", "VVPP", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "T\u00e4glich und findet sie nimmer,", "tokens": ["T\u00e4g\u00b7lich", "und", "fin\u00b7det", "sie", "nim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Aber es blieb, ob auch h\u00f6hnen die Knaben,", "tokens": ["A\u00b7ber", "es", "blieb", ",", "ob", "auch", "h\u00f6h\u00b7nen", "die", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUS", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Ihm noch ein rettender Schimmer.", "tokens": ["Ihm", "noch", "ein", "ret\u00b7ten\u00b7der", "Schim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Da\u00df er durch Zufall k\u00f6nne einst finden", "tokens": ["Da\u00df", "er", "durch", "Zu\u00b7fall", "k\u00f6n\u00b7ne", "einst", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "VMFIN", "ADV", "VVINF"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Jene verborgene Stelle,", "tokens": ["Je\u00b7ne", "ver\u00b7bor\u00b7ge\u00b7ne", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Und seine Mutter vom Orte der S\u00fcnden", "tokens": ["Und", "sei\u00b7ne", "Mut\u00b7ter", "vom", "Or\u00b7te", "der", "S\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Auff\u00fchr' zur himmlischen Helle.", "tokens": ["Auf\u00b7f\u00fchr'", "zur", "himm\u00b7li\u00b7schen", "Hel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "Darum w\u00fchlt er die fl\u00fcchtigen Zeichen", "tokens": ["Da\u00b7rum", "w\u00fchlt", "er", "die", "fl\u00fcch\u00b7ti\u00b7gen", "Zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Hastig in Wege und Gossen,", "tokens": ["Has\u00b7tig", "in", "We\u00b7ge", "und", "Gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Seh' ich ihn t\u00e4glich die Stra\u00dfen durchstreifen", "tokens": ["Seh'", "ich", "ihn", "t\u00e4g\u00b7lich", "die", "Stra\u00b7\u00dfen", "durch\u00b7strei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "ADJD", "ART", "NN", "VVINF"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Hoffnungsvoll und unverdrossen.", "tokens": ["Hoff\u00b7nungs\u00b7voll", "und", "un\u00b7ver\u00b7dros\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}