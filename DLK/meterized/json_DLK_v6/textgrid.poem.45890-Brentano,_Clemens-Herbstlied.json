{"textgrid.poem.45890": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Herbstlied", "genre": "verse", "period": "N.A.", "pub_year": 1802, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die gr\u00fcnen Bl\u00e4tter sind gefallen,", "tokens": ["Die", "gr\u00fc\u00b7nen", "Bl\u00e4t\u00b7ter", "sind", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Schwalben fortgezogen sind,", "tokens": ["Die", "Schwal\u00b7ben", "fort\u00b7ge\u00b7zo\u00b7gen", "sind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da will zu seiner Heimat wallen,", "tokens": ["Da", "will", "zu", "sei\u00b7ner", "Hei\u00b7mat", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bin armes elternloses Kind.", "tokens": ["Bin", "ar\u00b7mes", "el\u00b7tern\u00b7lo\u00b7ses", "Kind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Als F\u00fchrer auf der weiten Reise,", "tokens": ["Als", "F\u00fch\u00b7rer", "auf", "der", "wei\u00b7ten", "Rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Fliegt vor ihm hin ein Schmetterling,", "tokens": ["Fliegt", "vor", "ihm", "hin", "ein", "Schmet\u00b7ter\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr B\u00fcndelchen tr\u00e4gt selbst die Waise,", "tokens": ["Ihr", "B\u00fcn\u00b7del\u00b7chen", "tr\u00e4gt", "selbst", "die", "Wai\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Ihr Hab und Gut ist sehr gering.", "tokens": ["Ihr", "Hab", "und", "Gut", "ist", "sehr", "ge\u00b7ring", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "ADJD", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Vater ist ihm fr\u00fch gestorben,", "tokens": ["Der", "Va\u00b7ter", "ist", "ihm", "fr\u00fch", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Mutter hat es weggesandt,", "tokens": ["Die", "Mut\u00b7ter", "hat", "es", "weg\u00b7ge\u00b7sandt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Ausland hat es nichts erworben,", "tokens": ["Im", "Aus\u00b7land", "hat", "es", "nichts", "er\u00b7wor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und arm kehrt es zum Vaterland.", "tokens": ["Und", "arm", "kehrt", "es", "zum", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und wie sie durch die W\u00e4lder gingen,", "tokens": ["Und", "wie", "sie", "durch", "die", "W\u00e4l\u00b7der", "gin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Schmetterling zum Kinde spricht:", "tokens": ["Der", "Schmet\u00b7ter\u00b7ling", "zum", "Kin\u00b7de", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbum meinen Lohn ist noch zu dingen,", "tokens": ["\u00bb", "um", "mei\u00b7nen", "Lohn", "ist", "noch", "zu", "din\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPOSAT", "NN", "VAFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den kleinen Freund vergesse nicht.", "tokens": ["Den", "klei\u00b7nen", "Freund", "ver\u00b7ges\u00b7se", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich werde nicht mehr lange leben", "tokens": ["Ich", "wer\u00b7de", "nicht", "mehr", "lan\u00b7ge", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und m\u00f6chte mich noch einmal freun,", "tokens": ["Und", "m\u00f6ch\u00b7te", "mich", "noch", "ein\u00b7mal", "freun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu deiner Heimat will ich schweben,", "tokens": ["Zu", "dei\u00b7ner", "Hei\u00b7mat", "will", "ich", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Lohn gieb mir ein Bl\u00fcmelein.\u00ab", "tokens": ["Zum", "Lohn", "gieb", "mir", "ein", "Bl\u00fc\u00b7me\u00b7lein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "VVIMP", "PPER", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Das Kind sprach: \u00bbKeins ist hier zu sehen,", "tokens": ["Das", "Kind", "sprach", ":", "\u00bb", "Keins", "ist", "hier", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "PIS", "VAFIN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch ist in meinem Vaterland", "tokens": ["Doch", "ist", "in", "mei\u00b7nem", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein stiller Garten, darin stehen", "tokens": ["Ein", "stil\u00b7ler", "Gar\u00b7ten", ",", "da\u00b7rin", "ste\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PAV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der s\u00fc\u00dfen Blumen allerhand.", "tokens": ["Der", "s\u00fc\u00b7\u00dfen", "Blu\u00b7men", "al\u00b7ler\u00b7hand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PIAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ein Engel gehet in dem Garten,", "tokens": ["Ein", "En\u00b7gel", "ge\u00b7het", "in", "dem", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der giebt dir sicher doppelt Lohn,", "tokens": ["Der", "giebt", "dir", "si\u00b7cher", "dop\u00b7pelt", "Lohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PRF", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O wolle bis zur Heimat warten,", "tokens": ["O", "wol\u00b7le", "bis", "zur", "Hei\u00b7mat", "war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich irre, wenn du mir entflohn.", "tokens": ["Ich", "ir\u00b7re", ",", "wenn", "du", "mir", "ent\u00b7flohn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "In einem stillen Tale wohnet", "tokens": ["In", "ei\u00b7nem", "stil\u00b7len", "Ta\u00b7le", "woh\u00b7net"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Engel und ich bin sein Kind,", "tokens": ["Der", "En\u00b7gel", "und", "ich", "bin", "sein", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich wei\u00df, da\u00df er dich reich belohnet,", "tokens": ["Ich", "wei\u00df", ",", "da\u00df", "er", "dich", "reich", "be\u00b7loh\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Drum la\u00df uns eilen, schnell geschwind.\u00ab", "tokens": ["Drum", "la\u00df", "uns", "ei\u00b7len", ",", "schnell", "ge\u00b7schwind", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PAV", "VVFIN", "PPER", "VVFIN", "$,", "ADJD", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Der F\u00fchrer hebt die bunten Schwingen,", "tokens": ["Der", "F\u00fch\u00b7rer", "hebt", "die", "bun\u00b7ten", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der kleine Wandrer folgt ihm schnell.", "tokens": ["Der", "klei\u00b7ne", "Wand\u00b7rer", "folgt", "ihm", "schnell", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er spricht: \u00bbh\u00f6rst du die V\u00f6glein singen,", "tokens": ["Er", "spricht", ":", "\u00bb", "h\u00f6rst", "du", "die", "V\u00f6\u00b7glein", "sin\u00b7gen", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Garten singen sie so hell.", "tokens": ["Im", "Gar\u00b7ten", "sin\u00b7gen", "sie", "so", "hell", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Ich atme schon die Blumend\u00fcfte,", "tokens": ["Ich", "at\u00b7me", "schon", "die", "Blu\u00b7men\u00b7d\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O lieber F\u00fchrer eile schnell.\u00ab", "tokens": ["O", "lie\u00b7ber", "F\u00fch\u00b7rer", "ei\u00b7le", "schnell", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ITJ", "ADV", "NN", "VVFIN", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbich f\u00fchle nur die k\u00fchlen L\u00fcfte,\u00ab", "tokens": ["\u00bb", "ich", "f\u00fch\u00b7le", "nur", "die", "k\u00fch\u00b7len", "L\u00fcf\u00b7te", ",", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Spricht da der bunte Reis'gesell.", "tokens": ["Spricht", "da", "der", "bun\u00b7te", "Reis'\u00b7ge\u00b7sell", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "\u00bbo willst du nicht den Lohn mir geben,", "tokens": ["\u00bb", "o", "willst", "du", "nicht", "den", "Lohn", "mir", "ge\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "VMFIN", "PPER", "PTKNEG", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich sterb', eh' ich die Heimat seh',", "tokens": ["Ich", "sterb'", ",", "eh'", "ich", "die", "Hei\u00b7mat", "seh'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich werde nicht mehr lange leben,", "tokens": ["Ich", "wer\u00b7de", "nicht", "mehr", "lan\u00b7ge", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die k\u00fchlen L\u00fcfte tun mir weh.\u00ab", "tokens": ["Die", "k\u00fch\u00b7len", "L\u00fcf\u00b7te", "tun", "mir", "weh", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "\u00bbso nehme alles, was ich habe,\u00ab", "tokens": ["\u00bb", "so", "neh\u00b7me", "al\u00b7les", ",", "was", "ich", "ha\u00b7be", ",", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VVFIN", "PIS", "$,", "PWS", "PPER", "VAFIN", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sprach weinend da das arme Kind,", "tokens": ["Sprach", "wei\u00b7nend", "da", "das", "ar\u00b7me", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbvon jenem Engel eine Gabe,", "tokens": ["\u00bb", "von", "je\u00b7nem", "En\u00b7gel", "ei\u00b7ne", "Ga\u00b7be", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PDAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die welken Rosenbl\u00e4tter sind.\u00ab", "tokens": ["Die", "wel\u00b7ken", "Ro\u00b7sen\u00b7bl\u00e4t\u00b7ter", "sind", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Der F\u00fchrer stirbt und in den Rosen,", "tokens": ["Der", "F\u00fch\u00b7rer", "stirbt", "und", "in", "den", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Weiht ihm das Kind ein frommes Grab.", "tokens": ["Weiht", "ihm", "das", "Kind", "ein", "from\u00b7mes", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schon h\u00f6rt es nah des Stromes Tosen,", "tokens": ["Schon", "h\u00f6rt", "es", "nah", "des", "Stro\u00b7mes", "To\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und steiget zu dem Tal hinab.", "tokens": ["Und", "stei\u00b7get", "zu", "dem", "Tal", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Da steht es an der Heimat Sc