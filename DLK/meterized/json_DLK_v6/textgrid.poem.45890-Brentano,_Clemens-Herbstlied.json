,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo es der Fr\u00fchlingsschein gewiegt,", "tokens": ["Wo", "es", "der", "Fr\u00fch\u00b7lings\u00b7schein", "ge\u00b7wiegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da ist sein Grab nun bei den Rosen,", "tokens": ["Da", "ist", "sein", "Grab", "nun", "bei", "den", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und treu das V\u00f6glein niederfliegt,", "tokens": ["Und", "treu", "das", "V\u00f6\u00b7glein", "nie\u00b7der\u00b7fliegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.25": {"line.1": {"text": "Und deckte es mit welken Bl\u00fcten", "tokens": ["Und", "deck\u00b7te", "es", "mit", "wel\u00b7ken", "Bl\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PWAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Aus alter, treuer Freundschaft zu,", "tokens": ["Aus", "al\u00b7ter", ",", "treu\u00b7er", "Freund\u00b7schaft", "zu", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem V\u00f6glein m\u00f6g' es Gott verg\u00fcten,", "tokens": ["Dem", "V\u00f6\u00b7glein", "m\u00f6g'", "es", "Gott", "ver\u00b7g\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es sang das Kind wohl in die Ruh'.", "tokens": ["Es", "sang", "das", "Kind", "wohl", "in", "die", "Ruh'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.26": {"line.1": {"text": "\u00bbo Heimat in dem Fr\u00fchlingsscheine,", "tokens": ["\u00bb", "o", "Hei\u00b7mat", "in", "dem", "Fr\u00fch\u00b7lings\u00b7schei\u00b7ne", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O Jugend, harte Mutter sein,", "tokens": ["O", "Ju\u00b7gend", ",", "har\u00b7te", "Mut\u00b7ter", "sein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Kind, die Liebe ich beweine", "tokens": ["Dein", "Kind", ",", "die", "Lie\u00b7be", "ich", "be\u00b7wei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sein einz'ger Freund ein V\u00f6gelein.\u00ab", "tokens": ["Sein", "einz'\u00b7ger", "Freund", "ein", "V\u00f6\u00b7ge\u00b7lein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}