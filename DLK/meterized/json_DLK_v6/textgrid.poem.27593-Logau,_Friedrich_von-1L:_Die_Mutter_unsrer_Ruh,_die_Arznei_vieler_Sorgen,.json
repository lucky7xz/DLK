{"textgrid.poem.27593": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Mutter unsrer Ruh, die Arznei vieler Sorgen,", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Mutter unsrer Ruh, die Arznei vieler Sorgen,", "tokens": ["Die", "Mut\u00b7ter", "uns\u00b7rer", "Ruh", ",", "die", "Arz\u00b7nei", "vie\u00b7ler", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die finstre Nacht ist da; die Sonne geht verborgen;", "tokens": ["Die", "finst\u00b7re", "Nacht", "ist", "da", ";", "die", "Son\u00b7ne", "geht", "ver\u00b7bor\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "$.", "ART", "NN", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die halbe Welt ist schwartz, ist traurig ohne Liecht,", "tokens": ["Die", "hal\u00b7be", "Welt", "ist", "schwartz", ",", "ist", "trau\u00b7rig", "oh\u00b7ne", "Liecht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "$,", "VAFIN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ist gleichsam mehr nicht da, lebt zwar, lebt doch auch nicht.", "tokens": ["Ist", "gleich\u00b7sam", "mehr", "nicht", "da", ",", "lebt", "zwar", ",", "lebt", "doch", "auch", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "PTKNEG", "ADV", "$,", "VVFIN", "ADV", "$,", "VVFIN", "ADV", "ADV", "PTKNEG", "$."], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Herr Gott, du heller Glantz, la\u00df unser Herz und Sinnen", "tokens": ["Herr", "Gott", ",", "du", "hel\u00b7ler", "Glantz", ",", "la\u00df", "un\u00b7ser", "Herz", "und", "Sin\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "$,", "PPER", "ADJA", "NN", "$,", "VVIMP", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Im Finstren nimmer seyn; gib, da\u00df sie wachen k\u00f6nnen", "tokens": ["Im", "Finst\u00b7ren", "nim\u00b7mer", "seyn", ";", "gib", ",", "da\u00df", "sie", "wa\u00b7chen", "k\u00f6n\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "VAINF", "$.", "VVIMP", "$,", "KOUS", "PPER", "VVINF", "VMINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Auch mitten in dem Schlaf, auff da\u00df dein G\u00f6ttlich Schein", "tokens": ["Auch", "mit\u00b7ten", "in", "dem", "Schlaf", ",", "auff", "da\u00df", "dein", "G\u00f6tt\u00b7lich", "Schein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$,", "APPR", "KOUS", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "M\u00f6g' unsrer Seele Liecht und helle Fackel seyn!", "tokens": ["M\u00f6g'", "uns\u00b7rer", "See\u00b7le", "Liecht", "und", "hel\u00b7le", "Fa\u00b7ckel", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "NN", "KON", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wenn wir de\u00df Kummers Last zu unsren Haupten legen,", "tokens": ["Wenn", "wir", "de\u00df", "Kum\u00b7mers", "Last", "zu", "un\u00b7sren", "Haup\u00b7ten", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NE", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So la\u00df sich deinen Geist in unsrem Geiste regen", "tokens": ["So", "la\u00df", "sich", "dei\u00b7nen", "Geist", "in", "uns\u00b7rem", "Geis\u00b7te", "re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und schaffe, da\u00df die Nacht, wenn uns der Tag erweckt,", "tokens": ["Und", "schaf\u00b7fe", ",", "da\u00df", "die", "Nacht", ",", "wenn", "uns", "der", "Tag", "er\u00b7weckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ART", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der S\u00fcnden schn\u00f6de B\u00fcrd' in allem hat verdeckt!", "tokens": ["Der", "S\u00fcn\u00b7den", "schn\u00f6\u00b7de", "B\u00fcrd'", "in", "al\u00b7lem", "hat", "ver\u00b7deckt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "APPR", "PIS", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "La\u00df deiner Engel Dienst auch uns zu Dienste kommen!", "tokens": ["La\u00df", "dei\u00b7ner", "En\u00b7gel", "Dienst", "auch", "uns", "zu", "Diens\u00b7te", "kom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "NN", "ADV", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Gib, da\u00df von unsrem Haupt sey Schad' und Schmach genommen,", "tokens": ["Gib", ",", "da\u00df", "von", "uns\u00b7rem", "Haupt", "sey", "Schad'", "und", "Schmach", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "KOUS", "APPR", "PPOSAT", "NN", "VAFIN", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Auff da\u00df der starke Feind, der schwartze F\u00fcrst der Nacht,", "tokens": ["Auff", "da\u00df", "der", "star\u00b7ke", "Feind", ",", "der", "schwart\u00b7ze", "F\u00fcrst", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "De\u00df Leibes s\u00fcsse Ruh uns nicht verbittert macht!", "tokens": ["De\u00df", "Lei\u00b7bes", "s\u00fcs\u00b7se", "Ruh", "uns", "nicht", "ver\u00b7bit\u00b7tert", "macht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PPER", "PTKNEG", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Und so es so soll seyn, da\u00df heut ich noch soll gehen", "tokens": ["Und", "so", "es", "so", "soll", "seyn", ",", "da\u00df", "heut", "ich", "noch", "soll", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PPER", "ADV", "VMFIN", "VAINF", "$,", "KOUS", "ADV", "PPER", "ADV", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "De\u00df Todes finstren Gang, so wollstu bei mir stehen", "tokens": ["De\u00df", "To\u00b7des", "finst\u00b7ren", "Gang", ",", "so", "woll\u00b7stu", "bei", "mir", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ADV", "VMFIN", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und gehen f\u00fcr mir her ins Leben durch den Tod,", "tokens": ["Und", "ge\u00b7hen", "f\u00fcr", "mir", "her", "ins", "Le\u00b7ben", "durch", "den", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ADV", "APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "In Himmel au\u00df der Welt, zur Freude von der Noth!", "tokens": ["In", "Him\u00b7mel", "au\u00df", "der", "Welt", ",", "zur", "Freu\u00b7de", "von", "der", "Noth", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$,", "APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}