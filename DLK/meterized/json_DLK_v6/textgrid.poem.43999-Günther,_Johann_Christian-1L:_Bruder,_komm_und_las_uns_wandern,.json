{"textgrid.poem.43999": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bruder, komm und las uns wandern,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bruder, komm und las uns wandern,", "tokens": ["Bru\u00b7der", ",", "komm", "und", "las", "uns", "wan\u00b7dern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "KON", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Habe Leid und Lust gemein;", "tokens": ["Ha\u00b7be", "Leid", "und", "Lust", "ge\u00b7mein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kommt ein Wetter nach dem andern,", "tokens": ["Kommt", "ein", "Wet\u00b7ter", "nach", "dem", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hilf mir doch best\u00e4ndig seyn.", "tokens": ["Hilf", "mir", "doch", "be\u00b7st\u00e4n\u00b7dig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der Verdru\u00df vergangner Tage", "tokens": ["Der", "Ver\u00b7dru\u00df", "ver\u00b7gang\u00b7ner", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zeigt viel s\u00fc\u00df' Erinnerung;", "tokens": ["Zeigt", "viel", "s\u00fc\u00df'", "E\u00b7rin\u00b7ne\u00b7rung", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "--+-+--", "measure": "anapaest.init"}, "line.7": {"text": "Wir erdulden schwere Plage,", "tokens": ["Wir", "er\u00b7dul\u00b7den", "schwe\u00b7re", "Pla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Aber wir sind auch noch jung.", "tokens": ["A\u00b7ber", "wir", "sind", "auch", "noch", "jung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Gleiche Br\u00fcder, gleiche Kappen,", "tokens": ["Glei\u00b7che", "Br\u00fc\u00b7der", ",", "glei\u00b7che", "Kap\u00b7pen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einerley Gefahr und Muth!", "tokens": ["Ei\u00b7ner\u00b7ley", "Ge\u00b7fahr", "und", "Muth", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sollt uns auch der Feind ertappen,", "tokens": ["Sollt", "uns", "auch", "der", "Feind", "er\u00b7tap\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00e4mpfen wir vor Ruhm und Blut.", "tokens": ["K\u00e4mp\u00b7fen", "wir", "vor", "Ruhm", "und", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wir sind allzeit freye Leute;", "tokens": ["Wir", "sind", "all\u00b7zeit", "frey\u00b7e", "Leu\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJA", "NN", "$."], "meter": "---+--+-", "measure": "iambic.di.relaxed"}, "line.6": {"text": "Ob uns gleich die Armuth dr\u00fcckt,", "tokens": ["Ob", "uns", "gleich", "die", "Ar\u00b7muth", "dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Werden wir doch immer heute", "tokens": ["Wer\u00b7den", "wir", "doch", "im\u00b7mer", "heu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADV"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Durch geschwinden Trost erquickt.", "tokens": ["Durch", "ge\u00b7schwin\u00b7den", "Trost", "er\u00b7quickt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Jene, so in gro\u00dfen St\u00e4dten", "tokens": ["Je\u00b7ne", ",", "so", "in", "gro\u00b7\u00dfen", "St\u00e4d\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "$,", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unter Sammt und Seide gehn,", "tokens": ["Un\u00b7ter", "Sammt", "und", "Sei\u00b7de", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00fc\u00dfen, wenn sie Pflaster treten,", "tokens": ["M\u00fc\u00b7\u00dfen", ",", "wenn", "sie", "Pflas\u00b7ter", "tre\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Voller Furcht und Sorgen stehn;", "tokens": ["Vol\u00b7ler", "Furcht", "und", "Sor\u00b7gen", "stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ihrer \u00c4mter Schein und W\u00fcrde", "tokens": ["Ih\u00b7rer", "\u00c4m\u00b7ter", "Schein", "und", "W\u00fcr\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist ein Mantel der Gefahr,", "tokens": ["Ist", "ein", "Man\u00b7tel", "der", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und sie werden bey der B\u00fcrde", "tokens": ["Und", "sie", "wer\u00b7den", "bey", "der", "B\u00fcr\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ihres Lebens kaum gewahr.", "tokens": ["Ih\u00b7res", "Le\u00b7bens", "kaum", "ge\u00b7wahr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sag es, Bruder, unverholen,", "tokens": ["Sag", "es", ",", "Bru\u00b7der", ",", "un\u00b7ver\u00b7ho\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "$,", "NN", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind wir nicht weit be\u00dfer dran?", "tokens": ["Sind", "wir", "nicht", "weit", "be\u00b7\u00dfer", "dran", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADJD", "ADJD", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Unser Schaden sind nur Sohlen,", "tokens": ["Un\u00b7ser", "Scha\u00b7den", "sind", "nur", "Soh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die man leicht ersezen kan;", "tokens": ["Die", "man", "leicht", "er\u00b7se\u00b7zen", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADJD", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nichts verwirrt uns die Gem\u00fcther,", "tokens": ["Nichts", "ver\u00b7wirrt", "uns", "die", "Ge\u00b7m\u00fc\u00b7ther", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Niemand zwingt uns an das Joch;", "tokens": ["Nie\u00b7mand", "zwingt", "uns", "an", "das", "Joch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Raubt man uns so Ehr als G\u00fcter,", "tokens": ["Raubt", "man", "uns", "so", "Ehr", "als", "G\u00fc\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "ADV", "NN", "KOUS", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Bleibet unsre Hofnung doch.", "tokens": ["Blei\u00b7bet", "uns\u00b7re", "Hof\u00b7nung", "doch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Bey dem lustigen Erzehlen", "tokens": ["Bey", "dem", "lus\u00b7ti\u00b7gen", "Er\u00b7zeh\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird uns keine Meile lang;", "tokens": ["Wird", "uns", "kei\u00b7ne", "Mei\u00b7le", "lang", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn die Federn manchen qu\u00e4len,", "tokens": ["Wenn", "die", "Fe\u00b7dern", "man\u00b7chen", "qu\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ruhn wir auf der h\u00e4rtsten Banck.", "tokens": ["Ruhn", "wir", "auf", "der", "h\u00e4rts\u00b7ten", "Banck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wir durchgehn die meisten St\u00e4nde,", "tokens": ["Wir", "durch\u00b7gehn", "die", "meis\u00b7ten", "St\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "PIAT", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Sehn gemeiner Thorheit zu,", "tokens": ["Sehn", "ge\u00b7mei\u00b7ner", "Thor\u00b7heit", "zu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Lachen heimlich in die H\u00e4nde", "tokens": ["La\u00b7chen", "heim\u00b7lich", "in", "die", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und bef\u00f6rdern unsre Ruh.", "tokens": ["Und", "be\u00b7f\u00f6r\u00b7dern", "uns\u00b7re", "Ruh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Hat die Vorsicht ein Erbarmen,", "tokens": ["Hat", "die", "Vor\u00b7sicht", "ein", "Er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sieht sie Treu und Wei\u00dfheit an,", "tokens": ["Sieht", "sie", "Treu", "und", "Wei\u00df\u00b7heit", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "O so ist es mit uns Armen", "tokens": ["O", "so", "ist", "es", "mit", "uns", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VAFIN", "PPER", "APPR", "PPER", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Noch zur Zeit nicht gar gethan.", "tokens": ["Noch", "zur", "Zeit", "nicht", "gar", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PTKNEG", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gro\u00dfmuth macht den Neid zu Schanden,", "tokens": ["Gro\u00df\u00b7muth", "macht", "den", "Neid", "zu", "Schan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Naht sich doch wohl schon die Zeit,", "tokens": ["Naht", "sich", "doch", "wohl", "schon", "die", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ADV", "ADV", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da uns, was wir ausgestanden,", "tokens": ["Da", "uns", ",", "was", "wir", "aus\u00b7ge\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Ungemeine Lust verleiht.", "tokens": ["Un\u00b7ge\u00b7mei\u00b7ne", "Lust", "ver\u00b7leiht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Bruder, fort, es geht nach Jauer,", "tokens": ["Bru\u00b7der", ",", "fort", ",", "es", "geht", "nach", "Jau\u00b7er", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PTKVZ", "$,", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bruder, fort, und las uns gehn;", "tokens": ["Bru\u00b7der", ",", "fort", ",", "und", "las", "uns", "gehn", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird uns Weg und Wetter sauer,", "tokens": ["Wird", "uns", "Weg", "und", "Wet\u00b7ter", "sau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "KON", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Soll es doch bald anders stehn.", "tokens": ["Soll", "es", "doch", "bald", "an\u00b7ders", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Fort, ich h\u00f6re schon die Lieder", "tokens": ["Fort", ",", "ich", "h\u00f6\u00b7re", "schon", "die", "Lie\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf den nechsten Freudenschmaus!", "tokens": ["Auf", "den", "nechs\u00b7ten", "Freu\u00b7den\u00b7schmaus", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Mertsch\u00fcz sehn wir wohl nicht wieder;", "tokens": ["Mert\u00b7sch\u00fcz", "sehn", "wir", "wohl", "nicht", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Freund, wo geht der Weg hinaus?", "tokens": ["Freund", ",", "wo", "geht", "der", "Weg", "hin\u00b7aus", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}