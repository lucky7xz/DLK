{"dta.poem.7598": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "13.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Sprichwort sagt, da\u00df Art von Art nicht lass'; ich glaube,", "tokens": ["Das", "Sprich\u00b7wort", "sagt", ",", "da\u00df", "Art", "von", "Art", "nicht", "lass'", ";", "ich", "glau\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "NN", "APPR", "NN", "PTKNEG", "VVFIN", "$.", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df durch Erziehung nie zum Adler ward die Taube.", "tokens": ["Da\u00df", "durch", "Er\u00b7zie\u00b7hung", "nie", "zum", "Ad\u00b7ler", "ward", "die", "Tau\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "ADV", "APPRART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch innerhalb der Art, wird ganz von gleichem Stamm,", "tokens": ["Doch", "in\u00b7ner\u00b7halb", "der", "Art", ",", "wird", "ganz", "von", "glei\u00b7chem", "Stamm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "VAFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zum Widder hier, und dort zum Sch\u00f6pfen nur, das Lamm.", "tokens": ["Zum", "Wid\u00b7der", "hier", ",", "und", "dort", "zum", "Sch\u00f6p\u00b7fen", "nur", ",", "das", "Lamm", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$,", "KON", "ADV", "APPRART", "NN", "ADV", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und wie Erziehung selbst den Stand macht, ist erschienen", "tokens": ["Und", "wie", "Er\u00b7zie\u00b7hung", "selbst", "den", "Stand", "macht", ",", "ist", "er\u00b7schie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PWAV", "NN", "ADV", "ART", "NN", "VVFIN", "$,", "VAFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Am musterg\u00fcltigen Verfahren sinn'ger Bienen.", "tokens": ["Am", "mus\u00b7ter\u00b7g\u00fcl\u00b7ti\u00b7gen", "Ver\u00b7fah\u00b7ren", "sinn'\u00b7ger", "Bie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nur einen Weisel ziehn in einem Stock sie klug,", "tokens": ["Nur", "ei\u00b7nen", "Wei\u00b7sel", "ziehn", "in", "ei\u00b7nem", "Stock", "sie", "klug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil f\u00fcr ein ganzes Volk ein Herrscher ist genug.", "tokens": ["Weil", "f\u00fcr", "ein", "gan\u00b7zes", "Volk", "ein", "Herr\u00b7scher", "ist", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "ART", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Doch wenn zu Schaden kam die k\u00f6nigliche Brut,", "tokens": ["Doch", "wenn", "zu", "Scha\u00b7den", "kam", "die", "k\u00f6\u00b7nig\u00b7li\u00b7che", "Brut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So machen sie durch Kunst den Schaden wieder gut.", "tokens": ["So", "ma\u00b7chen", "sie", "durch", "Kunst", "den", "Scha\u00b7den", "wie\u00b7der", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Ein andres Bienenkind nehmen sie, das zu weiter", "tokens": ["Ein", "and\u00b7res", "Bie\u00b7nen\u00b7kind", "neh\u00b7men", "sie", ",", "das", "zu", "wei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nichts war bestimmt als zum einfachen Feldarbeiter;", "tokens": ["Nichts", "war", "be\u00b7stimmt", "als", "zum", "ein\u00b7fa\u00b7chen", "Feld\u00b7ar\u00b7bei\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "VVPP", "KOKOM", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Erweitern nur die Zell' in der es liegt, und legen", "tokens": ["Er\u00b7wei\u00b7tern", "nur", "die", "Zell'", "in", "der", "es", "liegt", ",", "und", "le\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ADV", "ART", "NN", "APPR", "PRELS", "PPER", "VVFIN", "$,", "KON", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihm bessre Nahrung zu, so w\u00e4chsts mit Zaubersegen.", "tokens": ["Ihm", "bess\u00b7re", "Nah\u00b7rung", "zu", ",", "so", "w\u00e4chsts", "mit", "Zau\u00b7ber\u00b7se\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PTKVZ", "$,", "ADV", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Aus einem Arbeitsmann ist schnell ein Weisel worden,", "tokens": ["Aus", "ei\u00b7nem", "Ar\u00b7beits\u00b7mann", "ist", "schnell", "ein", "Wei\u00b7sel", "wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "ADJD", "ART", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als \u00e4chter Stammf\u00fcrst anerkannt von seinen Horden.", "tokens": ["Als", "\u00e4ch\u00b7ter", "Stamm\u00b7f\u00fcrst", "an\u00b7er\u00b7kannt", "von", "sei\u00b7nen", "Hor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}