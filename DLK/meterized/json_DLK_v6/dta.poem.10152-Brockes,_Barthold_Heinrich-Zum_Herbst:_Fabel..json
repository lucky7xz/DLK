{"dta.poem.10152": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Zum Herbst:  \n Fabel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein j\u00fcngst noch dick belaubter Baum", "tokens": ["Ein", "j\u00fcngst", "noch", "dick", "be\u00b7laub\u00b7ter", "Baum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sah seines Wipfels Pracht erbleicht zu seinen F\u00fcssen,", "tokens": ["Sah", "sei\u00b7nes", "Wip\u00b7fels", "Pracht", "er\u00b7bleicht", "zu", "sei\u00b7nen", "F\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und, wie des Bodens runder Raum,", "tokens": ["Und", ",", "wie", "des", "Bo\u00b7dens", "run\u00b7der", "Raum", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den die so angenehm begr\u00fcnten Schatten", "tokens": ["Den", "die", "so", "an\u00b7ge\u00b7nehm", "be\u00b7gr\u00fcn\u00b7ten", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "ADV", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So offt gesch\u00fctzt, so offt bedecket hatten,", "tokens": ["So", "offt", "ge\u00b7sch\u00fctzt", ",", "so", "offt", "be\u00b7de\u00b7cket", "hat\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$,", "ADV", "ADV", "VVFIN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Den lieben Kinderchen zum Kirch-Hof werden m\u00fcssen.", "tokens": ["Den", "lie\u00b7ben", "Kin\u00b7der\u00b7chen", "zum", "Kirch\u00b7Hof", "wer\u00b7den", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es ri\u00df der kalt\u2019 und rauhe Nord", "tokens": ["Es", "ri\u00df", "der", "kalt'", "und", "rau\u00b7he", "Nord"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den d\u00fcnnen Uberrest noch immer mit sich fort,", "tokens": ["Den", "d\u00fcn\u00b7nen", "Ub\u00b7er\u00b7rest", "noch", "im\u00b7mer", "mit", "sich", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADV", "APPR", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sie taumelten recht Schaaren-weis\u2019 herab,", "tokens": ["Sie", "tau\u00b7mel\u00b7ten", "recht", "Schaa\u00b7ren\u00b7weis'", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und suncken in das finstre Grab.", "tokens": ["Und", "sun\u00b7cken", "in", "das", "finst\u00b7re", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}