{"textgrid.poem.25273": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Nacht, wie warst du sonst so heilig sch\u00f6n!", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Nacht, wie warst du sonst so heilig sch\u00f6n!", "tokens": ["O", "Nacht", ",", "wie", "warst", "du", "sonst", "so", "hei\u00b7lig", "sch\u00f6n", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Am Erdenbusen lag der Mondenschein;", "tokens": ["Am", "Er\u00b7den\u00b7bu\u00b7sen", "lag", "der", "Mon\u00b7den\u00b7schein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es war sein Traum ein silberzart Get\u00f6n;", "tokens": ["Es", "war", "sein", "Traum", "ein", "sil\u00b7ber\u00b7zart", "Ge\u00b7t\u00f6n", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und Ruheglocken summten aus den H\u00f6hn ...", "tokens": ["Und", "Ru\u00b7he\u00b7glo\u00b7cken", "summ\u00b7ten", "aus", "den", "H\u00f6hn", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "O Nacht, und nun? Wie schaurig kannst du sein!", "tokens": ["O", "Nacht", ",", "und", "nun", "?", "Wie", "schau\u00b7rig", "kannst", "du", "sein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "KON", "ADV", "$.", "PWAV", "ADJD", "VMFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da lieg ich lahm, zerschmettert mein Gebein;", "tokens": ["Da", "lieg", "ich", "lahm", ",", "zer\u00b7schmet\u00b7tert", "mein", "Ge\u00b7bein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Im Abgrund lieg ich, finster, ganz allein.", "tokens": ["Im", "Ab\u00b7grund", "lieg", "ich", ",", "fins\u00b7ter", ",", "ganz", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$,", "ADJA", "$,", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich st\u00f6hne, zitternd strecken sich die Arme:", "tokens": ["Ich", "st\u00f6h\u00b7ne", ",", "zit\u00b7ternd", "stre\u00b7cken", "sich", "die", "Ar\u00b7me", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVPP", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ist droben keiner, der sich mein erbarme?", "tokens": ["Ist", "dro\u00b7ben", "kei\u00b7ner", ",", "der", "sich", "mein", "er\u00b7bar\u00b7me", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIS", "$,", "PRELS", "PRF", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ist ausgeloschen aller Sterne Schimmern?", "tokens": ["Ist", "aus\u00b7ge\u00b7lo\u00b7schen", "al\u00b7ler", "Ster\u00b7ne", "Schim\u00b7mern", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ich h\u00f6re nur dein monotones Wimmern,", "tokens": ["Ich", "h\u00f6\u00b7re", "nur", "dein", "mo\u00b7no\u00b7to\u00b7nes", "Wim\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Du Unrastseele, ewger Jude Wind!", "tokens": ["Du", "Un\u00b7rast\u00b7see\u00b7le", ",", "ew\u00b7ger", "Ju\u00b7de", "Wind", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Stumm sei und stolz mein zuckender Mund!", "tokens": ["Stumm", "sei", "und", "stolz", "mein", "zu\u00b7cken\u00b7der", "Mund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "KON", "ADJD", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Was mich gest\u00fcrzt in den schwarzen Schlund,", "tokens": ["Was", "mich", "ge\u00b7st\u00fcrzt", "in", "den", "schwar\u00b7zen", "Schlund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Keinem Atmenden werd es kund!", "tokens": ["Kei\u00b7nem", "At\u00b7men\u00b7den", "werd", "es", "kund", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Sie schlafen; oder ihr Schwelgen lacht.", "tokens": ["Sie", "schla\u00b7fen", ";", "o\u00b7der", "ihr", "Schwel\u00b7gen", "lacht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Was k\u00fcmmert sie's, wenn ein Gr\u00fcbler wacht", "tokens": ["Was", "k\u00fcm\u00b7mert", "sie's", ",", "wenn", "ein", "Gr\u00fcb\u00b7ler", "wacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und sich qu\u00e4lt mit der schwarzen Nacht!", "tokens": ["Und", "sich", "qu\u00e4lt", "mit", "der", "schwar\u00b7zen", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Nur Toten", "tokens": ["Nur", "To\u00b7ten"], "token_info": ["word", "word"], "pos": ["ADV", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Sei mein Flehen entboten.", "tokens": ["Sei", "mein", "Fle\u00b7hen", "ent\u00b7bo\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Sie lassen sich rufen, sie neigen", "tokens": ["Sie", "las\u00b7sen", "sich", "ru\u00b7fen", ",", "sie", "nei\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "VVINF", "$,", "PPER", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Dem hilflos Einsamen ihre Huld,", "tokens": ["Dem", "hil\u00b7flos", "Ein\u00b7sa\u00b7men", "ih\u00b7re", "Huld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Haben f\u00fcr all sein Beichten Geduld", "tokens": ["Ha\u00b7ben", "f\u00fcr", "all", "sein", "Beich\u00b7ten", "Ge\u00b7duld"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PIAT", "PPOSAT", "ADJA", "NN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Und k\u00f6nnen wie Gr\u00fcfte schweigen.", "tokens": ["Und", "k\u00f6n\u00b7nen", "wie", "Gr\u00fcf\u00b7te", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "KOKOM", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Nun denn, mein Vater! komm aus deinem Grabe,", "tokens": ["Nun", "denn", ",", "mein", "Va\u00b7ter", "!", "komm", "aus", "dei\u00b7nem", "Gra\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PPOSAT", "NN", "$.", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus meinem Herzen komm und la\u00df dich schaun!", "tokens": ["Aus", "mei\u00b7nem", "Her\u00b7zen", "komm", "und", "la\u00df", "dich", "schaun", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "KON", "VVIMP", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Liebernstes Angesicht, sieh her! Dein Knabe,", "tokens": ["Lie\u00b7berns\u00b7tes", "An\u00b7ge\u00b7sicht", ",", "sieh", "her", "!", "Dein", "Kna\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVIMP", "PTKVZ", "$.", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er ist's, er liegt in Zweifel und in Graun", "tokens": ["Er", "ist's", ",", "er", "liegt", "in", "Zwei\u00b7fel", "und", "in", "Graun"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "PPER", "VVFIN", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und m\u00f6chte schluchzend sich dir anvertraun.", "tokens": ["Und", "m\u00f6ch\u00b7te", "schluch\u00b7zend", "sich", "dir", "an\u00b7ver\u00b7traun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADJD", "PRF", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Auch du, Gro\u00dfmutter mit den Silberlocken,", "tokens": ["Auch", "du", ",", "Gro\u00df\u00b7mut\u00b7ter", "mit", "den", "Sil\u00b7berl\u00b7o\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Du weise Frau, die g\u00fctig mich gekos't,", "tokens": ["Du", "wei\u00b7se", "Frau", ",", "die", "g\u00fc\u00b7tig", "mich", "ge\u00b7kos't", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PRELS", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wenn vor der Welt mein Kinderherz erschrocken,", "tokens": ["Wenn", "vor", "der", "Welt", "mein", "Kin\u00b7der\u00b7herz", "er\u00b7schro\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Dein gro\u00dfes blaues Auge sei mein Trost!", "tokens": ["Dein", "gro\u00b7\u00dfes", "blau\u00b7es", "Au\u00b7ge", "sei", "mein", "Trost", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Wohlan, ihr Treuen, la\u00dft euch klagen", "tokens": ["Wo\u00b7hlan", ",", "ihr", "Treu\u00b7en", ",", "la\u00dft", "euch", "kla\u00b7gen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "PPOSAT", "NN", "$,", "VVIMP", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und mein Geheimnis sagen ...", "tokens": ["Und", "mein", "Ge\u00b7heim\u00b7nis", "sa\u00b7gen", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch nein, nicht sagen! nur mit Schweigen spricht", "tokens": ["Doch", "nein", ",", "nicht", "sa\u00b7gen", "!", "nur", "mit", "Schwei\u00b7gen", "spricht"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PTKANT", "$,", "PTKNEG", "VVINF", "$.", "ADV", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Seele, wo sie hei\u00df aus Tiefen bricht.", "tokens": ["Die", "See\u00b7le", ",", "wo", "sie", "hei\u00df", "aus", "Tie\u00b7fen", "bricht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und ihr, dem Schattenreiche eigen,", "tokens": ["Und", "ihr", ",", "dem", "Schat\u00b7ten\u00b7rei\u00b7che", "ei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Liebt ja das Stumme und versteht mein Schweigen.", "tokens": ["Liebt", "ja", "das", "Stum\u00b7me", "und", "ver\u00b7steht", "mein", "Schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ich sp\u00fcre eure milden Augensterne;", "tokens": ["Ich", "sp\u00fc\u00b7re", "eu\u00b7re", "mil\u00b7den", "Au\u00b7gens\u00b7ter\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ihr Schauen dringt ins Tiefste mir und f\u00fchlt,", "tokens": ["Ihr", "Schau\u00b7en", "dringt", "ins", "Tiefs\u00b7te", "mir", "und", "f\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "ADJA", "PPER", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Was hier im Busen gl\u00fcht und zuckt und w\u00fchlt.", "tokens": ["Was", "hier", "im", "Bu\u00b7sen", "gl\u00fcht", "und", "zuckt", "und", "w\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPRART", "NN", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Wie lieg ich unter diesem Blick so gerne,", "tokens": ["Wie", "lieg", "ich", "un\u00b7ter", "die\u00b7sem", "Blick", "so", "ger\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "APPR", "PDAT", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Der mich wie Tau benetzt und meine Wunde k\u00fchlt.", "tokens": ["Der", "mich", "wie", "Tau", "be\u00b7netzt", "und", "mei\u00b7ne", "Wun\u00b7de", "k\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "KOKOM", "NN", "VVPP", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Bin ich genesen? Wieder heilig sch\u00f6n", "tokens": ["Bin", "ich", "ge\u00b7ne\u00b7sen", "?", "Wie\u00b7der", "hei\u00b7lig", "sch\u00f6n"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "VVPP", "$.", "ADV", "ADJD", "ADJD"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "D\u00fcnkt mich die Nacht. Die feierlichen Glocken", "tokens": ["D\u00fcnkt", "mich", "die", "Nacht", ".", "Die", "fei\u00b7er\u00b7li\u00b7chen", "Glo\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ersummen abermals in Himmelsh\u00f6hn,", "tokens": ["Er\u00b7sum\u00b7men", "a\u00b7ber\u00b7mals", "in", "Him\u00b7mels\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Als m\u00f6chten sie mir neues Hoffen locken.", "tokens": ["Als", "m\u00f6ch\u00b7ten", "sie", "mir", "neu\u00b7es", "Hof\u00b7fen", "lo\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und horch, was zwitschert sch\u00fcchtern sacht?", "tokens": ["Und", "