{"dta.poem.1625": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Vermischte  \n Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Bezwingerin der Welt/ wer hat dich zu dem Fall", "tokens": ["Be\u00b7zwin\u00b7ge\u00b7rin", "der", "Welt", "/", "wer", "hat", "dich", "zu", "dem", "Fall"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$(", "PWS", "VAFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-++--+", "measure": "iambic.hexa.chol"}, "line.2": {"text": "Von deinem Thron gebracht? wer hat der stoltzen Glieder", "tokens": ["Von", "dei\u00b7nem", "Thron", "ge\u00b7bracht", "?", "wer", "hat", "der", "stolt\u00b7zen", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$.", "PWS", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "en gro\u00dfen Leib beraubt? wer hat dir hin und wieder", "tokens": ["en", "gro\u00b7\u00dfen", "Leib", "be\u00b7raubt", "?", "wer", "hat", "dir", "hin", "und", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["FM", "ADJA", "NN", "VVPP", "$.", "PWS", "VAFIN", "PPER", "PTKVZ", "KON", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "ie Bein umher gestreut? kein schlauer Hannibal", "tokens": ["ie", "Bein", "um\u00b7her", "ge\u00b7streut", "?", "kein", "schlau\u00b7er", "Han\u00b7ni\u00b7bal"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "NN", "PTKVZ", "VVPP", "$.", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "at dich in Grund gest\u00fcrtzt/ kein Brennus umgekehrt.", "tokens": ["at", "dich", "in", "Grund", "ge\u00b7st\u00fcrtzt", "/", "kein", "Bren\u00b7nus", "um\u00b7ge\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "VVPP", "$(", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "om muste seinen Tod in eignem Busen haben/", "tokens": ["om", "mus\u00b7te", "sei\u00b7nen", "Tod", "in", "eig\u00b7nem", "Bu\u00b7sen", "ha\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "ich selbsten feinden an/ ert\u00f6dten und begraben:", "tokens": ["ich", "selbs\u00b7ten", "fein\u00b7den", "an", "/", "er\u00b7t\u00f6d\u00b7ten", "und", "be\u00b7gra\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "PTKVZ", "$(", "VVFIN", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "ie Hoffart war der Feind/ die Zwietracht war das Schwerdt.", "tokens": ["ie", "Hof\u00b7fart", "war", "der", "Feind", "/", "die", "Zwiet\u00b7racht", "war", "das", "Schwerdt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "ART", "NN", "$(", "ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}