{"textgrid.poem.54147": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Betriebsunfall", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hat eine Katze Ellenbogen?", "tokens": ["Hat", "ei\u00b7ne", "Kat\u00b7ze", "El\u00b7len\u00b7bo\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nein.", "tokens": ["Nein", "."], "token_info": ["word", "punct"], "pos": ["PTKANT", "$."], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Hat jemals ein Bankier betrogen?", "tokens": ["Hat", "je\u00b7mals", "ein", "Ban\u00b7kier", "be\u00b7tro\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+++-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Nein.", "tokens": ["Nein", "."], "token_info": ["word", "punct"], "pos": ["PTKANT", "$."], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Und wenn er mal und hat er mal und f\u00e4llt er schon mal rein:", "tokens": ["Und", "wenn", "er", "mal", "und", "hat", "er", "mal", "und", "f\u00e4llt", "er", "schon", "mal", "rein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "KON", "VAFIN", "PPER", "ADV", "KON", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "dann kann das kein Bankier gewesen sein.", "tokens": ["dann", "kann", "das", "kein", "Ban\u00b7kier", "ge\u00b7we\u00b7sen", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "PIAT", "NN", "VAPP", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Liest du das gern, nachmittags aufm Sofa?", "tokens": ["Liest", "du", "das", "gern", ",", "nach\u00b7mit\u00b7tags", "aufm", "So\u00b7fa", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "ADV", "$,", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nein.", "tokens": ["Nein", "."], "token_info": ["word", "punct"], "pos": ["PTKANT", "$."], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Entsch\u00e4digt einer Schulthei\u00df-Patzenhofa?", "tokens": ["Ent\u00b7sch\u00e4\u00b7digt", "ei\u00b7ner", "Schult\u00b7hei\u00df\u00b7Pat\u00b7zen\u00b7ho\u00b7fa", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Nein.", "tokens": ["Nein", "."], "token_info": ["word", "punct"], "pos": ["PTKANT", "$."], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Und setzt auch dem der Staatsanwalt in seinen Pelz ne Laus:", "tokens": ["Und", "setzt", "auch", "dem", "der", "Staats\u00b7an\u00b7walt", "in", "sei\u00b7nen", "Pelz", "ne", "Laus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "ART", "NN", "APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "dann holn ihn die Verteidiger wieder raus.", "tokens": ["dann", "holn", "ihn", "die", "Ver\u00b7tei\u00b7di\u00b7ger", "wie\u00b7der", "raus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Ist das nun f\u00fcr die B\u00f6rse sehr betr\u00fcblich?", "tokens": ["Ist", "das", "nun", "f\u00fcr", "die", "B\u00f6r\u00b7se", "sehr", "be\u00b7tr\u00fcb\u00b7lich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ADV", "APPR", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nein.", "tokens": ["Nein", "."], "token_info": ["word", "punct"], "pos": ["PTKANT", "$."], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Ist das auch bei den andern \u00fcblich?", "tokens": ["Ist", "das", "auch", "bei", "den", "an\u00b7dern", "\u00fcb\u00b7lich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ADV", "APPR", "ART", "ADJA", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ja.", "tokens": ["Ja", "."], "token_info": ["word", "punct"], "pos": ["PTKANT", "$."], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Ich lese still den Handelsteil, und seh ich so den Mist:", "tokens": ["Ich", "le\u00b7se", "still", "den", "Han\u00b7del\u00b7steil", ",", "und", "seh", "ich", "so", "den", "Mist", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "$,", "KON", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "man wei\u00df nie, was noch T\u00fcchtigkeit und was schon Schiebung ist.", "tokens": ["man", "wei\u00df", "nie", ",", "was", "noch", "T\u00fcch\u00b7tig\u00b7keit", "und", "was", "schon", "Schie\u00b7bung", "ist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "$,", "PRELS", "ADV", "NN", "KON", "PWS", "ADV", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}