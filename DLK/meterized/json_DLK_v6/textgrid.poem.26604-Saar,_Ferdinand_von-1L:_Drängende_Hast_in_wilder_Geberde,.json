{"textgrid.poem.26604": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dr\u00e4ngende Hast in wilder Geberde,", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dr\u00e4ngende Hast in wilder Geberde,", "tokens": ["Dr\u00e4n\u00b7gen\u00b7de", "Hast", "in", "wil\u00b7der", "Ge\u00b7ber\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+----", "measure": "iambic.tri.invert"}, "line.2": {"text": "Gabe heischend mit thierischem Laut,", "tokens": ["Ga\u00b7be", "hei\u00b7schend", "mit", "thie\u00b7ri\u00b7schem", "Laut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Steht sie vor mir, wie entwachsen der Erde,", "tokens": ["Steht", "sie", "vor", "mir", ",", "wie", "ent\u00b7wach\u00b7sen", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "$,", "PWAV", "VVPP", "ART", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Da\u00df es in tiefster Seele mir graut.", "tokens": ["Da\u00df", "es", "in", "tiefs\u00b7ter", "See\u00b7le", "mir", "graut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Aus dem Antlitz mit grellem Funkeln", "tokens": ["Aus", "dem", "Ant\u00b7litz", "mit", "grel\u00b7lem", "Fun\u00b7keln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Schauen die Augen voll Gier und Trutz,", "tokens": ["Schau\u00b7en", "die", "Au\u00b7gen", "voll", "Gier", "und", "Trutz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Um die Glieder, die schlanken, dunkeln,", "tokens": ["Um", "die", "Glie\u00b7der", ",", "die", "schlan\u00b7ken", ",", "dun\u00b7keln", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUI", "ART", "NN", "$,", "PRELS", "VVINF", "$,", "ADJA", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "H\u00e4ngt es in Lumpen, starrend von Schmutz.", "tokens": ["H\u00e4ngt", "es", "in", "Lum\u00b7pen", ",", "star\u00b7rend", "von", "Schmutz", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,", "ADJD", "APPR", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Doch so gewahr' ich strotzende Br\u00fcste,", "tokens": ["Doch", "so", "ge\u00b7wahr'", "ich", "strot\u00b7zen\u00b7de", "Br\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Feingeformt wie die schmale Hand,", "tokens": ["Fein\u00b7ge\u00b7formt", "wie", "die", "schma\u00b7le", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Und durch die H\u00fclle, die lose, w\u00fcste,", "tokens": ["Und", "durch", "die", "H\u00fcl\u00b7le", ",", "die", "lo\u00b7se", ",", "w\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "D\u00e4mmert der H\u00fcfte schwellender Rand. \u2013", "tokens": ["D\u00e4m\u00b7mert", "der", "H\u00fcf\u00b7te", "schwel\u00b7len\u00b7der", "Rand", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$.", "$("], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Da\u00df er zuletzt noch mit dir vers\u00f6hne,", "tokens": ["Da\u00df", "er", "zu\u00b7letzt", "noch", "mit", "dir", "ver\u00b7s\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Brauner Unhold, verfehmtes Weib,", "tokens": ["Brau\u00b7ner", "Un\u00b7hold", ",", "ver\u00b7fehm\u00b7tes", "Weib", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Weisest du achtlos in seiner Sch\u00f6ne", "tokens": ["Wei\u00b7sest", "du", "acht\u00b7los", "in", "sei\u00b7ner", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sieghaften Zaubers den Menschenleib!", "tokens": ["Sieg\u00b7haf\u00b7ten", "Zau\u00b7bers", "den", "Men\u00b7schen\u00b7leib", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}