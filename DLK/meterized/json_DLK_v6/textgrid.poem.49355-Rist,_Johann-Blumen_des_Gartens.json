{"textgrid.poem.49355": {"metadata": {"author": {"name": "Rist, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Blumen des Gartens", "genre": "verse", "period": "N.A.", "pub_year": 1637, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liebster, wilst du meiner warten,", "tokens": ["Liebs\u00b7ter", ",", "wilst", "du", "mei\u00b7ner", "war\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VMFIN", "PPER", "PPOSAT", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bis die Sonne bricht herf\u00fcr,", "tokens": ["Bis", "die", "Son\u00b7ne", "bricht", "her\u00b7f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mich f\u00fchren in den Garten", "tokens": ["Und", "mich", "f\u00fch\u00b7ren", "in", "den", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch der Andacht sch\u00f6ne Th\u00fcr,", "tokens": ["Durch", "der", "An\u00b7dacht", "sch\u00f6\u00b7ne", "Th\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Zarter Blumen Lieblichkeit", "tokens": ["Zar\u00b7ter", "Blu\u00b7men", "Lieb\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "In der s\u00fc\u00dfen Fr\u00fchlingszeit", "tokens": ["In", "der", "s\u00fc\u00b7\u00dfen", "Fr\u00fch\u00b7lings\u00b7zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Mit Verwundern zu besehen,", "tokens": ["Mit", "Ver\u00b7wun\u00b7dern", "zu", "be\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ei, so kom und la\u00df uns gehen!", "tokens": ["Ei", ",", "so", "kom", "und", "la\u00df", "uns", "ge\u00b7hen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "KON", "VVIMP", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Jesu, sol ich deinen Augen", "tokens": ["Je\u00b7su", ",", "sol", "ich", "dei\u00b7nen", "Au\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Einmal recht gef\u00e4llig sein,", "tokens": ["Ein\u00b7mal", "recht", "ge\u00b7f\u00e4l\u00b7lig", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Sol mein Schmuck nur etwas taugen,", "tokens": ["Sol", "mein", "Schmuck", "nur", "et\u00b7was", "tau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Sol ich prangen hell und rein", "tokens": ["Sol", "ich", "pran\u00b7gen", "hell", "und", "rein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Dir zur Ehr und mir zum Ruhm:", "tokens": ["Dir", "zur", "Ehr", "und", "mir", "zum", "Ruhm", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "KON", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Ei, so must du manche Blum'", "tokens": ["Ei", ",", "so", "must", "du", "man\u00b7che", "Blum'"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "VMFIN", "PPER", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "An den klaren Tugendb\u00e4chen,", "tokens": ["An", "den", "kla\u00b7ren", "Tu\u00b7gend\u00b7b\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Mich zu zieren, freundlich brechen.", "tokens": ["Mich", "zu", "zie\u00b7ren", ",", "freund\u00b7lich", "bre\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Ja, du f\u00fchrst mich bei den H\u00e4nden", "tokens": ["Ja", ",", "du", "f\u00fchrst", "mich", "bei", "den", "H\u00e4n\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Zu dem bunten Blumenheer;", "tokens": ["Zu", "dem", "bun\u00b7ten", "Blu\u00b7men\u00b7heer", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.19": {"text": "Ach, wohin sol ich mich wenden,", "tokens": ["Ach", ",", "wo\u00b7hin", "sol", "ich", "mich", "wen\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Finden, was ich l\u00e4ngst begehr?", "tokens": ["Fin\u00b7den", ",", "was", "ich", "l\u00e4ngst", "be\u00b7gehr", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Haben dort nicht ihre Stell'", "tokens": ["Ha\u00b7ben", "dort", "nicht", "ih\u00b7re", "Stell'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PTKNEG", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Edle Rosen, die so hell", "tokens": ["Ed\u00b7le", "Ro\u00b7sen", ",", "die", "so", "hell"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Und gar rot von Farben bl\u00fchen,", "tokens": ["Und", "gar", "rot", "von", "Far\u00b7ben", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Da\u00df sie Purpur vorzuziehen?", "tokens": ["Da\u00df", "sie", "Pur\u00b7pur", "vor\u00b7zu\u00b7zie\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Aber das so scharfe Stechen", "tokens": ["A\u00b7ber", "das", "so", "schar\u00b7fe", "Ste\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Ihrer Zweiglein thut mir weh;", "tokens": ["Ih\u00b7rer", "Zwei\u00b7glein", "thut", "mir", "weh", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Herr, du wollst es ja nicht r\u00e4chen,", "tokens": ["Herr", ",", "du", "wollst", "es", "ja", "nicht", "r\u00e4\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Wenn ich leider nochmals geh'", "tokens": ["Wenn", "ich", "lei\u00b7der", "noch\u00b7mals", "geh'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "In der schn\u00f6den Wollust Bahn,", "tokens": ["In", "der", "schn\u00f6\u00b7den", "Wol\u00b7lust", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.30": {"text": "Wie ich manchen Tag gethan,", "tokens": ["Wie", "ich", "man\u00b7chen", "Tag", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "So da\u00df ich in Schand und N\u00f6ten", "tokens": ["So", "da\u00df", "ich", "in", "Schand", "und", "N\u00f6\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.32": {"text": "Wie die Rose mu\u00df err\u00f6ten.", "tokens": ["Wie", "die", "Ro\u00b7se", "mu\u00df", "er\u00b7r\u00f6\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Lieblich zwar sind diese Rosen,", "tokens": ["Lieb\u00b7lich", "zwar", "sind", "die\u00b7se", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VAFIN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Dauern doch nur kurze Zeit;", "tokens": ["Dau\u00b7ern", "doch", "nur", "kur\u00b7ze", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "Solt' ich selber mich liebkosen", "tokens": ["Solt'", "ich", "sel\u00b7ber", "mich", "lieb\u00b7ko\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Wie ein Kind der Eitelkeit?", "tokens": ["Wie", "ein", "Kind", "der", "Ei\u00b7tel\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.37": {"text": "Nein, die Wollust fliegt dahin;", "tokens": ["Nein", ",", "die", "Wol\u00b7lust", "fliegt", "da\u00b7hin", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "VVFIN", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.38": {"text": "Auch des Lebens Rauberin,", "tokens": ["Auch", "des", "Le\u00b7bens", "Rau\u00b7be\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.39": {"text": "Unsre Zeit, mu\u00df schnell vergehen,", "tokens": ["Uns\u00b7re", "Zeit", ",", "mu\u00df", "schnell", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Wie die Rosen nicht bestehen.", "tokens": ["Wie", "die", "Ro\u00b7sen", "nicht", "be\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Liebster, f\u00fchre mich nur weiter", "tokens": ["Liebs\u00b7ter", ",", "f\u00fch\u00b7re", "mich", "nur", "wei\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Auf das klare Lilienfeld,", "tokens": ["Auf", "das", "kla\u00b7re", "Li\u00b7li\u00b7en\u00b7feld", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.43": {"text": "Brich mir eine, mein Begleiter!", "tokens": ["Brich", "mir", "ei\u00b7ne", ",", "mein", "Be\u00b7glei\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Bin ich dir doch zugesellt.", "tokens": ["Bin", "ich", "dir", "doch", "zu\u00b7ge\u00b7sellt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.45": {"text": "Ach, da\u00df solch ein edle Blum'", "tokens": ["Ach", ",", "da\u00df", "solch", "ein", "ed\u00b7le", "Blum'"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KOUS", "PIAT", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.46": {"text": "Ich in deinem Heiligtum", "tokens": ["Ich", "in", "dei\u00b7nem", "Hei\u00b7lig\u00b7tum"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.47": {"text": "M\u00f6cht' in rechter Unschuld hei\u00dfen", "tokens": ["M\u00f6cht'", "in", "rech\u00b7ter", "Un\u00b7schuld", "hei\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "Und von wahrer Tugend glei\u00dfen.", "tokens": ["Und", "von", "wah\u00b7rer", "Tu\u00b7gend", "glei\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.49": {"text": "Aller Menschen Schmuck und Prangen", "tokens": ["Al\u00b7ler", "Men\u00b7schen", "Schmuck", "und", "Pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.50": {"text": "Ist doch lauter Tr\u00fcgerei;", "tokens": ["Ist", "doch", "lau\u00b7ter", "Tr\u00fc\u00b7ge\u00b7rei", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.51": {"text": "Auch kein Kaiser kan's erlangen,", "tokens": ["Auch", "kein", "Kai\u00b7ser", "kan's", "er\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.52": {"text": "Da\u00df er gleich den Lilien sei.", "tokens": ["Da\u00df", "er", "gleich", "den", "Li\u00b7li\u00b7en", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.53": {"text": "Wil ich helle Kleider sehn,", "tokens": ["Wil", "ich", "hel\u00b7le", "Klei\u00b7der", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.54": {"text": "Darf ich nur zum Garten gehn,", "tokens": ["Darf", "ich", "nur", "zum", "Gar\u00b7ten", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.55": {"text": "Wo die Blumen auch erz\u00e4hlen,", "tokens": ["Wo", "die", "Blu\u00b7men", "auch", "er\u00b7z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.56": {"text": "Da\u00df uns Christen nichts kan fehlen.", "tokens": ["Da\u00df", "uns", "Chris\u00b7ten", "nichts", "kan", "feh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "PIS", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.57": {"text": "Ei, wie bl\u00fchen die Narcissen", "tokens": ["Ei", ",", "wie", "bl\u00fc\u00b7hen", "die", "Nar\u00b7cis\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.58": {"text": "Und Violen mancher Art!", "tokens": ["Und", "Vi\u00b7o\u00b7len", "man\u00b7cher", "Art", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.59": {"text": "Gleichwol l\u00e4\u00dft mein Freund mich wissen,", "tokens": ["Gleich\u00b7wol", "l\u00e4\u00dft", "mein", "Freund", "mich", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.60": {"text": "Da\u00df die Zeit sie nimmer spart.", "tokens": ["Da\u00df", "die", "Zeit", "sie", "nim\u00b7mer", "spart", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.61": {"text": "Was ist unser Leben doch!", "tokens": ["Was", "ist", "un\u00b7ser", "Le\u00b7ben", "doch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.62": {"text": "Wenn man ist bem\u00fchet noch,", "tokens": ["Wenn", "man", "ist", "be\u00b7m\u00fc\u00b7het", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VAFIN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.63": {"text": "Viel zu lernen, viel zu schaffen,", "tokens": ["Viel", "zu", "ler\u00b7nen", ",", "viel", "zu", "schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "$,", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.64": {"text": "Pflegt der Tod uns hinzuraffen.", "tokens": ["Pflegt", "der", "Tod", "uns", "hin\u00b7zu\u00b7raf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.65": {"text": "Meine Zeit ist fast vergangen:", "tokens": ["Mei\u00b7ne", "Zeit", "ist", "fast", "ver\u00b7gan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.66": {"text": "F\u00fchre mich, mein Jesu, hin,", "tokens": ["F\u00fch\u00b7re", "mich", ",", "mein", "Je\u00b7su", ",", "hin", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NE", "$,", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.67": {"text": "Wo sich stillet mein Verlangen", "tokens": ["Wo", "sich", "stil\u00b7let", "mein", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.68": {"text": "Und ich selbst dein Bl\u00fcmlein bin,", "tokens": ["Und", "ich", "selbst", "dein", "Bl\u00fcm\u00b7lein", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.69": {"text": "In das sch\u00f6nste Paradeis,", "tokens": ["In", "das", "sch\u00f6ns\u00b7te", "Pa\u00b7ra\u00b7deis", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.70": {"text": "Wo man nichts zu sagen wei\u00df", "tokens": ["Wo", "man", "nichts", "zu", "sa\u00b7gen", "wei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "PIS", "PTKZU", "VVINF", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.71": {"text": "Als von Jauchzen, Triumphieren,", "tokens": ["Als", "von", "Jauch\u00b7zen", ",", "Tri\u00b7um\u00b7phie\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.72": {"text": "Mit den Deinen zu regieren.", "tokens": ["Mit", "den", "Dei\u00b7nen", "zu", "re\u00b7gie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}