{"textgrid.poem.59715": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Mut des Geistes", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Klein wird die Erde, klein der Erde Sonne,", "tokens": ["Klein", "wird", "die", "Er\u00b7de", ",", "klein", "der", "Er\u00b7de", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "ADJD", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Meer der Sonnen o ein F\u00fcnkchen nur \u2013", "tokens": ["Im", "Meer", "der", "Son\u00b7nen", "o", "ein", "F\u00fcnk\u00b7chen", "nur", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "FM", "ART", "NN", "ADV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo bleibt dir da, o Mensch, die alte Wonne?", "tokens": ["Wo", "bleibt", "dir", "da", ",", "o", "Mensch", ",", "die", "al\u00b7te", "Won\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "$,", "FM", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo bleibt dein Stolz, du Endziel der Natur?", "tokens": ["Wo", "bleibt", "dein", "Stolz", ",", "du", "End\u00b7ziel", "der", "Na\u00b7tur", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "NN", "ART", "NN", "$."], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}}, "stanza.2": {"line.1": {"text": "Hast du den Mut, mit Erden zu zerstieben?", "tokens": ["Hast", "du", "den", "Mut", ",", "mit", "Er\u00b7den", "zu", "zer\u00b7stie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hast du den Mut, mit Sonnen zu vergehn?", "tokens": ["Hast", "du", "den", "Mut", ",", "mit", "Son\u00b7nen", "zu", "ver\u00b7gehn", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den G\u00f6ttermut, im allgemeinen Lieben", "tokens": ["Den", "G\u00f6t\u00b7ter\u00b7mut", ",", "im", "all\u00b7ge\u00b7mei\u00b7nen", "Lie\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im h\u00f6chsten Feuertode zu vergehn?", "tokens": ["Im", "h\u00f6chs\u00b7ten", "Feu\u00b7er\u00b7to\u00b7de", "zu", "ver\u00b7gehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Was Mut? Schaut Erden mir und Sonnen nieder!", "tokens": ["Was", "Mut", "?", "Schaut", "Er\u00b7den", "mir", "und", "Son\u00b7nen", "nie\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "$.", "VVFIN", "NN", "PPER", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sch\u00e4rft eurem Sehrohr t\u00e4glich weitres Ziel!", "tokens": ["Sch\u00e4rft", "eu\u00b7rem", "Seh\u00b7rohr", "t\u00e4g\u00b7lich", "weit\u00b7res", "Ziel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Denn meinen Mut, ihr schauet ihn nicht nieder,", "tokens": ["Denn", "mei\u00b7nen", "Mut", ",", "ihr", "schau\u00b7et", "ihn", "nicht", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wieviel Gefunkel eurem Rohr auch fiel.", "tokens": ["Wie\u00b7viel", "Ge\u00b7fun\u00b7kel", "eu\u00b7rem", "Rohr", "auch", "fiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Hoch \u00fcber euren Zahlen, euren Ma\u00dfen,", "tokens": ["Hoch", "\u00fc\u00b7ber", "eu\u00b7ren", "Zah\u00b7len", ",", "eu\u00b7ren", "Ma\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hoch \u00fcber eurem Gro\u00df und eurem Klein", "tokens": ["Hoch", "\u00fc\u00b7ber", "eu\u00b7rem", "Gro\u00df", "und", "eu\u00b7rem", "Klein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Fliegt er gl\u00fcckselig eigne Sonnenstra\u00dfen,", "tokens": ["Fliegt", "er", "gl\u00fcck\u00b7se\u00b7lig", "eig\u00b7ne", "Son\u00b7nen\u00b7stra\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Und keines eurer R\u00f6hren holt ihn ein.", "tokens": ["Und", "kei\u00b7nes", "eu\u00b7rer", "R\u00f6h\u00b7ren", "holt", "ihn", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "So bleibe mir mein K\u00fcglein, liebe Erde!", "tokens": ["So", "blei\u00b7be", "mir", "mein", "K\u00fcg\u00b7lein", ",", "lie\u00b7be", "Er\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So bleibe Erdensonnenf\u00fcnkchen mir!", "tokens": ["So", "blei\u00b7be", "Er\u00b7den\u00b7son\u00b7nen\u00b7f\u00fcnk\u00b7chen", "mir", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wohin von Gott ich auch verwehet werde,", "tokens": ["Wo\u00b7hin", "von", "Gott", "ich", "auch", "ver\u00b7we\u00b7het", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "PPER", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}