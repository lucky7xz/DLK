{"textgrid.poem.42896": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Einsamer Spazierflug", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun ich wie gestorben bin", "tokens": ["Nun", "ich", "wie", "ge\u00b7stor\u00b7ben", "bin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "KOKOM", "VVPP", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und wurde ein Engelein,", "tokens": ["Und", "wur\u00b7de", "ein", "En\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Fliege ich \u00fcber dein Wohnhaus hin.", "tokens": ["Flie\u00b7ge", "ich", "\u00fc\u00b7ber", "dein", "Wohn\u00b7haus", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "H\u00e4uschen klein.", "tokens": ["H\u00e4usc\u00b7hen", "klein", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Die du als Witwe wieder umworben", "tokens": ["Die", "du", "als", "Wit\u00b7we", "wie\u00b7der", "um\u00b7wor\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "KOUS", "NN", "ADV", "VVINF"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sein magst,", "tokens": ["Sein", "magst", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Da ich doch schon verstorben", "tokens": ["Da", "ich", "doch", "schon", "ver\u00b7stor\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bin \u2013. Was du wohl sagst?", "tokens": ["Bin", "\u2013", ".", "Was", "du", "wohl", "sagst", "?"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$(", "$.", "PWS", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Ob du gefa\u00dft bist oder klagst?", "tokens": ["Ob", "du", "ge\u00b7fa\u00dft", "bist", "o\u00b7der", "klagst", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Oder ob dein Humor wieder steht,", "tokens": ["O\u00b7der", "ob", "dein", "Hu\u00b7mor", "wie\u00b7der", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Du dessen eingedenk bist,", "tokens": ["Du", "des\u00b7sen", "ein\u00b7ge\u00b7denk", "bist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PDS", "VVFIN", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df ein aufrichtiges Gebet", "tokens": ["Da\u00df", "ein", "auf\u00b7rich\u00b7ti\u00b7ges", "Ge\u00b7bet"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ein unterweges Selbstgeschenk ist?", "tokens": ["Ein", "un\u00b7ter\u00b7we\u00b7ges", "Selbst\u00b7ge\u00b7schenk", "ist", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ach, wie es dir wohl geht?", "tokens": ["Ach", ",", "wie", "es", "dir", "wohl", "geht", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "PPER", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ob du dich verlassen meinst?", "tokens": ["Ob", "du", "dich", "ver\u00b7las\u00b7sen", "meinst", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ob du gar Gott verneinst,", "tokens": ["Ob", "du", "gar", "Gott", "ver\u00b7neinst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Anstatt da\u00df du dankbar", "tokens": ["An\u00b7statt", "da\u00df", "du", "dank\u00b7bar"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Bist. W\u00fc\u00dfte ich, da\u00df du jetzt so weinst", "tokens": ["Bist", ".", "W\u00fc\u00df\u00b7te", "ich", ",", "da\u00df", "du", "jetzt", "so", "weinst"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "$.", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "ADV", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wie einst, da ich krank war,", "tokens": ["Wie", "einst", ",", "da", "ich", "krank", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Kippte ich die Maschine kurz", "tokens": ["Kipp\u00b7te", "ich", "die", "Ma\u00b7schi\u00b7ne", "kurz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJD"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Steil ab auf Sturz.", "tokens": ["Steil", "ab", "auf", "Sturz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Oder sollte einem Engelein", "tokens": ["O\u00b7der", "soll\u00b7te", "ei\u00b7nem", "En\u00b7ge\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Solch ein Kegelpurz", "tokens": ["Solch", "ein", "Ke\u00b7gel\u00b7purz"], "token_info": ["word", "word", "word"], "pos": ["PIS", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Verboten sein??", "tokens": ["Ver\u00b7bo\u00b7ten", "sein", "??"], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}