{"textgrid.poem.30094": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie man Christi Leib kan essen, wie man Christi Blut kan trincken,", "tokens": ["Wie", "man", "Chris\u00b7ti", "Leib", "kan", "es\u00b7sen", ",", "wie", "man", "Chris\u00b7ti", "Blut", "kan", "trin\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "NE", "NN", "VMFIN", "VVINF", "$,", "PWAV", "PIS", "NE", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Lest sich jener di\u00df vernehmen, lest sich dieser das bed\u00fcncken.", "tokens": ["Lest", "sich", "je\u00b7ner", "di\u00df", "ver\u00b7neh\u00b7men", ",", "lest", "sich", "die\u00b7ser", "das", "be\u00b7d\u00fcn\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PDAT", "PDS", "VVINF", "$,", "VVFIN", "PRF", "PDAT", "PDS", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Der den Leib gab selbst zur Speise, der das Blut gab selbst zu trincken,", "tokens": ["Der", "den", "Leib", "gab", "selbst", "zur", "Spei\u00b7se", ",", "der", "das", "Blut", "gab", "selbst", "zu", "trin\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "ADV", "APPRART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Der wird leisten, was versprochen; ich wil glauben, du magst d\u00fcncken.", "tokens": ["Der", "wird", "leis\u00b7ten", ",", "was", "ver\u00b7spro\u00b7chen", ";", "ich", "wil", "glau\u00b7ben", ",", "du", "magst", "d\u00fcn\u00b7cken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VVINF", "$,", "PWS", "VVINF", "$.", "PPER", "VMFIN", "VVINF", "$,", "PPER", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}