{"textgrid.poem.25598": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nicht Poesie, nicht K\u00fcnste, die erg\u00f6tzen,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht Poesie, nicht K\u00fcnste, die erg\u00f6tzen,", "tokens": ["Nicht", "Poe\u00b7sie", ",", "nicht", "K\u00fcns\u00b7te", ",", "die", "er\u00b7g\u00f6t\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "PTKNEG", "NN", "$,", "PRELS", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nur das sch\u00e4tzt Crispus hoch, was Nutzen bringt.", "tokens": ["Nur", "das", "sch\u00e4tzt", "Cris\u00b7pus", "hoch", ",", "was", "Nut\u00b7zen", "bringt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VVFIN", "NE", "ADJD", "$,", "PWS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In einem Fall' nur nicht; denn, wie mich d\u00fcnkt,", "tokens": ["In", "ei\u00b7nem", "Fall'", "nur", "nicht", ";", "denn", ",", "wie", "mich", "d\u00fcnkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "PTKNEG", "$.", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Herr Crispus pflegt sich selbst sehr hoch zu sch\u00e4tzen.", "tokens": ["Herr", "Cris\u00b7pus", "pflegt", "sich", "selbst", "sehr", "hoch", "zu", "sch\u00e4t\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "PRF", "ADV", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}