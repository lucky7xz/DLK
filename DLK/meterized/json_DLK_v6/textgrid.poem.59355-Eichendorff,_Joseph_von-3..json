{"textgrid.poem.59355": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn zwei geschieden sind von Herz und Munde,", "tokens": ["Wenn", "zwei", "ge\u00b7schie\u00b7den", "sind", "von", "Herz", "und", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "CARD", "VVPP", "VAFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da ziehn Gedanken \u00fcber Berg' und Schl\u00fcfte", "tokens": ["Da", "ziehn", "Ge\u00b7dan\u00b7ken", "\u00fc\u00b7ber", "Ber\u00b7g'", "und", "Schl\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wie Tauben s\u00e4uselnd durch die blauen L\u00fcfte,", "tokens": ["Wie", "Tau\u00b7ben", "s\u00e4u\u00b7selnd", "durch", "die", "blau\u00b7en", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und tragen hin und wider s\u00fc\u00dfe Kunde.", "tokens": ["Und", "tra\u00b7gen", "hin", "und", "wi\u00b7der", "s\u00fc\u00b7\u00dfe", "Kun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "KON", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich schweif umsonst, so weit der Erde Runde,", "tokens": ["Ich", "schweif", "um\u00b7sonst", ",", "so", "weit", "der", "Er\u00b7de", "Run\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ADV", "ADJD", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und stieg ich hoch auch \u00fcber alle Kl\u00fcfte,", "tokens": ["Und", "stieg", "ich", "hoch", "auch", "\u00fc\u00b7ber", "al\u00b7le", "Kl\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dein Haus ist h\u00f6her noch als diese L\u00fcfte,", "tokens": ["Dein", "Haus", "ist", "h\u00f6\u00b7her", "noch", "als", "die\u00b7se", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "ADV", "KOUS", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da reicht kein Laut hin, noch zur\u00fcck zum Grunde.", "tokens": ["Da", "reicht", "kein", "Laut", "hin", ",", "noch", "zu\u00b7r\u00fcck", "zum", "Grun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "APPR", "PTKVZ", "$,", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ja, seit du tot \u2013 mit seinen bl\u00fchnden Borden", "tokens": ["Ja", ",", "seit", "du", "tot", "\u2013", "mit", "sei\u00b7nen", "bl\u00fchn\u00b7den", "Bor\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "ADJD", "$(", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wich ringsumher das Leben mir zur\u00fccke,", "tokens": ["Wich", "rings\u00b7um\u00b7her", "das", "Le\u00b7ben", "mir", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein weites Meer, wo keine Bahn zu finden.", "tokens": ["Ein", "wei\u00b7tes", "Meer", ",", "wo", "kei\u00b7ne", "Bahn", "zu", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch ist dein Bild zum Sterne mir geworden,", "tokens": ["Doch", "ist", "dein", "Bild", "zum", "Ster\u00b7ne", "mir", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "APPRART", "NN", "PPER", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der nach der Heimat weist mit stillem Blicke,", "tokens": ["Der", "nach", "der", "Hei\u00b7mat", "weist", "mit", "stil\u00b7lem", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df fromm der Schiffer streite mit den Winden.", "tokens": ["Da\u00df", "fromm", "der", "Schif\u00b7fer", "strei\u00b7te", "mit", "den", "Win\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}