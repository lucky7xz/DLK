{"textgrid.poem.55564": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Kleine Blumen, kleine Bl\u00e4tter", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kleine Blumen, kleine Bl\u00e4tter", "tokens": ["Klei\u00b7ne", "Blu\u00b7men", ",", "klei\u00b7ne", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Streuen mir mit leichter Hand", "tokens": ["Streu\u00b7en", "mir", "mit", "leich\u00b7ter", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gute junge Fr\u00fchlingsg\u00f6tter", "tokens": ["Gu\u00b7te", "jun\u00b7ge", "Fr\u00fch\u00b7lings\u00b7g\u00f6t\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tandlend auf ein luftig Band.", "tokens": ["Tand\u00b7lend", "auf", "ein", "luf\u00b7tig", "Band", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Zephyr, nimm's auf deine Fl\u00fcgel,", "tokens": ["Ze\u00b7phyr", ",", "nim\u00b7m's", "auf", "dei\u00b7ne", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Schling's um meiner Liebsten Kleid;", "tokens": ["Schling's", "um", "mei\u00b7ner", "Liebs\u00b7ten", "Kleid", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und dann tritt sie f\u00fcr den Spiegel", "tokens": ["Und", "dann", "tritt", "sie", "f\u00fcr", "den", "Spie\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit zufriedener Munterkeit.", "tokens": ["Mit", "zu\u00b7frie\u00b7de\u00b7ner", "Mun\u00b7ter\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Sieht mit Rosen sich umgeben,", "tokens": ["Sieht", "mit", "Ro\u00b7sen", "sich", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie wie eine Rose jung.", "tokens": ["Sie", "wie", "ei\u00b7ne", "Ro\u00b7se", "jung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KOKOM", "ART", "NN", "ADJD", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Einen Ku\u00df, geliebtes Leben!", "tokens": ["Ei\u00b7nen", "Ku\u00df", ",", "ge\u00b7lieb\u00b7tes", "Le\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ich bin belohnt genu(n)g.", "tokens": ["Und", "ich", "bin", "be\u00b7lohnt", "ge\u00b7nu", "(", "n", ")", "g."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "abbreviation"], "pos": ["KON", "PPER", "VAFIN", "VVPP", "NE", "$(", "XY", "$(", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Schicksal, segne diese Trieben,", "tokens": ["Schick\u00b7sal", ",", "seg\u00b7ne", "die\u00b7se", "Trie\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df mich ihr und la\u00df sie mein,", "tokens": ["La\u00df", "mich", "ihr", "und", "la\u00df", "sie", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "KON", "VVIMP", "PPER", "PPOSAT", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df das Leben unsrer Liebe", "tokens": ["La\u00df", "das", "Le\u00b7ben", "uns\u00b7rer", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch kein Rosenleben sein!", "tokens": ["Doch", "kein", "Ro\u00b7sen\u00b7le\u00b7ben", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "M\u00e4dchen, das wie ich empfindet,", "tokens": ["M\u00e4d\u00b7chen", ",", "das", "wie", "ich", "emp\u00b7fin\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "KOKOM", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Reich mir deine liebe Hand!", "tokens": ["Reich", "mir", "dei\u00b7ne", "lie\u00b7be", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und das Band, das uns verbindet,", "tokens": ["Und", "das", "Band", ",", "das", "uns", "ver\u00b7bin\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sei kein schwaches Rosenband!", "tokens": ["Sei", "kein", "schwa\u00b7ches", "Ro\u00b7sen\u00b7band", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}