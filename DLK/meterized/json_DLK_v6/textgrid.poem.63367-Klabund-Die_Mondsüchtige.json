{"textgrid.poem.63367": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Die Monds\u00fcchtige", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wandelnd auf des Daches First,", "tokens": ["Wan\u00b7delnd", "auf", "des", "Da\u00b7ches", "First", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf der Mauer schmalem Rande,", "tokens": ["Auf", "der", "Mau\u00b7er", "schma\u00b7lem", "Ran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schreitet sie, die Hohe, Milde,", "tokens": ["Schrei\u00b7tet", "sie", ",", "die", "Ho\u00b7he", ",", "Mil\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In des Mondes sanftem Licht.", "tokens": ["In", "des", "Mon\u00b7des", "sanf\u00b7tem", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie Musik ert\u00f6nt ihr Schweben,", "tokens": ["Wie", "Mu\u00b7sik", "er\u00b7t\u00f6nt", "ihr", "Schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ihre F\u00fc\u00dfe gleiten gl\u00e4sern.", "tokens": ["Ih\u00b7re", "F\u00fc\u00b7\u00dfe", "glei\u00b7ten", "gl\u00e4\u00b7sern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihre H\u00e4nde klingen leise,", "tokens": ["Ih\u00b7re", "H\u00e4n\u00b7de", "klin\u00b7gen", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihre Augen sind geschlossen.", "tokens": ["Ih\u00b7re", "Au\u00b7gen", "sind", "ge\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Hinter ihr der treue Diener", "tokens": ["Hin\u00b7ter", "ihr", "der", "treu\u00b7e", "Die\u00b7ner"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Achtet ihrer Schritte, da\u00df sie", "tokens": ["Ach\u00b7tet", "ih\u00b7rer", "Schrit\u00b7te", ",", "da\u00df", "sie"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KOUS", "PPER"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "\u00dcber einen Strahl nicht strauchle,", "tokens": ["\u00dc\u00b7ber", "ei\u00b7nen", "Strahl", "nicht", "strauch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sorglich h\u00fctet sie: ihr Schatten.", "tokens": ["Sorg\u00b7lich", "h\u00fc\u00b7tet", "sie", ":", "ihr", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Gottgeheimnis, G\u00f6tzenzauber,", "tokens": ["Gott\u00b7ge\u00b7heim\u00b7nis", ",", "G\u00f6t\u00b7zen\u00b7zau\u00b7ber", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wei\u00dfe Statue der Sehnsucht", "tokens": ["Wei\u00b7\u00dfe", "Sta\u00b7tue", "der", "Sehn\u00b7sucht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Schreitet sie: ich streck' vergeblich", "tokens": ["Schrei\u00b7tet", "sie", ":", "ich", "streck'", "ver\u00b7geb\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$.", "PPER", "VVFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine H\u00e4nde nach ihr aus.", "tokens": ["Mei\u00b7ne", "H\u00e4n\u00b7de", "nach", "ihr", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "O wie halt ich die Entschreitende,", "tokens": ["O", "wie", "halt", "ich", "die", "Ent\u00b7schrei\u00b7ten\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.2": {"text": "O wie bann ich die Entgleitende,", "tokens": ["O", "wie", "bann", "ich", "die", "Ent\u00b7glei\u00b7ten\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADV", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Aber ruf' ich: st\u00fcrzt sie nieder.", "tokens": ["A\u00b7ber", "ruf'", "ich", ":", "st\u00fcrzt", "sie", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$.", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber schrei ich: ists ihr Tod.", "tokens": ["A\u00b7ber", "schrei", "ich", ":", "ists", "ihr", "Tod", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$.", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und so schreitet sie vor\u00fcber,", "tokens": ["Und", "so", "schrei\u00b7tet", "sie", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist auf ewig mir verloren.", "tokens": ["Ist", "auf", "e\u00b7wig", "mir", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJD", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eine Wolke l\u00f6scht den Mond aus.", "tokens": ["Ei\u00b7ne", "Wol\u00b7ke", "l\u00f6scht", "den", "Mond", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einsam stehe ich im Dunkeln.", "tokens": ["Ein\u00b7sam", "ste\u00b7he", "ich", "im", "Dun\u00b7keln", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}