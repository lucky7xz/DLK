{"dta.poem.15651": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "84.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Je n\u00e4her jenem Kreis, wo graden Blicks die Sonne", "tokens": ["Je", "n\u00e4\u00b7her", "je\u00b7nem", "Kreis", ",", "wo", "gra\u00b7den", "Blicks", "die", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PDAT", "NN", "$,", "PWAV", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zur Erde niederschaut, je n\u00e4her Himmelswonne.", "tokens": ["Zur", "Er\u00b7de", "nie\u00b7der\u00b7schaut", ",", "je", "n\u00e4\u00b7her", "Him\u00b7mels\u00b7won\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "ADV", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Selbst minder schwer ist dort der ird'schen Stoffe Wucht,", "tokens": ["Selbst", "min\u00b7der", "schwer", "ist", "dort", "der", "ird'\u00b7schen", "Stof\u00b7fe", "Wucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VAFIN", "ADV", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo raschern Schwunges wirkt der Erde Mittelflucht.", "tokens": ["Wo", "ra\u00b7schern", "Schwun\u00b7ges", "wirkt", "der", "Er\u00b7de", "Mit\u00b7tel\u00b7flucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das Leben selbst ist leicht und gleich nur unterm Gleicher,", "tokens": ["Das", "Le\u00b7ben", "selbst", "ist", "leicht", "und", "gleich", "nur", "un\u00b7term", "Glei\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "ADJD", "KON", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das nach dem Pol hin wird ungleich und m\u00fchsalreicher.", "tokens": ["Das", "nach", "dem", "Pol", "hin", "wird", "un\u00b7gleich", "und", "m\u00fch\u00b7sal\u00b7rei\u00b7cher", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "ADV", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nur unterm Gleicher lag das Paradies vielleicht,", "tokens": ["Nur", "un\u00b7term", "Glei\u00b7cher", "lag", "das", "Pa\u00b7ra\u00b7dies", "viel\u00b7leicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo ganz das Leben ist gewesen gleich und leicht.", "tokens": ["Wo", "ganz", "das", "Le\u00b7ben", "ist", "ge\u00b7we\u00b7sen", "gleich", "und", "leicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VAFIN", "VAPP", "ADV", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ist dort vielleicht noch izt ein h\u00f6chster Berg zu finden,", "tokens": ["Ist", "dort", "viel\u00b7leicht", "noch", "izt", "ein", "h\u00f6chs\u00b7ter", "Berg", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADV", "ADV", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo Erd' und Himmel sich zum Paradies verbinden?", "tokens": ["Wo", "Erd'", "und", "Him\u00b7mel", "sich", "zum", "Pa\u00b7ra\u00b7dies", "ver\u00b7bin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "PRF", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Ein Berg, um den sich leicht im Tanz der Schatten dreht,", "tokens": ["Ein", "Berg", ",", "um", "den", "sich", "leicht", "im", "Tanz", "der", "Schat\u00b7ten", "dreht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUI", "ART", "PRF", "ADJD", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und auf des Mittags H\u00f6h' in lauter Glanz vergeht!", "tokens": ["Und", "auf", "des", "Mit\u00b7tags", "H\u00f6h'", "in", "lau\u00b7ter", "Glanz", "ver\u00b7geht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Wo grad die Sonnen auf, und grad hinunter steigen,", "tokens": ["Wo", "grad", "die", "Son\u00b7nen", "auf", ",", "und", "grad", "hin\u00b7un\u00b7ter", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "PTKVZ", "$,", "KON", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und keiner unterm Pol sich birgt vom Sternenreigen.", "tokens": ["Und", "kei\u00b7ner", "un\u00b7term", "Pol", "sich", "birgt", "vom", "Ster\u00b7nen\u00b7rei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPRART", "NN", "PRF", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Wo mit dem Herbste stets der Fr\u00fchling sich verm\u00e4hlt,", "tokens": ["Wo", "mit", "dem", "Herbs\u00b7te", "stets", "der", "Fr\u00fch\u00b7ling", "sich", "ver\u00b7m\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ADV", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und im Jahrzeitenchor allein der Winter fehlt.", "tokens": ["Und", "im", "Jahr\u00b7zei\u00b7ten\u00b7chor", "al\u00b7lein", "der", "Win\u00b7ter", "fehlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}