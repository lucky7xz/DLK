{"textgrid.poem.43686": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Guten Kindern geht es gut", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es wollt' ein Knabe fr\u00fch' aufstehn,", "tokens": ["Es", "wollt'", "ein", "Kna\u00b7be", "fr\u00fch'", "auf\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wollt' in gr\u00fcnen Wald nach Beeren gehn.", "tokens": ["Und", "wollt'", "in", "gr\u00fc\u00b7nen", "Wald", "nach", "Bee\u00b7ren", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "ADJA", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und als er kam in Wald hinein,", "tokens": ["Und", "als", "er", "kam", "in", "Wald", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da pfl\u00fcckt' er viele rothe Erdbeerlein.", "tokens": ["Da", "pfl\u00fcckt'", "er", "vie\u00b7le", "ro\u00b7the", "Erd\u00b7beer\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und als er wollte heim nach Haus,", "tokens": ["Und", "als", "er", "woll\u00b7te", "heim", "nach", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VMFIN", "PTKVZ", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da fand er nicht zum gr\u00fcnen Wald hinaus.", "tokens": ["Da", "fand", "er", "nicht", "zum", "gr\u00fc\u00b7nen", "Wald", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "\u00bbsoll ich die Nacht im Walde sein \u2013", "tokens": ["\u00bb", "soll", "ich", "die", "Nacht", "im", "Wal\u00b7de", "sein", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "PPER", "ART", "NN", "APPRART", "NN", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer tr\u00f6stet denn daheim mein M\u00fctterlein?\u00ab", "tokens": ["Wer", "tr\u00f6s\u00b7tet", "denn", "da\u00b7heim", "mein", "M\u00fct\u00b7ter\u00b7lein", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ADV", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Er weint und rennet hin und her:", "tokens": ["Er", "weint", "und", "ren\u00b7net", "hin", "und", "her", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbach, wenn ich doch bei meiner Mutter w\u00e4r'!\u00ab", "tokens": ["\u00bb", "ach", ",", "wenn", "ich", "doch", "bei", "mei\u00b7ner", "Mut\u00b7ter", "w\u00e4r'", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ITJ", "$,", "KOUS", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VAFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und endlich ist er m\u00fcd' und matt,", "tokens": ["Und", "end\u00b7lich", "ist", "er", "m\u00fcd'", "und", "matt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er sucht im Busch sich eine Lagerstatt.", "tokens": ["Er", "sucht", "im", "Busch", "sich", "ei\u00b7ne", "La\u00b7ger\u00b7statt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Es singen hell die V\u00f6gelein,", "tokens": ["Es", "sin\u00b7gen", "hell", "die", "V\u00f6\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er macht die Augen zu und schl\u00e4fet ein.", "tokens": ["Er", "macht", "die", "Au\u00b7gen", "zu", "und", "schl\u00e4\u00b7fet", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Da springt ganz leise husch husch husch", "tokens": ["Da", "springt", "ganz", "lei\u00b7se", "husch", "husch", "husch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADJD", "ADJD", "ADJD", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein graues M\u00e4nnlein aus dem dunkeln Busch.", "tokens": ["Ein", "grau\u00b7es", "M\u00e4nn\u00b7lein", "aus", "dem", "dun\u00b7keln", "Busch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Graum\u00e4nnlein weckt ihn alsobald", "tokens": ["Grau\u00b7m\u00e4nn\u00b7lein", "weckt", "ihn", "al\u00b7so\u00b7bald"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und f\u00fchrt ihn aus dem weiten gr\u00fcnen Wald.", "tokens": ["Und", "f\u00fchrt", "ihn", "aus", "dem", "wei\u00b7ten", "gr\u00fc\u00b7nen", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Und als der Knab' ins Dorf will gehn,", "tokens": ["Und", "als", "der", "Knab'", "ins", "Dorf", "will", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPRART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Graum\u00e4nnlein ist nicht weiter mehr zu sehn.", "tokens": ["Grau\u00b7m\u00e4nn\u00b7lein", "ist", "nicht", "wei\u00b7ter", "mehr", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PTKNEG", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Froh tritt der Knab' ins Haus hinein,", "tokens": ["Froh", "tritt", "der", "Knab'", "ins", "Haus", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erz\u00e4hlet Alles seinem M\u00fctterlein.", "tokens": ["Er\u00b7z\u00e4h\u00b7let", "Al\u00b7les", "sei\u00b7nem", "M\u00fct\u00b7ter\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Die Mutter spricht: \u00bbWie froh bin ich!", "tokens": ["Die", "Mut\u00b7ter", "spricht", ":", "\u00bb", "Wie", "froh", "bin", "ich", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "PWAV", "ADJD", "VAFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ach Gott, wie freu' ich mich herzinniglich!", "tokens": ["Ach", "Gott", ",", "wie", "freu'", "ich", "mich", "her\u00b7zin\u00b7nig\u00b7lich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "PWAV", "VVFIN", "PPER", "PRF", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "Den guten Kindern geht es gut,", "tokens": ["Den", "gu\u00b7ten", "Kin\u00b7dern", "geht", "es", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die hat der liebe Gott in seiner Hut.\u00ab", "tokens": ["Die", "hat", "der", "lie\u00b7be", "Gott", "in", "sei\u00b7ner", "Hut", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}