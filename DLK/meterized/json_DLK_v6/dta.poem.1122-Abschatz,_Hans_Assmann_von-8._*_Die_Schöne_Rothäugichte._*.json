{"dta.poem.1122": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "8. * Die Sch\u00f6ne Roth\u00e4ugichte. *", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Dorinde klaget sich beschwert mit vielen Fl\u00fcssen/ ", "tokens": ["Do\u00b7rin\u00b7de", "kla\u00b7get", "sich", "be\u00b7schwert", "mit", "vie\u00b7len", "Fl\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "VVPP", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil ihr vor langer Zeit ein feuchter Regenbogen", "tokens": ["Weil", "ihr", "vor", "lan\u00b7ger", "Zeit", "ein", "feuch\u00b7ter", "Re\u00b7gen\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den Himmelblauen Krei\u00df der Augen hat umzogen:", "tokens": ["Den", "Him\u00b7mel\u00b7blau\u00b7en", "Krei\u00df", "der", "Au\u00b7gen", "hat", "um\u00b7zo\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VAFIN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der heissen Seuffzer Rauch hat sie so sehr gebissen.", "tokens": ["Der", "heis\u00b7sen", "Seuff\u00b7zer", "Rauch", "hat", "sie", "so", "sehr", "ge\u00b7bis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Glutt/ die manchen Pfeil Cupidens schmiden m\u00fcssen/", "tokens": ["Die", "Glutt", "/", "die", "man\u00b7chen", "Pfeil", "Cu\u00b7pi\u00b7dens", "schmi\u00b7den", "m\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "PIAT", "NN", "NE", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ist allzusehr bewegt zur Ess\u2019 hinaus geflogen.", "tokens": ["Ist", "all\u00b7zu\u00b7sehr", "be\u00b7wegt", "zur", "Ess'", "hin\u00b7aus", "ge\u00b7flo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVFIN", "APPRART", "NN", "APZR", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Angst-Schwei\u00df/ den sie hat viel Hertzen abgesogen/", "tokens": ["Der", "Angst\u00b7Schwei\u00df", "/", "den", "sie", "hat", "viel", "Hert\u00b7zen", "ab\u00b7ge\u00b7so\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "PPER", "VAFIN", "PIAT", "NN", "VVPP", "$("], "meter": "-++--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Mu\u00df wieder Tropffenwei\u00df auff ihre Wangen schissen.", "tokens": ["Mu\u00df", "wie\u00b7der", "Tropf\u00b7fen\u00b7wei\u00df", "auff", "ih\u00b7re", "Wan\u00b7gen", "schis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die R\u00f6the giebt den Prei\u00df Rubinen und Corallen/", "tokens": ["Die", "R\u00f6\u00b7the", "giebt", "den", "Prei\u00df", "Ru\u00b7bi\u00b7nen", "und", "Co\u00b7ral\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Zinober hat den Ruhm vor andern Farben allen:", "tokens": ["Zi\u00b7no\u00b7ber", "hat", "den", "Ruhm", "vor", "an\u00b7dern", "Far\u00b7ben", "al\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "APPR", "ADJA", "NN", "PIAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Was schadets/ ob er nicht am rechten Orte steht?", "tokens": ["Was", "scha\u00b7dets", "/", "ob", "er", "nicht", "am", "rech\u00b7ten", "Or\u00b7te", "steht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$(", "KOUS", "PPER", "PTKNEG", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die Sonne sieht so aus/ wenn sie zu Golde geht/", "tokens": ["Die", "Son\u00b7ne", "sieht", "so", "aus", "/", "wenn", "sie", "zu", "Gol\u00b7de", "geht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "$(", "KOUS", "PPER", "APPR", "NE", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und also l\u00e4sset sich die Morgen-R\u00f6th\u2019 im Thauen", "tokens": ["Und", "al\u00b7so", "l\u00e4s\u00b7set", "sich", "die", "Mor\u00b7gen\u00b7R\u00f6\u00b7th'", "im", "Thau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PRF", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Mit Perlen-Thr\u00e4nen und in vollem Purpur schauen.", "tokens": ["Mit", "Per\u00b7len\u00b7Thr\u00e4\u00b7nen", "und", "in", "vol\u00b7lem", "Pur\u00b7pur", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}