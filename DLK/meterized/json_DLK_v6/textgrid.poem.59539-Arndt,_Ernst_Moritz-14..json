{"textgrid.poem.59539": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Traum der fliehenden Minuten,", "tokens": ["Traum", "der", "flie\u00b7hen\u00b7den", "Mi\u00b7nu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie auf Fluten", "tokens": ["Wie", "auf", "Flu\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "APPR", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Mondenschimmer wechselnd bebt,", "tokens": ["Mon\u00b7den\u00b7schim\u00b7mer", "wech\u00b7selnd", "bebt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie auf gr\u00fcnen Sommermatten", "tokens": ["Wie", "auf", "gr\u00fc\u00b7nen", "Som\u00b7mer\u00b7mat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Licht und Schatten", "tokens": ["Licht", "und", "Schat\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Fl\u00fcchtig durch einander schwebt \u2013", "tokens": ["Fl\u00fcch\u00b7tig", "durch", "ein\u00b7an\u00b7der", "schwebt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PRF", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Also st\u00fcrzt des Lebens Welle,", "tokens": ["Al\u00b7so", "st\u00fcrzt", "des", "Le\u00b7bens", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nacht und Helle", "tokens": ["Nacht", "und", "Hel\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Wechselnd sich ins eigne Grab,", "tokens": ["Wech\u00b7selnd", "sich", "ins", "eig\u00b7ne", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und das Liebste, was wir hatten,", "tokens": ["Und", "das", "Liebs\u00b7te", ",", "was", "wir", "hat\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Flieht als Schatten", "tokens": ["Flieht", "als", "Schat\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "KOUS", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Mit zur Schattenwelt hinab.", "tokens": ["Mit", "zur", "Schat\u00b7ten\u00b7welt", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Stolzer Mensch, was ist dein Eigen?", "tokens": ["Stol\u00b7zer", "Mensch", ",", "was", "ist", "dein", "Ei\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie ein Reigen", "tokens": ["Wie", "ein", "Rei\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Lieblich, aber kurz verklingt,", "tokens": ["Lieb\u00b7lich", ",", "a\u00b7ber", "kurz", "ver\u00b7klingt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "So verklingt der Jugend Sch\u00f6ne,", "tokens": ["So", "ver\u00b7klingt", "der", "Ju\u00b7gend", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Deren T\u00f6ne", "tokens": ["De\u00b7ren", "T\u00f6\u00b7ne"], "token_info": ["word", "word"], "pos": ["PDS", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Nur die Wehmutsglocke ringt.", "tokens": ["Nur", "die", "Weh\u00b7muts\u00b7glo\u00b7cke", "ringt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Was ist Liebe? S\u00fc\u00dfes Sehnen,", "tokens": ["Was", "ist", "Lie\u00b7be", "?", "S\u00fc\u00b7\u00dfes", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "NN", "$.", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Banges W\u00e4hnen,", "tokens": ["Ban\u00b7ges", "W\u00e4h\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Recht des eitlen Traumes Traum.", "tokens": ["Recht", "des", "eit\u00b7len", "Trau\u00b7mes", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die unsterblichen Gewalten", "tokens": ["Die", "uns\u00b7terb\u00b7li\u00b7chen", "Ge\u00b7wal\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Willst du halten,", "tokens": ["Willst", "du", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Und du h\u00e4ltst dich selber kaum.", "tokens": ["Und", "du", "h\u00e4ltst", "dich", "sel\u00b7ber", "kaum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Was ist Schwur und feste Treue?", "tokens": ["Was", "ist", "Schwur", "und", "fes\u00b7te", "Treu\u00b7e", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "VVFIN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wolkenbl\u00e4ue", "tokens": ["Wol\u00b7ken\u00b7bl\u00e4u\u00b7e"], "token_info": ["word"], "pos": ["NE"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Wechselt nicht wie Menschenwort;", "tokens": ["Wech\u00b7selt", "nicht", "wie", "Men\u00b7schen\u00b7wort", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "KOKOM", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und du nimmst, was auf dem Sande", "tokens": ["Und", "du", "nimmst", ",", "was", "auf", "dem", "San\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Steht, zum Pfande?", "tokens": ["Steht", ",", "zum", "Pfan\u00b7de", "?"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPRART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Doch wie Sand so flie\u00dft es fort.", "tokens": ["Doch", "wie", "Sand", "so", "flie\u00dft", "es", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NN", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Das Unendliche ergr\u00fcnden", "tokens": ["Das", "Un\u00b7end\u00b7li\u00b7che", "er\u00b7gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Willst du, finden,", "tokens": ["Willst", "du", ",", "fin\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Was die Weltenr\u00e4der treibt?", "tokens": ["Was", "die", "Wel\u00b7ten\u00b7r\u00e4\u00b7der", "treibt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Weise hab' ich viel vernommen,", "tokens": ["Wei\u00b7se", "hab'", "ich", "viel", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch beklommen", "tokens": ["Doch", "be\u00b7klom\u00b7men"], "token_info": ["word", "word"], "pos": ["KON", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Lernt' ich, da\u00df es R\u00e4tsel bleibt.", "tokens": ["Lernt'", "ich", ",", "da\u00df", "es", "R\u00e4t\u00b7sel", "bleibt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Deine Kunst, dein eitles Wissen", "tokens": ["Dei\u00b7ne", "Kunst", ",", "dein", "eit\u00b7les", "Wis\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Teufelskissen", "tokens": ["Teu\u00b7fels\u00b7kis\u00b7sen"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Ist es leerer Eitelkeit;", "tokens": ["Ist", "es", "lee\u00b7rer", "Ei\u00b7tel\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dennoch weckst du Dunst aus D\u00fcnsten,", "tokens": ["Den\u00b7noch", "weckst", "du", "Dunst", "aus", "D\u00fcns\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit Gespinsten", "tokens": ["Mit", "Ge\u00b7spins\u00b7ten"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Webst du golden dir dein Leid.", "tokens": ["Webst", "du", "gol\u00b7den", "dir", "dein", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJA", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Auf! aus Nacht der Eitelkeiten", "tokens": ["Auf", "!", "aus", "Nacht", "der", "Ei\u00b7tel\u00b7kei\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "$.", "APPR", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In die weiten", "tokens": ["In", "die", "wei\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Welten, leuchtend \u00fcber dir!", "tokens": ["Wel\u00b7ten", ",", "leuch\u00b7tend", "\u00fc\u00b7ber", "dir", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVPP", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus des Lebens reinen Quellen", "tokens": ["Aus", "des", "Le\u00b7bens", "rei\u00b7nen", "Quel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Trinke hellen", "tokens": ["Trin\u00b7ke", "hel\u00b7len"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Himmelsgeist und Wonne dir!", "tokens": ["Him\u00b7mels\u00b7geist", "und", "Won\u00b7ne", "dir", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Trinke heitern Geist der Wahrheit!", "tokens": ["Trin\u00b7ke", "hei\u00b7tern", "Geist", "der", "Wahr\u00b7heit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und in Klarheit", "tokens": ["Und", "in", "Klar\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Wird die T\u00e4uschung vor dir stehn;", "tokens": ["Wird", "die", "T\u00e4u\u00b7schung", "vor", "dir", "stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Weinen wirst du bittre Tr\u00e4nen,", "tokens": ["Wei\u00b7nen", "wirst", "du", "bitt\u00b7re", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch dein Sehnen", "tokens": ["Doch", "dein", "Seh\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Wird durch alle Himmel gehn.", "tokens": ["Wird", "durch", "al\u00b7le", "Him\u00b7mel", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Und von Gottes goldnen Kerzen", "tokens": ["Und", "von", "Got\u00b7tes", "gold\u00b7nen", "Ker\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Z\u00fcnd' im Herzen", "tokens": ["Z\u00fcnd'", "im", "Her\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["NE", "APPRART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Sich die Flamme keusch und rein,", "tokens": ["Sich", "die", "Flam\u00b7me", "keusch", "und", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die unsterblich Leben fodert,", "tokens": ["Die", "uns\u00b7terb\u00b7lich", "Le\u00b7ben", "fo\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aufw\u00e4rts lodert", "tokens": ["Auf\u00b7w\u00e4rts", "lo\u00b7dert"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Durch der Erde Nebelschein.", "tokens": ["Durch", "der", "Er\u00b7de", "Ne\u00b7bel\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Auf! mit stolzem Angesichte", "tokens": ["Auf", "!", "mit", "stol\u00b7zem", "An\u00b7ge\u00b7sich\u00b7te"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["APPR", "$.", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu dem Lichte!", "tokens": ["Zu", "dem", "Lich\u00b7te", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "--+-", "measure": "anapaest.init"}, "line.3": {"text": "Zu dem Lichte alles Lichts,", "tokens": ["Zu", "dem", "Lich\u00b7te", "al\u00b7les", "Lichts", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo die tausend Sonnen brennen!", "tokens": ["Wo", "die", "tau\u00b7send", "Son\u00b7nen", "bren\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "CARD", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Lern' erkennen:", "tokens": ["Lern'", "er\u00b7ken\u00b7nen", ":"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Gott ist alles, du bist nichts.", "tokens": ["Gott", "ist", "al\u00b7les", ",", "du", "bist", "nichts", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIS", "$,", "PPER", "VAFIN", "PIS", "$."], "meter": "+-+-+-+", "measure": 