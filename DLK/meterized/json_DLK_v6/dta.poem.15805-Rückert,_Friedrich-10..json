{"dta.poem.15805": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Zeiten sind vorbei, wo ein gefl\u00fcgelt Wort", "tokens": ["Die", "Zei\u00b7ten", "sind", "vor\u00b7bei", ",", "wo", "ein", "ge\u00b7fl\u00fc\u00b7gelt", "Wort"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PTKVZ", "$,", "PWAV", "ART", "VVPP", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus S\u00e4ngers Munde gieng von Mund zu Munde fort.", "tokens": ["Aus", "S\u00e4n\u00b7gers", "Mun\u00b7de", "gieng", "von", "Mund", "zu", "Mun\u00b7de", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVFIN", "APPR", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Jetzt, um zu fliegen, mu\u00df es sich papierne Schwingen", "tokens": ["Jetzt", ",", "um", "zu", "flie\u00b7gen", ",", "mu\u00df", "es", "sich", "pa\u00b7pier\u00b7ne", "Schwin\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUI", "PTKZU", "VVINF", "$,", "VMFIN", "PPER", "PRF", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Anheften, die es schwer von Ort zu Orte bringen.", "tokens": ["An\u00b7hef\u00b7ten", ",", "die", "es", "schwer", "von", "Ort", "zu", "Or\u00b7te", "brin\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "PPER", "ADJD", "APPR", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Verwundert und besch\u00e4mt seh' ich die B\u00fccherballen,", "tokens": ["Ver\u00b7wun\u00b7dert", "und", "be\u00b7sch\u00e4mt", "seh'", "ich", "die", "B\u00fc\u00b7cher\u00b7bal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf denen, was ich schrieb, in alle Welt soll wallen.", "tokens": ["Auf", "de\u00b7nen", ",", "was", "ich", "schrieb", ",", "in", "al\u00b7le", "Welt", "soll", "wal\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "$,", "PWS", "PPER", "VVFIN", "$,", "APPR", "PIAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wie leiblich massenhaft geworden ist der Geist;", "tokens": ["Wie", "leib\u00b7lich", "mas\u00b7sen\u00b7haft", "ge\u00b7wor\u00b7den", "ist", "der", "Geist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADJD", "VAPP", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So breit sich in der Welt zu machen, o wie dreist!", "tokens": ["So", "breit", "sich", "in", "der", "Welt", "zu", "ma\u00b7chen", ",", "o", "wie", "dreist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PRF", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,", "FM", "KOKOM", "CARD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Doch, wenn ich denke, da\u00df hier stehn versammelt k\u00f6nnten", "tokens": ["Doch", ",", "wenn", "ich", "den\u00b7ke", ",", "da\u00df", "hier", "stehn", "ver\u00b7sam\u00b7melt", "k\u00f6nn\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "PPER", "VVFIN", "$,", "KOUS", "ADV", "VVINF", "VVFIN", "VMFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wol tausend, die ein aufmerksames Ohr mir g\u00f6nnten \u2014", "tokens": ["Wol", "tau\u00b7send", ",", "die", "ein", "auf\u00b7merk\u00b7sa\u00b7mes", "Ohr", "mir", "g\u00f6nn\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PRELS", "ART", "ADJA", "NN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Nun sind die tausend nicht vereint auf einem Platz,", "tokens": ["Nun", "sind", "die", "tau\u00b7send", "nicht", "ver\u00b7eint", "auf", "ei\u00b7nem", "Platz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "CARD", "PTKNEG", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch vorenthalten sei drum ihnen nicht der Schatz.", "tokens": ["Doch", "vor\u00b7ent\u00b7hal\u00b7ten", "sei", "drum", "ih\u00b7nen", "nicht", "der", "Schatz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "PAV", "PPER", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "So send' ich tausendfach gedruckte B\u00e4ndchen aus,", "tokens": ["So", "send'", "ich", "tau\u00b7send\u00b7fach", "ge\u00b7druck\u00b7te", "B\u00e4nd\u00b7chen", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df sein besonder Theil jedwedem komm' ins Haus.", "tokens": ["Da\u00df", "sein", "be\u00b7son\u00b7der", "Theil", "jed\u00b7we\u00b7dem", "komm'", "ins", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "PIS", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Ein Uebelstand ist nur bei so vertheilten Gr\u00fc\u00dfen:", "tokens": ["Ein", "Ue\u00b7bel\u00b7stand", "ist", "nur", "bei", "so", "ver\u00b7theil\u00b7ten", "Gr\u00fc\u00b7\u00dfen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df die Empf\u00e4nger sie mit Geld bezahlen m\u00fc\u00dfen.", "tokens": ["Da\u00df", "die", "Emp\u00b7f\u00e4n\u00b7ger", "sie", "mit", "Geld", "be\u00b7zah\u00b7len", "m\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "APPR", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Strafe der Wi\u00dfbegier! entbehren will sie nicht", "tokens": ["Stra\u00b7fe", "der", "Wi\u00df\u00b7be\u00b7gier", "!", "ent\u00b7beh\u00b7ren", "will", "sie", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$.", "VVINF", "VMFIN", "PPER", "PTKNEG"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Ein Wort, das einsam mit sich selbst ein Dichter spricht.", "tokens": ["Ein", "Wort", ",", "das", "ein\u00b7sam", "mit", "sich", "selbst", "ein", "Dich\u00b7ter", "spricht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "APPR", "PRF", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}