{"textgrid.poem.43395": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sprach der K\u00f6nig: einen Orden", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sprach der K\u00f6nig: einen Orden", "tokens": ["Sprach", "der", "K\u00f6\u00b7nig", ":", "ei\u00b7nen", "Or\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "$.", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stift' ich f\u00fcr den Dichterchor:", "tokens": ["Stift'", "ich", "f\u00fcr", "den", "Dich\u00b7ter\u00b7chor", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer ein Dichter ist geworden,", "tokens": ["Wer", "ein", "Dich\u00b7ter", "ist", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VAFIN", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sing' ein Ordenslied mir vor!", "tokens": ["Sing'", "ein", "Or\u00b7dens\u00b7lied", "mir", "vor", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und es sang mit Jubelschalle", "tokens": ["Und", "es", "sang", "mit", "Ju\u00b7bel\u00b7schal\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mancher s\u00fc\u00df und miniglich,", "tokens": ["Man\u00b7cher", "s\u00fc\u00df", "und", "mi\u00b7nig\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und sie w\u00fcnschten alle, alle", "tokens": ["Und", "sie", "w\u00fcnschten", "al\u00b7le", ",", "al\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "PPER", "VVFIN", "PIS", "$,", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Diesen neuen Orden sich.", "tokens": ["Die\u00b7sen", "neu\u00b7en", "Or\u00b7den", "sich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Mancher andre triumphieret,", "tokens": ["Man\u00b7cher", "and\u00b7re", "tri\u00b7um\u00b7phie\u00b7ret", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der das Ordenslied vernahm;", "tokens": ["Der", "das", "Or\u00b7dens\u00b7lied", "ver\u00b7nahm", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Dichter lamentieret,", "tokens": ["Und", "der", "Dich\u00b7ter", "la\u00b7men\u00b7tie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der den Orden nicht bekam.", "tokens": ["Der", "den", "Or\u00b7den", "nicht", "be\u00b7kam", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}