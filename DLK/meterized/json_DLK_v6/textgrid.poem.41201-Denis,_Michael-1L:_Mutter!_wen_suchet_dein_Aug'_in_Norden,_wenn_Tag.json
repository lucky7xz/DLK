{"textgrid.poem.41201": {"metadata": {"author": {"name": "Denis, Michael", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mutter! wen suchet dein Aug' in Norden, wenn Tag", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mutter! wen suchet dein Aug' in Norden, wenn Tag", "tokens": ["Mut\u00b7ter", "!", "wen", "su\u00b7chet", "dein", "Aug'", "in", "Nor\u00b7den", ",", "wenn", "Tag"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$.", "PWS", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$,", "KOUS", "NN"], "meter": "+-+---+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Jetzo die Fernen erhellt?", "tokens": ["Jet\u00b7zo", "die", "Fer\u00b7nen", "er\u00b7hellt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Mutter! wem lauschet dein Ohr in Norden, wenn Nacht", "tokens": ["Mut\u00b7ter", "!", "wem", "lau\u00b7schet", "dein", "Ohr", "in", "Nor\u00b7den", ",", "wenn", "Nacht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$.", "PWS", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$,", "KOUS", "NN"], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Jetzo den Erdenkreis deckt?", "tokens": ["Jet\u00b7zo", "den", "Er\u00b7den\u00b7kreis", "deckt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Wolken des Staubes die wehn aus Norden. Doch sie", "tokens": ["Wol\u00b7ken", "des", "Stau\u00b7bes", "die", "wehn", "aus", "Nor\u00b7den", ".", "Doch", "sie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "ART", "VVFIN", "APPR", "NN", "$.", "KON", "PPER"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.6": {"text": "W\u00f6lkte sein Wagen nicht auf.", "tokens": ["W\u00f6lk\u00b7te", "sein", "Wa\u00b7gen", "nicht", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "N\u00e4chtliches Rollen erschallt aus Norden. Doch sind", "tokens": ["N\u00e4cht\u00b7li\u00b7ches", "Rol\u00b7len", "er\u00b7schallt", "aus", "Nor\u00b7den", ".", "Doch", "sind"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "NN", "$.", "KON", "VAFIN"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.8": {"text": "R\u00e4der von Joseph es nicht.", "tokens": ["R\u00e4\u00b7der", "von", "Jo\u00b7se\u00b7ph", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "PPER", "PTKNEG", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Aber dir pochet dein Herz, dein m\u00fctterlich Herz", "tokens": ["A\u00b7ber", "dir", "po\u00b7chet", "dein", "Herz", ",", "dein", "m\u00fct\u00b7ter\u00b7lich", "Herz"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "$,", "PPOSAT", "ADJD", "NN"], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.10": {"text": "Seinem Geliebtesten nach;", "tokens": ["Sei\u00b7nem", "Ge\u00b7lieb\u00b7tes\u00b7ten", "nach", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.11": {"text": "Sieht ihn in Wachen des Tag's, in Tr\u00e4umen der Nacht;", "tokens": ["Sieht", "ihn", "in", "Wa\u00b7chen", "des", "Tag's", ",", "in", "Tr\u00e4u\u00b7men", "der", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ART", "NN", "$,", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Ahnet Gefahren und pocht.", "tokens": ["Ah\u00b7net", "Ge\u00b7fah\u00b7ren", "und", "pocht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.13": {"text": "Denn er ist ferne, dein Sohn. Manch rei\u00dfender Strom", "tokens": ["Denn", "er", "ist", "fer\u00b7ne", ",", "dein", "Sohn", ".", "Manch", "rei\u00b7\u00dfen\u00b7der", "Strom"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ADV", "$,", "PPOSAT", "NN", "$.", "PIAT", "ADJA", "NN"], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.14": {"text": "Scheidet ihn, manches Gebirg'.", "tokens": ["Schei\u00b7det", "ihn", ",", "man\u00b7ches", "Ge\u00b7bir\u00b7g'", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PIAT", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.15": {"text": "Manchen unwirthlichen Hain, manch' \u00f6des Gefild',", "tokens": ["Man\u00b7chen", "un\u00b7wirth\u00b7li\u00b7chen", "Hain", ",", "man\u00b7ch'", "\u00f6\u00b7des", "Ge\u00b7fild'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.16": {"text": "Mutter! bewandelt sein Fu\u00df.", "tokens": ["Mut\u00b7ter", "!", "be\u00b7wan\u00b7delt", "sein", "Fu\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Allein gewaltig ist das gefl\u00fcgelte", "tokens": ["Al\u00b7lein", "ge\u00b7wal\u00b7tig", "ist", "das", "ge\u00b7fl\u00fc\u00b7gel\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "ADJA"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Geleit' von oben, das ihm Allvater gab.", "tokens": ["Ge\u00b7leit'", "von", "o\u00b7ben", ",", "das", "ihm", "All\u00b7va\u00b7ter", "gab", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADV", "$,", "PRELS", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und gab er ihm's denn erst nach Norden?", "tokens": ["Und", "gab", "er", "ihm's", "denn", "erst", "nach", "Nor\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "ADV", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Las er nicht lange schon, als der Wand'rer,", "tokens": ["Las", "er", "nicht", "lan\u00b7ge", "schon", ",", "als", "der", "Wan\u00b7d'\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PTKNEG", "ADV", "ADV", "$,", "KOUS", "ART", "NN", "$,"], "meter": "+--+--++-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Der grosse Wand'rer jetzo das erstemal", "tokens": ["Der", "gros\u00b7se", "Wan\u00b7d'\u00b7rer", "jet\u00b7zo", "das", "ers\u00b7te\u00b7mal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "ART", "NN"], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Wien's Freuden aufgab, hoher Entschl\u00fcsse voll", "tokens": ["Wien's", "Freu\u00b7den", "auf\u00b7gab", ",", "ho\u00b7her", "Ent\u00b7schl\u00fcs\u00b7se", "voll"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "$,", "ADJA", "NN", "ADJD"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Zu seh'n, zu dulden und zu lernen,", "tokens": ["Zu", "seh'n", ",", "zu", "dul\u00b7den", "und", "zu", "ler\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Itzo die Reise nach S\u00fcden antrat,", "tokens": ["It\u00b7zo", "die", "Rei\u00b7se", "nach", "S\u00fc\u00b7den", "an\u00b7trat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.4": {"line.1": {"text": "Von seinem Botenheere die m\u00e4chtigsten,", "tokens": ["Von", "sei\u00b7nem", "Bo\u00b7ten\u00b7hee\u00b7re", "die", "m\u00e4ch\u00b7tigs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "$,"], "meter": "-+-+-+--++-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dem Throne n\u00e4chsten, trautesten Schwingen aus?", "tokens": ["Dem", "Thro\u00b7ne", "n\u00e4chs\u00b7ten", ",", "trau\u00b7tes\u00b7ten", "Schwin\u00b7gen", "aus", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "\u00bbin Joseph hab' ich mir gefallen.", "tokens": ["\u00bb", "in", "Jo\u00b7se\u00b7ph", "hab'", "ich", "mir", "ge\u00b7fal\u00b7len", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NE", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Decket, umschattet auf jedem Pfade", "tokens": ["De\u00b7cket", ",", "um\u00b7schat\u00b7tet", "auf", "je\u00b7dem", "Pfa\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "APPR", "PIAT", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Theresien's Erstgebohrnen, mein Lieblingsbild!\u00ab", "tokens": ["The\u00b7re\u00b7si\u00b7en's", "Erst\u00b7ge\u00b7bohr\u00b7nen", ",", "mein", "Lieb\u00b7lings\u00b7bild", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NE", "NN", "$,", "PPOSAT", "NN", "$.", "$("], "meter": "+---+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Sie, froh des Auftrags, decken, umschatten ihn", "tokens": ["Sie", ",", "froh", "des", "Auf\u00b7trags", ",", "de\u00b7cken", ",", "um\u00b7schat\u00b7ten", "ihn"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "$,", "ADJD", "ART", "NN", "$,", "VVINF", "$,", "VVFIN", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "In S\u00fcden, Osten, Westen, Norden.", "tokens": ["In", "S\u00fc\u00b7den", ",", "Os\u00b7ten", ",", "Wes\u00b7ten", ",", "Nor\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Jegliches Unheil verweh't ihr Fittig.", "tokens": ["Jeg\u00b7li\u00b7ches", "Un\u00b7heil", "ver\u00b7weh't", "ihr", "Fit\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Dann athme Trost, o Mutter! Erreichet hat,", "tokens": ["Dann", "ath\u00b7me", "Trost", ",", "o", "Mut\u00b7ter", "!", "Er\u00b7rei\u00b7chet", "hat", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "FM", "NN", "$.", "VVFIN", "VAFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Erreichet er die gro\u00dfe Gebieterinn,", "tokens": ["Er\u00b7rei\u00b7chet", "er", "die", "gro\u00b7\u00dfe", "Ge\u00b7bie\u00b7te\u00b7rinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "(noch gr\u00f6\u00dfer, tr\u00fcgest du nicht, Mutter!", "tokens": ["(", "noch", "gr\u00f6\u00b7\u00dfer", ",", "tr\u00fc\u00b7gest", "du", "nicht", ",", "Mut\u00b7ter", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "$,", "VVFIN", "PPER", "PTKNEG", "$,", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Katharina zugleich die Krone,)", "tokens": ["Mit", "Ka\u00b7tha\u00b7ri\u00b7na", "zu\u00b7gleich", "die", "Kro\u00b7ne", ",", ")"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NE", "ADV", "ART", "NN", "$,", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Erreichet sie, die weise, bewunderte,", "tokens": ["Er\u00b7rei\u00b7chet", "sie", ",", "die", "wei\u00b7se", ",", "be\u00b7wun\u00b7der\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PRELS", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Geliebte V\u00f6lkermutter, und Bilderin", "tokens": ["Ge\u00b7lieb\u00b7te", "V\u00f6l\u00b7ker\u00b7mut\u00b7ter", ",", "und", "Bil\u00b7de\u00b7rin"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "NN", "$,", "KON", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Von zahl- und namenlosen St\u00e4mmen,", "tokens": ["Von", "zahl", "und", "na\u00b7men\u00b7lo\u00b7sen", "St\u00e4m\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie, die gepriesene K\u00fcnstefreundin,", "tokens": ["Sie", ",", "die", "ge\u00b7prie\u00b7se\u00b7ne", "K\u00fcns\u00b7te\u00b7freun\u00b7din", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Im d\u00fcst'ren Norden Schimmerverbreiterinn,", "tokens": ["Im", "d\u00fcst'\u00b7ren", "Nor\u00b7den", "Schim\u00b7mer\u00b7ver\u00b7brei\u00b7te\u00b7rinn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Mondentr\u00e4ger Schrecken auf Land und See,", "tokens": ["Der", "Mon\u00b7den\u00b7tr\u00e4\u00b7ger", "Schre\u00b7cken", "auf", "Land", "und", "See", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und \u2013 Barde! dieses t\u00f6n' in vollem", "tokens": ["Und", "\u2013", "Bar\u00b7de", "!", "die\u00b7ses", "t\u00f6n'", "in", "vol\u00b7lem"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$(", "NN", "$.", "PDS", "VVFIN", "APPR", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Stolze der Harfe \u2013 von deutscher Abkunft!", "tokens": ["Stol\u00b7ze", "der", "Har\u00b7fe", "\u2013", "von", "deut\u00b7scher", "Ab\u00b7kunft", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "ART", "NN", "$(", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "Nach ihr begehrte lange schon Joseph's Geist,", "tokens": ["Nach", "ihr", "be\u00b7gehr\u00b7te", "lan\u00b7ge", "schon", "Jo\u00b7se\u00b7ph's", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "ADV", "ADV", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Sch\u00e4tzer gro\u00dfer Geister. Schon lange war", "tokens": ["Der", "Sch\u00e4t\u00b7zer", "gro\u00b7\u00dfer", "Geis\u00b7ter", ".", "Schon", "lan\u00b7ge", "war"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$.", "ADV", "ADV", "VAFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "In Joseph's Geiste sich zu spiegeln", "tokens": ["In", "Jo\u00b7se\u00b7ph's", "Geis\u00b7te", "sich", "zu", "spie\u00b7geln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "PRF", "PTKZU", "VVINF"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wunsch der Gebieterinn \u2013 itzt erf\u00fcllet!", "tokens": ["Wunsch", "der", "Ge\u00b7bie\u00b7te\u00b7rinn", "\u2013", "itzt", "er\u00b7f\u00fcl\u00b7let", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$(", "ADV", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "s