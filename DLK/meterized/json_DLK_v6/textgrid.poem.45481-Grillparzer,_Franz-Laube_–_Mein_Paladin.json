{"textgrid.poem.45481": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Laube \u2013 Mein Paladin", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schon tot \u2013 wieder lebendig geworden,", "tokens": ["Schon", "tot", "\u2013", "wie\u00b7der", "le\u00b7ben\u00b7dig", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$(", "ADV", "ADJD", "VAPP", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Durch dich, mein tollk\u00fchner Sohn.", "tokens": ["Durch", "dich", ",", "mein", "toll\u00b7k\u00fch\u00b7ner", "Sohn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nimm, also den \u2013 Grillparzer-Orden,", "tokens": ["Nimm", ",", "al\u00b7so", "den", "\u2013", "Grill\u00b7pa\u00b7rzer\u00b7Or\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "ADV", "ART", "$(", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Sonst hast du gar nichts davon.", "tokens": ["Sonst", "hast", "du", "gar", "nichts", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PIS", "PAV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}