{"textgrid.poem.33929": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "In das Album meiner Frau", "genre": "verse", "period": "N.A.", "pub_year": 1853, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In deiner Seele unbeflecktem Adel,", "tokens": ["In", "dei\u00b7ner", "See\u00b7le", "un\u00b7be\u00b7fleck\u00b7tem", "A\u00b7del", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In ihrer Unschuld, wurzeln deine Schw\u00e4chen,", "tokens": ["In", "ih\u00b7rer", "Un\u00b7schuld", ",", "wur\u00b7zeln", "dei\u00b7ne", "Schw\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und was die Meisten vor gemeinem Tadel", "tokens": ["Und", "was", "die", "Meis\u00b7ten", "vor", "ge\u00b7mei\u00b7nem", "Ta\u00b7del"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bewahrt, das ist ihr innerstes Gebrechen.", "tokens": ["Be\u00b7wahrt", ",", "das", "ist", "ihr", "in\u00b7ners\u00b7tes", "Ge\u00b7bre\u00b7chen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Es k\u00f6nnte Einer dir das Leben rauben,", "tokens": ["Es", "k\u00f6nn\u00b7te", "Ei\u00b7ner", "dir", "das", "Le\u00b7ben", "rau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und w\u00e4re dir schon halb dein Blut entquollen,", "tokens": ["Und", "w\u00e4\u00b7re", "dir", "schon", "halb", "dein", "Blut", "ent\u00b7quol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADJD", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So w\u00fcrdest du ihm noch im Sterben glauben,", "tokens": ["So", "w\u00fcr\u00b7dest", "du", "ihm", "noch", "im", "Ster\u00b7ben", "glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er h\u00e4tt' dir blo\u00df die Ader \u00f6ffnen wollen.", "tokens": ["Er", "h\u00e4tt'", "dir", "blo\u00df", "die", "A\u00b7der", "\u00f6ff\u00b7nen", "wol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ART", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Will die Natur die Sch\u00f6nheit rein entfalten,", "tokens": ["Will", "die", "Na\u00b7tur", "die", "Sch\u00f6n\u00b7heit", "rein", "ent\u00b7fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So darf sie Nichts von ihrem Feind ihr sagen,", "tokens": ["So", "darf", "sie", "Nichts", "von", "ih\u00b7rem", "Feind", "ihr", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIS", "APPR", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie kann nur dann das Herrlichste gestalten,", "tokens": ["Sie", "kann", "nur", "dann", "das", "Herr\u00b7lichs\u00b7te", "ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch mu\u00df sie seinen Untergang auch wagen.", "tokens": ["Doch", "mu\u00df", "sie", "sei\u00b7nen", "Un\u00b7ter\u00b7gang", "auch", "wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Oft w\u00fcnscht' ich dir zu deinem vollen Frieden,", "tokens": ["Oft", "w\u00fcnscht'", "ich", "dir", "zu", "dei\u00b7nem", "vol\u00b7len", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du m\u00f6gtest in der Brust des Feindes lesen,", "tokens": ["Du", "m\u00f6g\u00b7test", "in", "der", "Brust", "des", "Fein\u00b7des", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch wei\u00df ich wohl, es wird dir nicht beschieden,", "tokens": ["Doch", "wei\u00df", "ich", "wohl", ",", "es", "wird", "dir", "nicht", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "PPER", "VAFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Denn dieser Mangel tr\u00e4gt dein ganzes Wesen!", "tokens": ["Denn", "die\u00b7ser", "Man\u00b7gel", "tr\u00e4gt", "dein", "gan\u00b7zes", "We\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}