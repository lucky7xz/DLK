{"textgrid.poem.25323": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im D\u00e4mmer glommen die gemalten W\u00e4nde.", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im D\u00e4mmer glommen die gemalten W\u00e4nde.", "tokens": ["Im", "D\u00e4m\u00b7mer", "glom\u00b7men", "die", "ge\u00b7mal\u00b7ten", "W\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich sah dich an vom gro\u00dfen Schweigen trunken:", "tokens": ["Ich", "sah", "dich", "an", "vom", "gro\u00b7\u00dfen", "Schwei\u00b7gen", "trun\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und bebend f\u00fchlt ich deine weichen H\u00e4nde", "tokens": ["Und", "be\u00b7bend", "f\u00fchlt", "ich", "dei\u00b7ne", "wei\u00b7chen", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und stammelnd sind wir uns ans Herz gesunken.", "tokens": ["und", "stam\u00b7melnd", "sind", "wir", "uns", "ans", "Herz", "ge\u00b7sun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "PPER", "PRF", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie Kinder die in wei\u00dfen Fr\u00fchlingskleidern", "tokens": ["Wie", "Kin\u00b7der", "die", "in", "wei\u00b7\u00dfen", "Fr\u00fch\u00b7lings\u00b7klei\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "ART", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "hinlaufen durch die knospenhellen Hecken", "tokens": ["hin\u00b7lau\u00b7fen", "durch", "die", "knos\u00b7pen\u00b7hel\u00b7len", "He\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und zwischen B\u00fcscheln lichtumsch\u00e4umter Weiden", "tokens": ["und", "zwi\u00b7schen", "B\u00fc\u00b7scheln", "lich\u00b7tum\u00b7sch\u00e4um\u00b7ter", "Wei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "ADJA", "NN"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "und braunen Halmen spielend sich verstecken", "tokens": ["und", "brau\u00b7nen", "Hal\u00b7men", "spie\u00b7lend", "sich", "ver\u00b7ste\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "ADJD", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "in Baches Silber wundernd sich beschauen", "tokens": ["in", "Ba\u00b7ches", "Sil\u00b7ber", "wun\u00b7dernd", "sich", "be\u00b7schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJD", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und jubelnd folgen bunter Falter Gl\u00e4nzen", "tokens": ["und", "ju\u00b7belnd", "fol\u00b7gen", "bun\u00b7ter", "Fal\u00b7ter", "Gl\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVINF", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und Knospen brechen von besternten Auen", "tokens": ["und", "Knos\u00b7pen", "bre\u00b7chen", "von", "bes\u00b7tern\u00b7ten", "Au\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "und singend sich mit Bl\u00fctenkronen kr\u00e4nzen", "tokens": ["und", "sin\u00b7gend", "sich", "mit", "Bl\u00fc\u00b7ten\u00b7kro\u00b7nen", "kr\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "PRF", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "bis gl\u00fchend sie in seligem Ermatten", "tokens": ["bis", "gl\u00fc\u00b7hend", "sie", "in", "se\u00b7li\u00b7gem", "Er\u00b7mat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "zur Quelle steigen leichten Spiels vergessen", "tokens": ["zur", "Quel\u00b7le", "stei\u00b7gen", "leich\u00b7ten", "Spiels", "ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und zitternd unter schwanker Birken Schatten", "tokens": ["und", "zit\u00b7ternd", "un\u00b7ter", "schwan\u00b7ker", "Bir\u00b7ken", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "die zarten Lippen ineinander pressen.", "tokens": ["die", "zar\u00b7ten", "Lip\u00b7pen", "in\u00b7ein\u00b7an\u00b7der", "pres\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}