{"dta.poem.9146": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Xi.  \n An eben dieselbe/ als sie ihren nahmens-tag begieng.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Wje sch\u00f6n hat doch der sonnen licht", "tokens": ["Wie", "sch\u00f6n", "hat", "doch", "der", "son\u00b7nen", "licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "VAFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu scheinen angefangen/", "tokens": ["Zu", "schei\u00b7nen", "an\u00b7ge\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie l\u00e4sst sie ihr gesicht", "tokens": ["Wie", "l\u00e4sst", "sie", "ihr", "ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In lauter neuen stralen prangen/", "tokens": ["In", "lau\u00b7ter", "neu\u00b7en", "stra\u00b7len", "pran\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ach dieses gilt gewi\u00df", "tokens": ["Ach", "die\u00b7ses", "gilt", "ge\u00b7wi\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["ITJ", "PDS", "VVFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der allersch\u00f6nsten Margaris.", "tokens": ["Der", "al\u00b7ler\u00b7sch\u00f6ns\u00b7ten", "Mar\u00b7ga\u00b7ris", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "2. Denn ihr beliebter namens-schein", "tokens": ["Denn", "ihr", "be\u00b7lieb\u00b7ter", "na\u00b7mens\u00b7schein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bricht nun mit gutem gl\u00fccke", "tokens": ["Bricht", "nun", "mit", "gu\u00b7tem", "gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bey unsern linden ein/", "tokens": ["Bey", "un\u00b7sern", "lin\u00b7den", "ein", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "ART", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und gibt uns solche blicke/", "tokens": ["Und", "gibt", "uns", "sol\u00b7che", "bli\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIS", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Bezeugt uns auch gewi\u00df/", "tokens": ["Be\u00b7zeugt", "uns", "auch", "ge\u00b7wi\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Sie sey die sch\u00f6nste Margaris.", "tokens": ["Sie", "sey", "die", "sch\u00f6ns\u00b7te", "Mar\u00b7ga\u00b7ris", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "3. Drum werd ich auch nicht unrecht thun/", "tokens": ["Drum", "werd", "ich", "auch", "nicht", "un\u00b7recht", "thun", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "PTKNEG", "NN", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wann ich mich auch ergetze/", "tokens": ["Wann", "ich", "mich", "auch", "er\u00b7get\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADV", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und meine feder nun", "tokens": ["Und", "mei\u00b7ne", "fe\u00b7der", "nun"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mit ihrem Tugend-lobe netze/", "tokens": ["Mit", "ih\u00b7rem", "Tu\u00b7gen\u00b7dlo\u00b7be", "net\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Weil ich in meinem sinn", "tokens": ["Weil", "ich", "in", "mei\u00b7nem", "sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Jhr sonsten hoch verpflichtet bin.", "tokens": ["Ihr", "sons\u00b7ten", "hoch", "ver\u00b7pflich\u00b7tet", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "4. Ich habe meine lust an ihr", "tokens": ["Ich", "ha\u00b7be", "mei\u00b7ne", "lust", "an", "ihr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "APPR", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und r\u00fchme sie vor vielen/", "tokens": ["Und", "r\u00fch\u00b7me", "sie", "vor", "vie\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PIAT", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn ich der Tugend zier", "tokens": ["Wenn", "ich", "der", "Tu\u00b7gend", "zier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Seh au\u00df der zarten sch\u00f6nheit spielen/", "tokens": ["Seh", "au\u00df", "der", "zar\u00b7ten", "sch\u00f6n\u00b7heit", "spie\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Und wenn die freundlichkeit", "tokens": ["Und", "wenn", "die", "freund\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die s\u00fcsse reitzungs-macht verneut.", "tokens": ["Die", "s\u00fcs\u00b7se", "reit\u00b7zungs\u00b7macht", "ver\u00b7neut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "5. Wie hab ich manche liebe zeit", "tokens": ["Wie", "hab", "ich", "man\u00b7che", "lie\u00b7be", "zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vor\u00fcber lassen schweben/", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "las\u00b7sen", "schwe\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Seit die gelegenheit", "tokens": ["Seit", "die", "ge\u00b7le\u00b7gen\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.4": {"text": "In ihrer gegenwart zu leben/", "tokens": ["In", "ih\u00b7rer", "ge\u00b7gen\u00b7wart", "zu", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mich bey der linden-stadt", "tokens": ["Mich", "bey", "der", "lin\u00b7den\u00b7stadt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Mehr als zu wohl ergetzet hat.", "tokens": ["Mehr", "als", "zu", "wohl", "er\u00b7get\u00b7zet", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "APPR", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "6. Derhalben weil ihr namens-Licht", "tokens": ["Der\u00b7hal\u00b7ben", "weil", "ihr", "na\u00b7mens\u00b7Licht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So fr\u00f6lich ist erschienen", "tokens": ["So", "fr\u00f6\u00b7lich", "ist", "er\u00b7schie\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Erk\u00fchnt sich meine pflicht", "tokens": ["Er\u00b7k\u00fchnt", "sich", "mei\u00b7ne", "pflicht"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sie gegenw\u00e4rtig zu bedienen/", "tokens": ["Sie", "ge\u00b7gen\u00b7w\u00e4r\u00b7tig", "zu", "be\u00b7die\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und wo ich das nicht kan/", "tokens": ["Und", "wo", "ich", "das", "nicht", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PDS", "PTKNEG", "VMFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "So nehme sie den willen an.", "tokens": ["So", "neh\u00b7me", "sie", "den", "wil\u00b7len", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "7. Der Himmel sey ihr ferner gut/", "tokens": ["Der", "Him\u00b7mel", "sey", "ihr", "fer\u00b7ner", "gut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und gebe sein gedeyen", "tokens": ["Und", "ge\u00b7be", "sein", "ge\u00b7de\u00b7yen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Zu allem was sie thut/", "tokens": ["Zu", "al\u00b7lem", "was", "sie", "thut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PWS", "PPER", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So werd ich gleichfals mich erfreuen", "tokens": ["So", "werd", "ich", "gleich\u00b7fals", "mich", "er\u00b7freu\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und werd ohn allen schein", "tokens": ["Und", "werd", "ohn", "al\u00b7len", "schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Jhr auffzuwarten m\u00fchsam seyn.", "tokens": ["Ihr", "auff\u00b7zu\u00b7war\u00b7ten", "m\u00fch\u00b7sam", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}