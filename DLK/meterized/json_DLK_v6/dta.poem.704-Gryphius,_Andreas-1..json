{"dta.poem.704": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Ade ver fluchtes Threnen-Thaal! ", "tokens": ["A\u00b7de", "ver", "fluch\u00b7tes", "Thre\u00b7nen\u00b7\u00b7T\u00b7haal", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Du ", "tokens": ["Du"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Du vngl\u00fccks ", "tokens": ["Du", "vn\u00b7gl\u00fccks"], "token_info": ["word", "word"], "pos": ["PPER", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Du Folter reiner Hertzen", "tokens": ["Du", "Fol\u00b7ter", "rei\u00b7ner", "Hert\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ade mein Kercker bricht entzwey.", "tokens": ["A\u00b7de", "mein", "Ker\u00b7cker", "bricht", "ent\u00b7zwey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Die Kette rei\u00dft/ mein Geist wird frey", "tokens": ["Die", "Ket\u00b7te", "rei\u00dft", "/", "mein", "Geist", "wird", "frey"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$(", "PPOSAT", "NN", "VAFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}}, "stanza.2": {"line.1": {"text": "Wilkommen offt gew\u00fcndschter Todt/", "tokens": ["Wil\u00b7kom\u00b7men", "offt", "ge\u00b7w\u00fcnd\u00b7schter", "Todt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo du ein Todt zunennen:", "tokens": ["Wo", "du", "ein", "Todt", "zu\u00b7nen\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wilkommen s\u00fcsser lebens Bott", "tokens": ["Wil\u00b7kom\u00b7men", "s\u00fcs\u00b7ser", "le\u00b7bens", "Bott"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wer kan die ", "tokens": ["Wer", "kan", "die"], "token_info": ["word", "word", "word"], "pos": ["PWS", "VMFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "In die vn\u00df GOTT durch dich einf\u00fchrt ", "tokens": ["In", "die", "vn\u00df", "GoTT", "durch", "dich", "ein\u00b7f\u00fchrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NE", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Schmuck mit welchem IESVS zihrt ", "tokens": ["Den", "Schmuck", "mit", "wel\u00b7chem", "IeSVS", "zihrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die standhafft hier gerungen. ", "tokens": ["Die", "stand\u00b7hafft", "hier", "ge\u00b7run\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mein jrrdisch ", "tokens": ["Mein", "jrr\u00b7disch"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Der Nothstall meiner Seelen/ ", "tokens": ["Der", "Noth\u00b7stall", "mei\u00b7ner", "See\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Stock/ die Werckstatt herber pein/", "tokens": ["Der", "Stock", "/", "die", "Wercks\u00b7tatt", "her\u00b7ber", "pein", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "ADV", "NE", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die enge Marter h\u00f6hlen ", "tokens": ["Die", "en\u00b7ge", "Mar\u00b7ter", "h\u00f6h\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der werthe Schatz bleibt vnverletzt:", "tokens": ["Der", "wert\u00b7he", "Schatz", "bleibt", "vn\u00b7ver\u00b7letzt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den wir/ ob schon der ", "tokens": ["Den", "wir", "/", "ob", "schon", "der"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "$(", "KOUS", "ADV", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Dem H\u00f6chsten widerbringen.", "tokens": ["Dem", "H\u00f6chs\u00b7ten", "wi\u00b7der\u00b7brin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Erden schaw' ich vnter mir! ", "tokens": ["Die", "Er\u00b7den", "scha\u00b7w'", "ich", "vn\u00b7ter", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ist di\u00df worumb wir k\u00e4mpffen", "tokens": ["Ist", "di\u00df", "wo\u00b7rumb", "wir", "k\u00e4mpf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PDS", "PWAV", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit Schwerd vnd flammen? Welche wir ", "tokens": ["Mit", "Schwerd", "vnd", "flam\u00b7men", "?", "Wel\u00b7che", "wir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "KON", "VVINF", "$.", "PWS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Blutt vnd leichen d\u00e4mpffen?", "tokens": ["Mit", "Blutt", "vnd", "lei\u00b7chen", "d\u00e4mpf\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "KON", "VVINF", "VMINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Handvoll Gra\u00df/ di\u00df H\u00e4ufflin Sandt/", "tokens": ["Die", "Hand\u00b7voll", "Gra\u00df", "/", "di\u00df", "H\u00e4uf\u00b7flin", "Sandt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "PDS", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vmb welches Eitelkeit vnd Tandt/", "tokens": ["Vmb", "wel\u00b7ches", "Ei\u00b7tel\u00b7keit", "vnd", "Tandt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Vnd fluch vnd Laster dingen.", "tokens": ["Vnd", "fluch", "vnd", "Las\u00b7ter", "din\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Hilff G", "tokens": ["Hilff", "G"], "token_info": ["word", "word"], "pos": ["NE", "NE"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Al\u00df zetter/ ach! vnd klagen.", "tokens": ["Al\u00df", "zet\u00b7ter", "/", "ach", "!", "vnd", "kla\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NN", "$(", "XY", "$.", "KON", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Al\u00df eine bittre threnen See?", "tokens": ["Al\u00df", "ei\u00b7ne", "bitt\u00b7re", "thre\u00b7nen", "See", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vnd H\u00f6llen grause plagen?", "tokens": ["Vnd", "H\u00f6l\u00b7len", "grau\u00b7se", "pla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Heist jhr di\u00df leben die jhr lebt!", "tokens": ["Heist", "jhr", "di\u00df", "le\u00b7ben", "die", "jhr", "lebt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PDS", "VVFIN", "ART", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vnd zwischen furcht vnd leiden schwebt", "tokens": ["Vnd", "zwi\u00b7schen", "furcht", "vnd", "lei\u00b7den", "schwebt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "VVINF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Angstvnd grimm verzehret.", "tokens": ["Die", "Angstvnd", "grimm", "ver\u00b7zeh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Dort f\u00e4llt ein Reich das ander kracht. ", "tokens": ["Dort", "f\u00e4llt", "ein", "Reich", "das", "an\u00b7der", "kracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vnd di\u00df wird nicht gefunden.", "tokens": ["Vnd", "di\u00df", "wird", "nicht", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dort schluckt die Erd' ein jhre pracht/ ", "tokens": ["Dort", "schluckt", "die", "Erd'", "ein", "jhre", "pracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "PPOSAT", "NN", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Die dar in Rauch verschwunden.", "tokens": ["Die", "dar", "in", "Rauch", "ver\u00b7schwun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKVZ", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Was nicht der strenge Nord au\u00dflescht!", "tokens": ["Was", "nicht", "der", "stren\u00b7ge", "Nord", "au\u00df\u00b7lescht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was nicht die stoltze Well\u2019 abw\u00e4scht", "tokens": ["Was", "nicht", "die", "stolt\u00b7ze", "Well'", "ab\u00b7w\u00e4scht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PTKNEG", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wird durch sich selbst verkehret.", "tokens": ["Wird", "durch", "sich", "selbst", "ver\u00b7keh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Vnd mag noch jemand seyn/ der mich", "tokens": ["Vnd", "mag", "noch", "je\u00b7mand", "seyn", "/", "der", "mich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VMFIN", "ADV", "PIS", "VAINF", "$(", "PRELS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit z\u00e4hren rufft zu r\u00fccke ", "tokens": ["Mit", "z\u00e4h\u00b7ren", "rufft", "zu", "r\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PTKZU", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Denckt liebsten wo jhr vnd wo ich!", "tokens": ["Denckt", "liebs\u00b7ten", "wo", "jhr", "vnd", "wo", "ich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "PWAV", "PPER", "KON", "PWAV", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Mi\u00dfg\u00f6nt man mir mein Gl\u00fccke:", "tokens": ["Mi\u00df\u00b7g\u00f6nt", "man", "mir", "mein", "Gl\u00fc\u00b7cke", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich lach' jhr weynt! ich sieg jhr kriegt! ", "tokens": ["Ich", "lach'", "jhr", "weynt", "!", "ich", "sieg", "jhr", "kriegt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich herrsch jhr dient/ ich steh\u2019 jhr liegt", "tokens": ["Ich", "herrsch", "jhr", "dient", "/", "ich", "steh'", "jhr", "liegt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "$(", "PPER", "VVFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich leb\u2019 jhr m\u00fcst verschmachten.", "tokens": ["Ich", "leb'", "jhr", "m\u00fcst", "ver\u00b7schmach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Jhr seyd vmb die man trawren sol; ", "tokens": ["Ihr", "seyd", "vmb", "die", "man", "traw\u00b7ren", "sol", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PRELS", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich den die Lust erquicket.", "tokens": ["Ich", "den", "die", "Lust", "er\u00b7quic\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Jhr zagt/ vnd mir ist ewig wol", "tokens": ["Ihr", "zagt", "/", "vnd", "mir", "ist", "e\u00b7wig", "wol"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "KON", "PPER", "VAFIN", "ADJD", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "GoTT hat mich heim geschicket;", "tokens": ["GoTT", "hat", "mich", "heim", "ge\u00b7schi\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "PTKVZ", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der euch bald ruffen wird zu mir.", "tokens": ["Der", "euch", "bald", "ruf\u00b7fen", "wird", "zu", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVINF", "VAFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In dessen lernt die falsche zier", "tokens": ["In", "des\u00b7sen", "lernt", "die", "fal\u00b7sche", "zier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDS", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der eiteln Welt verachten.", "tokens": ["Der", "ei\u00b7teln", "Welt", "ver\u00b7ach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Ade jhr liebsten ich mu\u00df fort/", "tokens": ["A\u00b7de", "jhr", "liebs\u00b7ten", "ich", "mu\u00df", "fort", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "PPER", "VMFIN", "PTKVZ", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "La\u00dft ab von ewren thr\u00e4nen.", "tokens": ["La\u00dft", "ab", "von", "ew\u00b7ren", "thr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Denckt da\u00df ich au\u00df-steig in den Port", "tokens": ["Denckt", "da\u00df", "ich", "au\u00df\u00b7steig", "in", "den", "Port"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KOUS", "PPER", "ADJD", "APPR", "ART", "NE"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Nach dem sich alle s\u00e4hnen", "tokens": ["Nach", "dem", "sich", "al\u00b