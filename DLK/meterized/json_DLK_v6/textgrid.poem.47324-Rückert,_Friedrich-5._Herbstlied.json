{"textgrid.poem.47324": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "5. Herbstlied", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was sagt der Herbst der Ros' ins Ohr.", "tokens": ["Was", "sagt", "der", "Herbst", "der", "Ros'", "ins", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df sie die Munterkeit verlor?", "tokens": ["Da\u00df", "sie", "die", "Mun\u00b7ter\u00b7keit", "ver\u00b7lor", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er mahnt sie an die Nichtigkeit", "tokens": ["Er", "mahnt", "sie", "an", "die", "Nich\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Treue, die der Lenz ihr schwor.", "tokens": ["Der", "Treu\u00b7e", ",", "die", "der", "Lenz", "ihr", "schwor", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie rei\u00dft entzwei den Schleier, den", "tokens": ["Sie", "rei\u00dft", "ent\u00b7zwei", "den", "Schlei\u00b7er", ",", "den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "ART"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.6": {"text": "Sie nahm, als er zur Braut sie kor;", "tokens": ["Sie", "nahm", ",", "als", "er", "zur", "Braut", "sie", "kor", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "APPRART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und wie sie bleich vom Throne sinkt,", "tokens": ["Und", "wie", "sie", "bleich", "vom", "Thro\u00b7ne", "sinkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Erseufzt der Nachtigallen Chor.", "tokens": ["Er\u00b7seufzt", "der", "Nach\u00b7ti\u00b7gal\u00b7len", "Chor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wer brach entzwei das Lilienschwert?", "tokens": ["Wer", "brach", "ent\u00b7zwei", "das", "Li\u00b7li\u00b7en\u00b7schwert", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "So blank geschliffen war's zuvor.", "tokens": ["So", "blank", "ge\u00b7schlif\u00b7fen", "wa\u00b7r's", "zu\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "VAFIN", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Die Tulp' entfloh so eilig, da\u00df", "tokens": ["Die", "Tul\u00b7p'", "ent\u00b7floh", "so", "ei\u00b7lig", ",", "da\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "$,", "KOUS"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Den Turban sie am Weg verlor.", "tokens": ["Den", "Tur\u00b7ban", "sie", "am", "Weg", "ver\u00b7lor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Besch\u00e4mt senkt der Iasmin sein Haupt,", "tokens": ["Be\u00b7sch\u00e4mt", "senkt", "der", "I\u00b7as\u00b7min", "sein", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Weil ihm der Ost die Locken schor.", "tokens": ["Weil", "ihm", "der", "Ost", "die", "Lo\u00b7cken", "schor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Es streut der Wind mit voller Hand", "tokens": ["Es", "streut", "der", "Wind", "mit", "vol\u00b7ler", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Von B\u00e4umen Bl\u00e4ttergold empor.", "tokens": ["Von", "B\u00e4u\u00b7men", "Bl\u00e4t\u00b7ter\u00b7gold", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Das d\u00fcrre Laub schwirrt durch die Luft,", "tokens": ["Das", "d\u00fcr\u00b7re", "Laub", "schwirrt", "durch", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Wie Flederm\u00e4us' aus Gr\u00e4berthor.", "tokens": ["Wie", "Fle\u00b7der\u00b7m\u00e4us'", "aus", "Gr\u00e4\u00b7bert\u00b7hor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Das Totenlied der Sch\u00f6pfung spielt", "tokens": ["Das", "To\u00b7ten\u00b7lied", "der", "Sch\u00f6p\u00b7fung", "spielt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Der Herbstwind auf geknicktem Rohr.", "tokens": ["Der", "Herbst\u00b7wind", "auf", "ge\u00b7knick\u00b7tem", "Rohr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Die finstre Tanne tr\u00e4gt den Schnee", "tokens": ["Die", "finst\u00b7re", "Tan\u00b7ne", "tr\u00e4gt", "den", "Schnee"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Wie wei\u00dfen Bund ums Haupt ein Mohr.", "tokens": ["Wie", "wei\u00b7\u00dfen", "Bund", "ums", "Haupt", "ein", "Mohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Der Berg nahm wei\u00dfen Hermelin,", "tokens": ["Der", "Berg", "nahm", "wei\u00b7\u00dfen", "Her\u00b7me\u00b7lin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Weil ihm die nackte Schulter fror.", "tokens": ["Weil", "ihm", "die", "nack\u00b7te", "Schul\u00b7ter", "fror", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "O sieh des Jahrs Verw\u00fcstung an", "tokens": ["O", "sieh", "des", "Jahrs", "Ver\u00b7w\u00fcs\u00b7tung", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Und hole frischen Wein hervor!", "tokens": ["Und", "ho\u00b7le", "fri\u00b7schen", "Wein", "her\u00b7vor", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Die Sonne sandt' uns, eh' sie wich,", "tokens": ["Die", "Son\u00b7ne", "sandt'", "uns", ",", "eh'", "sie", "wich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Den jungen Most ins Haus zuvor,", "tokens": ["Den", "jun\u00b7gen", "Most", "ins", "Haus", "zu\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Da\u00df er uns leucht' an ihrer Statt,", "tokens": ["Da\u00df", "er", "uns", "leucht'", "an", "ih\u00b7rer", "Statt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.30": {"text": "Wann ihre Kraft d\u00e4mpft Wolkenflor.", "tokens": ["Wann", "ih\u00b7re", "Kraft", "d\u00e4mpft", "Wol\u00b7ken\u00b7flor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Sieh, wie des Wintergreises Grimm", "tokens": ["Sieh", ",", "wie", "des", "Win\u00b7ter\u00b7grei\u00b7ses", "Grimm"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Des Fr\u00fchlingskindes Hauch beschwor.", "tokens": ["Des", "Fr\u00fch\u00b7lings\u00b7kin\u00b7des", "Hauch", "be\u00b7schwor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Er weckt im Bechert\u00f6nen ein", "tokens": ["Er", "weckt", "im", "Be\u00b7cher\u00b7t\u00f6\u00b7nen", "ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Verzaubert Nachtigallenchor;", "tokens": ["Ver\u00b7zau\u00b7bert", "Nach\u00b7ti\u00b7gal\u00b7len\u00b7chor", ";"], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Und trunkne Blicke sich ergehn", "tokens": ["Und", "trunk\u00b7ne", "Bli\u00b7cke", "sich", "er\u00b7gehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "PRF", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Auf sch\u00f6ner Wangen Rosenflor.", "tokens": ["Auf", "sch\u00f6\u00b7ner", "Wan\u00b7gen", "Ro\u00b7sen\u00b7flor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Du trink', und seufz' im Winter nicht;", "tokens": ["Du", "trink'", ",", "und", "seuf\u00b7z'", "im", "Win\u00b7ter", "nicht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "APPRART", "NN", "PTKNEG", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.38": {"text": "Denn auch im Fr\u00fchling seufzt ein Thor.", "tokens": ["Denn", "auch", "im", "Fr\u00fch\u00b7ling", "seufzt", "ein", "Thor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}