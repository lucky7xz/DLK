{"textgrid.poem.60103": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sch\u00f6n, o Sephine!", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6n, o Sephine!", "tokens": ["Sch\u00f6n", ",", "o", "Se\u00b7phi\u00b7ne", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Steht im blonden Haar der gr\u00fcne", "tokens": ["Steht", "im", "blon\u00b7den", "Haar", "der", "gr\u00fc\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lorber-Kranz", "tokens": ["Lor\u00b7ber\u00b7Kranz"], "token_info": ["word"], "pos": ["NE"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Den, im Weihe-Tanz", "tokens": ["Den", ",", "im", "Wei\u00b7he\u00b7Tanz"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$,", "APPRART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Holder Musen geschlungen,", "tokens": ["Hol\u00b7der", "Mu\u00b7sen", "ge\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Sich ein M\u00e4dchen ersungen.", "tokens": ["Sich", "ein", "M\u00e4d\u00b7chen", "er\u00b7sun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "VVPP", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "Aber milderen Glanz,", "tokens": ["A\u00b7ber", "mil\u00b7de\u00b7ren", "Glanz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "H\u00f6here Freuden, o Sephine,", "tokens": ["H\u00f6\u00b7he\u00b7re", "Freu\u00b7den", ",", "o", "Se\u00b7phi\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "FM", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Hat der gr\u00fcne", "tokens": ["Hat", "der", "gr\u00fc\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Myrthen-Kranz,", "tokens": ["Myr\u00b7then\u00b7Kranz", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "Den, im Weihe-Tanz,", "tokens": ["Den", ",", "im", "Wei\u00b7he\u00b7Tanz", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.12": {"text": "Mit der unschuldvollen Rechten", "tokens": ["Mit", "der", "un\u00b7schuld\u00b7vol\u00b7len", "Rech\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Liebes-G\u00f6tter flechten.", "tokens": ["Lie\u00b7bes\u00b7G\u00f6t\u00b7ter", "flech\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.14": {"text": "Sch\u00f6ner war,", "tokens": ["Sch\u00f6\u00b7ner", "war", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VAFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.15": {"text": "Auch im k\u00f6stlichsten Geschmeide,", "tokens": ["Auch", "im", "k\u00f6st\u00b7lichs\u00b7ten", "Ge\u00b7schmei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "--+---+-", "measure": "anapaest.init"}, "line.16": {"text": "Nie der K\u00f6niginnen Haar,", "tokens": ["Nie", "der", "K\u00f6\u00b7ni\u00b7gin\u00b7nen", "Haar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Als des M\u00e4dchens, wenn es beyde", "tokens": ["Als", "des", "M\u00e4d\u00b7chens", ",", "wenn", "es", "bey\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "$,", "KOUS", "PPER", "PIS"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Kr\u00e4nze vereint.", "tokens": ["Kr\u00e4n\u00b7ze", "ver\u00b7eint", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.19": {"text": "Selig scheint", "tokens": ["Se\u00b7lig", "scheint"], "token_info": ["word", "word"], "pos": ["NE", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.20": {"text": "Mir der J\u00fcngling, o Sephine,", "tokens": ["Mir", "der", "J\u00fcng\u00b7ling", ",", "o", "Se\u00b7phi\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "FM", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Der es wagt,", "tokens": ["Der", "es", "wagt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.22": {"text": "Und die Lorber-Umwundne fragt:", "tokens": ["Und", "die", "Lor\u00b7ber\u00b7Um\u00b7wund\u00b7ne", "fragt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.23": {"text": "Ob er sie verdiene?", "tokens": ["Ob", "er", "sie", "ver\u00b7die\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.24": {"text": "Dem die Myrthen-Umflochtne sagt:", "tokens": ["Dem", "die", "Myr\u00b7then\u00b7Um\u00b7flocht\u00b7ne", "sagt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.25": {"text": "J\u00fcngling! ich gehe mit dir;", "tokens": ["J\u00fcng\u00b7ling", "!", "ich", "ge\u00b7he", "mit", "dir", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+++-+", "measure": "unknown.measure.penta"}, "line.26": {"text": "Nimm von mir,", "tokens": ["Nimm", "von", "mir", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.27": {"text": "Was die G\u00f6tter mir gegeben:", "tokens": ["Was", "die", "G\u00f6t\u00b7ter", "mir", "ge\u00b7ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Nimm Gesang und Lieb' und Leben!", "tokens": ["Nimm", "Ge\u00b7sang", "und", "Lieb'", "und", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}