{"textgrid.poem.49476": {"metadata": {"author": {"name": "Verlaine, Paul-Marie", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Glocken, die von Rom uns gestern kehrten, dr\u00f6hnen", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Glocken, die von Rom uns gestern kehrten, dr\u00f6hnen", "tokens": ["Die", "Glo\u00b7cken", ",", "die", "von", "Rom", "uns", "ge\u00b7stern", "kehr\u00b7ten", ",", "dr\u00f6h\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NE", "PPER", "ADV", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+---+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Zum Himmel Lobgesang in feierlichen T\u00f6nen.", "tokens": ["Zum", "Him\u00b7mel", "Lob\u00b7ge\u00b7sang", "in", "fei\u00b7er\u00b7li\u00b7chen", "T\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Das Echo, das vom Turme m\u00e4chtig flutend ruft,", "tokens": ["Das", "E\u00b7cho", ",", "das", "vom", "Tur\u00b7me", "m\u00e4ch\u00b7tig", "flu\u00b7tend", "ruft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "ADJD", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verherrlicht rings die weiten Lande und die Luft.", "tokens": ["Ver\u00b7herr\u00b7licht", "rings", "die", "wei\u00b7ten", "Lan\u00b7de", "und", "die", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der Vogel, der geweiht vom Goldklang heil'ger Gr\u00fcsse,", "tokens": ["Der", "Vo\u00b7gel", ",", "der", "ge\u00b7weiht", "vom", "Gold\u00b7klang", "heil'\u00b7ger", "Gr\u00fcs\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "PRELS", "VVPP", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vergisst sein Klagen und stimmt an der Hymnen S\u00fcsse.", "tokens": ["Ver\u00b7gisst", "sein", "Kla\u00b7gen", "und", "stimmt", "an", "der", "Hym\u00b7nen", "S\u00fcs\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Und froh sein Halleluja zwitschernd durch die Welt", "tokens": ["Und", "froh", "sein", "Hal\u00b7le\u00b7lu\u00b7ja", "zwit\u00b7schernd", "durch", "die", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "PPOSAT", "NN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Singt er auf Busch und Baum, in Wiese, Wald und Feld.", "tokens": ["Singt", "er", "auf", "Busch", "und", "Baum", ",", "in", "Wie\u00b7se", ",", "Wald", "und", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NE", "$,", "APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Die Lerche hat mit Festgesang sich aufgeschwungen,", "tokens": ["Die", "Ler\u00b7che", "hat", "mit", "Fest\u00b7ge\u00b7sang", "sich", "auf\u00b7ge\u00b7schwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "PRF", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem tau'gen Morgen hat die Nachtigall gesungen.", "tokens": ["Dem", "tau'\u00b7gen", "Mor\u00b7gen", "hat", "die", "Nach\u00b7ti\u00b7gall", "ge\u00b7sun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Mit z\u00e4rtlich s\u00fcssen T\u00f6nen heisser Liebesglut,", "tokens": ["Mit", "z\u00e4rt\u00b7lich", "s\u00fcs\u00b7sen", "T\u00f6\u00b7nen", "heis\u00b7ser", "Lie\u00b7bes\u00b7glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der sonnenhell das Gl\u00fcck in stillem Herzen ruht,", "tokens": ["Der", "son\u00b7nen\u00b7hell", "das", "Gl\u00fcck", "in", "stil\u00b7lem", "Her\u00b7zen", "ruht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Lebt freudenvoll der Lenz, der gestern neu erstanden,", "tokens": ["Lebt", "freu\u00b7den\u00b7voll", "der", "Lenz", ",", "der", "ge\u00b7stern", "neu", "er\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$,", "PRELS", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So selig seufzt Natur, und in den weiten Landen", "tokens": ["So", "se\u00b7lig", "seufzt", "Na\u00b7tur", ",", "und", "in", "den", "wei\u00b7ten", "Lan\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "NN", "$,", "KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Von dunklen T\u00fcrmen manchen altersgrauen Baus", "tokens": ["Von", "dunk\u00b7len", "T\u00fcr\u00b7men", "man\u00b7chen", "al\u00b7ters\u00b7grau\u00b7en", "Baus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vom Campanile nieder und vom K\u00f6nigshaus.", "tokens": ["Vom", "Cam\u00b7pa\u00b7ni\u00b7le", "nie\u00b7der", "und", "vom", "K\u00f6\u00b7nigs\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Aus allen St\u00e4dten, da von Festgel\u00e4ut und Singen", "tokens": ["Aus", "al\u00b7len", "St\u00e4d\u00b7ten", ",", "da", "von", "Fest\u00b7ge\u00b7l\u00e4ut", "und", "Sin\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "KOUS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Paris und Moskau, London und Sevilla klingen,", "tokens": ["Pa\u00b7ris", "und", "Mos\u00b7kau", ",", "Lon\u00b7don", "und", "Se\u00b7vil\u00b7la", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "$,", "NE", "KON", "NE", "VVINF", "$,"], "meter": "-+-+-++-+--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "T\u00f6nt hell der Jubelruf der Glocken, der uns weiht", "tokens": ["T\u00f6nt", "hell", "der", "Ju\u00b7bel\u00b7ruf", "der", "Glo\u00b7cken", ",", "der", "uns", "weiht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ART", "NN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zum gnadenreichen Fest der heil'gen Osterzeit.", "tokens": ["Zum", "gna\u00b7den\u00b7rei\u00b7chen", "Fest", "der", "heil'\u00b7gen", "Os\u00b7ter\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Die Taube streift die Flur, das Lamm bl\u00f6kt im Gehege,", "tokens": ["Die", "Tau\u00b7be", "streift", "die", "Flur", ",", "das", "Lamm", "bl\u00f6kt", "im", "Ge\u00b7he\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wem bist, Maria, du, begegnet auf dem Wege?", "tokens": ["Wem", "bist", ",", "Ma\u00b7ria", ",", "du", ",", "be\u00b7geg\u00b7net", "auf", "dem", "We\u00b7ge", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "NE", "$,", "PPER", "$,", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.12": {"line.1": {"text": "Gold ist der Fluss, der neu der Sonne Glanz empfing.", "tokens": ["Gold", "ist", "der", "Fluss", ",", "der", "neu", "der", "Son\u00b7ne", "Glanz", "emp\u00b7fing", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "PRELS", "ADJD", "ART", "NN", "NN", "VVFIN", "$."], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Es ist der Herr, der einst in Galil\u00e4a ging.", "tokens": ["Es", "ist", "der", "Herr", ",", "der", "einst", "in", "Ga\u00b7li\u00b7l\u00e4a", "ging", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.13": {"line.1": {"text": "\u2013 Was w\u00e4scht das \u00f6de Herz sich nicht im gold'nen Strome,", "tokens": ["\u2013", "Was", "w\u00e4scht", "das", "\u00f6\u00b7de", "Herz", "sich", "nicht", "im", "gold'\u00b7nen", "Stro\u00b7me", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "ART", "ADJA", "NN", "PRF", "PTKNEG", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was heiligt nicht den Geist der goldne Klang vom Dome?", "tokens": ["Was", "hei\u00b7ligt", "nicht", "den", "Geist", "der", "gold\u00b7ne", "Klang", "vom", "Do\u00b7me", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PTKNEG", "ART", "NN", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.14": {"line.1": {"text": "Was fleht nicht wie ein Lamm der Seele bang Gebet,", "tokens": ["Was", "fleht", "nicht", "wie", "ein", "Lamm", "der", "See\u00b7le", "bang", "Ge\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PTKNEG", "KOKOM", "ART", "NN", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der weissen Taube gleich, da alles neu ersteht?", "tokens": ["Der", "weis\u00b7sen", "Tau\u00b7be", "gleich", ",", "da", "al\u00b7les", "neu", "er\u00b7steht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,", "KOUS", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.15": {"line.1": {"text": "Was zieht der Mensch, der einst in g\u00f6ttlichem Vertrauen,", "tokens": ["Was", "zieht", "der", "Mensch", ",", "der", "einst", "in", "g\u00f6tt\u00b7li\u00b7chem", "Ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$,", "PRELS", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht heute noch den Pfad nach Galil\u00e4as Auen?", "tokens": ["Nicht", "heu\u00b7te", "noch", "den", "Pfad", "nach", "Ga\u00b7li\u00b7l\u00e4\u00b7as", "Au\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "ART", "NN", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}