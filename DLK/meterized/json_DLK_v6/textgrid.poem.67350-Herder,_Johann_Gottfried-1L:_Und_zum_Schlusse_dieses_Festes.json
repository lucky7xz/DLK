{"textgrid.poem.67350": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und zum Schlusse dieses Festes", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und zum Schlusse dieses Festes", "tokens": ["Und", "zum", "Schlus\u00b7se", "die\u00b7ses", "Fes\u00b7tes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kosten wir ein Glas Madera,", "tokens": ["Kos\u00b7ten", "wir", "ein", "Glas", "Ma\u00b7de\u00b7ra", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "NE", "$,"], "meter": "+--+++-+", "measure": "dactylic.init"}, "line.3": {"text": "S\u00fc\u00df und traurig: ", "tokens": ["S\u00fc\u00df", "und", "trau\u00b7rig", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Robert Machin, Anna d'Arfet,", "tokens": ["Ro\u00b7bert", "Ma\u00b7chin", ",", "An\u00b7na", "d'\u00b7A\u00b7rfet", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NE", "NE", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Er ein edler Britenj\u00fcngling,", "tokens": ["Er", "ein", "ed\u00b7ler", "Bri\u00b7ten\u00b7j\u00fcng\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie die Tochter stolzer Eltern,", "tokens": ["Sie", "die", "Toch\u00b7ter", "stol\u00b7zer", "El\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Beide liebten sich, doch traurig.", "tokens": ["Bei\u00b7de", "lieb\u00b7ten", "sich", ",", "doch", "trau\u00b7rig", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "$,", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Hingeworfen ins Gef\u00e4ngni\u00df", "tokens": ["Hin\u00b7ge\u00b7wor\u00b7fen", "ins", "Ge\u00b7f\u00e4ng\u00b7ni\u00df"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von des M\u00e4dchens stolzen Eltern,", "tokens": ["Von", "des", "M\u00e4d\u00b7chens", "stol\u00b7zen", "El\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schmachtete der edle Machin;", "tokens": ["Schmach\u00b7te\u00b7te", "der", "ed\u00b7le", "Ma\u00b7chin", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Doch sein Herz blieb unver\u00e4ndert.", "tokens": ["Doch", "sein", "Herz", "blieb", "un\u00b7ver\u00b7\u00e4n\u00b7dert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und des jungen Mannes Freunde", "tokens": ["Und", "des", "jun\u00b7gen", "Man\u00b7nes", "Freun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "R\u00fcsten ihm ein Schiff am Ufer,", "tokens": ["R\u00fcs\u00b7ten", "ihm", "ein", "Schiff", "am", "U\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fchren Robert aus dem Kerker,", "tokens": ["F\u00fch\u00b7ren", "Ro\u00b7bert", "aus", "dem", "Ker\u00b7ker", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihm die Braut in seine Arme.", "tokens": ["Ihm", "die", "Braut", "in", "sei\u00b7ne", "Ar\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Willig folget ihm die treue", "tokens": ["Wil\u00b7lig", "fol\u00b7get", "ihm", "die", "treu\u00b7e"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Anna d'Arfet in die Wellen.", "tokens": ["An\u00b7na", "d'\u00b7A\u00b7rfet", "in", "die", "Wel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "ART", "NN", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Liebe Wellen, rauschet gl\u00fccklich!", "tokens": ["Lie\u00b7be", "Wel\u00b7len", ",", "rau\u00b7schet", "gl\u00fcck\u00b7lich", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fahret wohl, geliebte Beide!", "tokens": ["Fah\u00b7ret", "wohl", ",", "ge\u00b7lieb\u00b7te", "Bei\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Hin nach Frankreichs holdem Ufer", "tokens": ["Hin", "nach", "Fran\u00b7kreichs", "hol\u00b7dem", "U\u00b7fer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NE", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Steuern sie mit Macht und Kr\u00e4ften;", "tokens": ["Steu\u00b7ern", "sie", "mit", "Macht", "und", "Kr\u00e4f\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch die K\u00fcste schwindet traurig,", "tokens": ["Doch", "die", "K\u00fcs\u00b7te", "schwin\u00b7det", "trau\u00b7rig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Traurig seufzen alle Winde.", "tokens": ["Trau\u00b7rig", "seuf\u00b7zen", "al\u00b7le", "Win\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Dreizehn lange Tag' und N\u00e4chte", "tokens": ["Drei\u00b7zehn", "lan\u00b7ge", "Tag'", "und", "N\u00e4ch\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "ADJA", "NN", "KON", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Schweben sie auf offnem Meere,", "tokens": ["Schwe\u00b7ben", "sie", "auf", "off\u00b7nem", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ohne Weg' und ohne Rettung;", "tokens": ["Oh\u00b7ne", "Weg'", "und", "oh\u00b7ne", "Ret\u00b7tung", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rette sie, geliebte Liebe!", "tokens": ["Ret\u00b7te", "sie", ",", "ge\u00b7lieb\u00b7te", "Lie\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Da ging ihnen auf der Freude,", "tokens": ["Da", "ging", "ih\u00b7nen", "auf", "der", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf der Hoffnung Morgenr\u00f6the;", "tokens": ["Auf", "der", "Hoff\u00b7nung", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieh, ein nahes sch\u00f6nes Eiland,", "tokens": ["Sieh", ",", "ein", "na\u00b7hes", "sch\u00f6\u00b7nes", "Ei\u00b7land", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Namlos \u2013 jetzo hei\u00dft's ", "tokens": ["Nam\u00b7los", "\u2013", "jet\u00b7zo", "hei\u00dft's"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$(", "ADV", "NE"], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Neue V\u00f6gel, neue B\u00e4ume,", "tokens": ["Neu\u00b7e", "V\u00f6\u00b7gel", ",", "neu\u00b7e", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6ne Th\u00e4ler, holde H\u00fcgel", "tokens": ["Sch\u00f6\u00b7ne", "Th\u00e4\u00b7ler", ",", "hol\u00b7de", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Locken freundlich sie zur K\u00fcste,", "tokens": ["Lo\u00b7cken", "freund\u00b7lich", "sie", "zur", "K\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fliegen freundlich um ihr Segel.", "tokens": ["Flie\u00b7gen", "freund\u00b7lich", "um", "ihr", "Se\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "\u00bbach, es ist der Sitz der Liebe,\u00ab", "tokens": ["\u00bb", "ach", ",", "es", "ist", "der", "Sitz", "der", "Lie\u00b7be", ",", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ITJ", "$,", "PPER", "VAFIN", "ART", "NN", "ART", "NN", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spricht das freudetrunkne M\u00e4dchen,", "tokens": ["Spricht", "das", "freu\u00b7de\u00b7trunk\u00b7ne", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbmitten unter wilden Wellen", "tokens": ["\u00bb", "mit\u00b7ten", "un\u00b7ter", "wil\u00b7den", "Wel\u00b7len"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Uns vom Himmel zubereitet!", "tokens": ["Uns", "vom", "Him\u00b7mel", "zu\u00b7be\u00b7rei\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Ferne von Europa's Ufer,", "tokens": ["Fer\u00b7ne", "von", "Eu\u00b7ro\u00b7pa's", "U\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Von dem ungl\u00fccksel'gen Ufer,", "tokens": ["Von", "dem", "un\u00b7gl\u00fcck\u00b7sel'\u00b7gen", "U\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eine der gl\u00fccksel'gen Inseln", "tokens": ["Ei\u00b7ne", "der", "gl\u00fcck\u00b7sel'\u00b7gen", "In\u00b7seln"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus den alten Fabelzeiten.\u00ab", "tokens": ["Aus", "den", "al\u00b7ten", "Fa\u00b7bel\u00b7zei\u00b7ten", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Und sie steigen aus zum Lande,", "tokens": ["Und", "sie", "stei\u00b7gen", "aus", "zum", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gr\u00fc\u00dfend die geliebte K\u00fcste.", "tokens": ["Gr\u00fc\u00b7\u00dfend", "die", "ge\u00b7lieb\u00b7te", "K\u00fcs\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die krystallne Wasserwoge", "tokens": ["Die", "krys\u00b7tall\u00b7ne", "Was\u00b7ser\u00b7wo\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-++", "measure": "unknown.measure.penta"}, "line.4": {"text": "Kommt und spielt um ihre F\u00fc\u00dfe.", "tokens": ["Kommt", "und", "spielt", "um", "ih\u00b7re", "F\u00fc\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Wilde Thiere kommen schmeichelnd,", "tokens": ["Wil\u00b7de", "Thie\u00b7re", "kom\u00b7men", "schmei\u00b7chelnd", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Huldigend dem K\u00f6nigspaare;", "tokens": ["Hul\u00b7di\u00b7gend", "dem", "K\u00f6\u00b7nig\u00b7spaa\u00b7re", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$."], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.3": {"text": "Tausend Nachtigallen singen", "tokens": ["Tau\u00b7send", "Nach\u00b7ti\u00b7gal\u00b7len", "sin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["CARD", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihnen Lobgesang der Liebe.", "tokens": ["Ih\u00b7nen", "Lob\u00b7ge\u00b7sang", "der", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Und sie finden ein verborgnes", "tokens": ["Und", "sie", "fin\u00b7den", "ein", "ver\u00b7borg\u00b7nes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6nes Thal, von dichten B\u00e4umen", "tokens": ["Sch\u00f6\u00b7nes", "Thal", ",", "von", "dich\u00b7ten", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Rings umschattet, wie ein Tempel,", "tokens": ["Rings", "um\u00b7schat\u00b7tet", ",", "wie", "ein", "Tem\u00b7pel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ein Paradies der Liebe.", "tokens": ["Wie", "ein", "Pa\u00b7ra\u00b7dies", "der", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "\u00bbhier, Geliebter,\u00ab spricht das M\u00e4dchen,", "tokens": ["\u00bb", "hier", ",", "Ge\u00b7lieb\u00b7ter", ",", "\u00ab", "spricht", "das", "M\u00e4d\u00b7chen", ","], "token_info": ["punct", "word", "punct", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "NN", "$,", "$(", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbin dem Tempel la\u00df uns wohnen!", "tokens": ["\u00bb", "in", "dem", "Tem\u00b7pel", "la\u00df", "uns", "woh\u00b7nen", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "VVIMP", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Unter diesem heil'gen Baume", "tokens": ["Un\u00b7ter", "die\u00b7sem", "heil'\u00b7gen", "Bau\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": 