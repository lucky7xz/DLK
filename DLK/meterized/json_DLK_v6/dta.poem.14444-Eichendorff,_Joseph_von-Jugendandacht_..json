{"dta.poem.14444": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Jugendandacht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "War's niemals da, als rief die Eine, Deine?", "tokens": ["Wa\u00b7r's", "nie\u00b7mals", "da", ",", "als", "rief", "die", "Ei\u00b7ne", ",", "Dei\u00b7ne", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "$,", "KOUS", "VVFIN", "ART", "NN", "$,", "PPOSAT", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Lockt' Dich kein Weh, kein br\u00fcnstiges Verlangen", "tokens": ["Lockt'", "Dich", "kein", "Weh", ",", "kein", "br\u00fcns\u00b7ti\u00b7ges", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$,", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nach andrer Zeit, die lange schon vergangen,", "tokens": ["Nach", "an\u00b7drer", "Zeit", ",", "die", "lan\u00b7ge", "schon", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf ewig einzugeh'n in gr\u00fcne Scheine?", "tokens": ["Auf", "e\u00b7wig", "ein\u00b7zu\u00b7geh'n", "in", "gr\u00fc\u00b7ne", "Schei\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}