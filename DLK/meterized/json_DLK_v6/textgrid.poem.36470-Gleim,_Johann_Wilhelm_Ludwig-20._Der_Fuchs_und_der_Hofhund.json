{"textgrid.poem.36470": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "20. Der Fuchs und der Hofhund", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In K\u00f6nig L\u00f6wens Monarchie,", "tokens": ["In", "K\u00f6\u00b7nig", "L\u00f6\u00b7wens", "Mon\u00b7ar\u00b7chie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "(\u00e4sop und Ph\u00e4drus kannten sie,)", "tokens": ["(", "\u00e4\u00b7sop", "und", "Ph\u00e4d\u00b7rus", "kann\u00b7ten", "sie", ",", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "KON", "NE", "VVFIN", "PPER", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bestellen allemal die Erben,", "tokens": ["Be\u00b7stel\u00b7len", "al\u00b7le\u00b7mal", "die", "Er\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn ihnen reiche Vettern sterben,", "tokens": ["Wenn", "ih\u00b7nen", "rei\u00b7che", "Vet\u00b7tern", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Zum Lobredner den Fuchs.", "tokens": ["Zum", "Lob\u00b7red\u00b7ner", "den", "Fuchs", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NE", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Einst starb ein reicher Luchs;", "tokens": ["Einst", "starb", "ein", "rei\u00b7cher", "Luchs", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Da trat der Redner auf,", "tokens": ["Da", "trat", "der", "Red\u00b7ner", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Erz\u00e4hlte seinen Lebenslauf,", "tokens": ["Er\u00b7z\u00e4hl\u00b7te", "sei\u00b7nen", "Le\u00b7bens\u00b7lauf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sprach:", "tokens": ["Sprach", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-", "measure": "single.down"}}, "stanza.3": {"line.1": {"text": "Ein Hofhund stand auf beiden Hinterf\u00fc\u00dfen,", "tokens": ["Ein", "Hof\u00b7hund", "stand", "auf", "bei\u00b7den", "Hin\u00b7ter\u00b7f\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und macht' ein h\u00e4misches Gesicht,", "tokens": ["Und", "macht'", "ein", "h\u00e4\u00b7mi\u00b7sches", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem roten Redner, sagend: ", "tokens": ["Dem", "ro\u00b7ten", "Red\u00b7ner", ",", "sa\u00b7gend", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}