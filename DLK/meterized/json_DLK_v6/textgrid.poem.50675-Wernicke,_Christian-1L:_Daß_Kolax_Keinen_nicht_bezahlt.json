{"textgrid.poem.50675": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da\u00df Kolax Keinen nicht bezahlt", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df Kolax Keinen nicht bezahlt", "tokens": ["Da\u00df", "Ko\u00b7lax", "Kei\u00b7nen", "nicht", "be\u00b7zahlt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "NN", "PTKNEG", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und doch mit vielen Dienern prahlt,", "tokens": ["Und", "doch", "mit", "vie\u00b7len", "Die\u00b7nern", "prahlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df er in einer Kutsche f\u00e4hrt,", "tokens": ["Da\u00df", "er", "in", "ei\u00b7ner", "Kut\u00b7sche", "f\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist nicht so sehr Verwunderns werth.", "tokens": ["Ist", "nicht", "so", "sehr", "Ver\u00b7wun\u00b7derns", "werth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "ADV", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "La\u00df seine Gl\u00e4ub'ger sich gedulden:", "tokens": ["La\u00df", "sei\u00b7ne", "Gl\u00e4ub'\u00b7ger", "sich", "ge\u00b7dul\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zwei Pferde haben gnug zu ziehn an seinen Schulden.", "tokens": ["Zwei", "Pfer\u00b7de", "ha\u00b7ben", "gnug", "zu", "ziehn", "an", "sei\u00b7nen", "Schul\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "ADV", "PTKZU", "VVINF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}