{"dta.poem.6039": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Sonne, den Planeten,  \n und vom Firmament.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es scheint, ob wir die Sonn' im Creyse lauffen sehn,", "tokens": ["Es", "scheint", ",", "ob", "wir", "die", "Sonn'", "im", "Crey\u00b7se", "lauf\u00b7fen", "sehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "APPRART", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dieweil wir uns auf einer Kugel drehn.", "tokens": ["Die\u00b7weil", "wir", "uns", "auf", "ei\u00b7ner", "Ku\u00b7gel", "drehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn sie von Osten her, nach Westen scheint zu eilen,", "tokens": ["Wenn", "sie", "von", "Os\u00b7ten", "her", ",", "nach", "Wes\u00b7ten", "scheint", "zu", "ei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "PTKVZ", "$,", "APPR", "NN", "VVFIN", "PTKZU", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und aus dem Wasser hier, dort in die Fluht zu gehn;", "tokens": ["Und", "aus", "dem", "Was\u00b7ser", "hier", ",", "dort", "in", "die", "Fluht", "zu", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "$,", "ADV", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ist es die Erd allein, die, da sie gehet,", "tokens": ["Ist", "es", "die", "Erd", "al\u00b7lein", ",", "die", ",", "da", "sie", "ge\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "$,", "PRELS", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und sich um ihre Ax, von West nach Osten drehet;", "tokens": ["Und", "sich", "um", "ih\u00b7re", "Ax", ",", "von", "West", "nach", "Os\u00b7ten", "dre\u00b7het", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "PPOSAT", "NN", "$,", "APPR", "NE", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Verursacht, da\u00df es uns, als wenn die Welt", "tokens": ["Ver\u00b7ur\u00b7sacht", ",", "da\u00df", "es", "uns", ",", "als", "wenn", "die", "Welt"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "PPER", "$,", "KOKOM", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Gantz anders sich bewegt, ins Auge f\u00e4llt.", "tokens": ["Gantz", "an\u00b7ders", "sich", "be\u00b7wegt", ",", "ins", "Au\u00b7ge", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PRF", "VVFIN", "$,", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}