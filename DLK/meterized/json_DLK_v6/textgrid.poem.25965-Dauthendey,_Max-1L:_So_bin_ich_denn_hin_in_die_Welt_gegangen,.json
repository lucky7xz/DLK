{"textgrid.poem.25965": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "1L: So bin ich denn hin in die Welt gegangen,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So bin ich denn hin in die Welt gegangen,", "tokens": ["So", "bin", "ich", "denn", "hin", "in", "die", "Welt", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In den Herbst, wo die Winde sich Bl\u00e4tter fangen,", "tokens": ["In", "den", "Herbst", ",", "wo", "die", "Win\u00b7de", "sich", "Bl\u00e4t\u00b7ter", "fan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "ART", "NN", "PRF", "NN", "VVFIN", "$,"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Und langen N\u00e4chten entgegen.", "tokens": ["Und", "lan\u00b7gen", "N\u00e4ch\u00b7ten", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Doch immer sah ich auf allen Wegen", "tokens": ["Doch", "im\u00b7mer", "sah", "ich", "auf", "al\u00b7len", "We\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ein Weib mit Blicken, mit bangen,", "tokens": ["Ein", "Weib", "mit", "Bli\u00b7cken", ",", "mit", "ban\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,", "APPR", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Die blieben wie Schatten am Herzen mir hangen,", "tokens": ["Die", "blie\u00b7ben", "wie", "Schat\u00b7ten", "am", "Her\u00b7zen", "mir", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KOKOM", "NN", "APPRART", "NN", "PPER", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Und fielen zur Erde wie weinender Regen.", "tokens": ["Und", "fie\u00b7len", "zur", "Er\u00b7de", "wie", "wei\u00b7nen\u00b7der", "Re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "KOKOM", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}}}}