{"dta.poem.18579": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "L eib - Aria Mademoiselle. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Meine Losung heist Vergn\u00fcgen/", "tokens": ["Mei\u00b7ne", "Lo\u00b7sung", "heist", "Ver\u00b7gn\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die allzeit frohe Brust/", "tokens": ["Und", "die", "all\u00b7zeit", "fro\u00b7he", "Brust", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kan kein Ungel\u00fcck besigen/", "tokens": ["Kan", "kein", "Un\u00b7ge\u00b7l\u00fcck", "be\u00b7si\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn mir bleibet unbewust/", "tokens": ["Denn", "mir", "blei\u00b7bet", "un\u00b7be\u00b7wust", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie sich andre in der Seelen/", "tokens": ["Wie", "sich", "and\u00b7re", "in", "der", "See\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "PIS", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Uber ihr Verh\u00e4ngni\u00df qu\u00e4len.", "tokens": ["U\u00b7ber", "ihr", "Ver\u00b7h\u00e4ng\u00b7ni\u00df", "qu\u00e4\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Was der Himmel hat beschlossen/", "tokens": ["Was", "der", "Him\u00b7mel", "hat", "be\u00b7schlos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nehm' ich allzeit willig an.", "tokens": ["Nehm'", "ich", "all\u00b7zeit", "wil\u00b7lig", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn sich kr\u00e4ncken sind nur Possen/", "tokens": ["Denn", "sich", "kr\u00e4n\u00b7cken", "sind", "nur", "Pos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "VVINF", "VAFIN", "ADV", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil man es nicht \u00e4ndern kan.", "tokens": ["Weil", "man", "es", "nicht", "\u00e4n\u00b7dern", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und es mu\u00df nach Sturm und Krachen/", "tokens": ["Und", "es", "mu\u00df", "nach", "Sturm", "und", "Kra\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "APPR", "NN", "KON", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Doch die Sonne wieder lachen.", "tokens": ["Doch", "die", "Son\u00b7ne", "wie\u00b7der", "la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sucht mich Amor zu ber\u00fccken/", "tokens": ["Sucht", "mich", "A\u00b7mor", "zu", "be\u00b7r\u00fc\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NE", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denck ich doch in meinen Sinn/", "tokens": ["Denck", "ich", "doch", "in", "mei\u00b7nen", "Sinn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eben so mu\u00df mirs gel\u00fcck", "tokens": ["E\u00b7ben", "so", "mu\u00df", "mirs", "ge\u00b7l\u00fcck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VMFIN", "NE", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Der Verlust ist mein Gewinn.", "tokens": ["Der", "Ver\u00b7lust", "ist", "mein", "Ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich bin noch darzu vergn\u00fcget/", "tokens": ["Ich", "bin", "noch", "dar\u00b7zu", "ver\u00b7gn\u00fc\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PAV", "VVFIN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Da\u00df es sich so mit mir f\u00fcget.", "tokens": ["Da\u00df", "es", "sich", "so", "mit", "mir", "f\u00fc\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Mein Gem\u00fcthe liebt die Sch\u00e4tze/", "tokens": ["Mein", "Ge\u00b7m\u00fc\u00b7the", "liebt", "die", "Sch\u00e4t\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So der Tugend Reichthum schenckt.", "tokens": ["So", "der", "Tu\u00b7gend", "Reicht\u00b7hum", "schenckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wer in der Falschheit Netze/", "tokens": ["Und", "wer", "in", "der", "Falschheit", "Net\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und an Laster-Stricken henckt/", "tokens": ["Und", "an", "Las\u00b7ter\u00b7Stri\u00b7cken", "henckt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Den soll meine Seele neiden/", "tokens": ["Den", "soll", "mei\u00b7ne", "See\u00b7le", "nei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPOSAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und als Gifft und Schlangen meiden.", "tokens": ["Und", "als", "Gifft", "und", "Schlan\u00b7gen", "mei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Falsche M\u00e4uler m\u00f6gen reden/", "tokens": ["Fal\u00b7sche", "M\u00e4u\u00b7ler", "m\u00f6\u00b7gen", "re\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn mich gleich Verl\u00e4umdung trifft.", "tokens": ["Wenn", "mich", "gleich", "Ver\u00b7l\u00e4um\u00b7dung", "trifft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "De\u0303nn es saugen auch die Kr\u00f6ten/", "tokens": ["D\u1ebdnn", "es", "sau\u00b7gen", "auch", "die", "Kr\u00f6\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus den sch\u00f6nsten Blumen-Gifft.", "tokens": ["Aus", "den", "sch\u00f6ns\u00b7ten", "Blu\u00b7men\u00b7Gifft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Ich will allen Neid verh\u00f6nen/", "tokens": ["Ich", "will", "al\u00b7len", "Neid", "ver\u00b7h\u00f6\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und mich mit der Tugend kr\u00f6nen.", "tokens": ["Und", "mich", "mit", "der", "Tu\u00b7gend", "kr\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Viele gehn auf harten Wegen/", "tokens": ["Vie\u00b7le", "gehn", "auf", "har\u00b7ten", "We\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die in schlechten G\u00fccke stehn.", "tokens": ["Die", "in", "schlech\u00b7ten", "G\u00fc\u00b7cke", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich will auf den Freuden-Stegen/", "tokens": ["Ich", "will", "auf", "den", "Freu\u00b7den\u00b7Ste\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und auf lauter Liljen gehn.", "tokens": ["Und", "auf", "lau\u00b7ter", "Lil\u00b7jen", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "M\u00fcssen manchmahl Dornen stechen/", "tokens": ["M\u00fcs\u00b7sen", "manch\u00b7mahl", "Dor\u00b7nen", "ste\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird die Zeit auch Rosen brechen.", "tokens": ["Wird", "die", "Zeit", "auch", "Ro\u00b7sen", "bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Geh Melancholey Von hinnen/", "tokens": ["Geh", "Me\u00b7lan\u00b7cho\u00b7ley", "Von", "hin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "ADV", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ihr Sorgen packet euch:", "tokens": ["Und", "ihr", "Sor\u00b7gen", "pa\u00b7cket", "euch", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn ich bau in meinen Sinnen.", "tokens": ["Denn", "ich", "bau", "in", "mei\u00b7nen", "Sin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mir ein irrdisch Himmelreich.", "tokens": ["Mir", "ein", "irr\u00b7disch", "Him\u00b7mel\u00b7reich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nichts kan mein Gem\u00fcht besiegen/", "tokens": ["Nichts", "kan", "mein", "Ge\u00b7m\u00fcht", "be\u00b7sie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPOSAT", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Weil die Losung heist vergn\u00fcgen.", "tokens": ["Weil", "die", "Lo\u00b7sung", "heist", "ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}