{"textgrid.poem.34835": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr Olaf, es ist Mitternacht,", "tokens": ["Herr", "O\u00b7laf", ",", "es", "ist", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PPER", "VAFIN", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Dein Leben ist verflossen!", "tokens": ["Dein", "Le\u00b7ben", "ist", "ver\u00b7flos\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du hattest eines F\u00fcrstenkinds", "tokens": ["Du", "hat\u00b7test", "ei\u00b7nes", "F\u00fcrs\u00b7ten\u00b7kinds"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In freier Lust genossen.", "tokens": ["In", "frei\u00b7er", "Lust", "ge\u00b7nos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die M\u00f6nche murmeln das Totengebet,", "tokens": ["Die", "M\u00f6n\u00b7che", "mur\u00b7meln", "das", "To\u00b7ten\u00b7ge\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Mann im roten Rocke,", "tokens": ["Der", "Mann", "im", "ro\u00b7ten", "Ro\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er steht mit seinem blanken Beil", "tokens": ["Er", "steht", "mit", "sei\u00b7nem", "blan\u00b7ken", "Beil"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schon vor dem schwarzen Blocke.", "tokens": ["Schon", "vor", "dem", "schwar\u00b7zen", "Blo\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Herr Olaf steigt in den Hof hinab,", "tokens": ["Herr", "O\u00b7laf", "steigt", "in", "den", "Hof", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Da blinken viel Schwerter und Lichter.", "tokens": ["Da", "blin\u00b7ken", "viel", "Schwer\u00b7ter", "und", "Lich\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Es l\u00e4chelt des Ritters roter Mund,", "tokens": ["Es", "l\u00e4\u00b7chelt", "des", "Rit\u00b7ters", "ro\u00b7ter", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit l\u00e4chelndem Munde spricht er:", "tokens": ["Mit", "l\u00e4\u00b7cheln\u00b7dem", "Mun\u00b7de", "spricht", "er", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "\u00bbich segne die Sonne, ich segne den Mond,", "tokens": ["\u00bb", "ich", "seg\u00b7ne", "die", "Son\u00b7ne", ",", "ich", "seg\u00b7ne", "den", "Mond", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und die Stern', die am Himmel schweifen.", "tokens": ["Und", "die", "Stern'", ",", "die", "am", "Him\u00b7mel", "schwei\u00b7fen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVINF", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Ich segne auch die V\u00f6gelein,", "tokens": ["Ich", "seg\u00b7ne", "auch", "die", "V\u00f6\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die in den L\u00fcften pfeifen.", "tokens": ["Die", "in", "den", "L\u00fcf\u00b7ten", "pfei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ich segne das Meer, ich segne das Land,", "tokens": ["Ich", "seg\u00b7ne", "das", "Meer", ",", "ich", "seg\u00b7ne", "das", "Land", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und die Blumen auf der Aue.", "tokens": ["Und", "die", "Blu\u00b7men", "auf", "der", "Au\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich segne die Veilchen, sie sind so sanft", "tokens": ["Ich", "seg\u00b7ne", "die", "Veil\u00b7chen", ",", "sie", "sind", "so", "sanft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VAFIN", "ADV", "ADJD"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Wie die Augen meiner Fraue.", "tokens": ["Wie", "die", "Au\u00b7gen", "mei\u00b7ner", "Frau\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ihr Veilchenaugen meiner Frau,", "tokens": ["Ihr", "Veil\u00b7chen\u00b7au\u00b7gen", "mei\u00b7ner", "Frau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch euch verlier ich mein Leben!", "tokens": ["Durch", "euch", "ver\u00b7lier", "ich", "mein", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Ich segne auch den Holunderbaum,", "tokens": ["Ich", "seg\u00b7ne", "auch", "den", "Ho\u00b7lun\u00b7der\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wo du dich mir ergeben.\u00ab", "tokens": ["Wo", "du", "dich", "mir", "er\u00b7ge\u00b7ben", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPER", "PRF", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}