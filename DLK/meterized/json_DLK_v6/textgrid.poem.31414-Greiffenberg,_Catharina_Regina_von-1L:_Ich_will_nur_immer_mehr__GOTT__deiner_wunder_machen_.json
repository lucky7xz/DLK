{"textgrid.poem.31414": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich will nur immer mehr / GOTT / deiner wunder machen /", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich will nur immer mehr / GOTT / deiner wunder machen /", "tokens": ["Ich", "will", "nur", "im\u00b7mer", "mehr", "/", "GoTT", "/", "dei\u00b7ner", "wun\u00b7der", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "ADV", "$(", "NE", "$(", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "durch harren auff dein g\u00fct'. Ich la\u00df' / ich la\u00df dich nicht /", "tokens": ["durch", "har\u00b7ren", "auff", "dein", "g\u00fct'", ".", "Ich", "la\u00df'", "/", "ich", "la\u00df", "dich", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "APPR", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "$(", "PPER", "VVIMP", "PPER", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "ob mir der widerstand schon Hertz und Bein zerbricht /", "tokens": ["ob", "mir", "der", "wi\u00b7der\u00b7stand", "schon", "Hertz", "und", "Bein", "zer\u00b7bricht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "VVFIN", "ADV", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "las sterbend' auch nicht ab / du segnst denn meine sachen.", "tokens": ["las", "ster\u00b7bend'", "auch", "nicht", "ab", "/", "du", "segnst", "denn", "mei\u00b7ne", "sa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "PTKNEG", "PTKVZ", "$(", "PPER", "ADV", "KON", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du pflegst einmal f\u00fcr den / der dir vertraut / zu wachen.", "tokens": ["Du", "pflegst", "ein\u00b7mal", "f\u00fcr", "den", "/", "der", "dir", "ver\u00b7traut", "/", "zu", "wa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "$(", "PRELS", "PPER", "ADJD", "$(", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Dein Mund ja seine h\u00fclff dem Elenden verspricht /", "tokens": ["Dein", "Mund", "ja", "sei\u00b7ne", "h\u00fclff", "dem", "E\u00b7len\u00b7den", "ver\u00b7spricht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "PPOSAT", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "den ganzen Allmachts schwall / zu ihrem dienst verpflicht.", "tokens": ["den", "gan\u00b7zen", "All\u00b7machts", "schwall", "/", "zu", "ih\u00b7rem", "dienst", "ver\u00b7pflicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$(", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "wer / (bist du ihre st\u00e4rk) ist st\u00e4rker als die schwachen?", "tokens": ["wer", "/", "(", "bist", "du", "ih\u00b7re", "st\u00e4rk", ")", "ist", "st\u00e4r\u00b7ker", "als", "die", "schwa\u00b7chen", "?"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$(", "$(", "VAFIN", "PPER", "PPOSAT", "NN", "$(", "VAFIN", "ADJD", "KOKOM", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ist diese nur bey mir / so bin ich schon vergn\u00fcget:", "tokens": ["Ist", "die\u00b7se", "nur", "bey", "mir", "/", "so", "bin", "ich", "schon", "ver\u00b7gn\u00fc\u00b7get", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ADV", "APPR", "PPER", "$(", "ADV", "VAFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "sie zeig sich gleich im werk / durch offnes wunder\u00fcben:", "tokens": ["sie", "zeig", "sich", "gleich", "im", "werk", "/", "durch", "off\u00b7nes", "wun\u00b7der\u00b7\u00fc\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "APPRART", "NN", "$(", "APPR", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "wann zur erleuchtung dann ihr will der Geist belieben /", "tokens": ["wann", "zur", "er\u00b7leuch\u00b7tung", "dann", "ihr", "will", "der", "Geist", "be\u00b7lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "ADV", "PPER", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So ists mir gleichfalls recht / wann er nur wunder f\u00fcgt.", "tokens": ["So", "ists", "mir", "gleich\u00b7falls", "recht", "/", "wann", "er", "nur", "wun\u00b7der", "f\u00fcgt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "$(", "PWAV", "PPER", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Gott / du hast selbst die Lust in meinen Geist entz\u00fcndet:", "tokens": ["Gott", "/", "du", "hast", "selbst", "die", "Lust", "in", "mei\u00b7nen", "Geist", "ent\u00b7z\u00fcn\u00b7det", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PPER", "VAFIN", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Dieselb' ist nicht vergn\u00fcgt / bis sie dich selbst empfindet.", "tokens": ["Die\u00b7selb'", "ist", "nicht", "ver\u00b7gn\u00fcgt", "/", "bis", "sie", "dich", "selbst", "emp\u00b7fin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKNEG", "VVPP", "$(", "KOUS", "PPER", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}