{"textgrid.poem.34940": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die reichen Leute, die gewinnt", "tokens": ["Die", "rei\u00b7chen", "Leu\u00b7te", ",", "die", "ge\u00b7winnt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Man nur durch platte Schmeichelein \u2013", "tokens": ["Man", "nur", "durch", "plat\u00b7te", "Schmei\u00b7che\u00b7lein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Geld ist platt, mein liebes Kind,", "tokens": ["Das", "Geld", "ist", "platt", ",", "mein", "lie\u00b7bes", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und will auch platt geschmeichelt sein.", "tokens": ["Und", "will", "auch", "platt", "ge\u00b7schmei\u00b7chelt", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ADJD", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das Weihrauchfa\u00df, das schwinge keck", "tokens": ["Das", "Weih\u00b7rauch\u00b7fa\u00df", ",", "das", "schwin\u00b7ge", "keck"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vor jedem g\u00f6ttlich goldnen Kalb;", "tokens": ["Vor", "je\u00b7dem", "g\u00f6tt\u00b7lich", "gold\u00b7nen", "Kalb", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bet an im Staub, bet an im Dreck,", "tokens": ["Bet", "an", "im", "Staub", ",", "bet", "an", "im", "Dreck", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPRART", "NN", "$,", "VVFIN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor allem aber lob nicht halb.", "tokens": ["Vor", "al\u00b7lem", "a\u00b7ber", "lob", "nicht", "halb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADV", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Das Brot ist teuer dieses Jahr,", "tokens": ["Das", "Brot", "ist", "teu\u00b7er", "die\u00b7ses", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jedoch die sch\u00f6nsten Worte hat", "tokens": ["Je\u00b7doch", "die", "sch\u00f6ns\u00b7ten", "Wor\u00b7te", "hat"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man noch umsonst \u2013 Besinge gar", "tokens": ["Man", "noch", "um\u00b7sonst", "\u2013", "Be\u00b7sin\u00b7ge", "gar"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PIS", "ADV", "ADV", "$(", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "M\u00e4cenas' Hund, und fri\u00df dich satt!", "tokens": ["M\u00e4\u00b7ce\u00b7nas'", "Hund", ",", "und", "fri\u00df", "dich", "satt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "KON", "VVIMP", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}