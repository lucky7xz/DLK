{"textgrid.poem.25684": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "Grabschrift auf einen Geitzigen", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier ruht Avar, im Wuchern einst ein Held,", "tokens": ["Hier", "ruht", "A\u00b7var", ",", "im", "Wu\u00b7chern", "einst", "ein", "Held", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "$,", "APPRART", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wenn anders er kann Ruhe daf\u00fcr haben,", "tokens": ["Wenn", "an\u00b7ders", "er", "kann", "Ru\u00b7he", "da\u00b7f\u00fcr", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "VMFIN", "NN", "PAV", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df man ihn f\u00fcr sein baares Geld,", "tokens": ["Da\u00df", "man", "ihn", "f\u00fcr", "sein", "baa\u00b7res", "Geld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht gratis, hat begraben.", "tokens": ["Nicht", "gra\u00b7tis", ",", "hat", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}