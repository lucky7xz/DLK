{"textgrid.poem.40407": {"metadata": {"author": {"name": "Scheffel, Joseph Viktor von", "birth": "N.A.", "death": "N.A."}, "title": "Der Komet", "genre": "verse", "period": "N.A.", "pub_year": 1856, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich armer Komet in dem himmlischen Feld,", "tokens": ["Ich", "ar\u00b7mer", "Ko\u00b7met", "in", "dem", "himm\u00b7li\u00b7schen", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Wie ist's doch so windig mit mir bestellt!", "tokens": ["Wie", "ist's", "doch", "so", "win\u00b7dig", "mit", "mir", "be\u00b7stellt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "ADV", "ADJD", "APPR", "PPER", "VVPP", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich leb' in steten Sorgen,", "tokens": ["Ich", "leb'", "in", "ste\u00b7ten", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Licht selbst mu\u00df ich borgen ...", "tokens": ["Mein", "Licht", "selbst", "mu\u00df", "ich", "bor\u00b7gen", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VMFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich erscheine nur von Zeit zu Zeit,", "tokens": ["Ich", "er\u00b7schei\u00b7ne", "nur", "von", "Zeit", "zu", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Dann mu\u00df ich wieder fort in die Dunkelheit.", "tokens": ["Dann", "mu\u00df", "ich", "wie\u00b7der", "fort", "in", "die", "Dun\u00b7kel\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PTKVZ", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Frau Sonne die hat mir's angetan,", "tokens": ["Frau", "Son\u00b7ne", "die", "hat", "mir's", "an\u00b7ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ART", "VAFIN", "NE", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie zieht mich magnetisch zu sich hinan,", "tokens": ["Sie", "zieht", "mich", "mag\u00b7ne\u00b7tisch", "zu", "sich", "hi\u00b7nan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch kann mir's nie gelingen,", "tokens": ["Doch", "kann", "mir's", "nie", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "NE", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu ihr mich aufzuschwingen,", "tokens": ["Zu", "ihr", "mich", "auf\u00b7zu\u00b7schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich schmachte nach ihr nur aus luftiger Fern',", "tokens": ["Ich", "schmach\u00b7te", "nach", "ihr", "nur", "aus", "luf\u00b7ti\u00b7ger", "Fern'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Denn leider bin ich wirklich ein exzentrischer Stern.", "tokens": ["Denn", "lei\u00b7der", "bin", "ich", "wirk\u00b7lich", "ein", "ex\u00b7zent\u00b7ri\u00b7scher", "Stern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}, "stanza.3": {"line.1": {"text": "Die Fixstern' all' in bittrem Hohn", "tokens": ["Die", "Fix\u00b7stern'", "all'", "in", "bitt\u00b7rem", "Hohn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PIS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Betrachten mich wie einen verlorenen Sohn,", "tokens": ["Be\u00b7trach\u00b7ten", "mich", "wie", "ei\u00b7nen", "ver\u00b7lo\u00b7re\u00b7nen", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sie sagen, ich t\u00e4t' wanken", "tokens": ["Sie", "sa\u00b7gen", ",", "ich", "t\u00e4t'", "wan\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und hin und wieder schwanken,", "tokens": ["Und", "hin", "und", "wie\u00b7der", "schwan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "KON", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und wo ich einmal des Wegs gestrichen w\u00e4r',", "tokens": ["Und", "wo", "ich", "ein\u00b7mal", "des", "Wegs", "ge\u00b7stri\u00b7chen", "w\u00e4r'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Sei nichts als Dunst und Nebel ringsumher.", "tokens": ["Sei", "nichts", "als", "Dunst", "und", "Ne\u00b7bel", "rings\u00b7um\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "KOKOM", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die Planeten sehn mich ver\u00e4chtlich an,", "tokens": ["Die", "Pla\u00b7ne\u00b7ten", "sehn", "mich", "ver\u00b7\u00e4cht\u00b7lich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Als wollt' ich sie durchkreuzen auf ihrer Bahn;", "tokens": ["Als", "wollt'", "ich", "sie", "durch\u00b7kreu\u00b7zen", "auf", "ih\u00b7rer", "Bahn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Frau Venus und ihre Schwestern", "tokens": ["Frau", "Ve\u00b7nus", "und", "ih\u00b7re", "Schwes\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NE", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tun boshaft mich verl\u00e4stern:", "tokens": ["Tun", "bos\u00b7haft", "mich", "ver\u00b7l\u00e4s\u00b7tern", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbsein Schweif ist zu gro\u00df, sein Kern ist zu klein,", "tokens": ["\u00bb", "sein", "Schweif", "ist", "zu", "gro\u00df", ",", "sein", "Kern", "ist", "zu", "klein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "PTKA", "ADJD", "$,", "PPOSAT", "NN", "VAFIN", "PTKA", "ADJD", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ich m\u00f6chte kein so mi\u00dfgestalter Nachtwandler sein!\u00ab", "tokens": ["Ich", "m\u00f6ch\u00b7te", "kein", "so", "mi\u00df\u00b7ge\u00b7stal\u00b7ter", "Nacht\u00b7wand\u00b7ler", "sein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "ADV", "ADJA", "NN", "VAINF", "$.", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}, "stanza.5": {"line.1": {"text": "So hat man mir einen Leumund gemacht", "tokens": ["So", "hat", "man", "mir", "ei\u00b7nen", "Leu\u00b7mund", "ge\u00b7macht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIS", "PPER", "ART", "NN", "VVPP"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Als Schw\u00e4rmer und als Irrgeist, den jeder verlacht;", "tokens": ["Als", "Schw\u00e4r\u00b7mer", "und", "als", "Irr\u00b7geist", ",", "den", "je\u00b7der", "ver\u00b7lacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "KOUS", "NN", "$,", "PRELS", "PIS", "VVPP", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und drunten auf der Erden", "tokens": ["Und", "drun\u00b7ten", "auf", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verk\u00fcnden die Gelehrten:", "tokens": ["Ver\u00b7k\u00fcn\u00b7den", "die", "Ge\u00b7lehr\u00b7ten", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbes ist an ihm nichts fest, nichts dicht,", "tokens": ["\u00bb", "es", "ist", "an", "ihm", "nichts", "fest", ",", "nichts", "dicht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "APPR", "PPER", "PIS", "PTKVZ", "$,", "PIS", "ADJD", "$,"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und kreist er bis in Ewigkeit, solid wird er nicht.\u00ab", "tokens": ["Und", "kreist", "er", "bis", "in", "E\u00b7wig\u00b7keit", ",", "so\u00b7lid", "wird", "er", "nicht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,", "KON", "VAFIN", "PPER", "PTKNEG", "$.", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}, "stanza.6": {"line.1": {"text": "Selbst ", "tokens": ["Selbst"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Behandelt mich im \u00bb", "tokens": ["Be\u00b7han\u00b7delt", "mich", "im", "\u00bb"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Treib' ich solch' Schwindelwesen,", "tokens": ["Treib'", "ich", "solch'", "Schwin\u00b7del\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df man von mir darf lesen:", "tokens": ["Da\u00df", "man", "von", "mir", "darf", "le\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbes f\u00fcllt der Komet, viel d\u00fcnner denn Schaum,", "tokens": ["\u00bb", "es", "f\u00fcllt", "der", "Ko\u00b7met", ",", "viel", "d\u00fcn\u00b7ner", "denn", "Schaum", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "$,", "ADV", "ADJD", "KON", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Mit allerkleinster Masse den allergr\u00f6\u00dften Raum??\u00ab", "tokens": ["Mit", "al\u00b7ler\u00b7kleins\u00b7ter", "Mas\u00b7se", "den", "al\u00b7ler\u00b7gr\u00f6\u00df\u00b7ten", "Raum", "??", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Aber warte nur, du Sternguckerneid,", "tokens": ["A\u00b7ber", "war\u00b7te", "nur", ",", "du", "Stern\u00b7gu\u00b7cker\u00b7neid", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "PPER", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Ihr kennt mich noch nicht von der innersten Seit',", "tokens": ["Ihr", "kennt", "mich", "noch", "nicht", "von", "der", "in\u00b7ners\u00b7ten", "Seit'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Einst werd' ich euch begegnen,", "tokens": ["Einst", "werd'", "ich", "euch", "be\u00b7geg\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dann sollt ihr euch besegnen:", "tokens": ["Dann", "sollt", "ihr", "euch", "be\u00b7seg\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dann fahrt ihr durch mich durch und ich schnupp' euch noch was", "tokens": ["Dann", "fahrt", "ihr", "durch", "mich", "durch", "und", "ich", "schnupp'", "euch", "noch", "was"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "APPR", "KON", "PPER", "VVFIN", "PPER", "ADV", "PWS"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Und hagl' euch Meteorstein' ins Fernrohrglas.", "tokens": ["Und", "hagl'", "euch", "Me\u00b7te\u00b7or\u00b7stein'", "ins", "Fern\u00b7rohr\u00b7glas", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "NE", "APPRART", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}}}}