{"dta.poem.11318": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Eines  musici.  \n * *  R.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein stetes lustig-seyn war mir mein frohes leben,", "tokens": ["Ein", "ste\u00b7tes", "lus\u00b7tig\u00b7seyn", "war", "mir", "mein", "fro\u00b7hes", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "PPOSAT", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich spielte, was man wolt, und was man mir gegeben;", "tokens": ["Ich", "spiel\u00b7te", ",", "was", "man", "wolt", ",", "und", "was", "man", "mir", "ge\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "PIS", "VMFIN", "$,", "KON", "PWS", "PIS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jtzund pausir ich zwar, so fordert es der chor:", "tokens": ["Jt\u00b7zund", "pau\u00b7sir", "ich", "zwar", ",", "so", "for\u00b7dert", "es", "der", "chor", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ach! legte man mir einst nur kein ", "tokens": ["Ach", "!", "leg\u00b7te", "man", "mir", "einst", "nur", "kein"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "VVFIN", "PIS", "PPER", "ADV", "ADV", "PIAT"], "meter": "-+-+-+-+", "measure