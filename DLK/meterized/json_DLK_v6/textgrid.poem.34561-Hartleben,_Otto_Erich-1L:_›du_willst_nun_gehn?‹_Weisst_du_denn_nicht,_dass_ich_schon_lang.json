{"textgrid.poem.34561": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u203adu willst nun gehn?\u2039 Weisst du denn nicht, dass ich schon lang", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u203adu willst nun gehn?\u2039 Weisst du denn nicht, dass ich schon lang", "tokens": ["\u203a", "du", "willst", "nun", "gehn", "?", "\u2039", "Weisst", "du", "denn", "nicht", ",", "dass", "ich", "schon", "lang"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VMFIN", "ADV", "VVINF", "$.", "$(", "VVFIN", "PPER", "ADV", "PTKNEG", "$,", "KOUS", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "von dir gegangen bin? Dass nur ein Schatten noch,", "tokens": ["von", "dir", "ge\u00b7gan\u00b7gen", "bin", "?", "Dass", "nur", "ein", "Schat\u00b7ten", "noch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "VAFIN", "$.", "KOUS", "ADV", "ART", "NN", "ADV", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "ein Schein vor deinen Augen steht, den du nur siehst?", "tokens": ["ein", "Schein", "vor", "dei\u00b7nen", "Au\u00b7gen", "steht", ",", "den", "du", "nur", "siehst", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Fest glaubt ich mich gewappnet mit dem Panzerhemd", "tokens": ["Fest", "glaubt", "ich", "mich", "ge\u00b7wapp\u00b7net", "mit", "dem", "Pan\u00b7zer\u00b7hemd"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PRF", "VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "heiter klirrenden Hasses wider eine Welt,", "tokens": ["hei\u00b7ter", "klir\u00b7ren\u00b7den", "Has\u00b7ses", "wi\u00b7der", "ei\u00b7ne", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "nur wenige Eisenmaschen standen offen noch", "tokens": ["nur", "we\u00b7ni\u00b7ge", "Ei\u00b7sen\u00b7ma\u00b7schen", "stan\u00b7den", "of\u00b7fen", "noch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "ADJD", "ADV"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "von ungef\u00e4hr \u2013 die fandest du und trafst mich gut!", "tokens": ["von", "un\u00b7ge\u00b7f\u00e4hr", "\u2013", "die", "fan\u00b7dest", "du", "und", "trafst", "mich", "gut", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "$(", "ART", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wie einsam war ich schon \u2013 und war's noch nicht genug!", "tokens": ["Wie", "ein\u00b7sam", "war", "ich", "schon", "\u2013", "und", "wa\u00b7r's", "noch", "nicht", "ge\u00b7nug", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "ADV", "$(", "KON", "VAFIN", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Jetzt kann ich erst leicht mit vielen sp\u00f6ttisch und freundlich sein,", "tokens": ["Jetzt", "kann", "ich", "erst", "leicht", "mit", "vie\u00b7len", "sp\u00f6t\u00b7tisch", "und", "freund\u00b7lich", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADJD", "APPR", "PIAT", "ADJD", "KON", "ADJD", "VAINF", "$,"], "meter": "+---+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "in Stunden, wo der Ekel \u00fcberlistet ist \u2013", "tokens": ["in", "Stun\u00b7den", ",", "wo", "der", "E\u00b7kel", "\u00fc\u00b7berl\u00b7is\u00b7tet", "ist", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAV", "ART", "NN", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "jetzt tanzen die G\u00f6tter mir auf der flachen Hand!", "tokens": ["jetzt", "tan\u00b7zen", "die", "G\u00f6t\u00b7ter", "mir", "auf", "der", "fla\u00b7chen", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Und das dank ich dir und meinem geflickten Eisenwamms.", "tokens": ["Und", "das", "dank", "ich", "dir", "und", "mei\u00b7nem", "ge\u00b7flick\u00b7ten", "Ei\u00b7sen\u00b7wamms", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPER", "PPER", "KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}}, "stanza.4": {"line.1": {"text": "Du aber wusstest nicht, was du gethan \u2013 du stehst", "tokens": ["Du", "a\u00b7ber", "wuss\u00b7test", "nicht", ",", "was", "du", "ge\u00b7than", "\u2013", "du", "stehst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VVPP", "$(", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und fragst: \u203aDu willst nun gehn?\u2039 \u2013 Und bin doch schon so weit! \u2013", "tokens": ["und", "fragst", ":", "\u203a", "Du", "willst", "nun", "gehn", "?", "\u2039", "\u2013", "Und", "bin", "doch", "schon", "so", "weit", "!", "\u2013"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "PPER", "VMFIN", "ADV", "VVINF", "$.", "$(", "$(", "KON", "VAFIN", "ADV", "ADV", "ADV", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "O sichre dir in der Brust dein umf\u00fchlend Herz \u2013:", "tokens": ["O", "sich\u00b7re", "dir", "in", "der", "Brust", "dein", "um\u00b7f\u00fch\u00b7lend", "Herz", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "ART", "NN", "PPOSAT", "ADJD", "NN", "$(", "$."], "meter": "+---+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Wertvolleres Erbteil spendet uns die Erde nicht.", "tokens": ["Wert\u00b7vol\u00b7le\u00b7res", "Erb\u00b7teil", "spen\u00b7det", "uns", "die", "Er\u00b7de", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "PTKNEG", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}}}}}