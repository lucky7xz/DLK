{"textgrid.poem.25801": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "1L: Fern, irgendwo im Himmelblau,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fern, irgendwo im Himmelblau,", "tokens": ["Fern", ",", "ir\u00b7gend\u00b7wo", "im", "Him\u00b7mel\u00b7blau", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein sonderzartes Land.", "tokens": ["Ein", "son\u00b7derz\u00b7ar\u00b7tes", "Land", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Heiden wei\u00df,", "tokens": ["Die", "Hei\u00b7den", "wei\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Besprossen lilaklare Primelbl\u00fcten.", "tokens": ["Be\u00b7spros\u00b7sen", "li\u00b7lak\u00b7la\u00b7re", "Pri\u00b7mel\u00b7bl\u00fc\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Bl\u00fcten gro\u00df, offen erschlossen,", "tokens": ["Bl\u00fc\u00b7ten", "gro\u00df", ",", "of\u00b7fen", "er\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "ADJD", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Augen, weite Augen, die an Tr\u00e4nen saugen,", "tokens": ["Au\u00b7gen", ",", "wei\u00b7te", "Au\u00b7gen", ",", "die", "an", "Tr\u00e4\u00b7nen", "sau\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.7": {"text": "Sanfte Augen, die ein Paradies beh\u00fcten.", "tokens": ["Sanf\u00b7te", "Au\u00b7gen", ",", "die", "ein", "Pa\u00b7ra\u00b7dies", "be\u00b7h\u00fc\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Mit wei\u00dfen Fingern", "tokens": ["Mit", "wei\u00b7\u00dfen", "Fin\u00b7gern"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Ein stilles Kind", "tokens": ["Ein", "stil\u00b7les", "Kind"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Spielt mit den Primeln,", "tokens": ["Spielt", "mit", "den", "Pri\u00b7meln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Lacht mit dem Wind.", "tokens": ["Lacht", "mit", "dem", "Wind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Zaudernd auf schleichenden Zehen,", "tokens": ["Zau\u00b7dernd", "auf", "schlei\u00b7chen\u00b7den", "Ze\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "\u00dcber die Bl\u00fcten,", "tokens": ["\u00dc\u00b7ber", "die", "Bl\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Wei\u00dfe Rudel", "tokens": ["Wei\u00b7\u00dfe", "Ru\u00b7del"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Von wei\u00dfen Rehen.", "tokens": ["Von", "wei\u00b7\u00dfen", "Re\u00b7hen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Alles so licht und so eigen.", "tokens": ["Al\u00b7les", "so", "licht", "und", "so", "ei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Einsam entbl\u00e4ttert das Schweigen.", "tokens": ["Ein\u00b7sam", "ent\u00b7bl\u00e4t\u00b7tert", "das", "Schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}}}}