{"dta.poem.16702": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "61.  Wer sich verl\u00e4st/ findt GOtt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Wer sich verlohren hat/ und von sich selbst entbunden/", "tokens": ["Wer", "sich", "ver\u00b7loh\u00b7ren", "hat", "/", "und", "von", "sich", "selbst", "ent\u00b7bun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "VVPP", "VAFIN", "$(", "KON", "APPR", "PRF", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der hat GOtt seinen Trost/ und seinen Heyland funden.", "tokens": ["Der", "hat", "Gott", "sei\u00b7nen", "Trost", "/", "und", "sei\u00b7nen", "Hey\u00b7land", "fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "PPOSAT", "NN", "$(", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}