{"textgrid.poem.62851": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein H\u00e4nfling, den mit hoher Hand", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein H\u00e4nfling, den mit hoher Hand", "tokens": ["Ein", "H\u00e4nf\u00b7ling", ",", "den", "mit", "ho\u00b7her", "Hand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein kleiner gn\u00e4d'ger Herr, der nichts von Gnade wu\u00dfte,", "tokens": ["Ein", "klei\u00b7ner", "gn\u00e4d'\u00b7ger", "Herr", ",", "der", "nichts", "von", "Gna\u00b7de", "wu\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,", "PRELS", "PIS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "An eine Landgaleere band,", "tokens": ["An", "ei\u00b7ne", "Land\u00b7ga\u00b7lee\u00b7re", "band", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo er sein Futter ziehn, sein Wasser sch\u00f6pfen mu\u00dfte,", "tokens": ["Wo", "er", "sein", "Fut\u00b7ter", "ziehn", ",", "sein", "Was\u00b7ser", "sch\u00f6p\u00b7fen", "mu\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVINF", "$,", "PPOSAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sang einst sein Morgenlied im heitern Sonnenschein,", "tokens": ["Sang", "einst", "sein", "Mor\u00b7gen\u00b7lied", "im", "hei\u00b7tern", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.6": {"text": "Und spielte sorgenfrey mit seiner Kette.", "tokens": ["Und", "spiel\u00b7te", "sor\u00b7gen\u00b7frey", "mit", "sei\u00b7ner", "Ket\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Da rief aus ihrem Wochenbette", "tokens": ["Da", "rief", "aus", "ih\u00b7rem", "Wo\u00b7chen\u00b7bet\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ihm eine Schwalbe zu: mir w\u00e4r es H\u00f6llenpein", "tokens": ["Ihm", "ei\u00b7ne", "Schwal\u00b7be", "zu", ":", "mir", "w\u00e4r", "es", "H\u00f6l\u00b7len\u00b7pein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "PTKVZ", "$.", "PPER", "VAFIN", "PPER", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Mit Ketten so beschwert zu seyn;", "tokens": ["Mit", "Ket\u00b7ten", "so", "be\u00b7schwert", "zu", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Du spielst damit und scheinst sie nicht zu f\u00fchlen.", "tokens": ["Du", "spielst", "da\u00b7mit", "und", "scheinst", "sie", "nicht", "zu", "f\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "KON", "VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "O, glaube mir, erwiedert er,", "tokens": ["O", ",", "glau\u00b7be", "mir", ",", "er\u00b7wie\u00b7dert", "er", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Die Kunst sie loszudrehn, ist lange nicht so schwer", "tokens": ["Die", "Kunst", "sie", "los\u00b7zu\u00b7drehn", ",", "ist", "lan\u00b7ge", "nicht", "so", "schwer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "VVINF", "$,", "VAFIN", "ADV", "PTKNEG", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Als die \u2013 damit zu spielen.", "tokens": ["Als", "die", "\u2013", "da\u00b7mit", "zu", "spie\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "$(", "PAV", "PTKZU", "VVINF", "$."], "meter": "+-++-+-", "measure": "unknown.measure.tetra"}, "line.14": {"text": "O Freundin, diese Kunst besitzest du.", "tokens": ["O", "Freun\u00b7din", ",", "die\u00b7se", "Kunst", "be\u00b7sit\u00b7zest", "du", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PDAT", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Mit einem Heldenmuth, den keine Furcht bewegte,", "tokens": ["Mit", "ei\u00b7nem", "Hel\u00b7den\u00b7muth", ",", "den", "kei\u00b7ne", "Furcht", "be\u00b7weg\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Sahst du schon oft der Hand des Schicksals zu,", "tokens": ["Sahst", "du", "schon", "oft", "der", "Hand", "des", "Schick\u00b7sals", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Die sich auf deinen Nacken legte", "tokens": ["Die", "sich", "auf", "dei\u00b7nen", "Na\u00b7cken", "leg\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Und streicheltest die ehrne Hand.", "tokens": ["Und", "strei\u00b7chel\u00b7test", "die", "ehr\u00b7ne", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "O du, Sophiens Vaterland, Germania! wie oft benetzten deine Sch\u00f6nen", "tokens": ["O", "du", ",", "So\u00b7phi\u00b7ens", "Va\u00b7ter\u00b7land", ",", "Ger\u00b7ma\u00b7nia", "!", "wie", "oft", "be\u00b7netz\u00b7ten", "dei\u00b7ne", "Sch\u00f6\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "NE", "NN", "$,", "NE", "$.", "PWAV", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+--+-+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.20": {"text": "Ein Denkmal ihrer Kunst, ein gro\u00dfes Bild, mit Thr\u00e4nen,", "tokens": ["Ein", "Denk\u00b7mal", "ih\u00b7rer", "Kunst", ",", "ein", "gro\u00b7\u00dfes", "Bild", ",", "mit", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Zu dem ihr Geist in sich das Urbild fand.", "tokens": ["Zu", "dem", "ihr", "Geist", "in", "sich", "das", "Ur\u00b7bild", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "NN", "APPR", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Wann wirst du deine Lehrerin belohnen?", "tokens": ["Wann", "wirst", "du", "dei\u00b7ne", "Leh\u00b7re\u00b7rin", "be\u00b7loh\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Sie, der so manchen Kranz Apollo wand?", "tokens": ["Sie", ",", "der", "so", "man\u00b7chen", "Kranz", "A\u00b7pol\u00b7lo", "wand", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ADV", "PIAT", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Wann? ... Doch du hast f\u00fcr T\u00f6chter keine Kronen,", "tokens": ["Wann", "?", "...", "Doch", "du", "hast", "f\u00fcr", "T\u00f6ch\u00b7ter", "kei\u00b7ne", "Kro\u00b7nen", ","], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "$(", "KON", "PPER", "VAFIN", "APPR", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Die gab nur Rom und Griechenland.", "tokens": ["Die", "gab", "nur", "Rom", "und", "Grie\u00b7chen\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "NE", "KON", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}