{"textgrid.poem.34454": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1", "genre": "verse", "period": "N.A.", "pub_year": 1886, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ehmals glaubt ich im Rausch mich flammender Liebe ergeben,", "tokens": ["Eh\u00b7mals", "glaubt", "ich", "im", "Rausch", "mich", "flam\u00b7men\u00b7der", "Lie\u00b7be", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "PPER", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "J\u00fcnglingslieder voll Gluth sang ich in schmachtendem Ton.", "tokens": ["J\u00fcng\u00b7lings\u00b7lie\u00b7der", "voll", "Gluth", "sang", "ich", "in", "schmach\u00b7ten\u00b7dem", "Ton", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Besser nun kenn ich mich selbst und meide den lyrischen Dusel,", "tokens": ["Bes\u00b7ser", "nun", "kenn", "ich", "mich", "selbst", "und", "mei\u00b7de", "den", "ly\u00b7ri\u00b7schen", "Du\u00b7sel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVFIN", "PPER", "PRF", "ADV", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+-+---", "measure": "dactylic.di.plus"}, "line.4": {"text": "und es erhellt mir die Nacht ruhiger heitrer Genuss.", "tokens": ["und", "es", "er\u00b7hellt", "mir", "die", "Nacht", "ru\u00b7hi\u00b7ger", "hei\u00b7trer", "Ge\u00b7nuss", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ART", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}}}}}