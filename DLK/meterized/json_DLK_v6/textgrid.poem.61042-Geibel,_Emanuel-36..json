{"textgrid.poem.61042": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "36.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das ist's, was an der Menschenbrust", "tokens": ["Das", "ist's", ",", "was", "an", "der", "Men\u00b7schen\u00b7brust"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mich oftmals l\u00e4\u00dft verzagen,", "tokens": ["Mich", "oft\u00b7mals", "l\u00e4\u00dft", "ver\u00b7za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df sie den Kummer wie die Lust", "tokens": ["Da\u00df", "sie", "den", "Kum\u00b7mer", "wie", "die", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vergi\u00dft in wenig Tagen.", "tokens": ["Ver\u00b7gi\u00dft", "in", "we\u00b7nig", "Ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und ist der Schmerz, um den es weint,", "tokens": ["Und", "ist", "der", "Schmerz", ",", "um", "den", "es", "weint", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "$,", "KOUI", "ART", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Herzen noch so heilig -", "tokens": ["Dem", "Her\u00b7zen", "noch", "so", "hei\u00b7lig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ADV", "ADJD", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Vogel singt, die Sonne scheint,", "tokens": ["Der", "Vo\u00b7gel", "singt", ",", "die", "Son\u00b7ne", "scheint", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vergessen ist er eilig.", "tokens": ["Ver\u00b7ges\u00b7sen", "ist", "er", "ei\u00b7lig", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und war die Freude noch so s\u00fc\u00df -", "tokens": ["Und", "war", "die", "Freu\u00b7de", "noch", "so", "s\u00fc\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein W\u00f6lkchen kommt gezogen,", "tokens": ["Ein", "W\u00f6lk\u00b7chen", "kommt", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und vom getr\u00e4umten Paradies", "tokens": ["Und", "vom", "ge\u00b7tr\u00e4um\u00b7ten", "Pa\u00b7ra\u00b7dies"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist jede Spur verflogen.", "tokens": ["Ist", "je\u00b7de", "Spur", "ver\u00b7flo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und f\u00fchl' ich das, so wei\u00df ich kaum,", "tokens": ["Und", "f\u00fchl'", "ich", "das", ",", "so", "wei\u00df", "ich", "kaum", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PDS", "$,", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was weckt mir tiefern Schauer,", "tokens": ["Was", "weckt", "mir", "tie\u00b7fern", "Schau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df gar so kurz der Freude Traum,", "tokens": ["Da\u00df", "gar", "so", "kurz", "der", "Freu\u00b7de", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "ADJD", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Oder so kurz die Trauer?", "tokens": ["O\u00b7der", "so", "kurz", "die", "Trau\u00b7er", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}