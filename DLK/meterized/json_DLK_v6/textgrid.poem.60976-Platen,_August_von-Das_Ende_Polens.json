{"textgrid.poem.60976": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Das Ende Polens", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr edlen Schl\u00e4fer unterm Sand, o la\u00dft den Kampf euch nicht gereun.", "tokens": ["Ihr", "ed\u00b7len", "Schl\u00e4\u00b7fer", "un\u00b7term", "Sand", ",", "o", "la\u00dft", "den", "Kampf", "euch", "nicht", "ge\u00b7reun", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPRART", "NN", "$,", "FM", "VVFIN", "ART", "NN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Es wird der sp\u00e4tste Pilger einst auf eure H\u00fcgel Rosen streun.", "tokens": ["Es", "wird", "der", "sp\u00e4ts\u00b7te", "Pil\u00b7ger", "einst", "auf", "eu\u00b7re", "H\u00fc\u00b7gel", "Ro\u00b7sen", "streun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ADV", "APPR", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Und auch der Dichter eilt herbei, von keiner ird'schen Furcht besiegt,", "tokens": ["Und", "auch", "der", "Dich\u00b7ter", "eilt", "her\u00b7bei", ",", "von", "kei\u00b7ner", "ird'\u00b7schen", "Furcht", "be\u00b7siegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "PTKVZ", "$,", "APPR", "PIAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Wo rings um Warschau hingestreckt die gro\u00dfe Hekatombe liegt!", "tokens": ["Wo", "rings", "um", "Warsc\u00b7hau", "hin\u00b7ge\u00b7streckt", "die", "gro\u00b7\u00dfe", "He\u00b7ka\u00b7tom\u00b7be", "liegt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.5": {"text": "Denn kenntlich traun ist euer Grab, und keiner sucht's vergebens auf;", "tokens": ["Denn", "kennt\u00b7lich", "traun", "ist", "eu\u00b7er", "Grab", ",", "und", "kei\u00b7ner", "sucht's", "ver\u00b7ge\u00b7bens", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVINF", "VAFIN", "PPOSAT", "NN", "$,", "KON", "PIS", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.6": {"text": "Es sitzt die hohe Nemesis, ein riesengro\u00dfer Geist darauf!", "tokens": ["Es", "sitzt", "die", "ho\u00b7he", "Ne\u00b7me\u00b7sis", ",", "ein", "rie\u00b7sen\u00b7gro\u00b7\u00dfer", "Geist", "da\u00b7rauf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "PAV", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Als durch die Hauptstadt wohlbewehrt freiwilliger Scharen langer Zug", "tokens": ["Als", "durch", "die", "Haupt\u00b7stadt", "wohl\u00b7be\u00b7wehrt", "frei\u00b7wil\u00b7li\u00b7ger", "Scha\u00b7ren", "lan\u00b7ger", "Zug"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "ADJD", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+--+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Aus Kalisch angelangt, sich wand und Polens wei\u00dfe Fahne trug,", "tokens": ["Aus", "Ka\u00b7lisch", "an\u00b7ge\u00b7langt", ",", "sich", "wand", "und", "Po\u00b7lens", "wei\u00b7\u00dfe", "Fah\u00b7ne", "trug", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "PRF", "VVFIN", "KON", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Da brachte Warschaus reges Volk dem tapfern Schw\u00e4rme, der das Joch", "tokens": ["Da", "brach\u00b7te", "Warsc\u00b7haus", "re\u00b7ges", "Volk", "dem", "tap\u00b7fern", "Schw\u00e4r\u00b7me", ",", "der", "das", "Joch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADJA", "NN", "ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Entzweizubrechen war entflammt, den Kalischern, ein Lebehoch.", "tokens": ["Ent\u00b7zwei\u00b7zu\u00b7bre\u00b7chen", "war", "ent\u00b7flammt", ",", "den", "Ka\u00b7li\u00b7schern", ",", "ein", "Le\u00b7be\u00b7hoch", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$,", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.5": {"text": "Nein, rief ein J\u00fcngling aus dem Zug, und dr\u00fcckte fest ans Schwert die Hand:", "tokens": ["Nein", ",", "rief", "ein", "J\u00fcng\u00b7ling", "aus", "dem", "Zug", ",", "und", "dr\u00fcck\u00b7te", "fest", "ans", "Schwert", "die", "Hand", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,", "KON", "VVFIN", "ADJD", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.6": {"text": "Ein Sterbehoch den Kalischern! es lebe nur das Vaterland!", "tokens": ["Ein", "Ster\u00b7be\u00b7hoch", "den", "Ka\u00b7li\u00b7schern", "!", "es", "le\u00b7be", "nur", "das", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$.", "PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.3": {"line.1": {"text": "Doch weil ein H\u00e4uflein ihr so klein, dem, der so viele h\u00e4lt in Fron,", "tokens": ["Doch", "weil", "ein", "H\u00e4uf\u00b7lein", "ihr", "so", "klein", ",", "dem", ",", "der", "so", "vie\u00b7le", "h\u00e4lt", "in", "Fron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "ADV", "ADJD", "$,", "PRELS", "$,", "PRELS", "ADV", "PIS", "VVFIN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Entgegenstellt, verspottet euch Berlinerwitz und Preu\u00dfenhohn:", "tokens": ["Ent\u00b7ge\u00b7gen\u00b7stellt", ",", "ver\u00b7spot\u00b7tet", "euch", "Ber\u00b7li\u00b7ner\u00b7witz", "und", "Preu\u00b7\u00dfen\u00b7hohn", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "NE", "KON", "NN", "$."], "meter": "-+-+-+--+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Wahnsinnig schilt euch mancher Tor, weil ihr zu sterben wart bereit,", "tokens": ["Wahn\u00b7sin\u00b7nig", "schilt", "euch", "man\u00b7cher", "Tor", ",", "weil", "ihr", "zu", "ster\u00b7ben", "wart", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PIAT", "NN", "$,", "KOUS", "PPER", "PTKZU", "VVINF", "VVFIN", "ADJD", "$,"], "meter": "+--+-+-+-+-+-+-+", "measure": "iambic.octa.plus.invert"}, "line.4": {"text": "Als g\u00e4b es kein erhabner Gut als diese kurze Spanne Zeit!", "tokens": ["Als", "g\u00e4b", "es", "kein", "er\u00b7hab\u00b7ner", "Gut", "als", "die\u00b7se", "kur\u00b7ze", "Span\u00b7ne", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "PIAT", "ADJA", "NN", "KOUS", "PDAT", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.5": {"text": "Wahnsinnig bist du selbst, o Wicht! Wer freudig stirbt und l\u00f6wenbrav,", "tokens": ["Wahn\u00b7sin\u00b7nig", "bist", "du", "selbst", ",", "o", "Wicht", "!", "Wer", "freu\u00b7dig", "stirbt", "und", "l\u00f6\u00b7wen\u00b7brav", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "$,", "FM", "NN", "$.", "PWS", "ADJD", "VVFIN", "KON", "ADJD", "$,"], "meter": "++-+-+-+-+-+-+-+", "measure": "unknown.measure.octa.plus"}, "line.6": {"text": "Verdient den unverwelkbarn Kranz, und du verdienst die Fuchtel, Sklav!", "tokens": ["Ver\u00b7di\u00b7ent", "den", "un\u00b7ver\u00b7welk\u00b7barn", "Kranz", ",", "und", "du", "ver\u00b7dienst", "die", "Fuch\u00b7tel", ",", "Sklav", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "KON", "PPER", "VVFIN", "ART", "NN", "$,", "NE", "$."], "meter": "+---+-+-+-+-+-+-+", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "So hat umsonst verspr\u00fctzt das Blut der M\u00e4nner felsenfest Vertraun,", "tokens": ["So", "hat", "um\u00b7sonst", "ver\u00b7spr\u00fctzt", "das", "Blut", "der", "M\u00e4n\u00b7ner", "fel\u00b7sen\u00b7fest", "Ver\u00b7traun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Umsonst den Brautschmuck dargebracht das Hochgef\u00fchl der besten Fraun.", "tokens": ["Um\u00b7sonst", "den", "Brautsc\u00b7hmuck", "dar\u00b7ge\u00b7bracht", "das", "Hoch\u00b7ge\u00b7f\u00fchl", "der", "bes\u00b7ten", "Fraun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Sie liegen auf den Knien, indes von fern Kanonendonner kracht,", "tokens": ["Sie", "lie\u00b7gen", "auf", "den", "Kni\u00b7en", ",", "in\u00b7des", "von", "fern", "Ka\u00b7no\u00b7nen\u00b7don\u00b7ner", "kracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "Und flehn in Tempeln rings um Sieg f\u00fcr Polens allerletzte Schlacht.", "tokens": ["Und", "flehn", "in", "Tem\u00b7peln", "rings", "um", "Sieg", "f\u00fcr", "Po\u00b7lens", "al\u00b7ler\u00b7letz\u00b7te", "Schlacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ADV", "APPR", "NN", "APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.5": {"text": "Umsonst! Und zweifelnd fragt die Welt, seit euer Blut so reichlich troff,", "tokens": ["Um\u00b7sonst", "!", "Und", "zwei\u00b7felnd", "fragt", "die", "Welt", ",", "seit", "eu\u00b7er", "Blut", "so", "reich\u00b7lich", "troff", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "KON", "ADJD", "VVFIN", "ART", "NN", "$,", "APPR", "PPOSAT", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.6": {"text": "Ob je der Geist besiegen wird den knechtisch plumpen Erdenstoff?", "tokens": ["Ob", "je", "der", "Geist", "be\u00b7sie\u00b7gen", "wird", "den", "knech\u00b7tisch", "plum\u00b7pen", "Er\u00b7den\u00b7stoff", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVPP", "VAFIN", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.5": {"line.1": {"text": "Ukasenton der Z\u00e4rtlichkeit, wie christlich doch, wie fromm du sprichst!", "tokens": ["U\u00b7ka\u00b7sen\u00b7ton", "der", "Z\u00e4rt\u00b7lich\u00b7keit", ",", "wie", "christ\u00b7lich", "doch", ",", "wie", "fromm", "du", "sprichst", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "PWAV", "ADJD", "ADV", "$,", "PWAV", "ADJD", "PPER", "VVFIN", "$."], "meter": "+--+-+-+-+-+-+-+", "measure": "iambic.octa.plus.invert"}, "line.2": {"text": "Der Gute liebt sein Volk so sehr, da\u00df er's ermordet v\u00e4terlichst!", "tokens": ["Der", "Gu\u00b7te", "liebt", "sein", "Volk", "so", "sehr", ",", "da\u00df", "er's", "er\u00b7mor\u00b7det", "v\u00e4\u00b7ter\u00b7lichst", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "ADV", "ADV", "$,", "KOUS", "PIS", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Schamlos wie eine Metze dringt die Despotie sich auf und bl\u00e4ht", "tokens": ["Scham\u00b7los", "wie", "ei\u00b7ne", "Met\u00b7ze", "dringt", "die", "Des\u00b7po\u00b7tie", "sich", "auf", "und", "bl\u00e4ht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "ART", "NN", "VVFIN", "ART", "NN", "PRF", "PTKVZ", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Entgegen sich dem Gegenstand, der sie verachtet und verschm\u00e4ht.", "tokens": ["Ent\u00b7ge\u00b7gen", "sich", "dem", "Ge\u00b7gen\u00b7stand", ",", "der", "sie", "ver\u00b7ach\u00b7tet", "und", "ver\u00b7schm\u00e4ht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.5": {"text": "Vergebens ruft ein ganzes Volk: Wir wollen dich ja nicht, Tyrann!", "tokens": ["Ver\u00b7ge\u00b7bens", "ruft", "ein", "gan\u00b7zes", "Volk", ":", "Wir", "wol\u00b7len", "dich", "ja", "nicht", ",", "Ty\u00b7rann", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$.", "PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "$,", "NE", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.6": {"text": "Das ganze Volk, vernichtet wird's, auf da\u00df er's unterjochen kann.", "tokens": ["Das", "gan\u00b7ze", "Volk", ",", "ver\u00b7nich\u00b7tet", "wird's", ",", "auf", "da\u00df", "er's", "un\u00b7ter\u00b7jo\u00b7chen", "kann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVPP", "VAFIN", "$,", "APPR", "KOUS", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.6": {"line.1": {"text": "Wie r\u00e4cht sich nun der Autokrat, der ganz von Rachbegierde brennt?", "tokens": ["Wie", "r\u00e4cht", "sich", "nun", "der", "Au\u00b7to\u00b7krat", ",", "der", "ganz", "von", "Rach\u00b7be\u00b7gier\u00b7de", "brennt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PRF", "ADV", "ART", "NN", "$,", "PRELS", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Er macht zu Russen euch! Es ist die gr\u00f6\u00dfte Strafe, die er kennt.", "tokens": ["Er", "macht", "zu", "Rus\u00b7sen", "euch", "!", "Es", "ist", "die", "gr\u00f6\u00df\u00b7te", "Stra\u00b7fe", ",", "die", "er", "kennt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "PPER", "$.", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "F\u00fcrwahr, die gr\u00f6\u00dfte! Selig ihr, die solches Lohns ihr nicht bed\u00fcrft,", "tokens": ["F\u00fcr\u00b7wahr", ",", "die", "gr\u00f6\u00df\u00b7te", "!", "Se\u00b7lig", "ihr", ",", "die", "sol\u00b7ches", "Lohns", "ihr", "nicht", "be\u00b7d\u00fcrft", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "$.", "NE", "PPER", "$,", "PRELS", "PIAT", "NN", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Die aus der Ehre Becher ihr den Tod in vollen Z\u00fcgen schl\u00fcrft!", "tokens": ["Die", "aus", "der", "Eh\u00b7re", "Be\u00b7cher", "ihr", "den",