{"textgrid.poem.37608": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Bassewitz", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bassewitz auf seinem Rappen", "tokens": ["Bas\u00b7se\u00b7witz", "auf", "sei\u00b7nem", "Rap\u00b7pen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch die Weser ist geschwommen,", "tokens": ["Durch", "die", "We\u00b7ser", "ist", "ge\u00b7schwom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Um recht schnell zu Streit zu kommen;", "tokens": ["Um", "recht", "schnell", "zu", "Streit", "zu", "kom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ADJD", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fchrt mit Glanz sein altes Wappen.", "tokens": ["F\u00fchrt", "mit", "Glanz", "sein", "al\u00b7tes", "Wap\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00dcber Hecken, \u00fcber Z\u00e4une", "tokens": ["\u00dc\u00b7ber", "He\u00b7cken", ",", "\u00fc\u00b7ber", "Z\u00e4u\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Setzt er spielend in dem Sonnenscheine", "tokens": ["Setzt", "er", "spie\u00b7lend", "in", "dem", "Son\u00b7nen\u00b7schei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Als in Blut die Sonn' gesunken", "tokens": ["Als", "in", "Blut", "die", "Sonn'", "ge\u00b7sun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NN", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und sein Volk von Jena fl\u00fcchtet,", "tokens": ["Und", "sein", "Volk", "von", "Je\u00b7na", "fl\u00fcch\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "NE", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Er sich treu nach allem richtet,", "tokens": ["Er", "sich", "treu", "nach", "al\u00b7lem", "rich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "ADJD", "APPR", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sein Muth ist nicht gesunken.", "tokens": ["Und", "sein", "Muth", "ist", "nicht", "ge\u00b7sun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Trauern kann er nicht, nur lachen,", "tokens": ["Trau\u00b7ern", "kann", "er", "nicht", ",", "nur", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PTKNEG", "$,", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wei\u00df noch lustig sich mit Leid zu machen.", "tokens": ["Wei\u00df", "noch", "lus\u00b7tig", "sich", "mit", "Leid", "zu", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "PRF", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Als zum Meere sie gedr\u00e4nget,", "tokens": ["Als", "zum", "Mee\u00b7re", "sie", "ge\u00b7dr\u00e4n\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Viele das Gewehr schon strecken,", "tokens": ["Vie\u00b7le", "das", "Ge\u00b7wehr", "schon", "stre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mag er sich nicht mehr verstecken", "tokens": ["Mag", "er", "sich", "nicht", "mehr", "ver\u00b7ste\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PRF", "PTKNEG", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hinter Demuth, die ihn enget,", "tokens": ["Hin\u00b7ter", "De\u00b7muth", ",", "die", "ihn", "en\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Auf die Feinde loszuschlagen", "tokens": ["Auf", "die", "Fein\u00b7de", "los\u00b7zu\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kann er Einmal sich nicht mehr versagen.", "tokens": ["Kann", "er", "Ein\u00b7mal", "sich", "nicht", "mehr", "ver\u00b7sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PRF", "PTKNEG", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Blau und roth sein Degen flimmert", "tokens": ["Blau", "und", "roth", "sein", "De\u00b7gen", "flim\u00b7mert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und auf die geschlossenen Reihen", "tokens": ["Und", "auf", "die", "ge\u00b7schlos\u00b7se\u00b7nen", "Rei\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Hauet er allein mit Schreien,", "tokens": ["Hau\u00b7et", "er", "al\u00b7lein", "mit", "Schrei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Einen Adler ganz zertr\u00fcmmert;", "tokens": ["Ei\u00b7nen", "Ad\u00b7ler", "ganz", "zer\u00b7tr\u00fcm\u00b7mert", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch von vielen b\u00f6sen Hunden", "tokens": ["Doch", "von", "vie\u00b7len", "b\u00f6\u00b7sen", "Hun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird der L\u00f6we selber \u00fcberwunden.", "tokens": ["Wird", "der", "L\u00f6\u00b7we", "sel\u00b7ber", "\u00fc\u00b7berw\u00b7un\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NE", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Gegen die verschlossne Th\u00fcre", "tokens": ["Ge\u00b7gen", "die", "ver\u00b7schloss\u00b7ne", "Th\u00fc\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vom Begr\u00e4bni\u00df all der Seinen", "tokens": ["Vom", "Be\u00b7gr\u00e4b\u00b7ni\u00df", "all", "der", "Sei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PIAT", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie ihn dr\u00e4ngen und nicht meinen,", "tokens": ["Sie", "ihn", "dr\u00e4n\u00b7gen", "und", "nicht", "mei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "VVFIN", "KON", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df es nur sein Feuer sch\u00fcre,", "tokens": ["Da\u00df", "es", "nur", "sein", "Feu\u00b7er", "sch\u00fc\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn sie ihm das Leben schenken;", "tokens": ["Wenn", "sie", "ihm", "das", "Le\u00b7ben", "schen\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nicht von seinem Schimpfen kann's ihn lenken.", "tokens": ["Nicht", "von", "sei\u00b7nem", "Schimp\u00b7fen", "kann's", "ihn", "len\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Seine H\u00e4nde, festgehalten,", "tokens": ["Sei\u00b7ne", "H\u00e4n\u00b7de", ",", "fest\u00b7ge\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00f6nnen sich nicht los mehr rei\u00dfen,", "tokens": ["K\u00f6n\u00b7nen", "sich", "nicht", "los", "mehr", "rei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "PTKNEG", "ADJD", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch sein Mund kann sie noch bei\u00dfen,", "tokens": ["Doch", "sein", "Mund", "kann", "sie", "noch", "bei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Selbst den Ehrenkranz erhalten.", "tokens": ["Selbst", "den", "Eh\u00b7ren\u00b7kranz", "er\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und sie spie\u00dfen ihn mit Degen", "tokens": ["Und", "sie", "spie\u00b7\u00dfen", "ihn", "mit", "De\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "An die Th\u00fcre, seines Schimpfens wegen.", "tokens": ["An", "die", "Th\u00fc\u00b7re", ",", "sei\u00b7nes", "Schimp\u00b7fens", "we\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PPOSAT", "NN", "APPR", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Wie die Fledermaus genagelt,", "tokens": ["Wie", "die", "Fle\u00b7der\u00b7maus", "ge\u00b7na\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie der Falke an den Thoren,", "tokens": ["Wie", "der", "Fal\u00b7ke", "an", "den", "Tho\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lebend giebt er nichts verloren,", "tokens": ["Le\u00b7bend", "giebt", "er", "nichts", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PIS", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und er bei\u00dft noch in den Degen,", "tokens": ["Und", "er", "bei\u00dft", "noch", "in", "den", "De\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Der den Lebensfaden schneidet, \u2013", "tokens": ["Der", "den", "Le\u00b7bens\u00b7fa\u00b7den", "schnei\u00b7det", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "So den Tod er unbezwungen leidet. \u2013", "tokens": ["So", "den", "Tod", "er", "un\u00b7be\u00b7zwun\u00b7gen", "lei\u00b7det", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "NN", "PPER", "ADJD", "VVFIN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}