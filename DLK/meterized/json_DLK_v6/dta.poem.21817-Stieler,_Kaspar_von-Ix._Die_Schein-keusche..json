{"dta.poem.21817": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "Ix.  \n  Die Schein-keusche.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Gaminde sieht so ernstlich au\u00df/\nsie kan f\u00fcr allen Junggesellen\nsich so verzumfen stellen/", "tokens": ["Ga\u00b7min\u00b7de", "sieht", "so", "ernst\u00b7lich", "au\u00df", "/", "sie", "kan", "f\u00fcr", "al\u00b7len", "Jung\u00b7ge\u00b7sel\u00b7len", "sich", "so", "ver\u00b7zum\u00b7fen", "stel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ADJD", "PTKVZ", "$(", "PPER", "VMFIN", "APPR", "PIAT", "NN", "PRF", "ADV", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-+-+--+-+-+-", "measure": "iambic.octa.plus"}, "line.2": {"text": "wenn sie ihr sprechen zu:", "tokens": ["wenn", "sie", "ihr", "spre\u00b7chen", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "da\u00df man sie vor die keusche Dirne/", "tokens": ["da\u00df", "man", "sie", "vor", "die", "keu\u00b7sche", "Dir\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "die au\u00df Diespiters Gehirne", "tokens": ["die", "au\u00df", "Dies\u00b7pi\u00b7ters", "Ge\u00b7hir\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "gebohren/ halten solt\u2019", "tokens": ["ge\u00b7boh\u00b7ren", "/", "hal\u00b7ten", "solt'"], "token_info": ["word", "punct", "word", "word"], "pos": ["VVPP", "$(", "VVINF", "VMFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ey! keusche Pallas du/", "tokens": ["Ey", "!", "keu\u00b7sche", "Pal\u00b7las", "du", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADJA", "NN", "NE", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "wei\u00df auff den Schild/ ich wei\u00df/ er macht die", "tokens": ["wei\u00df", "auff", "den", "Schild", "/", "ich", "wei\u00df", "/", "er", "macht", "die"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "$(", "PPER", "VVFIN", "$(", "PPER", "VVFIN", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "macht dich und sie zu Narren.", "tokens": ["macht", "dich", "und", "sie", "zu", "Nar\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Gaminde/ Stolze/ meinstu wol", "tokens": ["Ga\u00b7min\u00b7de", "/", "Stol\u00b7ze", "/", "meins\u00b7tu", "wol"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$(", "ADJA", "$(", "ADV", "ADV"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "man werd\u2019 auff deine Keuschheit bauen?", "tokens": ["man", "werd'", "auff", "dei\u00b7ne", "Keuschheit", "bau\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "der geile Schmukk der Pfauen", "tokens": ["der", "gei\u00b7le", "Schmukk", "der", "Pfau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "verr\u00e4ht dich wer du bist.", "tokens": ["ver\u00b7r\u00e4ht", "dich", "wer", "du", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PWS", "PPER", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du bl\u00f6ssest die begriffnen Br\u00fcste/", "tokens": ["Du", "bl\u00f6s\u00b7sest", "die", "be\u00b7griff\u00b7nen", "Br\u00fcs\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "die keusche bergen solche L\u00fcste", "tokens": ["die", "keu\u00b7sche", "ber\u00b7gen", "sol\u00b7che", "L\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "und gehn beschn\u00fcrt herein.", "tokens": ["und", "gehn", "be\u00b7schn\u00fcrt", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Wo Keuschheit in dir ist", "tokens": ["Wo", "Keuschheit", "in", "dir", "ist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "APPR", "PPER", "VAFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Nein/ dir gef\u00e4llt das F\u00fchlen.", "tokens": ["Nein", "/", "dir", "ge\u00b7f\u00e4llt", "das", "F\u00fch\u00b7len", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Gaminde liegt zum Fenster au\u00df", "tokens": ["Ga\u00b7min\u00b7de", "liegt", "zum", "Fens\u00b7ter", "au\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPRART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und spottet aller Jung", "tokens": ["und", "spot\u00b7tet", "al\u00b7ler", "Jung"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "die sich verliebet stellen.", "tokens": ["die", "sich", "ver\u00b7lie\u00b7bet", "stel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRELS", "PRF", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schaut/ Br\u00fcder/ in die H\u00f6h/", "tokens": ["Schaut", "/", "Br\u00fc\u00b7der", "/", "in", "die", "H\u00f6h", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "NN", "$(", "APPR", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "im rotem Feuer angegangen/", "tokens": ["im", "ro\u00b7tem", "Feu\u00b7er", "an\u00b7ge\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "beschaut sie/ forschet nach/", "tokens": ["be\u00b7schaut", "sie", "/", "for\u00b7schet", "nach", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "VVFIN", "APPR", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "ob Pallas auch so seh\u2019", "tokens": ["ob", "Pal\u00b7las", "auch", "so", "seh'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "(ke:", "tokens": ["(", "ke", ":"], "token_info": ["punct", "word", "punct"], "pos": ["$(", "VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.9": {"text": "ob ihr beernst Gesicht au\u00df roter Menje blin-", "tokens": ["ob", "ihr", "be\u00b7ernst", "Ge\u00b7sicht", "au\u00df", "ro\u00b7ter", "Men\u00b7je", "blin"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "NN", "APPR", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "das Aas ist roht von Schminke.", "tokens": ["das", "Aas", "ist", "roht", "von", "Schmin\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}