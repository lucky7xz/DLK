{"textgrid.poem.25705": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: M\u00e4dchen! in der kleinen stillen H\u00fctte,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "M\u00e4dchen! in der kleinen stillen H\u00fctte,", "tokens": ["M\u00e4d\u00b7chen", "!", "in", "der", "klei\u00b7nen", "stil\u00b7len", "H\u00fct\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wo die Unschuld und die alte Sitte", "tokens": ["Wo", "die", "Un\u00b7schuld", "und", "die", "al\u00b7te", "Sit\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ganz allein dich gro\u00df gezogen hat;", "tokens": ["Ganz", "al\u00b7lein", "dich", "gro\u00df", "ge\u00b7zo\u00b7gen", "hat", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADJD", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Oder in dem l\u00e4ndlich wilden Garten,", "tokens": ["O\u00b7der", "in", "dem", "l\u00e4nd\u00b7lich", "wil\u00b7den", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Wo die Rosen heute dich erwarten,", "tokens": ["Wo", "die", "Ro\u00b7sen", "heu\u00b7te", "dich", "er\u00b7war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Sieh! da sucht, aus einer fremden Stadt,", "tokens": ["Sieh", "!", "da", "sucht", ",", "aus", "ei\u00b7ner", "frem\u00b7den", "Stadt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VVFIN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Dich des Unbekannten Blatt.", "tokens": ["Dich", "des", "Un\u00b7be\u00b7kann\u00b7ten", "Blatt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Der es schicket, hat dich nie gesehen,", "tokens": ["Der", "es", "schi\u00b7cket", ",", "hat", "dich", "nie", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Wei\u00df nicht, wie viel Reitze dich erh\u00f6hen?", "tokens": ["Wei\u00df", "nicht", ",", "wie", "viel", "Reit\u00b7ze", "dich", "er\u00b7h\u00f6\u00b7hen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "PWAV", "PIAT", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Deinen Namen selber wei\u00df er nicht.", "tokens": ["Dei\u00b7nen", "Na\u00b7men", "sel\u00b7ber", "wei\u00df", "er", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Wie viel andre M\u00e4dchen dich beneiden?", "tokens": ["Wie", "viel", "and\u00b7re", "M\u00e4d\u00b7chen", "dich", "be\u00b7nei\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Wie viel J\u00fcngling' heut' sich besser kleiden,", "tokens": ["Wie", "viel", "J\u00fcng\u00b7ling'", "heut'", "sich", "bes\u00b7ser", "klei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVFIN", "PRF", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Weil sich jeder einst dein Ja! verspricht?", "tokens": ["Weil", "sich", "je\u00b7der", "einst", "dein", "Ja", "!", "ver\u00b7spricht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PRF", "PIS", "ADV", "PPOSAT", "NN", "$.", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.14": {"text": "Nach dem allen fragt' er nicht.", "tokens": ["Nach", "dem", "al\u00b7len", "fragt'", "er", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIS", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Nur da\u00df dir ein Herz im Busen schl\u00e4get,", "tokens": ["Nur", "da\u00df", "dir", "ein", "Herz", "im", "Bu\u00b7sen", "schl\u00e4\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Sch\u00f6ner, als es oft ein Fr\u00e4ulein tr\u00e4get", "tokens": ["Sch\u00f6\u00b7ner", ",", "als", "es", "oft", "ein", "Fr\u00e4u\u00b7lein", "tr\u00e4\u00b7get"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "Unter einem Orden auf der Brust;", "tokens": ["Un\u00b7ter", "ei\u00b7nem", "Or\u00b7den", "auf", "der", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.18": {"text": "Da\u00df du zwar, an Kunst in deinen Mienen,", "tokens": ["Da\u00df", "du", "zwar", ",", "an", "Kunst", "in", "dei\u00b7nen", "Mie\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "APPR", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "Hundert Damen, aber unter ihnen", "tokens": ["Hun\u00b7dert", "Da\u00b7men", ",", "a\u00b7ber", "un\u00b7ter", "ih\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["CARD", "NN", "$,", "ADV", "APPR", "PPER"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "Keiner in der Unschuld weichen mu\u00dft,", "tokens": ["Kei\u00b7ner", "in", "der", "Un\u00b7schuld", "wei\u00b7chen", "mu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.21": {"text": "Dieses hat er nur gewu\u00dft.", "tokens": ["Die\u00b7ses", "hat", "er", "nur", "ge\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Auf! verla\u00df, du taubenfrommes M\u00e4dchen,", "tokens": ["Auf", "!", "ver\u00b7la\u00df", ",", "du", "tau\u00b7ben\u00b7from\u00b7mes", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "VVFIN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.23": {"text": "Heut' einmal dein bunt gemaltes R\u00e4dchen,", "tokens": ["Heut'", "ein\u00b7mal", "dein", "bunt", "ge\u00b7mal\u00b7tes", "R\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "ADJD", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.24": {"text": "Deine L\u00e4mmer weid' ein andrer heut.", "tokens": ["Dei\u00b7ne", "L\u00e4m\u00b7mer", "weid'", "ein", "an\u00b7drer", "heut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "ADJA", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.25": {"text": "Sieh, das Dorf pocht fr\u00fch an deine H\u00fctte,", "tokens": ["Sieh", ",", "das", "Dorf", "pocht", "fr\u00fch", "an", "dei\u00b7ne", "H\u00fct\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.26": {"text": "Mit verstecktem Kranz' in seiner Mitte,", "tokens": ["Mit", "ver\u00b7steck\u00b7tem", "Kranz'", "in", "sei\u00b7ner", "Mit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.27": {"text": "Dich mit ungest\u00fcmer Fr\u00f6hlichkeit", "tokens": ["Dich", "mit", "un\u00b7ge\u00b7st\u00fc\u00b7mer", "Fr\u00f6h\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.28": {"text": "Zu begleiten schon bereit.", "tokens": ["Zu", "be\u00b7glei\u00b7ten", "schon", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Sieh, du Traute, um dich her, wie heute", "tokens": ["Sieh", ",", "du", "Trau\u00b7te", ",", "um", "dich", "her", ",", "wie", "heu\u00b7te"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "PPER", "NN", "$,", "KOUI", "PRF", "PTKVZ", "$,", "PWAV", "ADV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.30": {"text": "Jeder J\u00fcngling seinen Vordermann, zur Seite,", "tokens": ["Je\u00b7der", "J\u00fcng\u00b7ling", "sei\u00b7nen", "Vor\u00b7der\u00b7mann", ",", "zur", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPOSAT", "NN", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.31": {"text": "Um von dir gesehn zu werden, schiebt;", "tokens": ["Um", "von", "dir", "ge\u00b7sehn", "zu", "wer\u00b7den", ",", "schiebt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUI", "APPR", "PPER", "VVPP", "PTKZU", "VAINF", "$,", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.32": {"text": "Deine Schulgespielen nach dir blicken,", "tokens": ["Dei\u00b7ne", "Schul\u00b7ge\u00b7spie\u00b7len", "nach", "dir", "bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.33": {"text": "Freundlich mit den K\u00f6pfen nach dir nicken,", "tokens": ["Freund\u00b7lich", "mit", "den", "K\u00f6p\u00b7fen", "nach", "dir", "ni\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.34": {"text": "Jede stolz es zu verstehen gibt,", "tokens": ["Je\u00b7de", "stolz", "es", "zu", "ver\u00b7ste\u00b7hen", "gibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "PPER", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.35": {"text": "Sie auch sey von dir geliebt.", "tokens": ["Sie", "auch", "sey", "von", "dir", "ge\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.36": {"text": "Zwar es wartet dein kein goldner Wagen,", "tokens": ["Zwar", "es", "war\u00b7tet", "dein", "kein", "gold\u00b7ner", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PPOSAT", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.37": {"text": "Dich zu deinem Kr\u00f6nungsfest' zu tragen,", "tokens": ["Dich", "zu", "dei\u00b7nem", "Kr\u00f6\u00b7nungs\u00b7fest'", "zu", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.38": {"text": "Aber auf den Schultern tr\u00e4gt man dich.", "tokens": ["A\u00b7ber", "auf", "den", "Schul\u00b7tern", "tr\u00e4gt", "man", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PIS", "PRF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.39": {"text": "Keiner f\u00fchlt, da\u00df sich der Platz verenget,", "tokens": ["Kei\u00b7ner", "f\u00fchlt", ",", "da\u00df", "sich", "der", "Platz", "ver\u00b7en\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.40": {"text": "Keiner merkt, da\u00df ihn ein andrer dr\u00e4nget,", "tokens": ["Kei\u00b7ner", "merkt", ",", "da\u00df", "ihn", "ein", "an\u00b7drer", "dr\u00e4n\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "PPER", "ART", "ADJA", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.41": {"text": "Denn ein jeder selber dr\u00e4nget sich", "tokens": ["Denn", "ein", "je\u00b7der", "sel\u00b7ber", "dr\u00e4n\u00b7get", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "PIS", "ADV", "VVFIN", "PRF"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.42": {"text": "Immer mehr heran an dich.", "tokens": ["Im\u00b7mer", "mehr", "he\u00b7ran", "an", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKVZ", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.43": {"text": "Geh hinein zu Gottes Heiligthume,", "tokens": ["Geh", "hin\u00b7ein", "zu", "Got\u00b7tes", "Hei\u00b7lig\u00b7thu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.44": {"text": "Tritt so sch\u00f6n err\u00f6thend, wie die Blume,", "tokens": ["Tritt", "so", "sch\u00f6n", "er\u00b7r\u00f6\u00b7thend", ",", "wie", "die", "Blu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "VVPP", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.45": {"text": "Die dich kr\u00f6nen soll, an den Altar.", "tokens": ["Die", "dich", "kr\u00f6\u00b7nen", "soll", ",", "an", "den", "Al\u00b7tar", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "VMFIN", "$,", "APPR", "ART", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.46": {"text": "Engel, die begierig um dich stehen,", "tokens": ["En\u00b7gel", ",", "die", "be\u00b7gie\u00b7rig", "um", "dich", "ste\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.47": {"text": "Und dein Herz in deiner Miene sehen,", "tokens": ["Und", "dein", "Herz", "in", "dei\u00b7ner", "Mie\u00b7ne", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.48": {"text": "Preisen gl\u00fccklich ", "tokens": ["Prei\u00b7sen", "gl\u00fcck\u00b7lich"], "token_info": ["word", "word"], "pos": ["NN", "ADJD"], "meter": "+-+-", "measure": "trochaic.di"}, "line.49": {"text": "Der bisher dein Schutzgeist war.", "tokens": ["Der", "bis\u00b7her", "dein", "Schutz\u00b7geist", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.50": {"text": "O wie sch\u00f6n bist du in diesem Kranze!", "tokens": ["O", "wie", "sch\u00f6n", "bist", "du", "in", "die\u00b7sem", "Kran\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "VAFIN", "PPER", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.51": {"text": "Eine Heilige im Sternenglanze", "tokens": ["Ei\u00b7ne", "Hei\u00b7li\u00b7ge", "im", "Ster\u00b7nen\u00b7glan\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.52": {"text": "W\u00fcrde selbst dich mit Vergn\u00fcgen sehn.", "tokens": ["W\u00fcr\u00b7de", "selbst", "dich", "mit", "Ver\u00b7gn\u00fc\u00b7gen", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PRF", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.53": {"text": "Sieh! aus eines guten J\u00fcnglings Blicke", "tokens": ["Sieh", "!", "aus", "ei\u00b7nes", "gu\u00b7ten", "J\u00fcng\u00b7lings", "Bli\u00b7cke"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.54": {"text": "Spricht der Wunsch: \u00bbAch! w\u00fcrde mir's vom Gl\u00fccke,", "tokens": ["Spricht", "der", "Wunsch", ":", "\u00bb", "Ach", "!", "w\u00fcr\u00b7de", "mir's", "vom", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "$(", "ITJ", "$.", "VAFIN", "NE", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.55": {"text": "Neben ihr am Traualtar' zu stehn!\u00ab", "tokens": ["Ne\u00b7ben", "ihr", "am", "Trau\u00b7al\u00b7ta\u00b7r'", "zu", "stehn", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "APPRART", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.56": {"text": "Sage, soll es bald geschehn?", "tokens": ["Sa\u00b7ge", ",", "soll", "es", "bald", "ge\u00b7schehn", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.57": {"text": "Gib ihm deine Hand, und ziehet Kinder,", "tokens": ["Gib", "ihm", "dei\u00b7ne", "Hand", ",", "und", "zie\u00b7het", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "$,", "KON", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.58": {"text": "Die der gute Stifter einst nicht minder,", "tokens": ["Die", "der", "gu\u00b7te", "Stif\u00b7ter", "einst", "nicht", "min\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "ADV", "PTKNEG", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.59": {"text": "Als die Mutter heute, kr\u00f6nen l\u00e4\u00dft.", "tokens": ["Als", "die", "Mut\u00b7ter", "heu\u00b7te", ",", "kr\u00f6\u00b7nen", "l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "$,", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.60": {"text": "Sag' dem Stifter: Der die\u00df Lied gesungen,", "tokens": ["Sag'", "dem", "Stif\u00b7ter", ":", "Der", "die\u00df", "Lied", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "ART", "PDS", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.61": {"text": "Kenn' ihn nicht; sey aber tief durchdrungen,", "tokens": ["Kenn'", "ihn", "nicht", ";", "sey", "a\u00b7ber", "tief", "durch\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "$.", "VAFIN", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.62": {"text": "Von des Mannes Tugend, der ein Fest", "tokens": ["Von", "des", "Man\u00b7nes", "Tu\u00b7gend", ",", "der", "ein", "Fest"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.63": {"text": "F\u00fcr die Unschuld feiern l\u00e4\u00dft.", "tokens": ["F\u00fcr", "die", "Un\u00b7schuld", "fei\u00b7ern", "l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}