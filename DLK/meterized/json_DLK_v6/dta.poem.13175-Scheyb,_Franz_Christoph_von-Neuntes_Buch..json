{"dta.poem.13175": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Neuntes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der F\u00fcrst der Ehgemahl (sein Ansehn stellte dar,", "tokens": ["Der", "F\u00fcrst", "der", "Eh\u00b7ge\u00b7mahl", "(", "sein", "An\u00b7sehn", "stell\u00b7te", "dar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$(", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was in dem Tugend-Rath von ihm erw\u00e4hnet war)", "tokens": ["Was", "in", "dem", "Tu\u00b7gen\u00b7dRath", "von", "ihm", "er\u00b7w\u00e4h\u00b7net", "war", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "APPR", "PPER", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "435Der Ehgemahl, sag' ich, kam an der lincken Seite,", "tokens": ["Eh\u00b7ge\u00b7mahl", ",", "sag'", "ich", ",", "kam", "an", "der", "lin\u00b7cken", "Sei\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Und war dem ersten Sohn zum F\u00fchrer und Geleite.", "tokens": ["Und", "war", "dem", "ers\u00b7ten", "Sohn", "zum", "F\u00fch\u00b7rer", "und", "Ge\u00b7lei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "APPRART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er hielt ihn an der Hand. Da glimmte Lieb und Lust.", "tokens": ["Er", "hielt", "ihn", "an", "der", "Hand", ".", "Da", "glimm\u00b7te", "Lieb", "und", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "ADV", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Thalia sagte mir: ihr wallen in der Brust", "tokens": ["Tha\u00b7lia", "sag\u00b7te", "mir", ":", "ihr", "wal\u00b7len", "in", "der", "Brust"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$.", "PPER", "VVINF", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.7": {"text": "Die Vorbedeutungen von grossen Helden-Thaten,", "tokens": ["Die", "Vor\u00b7be\u00b7deu\u00b7tun\u00b7gen", "von", "gros\u00b7sen", "Hel\u00b7den\u00b7Tha\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "440Von Wohlfart, Heil und Gl\u00fcck, von Fried und Ruh der Staaten,", "tokens": ["Wohl\u00b7fart", ",", "Heil", "und", "Gl\u00fcck", ",", "von", "Fried", "und", "Ruh", "der", "Staa\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,", "APPR", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.9": {"text": "Wann sie das Paar betracht\u2019. O drey Mahl fester Thron!", "tokens": ["Wann", "sie", "das", "Paar", "be\u00b7tracht'", ".", "O", "drey", "Mahl", "fes\u00b7ter", "Thron", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$.", "NE", "CARD", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Auf dem die Mutter sizt, der Vatter, dieser Sohn!", "tokens": ["Auf", "dem", "die", "Mut\u00b7ter", "sizt", ",", "der", "Vat\u00b7ter", ",", "die\u00b7ser", "Sohn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVFIN", "$,", "ART", "NN", "$,", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}