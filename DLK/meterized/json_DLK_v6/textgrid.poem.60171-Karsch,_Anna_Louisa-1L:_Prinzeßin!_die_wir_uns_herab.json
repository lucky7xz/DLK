{"textgrid.poem.60171": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Prinze\u00dfin! die wir uns herab", "genre": "verse", "period": "N.A.", "pub_year": 1762, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Prinze\u00dfin! die wir uns herab", "tokens": ["Prin\u00b7ze\u00b7\u00dfin", "!", "die", "wir", "uns", "her\u00b7ab"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "PRELS", "PPER", "PRF", "ADV"], "meter": "+----+-+", "measure": "dactylic.init"}, "line.2": {"text": "Von dem Olymp zur Erde bathen,", "tokens": ["Von", "dem", "O\u00b7lymp", "zur", "Er\u00b7de", "ba\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Du Kind, das uns die Liebe gab,", "tokens": ["Du", "Kind", ",", "das", "uns", "die", "Lie\u00b7be", "gab", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "La\u00df dir erz\u00e4hlen Heinrichs Thaten,", "tokens": ["La\u00df", "dir", "er\u00b7z\u00e4h\u00b7len", "Hein\u00b7richs", "Tha\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dein Onkel ist Er, und ein Held", "tokens": ["Dein", "On\u00b7kel", "ist", "Er", ",", "und", "ein", "Held"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Legionen fortgetrieben,", "tokens": ["Der", "Le\u00b7gi\u00b7o\u00b7nen", "fort\u00b7ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und mitten auf dem Siegesfeld,", "tokens": ["Und", "mit\u00b7ten", "auf", "dem", "Sie\u00b7ges\u00b7feld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An deinen Vater froh geschrieben;", "tokens": ["An", "dei\u00b7nen", "Va\u00b7ter", "froh", "ge\u00b7schrie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie sch\u00fcchtern \u00fcber Berg und Thal", "tokens": ["Wie", "sch\u00fcch\u00b7tern", "\u00fc\u00b7ber", "Berg", "und", "Thal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Feindes Haufen schleunig flogen;", "tokens": ["Des", "Fein\u00b7des", "Hau\u00b7fen", "schleu\u00b7nig", "flo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So ungez\u00e4hlt in ihrer Zahl,", "tokens": ["So", "un\u00b7ge\u00b7z\u00e4hlt", "in", "ih\u00b7rer", "Zahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als kaum des wilden Meeres Wogen.", "tokens": ["Als", "kaum", "des", "wil\u00b7den", "Mee\u00b7res", "Wo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ein kleiner Haufen Krieger stritt,", "tokens": ["Ein", "klei\u00b7ner", "Hau\u00b7fen", "Krie\u00b7ger", "stritt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nur da wo Heinrich hin und wieder", "tokens": ["Nur", "da", "wo", "Hein\u00b7rich", "hin", "und", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PWAV", "NE", "PTKVZ", "KON", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Auf Seinem Streiter Rosse ritt,", "tokens": ["Auf", "Sei\u00b7nem", "Strei\u00b7ter", "Ros\u00b7se", "ritt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wie ein L\u00f6we warf darnieder,", "tokens": ["Und", "wie", "ein", "L\u00f6\u00b7we", "warf", "dar\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NE", "VVFIN", "PAV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Was noch den Nacken umgewandt,", "tokens": ["Was", "noch", "den", "Na\u00b7cken", "um\u00b7ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Troz auf der Stirne noch getragen", "tokens": ["Troz", "auf", "der", "Stir\u00b7ne", "noch", "ge\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und Gegenwehr in Feindes Hand,", "tokens": ["Und", "Ge\u00b7gen\u00b7wehr", "in", "Fein\u00b7des", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das ward von Heinrichs Blitz erschlagen.", "tokens": ["Das", "ward", "von", "Hein\u00b7richs", "Blitz", "er\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "NE", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "In Seiner Rechten trug er ihn,", "tokens": ["In", "Sei\u00b7ner", "Rech\u00b7ten", "trug", "er", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Donner, der sie todt geschlagen,", "tokens": ["Den", "Don\u00b7ner", ",", "der", "sie", "todt", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Feinde, welche nach Berlin,", "tokens": ["Die", "Fein\u00b7de", ",", "wel\u00b7che", "nach", "Ber\u00b7lin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich einst mit Frechheit durften wagen.", "tokens": ["Sich", "einst", "mit", "Frech\u00b7heit", "durf\u00b7ten", "wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Nun kommen sie nicht mehr, du Kind", "tokens": ["Nun", "kom\u00b7men", "sie", "nicht", "mehr", ",", "du", "Kind"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "ADV", "$,", "PPER", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der honigs\u00fcssen Liebesg\u00f6tter,", "tokens": ["Der", "ho\u00b7ni\u00b7gs\u00fcs\u00b7sen", "Lie\u00b7bes\u00b7g\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Erzittre nicht, die Feinde sind", "tokens": ["Er\u00b7zitt\u00b7re", "nicht", ",", "die", "Fein\u00b7de", "sind"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "$,", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zerstreuet, wie die Birken-Bl\u00e4tter.", "tokens": ["Zer\u00b7streu\u00b7et", ",", "wie", "die", "Bir\u00b7ken\u00b7Bl\u00e4t\u00b7ter", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "F\u00fchl in dem kleinen Busen ganz,", "tokens": ["F\u00fchl", "in", "dem", "klei\u00b7nen", "Bu\u00b7sen", "ganz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Helden siegrisches Erg\u00f6tzen,", "tokens": ["Des", "Hel\u00b7den", "sieg\u00b7ri\u00b7sches", "Er\u00b7g\u00f6t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Einst wird Er seinen Lorbeercranz,", "tokens": ["Einst", "wird", "Er", "sei\u00b7nen", "Lor\u00b7beer\u00b7cranz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf deine goldne Locken setzen.", "tokens": ["Auf", "dei\u00b7ne", "gold\u00b7ne", "Lo\u00b7cken", "set\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Wenn du entgegen h\u00fcpfen wirst,", "tokens": ["Wenn", "du", "ent\u00b7ge\u00b7gen", "h\u00fcp\u00b7fen", "wirst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKVZ", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Triumphirern, wenn Sie kommen,", "tokens": ["Den", "Tri\u00b7um\u00b7phi\u00b7rern", ",", "wenn", "Sie", "kom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und bald der K\u00f6nig, bald der F\u00fcrst,", "tokens": ["Und", "bald", "der", "K\u00f6\u00b7nig", ",", "bald", "der", "F\u00fcrst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dich Gratie im Arm genommen,", "tokens": ["Dich", "Gra\u00b7tie", "im", "Arm", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "Und jeder deine Lippen dr\u00fcckt,", "tokens": ["Und", "je\u00b7der", "dei\u00b7ne", "Lip\u00b7pen", "dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sanft an die seinen, und empfindet,", "tokens": ["Sanft", "an", "die", "sei\u00b7nen", ",", "und", "emp\u00b7fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "PPOSAT", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie sch\u00f6n dein blaues Auge blickt,", "tokens": ["Wie", "sch\u00f6n", "dein", "blau\u00b7es", "Au\u00b7ge", "blickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Arm sich um die Schulter windet;", "tokens": ["Dein", "Arm", "sich", "um", "die", "Schul\u00b7ter", "win\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Dein Mund mit holden Stammeln fr\u00e4gt,", "tokens": ["Dein", "Mund", "mit", "hol\u00b7den", "Stam\u00b7meln", "fr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie es der Held im Felde machet,", "tokens": ["Wie", "es", "der", "Held", "im", "Fel\u00b7de", "ma\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df ihm Sein Herz nicht furchtsam schl\u00e4gt,", "tokens": ["Da\u00df", "ihm", "Sein", "Herz", "nicht", "furcht\u00b7sam", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "PTKNEG", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn rund umher ein Wetter krachet.", "tokens": ["Wenn", "rund", "um\u00b7her", "ein", "Wet\u00b7ter", "kra\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PTKVZ", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Dann l\u00e4cheln dich die Krieger an,", "tokens": ["Dann", "l\u00e4\u00b7cheln", "dich", "die", "Krie\u00b7ger", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So lieblich da\u00df du sie must fragen,", "tokens": ["So", "lieb\u00b7lich", "da\u00df", "du", "sie", "must", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOUS", "PPER", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ob mit so s\u00fcsser Miene kann,", "tokens": ["Ob", "mit", "so", "s\u00fcs\u00b7ser", "Mie\u00b7ne", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADV", "ADJA", "NN", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Held, die Menschen niederschlagen?", "tokens": ["Der", "Held", ",", "die", "Men\u00b7schen", "nie\u00b7der\u00b7schla\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Dann saget Vater Ferdinand,", "tokens": ["Dann", "sa\u00b7get", "Va\u00b7ter", "Fer\u00b7di\u00b7nand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Friedrichen zur Seite sitzet,", "tokens": ["Der", "Fried\u00b7ri\u00b7chen", "zur", "Sei\u00b7te", "sit\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Wie das bedr\u00e4ngte Vaterland,", "tokens": ["Wie", "das", "be\u00b7dr\u00e4ng\u00b7te", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Degen vor Gewalt besch\u00fctzet.", "tokens": ["Ihr", "De\u00b7gen", "vor", "Ge\u00b7walt", "be\u00b7sch\u00fct\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Er sagts, und Freude lachet dir;", "tokens": ["Er", "sagts", ",", "und", "Freu\u00b7de", "la\u00b7chet", "dir", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du stammelst: lieblich-goldner Friede;", "tokens": ["Du", "stam\u00b7melst", ":", "lieb\u00b7lich\u00b7gold\u00b7ner", "Frie\u00b7de", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Willst mehr noch sprechen, aber wir", "tokens": ["Willst", "mehr", "noch", "spre\u00b7chen", ",", "a\u00b7ber", "wir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "ADV", "ADV", "VVINF", "$,", "KON", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bet\u00e4uben dich, mit unserm Liede.", "tokens": ["Be\u00b7t\u00e4u\u00b7ben", "dich", ",", "mit", "un\u00b7serm", "Lie\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}