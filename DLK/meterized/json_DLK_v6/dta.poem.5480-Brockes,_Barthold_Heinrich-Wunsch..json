{"dta.poem.5480": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Wunsch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "M\u00f6gt', o HErr, von deiner G\u00fcte", "tokens": ["M\u00f6gt'", ",", "o", "Herr", ",", "von", "dei\u00b7ner", "G\u00fc\u00b7te"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "$,", "FM", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und von deiner Allmacht Schein", "tokens": ["Und", "von", "dei\u00b7ner", "All\u00b7macht", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mein betrachtendes Gem\u00fchte", "tokens": ["Mein", "be\u00b7trach\u00b7ten\u00b7des", "Ge\u00b7m\u00fch\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ein vern\u00fcnft\u2019ger Spiegel seyn!", "tokens": ["Ein", "ver\u00b7n\u00fcnft'\u00b7ger", "Spie\u00b7gel", "seyn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "M\u00f6gt\u2019 in mir, wenn Danck und Liebe,", "tokens": ["M\u00f6gt'", "in", "mir", ",", "wenn", "Danck", "und", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPER", "$,", "KOUS", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sammt der Andacht reinem Triebe,", "tokens": ["Sammt", "der", "An\u00b7dacht", "rei\u00b7nem", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sich zu meiner Frende f\u00fcgen;", "tokens": ["Sich", "zu", "mei\u00b7ner", "Fren\u00b7de", "f\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In mir, als im Wiederschein,", "tokens": ["In", "mir", ",", "als", "im", "Wie\u00b7der\u00b7schein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "KOUS", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Dir dein\u2019 Allmacht sichtbar seyn;", "tokens": ["Dir", "dein'", "All\u00b7macht", "sicht\u00b7bar", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Mein Vergn\u00fcgen dich vergn\u00fcgen!", "tokens": ["Mein", "Ver\u00b7gn\u00fc\u00b7gen", "dich", "ver\u00b7gn\u00fc\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}