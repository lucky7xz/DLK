{"textgrid.poem.42100": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "M\u00f6we \u00fcber der Br\u00fccke", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dir unterm Fu\u00df,", "tokens": ["Dir", "un\u00b7term", "Fu\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Zwischen den Ufern Schreitender, spannt", "tokens": ["Zwi\u00b7schen", "den", "U\u00b7fern", "Schrei\u00b7ten\u00b7der", ",", "spannt"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "NN", "$,", "VVFIN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Sich der Br\u00fccke gew\u00f6lbter Bogen.", "tokens": ["Sich", "der", "Br\u00fc\u00b7cke", "ge\u00b7w\u00f6lb\u00b7ter", "Bo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "ADJA", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Und eine M\u00f6we,", "tokens": ["Und", "ei\u00b7ne", "M\u00f6\u00b7we", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Wie ein Gedanke fernher blitzend,", "tokens": ["Wie", "ein", "Ge\u00b7dan\u00b7ke", "fern\u00b7her", "blit\u00b7zend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Schie\u00dft auf dich ihre blendende Bahn.", "tokens": ["Schie\u00dft", "auf", "dich", "ih\u00b7re", "blen\u00b7den\u00b7de", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Eine Sekunde", "tokens": ["Ei\u00b7ne", "Se\u00b7kun\u00b7de"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "St\u00f6\u00dft ihr Auge in deines, greift", "tokens": ["St\u00f6\u00dft", "ihr", "Au\u00b7ge", "in", "dei\u00b7nes", ",", "greift"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "$,", "VVFIN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Dich der wei\u00dfen Schwinge Umarmung.", "tokens": ["Dich", "der", "wei\u00b7\u00dfen", "Schwin\u00b7ge", "Um\u00b7ar\u00b7mung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Eine Sekunde", "tokens": ["Ei\u00b7ne", "Se\u00b7kun\u00b7de"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Hebt dich der Flug, tr\u00e4gt dich der Geist,", "tokens": ["Hebt", "dich", "der", "Flug", ",", "tr\u00e4gt", "dich", "der", "Geist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der schwerelose, brausend empor.", "tokens": ["Der", "schwe\u00b7re\u00b7lo\u00b7se", ",", "brau\u00b7send", "em\u00b7por", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "Es weht dich an", "tokens": ["Es", "weht", "dich", "an"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Der unendliche Raum, es rauscht", "tokens": ["Der", "un\u00b7end\u00b7li\u00b7che", "Raum", ",", "es", "rauscht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "VVFIN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Freiheit dir unerme\u00dflich ums Haupt.", "tokens": ["Frei\u00b7heit", "dir", "un\u00b7er\u00b7me\u00df\u00b7lich", "ums", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.6": {"line.1": {"text": "Wie ein Gedanke", "tokens": ["Wie", "ein", "Ge\u00b7dan\u00b7ke"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Der wei\u00dfe Vogel, fernhin sich windend,", "tokens": ["Der", "wei\u00b7\u00dfe", "Vo\u00b7gel", ",", "fern\u00b7hin", "sich", "win\u00b7dend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "PRF", "VVPP", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und kehrt dir einmal wieder vielleicht", "tokens": ["Und", "kehrt", "dir", "ein\u00b7mal", "wie\u00b7der", "viel\u00b7leicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.7": {"line.1": {"text": "Solange noch", "tokens": ["So\u00b7lan\u00b7ge", "noch"], "token_info": ["word", "word"], "pos": ["VMFIN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Von Ufer zu Ufer, Wanderer, dich", "tokens": ["Von", "U\u00b7fer", "zu", "U\u00b7fer", ",", "Wan\u00b7de\u00b7rer", ",", "dich"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "NN", "APPR", "NN", "$,", "NN", "$,", "PPER"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der Br\u00fccke schweigender Bogen tr\u00e4gt.", "tokens": ["Der", "Br\u00fc\u00b7cke", "schwei\u00b7gen\u00b7der", "Bo\u00b7gen", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}