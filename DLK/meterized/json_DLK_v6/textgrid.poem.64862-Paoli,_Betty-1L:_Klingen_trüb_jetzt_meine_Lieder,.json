{"textgrid.poem.64862": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Klingen tr\u00fcb jetzt meine Lieder,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Klingen tr\u00fcb jetzt meine Lieder,", "tokens": ["Klin\u00b7gen", "tr\u00fcb", "jetzt", "mei\u00b7ne", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "O so habt mit mir Geduld!", "tokens": ["O", "so", "habt", "mit", "mir", "Ge\u00b7duld", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VAFIN", "APPR", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kehren bess're Zeiten wieder,", "tokens": ["Keh\u00b7ren", "bess'\u00b7re", "Zei\u00b7ten", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Will ich zahlen alte Schuld.", "tokens": ["Will", "ich", "zah\u00b7len", "al\u00b7te", "Schuld", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wollt den Baum ihr niederhauen,", "tokens": ["Wollt", "den", "Baum", "ihr", "nie\u00b7der\u00b7hau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil der Herbstwind ihn entlaubt,", "tokens": ["Weil", "der", "Herbst\u00b7wind", "ihn", "ent\u00b7laubt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und des Winters b\u00f6ses Grauen", "tokens": ["Und", "des", "Win\u00b7ters", "b\u00f6\u00b7ses", "Grau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihm die Bl\u00fcthenzier geraubt?", "tokens": ["Ihm", "die", "Bl\u00fc\u00b7then\u00b7zier", "ge\u00b7raubt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nein! ihr la\u00dft ihn hoffend stehen,", "tokens": ["Nein", "!", "ihr", "la\u00dft", "ihn", "hof\u00b7fend", "ste\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPER", "VVFIN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um ihn einst zur Lebenszeit", "tokens": ["Um", "ihn", "einst", "zur", "Le\u00b7bens\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "PPER", "ADV", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wieder neugeschm\u00fcckt zu sehen,", "tokens": ["Wie\u00b7der", "neu\u00b7ge\u00b7schm\u00fcckt", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schattig, bl\u00fcthen\u00fcberschneit.", "tokens": ["Schat\u00b7tig", ",", "bl\u00fc\u00b7then\u00b7\u00fc\u00b7bersc\u00b7hneit", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und die ihn geduldet haben,", "tokens": ["Und", "die", "ihn", "ge\u00b7dul\u00b7det", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "VVPP", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ehrt der Baum mit frommem Sinn,", "tokens": ["Ehrt", "der", "Baum", "mit", "from\u00b7mem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Legt dann dankbar seine Gaben", "tokens": ["Legt", "dann", "dank\u00b7bar", "sei\u00b7ne", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADJD", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gern zu ihren F\u00fc\u00dfen hin.", "tokens": ["Gern", "zu", "ih\u00b7ren", "F\u00fc\u00b7\u00dfen", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Also steh auch ich entbl\u00e4ttert,", "tokens": ["Al\u00b7so", "steh", "auch", "ich", "ent\u00b7bl\u00e4t\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von des Ungl\u00fccks rauhem Nord,", "tokens": ["Von", "des", "Un\u00b7gl\u00fccks", "rau\u00b7hem", "Nord", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Von dem Blitzstrahl fast zerschmettert,", "tokens": ["Von", "dem", "Blitz\u00b7strahl", "fast", "zer\u00b7schmet\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der mich traf in einem Wort.", "tokens": ["Der", "mich", "traf", "in", "ei\u00b7nem", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Doch es werden Zeiten kommen,", "tokens": ["Doch", "es", "wer\u00b7den", "Zei\u00b7ten", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo das Herz mir wieder frei,", "tokens": ["Wo", "das", "Herz", "mir", "wie\u00b7der", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und zu heitrem Sang verschwommen", "tokens": ["Und", "zu", "heit\u00b7rem", "Sang", "ver\u00b7schwom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meiner Seele Jammerschrei.", "tokens": ["Mei\u00b7ner", "See\u00b7le", "Jam\u00b7mer\u00b7schrei", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Wo nach Winters Finsternissen", "tokens": ["Wo", "nach", "Win\u00b7ters", "Fins\u00b7ter\u00b7nis\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir erbl\u00fcht ein Fr\u00fchlingstag,", "tokens": ["Mir", "er\u00b7bl\u00fcht", "ein", "Fr\u00fch\u00b7lings\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo ich werd' zu singen wissen,", "tokens": ["Wo", "ich", "werd'", "zu", "sin\u00b7gen", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "PTKZU", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ich jetzt nur weinen mag.", "tokens": ["Wie", "ich", "jetzt", "nur", "wei\u00b7nen", "mag."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["PWAV", "PPER", "ADV", "ADV", "VVINF", "VMFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Und von froher Lieder Bl\u00fcthe", "tokens": ["Und", "von", "fro\u00b7her", "Lie\u00b7der", "Bl\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Werde dann die Stirn umrauscht", "tokens": ["Wer\u00b7de", "dann", "die", "Stirn", "um\u00b7rauscht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVPP"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Derer, die mit stiller G\u00fcte", "tokens": ["De\u00b7rer", ",", "die", "mit", "stil\u00b7ler", "G\u00fc\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meinem Schmerzensang gelauscht.", "tokens": ["Mei\u00b7nem", "Schmer\u00b7zen\u00b7sang", "ge\u00b7lauscht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}