{"dta.poem.18645": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "D er liebe  H andelschafft.  \n Auf die Verehligung eines vornehmen  \n Kauffmannes den 12. Septembr.  \n 1701. in Namen eines andern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Indem der Orient durch seine Kostbarkeiten", "tokens": ["In\u00b7dem", "der", "O\u00b7rient", "durch", "sei\u00b7ne", "Kost\u00b7bar\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Durch Perl und Diamant das wehrte Hamburg", "tokens": ["Durch", "Perl", "und", "Di\u00b7a\u00b7mant", "das", "wehr\u00b7te", "Ham\u00b7burg"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "schm\u00fcckt/", "tokens": ["schm\u00fcckt", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Da Holl- und Engelland von allen Seltenheiten/", "tokens": ["Da", "Holl", "und", "En\u00b7gel\u00b7land", "von", "al\u00b7len", "Sel\u00b7ten\u00b7hei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "TRUNC", "KON", "NE", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und was es sch\u00e4tzbars hat der sch\u00e4tzbarn Stadt zu schickt:", "tokens": ["Und", "was", "es", "sch\u00e4tz\u00b7bars", "hat", "der", "sch\u00e4tz\u00b7barn", "Stadt", "zu", "schickt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VAFIN", "ART", "ADJA", "NN", "PTKZU", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da Franckreich sie galant/ und Spanjen k\u00f6stlich machet/", "tokens": ["Da", "Fran\u00b7ck\u00b7reich", "sie", "ga\u00b7lant", "/", "und", "Span\u00b7jen", "k\u00f6st\u00b7lich", "ma\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPER", "ADJD", "$(", "KON", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Ja da so Land als See sich hat mit ihr verm\u00e4hlt/", "tokens": ["Ja", "da", "so", "Land", "als", "See", "sich", "hat", "mit", "ihr", "ver\u00b7m\u00e4hlt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ADV", "NN", "KOUS", "NN", "PRF", "VAFIN", "APPR", "PPOSAT", "VVPP", "$("], "meter": "-+-+-+-+---+", "measure": "unknown.measure.penta"}, "line.8": {"text": "Und da die Kauffmanschafft in solchen Flore lachet/", "tokens": ["Und", "da", "die", "Kauf\u00b7fman\u00b7schafft", "in", "sol\u00b7chen", "Flo\u00b7re", "la\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPR", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Da\u00df ihrer Sch\u00f6nheit nichts/ als mehr Verwundrung fehlt/", "tokens": ["Da\u00df", "ih\u00b7rer", "Sch\u00f6n\u00b7heit", "nichts", "/", "als", "mehr", "Ver\u00b7wund\u00b7rung", "fehlt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PIS", "$(", "KOKOM", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Was Wunders da\u00df in sie die Liebe sich verliebet/", "tokens": ["Was", "Wun\u00b7ders", "da\u00df", "in", "sie", "die", "Lie\u00b7be", "sich", "ver\u00b7lie\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KOUS", "APPR", "PPER", "ART", "NN", "PRF", "VVFIN", "$("], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.11": {"text": "Und in der sch\u00f6nen Stadt auch ihren Handel \u00fcbet/", "tokens": ["Und", "in", "der", "sch\u00f6\u00b7nen", "Stadt", "auch", "ih\u00b7ren", "Han\u00b7del", "\u00fc\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ADV", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}