{"textgrid.poem.66585": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: In den Tagen stiller Trauer,", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In den Tagen stiller Trauer,", "tokens": ["In", "den", "Ta\u00b7gen", "stil\u00b7ler", "Trau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der sich stumm die Seele neigt,", "tokens": ["Der", "sich", "stumm", "die", "See\u00b7le", "neigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bringt ein Lichtstrahl selbst dir Schauer,", "tokens": ["Bringt", "ein", "Licht\u00b7strahl", "selbst", "dir", "Schau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "PPER", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der aus fremden Blicken steigt.", "tokens": ["Der", "aus", "frem\u00b7den", "Bli\u00b7cken", "steigt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Freude wandelt sich in Bangen,", "tokens": ["Freu\u00b7de", "wan\u00b7delt", "sich", "in", "Ban\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn das Jahr sich hold erneut,", "tokens": ["Wenn", "das", "Jahr", "sich", "hold", "er\u00b7neut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jedem hoffenden Verlangen", "tokens": ["Je\u00b7dem", "hof\u00b7fen\u00b7den", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ungez\u00e4hlte Bl\u00fcthen streut.", "tokens": ["Un\u00b7ge\u00b7z\u00e4hl\u00b7te", "Bl\u00fc\u00b7then", "streut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Was seit Jahren aufgegeben,", "tokens": ["Was", "seit", "Jah\u00b7ren", "auf\u00b7ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Leuchtet schmerzlich durch die Brust,", "tokens": ["Leuch\u00b7tet", "schmerz\u00b7lich", "durch", "die", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und du f\u00fchlst, was f\u00fcr das Leben", "tokens": ["Und", "du", "f\u00fchlst", ",", "was", "f\u00fcr", "das", "Le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ewig du entbehren mu\u00dft.", "tokens": ["E\u00b7wig", "du", "ent\u00b7beh\u00b7ren", "mu\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}