{"textgrid.poem.35818": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Kreuz, o du mein Streitpanier,", "genre": "verse", "period": "N.A.", "pub_year": 1818, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Kreuz, o du mein Streitpanier,", "tokens": ["O", "Kreuz", ",", "o", "du", "mein", "Streit\u00b7pa\u00b7nier", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dich gr\u00fc\u00df' ich, heil'ges Kreuz!", "tokens": ["Dich", "gr\u00fc\u00df'", "ich", ",", "heil'\u00b7ges", "Kreuz", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sei du mir Schmuck und h\u00f6chste Zier,", "tokens": ["Sei", "du", "mir", "Schmuck", "und", "h\u00f6chs\u00b7te", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sei du mein einz'ger Reiz.", "tokens": ["Sei", "du", "mein", "einz'\u00b7ger", "Reiz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O Kreuz, du k\u00f6stlich Gnadengut,", "tokens": ["O", "Kreuz", ",", "du", "k\u00f6st\u00b7lich", "Gna\u00b7den\u00b7gut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sei tausendmal gegr\u00fc\u00dft,", "tokens": ["Sei", "tau\u00b7send\u00b7mal", "ge\u00b7gr\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ob jener theuern Purpurfluth,", "tokens": ["Ob", "je\u00b7ner", "theu\u00b7ern", "Pur\u00b7pur\u00b7fluth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die an dir niederflie\u00dft!", "tokens": ["Die", "an", "dir", "nie\u00b7der\u00b7flie\u00dft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Eh' Adam scheu aus Eden floh,", "tokens": ["Eh'", "A\u00b7dam", "scheu", "aus", "E\u00b7den", "floh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich tiefer Schuld bewu\u00dft,", "tokens": ["Sich", "tie\u00b7fer", "Schuld", "be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da war der Mensch noch reich und froh,", "tokens": ["Da", "war", "der", "Mensch", "noch", "reich", "und", "froh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Vaters liebste Lust.", "tokens": ["Des", "Va\u00b7ters", "liebs\u00b7te", "Lust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Unsel'ger Geist der Finsterni\u00df,", "tokens": ["Un\u00b7sel'\u00b7ger", "Geist", "der", "Fins\u00b7ter\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weh, da\u00df dein schwarzer Neid", "tokens": ["Weh", ",", "da\u00df", "dein", "schwar\u00b7zer", "Neid"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihn nieder von der H\u00f6he ri\u00df", "tokens": ["Ihn", "nie\u00b7der", "von", "der", "H\u00f6\u00b7he", "ri\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PTKVZ", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Angst und S\u00fcnd' und Streit!", "tokens": ["In", "Angst", "und", "S\u00fcnd'", "und", "Streit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "O, Schade, Schade, menschlich Herz,", "tokens": ["O", ",", "Scha\u00b7de", ",", "Scha\u00b7de", ",", "menschlich", "Herz", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,", "ADJD", "$,", "ADJD", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Du warst so hoch gestellt!", "tokens": ["Du", "warst", "so", "hoch", "ge\u00b7stellt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun geh und ringe mit dem Schmerz;", "tokens": ["Nun", "geh", "und", "rin\u00b7ge", "mit", "dem", "Schmerz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Fremd bleibst du doch der Welt.", "tokens": ["Fremd", "bleibst", "du", "doch", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Dich martern Reu' und Sehnsucht nun,", "tokens": ["Dich", "mar\u00b7tern", "Reu'", "und", "Sehn\u00b7sucht", "nun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dich schreckt Gericht und Tod;", "tokens": ["Dich", "schreckt", "Ge\u00b7richt", "und", "Tod", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du f\u00fchlst: in Gott nur kannst du ruhn,", "tokens": ["Du", "f\u00fchlst", ":", "in", "Gott", "nur", "kannst", "du", "ruhn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "APPR", "NN", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du bist verwandt mit Gott.", "tokens": ["Du", "bist", "ver\u00b7wandt", "mit", "Gott", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und diesen Gott hast du gekr\u00e4nkt,", "tokens": ["Und", "die\u00b7sen", "Gott", "hast", "du", "ge\u00b7kr\u00e4nkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ja, bis zum Tod betr\u00fcbt,", "tokens": ["Ja", ",", "bis", "zum", "Tod", "be\u00b7tr\u00fcbt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Alles, Alles dir geschenkt,", "tokens": ["Der", "Al\u00b7les", ",", "Al\u00b7les", "dir", "ge\u00b7schenkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "$,", "PIS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der dich so treu geliebt.", "tokens": ["Der", "dich", "so", "treu", "ge\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ja, bis zum Tod, zum Kreuzestod", "tokens": ["Ja", ",", "bis", "zum", "Tod", ",", "zum", "Kreu\u00b7ze\u00b7stod"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKANT", "$,", "KOUS", "APPRART", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat Ihn dein Fall betr\u00fcbt \u2013", "tokens": ["Hat", "Ihn", "dein", "Fall", "be\u00b7tr\u00fcbt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O, der uns\u00e4glich treue Gott,", "tokens": ["O", ",", "der", "un\u00b7s\u00e4g\u00b7lich", "treu\u00b7e", "Gott", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ADJD", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Wie hat Er dich geliebt!", "tokens": ["Wie", "hat", "Er", "dich", "ge\u00b7liebt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Dein Ungehorsam bricht das Herz,", "tokens": ["Dein", "Un\u00b7ge\u00b7hor\u00b7sam", "bricht", "das", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das dir ja Alles gab;", "tokens": ["Das", "dir", "ja", "Al\u00b7les", "gab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "PIS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dein' Hoffart zollt Ihm Hohn und Schmerz,", "tokens": ["Dein'", "Hof\u00b7fart", "zollt", "Ihm", "Hohn", "und", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Undank gr\u00e4bt Sein Grab.", "tokens": ["Dein", "Un\u00b7dank", "gr\u00e4bt", "Sein", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "O Gott! mein Gott! mein einzig Gut!", "tokens": ["O", "Gott", "!", "mein", "Gott", "!", "mein", "ein\u00b7zig", "Gut", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PPOSAT", "NN", "$.", "PPOSAT", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie schwer verging ich mich!", "tokens": ["Wie", "schwer", "ver\u00b7ging", "ich", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und dennoch bist Du mir noch gut \u2013", "tokens": ["Und", "den\u00b7noch", "bist", "Du", "mir", "noch", "gut", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "PPER", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erbarm', erbarme Dich!", "tokens": ["Er\u00b7barm'", ",", "er\u00b7bar\u00b7me", "Dich", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Du kennst noch Dein entstelltes Kind:", "tokens": ["Du", "kennst", "noch", "Dein", "ent\u00b7stell\u00b7tes", "Kind", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich bringe Dir Dein Kreuz;", "tokens": ["Ich", "brin\u00b7ge", "Dir", "Dein", "Kreuz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es ist mein Stab, denn ich bin blind,", "tokens": ["Es", "ist", "mein", "Stab", ",", "denn", "ich", "bin", "blind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "KON", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es ist mein Schmuck, mein Reiz.", "tokens": ["Es", "ist", "mein", "Schmuck", ",", "mein", "Reiz", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Es ist mein Schwert, mein Streitpanier,", "tokens": ["Es", "ist", "mein", "Schwert", ",", "mein", "Streit\u00b7pa\u00b7nier", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Schild, mein Grubenlicht.", "tokens": ["Mein", "Schild", ",", "mein", "Gru\u00b7ben\u00b7licht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Herr! schwer ist es, doch trag' ich's Dir,", "tokens": ["Herr", "!", "schwer", "ist", "es", ",", "doch", "trag'", "ich's", "Dir", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADJD", "VAFIN", "PPER", "$,", "ADV", "VVFIN", "PIS", "PPER", "$,"], "meter": "-----+-+", "measure": "unknown.measure.di"}, "line.4": {"text": "Bis mir das Herz dran bricht.", "tokens": ["Bis", "mir", "das", "Herz", "dran", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "PAV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}