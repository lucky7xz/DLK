{"dta.poem.9900": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wir sind erstlich wei\u00df wie schnee/ wen\u0303 wir an zu leben fangen/", "tokens": ["Wir", "sind", "erst\u00b7lich", "wei\u00df", "wie", "schnee", "/", "we\u00f1", "wir", "an", "zu", "le\u00b7ben", "fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "VVFIN", "KOKOM", "NN", "$(", "KOUS", "PPER", "PTKVZ", "PTKZU", "VVINF", "VVFIN", "$("], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ohn uns kan nicht leicht ein mensch recht mit seiner sch\u00f6nheit", "tokens": ["Ohn", "uns", "kan", "nicht", "leicht", "ein", "mensch", "recht", "mit", "sei\u00b7ner", "sch\u00f6n\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PPER", "VMFIN", "PTKNEG", "ADJD", "ART", "NN", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "prangen/", "tokens": ["pran\u00b7gen", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Wir bewegen uns von unten/ oben stehen wir gar still/", "tokens": ["Wir", "be\u00b7we\u00b7gen", "uns", "von", "un\u00b7ten", "/", "o\u00b7ben", "ste\u00b7hen", "wir", "gar", "still", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADV", "$(", "ADV", "VVFIN", "PPER", "ADV", "ADJD", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Wenn die gelbe farb uns mahlet/ niemand uns mehr habe\u0303 will.", "tokens": ["Wenn", "die", "gel\u00b7be", "farb", "uns", "mah\u00b7let", "/", "nie\u00b7mand", "uns", "mehr", "hab\u1ebd", "will", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "VVFIN", "PPER", "VVFIN", "$(", "PIS", "PPER", "ADV", "VAINF", "VMFIN", "$."], "meter": "-+--+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}}}}