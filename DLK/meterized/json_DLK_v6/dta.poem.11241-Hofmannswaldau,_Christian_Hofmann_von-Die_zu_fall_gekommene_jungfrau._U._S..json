{"dta.poem.11241": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die zu fall gekommene jungfrau.  \n U. S.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von mancher jungfer heists: Die ist zu falle kommen,", "tokens": ["Von", "man\u00b7cher", "jung\u00b7fer", "heists", ":", "Die", "ist", "zu", "fal\u00b7le", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$.", "PDS", "VAFIN", "PTKZU", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dieweil ein doppelt hertz in ihrem leibe schl\u00e4gt;", "tokens": ["Die\u00b7weil", "ein", "dop\u00b7pelt", "hertz", "in", "ih\u00b7rem", "lei\u00b7be", "schl\u00e4gt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJD", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Allein, wer hat wohl das vor fallen angenommen,", "tokens": ["Al\u00b7lein", ",", "wer", "hat", "wohl", "das", "vor", "fal\u00b7len", "an\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWS", "VAFIN", "ADV", "ART", "APPR", "VVINF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn man sich mit bedacht freywillig niederlegt?", "tokens": ["Wenn", "man", "sich", "mit", "be\u00b7dacht", "frey\u00b7wil\u00b7lig", "nie\u00b7der\u00b7legt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "APPR", "ADJD", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}