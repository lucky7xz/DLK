{"textgrid.poem.67058": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die heilige Bahn", "genre": "verse", "period": "N.A.", "pub_year": 1789, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist also dies die heilige Bahn?", "tokens": ["Ist", "al\u00b7so", "dies", "die", "hei\u00b7li\u00b7ge", "Bahn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PDS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Herrlicher Blick \u2013 o tr\u00fcge mich nicht!", "tokens": ["Herr\u00b7li\u00b7cher", "Blick", "\u2013", "o", "tr\u00fc\u00b7ge", "mich", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "FM", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Diese geh ich?? schwebend auf des Liedes", "tokens": ["Die\u00b7se", "geh", "ich", "??", "schwe\u00b7bend", "auf", "des", "Lie\u00b7des"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "$.", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Hoher fliegender Morgenwolke?", "tokens": ["Ho\u00b7her", "flie\u00b7gen\u00b7der", "Mor\u00b7gen\u00b7wol\u00b7ke", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Und welch ist jene? k\u00fcnstlich gebaut", "tokens": ["Und", "welch", "ist", "je\u00b7ne", "?", "k\u00fcnst\u00b7lich", "ge\u00b7baut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PWAT", "VAFIN", "PDS", "$.", "ADJD", "VVPP"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Eben hinaus, mit Marmor beschr\u00e4nkt,", "tokens": ["E\u00b7ben", "hin\u00b7aus", ",", "mit", "Mar\u00b7mor", "be\u00b7schr\u00e4nkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Pr\u00e4chtig gerad, gleich den Sonnenstrahlen \u2013", "tokens": ["Pr\u00e4ch\u00b7tig", "ge\u00b7rad", ",", "gleich", "den", "Son\u00b7nen\u00b7strah\u00b7len", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "$,", "ADV", "ART", "NN", "$("], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "An der Pforte ein hoher Richtstuhl?", "tokens": ["An", "der", "Pfor\u00b7te", "ein", "ho\u00b7her", "Richt\u00b7stuhl", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Ha! wie den Richtstuhl Purpur umflie\u00dft,", "tokens": ["Ha", "!", "wie", "den", "Richt\u00b7stuhl", "Pur\u00b7pur", "um\u00b7flie\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Und der Smaragd, wie blendend er gl\u00e4nzt,", "tokens": ["Und", "der", "Sma\u00b7ragd", ",", "wie", "blen\u00b7dend", "er", "gl\u00e4nzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PWAV", "ADJD", "PPER", "VVFIN", "$,"], "meter": "--++-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Und auf dem Stuhl, mit dem gro\u00dfen Szepter", "tokens": ["Und", "auf", "dem", "Stuhl", ",", "mit", "dem", "gro\u00b7\u00dfen", "Szep\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Aristoteles hinw\u00e4rts blickend", "tokens": ["A\u00b7ris\u00b7to\u00b7te\u00b7les", "hin\u00b7w\u00e4rts", "bli\u00b7ckend"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADV", "ADJD"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Mit hellem scharfem Aug auf des Lieds", "tokens": ["Mit", "hel\u00b7lem", "schar\u00b7fem", "Aug", "auf", "des", "Lieds"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Feurigen Lauf \u2013 und jenes Gebirg", "tokens": ["Feu\u00b7ri\u00b7gen", "Lauf", "\u2013", "und", "je\u00b7nes", "Ge\u00b7birg"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$(", "KON", "PDAT", "NN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Eilt sie hinweg \u2013 mutig in die T\u00e4ler", "tokens": ["Eilt", "sie", "hin\u00b7weg", "\u2013", "mu\u00b7tig", "in", "die", "T\u00e4\u00b7ler"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$(", "ADJD", "APPR", "ART", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "St\u00fcrzt sie, ungest\u00fcm, und ihr Boden", "tokens": ["St\u00fcrzt", "sie", ",", "un\u00b7ge\u00b7st\u00fcm", ",", "und", "ihr", "Bo\u00b7den"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "ADJD", "$,", "KON", "PPOSAT", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Ist wie des Nordens Flammengew\u00f6lk,", "tokens": ["Ist", "wie", "des", "Nor\u00b7dens", "Flam\u00b7men\u00b7ge\u00b7w\u00f6lk", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "NN", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Wallend vom Tritt des rennenden Gangs \u2013", "tokens": ["Wal\u00b7lend", "vom", "Tritt", "des", "ren\u00b7nen\u00b7den", "Gangs", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "ART", "ADJA", "NN", "$("], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Waffenger\u00e4usch rauschen seine Tritte", "tokens": ["Waf\u00b7fen\u00b7ge\u00b7r\u00e4usch", "rau\u00b7schen", "sei\u00b7ne", "Trit\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN"], "meter": "+--++-+-+-", "measure": "dactylic.init"}, "line.4": {"text": "\u00dcber alternde Wolkenfelsen.", "tokens": ["\u00dc\u00b7ber", "al\u00b7tern\u00b7de", "Wol\u00b7ken\u00b7fel\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Ha! sie ist hei\u00df, die heilige Bahn \u2013", "tokens": ["Ha", "!", "sie", "ist", "hei\u00df", ",", "die", "hei\u00b7li\u00b7ge", "Bahn", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VAFIN", "ADJD", "$,", "ART", "ADJA", "NN", "$("], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Ach wie ge\u00fcbt der Gro\u00dfe dort rennt,", "tokens": ["Ach", "wie", "ge\u00b7\u00fcbt", "der", "Gro\u00b7\u00dfe", "dort", "rennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "VVPP", "ART", "ADJA", "ADV", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Um ihn herum \u2013 wie da Staunen wimmelt,", "tokens": ["Um", "ihn", "he\u00b7rum", "\u2013", "wie", "da", "Stau\u00b7nen", "wim\u00b7melt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "PTKVZ", "$(", "KOKOM", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Freunde \u2013 Vaterland \u2013 fernes Ausland.", "tokens": ["Freun\u00b7de", "\u2013", "Va\u00b7ter\u00b7land", "\u2013", "fer\u00b7nes", "Aus\u00b7land", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "ADJA", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Und ich um ihn mit M\u00fcckengesums", "tokens": ["Und", "ich", "um", "ihn", "mit", "M\u00fc\u00b7cken\u00b7ge\u00b7sums"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "PPER", "APPR", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Niedrig \u2013 im Staub \u2013 Nein, Gro\u00dfer, das nicht.", "tokens": ["Nied\u00b7rig", "\u2013", "im", "Staub", "\u2013", "Nein", ",", "Gro\u00b7\u00dfer", ",", "das", "nicht", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$(", "APPRART", "NN", "$(", "PTKANT", "$,", "NN", "$,", "PRELS", "PTKNEG", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Mutig hinan! \u2013 ! \u2013 Wanns nun da ist, voll ist...", "tokens": ["Mu\u00b7tig", "hi\u00b7nan", "!", "\u2013", "!", "\u2013", "Wanns", "nun", "da", "ist", ",", "voll", "ist", "..."], "token_info": ["word", "word", "punct", "punct", "punct", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$.", "$(", "$.", "$(", "PWAV", "ADV", "ADV", "VAFIN", "$,", "ADJD", "VAFIN", "$("], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}}}}