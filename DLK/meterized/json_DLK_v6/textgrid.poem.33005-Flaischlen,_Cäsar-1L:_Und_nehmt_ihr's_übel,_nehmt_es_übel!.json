{"textgrid.poem.33005": {"metadata": {"author": {"name": "Flaischlen, C\u00e4sar", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und nehmt ihr's \u00fcbel, nehmt es \u00fcbel!", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und nehmt ihr's \u00fcbel, nehmt es \u00fcbel!", "tokens": ["Und", "nehmt", "ih\u00b7r's", "\u00fc\u00b7bel", ",", "nehmt", "es", "\u00fc\u00b7bel", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$,", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "in Gottes Namen, rei\u00dft es zum Ri\u00df!", "tokens": ["in", "Got\u00b7tes", "Na\u00b7men", ",", "rei\u00dft", "es", "zum", "Ri\u00df", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "ich kann und ... will auch nicht! Gewi\u00df,", "tokens": ["ich", "kann", "und", "...", "will", "auch", "nicht", "!", "Ge\u00b7wi\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "KON", "$(", "VMFIN", "ADV", "PTKNEG", "$.", "PTKANT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "ich will auch nicht! ...", "tokens": ["ich", "will", "auch", "nicht", "!", "..."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PTKNEG", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Die zwei, drei Stunden,", "tokens": ["Die", "zwei", ",", "drei", "Stun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "CARD", "$,", "CARD", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "die mir als letzten, m\u00fcden Rest", "tokens": ["die", "mir", "als", "letz\u00b7ten", ",", "m\u00fc\u00b7den", "Rest"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PPER", "KOUS", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "des Tages Arbeit \u00fcbrig l\u00e4\u00dft,", "tokens": ["des", "Ta\u00b7ges", "Ar\u00b7beit", "\u00fcb\u00b7rig", "l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "ich will sie nicht so zwecklos vergeuden", "tokens": ["ich", "will", "sie", "nicht", "so", "zweck\u00b7los", "ver\u00b7geu\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "mit hohlem Gerede und mit Leuten,", "tokens": ["mit", "hoh\u00b7lem", "Ge\u00b7re\u00b7de", "und", "mit", "Leu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+----+-+-", "measure": "dactylic.init"}, "line.10": {"text": "f\u00fcr die ich genau so viel und so wenig", "tokens": ["f\u00fcr", "die", "ich", "ge\u00b7nau", "so", "viel", "und", "so", "we\u00b7nig"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "ADJD", "ADV", "ADV", "KON", "ADV", "PIS"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "als sie f\u00fcr mich ...", "tokens": ["als", "sie", "f\u00fcr", "mich", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "mit denen ich sitze und Braten esse,", "tokens": ["mit", "de\u00b7nen", "ich", "sit\u00b7ze", "und", "Bra\u00b7ten", "es\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "KON", "NN", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.13": {"text": "und die ich nach eilig steifem Adieu", "tokens": ["und", "die", "ich", "nach", "ei\u00b7lig", "stei\u00b7fem", "A\u00b7die\u00b7u"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PRELS", "PPER", "APPR", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.14": {"text": "vor der Haust\u00fcr unten wieder vergesse", "tokens": ["vor", "der", "Haus\u00b7t\u00fcr", "un\u00b7ten", "wie\u00b7der", "ver\u00b7ges\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "ADV", "VVFIN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.15": {"text": "und all mein Lebtag nicht wiederseh.", "tokens": ["und", "all", "mein", "Leb\u00b7tag", "nicht", "wie\u00b7der\u00b7seh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Die zwei, drei Stunden am sp\u00e4ten Abend", "tokens": ["Die", "zwei", ",", "drei", "Stun\u00b7den", "am", "sp\u00e4\u00b7ten", "A\u00b7bend"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "CARD", "$,", "CARD", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "sie sind das einzige, was ich habe,", "tokens": ["sie", "sind", "das", "ein\u00b7zi\u00b7ge", ",", "was", "ich", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "$,", "PWS", "PPER", "VAFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "sie sind mein Lohn und sind mein Leben", "tokens": ["sie", "sind", "mein", "Lohn", "und", "sind", "mein", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "KON", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "und kosten mich denn doch zu viel,", "tokens": ["und", "kos\u00b7ten", "mich", "denn", "doch", "zu", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "PTKA", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "um sie so planlos zu verl\u00e4ppern", "tokens": ["um", "sie", "so", "plan\u00b7los", "zu", "ver\u00b7l\u00e4p\u00b7pern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PPER", "ADV", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "f\u00fcr andere zu blo\u00dfem Spiel!", "tokens": ["f\u00fcr", "an\u00b7de\u00b7re", "zu", "blo\u00b7\u00dfem", "Spiel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PTKZU", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und wenn ich auch nichts weiter tue,", "tokens": ["Und", "wenn", "ich", "auch", "nichts", "wei\u00b7ter", "tue", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "als da\u00df ich mich in aller Ruhe", "tokens": ["als", "da\u00df", "ich", "mich", "in", "al\u00b7ler", "Ru\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOKOM", "KOUS", "PPER", "PRF", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "zu Haus einmal aufs Sofa strecke", "tokens": ["zu", "Haus", "ein\u00b7mal", "aufs", "So\u00b7fa", "stre\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "und \u00fcber alten Pl\u00e4nen hecke", "tokens": ["und", "\u00fc\u00b7ber", "al\u00b7ten", "Pl\u00e4\u00b7nen", "he\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "und ein paar Verse reime ...", "tokens": ["und", "ein", "paar", "Ver\u00b7se", "rei\u00b7me", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PIAT", "NN", "VVFIN", "$("], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "oder tr\u00e4ume ...", "tokens": ["o\u00b7der", "tr\u00e4u\u00b7me", "..."], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "wie man so tr\u00e4umt,", "tokens": ["wie", "man", "so", "tr\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "wenn man vom Leben", "tokens": ["wenn", "man", "vom", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPRART", "NN"], "meter": "++-+-", "measure": "iambic.di"}, "line.9": {"text": "ein bi\u00dfchen mehr will, als blo\u00df eben", "tokens": ["ein", "bi\u00df\u00b7chen", "mehr", "will", ",", "als", "blo\u00df", "e\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PIS", "ADV", "VMFIN", "$,", "KOUS", "ADV", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "...leben!", "tokens": ["...", "le\u00b7ben", "!"], "token_info": ["punct", "word", "punct"], "pos": ["$(", "VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}