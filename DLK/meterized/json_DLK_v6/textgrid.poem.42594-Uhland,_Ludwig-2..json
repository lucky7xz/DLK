{"textgrid.poem.42594": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Grab, o Mutter, ist gegraben dir", "tokens": ["Ein", "Grab", ",", "o", "Mut\u00b7ter", ",", "ist", "ge\u00b7gra\u00b7ben", "dir"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "FM", "NN", "$,", "VAFIN", "VVPP", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "An einer stillen, dir bekannten Stelle,", "tokens": ["An", "ei\u00b7ner", "stil\u00b7len", ",", "dir", "be\u00b7kann\u00b7ten", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein heimatlicher Schatten wehet hier,", "tokens": ["Ein", "hei\u00b7mat\u00b7li\u00b7cher", "Schat\u00b7ten", "we\u00b7het", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Auch fehlen Blumen nicht an seiner Schwelle.", "tokens": ["Auch", "feh\u00b7len", "Blu\u00b7men", "nicht", "an", "sei\u00b7ner", "Schwel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Drin liegst du, wie du starbest, unversehrt,", "tokens": ["Drin", "liegst", "du", ",", "wie", "du", "star\u00b7best", ",", "un\u00b7ver\u00b7sehrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VVFIN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit jedem Zug des Friedens und der Schmerzen;", "tokens": ["Mit", "je\u00b7dem", "Zug", "des", "Frie\u00b7dens", "und", "der", "Schmer\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auch aufzuleben ist dir nicht verwehrt:", "tokens": ["Auch", "auf\u00b7zu\u00b7le\u00b7ben", "ist", "dir", "nicht", "ver\u00b7wehrt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich grub dir dieses Grab in meinem Herzen.", "tokens": ["Ich", "grub", "dir", "die\u00b7ses", "Grab", "in", "mei\u00b7nem", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PDAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}