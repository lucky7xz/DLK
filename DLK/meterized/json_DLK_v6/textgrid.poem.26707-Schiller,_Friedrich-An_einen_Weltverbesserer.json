{"textgrid.poem.26707": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An einen Weltverbesserer", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bballes opfert' ich hin\u00ab, sprichst du, \u00bbder Menscheit zu helfen,", "tokens": ["\u00bb", "al\u00b7les", "op\u00b7fert'", "ich", "hin", "\u00ab", ",", "sprichst", "du", ",", "\u00bb", "der", "Men\u00b7scheit", "zu", "hel\u00b7fen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "VVFIN", "PPER", "PTKVZ", "$(", "$,", "VVFIN", "PPER", "$,", "$(", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+--++-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Eitel war der Erfolg, Ha\u00df und Verfolgung der Lohn.\u00ab \u2013", "tokens": ["Ei\u00b7tel", "war", "der", "Er\u00b7folg", ",", "Ha\u00df", "und", "Ver\u00b7fol\u00b7gung", "der", "Lohn", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "NN", "KON", "NN", "ART", "NN", "$.", "$(", "$("], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Soll ich dir sagen, Freund, wie ich mit Menschen es halte?", "tokens": ["Soll", "ich", "dir", "sa\u00b7gen", ",", "Freund", ",", "wie", "ich", "mit", "Men\u00b7schen", "es", "hal\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVINF", "$,", "NN", "$,", "PWAV", "PPER", "APPR", "NN", "PPER", "VVFIN", "$."], "meter": "---+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Traue dem Spruche! noch nie hat mich der F\u00fchrer get\u00e4uscht,", "tokens": ["Trau\u00b7e", "dem", "Spru\u00b7che", "!", "noch", "nie", "hat", "mich", "der", "F\u00fch\u00b7rer", "ge\u00b7t\u00e4uscht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "ADV", "ADV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Von der Menschheit \u2013 du kannst von ihr nie gro\u00df genug denken,", "tokens": ["Von", "der", "Menschheit", "\u2013", "du", "kannst", "von", "ihr", "nie", "gro\u00df", "ge\u00b7nug", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$(", "PPER", "VMFIN", "APPR", "PPER", "ADV", "ADJD", "ADV", "VVINF", "$,"], "meter": "--+-+--++-+--", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Wie du im Busen sie tr\u00e4gst, pr\u00e4gst du in Taten sie aus.", "tokens": ["Wie", "du", "im", "Bu\u00b7sen", "sie", "tr\u00e4gst", ",", "pr\u00e4gst", "du", "in", "Ta\u00b7ten", "sie", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "NN", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "APPR", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Auch dem Menschen, der dir im engen Leben begegnet,", "tokens": ["Auch", "dem", "Men\u00b7schen", ",", "der", "dir", "im", "en\u00b7gen", "Le\u00b7ben", "be\u00b7geg\u00b7net", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "PPER", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Reich ihm, wenn er sie mag, freundlich die helfende Hand.", "tokens": ["Reich", "ihm", ",", "wenn", "er", "sie", "mag", ",", "freund\u00b7lich", "die", "hel\u00b7fen\u00b7de", "Hand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KOUS", "PPER", "PPER", "VMFIN", "$,", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "Nur f\u00fcr Regen und Tau und f\u00fcrs Wohl der Menschengeschlechter", "tokens": ["Nur", "f\u00fcr", "Re\u00b7gen", "und", "Tau", "und", "f\u00fcrs", "Wohl", "der", "Men\u00b7schen\u00b7ge\u00b7schlech\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "KON", "APPRART", "NN", "ART", "NN"], "meter": "--+--+-++-+--+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "La\u00df du den Himmel, Freund, sorgen wie gestern so heut.", "tokens": ["La\u00df", "du", "den", "Him\u00b7mel", ",", "Freund", ",", "sor\u00b7gen", "wie", "ge\u00b7stern", "so", "heut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,", "NN", "$,", "VVFIN", "KOKOM", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-++-+-+-+", "measure": "unknown.measure.septa"}}}}}