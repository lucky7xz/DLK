{"dta.poem.23730": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die Flucht aus dem Lager  \n vor Prag.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-200905197682", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Liebster, sprach ich, liebster Amor,", "tokens": ["Liebs\u00b7ter", ",", "sprach", "ich", ",", "liebs\u00b7ter", "A\u00b7mor", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "ADJA", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Komst du ietzt von meinem M\u00e4dchen?", "tokens": ["Komst", "du", "ietzt", "von", "mei\u00b7nem", "M\u00e4d\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber er verschwieg die Antwort,", "tokens": ["A\u00b7ber", "er", "ver\u00b7schwieg", "die", "Ant\u00b7wort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ergrif den Stab im Zelte,", "tokens": ["Und", "er\u00b7grif", "den", "Stab", "im", "Zel\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Der die Leinwand unterst\u00fctzet,", "tokens": ["Der", "die", "Lein\u00b7wand", "un\u00b7ter\u00b7st\u00fct\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und der Stab ward weis wie Silber,", "tokens": ["Und", "der", "Stab", "ward", "weis", "wie", "Sil\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PTKVZ", "KOKOM", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und das Zelt fing an zu fallen,", "tokens": ["Und", "das", "Zelt", "fing", "an", "zu", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und er trieb mich, mit dem Stabe,", "tokens": ["Und", "er", "trieb", "mich", ",", "mit", "dem", "Sta\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Aus dem Zelt und aus dem Lager.", "tokens": ["Aus", "dem", "Zelt", "und", "aus", "dem", "La\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "H\u00e4tten Krieger zugesehen,", "tokens": ["H\u00e4t\u00b7ten", "Krie\u00b7ger", "zu\u00b7ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Als mich Amor, mit dem Stabe,", "tokens": ["Als", "mich", "A\u00b7mor", ",", "mit", "dem", "Sta\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Zornig aus dem Lager iagte;", "tokens": ["Zor\u00b7nig", "aus", "dem", "La\u00b7ger", "i\u00b7ag\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.13": {"text": "O wie h\u00e4tten sie gelachet!", "tokens": ["O", "wie", "h\u00e4t\u00b7ten", "sie", "ge\u00b7la\u00b7chet", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Doch, es l\u00e4\u00dft der Gott der Liebe", "tokens": ["Doch", ",", "es", "l\u00e4\u00dft", "der", "Gott", "der", "Lie\u00b7be"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "PPER", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Sich von keinem Krieger sehen.", "tokens": ["Sich", "von", "kei\u00b7nem", "Krie\u00b7ger", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}