{"dta.poem.16027": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "81.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Unendlich f\u00fchlest du dich in dir selbst, doch endlich", "tokens": ["Un\u00b7end\u00b7lich", "f\u00fch\u00b7lest", "du", "dich", "in", "dir", "selbst", ",", "doch", "end\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PRF", "APPR", "PPER", "ADV", "$,", "ADV", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nach au\u00dfen hin, und bist dir selber unverst\u00e4ndlich.", "tokens": ["Nach", "au\u00b7\u00dfen", "hin", ",", "und", "bist", "dir", "sel\u00b7ber", "un\u00b7ver\u00b7st\u00e4nd\u00b7lich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PTKVZ", "$,", "KON", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Versteh! Unendliches und Endlichs, das dir scheint", "tokens": ["Ver\u00b7steh", "!", "Un\u00b7end\u00b7li\u00b7ches", "und", "End\u00b7lichs", ",", "das", "dir", "scheint"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$.", "ADJA", "KON", "NN", "$,", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So unvereinbar, ist durch Eines doch vereint.", "tokens": ["So", "un\u00b7ver\u00b7ein\u00b7bar", ",", "ist", "durch", "Ei\u00b7nes", "doch", "ver\u00b7eint", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "VAFIN", "APPR", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Du bist ein werdendes, nicht ein gewordnes Ich,", "tokens": ["Du", "bist", "ein", "wer\u00b7den\u00b7des", ",", "nicht", "ein", "ge\u00b7word\u00b7nes", "Ich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PTKNEG", "ART", "ADJA", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und alles Werden ist im Widerspruch mit sich.", "tokens": ["Und", "al\u00b7les", "Wer\u00b7den", "ist", "im", "Wi\u00b7der\u00b7spruch", "mit", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "VAFIN", "APPRART", "NN", "APPR", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Unendliches, das wird, mu\u00df endlich sich geberden,", "tokens": ["Un\u00b7end\u00b7li\u00b7ches", ",", "das", "wird", ",", "mu\u00df", "end\u00b7lich", "sich", "ge\u00b7ber\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "PDS", "VAFIN", "$,", "VMFIN", "ADV", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Endlichs will, indem es wird, unendlich werden.", "tokens": ["Und", "End\u00b7lichs", "will", ",", "in\u00b7dem", "es", "wird", ",", "un\u00b7end\u00b7lich", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "VMFIN", "$,", "KOUS", "PPER", "VAFIN", "$,", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}