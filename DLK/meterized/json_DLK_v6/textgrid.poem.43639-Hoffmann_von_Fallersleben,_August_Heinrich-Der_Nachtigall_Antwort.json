{"textgrid.poem.43639": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Nachtigall Antwort", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nachtigall, wie sangst du so sch\u00f6n", "tokens": ["Nach\u00b7ti\u00b7gall", ",", "wie", "sangst", "du", "so", "sch\u00f6n"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "VVFIN", "PPER", "ADV", "ADJD"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Vor allen V\u00f6gelein!", "tokens": ["Vor", "al\u00b7len", "V\u00f6\u00b7ge\u00b7lein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nachtigall, wie drang doch dein Lied", "tokens": ["Nach\u00b7ti\u00b7gall", ",", "wie", "drang", "doch", "dein", "Lied"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "In jedes Herz hinein!", "tokens": ["In", "je\u00b7des", "Herz", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wenn du sangest, rief die ganze Welt:", "tokens": ["Wenn", "du", "san\u00b7gest", ",", "rief", "die", "gan\u00b7ze", "Welt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Jetzt mu\u00df es Fr\u00fchling sein.", "tokens": ["Jetzt", "mu\u00df", "es", "Fr\u00fch\u00b7ling", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Nachtigall, wie drang doch dein Lied", "tokens": ["Nach\u00b7ti\u00b7gall", ",", "wie", "drang", "doch", "dein", "Lied"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "In jedes Herz hinein!", "tokens": ["In", "je\u00b7des", "Herz", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nachtigall, was schweigest du nun?", "tokens": ["Nach\u00b7ti\u00b7gall", ",", "was", "schwei\u00b7gest", "du", "nun", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Du sangst so kurze Zeit.", "tokens": ["Du", "sangst", "so", "kur\u00b7ze", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Warum willst du singen nicht mehr?", "tokens": ["Wa\u00b7rum", "willst", "du", "sin\u00b7gen", "nicht", "mehr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Das thut mir gar zu leid.", "tokens": ["Das", "thut", "mir", "gar", "zu", "leid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wenn du sangest, war mein Herz so voll", "tokens": ["Wenn", "du", "san\u00b7gest", ",", "war", "mein", "Herz", "so", "voll"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Von Lust und Fr\u00f6hlichkeit.", "tokens": ["Von", "Lust", "und", "Fr\u00f6h\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Warum willst du singen nicht mehr?", "tokens": ["Wa\u00b7rum", "willst", "du", "sin\u00b7gen", "nicht", "mehr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Das thut mir gar zu leid.", "tokens": ["Das", "thut", "mir", "gar", "zu", "leid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u00bbwenn der Mai, wenn der liebliche Mai", "tokens": ["\u00bb", "wenn", "der", "Mai", ",", "wenn", "der", "lieb\u00b7li\u00b7che", "Mai"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "KOUS", "ART", "NN", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Mit seinen Blumen flieht,", "tokens": ["Mit", "sei\u00b7nen", "Blu\u00b7men", "flieht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ist es mir so eigen ums Herz,", "tokens": ["Ist", "es", "mir", "so", "ei\u00b7gen", "ums", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Wei\u00df nicht, wie mir geschieht.", "tokens": ["Wei\u00df", "nicht", ",", "wie", "mir", "ge\u00b7schieht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wollt' ich singen auch, ich k\u00f6nnt' es nicht,", "tokens": ["Wollt'", "ich", "sin\u00b7gen", "auch", ",", "ich", "k\u00f6nnt'", "es", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "ADV", "$,", "PPER", "VMFIN", "PPER", "PTKNEG", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Mir gelingt kein einzig Lied.", "tokens": ["Mir", "ge\u00b7lingt", "kein", "ein\u00b7zig", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ja es ist mir so eigen ums Herz,", "tokens": ["Ja", "es", "ist", "mir", "so", "ei\u00b7gen", "ums", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VAFIN", "PPER", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.8": {"text": "Wei\u00df nicht, wie mir geschieht.\u00ab", "tokens": ["Wei\u00df", "nicht", ",", "wie", "mir", "ge\u00b7schieht", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}