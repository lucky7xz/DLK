{"textgrid.poem.56203": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "3", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "M\u00f6chte doch einer die F\u00e4uste sich nagen!", "tokens": ["M\u00f6ch\u00b7te", "doch", "ei\u00b7ner", "die", "F\u00e4us\u00b7te", "sich", "na\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PIS", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Also zu jung! nicht stark noch genug!", "tokens": ["Al\u00b7so", "zu", "jung", "!", "nicht", "stark", "noch", "ge\u00b7nug", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKA", "ADJD", "$.", "PTKNEG", "ADJD", "ADV", "ADV", "$."], "meter": "+----+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "H\u00f6ren mu\u00df ich die Trommel schlagen,", "tokens": ["H\u00f6\u00b7ren", "mu\u00df", "ich", "die", "Trom\u00b7mel", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Sehen die andern Waffen tragen,", "tokens": ["Se\u00b7hen", "die", "an\u00b7dern", "Waf\u00b7fen", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Fernab ziehen, verschwinden den Zug.", "tokens": ["Fern\u00b7ab", "zie\u00b7hen", ",", "ver\u00b7schwin\u00b7den", "den", "Zug", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "VVFIN", "ART", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.2": {"line.1": {"text": "H\u00f6ren mu\u00df ich, und ruhig kauern,", "tokens": ["H\u00f6\u00b7ren", "mu\u00df", "ich", ",", "und", "ru\u00b7hig", "kau\u00b7ern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "$,", "KON", "ADJD", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Schelten der Fremden \u00dcbermut;", "tokens": ["Schel\u00b7ten", "der", "Frem\u00b7den", "\u00dc\u00b7ber\u00b7mut", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Sehen die Mutter beten und trauern,", "tokens": ["Se\u00b7hen", "die", "Mut\u00b7ter", "be\u00b7ten", "und", "trau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVINF", "KON", "VVINF", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Aber gefangen in diesen Mauern", "tokens": ["A\u00b7ber", "ge\u00b7fan\u00b7gen", "in", "die\u00b7sen", "Mau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "PDAT", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "K\u00fchlen am Tacitus meine Wut.", "tokens": ["K\u00fch\u00b7len", "am", "Ta\u00b7ci\u00b7tus", "mei\u00b7ne", "Wut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NE", "PPOSAT", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Ziehet, ihr gl\u00fccklichen fr\u00f6hlichen Fechter,", "tokens": ["Zie\u00b7het", ",", "ihr", "gl\u00fcck\u00b7li\u00b7chen", "fr\u00f6h\u00b7li\u00b7chen", "Fech\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Sorget, da\u00df ihr vom Joch uns befreit,", "tokens": ["Sor\u00b7get", ",", "da\u00df", "ihr", "vom", "Joch", "uns", "be\u00b7freit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Aber bestellt mich vertrauend zum W\u00e4chter", "tokens": ["A\u00b7ber", "be\u00b7stellt", "mich", "ver\u00b7trau\u00b7end", "zum", "W\u00e4ch\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "VVPP", "APPRART", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "\u00dcber die k\u00fcnftigen Schergengeschlechter,", "tokens": ["\u00dc\u00b7ber", "die", "k\u00fcnf\u00b7ti\u00b7gen", "Scher\u00b7gen\u00b7ge\u00b7schlech\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.5": {"text": "Einst auch kommen wird meine Zeit.", "tokens": ["Einst", "auch", "kom\u00b7men", "wird", "mei\u00b7ne", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}