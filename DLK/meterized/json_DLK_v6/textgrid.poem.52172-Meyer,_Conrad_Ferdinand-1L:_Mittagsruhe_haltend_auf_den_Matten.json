{"textgrid.poem.52172": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mittagsruhe haltend auf den Matten", "genre": "verse", "period": "N.A.", "pub_year": 1861, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mittagsruhe haltend auf den Matten", "tokens": ["Mit\u00b7tags\u00b7ru\u00b7he", "hal\u00b7tend", "auf", "den", "Mat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "APPR", "ART", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In der morschen Burg gezacktem Schatten,", "tokens": ["In", "der", "mor\u00b7schen", "Burg", "ge\u00b7zack\u00b7tem", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Vor dem T\u00fcrmchen eppich\u00fcbersponnen,", "tokens": ["Vor", "dem", "T\u00fcrm\u00b7chen", "ep\u00b7pich\u00b7\u00fc\u00b7bers\u00b7pon\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Hab ich einen Sommerwunsch gesonnen,", "tokens": ["Hab", "ich", "ei\u00b7nen", "Som\u00b7mer\u00b7wunsch", "ge\u00b7son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "W\u00e4hrend ich ein Eidechsschw\u00e4nzchen blitzen", "tokens": ["W\u00e4h\u00b7rend", "ich", "ein", "Ei\u00b7dechs\u00b7schw\u00e4nz\u00b7chen", "blit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Sah und, husch, verschwinden durch die Ritzen...", "tokens": ["Sah", "und", ",", "husch", ",", "ver\u00b7schwin\u00b7den", "durch", "die", "Rit\u00b7zen", "..."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "$,", "ADJD", "$,", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Wenn es lauschte... wenn es meiner harrte...", "tokens": ["Wenn", "es", "lauschte", "...", "wenn", "es", "mei\u00b7ner", "harr\u00b7te", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$(", "KOUS", "PPER", "PPOSAT", "ADJA", "$("], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Wenn \u2013 das Pf\u00f6rtchen in der Mauer...", "tokens": ["Wenn", "\u2013", "das", "Pf\u00f6rt\u00b7chen", "in", "der", "Mau\u00b7er", "..."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dem Ger\u00e4usche folgend einer Schleppe,", "tokens": ["Dem", "Ge\u00b7r\u00e4u\u00b7sche", "fol\u00b7gend", "ei\u00b7ner", "Schlep\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "F\u00e4nd ich eine schmale Wendeltreppe", "tokens": ["F\u00e4nd", "ich", "ei\u00b7ne", "schma\u00b7le", "Wen\u00b7del\u00b7trep\u00b7pe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und, von leiser Hand emporgeleitet,", "tokens": ["Und", ",", "von", "lei\u00b7ser", "Hand", "em\u00b7por\u00b7ge\u00b7lei\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Droben einen Becher Wein bereitet...", "tokens": ["Dro\u00b7ben", "ei\u00b7nen", "Be\u00b7cher", "Wein", "be\u00b7rei\u00b7tet", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Dann im Erker s\u00e4\u00dfen wir alleine,", "tokens": ["Dann", "im", "Er\u00b7ker", "s\u00e4\u00b7\u00dfen", "wir", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Plauderten von nichts im D\u00e4mmerscheine,", "tokens": ["Plau\u00b7der\u00b7ten", "von", "nichts", "im", "D\u00e4m\u00b7mer\u00b7schei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIS", "APPRART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Bis der Pendel st\u00fcnde, der da tickte,", "tokens": ["Bis", "der", "Pen\u00b7del", "st\u00fcn\u00b7de", ",", "der", "da", "tick\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Und ein blondes Haupt entschlummernd nickte,", "tokens": ["Und", "ein", "blon\u00b7des", "Haupt", "ent\u00b7schlum\u00b7mernd", "nick\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVPP", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Unter seines Lides d\u00fcnner H\u00fclle", "tokens": ["Un\u00b7ter", "sei\u00b7nes", "Li\u00b7des", "d\u00fcn\u00b7ner", "H\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Regte sich des blauen Quelles F\u00fclle...", "tokens": ["Reg\u00b7te", "sich", "des", "blau\u00b7en", "Quel\u00b7les", "F\u00fcl\u00b7le", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Und das unbekannte Antlitz tr\u00fcge", "tokens": ["Und", "das", "un\u00b7be\u00b7kann\u00b7te", "Ant\u00b7litz", "tr\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "\u00c4hnlichkeiten und Geschwisterz\u00fcge", "tokens": ["\u00c4hn\u00b7lich\u00b7kei\u00b7ten", "und", "Ge\u00b7schwis\u00b7ter\u00b7z\u00fc\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "Alles Sch\u00f6nen, was mir je entgegen", "tokens": ["Al\u00b7les", "Sch\u00f6\u00b7nen", ",", "was", "mir", "je", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "PWS", "PPER", "ADV", "PTKVZ"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Trat auf allen meinen Erdewegen...", "tokens": ["Trat", "auf", "al\u00b7len", "mei\u00b7nen", "Er\u00b7de\u00b7we\u00b7gen", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIAT", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "Was ich Tiefstes, Zartestes empfunden,", "tokens": ["Was", "ich", "Tiefs\u00b7tes", ",", "Zar\u00b7tes\u00b7tes", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "NE", "$,", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "W\u00e4r an dieses blonde Haupt gebunden", "tokens": ["W\u00e4r", "an", "die\u00b7ses", "blon\u00b7de", "Haupt", "ge\u00b7bun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PDAT", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "Und in eine Schlummernde vereinigt,", "tokens": ["Und", "in", "ei\u00b7ne", "Schlum\u00b7mern\u00b7de", "ver\u00b7ei\u00b7nigt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "Was mich je beseligt und gepeinigt...", "tokens": ["Was", "mich", "je", "be\u00b7se\u00b7ligt", "und", "ge\u00b7pei\u00b7nigt", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVPP", "KON", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.21": {"text": "Dringend h\u00e4tt es mich emporgerufen", "tokens": ["Drin\u00b7gend", "h\u00e4tt", "es", "mich", "em\u00b7por\u00b7ge\u00b7ru\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PPER", "PRF", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.22": {"text": "Dieser Wendeltreppe Tr\u00fcmmerstufen,", "tokens": ["Die\u00b7ser", "Wen\u00b7del\u00b7trep\u00b7pe", "Tr\u00fcm\u00b7mer\u00b7stu\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.23": {"text": "Da\u00df ich einem ganzen vollen Gl\u00fccke", "tokens": ["Da\u00df", "ich", "ei\u00b7nem", "gan\u00b7zen", "vol\u00b7len", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.24": {"text": "Stillen Ku\u00df auf stumme Lippen dr\u00fccke...", "tokens": ["Stil\u00b7len", "Ku\u00df", "auf", "stum\u00b7me", "Lip\u00b7pen", "dr\u00fc\u00b7cke", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.25": {"text": "Einmal nur in einem Menschenleben \u2013", "tokens": ["Ein\u00b7mal", "nur", "in", "ei\u00b7nem", "Men\u00b7schen\u00b7le\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.26": {"text": "Aber nimmer wird es sich begeben!", "tokens": ["A\u00b7ber", "nim\u00b7mer", "wird", "es", "sich", "be\u00b7ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "PRF", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}