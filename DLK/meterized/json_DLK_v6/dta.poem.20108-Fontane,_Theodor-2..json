{"dta.poem.20108": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Noch eh\u2019 die Sonn\u2019 heraufgezogen,", "tokens": ["Noch", "eh'", "die", "Sonn'", "her\u00b7auf\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sind die Zigeuner ausgeflogen.", "tokens": ["Sind", "die", "Zi\u00b7geu\u00b7ner", "aus\u00b7ge\u00b7flo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Als Kesselflicker, Rattenf\u00e4nger,", "tokens": ["Als", "Kes\u00b7sel\u00b7fli\u00b7cker", ",", "Rat\u00b7ten\u00b7f\u00e4n\u00b7ger", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KOUS", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hanswurst, Prophet und B\u00e4nkels\u00e4nger, \u2014", "tokens": ["Hans\u00b7wurst", ",", "Pro\u00b7phet", "und", "B\u00e4n\u00b7kel\u00b7s\u00e4n\u00b7ger", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "$,", "NN", "KON", "NN", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Eine rechts, der Andere links,", "tokens": ["Der", "Ei\u00b7ne", "rechts", ",", "der", "An\u00b7de\u00b7re", "links", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADV", "$,", "PRELS", "PIS", "ADV", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Zog Alles in die D\u00f6rfer rings.", "tokens": ["Zog", "Al\u00b7les", "in", "die", "D\u00f6r\u00b7fer", "rings", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nur eine Alte, welk und braun,", "tokens": ["Nur", "ei\u00b7ne", "Al\u00b7te", ",", "welk", "und", "braun", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und unerquicklich anzuschaun,", "tokens": ["Und", "un\u00b7er\u00b7quick\u00b7lich", "an\u00b7zu\u00b7schaun", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Auf deren Antlitz, vielerfahren,", "tokens": ["Auf", "de\u00b7ren", "Ant\u00b7litz", ",", "vie\u00b7ler\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Sich List und Herzensg\u00fcte paaren,", "tokens": ["Sich", "List", "und", "Her\u00b7zens\u00b7g\u00fc\u00b7te", "paa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NE", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Sucht noch, mit ihren gelben H\u00e4nden,", "tokens": ["Sucht", "noch", ",", "mit", "ih\u00b7ren", "gel\u00b7ben", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Sch\u00f6n-Sittah\u2019s Anzug zu vollenden.", "tokens": ["Sch\u00f6n\u00b7Sit\u00b7tah's", "An\u00b7zug", "zu", "voll\u00b7en\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Zw\u00f6lf Jahre mocht\u2019 die Kleine z\u00e4hlen,", "tokens": ["Zw\u00f6lf", "Jah\u00b7re", "mocht'", "die", "Klei\u00b7ne", "z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Und w\u00e4hrend das Zigeunerweib", "tokens": ["Und", "w\u00e4h\u00b7rend", "das", "Zi\u00b7geu\u00b7ner\u00b7weib"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Sich eilt ihr schwarzes Haar zu str\u00e4hlen,", "tokens": ["Sich", "eilt", "ihr", "schwar\u00b7zes", "Haar", "zu", "str\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Schwatzt sie zu Sittah\u2019s Zeitvertreib:", "tokens": ["Schwatzt", "sie", "zu", "Sit\u00b7tah's", "Zeit\u00b7ver\u00b7treib", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "\u201edie Flechte noch, \u2014 mein Herzenskind,", "tokens": ["\u201e", "die", "Flech\u00b7te", "noch", ",", "mein", "Her\u00b7zens\u00b7kind", ","], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "ADV", "$,", "$(", "PPOSAT", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.18": {"text": "Dann auf, in\u2019s n\u00e4chste Dorf, geschwind,", "tokens": ["Dann", "auf", ",", "in's", "n\u00e4chs\u00b7te", "Dorf", ",", "ge\u00b7schwind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "APPRART", "ADJA", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Dort mach\u2019, auf jedem Pachterhofe,", "tokens": ["Dort", "mach'", ",", "auf", "je\u00b7dem", "Pach\u00b7ter\u00b7ho\u00b7fe", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Dich flugs an Tochter oder Zofe;", "tokens": ["Dich", "flugs", "an", "Toch\u00b7ter", "o\u00b7der", "Zo\u00b7fe", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Nimm, wenn sich keine Karte fand,", "tokens": ["Nimm", ",", "wenn", "sich", "kei\u00b7ne", "Kar\u00b7te", "fand", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PRF", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Die Heirathslustge bei der Hand,", "tokens": ["Die", "Hei\u00b7raths\u00b7lust\u00b7ge", "bei", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Und sag ihr, noch in diesem Jahre,", "tokens": ["Und", "sag", "ihr", ",", "noch", "in", "die\u00b7sem", "Jah\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "F\u00fchr\u2019 sie der Liebste zum Altare.", "tokens": ["F\u00fchr'", "sie", "der", "Liebs\u00b7te", "zum", "Al\u00b7ta\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Kann sein, es leuchtet ihr nicht ein,", "tokens": ["Kann", "sein", ",", "es", "leuch\u00b7tet", "ihr", "nicht", "ein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VAINF", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Doch denkt sie drum, es ", "tokens": ["Doch", "denkt", "sie", "drum", ",", "es"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "PPER", "PAV", "$,", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.27": {"text": "Vor allen aber achte schlau,", "tokens": ["Vor", "al\u00b7len", "a\u00b7ber", "ach\u00b7te", "schlau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADV", "ADJA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Ob eine junge Pachtersfrau", "tokens": ["Ob", "ei\u00b7ne", "jun\u00b7ge", "Pach\u00b7ters\u00b7frau"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Vielleicht um Kinder, im Gebet", "tokens": ["Viel\u00b7leicht", "um", "Kin\u00b7der", ",", "im", "Ge\u00b7bet"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPR", "NN", "$,", "APPRART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.30": {"text": "Seit lange schon vergeblich fleht, \u2014", "tokens": ["Seit", "lan\u00b7ge", "schon", "ver\u00b7geb\u00b7lich", "fleht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADV", "ADV", "ADJD", "VVFIN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Und Herzchen, hast du ", "tokens": ["Und", "Herz\u00b7chen", ",", "hast", "du"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "NN", "$,", "VAFIN", "PPER"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.32": {"text": "So sag der Aermsten, unumwunden,", "tokens": ["So", "sag", "der", "A\u00b7erms\u00b7ten", ",", "un\u00b7um\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADJD", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.33": {"text": "Da\u00df eh\u2019 der Kuckuk wiederkehre,", "tokens": ["Da\u00df", "eh'", "der", "Kuc\u00b7kuk", "wie\u00b7der\u00b7keh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Ein Kindlein ihr geboren w\u00e4re; \u2014", "tokens": ["Ein", "Kin\u00b7dlein", "ihr", "ge\u00b7bo\u00b7ren", "w\u00e4\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "VAFIN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Sie mag dann sehn ihr Gl\u00fcck zu haschen, \u2014", "tokens": ["Sie", "mag", "dann", "sehn", "ihr", "Gl\u00fcck", "zu", "ha\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVFIN", "PPOSAT", "NN", "PTKZU", "VVINF", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Wir aber kriegen volle Taschen.\u201c", "tokens": ["Wir", "a\u00b7ber", "krie\u00b7gen", "vol\u00b7le", "Ta\u00b7schen", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADV", "VVINF", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Die Alte spricht\u2019s, die Kleine lauscht,", "tokens": ["Die", "Al\u00b7te", "spricht's", ",", "die", "Klei\u00b7ne", "lauscht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Die letzte Flechte wird beendet,", "tokens": ["Die", "letz\u00b7te", "Flech\u00b7te", "wird", "be\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "Und als sie Gru\u00df und Ku\u00df getauscht,", "tokens": ["Und", "als", "sie", "Gru\u00df", "und", "Ku\u00df", "ge\u00b7tauscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Hat Sittah sich in\u2019s Dorf gewendet.", "tokens": ["Hat", "Sit\u00b7tah", "sich", "in's", "Dorf", "ge\u00b7wen\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "PRF", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Ob sie der jungen Pachterfrau", "tokens": ["Ob", "sie", "der", "jun\u00b7gen", "Pach\u00b7ter\u00b7frau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Ihr unfehlbares Schicksal lehrte, \u2014", "tokens": ["Ihr", "un\u00b7fehl\u00b7ba\u00b7res", "Schick\u00b7sal", "lehr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Erfahren hat man\u2019s nie genau;", "tokens": ["Er\u00b7fah\u00b7ren", "hat", "man's", "nie", "ge\u00b7nau", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIS", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Doch als sie Abends heimw\u00e4rts kehrte", "tokens": ["Doch", "als", "sie", "A\u00b7bends", "heim\u00b7w\u00e4rts", "kehr\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "Und dicht an eines Abgrunds Rand,", "tokens": ["Und", "dicht", "an", "ei\u00b7nes", "Ab\u00b7grunds", "Rand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "An dem der schmale Pfad sich wand,", "tokens": ["An", "dem", "der", "schma\u00b7le", "Pfad", "sich", "wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "In heitrem Muth vor\u00fcberschritt, \u2014", "tokens": ["In", "heit\u00b7rem", "Muth", "vor\u00b7\u00fc\u00b7bersc\u00b7hritt", ","], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Nahm sie ein volles T\u00e4schchen mit.", "tokens": ["Nahm", "sie", "ein", "vol\u00b7les", "T\u00e4schchen", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.49": {"text": "Die Dornen hatten sie geritzt,", "tokens": ["Die", "Dor\u00b7nen", "hat\u00b7ten", "sie", "ge\u00b7ritzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Der weite Weg ihr Blut erhitzt,", "tokens": ["Der", "wei\u00b7te", "Weg", "ihr", "Blut", "er\u00b7hitzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Sie h\u00e4tt\u2019 \u2019nen Tag von ihrem Leben", "tokens": ["Sie", "h\u00e4tt'", "'nen", "Tag", "von", "ih\u00b7rem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.52": {"text": "F\u00fcr wenig Wasser hingegeben.", "tokens": ["F\u00fcr", "we\u00b7nig", "Was\u00b7ser", "hin\u00b7ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.53": {"text": "So eilt den Felsweg sie entlang;", "tokens": ["So", "eilt", "den", "Fels\u00b7weg", "sie", "ent\u00b7lang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.54": {"text": "Da fordert schier, am Bergeshang,", "tokens": ["Da", "for\u00b7dert", "schier", ",", "am", "Ber\u00b7ges\u00b7hang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.55": {"text": "Ein Brombeerstrauch mit schwarzen Beeren,", 