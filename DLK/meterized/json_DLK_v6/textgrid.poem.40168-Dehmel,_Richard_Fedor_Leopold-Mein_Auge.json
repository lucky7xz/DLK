{"textgrid.poem.40168": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Mein Auge", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist mein Auge! \u2013 Du durchdringst mich ganz,", "tokens": ["Du", "bist", "mein", "Au\u00b7ge", "!", "\u2013", "Du", "durch\u00b7dringst", "mich", "ganz", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$.", "$(", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "mein ganzes Wesen hast du mir erhellt,", "tokens": ["mein", "gan\u00b7zes", "We\u00b7sen", "hast", "du", "mir", "er\u00b7hellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "mein ganzes Leben du erf\u00fcllt mit Glanz,", "tokens": ["mein", "gan\u00b7zes", "Le\u00b7ben", "du", "er\u00b7f\u00fcllt", "mit", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "mich Strauchelnden auf sichern Pfad gestellt!", "tokens": ["mich", "Strau\u00b7cheln\u00b7den", "auf", "si\u00b7chern", "Pfad", "ge\u00b7stellt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Mein Auge du! \u2013 Wie war ich doch so blind", "tokens": ["Mein", "Au\u00b7ge", "du", "!", "\u2013", "Wie", "war", "ich", "doch", "so", "blind"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PPER", "$.", "$(", "PWAV", "VAFIN", "PPER", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "an Herz und Sinn, eh' ", "tokens": ["an", "Herz", "und", "Sinn", ",", "eh'"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "NN", "KON", "NN", "$,", "KOUS"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "und wie durchstr\u00f6mt mich jetzt so licht, so lind", "tokens": ["und", "wie", "durch\u00b7str\u00f6mt", "mich", "jetzt", "so", "licht", ",", "so", "lind"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PWAV", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "$,", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "verkl\u00e4rt der Abglanz dieser ganzen Welt!", "tokens": ["ver\u00b7kl\u00e4rt", "der", "Ab\u00b7glanz", "die\u00b7ser", "gan\u00b7zen", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}