{"textgrid.poem.34617": {"metadata": {"author": {"name": "Neukirch, Benjamin", "birth": "N.A.", "death": "N.A."}, "title": "Auff den K\u00f6nig in Franckreich/ als er Stra\u00dfburg wegnahm", "genre": "verse", "period": "N.A.", "pub_year": 1697, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Deutschen saget doch zu euren nachbarn nicht/", "tokens": ["Ihr", "Deut\u00b7schen", "sa\u00b7get", "doch", "zu", "eu\u00b7ren", "nach\u00b7barn", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df Franckreichs Ludewig den frieden mit euch bricht/", "tokens": ["Da\u00df", "Fran\u00b7ck\u00b7reichs", "Lu\u00b7de\u00b7wig", "den", "frie\u00b7den", "mit", "euch", "bricht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NE", "ART", "NN", "APPR", "PPER", "VVFIN", "$("], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Indem er Stra\u00dfburg nimmt. Er spricht: Es ist erlogen/", "tokens": ["In\u00b7dem", "er", "Stra\u00df\u00b7burg", "nimmt", ".", "Er", "spricht", ":", "Es", "ist", "er\u00b7lo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "VVFIN", "$.", "PPER", "VVFIN", "$.", "PPER", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich hab euch nicht bekriegt/ ich hab euch nur betrogen.", "tokens": ["Ich", "hab", "euch", "nicht", "be\u00b7kriegt", "/", "ich", "hab", "euch", "nur", "be\u00b7tro\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "VVPP", "$(", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}