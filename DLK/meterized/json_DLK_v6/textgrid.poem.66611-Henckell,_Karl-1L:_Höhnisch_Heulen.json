{"textgrid.poem.66611": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: H\u00f6hnisch Heulen", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6hnisch Heulen", "tokens": ["H\u00f6h\u00b7nisch", "Heu\u00b7len"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Von herben Winden!", "tokens": ["Von", "her\u00b7ben", "Win\u00b7den", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Rauhe Schauer", "tokens": ["Rau\u00b7he", "Schau\u00b7er"], "token_info": ["word", "word"], "pos": ["NN", "NE"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Rieseln durch Mark und Bein.", "tokens": ["Rie\u00b7seln", "durch", "Mark", "und", "Bein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Wirbelnde Bl\u00e4tter", "tokens": ["Wir\u00b7beln\u00b7de", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Von den Linden", "tokens": ["Von", "den", "Lin\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NE"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Schleifen in \u00f6den,", "tokens": ["Schlei\u00b7fen", "in", "\u00f6\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Schl\u00fcpfrigen Schlamm hinein.", "tokens": ["Schl\u00fcpf\u00b7ri\u00b7gen", "Schlamm", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.9": {"text": "Wolken weinen da droben;", "tokens": ["Wol\u00b7ken", "wei\u00b7nen", "da", "dro\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ADV", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.10": {"text": "Pessimistische Z\u00e4hren", "tokens": ["Pes\u00b7si\u00b7mis\u00b7ti\u00b7sche", "Z\u00e4h\u00b7ren"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.11": {"text": "Spritzt mir der Sturm ins Gesicht \u2013", "tokens": ["Spritzt", "mir", "der", "Sturm", "ins", "Ge\u00b7sicht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.12": {"text": "Leben voll Jammer und Schw\u00e4ren!", "tokens": ["Le\u00b7ben", "voll", "Jam\u00b7mer", "und", "Schw\u00e4\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "KON", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.13": {"text": "Trotzig dich wehren!", "tokens": ["Trot\u00b7zig", "dich", "weh\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "K\u00e4mpfend verkl\u00e4ren!", "tokens": ["K\u00e4mp\u00b7fend", "ver\u00b7kl\u00e4\u00b7ren", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.15": {"text": "Lockensch\u00fcttelnd das Haupt erhoben,", "tokens": ["Lo\u00b7cken\u00b7sch\u00fct\u00b7telnd", "das", "Haupt", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "VVPP", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.16": {"text": "Seele voll Licht!", "tokens": ["See\u00b7le", "voll", "Licht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.17": {"text": "Freude geb\u00e4ren!", "tokens": ["Freu\u00b7de", "ge\u00b7b\u00e4\u00b7ren", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.18": {"text": "Modre, vermodre", "tokens": ["Mod\u00b7re", ",", "ver\u00b7mod\u00b7re"], "token_info": ["word", "punct", "word"], "pos": ["NE", "$,", "VVFIN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.19": {"text": "Du nur, du nur im Sumpfe nicht!", "tokens": ["Du", "nur", ",", "du", "nur", "im", "Sump\u00b7fe", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PPER", "ADV", "APPRART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}