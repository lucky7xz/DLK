{"textgrid.poem.46782": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[ich war gewohnt, euch etwas mitzubringen]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich war gewohnt, euch etwas mitzubringen,", "tokens": ["Ich", "war", "ge\u00b7wohnt", ",", "euch", "et\u00b7was", "mit\u00b7zu\u00b7brin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "PPER", "PIS", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und ihr, mitbringen etwas euch zu lassen.", "tokens": ["Und", "ihr", ",", "mit\u00b7brin\u00b7gen", "et\u00b7was", "euch", "zu", "las\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "APPR", "PIS", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So nehmt die Blumen hier, die ersten blassen,", "tokens": ["So", "nehmt", "die", "Blu\u00b7men", "hier", ",", "die", "ers\u00b7ten", "blas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "$,", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die diesem mir betr\u00fcbten Lenz entspringen!", "tokens": ["Die", "die\u00b7sem", "mir", "be\u00b7tr\u00fcb\u00b7ten", "Lenz", "ent\u00b7sprin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wohl pflegtet ihr entgegen sonst zu springen;", "tokens": ["Wohl", "pfleg\u00b7tet", "ihr", "ent\u00b7ge\u00b7gen", "sonst", "zu", "sprin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das, wei\u00df ich, mu\u00df ich diesmal euch erlassen.", "tokens": ["Das", ",", "wei\u00df", "ich", ",", "mu\u00df", "ich", "dies\u00b7mal", "euch", "er\u00b7las\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "VVFIN", "PPER", "$,", "VMFIN", "PPER", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Streckt wenigstens die H\u00e4ndchen aus zum Fassen!", "tokens": ["Streckt", "we\u00b7nigs\u00b7tens", "die", "H\u00e4nd\u00b7chen", "aus", "zum", "Fas\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "K\u00f6nnt ihr sie nicht der schweren Deck' entringen?", "tokens": ["K\u00f6nnt", "ihr", "sie", "nicht", "der", "schwe\u00b7ren", "Deck'", "ent\u00b7rin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PTKNEG", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich will die Gab' euch auf die Decke legen,", "tokens": ["Ich", "will", "die", "Gab'", "euch", "auf", "die", "De\u00b7cke", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und denken doch ihr habt sie angenommen;", "tokens": ["Und", "den\u00b7ken", "doch", "ihr", "habt", "sie", "an\u00b7ge\u00b7nom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "KON", "PPER", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gebt mir ein Zeichen nur, es auszulegen!", "tokens": ["Gebt", "mir", "ein", "Zei\u00b7chen", "nur", ",", "es", "aus\u00b7zu\u00b7le\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "ADV", "$,", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die Sonn' ist hoch in Wolken angeglommen,", "tokens": ["Die", "Sonn'", "ist", "hoch", "in", "Wol\u00b7ken", "an\u00b7ge\u00b7glom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein heller Blick sagt mir von euretwegen,", "tokens": ["Ein", "hel\u00b7ler", "Blick", "sagt", "mir", "von", "eu\u00b7ret\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df ihr der Mutter Gaben hei\u00dft willkommen.", "tokens": ["Da\u00df", "ihr", "der", "Mut\u00b7ter", "Ga\u00b7ben", "hei\u00dft", "will\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}