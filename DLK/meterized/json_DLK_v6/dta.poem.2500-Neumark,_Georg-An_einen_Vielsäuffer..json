{"dta.poem.2500": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "An einen Viels\u00e4uffer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Du prahlst: ich sauffe viel krieg aber keinen Dummel/", "tokens": ["Du", "prahlst", ":", "ich", "sauf\u00b7fe", "viel", "krieg", "a\u00b7ber", "kei\u00b7nen", "Dum\u00b7mel", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "PIAT", "NN", "ADV", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und s\u00f6ff ich auch das Bier bey halb\u2019 und gantzen ein/", "tokens": ["Und", "s\u00f6ff", "ich", "auch", "das", "Bier", "bey", "halb'", "und", "gant\u00b7zen", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "NN", "KON", "ADJA", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So werd\u2019 ich doch nicht voll. H\u00f6r du versossne Hummel/", "tokens": ["So", "werd'", "ich", "doch", "nicht", "voll", ".", "H\u00f6r", "du", "ver\u00b7soss\u00b7ne", "Hum\u00b7mel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PTKNEG", "ADJD", "$.", "NE", "NE", "ADJA", "NN", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Gleich solch\u2019 ein\u2019 Heldenthat kan auch ein grosses", "tokens": ["Gleich", "solch'", "ein'", "Hel\u00b7den\u00b7that", "kan", "auch", "ein", "gros\u00b7ses"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "ART", "NN", "VMFIN", "ADV", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Schwein.", "tokens": ["Schwein", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}}}}}