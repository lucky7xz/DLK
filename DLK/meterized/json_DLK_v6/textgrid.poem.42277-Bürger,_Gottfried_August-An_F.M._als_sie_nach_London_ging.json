{"textgrid.poem.42277": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "An F.M. als sie nach London ging", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K\u00f6nnt' auf v\u00e4terlichen Auen", "tokens": ["K\u00f6nnt'", "auf", "v\u00e4\u00b7ter\u00b7li\u00b7chen", "Au\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ein verk\u00fcmmerter Poet,", "tokens": ["Ein", "ver\u00b7k\u00fcm\u00b7mer\u00b7ter", "Po\u00b7et", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00f6nnt' er dir ein H\u00fcttchen bauen,", "tokens": ["K\u00f6nnt'", "er", "dir", "ein", "H\u00fctt\u00b7chen", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie es vor dem Geist' ihm steht;", "tokens": ["Wie", "es", "vor", "dem", "Geist'", "ihm", "steht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "In der H\u00fctt' ein frohes St\u00fcbchen,", "tokens": ["In", "der", "H\u00fctt'", "ein", "fro\u00b7hes", "St\u00fcb\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gro\u00df genug f\u00fcr Weib und Mann,", "tokens": ["Gro\u00df", "ge\u00b7nug", "f\u00fcr", "Weib", "und", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und zwei M\u00e4dchen, oder B\u00fcbchen,", "tokens": ["Und", "zwei", "M\u00e4d\u00b7chen", ",", "o\u00b7der", "B\u00fcb\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "$,", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die Gott leicht bescheren kann;", "tokens": ["Die", "Gott", "leicht", "be\u00b7sche\u00b7ren", "kann", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "In der Stub' ein n\u00e4hrend Tischchen,", "tokens": ["In", "der", "Stub'", "ein", "n\u00e4h\u00b7rend", "Tischchen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "T\u00e4glich bietend Wein und Brot,", "tokens": ["T\u00e4g\u00b7lich", "bie\u00b7tend", "Wein", "und", "Brot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auch wohl Br\u00e4tchen, oder Fischchen,", "tokens": ["Auch", "wohl", "Br\u00e4t\u00b7chen", ",", "o\u00b7der", "Fischchen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "$,", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Unversalzt durch Schuldennot;", "tokens": ["Un\u00b7ver\u00b7salzt", "durch", "Schul\u00b7den\u00b7not", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Neben an zur Gartenseite", "tokens": ["Ne\u00b7ben", "an", "zur", "Gar\u00b7ten\u00b7sei\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ein vertrautes K\u00e4mmerlein,", "tokens": ["Ein", "ver\u00b7trau\u00b7tes", "K\u00e4m\u00b7mer\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "D'rin ein Bett', an L\u00e4ng' und Breite,", "tokens": ["D'\u00b7rin", "ein", "Bett'", ",", "an", "L\u00e4ng'", "und", "Brei\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr ein P\u00e4rchen nicht zu klein,", "tokens": ["F\u00fcr", "ein", "P\u00e4r\u00b7chen", "nicht", "zu", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKNEG", "PTKA", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wo du gern hinein dich bettest,", "tokens": ["Wo", "du", "gern", "hin\u00b7ein", "dich", "bet\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "PPER", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Wo du ruhest, weich und warm,", "tokens": ["Wo", "du", "ru\u00b7hest", ",", "weich", "und", "warm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit dem Mann, den du gern h\u00e4ttest,", "tokens": ["Mit", "dem", "Mann", ",", "den", "du", "gern", "h\u00e4t\u00b7test", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fest verschlungen Arm in Arm;", "tokens": ["Fest", "ver\u00b7schlun\u00b7gen", "Arm", "in", "Arm", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "K\u00f6nnte das, mein gutes M\u00e4dchen,", "tokens": ["K\u00f6nn\u00b7te", "das", ",", "mein", "gu\u00b7tes", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDS", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ein verarmter Leiermann,", "tokens": ["Ein", "ver\u00b7arm\u00b7ter", "Lei\u00b7er\u00b7mann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der nur auf dies Spinnenf\u00e4dchen", "tokens": ["Der", "nur", "auf", "dies", "Spin\u00b7nen\u00b7f\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "PDS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wunschkorallen reihen kann:", "tokens": ["Wunschko\u00b7ral\u00b7len", "rei\u00b7hen", "kann", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Heut noch br\u00e4cht' er froh den Schl\u00fcssel", "tokens": ["Heut", "noch", "br\u00e4cht'", "er", "froh", "den", "Schl\u00fcs\u00b7sel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADJD", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dir zu Stub' und K\u00e4mmerlein,", "tokens": ["Dir", "zu", "Stub'", "und", "K\u00e4m\u00b7mer\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fchrte dich zu Krug und Sch\u00fcssel,", "tokens": ["F\u00fchr\u00b7te", "dich", "zu", "Krug", "und", "Sch\u00fcs\u00b7sel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Spr\u00e4che: \u00bbBleib, denn dies ist dein!\u00ab", "tokens": ["Spr\u00e4\u00b7che", ":", "\u00bb", "Bleib", ",", "denn", "dies", "ist", "dein", "!", "\u00ab"], "token_info": ["word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$.", "$(", "NN", "$,", "KON", "PDS", "VAFIN", "PPOSAT", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "\u00bbbleib, w\u00fcrd' er ins Ohr dir raunen,", "tokens": ["\u00bb", "bleib", ",", "w\u00fcrd'", "er", "ins", "Ohr", "dir", "rau\u00b7nen", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "VAFIN", "PPER", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hier ist gut und besser sein,", "tokens": ["Hier", "ist", "gut", "und", "bes\u00b7ser", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "KON", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als sich mit des Hofes Launen", "tokens": ["Als", "sich", "mit", "des", "Ho\u00b7fes", "Lau\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu St. James herum kastein.\u00ab \u2013", "tokens": ["Zu", "St.", "Ja\u00b7mes", "he\u00b7rum", "kas\u00b7tein", ".", "\u00ab", "\u2013"], "token_info": ["word", "abbreviation", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "NE", "NE", "APZR", "PTKVZ", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Aber ach! durch Sturm und Regen", "tokens": ["A\u00b7ber", "ach", "!", "durch", "Sturm", "und", "Re\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$.", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mu\u00df er fort dich wandern sehn;", "tokens": ["Mu\u00df", "er", "fort", "dich", "wan\u00b7dern", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKVZ", "PPER", "VVFIN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nichts kann er als Gottes Segen", "tokens": ["Nichts", "kann", "er", "als", "Got\u00b7tes", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "PPER", "KOUS", "NN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Zum Begleiter dir erflehn.", "tokens": ["Zum", "Be\u00b7glei\u00b7ter", "dir", "er\u00b7flehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}