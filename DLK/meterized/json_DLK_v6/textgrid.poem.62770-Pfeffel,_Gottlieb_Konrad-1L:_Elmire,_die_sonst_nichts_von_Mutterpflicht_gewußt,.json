{"textgrid.poem.62770": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Elmire, die sonst nichts von Mutterpflicht gewu\u00dft,", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Elmire, die sonst nichts von Mutterpflicht gewu\u00dft,", "tokens": ["El\u00b7mi\u00b7re", ",", "die", "sonst", "nichts", "von", "Mut\u00b7ter\u00b7pflicht", "ge\u00b7wu\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ADV", "PIS", "APPR", "NN", "VVPP", "$,"], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Zu vornehm, etwas mehr als ihren Mops zu lieben,", "tokens": ["Zu", "vor\u00b7nehm", ",", "et\u00b7was", "mehr", "als", "ih\u00b7ren", "Mops", "zu", "lie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "$,", "ADV", "PIAT", "KOKOM", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Stillt ihren Benjamin an ihrer eignen Brust.", "tokens": ["Stillt", "ih\u00b7ren", "Ben\u00b7ja\u00b7min", "an", "ih\u00b7rer", "eig\u00b7nen", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vielleicht hat ihm der Arzt die Eselsmilch verschrieben.", "tokens": ["Viel\u00b7leicht", "hat", "ihm", "der", "Arzt", "die", "E\u00b7sels\u00b7milch", "ver\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "ART", "NN", "VVINF", "$."], "mete