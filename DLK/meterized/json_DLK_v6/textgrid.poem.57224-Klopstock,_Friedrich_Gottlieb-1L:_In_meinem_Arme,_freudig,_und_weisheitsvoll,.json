{"textgrid.poem.57224": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: In meinem Arme, freudig, und weisheitsvoll,", "genre": "verse", "period": "N.A.", "pub_year": 1763, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In meinem Arme, freudig, und weisheitsvoll,", "tokens": ["In", "mei\u00b7nem", "Ar\u00b7me", ",", "freu\u00b7dig", ",", "und", "weis\u00b7heits\u00b7voll", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADJD", "$,", "KON", "ADJD", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sang Ebert: Evan, Evoe Hagedorn!", "tokens": ["Sang", "E\u00b7bert", ":", "E\u00b7van", ",", "E\u00b7voe", "Ha\u00b7ge\u00b7dorn", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "NE", "$,", "NE", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da tritt er auf dem Rebenlaube", "tokens": ["Da", "tritt", "er", "auf", "dem", "Re\u00b7ben\u00b7lau\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Muthig einher, wie Ly\u00e4us, Zeus Sohn!", "tokens": ["Mut\u00b7hig", "ein\u00b7her", ",", "wie", "Ly\u00b7\u00e4us", ",", "Zeus", "Sohn", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "PWAV", "NE", "$,", "NE", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Mein Herz entgl\u00fchet! herschend und ungest\u00fcm", "tokens": ["Mein", "Herz", "ent\u00b7gl\u00fc\u00b7het", "!", "her\u00b7schend", "und", "un\u00b7ge\u00b7st\u00fcm"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "$.", "VVPP", "KON", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Bebt mir die Freude durch mein Gebein dahin!", "tokens": ["Bebt", "mir", "die", "Freu\u00b7de", "durch", "mein", "Ge\u00b7bein", "da\u00b7hin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "PAV", "$."], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Evan, mit deinem Weinlaubstabe", "tokens": ["E\u00b7van", ",", "mit", "dei\u00b7nem", "Wein\u00b7laub\u00b7sta\u00b7be"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schone mit deiner gef\u00fcllten Schale!", "tokens": ["Scho\u00b7ne", "mit", "dei\u00b7ner", "ge\u00b7f\u00fcll\u00b7ten", "Scha\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Ihn deckt' als J\u00fcngling eine Ly\u00e4erin,", "tokens": ["Ihn", "deckt'", "als", "J\u00fcng\u00b7ling", "ei\u00b7ne", "Ly\u00b7\u00e4\u00b7e\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "NN", "ART", "NN", "$,"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Nicht Orpheus Feindin, weislich mit Reben zu!", "tokens": ["Nicht", "Or\u00b7pheus", "Fein\u00b7din", ",", "weis\u00b7lich", "mit", "Re\u00b7ben", "zu", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NE", "NN", "$,", "ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und diess war allen Wassertrinkern", "tokens": ["Und", "diess", "war", "al\u00b7len", "Was\u00b7ser\u00b7trin\u00b7kern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wundersam, und die in Th\u00e4lern wohnen,", "tokens": ["Wun\u00b7der\u00b7sam", ",", "und", "die", "in", "Th\u00e4\u00b7lern", "woh\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KON", "ART", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "In die des Wassers viel von den H\u00fcgeln her", "tokens": ["In", "die", "des", "Was\u00b7sers", "viel", "von", "den", "H\u00fc\u00b7geln", "her"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "NN", "ADV", "APPR", "ART", "NN", "APZR"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "St\u00fcrzt, und kein Weinberg l\u00e4ngere Schatten streckt.", "tokens": ["St\u00fcrzt", ",", "und", "kein", "Wein\u00b7berg", "l\u00e4n\u00b7ge\u00b7re", "Schat\u00b7ten", "streckt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "PIAT", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "So schlief er, keinen Schw\u00e4tzer f\u00fcrchtend,", "tokens": ["So", "schlief", "er", ",", "kei\u00b7nen", "Schw\u00e4t\u00b7zer", "f\u00fcrch\u00b7tend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht ohne G\u00f6tter, ein k\u00fchner J\u00fcngling.", "tokens": ["Nicht", "oh\u00b7ne", "G\u00f6t\u00b7ter", ",", "ein", "k\u00fch\u00b7ner", "J\u00fcng\u00b7ling", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Mit seinem Lorber hat dir auch Patareus", "tokens": ["Mit", "sei\u00b7nem", "Lor\u00b7ber", "hat", "dir", "auch", "Pa\u00b7ta\u00b7reus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPER", "ADV", "NE"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und eingeflochtner Myrte das Haupt umkr\u00e4nzt!", "tokens": ["Und", "ein\u00b7ge\u00b7flocht\u00b7ner", "Myr\u00b7te", "das", "Haupt", "um\u00b7kr\u00e4nzt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wie Pfeile von dem goldnen K\u00f6cher,", "tokens": ["Wie", "Pfei\u00b7le", "von", "dem", "gold\u00b7nen", "K\u00f6\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "T\u00f6net dein Lied, wie des J\u00fcnglings Pfeile", "tokens": ["T\u00f6\u00b7net", "dein", "Lied", ",", "wie", "des", "J\u00fcng\u00b7lings", "Pfei\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Schnellrauschend klangen, da der Unsterbliche", "tokens": ["Schnell\u00b7rau\u00b7schend", "klan\u00b7gen", ",", "da", "der", "U\u00b7nsterb\u00b7li\u00b7che"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+---", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Nach Peneus Tochter durch die Gefilde flog!", "tokens": ["Nach", "Pe\u00b7neus", "Toch\u00b7ter", "durch", "die", "Ge\u00b7fil\u00b7de", "flog", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Oft wie des Satyrs Hohngel\u00e4chter,", "tokens": ["Oft", "wie", "des", "Sa\u00b7tyrs", "Hohn\u00b7ge\u00b7l\u00e4ch\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als er den Wald noch nicht laut durchlachte.", "tokens": ["Als", "er", "den", "Wald", "noch", "nicht", "laut", "durch\u00b7lach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "PTKNEG", "ADJD", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Zu Wein und Liedern w\u00e4hnen die Thoren dich", "tokens": ["Zu", "Wein", "und", "Lie\u00b7dern", "w\u00e4h\u00b7nen", "die", "Tho\u00b7ren", "dich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "ART", "NN", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Allein geschaffen. Denn den Unwissenden", "tokens": ["Al\u00b7lein", "ge\u00b7schaf\u00b7fen", ".", "Denn", "den", "Un\u00b7wis\u00b7sen\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVPP", "$.", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hat, was das Herz der Edlen hebet,", "tokens": ["Hat", ",", "was", "das", "Herz", "der", "Ed\u00b7len", "he\u00b7bet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PRELS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Stets sich in d\u00e4mmernder Fern' verloren!", "tokens": ["Stets", "sich", "in", "d\u00e4m\u00b7mern\u00b7der", "Fern'", "ver\u00b7lo\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Dir schl\u00e4gt ein m\u00e4nnlich Herz auch! Dein Leben t\u00f6nt", "tokens": ["Dir", "schl\u00e4gt", "ein", "m\u00e4nn\u00b7lich", "Herz", "auch", "!", "Dein", "Le\u00b7ben", "t\u00f6nt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "NN", "ADV", "$.", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mehr Harmonieen, als ein unsterblich Lied!", "tokens": ["Mehr", "Har\u00b7mo\u00b7ni\u00b7e\u00b7en", ",", "als", "ein", "uns\u00b7terb\u00b7lich", "Lied", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "KOUS", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In unsokratischem Jahrhundert", "tokens": ["In", "un\u00b7so\u00b7kra\u00b7ti\u00b7schem", "Jahr\u00b7hun\u00b7dert"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bist du f\u00fcr wenige Freund' ein Muster!", "tokens": ["Bist", "du", "f\u00fcr", "we\u00b7ni\u00b7ge", "Freund'", "ein", "Mus\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PIAT", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}