{"textgrid.poem.59866": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "30.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ich der Weisheit nachgestrebt,", "tokens": ["Als", "ich", "der", "Weis\u00b7heit", "nach\u00b7ge\u00b7strebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kam ich den Toren t\u00f6richt vor \u2013", "tokens": ["Kam", "ich", "den", "To\u00b7ren", "t\u00f6\u00b7richt", "vor", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "NN", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und klug, da ich wie sie gelebt \u2013", "tokens": ["Und", "klug", ",", "da", "ich", "wie", "sie", "ge\u00b7lebt", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "KOUS", "PPER", "PWAV", "PPER", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr weise h\u00e4lt sich nur der Tor!", "tokens": ["F\u00fcr", "wei\u00b7se", "h\u00e4lt", "sich", "nur", "der", "Tor", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VVFIN", "PRF", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}