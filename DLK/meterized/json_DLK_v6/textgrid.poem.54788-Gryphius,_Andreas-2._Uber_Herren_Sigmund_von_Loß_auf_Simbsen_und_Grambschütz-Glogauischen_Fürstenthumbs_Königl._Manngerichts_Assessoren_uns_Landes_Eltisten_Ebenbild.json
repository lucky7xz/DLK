{"textgrid.poem.54788": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "2. Uber Herren Sigmund von Lo\u00df auf Simbsen und Grambsch\u00fctz-Glogauischen F\u00fcrstenthumbs K\u00f6nigl. Manngerichts Assessoren uns Landes Eltisten Ebenbild", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der sich der Redligkeit hat Lebenslang beflissen/", "tokens": ["Der", "sich", "der", "Red\u00b7lig\u00b7keit", "hat", "Le\u00b7bens\u00b7lang", "be\u00b7flis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ART", "NN", "VAFIN", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der dises Land erhilt durch Wissen und Gewissen/", "tokens": ["Der", "di\u00b7ses", "Land", "er\u00b7hilt", "durch", "Wis\u00b7sen", "und", "Ge\u00b7wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Eilt als hier alles Fill zu Gott sich zu erheben", "tokens": ["Eilt", "als", "hier", "al\u00b7les", "Fill", "zu", "Gott", "sich", "zu", "er\u00b7he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KOKOM", "ADV", "PIAT", "NN", "APPR", "NN", "PRF", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du/ denck an seinen Todt/ und folge seinem Leben.", "tokens": ["Du", "/", "denck", "an", "sei\u00b7nen", "Todt", "/", "und", "fol\u00b7ge", "sei\u00b7nem", "Le\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "pun