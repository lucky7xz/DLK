{"dta.poem.19239": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "86.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Herz, sieh an den Baum in seiner Bl\u00fctenpracht;", "tokens": ["Mein", "Herz", ",", "sieh", "an", "den", "Baum", "in", "sei\u00b7ner", "Bl\u00fc\u00b7ten\u00b7pracht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVIMP", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es wird ihm gar nicht schwer, was ihn so herrlich macht.", "tokens": ["Es", "wird", "ihm", "gar", "nicht", "schwer", ",", "was", "ihn", "so", "herr\u00b7lich", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PTKNEG", "ADJD", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Aus seinem Innern scheint, er braucht sich nicht zu zwingen,", "tokens": ["Aus", "sei\u00b7nem", "In\u00b7nern", "scheint", ",", "er", "braucht", "sich", "nicht", "zu", "zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,", "PPER", "VVFIN", "PRF", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Strom von Lust und Licht und Liebe zu entspringen.", "tokens": ["Ein", "Strom", "von", "Lust", "und", "Licht", "und", "Lie\u00b7be", "zu", "ent\u00b7sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Mit M\u00fche ringt er nicht, das Einzle zu geb\u00e4ren;", "tokens": ["Mit", "M\u00fc\u00b7he", "ringt", "er", "nicht", ",", "das", "Einz\u00b7le", "zu", "ge\u00b7b\u00e4\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PTKNEG", "$,", "PRELS", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Ganze lebt und wirkt, er l\u00e4sset es gew\u00e4hren.", "tokens": ["Das", "Gan\u00b7ze", "lebt", "und", "wirkt", ",", "er", "l\u00e4s\u00b7set", "es", "ge\u00b7w\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Du solltest deine Pflicht, wie er die seine, thun,", "tokens": ["Du", "soll\u00b7test", "dei\u00b7ne", "Pflicht", ",", "wie", "er", "die", "sei\u00b7ne", ",", "thun", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "$,", "PWAV", "PPER", "ART", "PPOSAT", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dann w\u00e4rest du so licht, und bist so tr\u00fcbe nun.", "tokens": ["Dann", "w\u00e4\u00b7rest", "du", "so", "licht", ",", "und", "bist", "so", "tr\u00fc\u00b7be", "nun", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "$,", "KON", "VAFIN", "ADV", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}