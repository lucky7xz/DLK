{"textgrid.poem.63951": {"metadata": {"author": {"name": "Zachari\u00e4, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mach' dich mit leerem Stolz nicht breit,", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mach' dich mit leerem Stolz nicht breit,", "tokens": ["Mach'", "dich", "mit", "lee\u00b7rem", "Stolz", "nicht", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Man lacht nur deiner Eitelkeit.", "tokens": ["Man", "lacht", "nur", "dei\u00b7ner", "Ei\u00b7tel\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Vier Pferde zogen einen Wagen,", "tokens": ["Vier", "Pfer\u00b7de", "zo\u00b7gen", "ei\u00b7nen", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und lie\u00dfen in dem schnellen Jagen", "tokens": ["Und", "lie\u00b7\u00dfen", "in", "dem", "schnel\u00b7len", "Ja\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gar einen gro\u00dfen Staub zur\u00fcck.", "tokens": ["Gar", "ei\u00b7nen", "gro\u00b7\u00dfen", "Staub", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es schwang sich in dem Augenblick", "tokens": ["Es", "schwang", "sich", "in", "dem", "Au\u00b7gen\u00b7blick"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auch eine Fliege mit hinauf,", "tokens": ["Auch", "ei\u00b7ne", "Flie\u00b7ge", "mit", "hin\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und rufte bei des Wagens Lauf:", "tokens": ["Und", "ruf\u00b7te", "bei", "des", "Wa\u00b7gens", "Lauf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ihr guten Leute, gebt doch Acht,", "tokens": ["Ihr", "gu\u00b7ten", "Leu\u00b7te", ",", "gebt", "doch", "Acht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VVFIN", "ADV", "CARD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den gro\u00dfen Staub hab' ich gemacht!", "tokens": ["Den", "gro\u00b7\u00dfen", "Staub", "hab'", "ich", "ge\u00b7macht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}