{"textgrid.poem.41636": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Geschirmt von schwarzen Eibenb\u00e4umen,", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geschirmt von schwarzen Eibenb\u00e4umen,", "tokens": ["Ge\u00b7schirmt", "von", "schwar\u00b7zen", "Ei\u00b7ben\u00b7b\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sitzt stumm der Eulen Schwarm gereiht,", "tokens": ["Sitzt", "stumm", "der", "Eu\u00b7len", "Schwarm", "ge\u00b7reiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie fremde G\u00f6tzen grauer Zeit", "tokens": ["Wie", "frem\u00b7de", "G\u00f6t\u00b7zen", "grau\u00b7er", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr rotes Auge gl\u00fcht. Sie tr\u00e4umen.", "tokens": ["Ihr", "ro\u00b7tes", "Au\u00b7ge", "gl\u00fcht", ".", "Sie", "tr\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$.", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So halten sie sich regungslos,", "tokens": ["So", "hal\u00b7ten", "sie", "sich", "re\u00b7gungs\u00b7los", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Bis zu der Stunde still verbleibend,", "tokens": ["Bis", "zu", "der", "Stun\u00b7de", "still", "ver\u00b7blei\u00b7bend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da schr\u00e4gen Sonnenstrahl vertreibend", "tokens": ["Da", "schr\u00e4\u00b7gen", "Son\u00b7nen\u00b7strahl", "ver\u00b7trei\u00b7bend"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Nacht sich breitet; schwarz und gro\u00df.", "tokens": ["Die", "Nacht", "sich", "brei\u00b7tet", ";", "schwarz", "und", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVFIN", "$.", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dem Weisen lehrt die Ruhgeb\u00e4rde,", "tokens": ["Dem", "Wei\u00b7sen", "lehrt", "die", "Ruh\u00b7ge\u00b7b\u00e4r\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df er mit Recht auf dieser Erde", "tokens": ["Da\u00df", "er", "mit", "Recht", "auf", "die\u00b7ser", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "L\u00e4rm und Bewegung f\u00fcrchten mag.", "tokens": ["L\u00e4rm", "und", "Be\u00b7we\u00b7gung", "f\u00fcrch\u00b7ten", "mag."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["NN", "KON", "NN", "VVFIN", "NE"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Den Menschen, den ein Nichts erregte,", "tokens": ["Den", "Men\u00b7schen", ",", "den", "ein", "Nichts", "er\u00b7reg\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Trifft stets der Strafe harter Schlag,", "tokens": ["Trifft", "stets", "der", "Stra\u00b7fe", "har\u00b7ter", "Schlag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Da\u00df er vom Platze sich bewegte.", "tokens": ["Da\u00df", "er", "vom", "Plat\u00b7ze", "sich", "be\u00b7weg\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}