{"textgrid.poem.41321": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Lindor", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du sagst, da\u00df Lindor Daphnen k\u00fc\u00dft,", "tokens": ["Du", "sagst", ",", "da\u00df", "Lin\u00b7dor", "Daph\u00b7nen", "k\u00fc\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "NE", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Allein, du fehlest weit:", "tokens": ["Al\u00b7lein", ",", "du", "feh\u00b7lest", "weit", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denn kein verliebter Sch\u00e4fer ist", "tokens": ["Denn", "kein", "ver\u00b7lieb\u00b7ter", "Sch\u00e4\u00b7fer", "ist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So voll Bescheidenheit.", "tokens": ["So", "voll", "Be\u00b7schei\u00b7den\u00b7heit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Finette, die dir widerspricht,", "tokens": ["Fi\u00b7net\u00b7te", ",", "die", "dir", "wi\u00b7der\u00b7spricht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Macht beider Unschuld kund:", "tokens": ["Macht", "bei\u00b7der", "Un\u00b7schuld", "kund", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Die sch\u00f6ne Daphne k\u00fc\u00dft er nicht,", "tokens": ["Die", "sch\u00f6\u00b7ne", "Daph\u00b7ne", "k\u00fc\u00dft", "er", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Er k\u00fc\u00dft nur ihren Hund.", "tokens": ["Er", "k\u00fc\u00dft", "nur", "ih\u00b7ren", "Hund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}