{"textgrid.poem.26052": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Dein Haar ist mein z\u00e4rtliches Kissen", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und schm\u00fcckt dein Haar meine Kissen,", "tokens": ["Und", "schm\u00fcckt", "dein", "Haar", "mei\u00b7ne", "Kis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wie wird die Welt mir so gut;", "tokens": ["Wie", "wird", "die", "Welt", "mir", "so", "gut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Deinem Haar verschrieb ich mein Blut,", "tokens": ["Dei\u00b7nem", "Haar", "ver\u00b7schrieb", "ich", "mein", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Deinem Haar, das im Dunkel noch lacht,", "tokens": ["Dei\u00b7nem", "Haar", ",", "das", "im", "Dun\u00b7kel", "noch", "lacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "APPRART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Und das der Leidenschaft Geste", "tokens": ["Und", "das", "der", "Lei\u00b7den\u00b7schaft", "Ges\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "ART", "NN", "NN"], "meter": "---+--+-", "measure": "iambic.di.relaxed"}, "line.6": {"text": "Stumm wie das Feuer nachmacht.", "tokens": ["Stumm", "wie", "das", "Feu\u00b7er", "nach\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "VVPP", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Dein Haar schreibt viel brennende Zeilen", "tokens": ["Dein", "Haar", "schreibt", "viel", "bren\u00b7nen\u00b7de", "Zei\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PIAT", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Dein Bett ist der hei\u00dfeste Brief;", "tokens": ["Dein", "Bett", "ist", "der", "hei\u00b7\u00dfes\u00b7te", "Brief", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Dein Haar ist mein z\u00e4rtlichstes Kissen,", "tokens": ["Dein", "Haar", "ist", "mein", "z\u00e4rt\u00b7lichs\u00b7tes", "Kis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Auf dem meine Sehnsucht entschlief.", "tokens": ["Auf", "dem", "mei\u00b7ne", "Sehn\u00b7sucht", "ent\u00b7schlief", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}}}}