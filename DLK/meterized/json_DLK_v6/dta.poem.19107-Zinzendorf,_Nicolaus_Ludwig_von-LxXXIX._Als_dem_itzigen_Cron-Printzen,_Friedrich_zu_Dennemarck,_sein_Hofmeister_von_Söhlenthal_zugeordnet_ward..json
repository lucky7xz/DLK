{"dta.poem.19107": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "LxXXIX.   Als dem itzigen Cron-Printzen,  \n Friedrich zu Dennemarck, sein Hofmeister  \n von S\u00f6hlenthal zugeordnet ward.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Printz! der erstgebohrne Sohn,", "tokens": ["Printz", "!", "der", "erst\u00b7ge\u00b7bohr\u00b7ne", "Sohn", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Des unendlichen Monarchen,", "tokens": ["Des", "un\u00b7end\u00b7li\u00b7chen", "Mon\u00b7ar\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sitzt zur rechten Hand im Thron,", "tokens": ["Sitzt", "zur", "rech\u00b7ten", "Hand", "im", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ist Noah von den Archen,", "tokens": ["Und", "ist", "Noah", "von", "den", "Ar\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NE", "APPR", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Wo die Menschen vor den St\u00fcrmen", "tokens": ["Wo", "die", "Men\u00b7schen", "vor", "den", "St\u00fcr\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dieses Welt-Laufs sicher ruhn;", "tokens": ["Die\u00b7ses", "Welt\u00b7Laufs", "si\u00b7cher", "ruhn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und sein Arm hat Macht zu schirmen", "tokens": ["Und", "sein", "Arm", "hat", "Macht", "zu", "schir\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Alle, die sich zu ihm thun.", "tokens": ["Al\u00b7le", ",", "die", "sich", "zu", "ihm", "thun", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "PRF", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "War er gleich ein Potentat,", "tokens": ["War", "er", "gleich", "ein", "Po\u00b7ten\u00b7tat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der dem ", "tokens": ["Der", "dem"], "token_info": ["word", "word"], "pos": ["ART", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Der im hohen W\u00e4chter Rath,", "tokens": ["Der", "im", "ho\u00b7hen", "W\u00e4ch\u00b7ter", "Rath", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Von Geburt den Zepter f\u00fchrte;", "tokens": ["Von", "Ge\u00b7burt", "den", "Zep\u00b7ter", "f\u00fchr\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch beschlo\u00df der Herr und er,", "tokens": ["Doch", "be\u00b7schlo\u00df", "der", "Herr", "und", "er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "KON", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df der Erb-Herr aller Seelen", "tokens": ["Da\u00df", "der", "Er\u00b7bHerr", "al\u00b7ler", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PIAT", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "Nirgends anders K\u00f6nig w\u00e4r,", "tokens": ["Nir\u00b7gends", "an\u00b7ders", "K\u00f6\u00b7nig", "w\u00e4r", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Als bey denen, die ihn ", "tokens": ["Als", "bey", "de\u00b7nen", ",", "die", "ihn"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "APPR", "PRELS", "$,", "PRELS", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Freude war ihm zugedacht;", "tokens": ["Freu\u00b7de", "war", "ihm", "zu\u00b7ge\u00b7dacht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber ihm gefiel das Leiden;", "tokens": ["A\u00b7ber", "ihm", "ge\u00b7fiel", "das", "Lei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und des ewgen Felsen Pracht", "tokens": ["Und", "des", "ew\u00b7gen", "Fel\u00b7sen", "Pracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Setzt er dran im Thal zu weyden.", "tokens": ["Setzt", "er", "dran", "im", "Thal", "zu", "wey\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Was ein Mensch erfahren kan,", "tokens": ["Was", "ein", "Mensch", "er\u00b7fah\u00b7ren", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df auch einen Stein bewegte,", "tokens": ["Da\u00df", "auch", "ei\u00b7nen", "Stein", "be\u00b7weg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nahm er alles auf und an,", "tokens": ["Nahm", "er", "al\u00b7les", "auf", "und", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Bi\u00df man ihn aufs Creutze legte.", "tokens": ["Bi\u00df", "man", "ihn", "aufs", "Creut\u00b7ze", "leg\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Erb-Printz von der Crone Dan,\n(Der die Cronen von den Reichen,", "tokens": ["Er\u00b7bPrintz", "von", "der", "Cro\u00b7ne", "Dan", ",", "(", "Der", "die", "Cro\u00b7nen", "von", "den", "Rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADV", "$,", "$(", "ART", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Seit den Kindern Canaan", "tokens": ["Seit", "den", "Kin\u00b7dern", "Ca\u00b7naan"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NE"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Alters halb die Segel streichen:)", "tokens": ["Al\u00b7ters", "halb", "die", "Se\u00b7gel", "strei\u00b7chen", ":)"], "token_info": ["word", "word", "word", "word", "word", "emoticon"], "pos": ["NN", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Allzu neu und allzu enge;", "tokens": ["All\u00b7zu", "neu", "und", "all\u00b7zu", "en\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "KON", "PTKA", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Seiner Stimme schw\u00e4chster Thon,", "tokens": ["Sei\u00b7ner", "Stim\u00b7me", "schw\u00e4chs\u00b7ter", "Thon", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Bringt den Abgrund ins Gedr\u00e4nge.", "tokens": ["Bringt", "den", "Ab\u00b7grund", "ins", "Ge\u00b7dr\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Christian, des K\u00f6nigs Sohn,", "tokens": ["Chris\u00b7ti\u00b7an", ",", "des", "K\u00f6\u00b7nigs", "Sohn", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Tr\u00e4get seinen sch\u00f6nen Nahmen:", "tokens": ["Tr\u00e4\u00b7get", "sei\u00b7nen", "sch\u00f6\u00b7nen", "Nah\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und sein Hertze regt sich schon", "tokens": ["Und", "sein", "Hert\u00b7ze", "regt", "sich", "schon"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PRF", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu dem unbekannten Saamen", "tokens": ["Zu", "dem", "un\u00b7be\u00b7kann\u00b7ten", "Saa\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dieses K\u00f6nigs, seines Herrn;", "tokens": ["Die\u00b7ses", "K\u00f6\u00b7nigs", ",", "sei\u00b7nes", "Herrn", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Seine Majest\u00e4tsche Sonne", "tokens": ["Sei\u00b7ne", "Ma\u00b7jes\u00b7t\u00e4t\u00b7sche", "Son\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Neigt sich vor dem Morgen-Stern", "tokens": ["Neigt", "sich", "vor", "dem", "Mor\u00b7gen\u00b7S\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.8": {"text": "Jener Weisen grosser Wonne.", "tokens": ["Je\u00b7ner", "Wei\u00b7sen", "gros\u00b7ser", "Won\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Friedrich, wilst du mit der Zeit,", "tokens": ["Fried\u00b7rich", ",", "wilst", "du", "mit", "der", "Zeit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VMFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn dein Vater Christi Kriege", "tokens": ["Wenn", "dein", "Va\u00b7ter", "Chris\u00b7ti", "Krie\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "NE", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ausgef\u00fchrt zur Ewigkeit,", "tokens": ["Aus\u00b7ge\u00b7f\u00fchrt", "zur", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und so manch Gericht zum Siege,", "tokens": ["Und", "so", "manch", "Ge\u00b7richt", "zum", "Sie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wilst du sein Vollender seyn,", "tokens": ["Wilst", "du", "sein", "Voll\u00b7en\u00b7der", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VAINF", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.6": {"text": "Und am Tempel GOttes bauen?", "tokens": ["Und", "am", "Tem\u00b7pel", "Got\u00b7tes", "bau\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Soll die heilige Gemein", "tokens": ["Soll", "die", "hei\u00b7li\u00b7ge", "Ge\u00b7mein"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Gottes Wunder an dir schauen?", "tokens": ["Got\u00b7tes", "Wun\u00b7der", "an", "dir", "schau\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Dencke deiner Hoheit nicht,", "tokens": ["Den\u00b7cke", "dei\u00b7ner", "Ho\u00b7heit", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Setz die Majest\u00e4t ins Dunckle:", "tokens": ["Setz", "die", "Ma\u00b7jes\u00b7t\u00e4t", "ins", "Dunck\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber in dem wahren Licht", "tokens": ["A\u00b7ber", "in", "dem", "wah\u00b7ren", "Licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Neuer Zeugung, brenn und funckle.", "tokens": ["Neu\u00b7er", "Zeu\u00b7gung", ",", "brenn", "und", "funck\u00b7le", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Liebe nicht die Herrlichkeit,", "tokens": ["Lie\u00b7be", "nicht", "die", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Die ein Harpax nach kan machen,", "tokens": ["Die", "ein", "Har\u00b7pax", "nach", "kan", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NE", "APPR", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn er nicht die Kosten scheut,", "tokens": ["Wenn", "er", "nicht", "die", "Kos\u00b7ten", "scheut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Dr\u00fcber oft die Diener lachen.", "tokens": ["Dr\u00fc\u00b7ber", "oft", "die", "Die\u00b7ner", "la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Setze deinen tapfern Muth", "tokens": ["Set\u00b7ze", "dei\u00b7nen", "tap\u00b7fern", "Muth"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht in Alexanders Thaten,", "tokens": ["Nicht", "in", "A\u00b7lex\u00b7an\u00b7ders", "Tha\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht in abgedrungnes Gut", "tokens": ["Nicht", "in", "ab\u00b7ge\u00b7drung\u00b7nes", "Gut"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Deines Volcks und fremder Staaten.", "tokens": ["Dei\u00b7nes", "Volcks", "und", "frem\u00b7der", "Staa\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Solcher Herrschaft kommt uns f\u00fcr", "tokens": ["Sol\u00b7cher", "Herr\u00b7schaft", "kommt", "uns", "f\u00fcr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "APPR"], "meter": "+-+