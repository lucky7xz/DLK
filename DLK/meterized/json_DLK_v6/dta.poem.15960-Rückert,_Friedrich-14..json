{"dta.poem.15960": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Doppelb\u00fcndelein hat jeder Mann empfangen,", "tokens": ["Ein", "Dop\u00b7pel\u00b7b\u00fcn\u00b7del\u00b7ein", "hat", "je\u00b7der", "Mann", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das er halb vorn herab und halb l\u00e4\u00dft hinten hangen.", "tokens": ["Das", "er", "halb", "vorn", "her\u00b7ab", "und", "halb", "l\u00e4\u00dft", "hin\u00b7ten", "han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADJD", "ADV", "ADV", "KON", "ADJD", "VVFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Fehler tr\u00e4gt er vorn, die seinen N\u00e4chsten schm\u00fccken,", "tokens": ["Die", "Feh\u00b7ler", "tr\u00e4gt", "er", "vorn", ",", "die", "sei\u00b7nen", "N\u00e4chs\u00b7ten", "schm\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,", "PRELS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch seine eigenen sind schwer auf seinem R\u00fccken.", "tokens": ["Doch", "sei\u00b7ne", "ei\u00b7ge\u00b7nen", "sind", "schwer", "auf", "sei\u00b7nem", "R\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "VAFIN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So sieht er immer die der andern, seine nie,", "tokens": ["So", "sieht", "er", "im\u00b7mer", "die", "der", "an\u00b7dern", ",", "sei\u00b7ne", "nie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ART", "ADJA", "$,", "PPOSAT", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Allein es gleicht sich aus: die andern sehen sie.", "tokens": ["Al\u00b7lein", "es", "gleicht", "sich", "aus", ":", "die", "an\u00b7dern", "se\u00b7hen", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PRF", "PTKVZ", "$.", "ART", "ADJA", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}