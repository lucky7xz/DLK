{"textgrid.poem.59777": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "20.", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00e4mst du dich, da\u00df Schelme sind?", "tokens": ["Sch\u00e4mst", "du", "dich", ",", "da\u00df", "Schel\u00b7me", "sind", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "KOUS", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Willst du deutsche Schelme streicheln,", "tokens": ["Willst", "du", "deut\u00b7sche", "Schel\u00b7me", "strei\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die dich dem Aprilenwind", "tokens": ["Die", "dich", "dem", "Ap\u00b7ri\u00b7len\u00b7wind"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PRF", "ART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Gleich mit Wechseln auch umschmeicheln?", "tokens": ["Gleich", "mit", "Wech\u00b7seln", "auch", "um\u00b7schmei\u00b7cheln", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nein, den Handschuh frisch heraus!", "tokens": ["Nein", ",", "den", "Hand\u00b7schuh", "frisch", "he\u00b7raus", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Feig wird, wer den Feigen weichet \u2013", "tokens": ["Feig", "wird", ",", "wer", "den", "Fei\u00b7gen", "wei\u00b7chet", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "PWS", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Lust und Mut w\u00e4chst \u00fcberaus,", "tokens": ["Lust", "und", "Mut", "w\u00e4chst", "\u00fc\u00b7be\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wenn man Schelmenbacken streichet.", "tokens": ["Wenn", "man", "Schel\u00b7men\u00b7ba\u00b7cken", "strei\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}