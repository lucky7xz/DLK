{"textgrid.poem.61729": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Durch die klare Luft im Winde", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch die klare Luft im Winde", "tokens": ["Durch", "die", "kla\u00b7re", "Luft", "im", "Win\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Segeln heut mir die Gedanken,", "tokens": ["Se\u00b7geln", "heut", "mir", "die", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dich, mein hoher Freund, zu gr\u00fc\u00dfen", "tokens": ["Dich", ",", "mein", "ho\u00b7her", "Freund", ",", "zu", "gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "$,", "PPOSAT", "ADJA", "NN", "$,", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ziehn sie nach dem Strand der Oder.", "tokens": ["Ziehn", "sie", "nach", "dem", "Strand", "der", "O\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nicht im engen Krankenzimmer,", "tokens": ["Nicht", "im", "en\u00b7gen", "Kran\u00b7ken\u00b7zim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo ich, ach, dich lie\u00df beim Scheiden,", "tokens": ["Wo", "ich", ",", "ach", ",", "dich", "lie\u00df", "beim", "Schei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "ITJ", "$,", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Im bereiften Winterfroste", "tokens": ["Im", "be\u00b7reif\u00b7ten", "Win\u00b7ter\u00b7fros\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Suchen sie den r\u00fcst'gen Weidmann.", "tokens": ["Su\u00b7chen", "sie", "den", "r\u00fcst'\u00b7gen", "Weid\u00b7mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Frischen Muts und hellen Auges", "tokens": ["Fri\u00b7schen", "Muts", "und", "hel\u00b7len", "Au\u00b7ges"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hoffen sie dich dort zu finden,", "tokens": ["Hof\u00b7fen", "sie", "dich", "dort", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Heiter, wie in jenen Tagen,", "tokens": ["Hei\u00b7ter", ",", "wie", "in", "je\u00b7nen", "Ta\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da du zu Gastein dich sonntest.", "tokens": ["Da", "du", "zu", "Gas\u00b7tein", "dich", "sonn\u00b7test", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Sch\u00f6nes Wildbad! Oft noch steigst du", "tokens": ["Sch\u00f6\u00b7nes", "Wild\u00b7bad", "!", "Oft", "noch", "steigst", "du"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "ADV", "ADV", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vor mir auf; in meine Tr\u00e4ume", "tokens": ["Vor", "mir", "auf", ";", "in", "mei\u00b7ne", "Tr\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPER", "PTKVZ", "$.", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weht es k\u00fchl dann wie Gebirgsluft,", "tokens": ["Weht", "es", "k\u00fchl", "dann", "wie", "Ge\u00b7birgs\u00b7luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ADV", "KOKOM", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Klingt es wie des \u00c4lplers Zither.", "tokens": ["Klingt", "es", "wie", "des", "\u00c4lp\u00b7lers", "Zi\u00b7ther", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wieder dann die schwarzen Tannen", "tokens": ["Wie\u00b7der", "dann", "die", "schwar\u00b7zen", "Tan\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Seh' ich nicken \u00fcberm Abgrund,", "tokens": ["Seh'", "ich", "ni\u00b7cken", "\u00fc\u00b7berm", "Ab\u00b7grund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVINF", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und den Sturzbach durchs Gekl\u00fcft", "tokens": ["Und", "den", "Sturz\u00b7bach", "durchs", "Ge\u00b7kl\u00fcft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00f6r' ich leidenschaftlich brausen.", "tokens": ["H\u00f6r'", "ich", "lei\u00b7den\u00b7schaft\u00b7lich", "brau\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und die himmelhohen W\u00e4nde", "tokens": ["Und", "die", "him\u00b7mel\u00b7ho\u00b7hen", "W\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gipfeln sich vor mir wie Zinnen", "tokens": ["Gip\u00b7feln", "sich", "vor", "mir", "wie", "Zin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "PPER", "KOKOM", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Einer Geisterburg; du trafst", "tokens": ["Ei\u00b7ner", "Geis\u00b7ter\u00b7burg", ";", "du", "trafst"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$.", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dort mit sichrem Blei die Gemse.", "tokens": ["Dort", "mit", "sich\u00b7rem", "Blei", "die", "Gem\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Dann gedenk' ich auch des Tages,", "tokens": ["Dann", "ge\u00b7denk'", "ich", "auch", "des", "Ta\u00b7ges", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da durch Alpenrosenfelder,", "tokens": ["Da", "durch", "Al\u00b7pen\u00b7ro\u00b7sen\u00b7fel\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch Ger\u00f6ll und Schnee wir klommen", "tokens": ["Durch", "Ge\u00b7r\u00f6ll", "und", "Schnee", "wir", "klom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nach des Gamskarkogels Spitze.", "tokens": ["Nach", "des", "Gams\u00b7kar\u00b7ko\u00b7gels", "Spit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "M\u00fchsam war der Pfad; die Pferde", "tokens": ["M\u00fch\u00b7sam", "war", "der", "Pfad", ";", "die", "Pfer\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VAFIN", "ART", "NN", "$.", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stutzten oft am j\u00e4hen Abhang,", "tokens": ["Stutz\u00b7ten", "oft", "am", "j\u00e4\u00b7hen", "Ab\u00b7hang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber droben im kristallnen", "tokens": ["A\u00b7ber", "dro\u00b7ben", "im", "kris\u00b7tall\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mittagsglanze welch ein Ausblick!", "tokens": ["Mit\u00b7tags\u00b7glan\u00b7ze", "welch", "ein", "Aus\u00b7blick", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PWAT", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Um uns her unendlich lag es", "tokens": ["Um", "uns", "her", "un\u00b7end\u00b7lich", "lag", "es"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PRF", "APZR", "ADJD", "VVFIN", "PPER"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Wie ein Meer von Riesenwogen,", "tokens": ["Wie", "ein", "Meer", "von", "Rie\u00b7sen\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jede Wog' ein Bergesgipfel,", "tokens": ["Je\u00b7de", "Wo\u00b7g'", "ein", "Ber\u00b7ges\u00b7gip\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Jeder Woge Schaum Lawinen.", "tokens": ["Je\u00b7der", "Wo\u00b7ge", "Schaum", "La\u00b7wi\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Und du nanntest mir die H\u00f6hen:", "tokens": ["Und", "du", "nann\u00b7test", "mir", "die", "H\u00f6\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Watzmann, Herzog Ernst, Gro\u00dfglockner \u2013", "tokens": ["Watz\u00b7mann", ",", "Her\u00b7zog", "Ernst", ",", "Gro\u00df\u00b7glock\u00b7ner", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NE", "NE", "$,", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch den h\u00f6chsten Berg in \u00d6streich", "tokens": ["Doch", "den", "h\u00f6chs\u00b7ten", "Berg", "in", "\u00d6s\u00b7treich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "NE"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Hab' ich damals nicht gesehen.", "tokens": ["Hab'", "ich", "da\u00b7mals", "nicht", "ge\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Schwarzenberg ist der gehei\u00dfen", "tokens": ["Schwar\u00b7zen\u00b7berg", "ist", "der", "ge\u00b7hei\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ART", "NN"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Und zur Zeit so hoch geworden,", "tokens": ["Und", "zur", "Zeit", "so", "hoch", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ADV", "ADJD", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df er seinen kalten Schatten", "tokens": ["Da\u00df", "er", "sei\u00b7nen", "kal\u00b7ten", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wirft von Wien bis in die Ostsee.", "tokens": ["Wirft", "von", "Wi\u00b7en", "bis", "in", "die", "Ost\u00b7see", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "KON", "APPR", "ART", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.12": {"line.1": {"text": "In dem Schatten dieses Berges", "tokens": ["In", "dem", "Schat\u00b7ten", "die\u00b7ses", "Ber\u00b7ges"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wachsen auch die Zauberst\u00e4be,", "tokens": ["Wach\u00b7sen", "auch", "die", "Zau\u00b7ber\u00b7st\u00e4\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Welche jetzt die Welt regieren", "tokens": ["Wel\u00b7che", "jetzt", "die", "Welt", "re\u00b7gie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und das deutsche Reich insonders.", "tokens": ["Und", "das", "deut\u00b7sche", "Reich", "in\u00b7son\u00b7ders", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Haselst\u00f6cke nennt das Volk sie;", "tokens": ["Ha\u00b7sel\u00b7st\u00f6\u00b7cke", "nennt", "das", "Volk", "sie", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach, von wei\u00dfen Hexenmeistern", "tokens": ["Ach", ",", "von", "wei\u00b7\u00dfen", "He\u00b7xen\u00b7meis\u00b7tern"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nach dem Takt geschwenkt, du glaubst nicht,", "tokens": ["Nach", "dem", "Takt", "ge\u00b7schwenkt", ",", "du", "glaubst", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,", "PPER", "VVFIN", "PTKNEG", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Welche Wunder sie verrichten.", "tokens": ["Wel\u00b7che", "Wun\u00b7der", "sie", "ver\u00b7rich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Blutrot wandeln sie in Schwarzgelb,", "tokens": ["Blut\u00b7rot", "wan\u00b7deln", "sie", "in", "Schwarz\u00b7gelb", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Adler in geduld'ge Spatzen,", "tokens": ["Ad\u00b7ler", "in", "ge\u00b7duld'\u00b7ge", "Spat\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ja, man lernt sogar Geschichte", "tokens": ["Ja", ",", "man", "lernt", "so\u00b7gar", "Ge\u00b7schich\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PIS", "VVFIN", "ADV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und Geographie von ihnen,", "tokens": ["Und", "Geo\u00b7gra\u00b7phie", "von", "ih\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Lernt, da\u00df Slawen stets und Deutsche", "tokens": ["Lernt", ",", "da\u00df", "Sla\u00b7wen", "stets", "und", "Deut\u00b7sche"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "NN", "ADV", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind ein Brudervolk