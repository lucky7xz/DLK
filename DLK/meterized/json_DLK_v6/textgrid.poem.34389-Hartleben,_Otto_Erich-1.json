{"textgrid.poem.34389": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1", "genre": "verse", "period": "N.A.", "pub_year": 1883, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich den Wellenschlag des Meeres h\u00f6re,", "tokens": ["Wenn", "ich", "den", "Wel\u00b7len\u00b7schlag", "des", "Mee\u00b7res", "h\u00f6\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "eint\u00f6nig rauschend, Nachts, in dunkler Stunde,", "tokens": ["ein\u00b7t\u00f6\u00b7nig", "rau\u00b7schend", ",", "Nachts", ",", "in", "dunk\u00b7ler", "Stun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "aufblutet des Gewissens alte Wunde,", "tokens": ["auf\u00b7blu\u00b7tet", "des", "Ge\u00b7wis\u00b7sens", "al\u00b7te", "Wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "so stark ich auch ", "tokens": ["so", "stark", "ich", "auch"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPER", "ADV"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Ich seh ein Weib, geh\u00fcllt in Trauerfl\u00f6re,", "tokens": ["Ich", "seh", "ein", "Weib", ",", "ge\u00b7h\u00fcllt", "in", "Trau\u00b7er\u00b7fl\u00f6\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "das murmelt dumpf mit todesblassem Munde,", "tokens": ["das", "mur\u00b7melt", "dumpf", "mit", "to\u00b7des\u00b7blas\u00b7sem", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "was mich vor Graun erbeben macht, die Kunde,", "tokens": ["was", "mich", "vor", "Graun", "er\u00b7be\u00b7ben", "macht", ",", "die", "Kun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "VVINF", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "dass sie der Schande Fluch im Grabe st\u00f6re.", "tokens": ["dass", "sie", "der", "Schan\u00b7de", "Fluch", "im", "Gra\u00b7be", "st\u00f6\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Weh meinem fiebergluth-durchlohten Hirne!", "tokens": ["Weh", "mei\u00b7nem", "fie\u00b7ber\u00b7gluth\u00b7durch\u00b7loh\u00b7ten", "Hir\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich seh sie winken mir mit schmalen H\u00e4nden \u2013", "tokens": ["Ich", "seh", "sie", "win\u00b7ken", "mir", "mit", "schma\u00b7len", "H\u00e4n\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und kalte Tropfen perlen von der Stirne.", "tokens": ["und", "kal\u00b7te", "Trop\u00b7fen", "per\u00b7len", "von", "der", "Stir\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der Rache Faust seh ich auf mich sie wenden,", "tokens": ["Der", "Ra\u00b7che", "Faust", "seh", "ich", "auf", "mich", "sie", "wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "APPR", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "weil sie durch mich erniedrigt ward zur Dirne \u2013", "tokens": ["weil", "sie", "durch", "mich", "er\u00b7nied\u00b7rigt", "ward", "zur", "Dir\u00b7ne", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVPP", "VAFIN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "in Qualen f\u00fchl ich meine N\u00e4chte enden.", "tokens": ["in", "Qua\u00b7len", "f\u00fchl", "ich", "mei\u00b7ne", "N\u00e4ch\u00b7te", "en\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}