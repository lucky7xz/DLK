{"textgrid.poem.42832": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Geburtstagsgru\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach wie sch\u00f6n, da\u00df Du geboren bist!", "tokens": ["Ach", "wie", "sch\u00f6n", ",", "da\u00df", "Du", "ge\u00b7bo\u00b7ren", "bist", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ADJD", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Gratuliere uns, da\u00df wir Dich haben,", "tokens": ["Gra\u00b7tu\u00b7lie\u00b7re", "uns", ",", "da\u00df", "wir", "Dich", "ha\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KOUS", "PPER", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Da\u00df wir Deines Herzens gute Gaben", "tokens": ["Da\u00df", "wir", "Dei\u00b7nes", "Her\u00b7zens", "gu\u00b7te", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Oft genie\u00dfen d\u00fcrfen ohne List.", "tokens": ["Oft", "ge\u00b7nie\u00b7\u00dfen", "d\u00fcr\u00b7fen", "oh\u00b7ne", "List", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Deine M\u00e4ngel, Deine Fehler sind", "tokens": ["Dei\u00b7ne", "M\u00e4n\u00b7gel", ",", "Dei\u00b7ne", "Feh\u00b7ler", "sind"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "VAFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Gegen das gewogen harmlos klein.", "tokens": ["Ge\u00b7gen", "das", "ge\u00b7wo\u00b7gen", "harm\u00b7los", "klein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJD", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Heut nach vierzig Jahren wirst Du sein:", "tokens": ["Heut", "nach", "vier\u00b7zig", "Jah\u00b7ren", "wirst", "Du", "sein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "CARD", "NN", "VAFIN", "PPER", "VAINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Immer noch ein Geburtstagskind.", "tokens": ["Im\u00b7mer", "noch", "ein", "Ge\u00b7burts\u00b7tags\u00b7kind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$."], "meter": "+-++-+-+", "measure": "unknown.measure.penta"}}, "stanza.3": {"line.1": {"text": "M\u00f6chtest Du: nie lange traurig oder krank", "tokens": ["M\u00f6ch\u00b7test", "Du", ":", "nie", "lan\u00b7ge", "trau\u00b7rig", "o\u00b7der", "krank"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "$.", "ADV", "ADV", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Sein. Und: wenig H\u00e4\u00dfliches erfahren. \u2013", "tokens": ["Sein", ".", "Und", ":", "we\u00b7nig", "H\u00e4\u00df\u00b7li\u00b7ches", "er\u00b7fah\u00b7ren", ".", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "$.", "KON", "$.", "PIAT", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Deinen Eltern sagen wir unseren fr\u00f6hlichen Dank", "tokens": ["Dei\u00b7nen", "El\u00b7tern", "sa\u00b7gen", "wir", "un\u00b7se\u00b7ren", "fr\u00f6h\u00b7li\u00b7chen", "Dank"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+--+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Daf\u00fcr, da\u00df sie Dich gebaren.", "tokens": ["Da\u00b7f\u00fcr", ",", "da\u00df", "sie", "Dich", "ge\u00b7ba\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "KOUS", "PPER", "PRF", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Gott bewinke Dir", "tokens": ["Gott", "be\u00b7win\u00b7ke", "Dir"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVFIN", "PPER"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Alle Deine Schritte;", "tokens": ["Al\u00b7le", "Dei\u00b7ne", "Schrit\u00b7te", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ja, das w\u00fcnschen wir,", "tokens": ["Ja", ",", "das", "w\u00fcn\u00b7schen", "wir", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PDS", "VVFIN", "PPER", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Deine Freunde und darunter (bitte)", "tokens": ["Dei\u00b7ne", "Freun\u00b7de", "und", "da\u00b7run\u00b7ter", "(", "bit\u00b7te", ")"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PAV", "$(", "PTKANT", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Dein", "tokens": ["Dein"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "+", "measure": "single.up"}}}}}