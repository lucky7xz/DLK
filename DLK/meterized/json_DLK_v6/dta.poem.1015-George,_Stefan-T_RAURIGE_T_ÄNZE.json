{"dta.poem.1015": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "T RAURIGE   T \u00c4NZE", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Da vieles wankt und blasst und sinkt und splittert               ", "tokens": ["Da", "vie\u00b7les", "wankt", "und", "blasst", "und", "sinkt", "und", "split\u00b7tert"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erstirbt das lied von dunst und schlaf umflutet               ", "tokens": ["Er\u00b7stirbt", "das", "lied", "von", "dunst", "und", "schlaf", "um\u00b7flu\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJD", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bis j&#228;her stoss das m&#252;rbe laub zerknittert               ", "tokens": ["Bis", "j", "&#228;", "her", "stoss", "das", "m", "&#252;", "rbe", "laub", "zer\u00b7knit\u00b7tert"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word", "XML_entity", "word", "word", "word"], "pos": ["APPR", "NE", "$(", "APZR", "VVFIN", "ART", "NN", "$(", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von ehmals wilde wunde wieder blutet &#8212;             ", "tokens": ["Von", "eh\u00b7mals", "wil\u00b7de", "wun\u00b7de", "wie\u00b7der", "blu\u00b7tet", "&#8212;"], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity"], "pos": ["APPR", "ADV", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Bis pl\u00f6tzlich sonne zuckt aus nassen wettern               ", "tokens": ["Bis", "pl\u00f6tz\u00b7lich", "son\u00b7ne", "zuckt", "aus", "nas\u00b7sen", "wet\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein schwarzer fluss die bleichen felder spreitet               ", "tokens": ["Ein", "schwar\u00b7zer", "fluss", "die", "blei\u00b7chen", "fel\u00b7der", "sprei\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und seltne donner durch die fr\u00f6ste schmettern               ", "tokens": ["Und", "selt\u00b7ne", "don\u00b7ner", "durch", "die", "fr\u00f6s\u00b7te", "schmet\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "APPR", "ART", "ADJA", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es merkt nur in dem zug der grabw&#228;rts gleitet             ", "tokens": ["Es", "merkt", "nur", "in", "dem", "zug", "der", "grabw", "&#228;", "rts", "glei\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "XML_entity", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "ART", "NE", "$(", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die fackeln zwischen den geneigten nacken               ", "tokens": ["Die", "fa\u00b7ckeln", "zwi\u00b7schen", "den", "ge\u00b7neig\u00b7ten", "na\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der kl&#228;nge dr\u00f6hnen aus dem trauerprunke               ", "tokens": ["Der", "kl", "&#228;", "nge", "dr\u00f6h\u00b7nen", "aus", "dem", "trau\u00b7er\u00b7prun\u00b7ke"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und sucht ob unter rauhen leides schlacken               ", "tokens": ["Und", "sucht", "ob", "un\u00b7ter", "rau\u00b7hen", "lei\u00b7des", "schla\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KOUS", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Noch glimme ewig klarer freude funke.             ", "tokens": ["Noch", "glim\u00b7me", "e\u00b7wig", "kla\u00b7rer", "freu\u00b7de", "fun\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}