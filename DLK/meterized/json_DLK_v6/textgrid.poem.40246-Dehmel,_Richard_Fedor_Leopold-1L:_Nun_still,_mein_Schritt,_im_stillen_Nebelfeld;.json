{"textgrid.poem.40246": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nun still, mein Schritt, im stillen Nebelfeld;", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun still, mein Schritt, im stillen Nebelfeld;", "tokens": ["Nun", "still", ",", "mein", "Schritt", ",", "im", "stil\u00b7len", "Ne\u00b7bel\u00b7feld", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "hier r\u00fchrt kein Leben mehr an meine Ruhe,", "tokens": ["hier", "r\u00fchrt", "kein", "Le\u00b7ben", "mehr", "an", "mei\u00b7ne", "Ru\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "hier darf ich f\u00fchlen, da\u00df ich einsam bin.", "tokens": ["hier", "darf", "ich", "f\u00fch\u00b7len", ",", "da\u00df", "ich", "ein\u00b7sam", "bin", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Kein Laut; kein Hauch; der bleiche Abend h\u00e4lt", "tokens": ["Kein", "Laut", ";", "kein", "Hauch", ";", "der", "blei\u00b7che", "A\u00b7bend", "h\u00e4lt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "$.", "PIAT", "NN", "$.", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "im dichten Mantel schwer die Luft gefangen.", "tokens": ["im", "dich\u00b7ten", "Man\u00b7tel", "schwer", "die", "Luft", "ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So thut es wohl dem unbewegten Sinn.", "tokens": ["So", "thut", "es", "wohl", "dem", "un\u00b7be\u00b7weg\u00b7ten", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mein Herz nur h\u00f6r'ich noch; doch kein Verlangen", "tokens": ["Mein", "Herz", "nur", "h\u00f6r'\u00b7ich", "noch", ";", "doch", "kein", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "ADV", "$.", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "nach Leben ist dies Klopfen. Lust und Schmerz", "tokens": ["nach", "Le\u00b7ben", "ist", "dies", "Klop\u00b7fen", ".", "Lust", "und", "Schmerz"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "PDS", "NN", "$.", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "ruhn hinter mir versunken \u2013 gleich zwei St\u00fcrmen,", "tokens": ["ruhn", "hin\u00b7ter", "mir", "ver\u00b7sun\u00b7ken", "\u2013", "gleich", "zwei", "St\u00fcr\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "VVINF", "$(", "ADV", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "die sich umarmen und im Wirbel sterben;", "tokens": ["die", "sich", "um\u00b7ar\u00b7men", "und", "im", "Wir\u00b7bel", "ster\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PRF", "VVINF", "KON", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "was st\u00f6rst du mich, mein allzu lautes Herz!", "tokens": ["was", "st\u00f6rst", "du", "mich", ",", "mein", "all\u00b7zu", "lau\u00b7tes", "Herz", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "$,", "PPOSAT", "PTKA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sie haben Alle nie wie du gef\u00fchlt,", "tokens": ["Sie", "ha\u00b7ben", "Al\u00b7le", "nie", "wie", "du", "ge\u00b7f\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "ADV", "KOKOM", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "wie Du allein; nicht Freund, nicht Weib noch Kind;", "tokens": ["wie", "Du", "al\u00b7lein", ";", "nicht", "Freund", ",", "nicht", "Weib", "noch", "Kind", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "$.", "PTKNEG", "NN", "$,", "PTKNEG", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "sie sind ", "tokens": ["sie", "sind"], "token_info": ["word", "word"], "pos": ["PPER", "VAFIN"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "m\u00fcht sich ein gr\u00fcner Schein im Nebelmeer,", "tokens": ["m\u00fcht", "sich", "ein", "gr\u00fc\u00b7ner", "Schein", "im", "Ne\u00b7bel\u00b7meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "ein Bahnlicht \u2013 sieh: so glimmst auch du im Tr\u00fcben.", "tokens": ["ein", "Bahn\u00b7licht", "\u2013", "sieh", ":", "so", "glimmst", "auch", "du", "im", "Tr\u00fc\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "VVIMP", "$.", "ADV", "VVFIN", "ADV", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Hinaus, hinaus, wo keine Menschen sind!", "tokens": ["Hin\u00b7aus", ",", "hin\u00b7aus", ",", "wo", "kei\u00b7ne", "Men\u00b7schen", "sind", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PTKVZ", "$,", "PWAV", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Was wollt ihr noch? Weiter! auf jenen H\u00fcgel,", "tokens": ["Was", "wollt", "ihr", "noch", "?", "Wei\u00b7ter", "!", "auf", "je\u00b7nen", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "$.", "NN", "$.", "APPR", "PDAT", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "der grau zu Dunkel schwillt; Gesichter, weicht!", "tokens": ["der", "grau", "zu", "Dun\u00b7kel", "schwillt", ";", "Ge\u00b7sich\u00b7ter", ",", "weicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$.", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "sie folgen mir; o h\u00e4tt'ich Fl\u00fcgel.", "tokens": ["sie", "fol\u00b7gen", "mir", ";", "o", "h\u00e4tt'\u00b7ich", "Fl\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "FM", "ADJD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und aus dem bleichen Feld tauchen die Str\u00e4ucher", "tokens": ["Und", "aus", "dem", "blei\u00b7chen", "Feld", "tau\u00b7chen", "die", "Str\u00e4u\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "und sehen zu \u2013 der H\u00fcgel raucht:", "tokens": ["und", "se\u00b7hen", "zu", "\u2013", "der", "H\u00fc\u00b7gel", "raucht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "PTKZU", "$(", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "wie feucht von Schwei\u00df sich starr und breit", "tokens": ["wie", "feucht", "von", "Schwei\u00df", "sich", "starr", "und", "breit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "APPR", "NN", "PRF", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "der Dunstalb an die Brust der Erde saugt.", "tokens": ["der", "Duns\u00b7talb", "an", "die", "Brust", "der", "Er\u00b7de", "saugt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Gesichter, weicht! weicht! Seht mich keuchen!", "tokens": ["Ge\u00b7sich\u00b7ter", ",", "weicht", "!", "weicht", "!", "Seht", "mich", "keu\u00b7chen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$.", "VVFIN", "$.", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Sie folgen mir. Oh Qual der Einsamkeit.", "tokens": ["Sie", "fol\u00b7gen", "mir", ".", "Oh", "Qual", "der", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "XY", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Am Bahndamm niederzittr'ich in den Sand,", "tokens": ["Am", "Bahn\u00b7damm", "nie\u00b7der\u00b7zit\u00b7tr'\u00b7ich", "in", "den", "Sand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "die gl\u00fchende Stirne auf die nasse Schiene:", "tokens": ["die", "gl\u00fc\u00b7hen\u00b7de", "Stir\u00b7ne", "auf", "die", "nas\u00b7se", "Schie\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "o k\u00e4me jetzt das Eisenrad gerannt!", "tokens": ["o", "k\u00e4\u00b7me", "jetzt", "das", "Ei\u00b7sen\u00b7rad", "ge\u00b7rannt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "VVFIN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Kalt fri\u00dft sich mir der blanke Strom ins Mark,", "tokens": ["Kalt", "fri\u00dft", "sich", "mir", "der", "blan\u00b7ke", "Strom", "ins", "Mark", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "PPER", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "die H\u00e4nde pressen wild den harten Reifen \u2013", "tokens": ["die", "H\u00e4n\u00b7de", "pres\u00b7sen", "wild", "den", "har\u00b7ten", "Rei\u00b7fen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "ich kann nicht mehr! Da \u2013\u2013 horch: sei stark:", "tokens": ["ich", "kann", "nicht", "mehr", "!", "Da", "\u2013", "\u2013", "horch", ":", "sei", "stark", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "$.", "KOUS", "$(", "$(", "VVIMP", "$.", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Gellend am Horizont ein hohles Pfeifen,", "tokens": ["Gel\u00b7lend", "am", "Ho\u00b7ri\u00b7zont", "ein", "hoh\u00b7les", "Pfei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "zwei Augen quellen stechend aus der matten", "tokens": ["zwei", "Au\u00b7gen", "quel\u00b7len", "ste\u00b7chend", "aus", "der", "mat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "ADV", "ADJD", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dunstferne, und \u2013 was will der Schatten,", "tokens": ["Dunst\u00b7fer\u00b7ne", ",", "und", "\u2013", "was", "will", "der", "Schat\u00b7ten", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "KON", "$(", "PWS", "VMFIN", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "was dunkelt dort der Erlenbusch?", "tokens": ["was", "dun\u00b7kelt", "dort", "der", "Er\u00b7len\u00b7busch", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Er l\u00f6st sich, kommt; es rei\u00dft mich hoch,", "tokens": ["Er", "l\u00f6st", "sich", ",", "kommt", ";", "es", "rei\u00dft", "mich", "hoch", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "er ist schon nah, ich will's begreifen,", "tokens": ["er", "ist", "schon", "nah", ",", "ich", "will's", "be\u00b7grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "es nimmt Gestalt an, \u2013 Wahnsinn? Da:", "tokens": ["es", "nimmt", "Ge\u00b7stalt", "an", ",", "\u2013", "Wahn\u00b7sinn", "?", "Da", ":"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PTKVZ", "$,", "$(", "NN", "$.", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "den Nebel teilt ein schwarzer Streifen,", "tokens": ["den", "Ne\u00b7bel", "teilt", "ein", "schwar\u00b7zer", "Strei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "mein w\u00fchlender Blick wird still und weit:", "tokens": ["mein", "w\u00fch\u00b7len\u00b7der", "Blick", "wird", "still", "und", "weit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Jubel \u2013 stumm sch\u00fcttelt mich ein Schrei:", "tokens": ["Ju\u00b7bel", "\u2013", "stumm", "sch\u00fct\u00b7telt", "mich", "ein", "Schrei", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ADJD", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Jubel, ein ", "tokens": ["Ju\u00b7bel", ",", "ein"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "ART"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "und knatternd stampft der Dampfzug mir vorbei.", "tokens": ["und", "knat\u00b7ternd", "stampft", "der", "Dampf\u00b7zug", "mir", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}