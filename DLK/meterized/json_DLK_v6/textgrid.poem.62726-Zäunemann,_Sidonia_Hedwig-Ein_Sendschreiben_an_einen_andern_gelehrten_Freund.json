{"textgrid.poem.62726": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Ein Sendschreiben an einen andern gelehrten Freund", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Deine Schrift ist mir angenehm gewesen,", "tokens": ["Dei\u00b7ne", "Schrift", "ist", "mir", "an\u00b7ge\u00b7nehm", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADJD", "VAPP", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Und ich habe deinen Sinn und den Inhalt gerne gelesen.", "tokens": ["Und", "ich", "ha\u00b7be", "dei\u00b7nen", "Sinn", "und", "den", "In\u00b7halt", "ger\u00b7ne", "ge\u00b7le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPOSAT", "NN", "KON", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+--+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Ich gedacht, da ich sie lase: Wer schreibt oder leget mir", "tokens": ["Ich", "ge\u00b7dacht", ",", "da", "ich", "sie", "la\u00b7se", ":", "Wer", "schreibt", "o\u00b7der", "le\u00b7get", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVPP", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$.", "PWS", "VVFIN", "KON", "VVFIN", "PPER"], "meter": "--+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "In der allerersten Zuschrift, hier und dorten Regeln f\u00fcr?", "tokens": ["In", "der", "al\u00b7le\u00b7rers\u00b7ten", "Zu\u00b7schrift", ",", "hier", "und", "dor\u00b7ten", "Re\u00b7geln", "f\u00fcr", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "ADV", "KON", "ADJA", "NN", "APPR", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Nun ergreif ich auch den Kiel, und schreib jetzo, wie ichs meine.", "tokens": ["Nun", "er\u00b7greif", "ich", "auch", "den", "Kiel", ",", "und", "schreib", "jet\u00b7zo", ",", "wie", "ichs", "mei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NE", "$,", "KON", "VVFIN", "ADV", "$,", "PWAV", "PIS", "VVFIN", "$."], "meter": "--+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Bist du himmlisch; so ists gut, bleib darbey, der Nutz ist deine.", "tokens": ["Bist", "du", "himm\u00b7lisch", ";", "so", "ists", "gut", ",", "bleib", "dar\u00b7bey", ",", "der", "Nutz", "ist", "dei\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$.", "ADV", "VAFIN", "ADJD", "$,", "VVFIN", "PAV", "$,", "ART", "NN", "VAFIN", "PPOSAT", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.7": {"text": "Aber woher wei\u00dft du dieses, da\u00df ich noch nicht himmlisch sey?", "tokens": ["A\u00b7ber", "wo\u00b7her", "wei\u00dft", "du", "die\u00b7ses", ",", "da\u00df", "ich", "noch", "nicht", "himm\u00b7lisch", "sey", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "PPER", "PDAT", "$,", "KOUS", "PPER", "ADV", "PTKNEG", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Hast du mir ins Herz gesehen? Ist dein Urteil nicht zu frey?", "tokens": ["Hast", "du", "mir", "ins", "Herz", "ge\u00b7se\u00b7hen", "?", "Ist", "dein", "Ur\u00b7teil", "nicht", "zu", "frey", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "APPRART", "NN", "VVPP", "$.", "VAFIN", "PPOSAT", "NN", "PTKNEG", "PTKA", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Du kennst mich durch Umgang nicht, du hast mich noch nie gesprochen,", "tokens": ["Du", "kennst", "mich", "durch", "Um\u00b7gang", "nicht", ",", "du", "hast", "mich", "noch", "nie", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "PTKNEG", "$,", "PPER", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.10": {"text": "Gleichwohl hast du schon den Stab \u00fcber mich bereits gebrochen.", "tokens": ["Gleich\u00b7wohl", "hast", "du", "schon", "den", "Stab", "\u00fc\u00b7ber", "mich", "be\u00b7reits", "ge\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "APPR", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-++-+-+-+-", "measure": "unknown.measure.octa.plus"}, "line.11": {"text": "Der so Herz als Nieren pr\u00fcfet, und in alle Winkel schaut,", "tokens": ["Der", "so", "Herz", "als", "Nie\u00b7ren", "pr\u00fc\u00b7fet", ",", "und", "in", "al\u00b7le", "Win\u00b7kel", "schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "KOUS", "NN", "VVFIN", "$,", "KON", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Wei\u00df am besten, wer ihm dienet, und sein kleines H\u00e4uflein baut.", "tokens": ["Wei\u00df", "am", "bes\u00b7ten", ",", "wer", "ihm", "die\u00b7net", ",", "und", "sein", "klei\u00b7nes", "H\u00e4uf\u00b7lein", "baut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "$,", "PWS", "PPER", "VVFIN", "$,", "KON", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.13": {"text": "Wilst du meine Fr\u00f6mmigkeit daraus sehen, daraus schliessen,", "tokens": ["Wilst", "du", "mei\u00b7ne", "Fr\u00f6m\u00b7mig\u00b7keit", "da\u00b7raus", "se\u00b7hen", ",", "da\u00b7raus", "schlies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "PAV", "VVINF", "$,", "PAV", "VVINF", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "Weil ich gar kein geistlich St\u00fcck lie\u00df aus meiner Feder fliessen?", "tokens": ["Weil", "ich", "gar", "kein", "geist\u00b7lich", "St\u00fcck", "lie\u00df", "aus", "mei\u00b7ner", "Fe\u00b7der", "flies\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIAT", "ADJD", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Die\u00df beweist noch nicht die Sache. Mein Beruf verlangt di\u00df nicht,", "tokens": ["Die\u00df", "be\u00b7weist", "noch", "nicht", "die", "Sa\u00b7che", ".", "Mein", "Be\u00b7ruf", "ver\u00b7langt", "di\u00df", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PTKNEG", "ART", "NN", "$.", "PPOSAT", "NN", "VVFIN", "PDS", "PTKNEG", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.16": {"text": "Und zum Scheine geistlich schreiben fordert nicht der Christen Pflicht.", "tokens": ["Und", "zum", "Schei\u00b7ne", "geist\u00b7lich", "schrei\u00b7ben", "for\u00b7dert", "nicht", "der", "Chris\u00b7ten", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ADJD", "VVINF", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.17": {"text": "Wei\u00dft du doch nicht wie ich Gott in dem K\u00e4mmerlein verehre,", "tokens": ["Wei\u00dft", "du", "doch", "nicht", "wie", "ich", "Gott", "in", "dem", "K\u00e4m\u00b7mer\u00b7lein", "ver\u00b7eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "PWAV", "PPER", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+++-+-+-+-", "measure": "unknown.measure.octa.plus"}, "line.18": {"text": "Und im Stillen durch den Kiel auch von meines Gottes Ehre,", "tokens": ["Und", "im", "Stil\u00b7len", "durch", "den", "Kiel", "auch", "von", "mei\u00b7nes", "Got\u00b7tes", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "APPR", "ART", "NE", "ADV", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.19": {"text": "Und von seinen Wundern singe? Wenn es Zeit und n\u00f6thig ist,", "tokens": ["Und", "von", "sei\u00b7nen", "Wun\u00b7dern", "sin\u00b7ge", "?", "Wenn", "es", "Zeit", "und", "n\u00f6\u00b7thig", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN", "$.", "KOUS", "PPER", "NN", "KON", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.20": {"text": "Schreib ich nach der Dichter Weise, und auch als ein guter Christ.", "tokens": ["Schreib", "ich", "nach", "der", "Dich\u00b7ter", "Wei\u00b7se", ",", "und", "auch", "als", "ein", "gu\u00b7ter", "Christ", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "NN", "$,", "KON", "ADV", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+--+--+-+", "measure": "trochaic.septa.relaxed"}, "line.21": {"text": "Sieh die Feuer-Ode an, wie ich da die H\u00e4nde falte,", "tokens": ["Sieh", "die", "Feu\u00b7er\u00b7O\u00b7de", "an", ",", "wie", "ich", "da", "die", "H\u00e4n\u00b7de", "fal\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PTKVZ", "$,", "PWAV", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.22": {"text": "Wie ich bete, und zugleich Gott gelassen stille halte.", "tokens": ["Wie", "ich", "be\u00b7te", ",", "und", "zu\u00b7gleich", "Gott", "ge\u00b7las\u00b7sen", "stil\u00b7le", "hal\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "KON", "ADV", "NN", "VVPP", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.23": {"text": "Lie\u00df, betracht mein Bergwerks-Carmen, o! so wirst du mir gestehn,", "tokens": ["Lie\u00df", ",", "be\u00b7tracht", "mein", "Berg\u00b7werks\u00b7Car\u00b7men", ",", "o", "!", "so", "wirst", "du", "mir", "ge\u00b7stehn", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "$,", "FM", "$.", "ADV", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.24": {"text": "Da\u00df hier meine Christen-Pflichten Gott zum Lobe sind geschehn.", "tokens": ["Da\u00df", "hier", "mei\u00b7ne", "Chris\u00b7ten\u00b7Pflich\u00b7ten", "Gott", "zum", "Lo\u00b7be", "sind", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "NN", "APPRART", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.25": {"text": "Liebeslieder hab ich nie aufgesetzt noch abgesungen.", "tokens": ["Lie\u00b7bes\u00b7lie\u00b7der", "hab", "ich", "nie", "auf\u00b7ge\u00b7setzt", "noch", "ab\u00b7ge\u00b7sun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "VVFIN", "ADV", "VVPP", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.26": {"text": "Doch ist mir ein Ehren-St\u00fcck und ein Heldenlied gelungen,", "tokens": ["Doch", "ist", "mir", "ein", "Eh\u00b7ren\u00b7St\u00fcck", "und", "ein", "Hel\u00b7den\u00b7lied", "ge\u00b7lun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ART", "NN", "KON", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.27": {"text": "Darf ich bitten, o! so schicke mir einmahl ein solches Blat,", "tokens": ["Darf", "ich", "bit\u00b7ten", ",", "o", "!", "so", "schi\u00b7cke", "mir", "ein\u00b7mahl", "ein", "sol\u00b7ches", "Blat", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "FM", "$.", "ADV", "VVFIN", "PPER", "ADV", "ART", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.28": {"text": "Das zum Endzweck einen Helden und Regenten vor sich hat.", "tokens": ["Das", "zum", "End\u00b7zweck", "ei\u00b7nen", "Hel\u00b7den", "und", "Re\u00b7gen\u00b7ten", "vor", "sich", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPRART", "NN", "ART", "NN", "KON", "NN", "APPR", "PRF", "VAFIN", "$."], "meter": "--+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.29": {"text": "Dieses weis ich allzuwohl: wenn die grossen Dichter dichten,", "tokens": ["Die\u00b7ses", "weis", "ich", "all\u00b7zu\u00b7wohl", ":", "wenn", "die", "gros\u00b7sen", "Dich\u00b7ter", "dich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKVZ", "PPER", "ADV", "$.", "KOUS", "ART", "ADJA", "NN", "ADJA", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.30": {"text": "Suchen sie zu ihren Ruhm sich ein Denkmaal aufzurichten,", "tokens": ["Su\u00b7chen", "sie", "zu", "ih\u00b7ren", "Ruhm", "sich", "ein", "Denk\u00b7maal", "auf\u00b7zu\u00b7rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "PRF", "ART", "NN", "VVIZU", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.31": {"text": "Und das Werk, die That, das Leben eines Helden mu\u00df der Schein", "tokens": ["Und", "das", "Werk", ",", "die", "That", ",", "das", "Le\u00b7ben", "ei\u00b7nes", "Hel\u00b7den", "mu\u00df", "der", "Schein"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "VMFIN", "ART", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.32": {"text": "Ihrer Ehr- und Lob-Begierde \u00f6fters, wo nicht allzeit seyn.", "tokens": ["Ih\u00b7rer", "Ehr", "und", "Lob\u00b7Be\u00b7gier\u00b7de", "\u00f6f\u00b7ters", ",", "wo", "nicht", "all\u00b7zeit", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "TRUNC", "KON", "NN", "ADV", "$,", "PWAV", "PTKNEG", "ADV", "VAINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.33": {"text": "Doch auf so bescheidne Art, ist die Ehrfurcht zugelassen,", "tokens": ["Doch", "auf", "so", "be\u00b7scheid\u00b7ne", "Art", ",", "ist", "die", "Ehr\u00b7furcht", "zu\u00b7ge\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADV", "ADJA", "NN", "$,", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.34": {"text": "Soll das Pfund vergraben seyn? Wer wird mich deswegen hassen,", "tokens": ["Soll", "das", "Pfund", "ver\u00b7gra\u00b7ben", "seyn", "?", "Wer", "wird", "mich", "des\u00b7we\u00b7gen", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VVPP", "VAINF", "$.", "PWS", "VAFIN", "PRF", "PAV", "VVINF", "$,"], "meter": "+-+-+---+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.35": {"text": "Wenn ich damit Wucher treibe, und darbey in meinem Sinn", "tokens": ["Wenn", "ich", "da\u00b7mit", "Wu\u00b7cher", "trei\u00b7be", ",", "und", "dar\u00b7bey", "in", "mei\u00b7nem", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PAV", "NN", "VVFIN", "$,", "KON", "PAV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.36": {"text": "Mich auf keine Art erhebe, sondern stets bescheiden bin?",