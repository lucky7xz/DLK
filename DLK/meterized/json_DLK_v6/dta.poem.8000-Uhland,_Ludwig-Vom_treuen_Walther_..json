{"dta.poem.8000": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Vom treuen Walther .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Der treue Walther ritt vorbei", "tokens": ["Der", "treu\u00b7e", "Walt\u00b7her", "ritt", "vor\u00b7bei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An unsrer Frau Kapelle.", "tokens": ["An", "uns\u00b7rer", "Frau", "Ka\u00b7pel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da kniete gar in tiefer Reu\u2019", "tokens": ["Da", "knie\u00b7te", "gar", "in", "tie\u00b7fer", "Reu'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein M\u00e4gdlein an der Schwelle.", "tokens": ["Ein", "M\u00e4gd\u00b7lein", "an", "der", "Schwel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u201ehalt an, halt an, mein Walther traut!", "tokens": ["\u201e", "halt", "an", ",", "halt", "an", ",", "mein", "Walt\u00b7her", "traut", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kennst du nicht mehr der Stimme Laut,", "tokens": ["Kennst", "du", "nicht", "mehr", "der", "Stim\u00b7me", "Laut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "ART", "NN", "APPR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die du so gerne h\u00f6rtest?\u201c", "tokens": ["Die", "du", "so", "ger\u00b7ne", "h\u00f6r\u00b7test", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u201ewen seh\u2019 ich hier? Die falsche Maid,", "tokens": ["\u201e", "wen", "seh'", "ich", "hier", "?", "Die", "fal\u00b7sche", "Maid", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "ADV", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ach! weiland, ach, die Meine!", "tokens": ["Ach", "!", "wei\u00b7land", ",", "ach", ",", "die", "Mei\u00b7ne", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "$,", "ITJ", "$,", "PRELS", "PPOSAT", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo liessest du dein seiden Kleid?", "tokens": ["Wo", "lies\u00b7sest", "du", "dein", "sei\u00b7den", "Kleid", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo Gold und Edelsteine?\u201c", "tokens": ["Wo", "Gold", "und", "E\u00b7del\u00b7stei\u00b7ne", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u201eo da\u00df ich von der Treue lie\u00df!", "tokens": ["\u201e", "o", "da\u00df", "ich", "von", "der", "Treu\u00b7e", "lie\u00df", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Verloren ist mein Paradies,", "tokens": ["Ver\u00b7lo\u00b7ren", "ist", "mein", "Pa\u00b7ra\u00b7dies", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Bei dir nur find\u2019 ich\u2019s wieder.\u201c", "tokens": ["Bei", "dir", "nur", "find'", "ich's", "wie\u00b7der", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPER", "ADV", "VVFIN", "PIS", "ADV", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er hub zu Ro\u00df das sch\u00f6ne Weib,", "tokens": ["Er", "hub", "zu", "Ro\u00df", "das", "sch\u00f6\u00b7ne", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er trug ein sanft Erbarmen;", "tokens": ["Er", "trug", "ein", "sanft", "Er\u00b7bar\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie schlang sich fest um seinen Leib", "tokens": ["Sie", "schlang", "sich", "fest", "um", "sei\u00b7nen", "Leib"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit weissen, weichen Armen.", "tokens": ["Mit", "weis\u00b7sen", ",", "wei\u00b7chen", "Ar\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "VVINF", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u201each, Walther traut! mein liebend Herz,", "tokens": ["\u201e", "ach", ",", "Walt\u00b7her", "traut", "!", "mein", "lie\u00b7bend", "Herz", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "NE", "VVFIN", "$.", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es schl\u00e4gt an kaltes, starres Erz,", "tokens": ["Es", "schl\u00e4gt", "an", "kal\u00b7tes", ",", "star\u00b7res", "Erz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es klopft nicht an dem deinen.\u201c", "tokens": ["Es", "klopft", "nicht", "an", "dem", "dei\u00b7nen", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "ART", "PPOSAT", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie ritten ein in Walthers Schlo\u00df,", "tokens": ["Sie", "rit\u00b7ten", "ein", "in", "Walt\u00b7hers", "Schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Schlo\u00df war \u00f6d\u2019 und stille,", "tokens": ["Das", "Schlo\u00df", "war", "\u00f6d'", "und", "stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie band den Helm dem Ritter los;", "tokens": ["Sie", "band", "den", "Helm", "dem", "Rit\u00b7ter", "los", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hin war der Sch\u00f6nheit F\u00fclle.", "tokens": ["Hin", "war", "der", "Sch\u00f6n\u00b7heit", "F\u00fcl\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "\u201edie Wangen bleich, die Augen tr\u00fcb,", "tokens": ["\u201e", "die", "Wan\u00b7gen", "bleich", ",", "die", "Au\u00b7gen", "tr\u00fcb", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "ADJD", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie sind dein Schmuck, du treues Lieb!", "tokens": ["Sie", "sind", "dein", "Schmuck", ",", "du", "treu\u00b7es", "Lieb", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du warst mir nie so lieblich.\u201c", "tokens": ["Du", "warst", "mir", "nie", "so", "lieb\u00b7lich", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die R\u00fcstung l\u00f6st die fromme Maid", "tokens": ["Die", "R\u00fcs\u00b7tung", "l\u00f6st", "die", "from\u00b7me", "Maid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Herrn, den sie betr\u00fcbet.", "tokens": ["Dem", "Herrn", ",", "den", "sie", "be\u00b7tr\u00fc\u00b7bet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u201ewas seh\u2019 ich? ach! ein schwarzes Kleid!", "tokens": ["\u201e", "was", "seh'", "ich", "?", "ach", "!", "ein", "schwar\u00b7zes", "Kleid", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "$.", "XY", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wer starb, den du geliebet?\u201c", "tokens": ["Wer", "starb", ",", "den", "du", "ge\u00b7lie\u00b7bet", "?", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "$,", "PRELS", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u201edie Liebste mein betraur\u2019 ich sehr,", "tokens": ["\u201e", "die", "Liebs\u00b7te", "mein", "be\u00b7traur'", "ich", "sehr", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "PPOSAT", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die ich auf Erden nimmermehr,", "tokens": ["Die", "ich", "auf", "Er\u00b7den", "nim\u00b7mer\u00b7mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Noch \u00fcber\u2019m Grabe finde.\u201c", "tokens": ["Noch", "\u00fc\u00b7ber'm", "Gra\u00b7be", "fin\u00b7de", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Sie sinkt zu seinen F\u00fc\u00dfen hin,", "tokens": ["Sie", "sinkt", "zu", "sei\u00b7nen", "F\u00fc\u00b7\u00dfen", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit ausgestreckten Armen.", "tokens": ["Mit", "aus\u00b7ge\u00b7streck\u00b7ten", "Ar\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u201eda lieg\u2019 ich arme B\u00fc\u00dferin,", "tokens": ["\u201e", "da", "lieg'", "ich", "ar\u00b7me", "B\u00fc\u00b7\u00dfe\u00b7rin", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dich fleh\u2019 ich um Erbarmen.", "tokens": ["Dich", "fleh'", "ich", "um", "Er\u00b7bar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Erhebe mich zu neuer Lust!", "tokens": ["Er\u00b7he\u00b7be", "mich", "zu", "neu\u00b7er", "Lust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "La\u00df mich an deiner treuen Brust", "tokens": ["La\u00df", "mich", "an", "dei\u00b7ner", "treu\u00b7en", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Von allem Leid genesen!\u201c", "tokens": ["Von", "al\u00b7lem", "Leid", "ge\u00b7ne\u00b7sen", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PIS", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "\u201esteh auf, steh auf, du armes Kind!", "tokens": ["\u201e", "steh", "auf", ",", "steh", "auf", ",", "du", "ar\u00b7mes", "Kind", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich kann dich nicht erheben;", "tokens": ["Ich", "kann", "dich", "nicht", "er\u00b7he\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Arme mir verschlossen sind,", "tokens": ["Die", "Ar\u00b7me", "mir", "ver\u00b7schlos\u00b7sen", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Brust ist ohne Leben.", "tokens": ["Die", "Brust", "ist", "oh\u00b7ne", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sey traurig stets, wie ich es bin!", "tokens": ["Sey", "trau\u00b7rig", "stets", ",", "wie", "ich", "es", "bin", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "VVFIN", "$,", "PWAV", "PPER", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Lieb\u2019 ist hin, die Lieb\u2019 ist hin,", "tokens": ["Die", "Lieb'", "ist", "hin", ",", "die", "Lieb'", "ist", "hin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,", "ART", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und kehret niemals wieder.\u201c", "tokens": ["Und", "keh\u00b7ret", "nie\u00b7mals", "wie\u00b7der", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}