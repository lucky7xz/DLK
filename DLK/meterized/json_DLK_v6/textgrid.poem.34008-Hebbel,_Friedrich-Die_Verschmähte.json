{"textgrid.poem.34008": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Verschm\u00e4hte", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du liebst mich nicht! Wie sollt' ich l\u00e4nger leben!", "tokens": ["Du", "liebst", "mich", "nicht", "!", "Wie", "sollt'", "ich", "l\u00e4n\u00b7ger", "le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$.", "PWAV", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Hoffnung, endlich in dein Herz zu dringen,", "tokens": ["Die", "Hoff\u00b7nung", ",", "end\u00b7lich", "in", "dein", "Herz", "zu", "drin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erhielt mich, doch es wird mir nie gelingen!", "tokens": ["Er\u00b7hielt", "mich", ",", "doch", "es", "wird", "mir", "nie", "ge\u00b7lin\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "PPER", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich f\u00fchl's, und dieses mu\u00df den Tod mir geben.", "tokens": ["Ich", "f\u00fchl's", ",", "und", "die\u00b7ses", "mu\u00df", "den", "Tod", "mir", "ge\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "PDS", "VMFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Er naht mir schon, ich seh' ihn ohne Beben,", "tokens": ["Er", "naht", "mir", "schon", ",", "ich", "seh'", "ihn", "oh\u00b7ne", "Be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PPER", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er wird zur\u00fcck mich zu der Mutter bringen;", "tokens": ["Er", "wird", "zu\u00b7r\u00fcck", "mich", "zu", "der", "Mut\u00b7ter", "brin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch kann ich nicht den letzten Schmerz bezwingen,", "tokens": ["Doch", "kann", "ich", "nicht", "den", "letz\u00b7ten", "Schmerz", "be\u00b7zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und mit mir selbst erst wird er ganz verschweben!", "tokens": ["Und", "mit", "mir", "selbst", "erst", "wird", "er", "ganz", "ver\u00b7schwe\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ADV", "ADV", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O, w\u00e4r' ich, statt mit buntem Staub umkleidet,", "tokens": ["O", ",", "w\u00e4r'", "ich", ",", "statt", "mit", "bun\u00b7tem", "Staub", "um\u00b7klei\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VAFIN", "PPER", "$,", "KOUI", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als stummes Traumbild vor dich hingetreten,", "tokens": ["Als", "stum\u00b7mes", "Traum\u00b7bild", "vor", "dich", "hin\u00b7ge\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du h\u00e4ttest hei\u00df das D\u00e4mmernde umschlossen!", "tokens": ["Du", "h\u00e4t\u00b7test", "hei\u00df", "das", "D\u00e4m\u00b7mern\u00b7de", "um\u00b7schlos\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich ward dir dadurch, da\u00df ich war, verleidet,", "tokens": ["Ich", "ward", "dir", "da\u00b7durch", ",", "da\u00df", "ich", "war", ",", "ver\u00b7lei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PAV", "$,", "KOUS", "PPER", "VAFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du h\u00e4ttest sonst mich selbst von Gott erbeten,", "tokens": ["Du", "h\u00e4t\u00b7test", "sonst", "mich", "selbst", "von", "Gott", "er\u00b7be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPER", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und ich in deinem Wunsch mein Gl\u00fcck genossen!", "tokens": ["Und", "ich", "in", "dei\u00b7nem", "Wunsch", "mein", "Gl\u00fcck", "ge\u00b7nos\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}