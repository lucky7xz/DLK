{"textgrid.poem.65769": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "43", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am besten dient mein Auge blinzend mir;", "tokens": ["Am", "bes\u00b7ten", "dient", "mein", "Au\u00b7ge", "blin\u00b7zend", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VVFIN", "PPOSAT", "NN", "ADJD", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Denn unbeachtet geht der Tag an ihm vor\u00fcber:", "tokens": ["Denn", "un\u00b7be\u00b7ach\u00b7tet", "geht", "der", "Tag", "an", "ihm", "vor\u00b7\u00fc\u00b7ber", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Allein im Schlaf, im Traume sieht's nach dir", "tokens": ["Al\u00b7lein", "im", "Schlaf", ",", "im", "Trau\u00b7me", "sieht's", "nach", "dir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "$,", "APPRART", "NN", "NE", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Aus Nacht in Helligkeit, nachthell hin\u00fcber.", "tokens": ["Aus", "Nacht", "in", "Hel\u00b7lig\u00b7keit", ",", "nach\u00b7thell", "hin\u00b7\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$,", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Du, dessen Schatten nun die Schatten so erhellt,", "tokens": ["Du", ",", "des\u00b7sen", "Schat\u00b7ten", "nun", "die", "Schat\u00b7ten", "so", "er\u00b7hellt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELAT", "NN", "ADV", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie wird am Tag erst deines Schattens Wesen", "tokens": ["Wie", "wird", "am", "Tag", "erst", "dei\u00b7nes", "Schat\u00b7tens", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "APPRART", "NN", "ADV", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Mit seinem h\u00f6chsten Licht erfreun die Welt,", "tokens": ["Mit", "sei\u00b7nem", "h\u00f6chs\u00b7ten", "Licht", "er\u00b7freun", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wenn blinde Augen schon am Schatten so genesen!", "tokens": ["Wenn", "blin\u00b7de", "Au\u00b7gen", "schon", "am", "Schat\u00b7ten", "so", "ge\u00b7ne\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ADV", "APPRART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Wie selig, sag' ich, w\u00e4r mein Auge nun,", "tokens": ["Wie", "se\u00b7lig", ",", "sag'", "ich", ",", "w\u00e4r", "mein", "Au\u00b7ge", "nun", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "VVFIN", "PPER", "$,", "VAFIN", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "H\u00e4tt' ich am heitern Tag erst dich gewahrt,", "tokens": ["H\u00e4tt'", "ich", "am", "hei\u00b7tern", "Tag", "erst", "dich", "ge\u00b7wahrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "VVFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Wenn \u00f6de Nacht den Augen, wie sie ruhn,", "tokens": ["Wenn", "\u00f6\u00b7de", "Nacht", "den", "Au\u00b7gen", ",", "wie", "sie", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ART", "NN", "$,", "PWAV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Dein sch\u00f6nes bleiches Trugbild offenbart.", "tokens": ["Dein", "sch\u00f6\u00b7nes", "blei\u00b7ches", "Trug\u00b7bild", "of\u00b7fen\u00b7bart", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Mir scheint Nacht jeder Tag, getrennt von dir,", "tokens": ["Mir", "scheint", "Nacht", "je\u00b7der", "Tag", ",", "ge\u00b7trennt", "von", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PIAT", "NN", "$,", "VVPP", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Und N\u00e4chte hell wie Tag, zeigst du im Traum dich mir.", "tokens": ["Und", "N\u00e4ch\u00b7te", "hell", "wie", "Tag", ",", "zeigst", "du", "im", "Traum", "dich", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "KOKOM", "NN", "$,", "VVFIN", "PPER", "APPRART", "NN", "PPER", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}