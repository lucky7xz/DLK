{"textgrid.poem.63295": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "In der Stadtbahn", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein feiles M\u00e4dchen, sch\u00f6n und aufgetakelt,", "tokens": ["Ein", "fei\u00b7les", "M\u00e4d\u00b7chen", ",", "sch\u00f6n", "und", "auf\u00b7ge\u00b7ta\u00b7kelt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr gegen\u00fcber, gr\u00fcn und unbemakelt,", "tokens": ["Ihr", "ge\u00b7gen\u00b7\u00fc\u00b7ber", ",", "gr\u00fcn", "und", "un\u00b7be\u00b7ma\u00b7kelt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein J\u00fcngling, dessen H\u00e4nde sanft beh\u00fcten", "tokens": ["Ein", "J\u00fcng\u00b7ling", ",", "des\u00b7sen", "H\u00e4n\u00b7de", "sanft", "be\u00b7h\u00fc\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELAT", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zwei Veilchenstr\u00e4u\u00dfchen in den Seidend\u00fcten.", "tokens": ["Zwei", "Veil\u00b7chen\u00b7str\u00e4u\u00df\u00b7chen", "in", "den", "Sei\u00b7den\u00b7d\u00fc\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sie sieht ihn an. Er l\u00e4chelt traurig bl\u00f6de:", "tokens": ["Sie", "sieht", "ihn", "an", ".", "Er", "l\u00e4\u00b7chelt", "trau\u00b7rig", "bl\u00f6\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$.", "PPER", "VVFIN", "ADJD", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mein Gott, wie wird das heute wieder \u00f6de", "tokens": ["Mein", "Gott", ",", "wie", "wird", "das", "heu\u00b7te", "wie\u00b7der", "\u00f6\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "VAFIN", "PDS", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Bei Tante Linchen, die Geburtstag feiert. \u2013", "tokens": ["Bei", "Tan\u00b7te", "Lin\u00b7chen", ",", "die", "Ge\u00b7burts\u00b7tag", "fei\u00b7ert", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "NE", "$,", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Dame hat sich nunmehr ganz entschleiert.", "tokens": ["Die", "Da\u00b7me", "hat", "sich", "nun\u00b7mehr", "ganz", "ent\u00b7schlei\u00b7ert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PRF", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da ist er hingerissen, starrt ein Weilchen,", "tokens": ["Da", "ist", "er", "hin\u00b7ge\u00b7ris\u00b7sen", ",", "starrt", "ein", "Weil\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und reicht ihr wortlos alle seine Veilchen.", "tokens": ["Und", "reicht", "ihr", "wort\u00b7los", "al\u00b7le", "sei\u00b7ne", "Veil\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nun hat er nichts, f\u00fcr Tante kein Pr\u00e4sent...", "tokens": ["Nun", "hat", "er", "nichts", ",", "f\u00fcr", "Tan\u00b7te", "kein", "Pr\u00e4\u00b7sent", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "$,", "APPR", "NN", "PIAT", "NN", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Er wundert sich \u2013 das sch\u00f6ne Fr\u00e4ulein flennt:", "tokens": ["Er", "wun\u00b7dert", "sich", "\u2013", "das", "sch\u00f6\u00b7ne", "Fr\u00e4u\u00b7lein", "flennt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$(", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und ihre blassen Tr\u00e4nen auf die blauen", "tokens": ["Und", "ih\u00b7re", "blas\u00b7sen", "Tr\u00e4\u00b7nen", "auf", "die", "blau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "M\u00e4rzveilchen wie Gel\u00fcbde niedertauen.", "tokens": ["M\u00e4rz\u00b7veil\u00b7chen", "wie", "Ge\u00b7l\u00fcb\u00b7de", "nie\u00b7der\u00b7tau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}