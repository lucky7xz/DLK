{"dta.poem.13373": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Nachrichten  \n von gelehrten Schriften.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1742", "urn": "urn:nbn:de:kobv:b4-200905198519", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du wunderst dich vielleicht, gepriesner M\u00e4cenat,", "tokens": ["Du", "wun\u00b7derst", "dich", "viel\u00b7leicht", ",", "ge\u00b7pri\u00b7es\u00b7ner", "M\u00e4\u00b7ce\u00b7nat", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+--++-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Warum die Wahrheit dir noch nie geschrieben hat?", "tokens": ["Wa\u00b7rum", "die", "Wahr\u00b7heit", "dir", "noch", "nie", "ge\u00b7schrie\u00b7ben", "hat", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "ADV", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}