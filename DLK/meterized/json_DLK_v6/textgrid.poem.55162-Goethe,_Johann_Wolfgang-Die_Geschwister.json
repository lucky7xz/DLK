{"textgrid.poem.55162": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Die Geschwister", "genre": "verse", "period": "N.A.", "pub_year": 1785, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schlummer und Schlaf, zwei Br\u00fcder, zum Dienste der G\u00f6tter berufen,", "tokens": ["Schlum\u00b7mer", "und", "Schlaf", ",", "zwei", "Br\u00fc\u00b7der", ",", "zum", "Diens\u00b7te", "der", "G\u00f6t\u00b7ter", "be\u00b7ru\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "CARD", "NN", "$,", "APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Bat sich Prometheus herab seinem Geschlechte zum Trost;", "tokens": ["Bat", "sich", "Pro\u00b7me\u00b7theus", "her\u00b7ab", "sei\u00b7nem", "Ge\u00b7schlech\u00b7te", "zum", "Trost", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "NE", "ADV", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Aber den G\u00f6ttern so leicht, doch schwer zu ertragen den Menschen,", "tokens": ["A\u00b7ber", "den", "G\u00f6t\u00b7tern", "so", "leicht", ",", "doch", "schwer", "zu", "er\u00b7tra\u00b7gen", "den", "Men\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "ADJD", "$,", "ADV", "ADJD", "PTKZU", "VVINF", "ART", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Ward nun ihr Schlummer uns Schlaf, ward nun ihr Schlaf uns zum Tod.", "tokens": ["Ward", "nun", "ihr", "Schlum\u00b7mer", "uns", "Schlaf", ",", "ward", "nun", "ihr", "Schlaf", "un