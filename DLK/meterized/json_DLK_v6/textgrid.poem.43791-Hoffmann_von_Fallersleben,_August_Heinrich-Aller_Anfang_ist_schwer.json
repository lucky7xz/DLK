{"textgrid.poem.43791": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Aller Anfang ist schwer", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Michel wollte schlittschuhlaufen lernen geh'n,", "tokens": ["Mi\u00b7chel", "woll\u00b7te", "schlitt\u00b7schuhl\u00b7au\u00b7fen", "ler\u00b7nen", "geh'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "VVINF", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Juchhe! juchhe!", "tokens": ["Juch\u00b7he", "!", "juch\u00b7he", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Lie\u00df sich auf dem blanken Eise seh'n,", "tokens": ["Lie\u00df", "sich", "auf", "dem", "blan\u00b7ken", "Ei\u00b7se", "seh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "He didel dum! Eise seh'n, he didel didel de!", "tokens": ["He", "di\u00b7del", "dum", "!", "Ei\u00b7se", "seh'n", ",", "he", "di\u00b7del", "di\u00b7del", "de", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$.", "NN", "VVINF", "$,", "FM", "FM", "FM", "FM", "$."], "meter": "---+--+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "\u00bbmuth gefa\u00dft! jetzt schwenk' ich mich herum, herum!\u00ab", "tokens": ["\u00bb", "muth", "ge\u00b7fa\u00dft", "!", "jetzt", "schwenk'", "ich", "mich", "he\u00b7rum", ",", "he\u00b7rum", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "NN", "VVPP", "$.", "ADV", "VVFIN", "PPER", "PRF", "APZR", "$,", "PTKVZ", "$.", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Juchhe! juchhe!", "tokens": ["Juch\u00b7he", "!", "juch\u00b7he", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Hat es kaum gedacht, da f\u00e4llt er um,", "tokens": ["Hat", "es", "kaum", "ge\u00b7dacht", ",", "da", "f\u00e4llt", "er", "um", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "He didel dum! f\u00e4llt er um, he didel didel de!", "tokens": ["He", "di\u00b7del", "dum", "!", "f\u00e4llt", "er", "um", ",", "he", "di\u00b7del", "di\u00b7del", "de", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$.", "VVFIN", "PPER", "PTKVZ", "$,", "FM", "FM", "FM", "FM", "$."], "meter": "-+--+-++-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Und er stehet auf: \u00bbder Anfang ist gemacht!\u00ab", "tokens": ["Und", "er", "ste\u00b7het", "auf", ":", "\u00bb", "der", "An\u00b7fang", "ist", "ge\u00b7macht", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ", "$.", "$(", "ART", "NN", "VAFIN", "VVPP", "$.", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Juchhe! juchhe!", "tokens": ["Juch\u00b7he", "!", "juch\u00b7he", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Und er purzelt wieder, da\u00df es kracht,", "tokens": ["Und", "er", "pur\u00b7zelt", "wie\u00b7der", ",", "da\u00df", "es", "kracht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "He didel dum! da\u00df es kracht, he didel didel de!", "tokens": ["He", "di\u00b7del", "dum", "!", "da\u00df", "es", "kracht", ",", "he", "di\u00b7del", "di\u00b7del", "de", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$.", "KOUS", "PPER", "VVFIN", "$,", "FM", "FM", "FM", "FM", "$."], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}}, "stanza.4": {"line.1": {"text": "Oft noch f\u00e4llt er, stehet immer wieder auf,", "tokens": ["Oft", "noch", "f\u00e4llt", "er", ",", "ste\u00b7het", "im\u00b7mer", "wie\u00b7der", "auf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "$,", "VVFIN", "ADV", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Juchhe! juchhe!", "tokens": ["Juch\u00b7he", "!", "juch\u00b7he", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Endlich lernt er doch den Schlittschuhlauf,", "tokens": ["End\u00b7lich", "lernt", "er", "doch", "den", "Schlitt\u00b7schuhl\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "He didel dum! Schlittschuhlauf, he didel didel de!", "tokens": ["He", "di\u00b7del", "dum", "!", "Schlitt\u00b7schuhl\u00b7auf", ",", "he", "di\u00b7del", "di\u00b7del", "de", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$.", "NN", "$,", "FM", "FM", "FM", "FM", "$."], "meter": "-+--+-+-++-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Aller Anfang ist ja schwer \u2013 wer das nicht wei\u00df,", "tokens": ["Al\u00b7ler", "An\u00b7fang", "ist", "ja", "schwer", "\u2013", "wer", "das", "nicht", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ADV", "ADJD", "$(", "PWS", "PDS", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Juchhe! juchhe!", "tokens": ["Juch\u00b7he", "!", "juch\u00b7he", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Ei, der bleibe lieber von dem Eis,", "tokens": ["Ei", ",", "der", "blei\u00b7be", "lie\u00b7ber", "von", "dem", "Eis", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "He didel dum! von dem Eis, he didel didel de!", "tokens": ["He", "di\u00b7del", "dum", "!", "von", "dem", "Eis", ",", "he", "di\u00b7del", "di\u00b7del", "de", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$.", "APPR", "ART", "NN", "$,", "FM", "FM", "FM", "FM", "$."], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}}}}}