{"dta.poem.13439": {"metadata": {"author": {"name": "Suppius, Christoph Eusebius", "birth": "N.A.", "death": "N.A."}, "title": "Brockes.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1749", "urn": "urn:nbn:de:kobv:b4-20594-7", "language": ["de:0.99"], "booktitle": "Suppius, Christoph Eusebius: Oden und Lieder. Gotha, 1749."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr Winde, still! schweigt, Nachtigallen!", "tokens": ["Ihr", "Win\u00b7de", ",", "still", "!", "schweigt", ",", "Nach\u00b7ti\u00b7gal\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "$.", "VVFIN", "$,", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Natur und Himmel, h\u00f6rt mir zu!", "tokens": ["Na\u00b7tur", "und", "Him\u00b7mel", ",", "h\u00f6rt", "mir", "zu", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Saitenspiel mu\u00df laut erschallen,", "tokens": ["Mein", "Sai\u00b7ten\u00b7spiel", "mu\u00df", "laut", "er\u00b7schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Dichter habe vor euch Ruh;", "tokens": ["Der", "Dich\u00b7ter", "ha\u00b7be", "vor", "euch", "Ruh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zwar sing ich nicht den Gott der G\u00f6tter,", "tokens": ["Zwar", "sing", "ich", "nicht", "den", "Gott", "der", "G\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mein Lied ist nicht der Herr im Wetter,", "tokens": ["Mein", "Lied", "ist", "nicht", "der", "Herr", "im", "Wet\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Zu Helden hab ich nicht Beruf;", "tokens": ["Zu", "Hel\u00b7den", "hab", "ich", "nicht", "Be\u00b7ruf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das konnte Brockes nur erheben,", "tokens": ["Das", "konn\u00b7te", "Bro\u00b7ckes", "nur", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "NE", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Das ist der Mann, den sing ich eben,", "tokens": ["Das", "ist", "der", "Mann", ",", "den", "sing", "ich", "e\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "ART", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Der malet, wie der H\u00f6chste schuf.", "tokens": ["Der", "ma\u00b7let", ",", "wie", "der", "H\u00f6chs\u00b7te", "schuf", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie? soll dem so erhabnen Geiste", "tokens": ["Wie", "?", "soll", "dem", "so", "er\u00b7hab\u00b7nen", "Geis\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$.", "VMFIN", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Lied im h\u00f6hern Ton entstehn?", "tokens": ["Ein", "Lied", "im", "h\u00f6\u00b7hern", "Ton", "ent\u00b7stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der die Natur so herrlich preiste,", "tokens": ["Der", "die", "Na\u00b7tur", "so", "herr\u00b7lich", "preis\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df sie itzt noch einmal so sch\u00f6n?", "tokens": ["Da\u00df", "sie", "itzt", "noch", "ein\u00b7mal", "so", "sch\u00f6n", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O nein! den k\u00fcnstigen Geschichten", "tokens": ["O", "nein", "!", "den", "k\u00fcns\u00b7ti\u00b7gen", "Ge\u00b7schich\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PTKANT", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "M\u00fc\u00dft ihr, ihr Winde! selbst berichten,", "tokens": ["M\u00fc\u00dft", "ihr", ",", "ihr", "Win\u00b7de", "!", "selbst", "be\u00b7rich\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$.", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Was f\u00fcr Begeistrung mich erregt;", "tokens": ["Was", "f\u00fcr", "Be\u00b7geis\u00b7trung", "mich", "er\u00b7regt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Auch euch sey solches unentfallen,", "tokens": ["Auch", "euch", "sey", "sol\u00b7ches", "un\u00b7ent\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "PIAT", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Jhr Buschsirenen, Nachtigallen!", "tokens": ["Ihr", "Buschsi\u00b7re\u00b7nen", ",", "Nach\u00b7ti\u00b7gal\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Wenn euch ein frischer Lenz bewegt.", "tokens": ["Wenn", "euch", "ein", "fri\u00b7scher", "Lenz", "be\u00b7wegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mein Lobgesang meynt Brockes Ehre,", "tokens": ["Mein", "Lob\u00b7ge\u00b7sang", "meynt", "Bro\u00b7ckes", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NE", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und seinen Ruhm ert\u00f6nt mein Mund;", "tokens": ["Und", "sei\u00b7nen", "Ruhm", "er\u00b7t\u00f6nt", "mein", "Mund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er macht uns oft die lauten Ch\u00f6re", "tokens": ["Er", "macht", "uns", "oft", "die", "lau\u00b7ten", "Ch\u00f6\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Morgensterne droben kund,", "tokens": ["Der", "Mor\u00b7gens\u00b7ter\u00b7ne", "dro\u00b7ben", "kund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Kein Plato kann von Himmels-Kreisen", "tokens": ["Kein", "Pla\u00b7to", "kann", "von", "Him\u00b7mels\u00b7Krei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NE", "VMFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Welt solch einen Wohlklang preisen,", "tokens": ["Der", "Welt", "solch", "ei\u00b7nen", "Wohl\u00b7klang", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Als Brockes diese Sph\u00e4ren stimmt;", "tokens": ["Als", "Bro\u00b7ckes", "die\u00b7se", "Sph\u00e4\u00b7ren", "stimmt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Von dorther schallet nichts als Wonne,", "tokens": ["Von", "dor\u00b7ther", "schal\u00b7let", "nichts", "als", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PIS", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Wo manche nie entdeckte Sonne", "tokens": ["Wo", "man\u00b7che", "nie", "ent\u00b7deck\u00b7te", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "ADV", "VVFIN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Voll von Jehova Wundern glimmt.", "tokens": ["Voll", "von", "Je\u00b7ho\u00b7va", "Wun\u00b7dern", "glimmt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NE", "NN", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Ein Orpheus hat hier ausgerichtet,", "tokens": ["Ein", "Or\u00b7pheus", "hat", "hier", "aus\u00b7ge\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wozu er sich von selbst geweiht,", "tokens": ["Wo\u00b7zu", "er", "sich", "von", "selbst", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "APPR", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von dieser Welt hat er gedichtet,", "tokens": ["Von", "die\u00b7ser", "Welt", "hat", "er", "ge\u00b7dich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und sang doch um die Ewigkeit;", "tokens": ["Und", "sang", "doch", "um", "die", "E\u00b7wig\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sein Lied war GOtt, in Creaturen", "tokens": ["Sein", "Lied", "war", "Gott", ",", "in", "Crea\u00b7tu\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$,", "APPR", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Entdeckt er dessen Darseyns-Spuhren,", "tokens": ["Ent\u00b7deckt", "er", "des\u00b7sen", "Dar\u00b7seyns\u00b7Sp\u00b7uh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und seine grosse Wunderpracht,", "tokens": ["Und", "sei\u00b7ne", "gros\u00b7se", "Wun\u00b7der\u00b7pracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In der unendlichtiefen Ferne", "tokens": ["In", "der", "un\u00b7end\u00b7lich\u00b7tie\u00b7fen", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Besucht er gar die Welt der Sterne,", "tokens": ["Be\u00b7sucht", "er", "gar", "die", "Welt", "der", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die er erbaulich schimmernd macht.", "tokens": ["Die", "er", "er\u00b7bau\u00b7lich", "schim\u00b7mernd", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Er braust in St\u00fcrmen, schiesset Blitze,", "tokens": ["Er", "braust", "in", "St\u00fcr\u00b7men", ",", "schies\u00b7set", "Blit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Donner br\u00fcllt, es tobt sein Meer,", "tokens": ["Sein", "Don\u00b7ner", "br\u00fcllt", ",", "es", "tobt", "sein", "Meer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die\u00df zeigt er uns in Wuth und Hitze,", "tokens": ["Die\u00df", "zeigt", "er", "uns", "in", "Wuth", "und", "Hit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PRF", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch f\u00fcrchten wir uns nicht so sehr;", "tokens": ["Doch", "f\u00fcrch\u00b7ten", "wir", "uns", "nicht", "so", "sehr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "PTKNEG", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Allmacht Wollen hie\u00df: Es werde!", "tokens": ["Der", "All\u00b7macht", "Wol\u00b7len", "hie\u00df", ":", "Es", "wer\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$.", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da wurden sie, da ward die Erde,", "tokens": ["Da", "wur\u00b7den", "sie", ",", "da", "ward", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die Elemente sahen sich;", "tokens": ["Die", "E\u00b7le\u00b7men\u00b7te", "sa\u00b7hen", "sich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "Doch sie bestimmt auch ihre Kr\u00e4fte,", "tokens": ["Doch", "sie", "be\u00b7stimmt", "auch", "ih\u00b7re", "Kr\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Sie sind der Vorsehung Gesch\u00e4fte,", "tokens": ["Sie", "sind", "der", "Vor\u00b7se\u00b7hung", "Ge\u00b7sch\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Gemalt in Brockes Pinselstrich.", "tokens": ["Ge\u00b7malt", "in", "Bro\u00b7ckes", "Pin\u00b7sel\u00b7strich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Du, Qvell des Lichts! Monarch der Zeiten!", "tokens": ["Du", ",", "Qvell", "des", "Lichts", "!", "Mon\u00b7arch", "der", "Zei\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "ART", "NN", "$.", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verblendest mir das Auge schon,", "tokens": ["Ver\u00b7blen\u00b7dest", "mir", "das", "Au\u00b7ge", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Seh ich nur deinen Glanz von weiten,", "tokens": ["Seh", "ich", "nur", "dei\u00b7nen", "Glanz", "von", "wei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und dich auf deinem blauen Thron;", "tokens": ["Und", "dich", "auf", "dei\u00b7nem", "blau\u00b7en", "Thron", ";"], "