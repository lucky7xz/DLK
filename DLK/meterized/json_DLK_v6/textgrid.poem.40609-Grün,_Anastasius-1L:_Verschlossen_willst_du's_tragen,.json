{"textgrid.poem.40609": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Verschlossen willst du's tragen,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verschlossen willst du's tragen,", "tokens": ["Ver\u00b7schlos\u00b7sen", "willst", "du's", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du willst es nie ihr sagen,", "tokens": ["Du", "willst", "es", "nie", "ihr", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wovon dein Herz so wund;", "tokens": ["Wo\u00b7von", "dein", "Herz", "so", "wund", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sie wird ja nie dein eigen,", "tokens": ["Sie", "wird", "ja", "nie", "dein", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PPOSAT", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Drum h\u00fcte m\u00e4nnlich Schweigen", "tokens": ["Drum", "h\u00fc\u00b7te", "m\u00e4nn\u00b7lich", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den Hort im Seelengrund.", "tokens": ["Den", "Hort", "im", "See\u00b7len\u00b7grund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Doch da vernimmt dein Lauschen", "tokens": ["Doch", "da", "ver\u00b7nimmt", "dein", "Lau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Leis ihres Kleides Rauschen,", "tokens": ["Leis", "ih\u00b7res", "Klei\u00b7des", "Rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den Schritt, dir wohlbekannt,", "tokens": ["Den", "Schritt", ",", "dir", "wohl\u00b7be\u00b7kannt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die\u00df leichtbeschwingte Schreiten,", "tokens": ["Die\u00df", "leicht\u00b7be\u00b7schwing\u00b7te", "Schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wie Fee'n im Mondlicht gleiten,", "tokens": ["Wie", "Fee'n", "im", "Mond\u00b7licht", "glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bis selbst sie vor dir stand.", "tokens": ["Bis", "selbst", "sie", "vor", "dir", "stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Brust wird dir so enge,", "tokens": ["Die", "Brust", "wird", "dir", "so", "en\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Athem stockt, es dr\u00e4nge", "tokens": ["Der", "A\u00b7them", "stockt", ",", "es", "dr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Heraus kein W\u00f6rtchen klein;", "tokens": ["He\u00b7raus", "kein", "W\u00f6rt\u00b7chen", "klein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mit Schauern, die begl\u00fccken,", "tokens": ["Mit", "Schau\u00b7ern", ",", "die", "be\u00b7gl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mit Gluthen, die erquicken,", "tokens": ["Mit", "Glu\u00b7then", ",", "die", "er\u00b7qui\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Durchfiebert's dein Gebein.", "tokens": ["Durch\u00b7fie\u00b7bert's", "dein", "Ge\u00b7bein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Es will das Knie sich beugen,", "tokens": ["Es", "will", "das", "Knie", "sich", "beu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von ihrem Werth zu zeugen,", "tokens": ["Von", "ih\u00b7rem", "Werth", "zu", "zeu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zu huld'gen ihrer Macht;", "tokens": ["Zu", "huld'\u00b7gen", "ih\u00b7rer", "Macht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Arme m\u00f6chten fliegen,", "tokens": ["Die", "Ar\u00b7me", "m\u00f6ch\u00b7ten", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Den Liebreiz zu umschmiegen,", "tokens": ["Den", "Lieb\u00b7reiz", "zu", "um\u00b7schmie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Doch h\u00e4ltst du strenge Wacht.", "tokens": ["Doch", "h\u00e4ltst", "du", "stren\u00b7ge", "Wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wie deine Augen leuchten,", "tokens": ["Wie", "dei\u00b7ne", "Au\u00b7gen", "leuch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dann wieder mild sich feuchten,", "tokens": ["Dann", "wie\u00b7der", "mild", "sich", "feuch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "PRF", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie dir die Wange gl\u00fcht!", "tokens": ["Wie", "dir", "die", "Wan\u00b7ge", "gl\u00fcht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das Herz mu\u00df h\u00f6rbar schlagen;", "tokens": ["Das", "Herz", "mu\u00df", "h\u00f6r\u00b7bar", "schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wie sich die Pulse jagen,", "tokens": ["Wie", "sich", "die", "Pul\u00b7se", "ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie's durch die Adern spr\u00fcht!", "tokens": ["Wie's", "durch", "die", "A\u00b7dern", "spr\u00fcht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ein Aufschrei aller Sinne", "tokens": ["Ein", "Auf\u00b7schrei", "al\u00b7ler", "Sin\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verr\u00e4th die stille Minne,", "tokens": ["Ver\u00b7r\u00e4\u00b7th", "die", "stil\u00b7le", "Min\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Gibt dein Geheimni\u00df kund;", "tokens": ["Gibt", "dein", "Ge\u00b7heim\u00b7ni\u00df", "kund", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und reden solche Zeugen,", "tokens": ["Und", "re\u00b7den", "sol\u00b7che", "Zeu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dann spricht mit seinem Schweigen", "tokens": ["Dann", "spricht", "mit", "sei\u00b7nem", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Viel lauter noch dein Mund.", "tokens": ["Viel", "lau\u00b7ter", "noch", "dein", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}