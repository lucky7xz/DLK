{"dta.poem.12425": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Um die Kinder still und artig zu machen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es kam ein Herr zum Schl\u00f6\u00dfly               ", "tokens": ["Es", "kam", "ein", "Herr", "zum", "Schl\u00f6\u00df\u00b7ly"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf einem sch\u00f6nen R\u00f6\u00dfly,", "tokens": ["Auf", "ei\u00b7nem", "sch\u00f6\u00b7nen", "R\u00f6\u00df\u00b7ly", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da lugt die Frau zum Fenster aus", "tokens": ["Da", "lugt", "die", "Frau", "zum", "Fens\u00b7ter", "aus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sagt: \u201eDer Mann ist nicht zu Haus.", "tokens": ["Und", "sagt", ":", "\u201e", "Der", "Mann", "ist", "nicht", "zu", "Haus", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "ART", "NN", "VAFIN", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u201eund niemand heim als Kinder", "tokens": ["\u201e", "und", "nie\u00b7mand", "heim", "als", "Kin\u00b7der"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "KON", "PIS", "PTKVZ", "KOKOM", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u201eunds M\u00e4dchen auf der Winden.\u201c", "tokens": ["\u201e", "unds", "M\u00e4d\u00b7chen", "auf", "der", "Win\u00b7den", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "NN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Herr auf seinem R\u00f6\u00dfly,", "tokens": ["Der", "Herr", "auf", "sei\u00b7nem", "R\u00f6\u00df\u00b7ly", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sagt zu der Frau im Schl\u00f6\u00dfly:", "tokens": ["Sagt", "zu", "der", "Frau", "im", "Schl\u00f6\u00df\u00b7ly", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u201esinds gute Kind, sinds b\u00f6se Kind?", "tokens": ["\u201e", "sinds", "gu\u00b7te", "Kind", ",", "sinds", "b\u00f6\u00b7se", "Kind", "?"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ADJA", "NN", "$,", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201each liebe Frau, ach sagt geschwind.\u201c", "tokens": ["\u201e", "ach", "lie\u00b7be", "Frau", ",", "ach", "sagt", "ge\u00b7schwind", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ITJ", "VVFIN", "NN", "$,", "ITJ", "VVFIN", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Frau, die sagt: \u201eSehr b\u00f6se Kind,", "tokens": ["Die", "Frau", ",", "die", "sagt", ":", "\u201e", "Sehr", "b\u00f6\u00b7se", "Kind", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "$.", "$(", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201esie folgen Muttern nicht geschwind.\u201c", "tokens": ["\u201e", "sie", "fol\u00b7gen", "Mut\u00b7tern", "nicht", "ge\u00b7schwind", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "ADJA", "NN", "PTKNEG", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da sagt der Herr: \u201eSo reit ich heim,", "tokens": ["Da", "sagt", "der", "Herr", ":", "\u201e", "So", "reit", "ich", "heim", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "$(", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201edergleichen Kinder brauch ich kein.\u201c", "tokens": ["\u201e", "derg\u00b7lei\u00b7chen", "Kin\u00b7der", "brauch", "ich", "kein", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PIS", "NN", "VVFIN", "PPER", "PIAT", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und reit auf seinem R\u00f6\u00dfly,", "tokens": ["Und", "reit", "auf", "sei\u00b7nem", "R\u00f6\u00df\u00b7ly", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Weit, weit entweg vom Schl\u00f6\u00dfly.", "tokens": ["Weit", ",", "weit", "ent\u00b7weg", "vom", "Schl\u00f6\u00df\u00b7ly", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}