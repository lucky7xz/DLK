{"dta.poem.8419": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Christel und Hannchen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Und das Gold der sinkenden Sonn' umbebte die Aehren", "tokens": ["Und", "das", "Gold", "der", "sin\u00b7ken\u00b7den", "Sonn'", "um\u00b7beb\u00b7te", "die", "A\u00b7eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "--+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und die ragenden Garben, als Schnitter Christel", "tokens": ["Und", "die", "ra\u00b7gen\u00b7den", "Gar\u00b7ben", ",", "als", "Schnit\u00b7ter", "Chris\u00b7tel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "KOUS", "NN", "NE"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "sein Hannchen", "tokens": ["sein", "Hann\u00b7chen"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Rief zum duftenden Busch, wo tausend l\u00e4ndliche", "tokens": ["Rief", "zum", "duf\u00b7ten\u00b7den", "Busch", ",", "wo", "tau\u00b7send", "l\u00e4nd\u00b7li\u00b7che"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPRART", "ADJA", "NN", "$,", "PWAV", "ADJD", "ADJA"], "meter": "+-+--+-+-+--", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Grillen", "tokens": ["Gril\u00b7len"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Liebe zirpten und Ruh. Sie waren beide verlobet,", "tokens": ["Lie\u00b7be", "zirp\u00b7ten", "und", "Ruh", ".", "Sie", "wa\u00b7ren", "bei\u00b7de", "ver\u00b7lo\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "NN", "$.", "PPER", "VAFIN", "PIS", "VVFIN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.7": {"text": "Harrten beid' entgegen der Stunde der frohen", "tokens": ["Harr\u00b7ten", "beid'", "ent\u00b7ge\u00b7gen", "der", "Stun\u00b7de", "der", "fro\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NN", "ART", "ADJA"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Verm\u00e4hlung.", "tokens": ["Ver\u00b7m\u00e4h\u00b7lung", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Christel hatt' ihr bereits, zum Pfande der br\u00e4utlichen", "tokens": ["Chris\u00b7tel", "hatt'", "ihr", "be\u00b7reits", ",", "zum", "Pfan\u00b7de", "der", "br\u00e4ut\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "ADV", "$,", "APPRART", "NN", "ART", "ADJA"], "meter": "+-+--+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "Treue,", "tokens": ["Treu\u00b7e", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Eine Bibel geschenkt, und ein rothvergoldetes", "tokens": ["Ei\u00b7ne", "Bi\u00b7bel", "ge\u00b7schenkt", ",", "und", "ein", "roth\u00b7ver\u00b7gol\u00b7de\u00b7tes"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "KON", "ART", "ADJA"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.12": {"text": "Psalmbuch;", "tokens": ["Psalm\u00b7buch", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Und das liebende M\u00e4dchen, zur Gegengabe, dem", "tokens": ["Und", "das", "lie\u00b7ben\u00b7de", "M\u00e4d\u00b7chen", ",", "zur", "Ge\u00b7gen\u00b7ga\u00b7be", ",", "dem"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "APPRART", "NN", "$,", "PRELS"], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.14": {"text": "J\u00fcngling", "tokens": ["J\u00fcng\u00b7ling"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "Einen prunkenden Hut und statliche Br\u00e4utigams-", "tokens": ["Ei\u00b7nen", "prun\u00b7ken\u00b7den", "Hut", "und", "stat\u00b7li\u00b7che", "Br\u00e4u\u00b7ti\u00b7gams"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "ADJA", "TRUNC"], "meter": "+-+--+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.16": {"text": "hemde.", "tokens": ["hem\u00b7de", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.17": {"text": "Von der Abendk\u00fchle des d\u00e4mmernden Strauches", "tokens": ["Von", "der", "A\u00b7bend\u00b7k\u00fch\u00b7le", "des", "d\u00e4m\u00b7mern\u00b7den", "Strau\u00b7ches"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "ums\u00e4uselt,", "tokens": ["um\u00b7s\u00e4u\u00b7selt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.19": {"text": "Ruhte das gl\u00fcckliche Paar; indess die Schnitter", "tokens": ["Ruh\u00b7te", "das", "gl\u00fcck\u00b7li\u00b7che", "Paar", ";", "in\u00b7dess", "die", "Schnit\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$.", "ADV", "ART", "NN"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.20": {"text": "und M\u00e4dchen", "tokens": ["und", "M\u00e4d\u00b7chen"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.21": {"text": "Ihre Kleider suchten, sich haschten, und scherzten", "tokens": ["Ih\u00b7re", "Klei\u00b7der", "such\u00b7ten", ",", "sich", "haschten", ",", "und", "scherz\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PRF", "VVFIN", "$,", "KON", "VVFIN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.22": {"text": "und sangen.", "tokens": ["und", "san\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.23": {"text": "Bald beginnet der Tag des Hochzeitkranzes,", "tokens": ["Bald", "be\u00b7gin\u00b7net", "der", "Tag", "des", "Hoch\u00b7zeit\u00b7kran\u00b7zes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.24": {"text": "o Hannchen!", "tokens": ["o", "Hann\u00b7chen", "!"], "token_info": ["word", "word", "punct"], "pos": ["FM", "NE", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.25": {"text": "Bald, bald nenn' ich dich Weib, und theile die", "tokens": ["Bald", ",", "bald", "nenn'", "ich", "dich", "Weib", ",", "und", "thei\u00b7le", "die"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "VVFIN", "PPER", "PRF", "NE", "$,", "KON", "VVFIN", "ART"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Sorgen der Wirtschaft,", "tokens": ["Sor\u00b7gen", "der", "Wirt\u00b7schaft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.27": {"text": "Hannchen, Hannchen, mit dir! Bewehn die Winde", "tokens": ["Hann\u00b7chen", ",", "Hann\u00b7chen", ",", "mit", "dir", "!", "Be\u00b7wehn", "die", "Win\u00b7de"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "NE", "$,", "APPR", "PPER", "$.", "VVFIN", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.28": {"text": "die Stoppeln,", "tokens": ["die", "Stop\u00b7peln", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.29": {"text": "R\u00f6theln vom bunten Baume die Aepfel uns heller", "tokens": ["R\u00f6\u00b7theln", "vom", "bun\u00b7ten", "Bau\u00b7me", "die", "A\u00b7e\u00b7pfel", "uns", "hel\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "ADJA", "NN", "ART", "NN", "PPER", "ADJA"], "meter": "-+-+-+--++--+-", "measure": "iambic.hexa.relaxed"}, "line.30": {"text": "entgegen;", "tokens": ["ent\u00b7ge\u00b7gen", ";"], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.31": {"text": "Dann beginnet der Tag des Hochzeitkranzes,", "tokens": ["Dann", "be\u00b7gin\u00b7net", "der", "Tag", "des", "Hoch\u00b7zeit\u00b7kran\u00b7zes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.32": {"text": "o Hannchen!", "tokens": ["o", "Hann\u00b7chen", "!"], "token_info": ["word", "word", "punct"], "pos": ["FM", "NE", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.33": {"text": "Jede kommende Nacht umschwebt mich dein l\u00e4chelndes", "tokens": ["Je\u00b7de", "kom\u00b7men\u00b7de", "Nacht", "um\u00b7schwebt", "mich", "dein", "l\u00e4\u00b7cheln\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PPER", "PPOSAT", "ADJA"], "meter": "+-+--+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.34": {"text": "Bildniss,", "tokens": ["Bild\u00b7niss", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.35": {"text": "Bald im Hochzeitgeschmuck, von rothen B\u00e4ndern", "tokens": ["Bald", "im", "Hoch\u00b7zeit\u00b7ge\u00b7schmuck", ",", "von", "ro\u00b7then", "B\u00e4n\u00b7dern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.36": {"text": "umflattert,", "tokens": ["um\u00b7flat\u00b7tert", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.37": {"text": "Bald im Schnitterh\u00fctchen, im blauen Kranze der Ernte.", "tokens": ["Bald", "im", "Schnit\u00b7ter\u00b7h\u00fct\u00b7chen", ",", "im", "blau\u00b7en", "Kran\u00b7ze", "der", "Ern\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.38": {"text": "Dann erwach' ich, und hasche dein Bild, und horche", "tokens": ["Dann", "er\u00b7wach'", "ich", ",", "und", "ha\u00b7sche", "dein", "Bild", ",", "und", "hor\u00b7che"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$,", "KON", "ADJA"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.39": {"text": "der Grille,", "tokens": ["der", "Gril\u00b7le", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.40": {"text": "Und ein Seufzer entfliegt zu deiner einsamen H\u00fctte.", "tokens": ["Und", "ein", "Seuf\u00b7zer", "ent\u00b7fliegt", "zu", "dei\u00b7ner", "ein\u00b7sa\u00b7men", "H\u00fct\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.41": {"text": "Lieber Christel! lispelte Hannchen, und dr\u00fcckt'", "tokens": ["Lie\u00b7ber", "Chris\u00b7tel", "!", "lis\u00b7pel\u00b7te", "Hann\u00b7chen", ",", "und", "dr\u00fcckt'"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "NE", "$.", "VVFIN", "NE", "$,", "KON", "VVFIN"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.42": {"text": "ihm die H\u00e4nde,", "tokens": ["ihm", "die", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.43": {"text": "Und verstummt' ein Weilchen: o mehr, als Vater", "tokens": ["Und", "ver\u00b7stummt'", "ein", "Weil\u00b7chen", ":", "o", "mehr", ",", "als", "Va\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "$.", "FM", "ADV", "$,", "KOUS", "NN"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.44": {"text": "und Mutter,", "tokens": ["und", "Mut\u00b7ter", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.45": {"text": "Lieb' ich dich, Christel, und will, so lang' ich athme,", "tokens": ["Lieb'", "ich", "dich", ",", "Chris\u00b7tel", ",", "und", "will", ",", "so", "lang'", "ich", "ath\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "NE", "$,", "KON", "VMFIN", "$,", "ADV", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.46": {"text": "dich lieben!", "tokens": ["dich", "lie\u00b7ben", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.47": {"text": "Alles wird mir so wehrt, was deine H\u00e4nde ber\u00fchren,", "tokens": ["Al\u00b7les", "wird", "mir", "so", "wehrt", ",", "was", "dei\u00b7ne", "H\u00e4n\u00b7de", "be\u00b7r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADV", "VVFIN", "$,", "PRELS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.48": {"text": "Als ein Patengeschenk. Seit du mir die Bibel", "tokens": ["Als", "ein", "Pa\u00b7ten\u00b7ge\u00b7schenk", ".", "Seit", "du", "mir", "die", "Bi\u00b7bel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "$.", "KOUS", "PPER", "PPER", "ART", "NN"], "meter": "--+--++-+-+-", "measure": "anapaest.di.plus"}, "line.49": {"text": "geschenkt hast,", "tokens": ["ge\u00b7schenkt", "hast", ","], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.50": {"text": "Les' ich so fleissig darin, und zeichne die sch\u00f6nen", "tokens": ["Les'", "ich", "so", "fleis\u00b7sig", "da\u00b7rin", ",", "und", "zeich\u00b7ne", "die", "sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "ADJD", "PAV", "$,", "KON", "VVFIN", "ART", "ADJA"], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.51": {"text": "Geschichten", "tokens": ["Ge\u00b7schich\u00b7ten"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.52": {"text": "Von Rebekka, und Rahel, und Judith, mit goldenen", "tokens": ["Von", "Re\u00b7bek\u00b7ka", ",", "und", "Ra\u00b7hel", ",", "und", "Ju\u00b7dith", ",", "mit", "gol\u00b7de\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NE", "$,", "KON", "NE", "$,", "KON", "NE", "$,", "APPR", "ADJA"], "meter": "-+-+-+--+--+--", "measure": "iambic.penta.relaxed"}, "line.53": {"text": "Bildern.", "tokens": ["Bil\u00b7dern", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.54": {"text": "Schon entstieg der freundliche Mond dem Thau-", "tokens": ["Schon", "ent\u00b7stieg", "der", "freund\u00b7li\u00b7che", "Mond", "dem", "Thau"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.55": {"text": "gew\u00f6lke,", "tokens": ["ge\u00b7w\u00f6l\u00b7ke", ","], "token_info": ["word", "punct"], "pos": ["ADJA", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.56": {"text": "Und die zitternden Weizenwogen schwammen in", "tokens": ["Und", "die", "zit\u00b7tern\u00b7den", "Wei\u00b7zen\u00b7wo\u00b7gen", "schwam\u00b7men", "in"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "APPR"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.57": {"text": "Silber;", "tokens": ["Sil\u00b7ber", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.58": {"text": "Da ergriffen die Schnitter die Sensen, und sch\u00e4kerten"