{"textgrid.poem.57569": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Juli", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wei\u00dfgl\u00fchende Sonne und staubige Luft,", "tokens": ["Wei\u00df\u00b7gl\u00fc\u00b7hen\u00b7de", "Son\u00b7ne", "und", "stau\u00b7bi\u00b7ge", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Kopfschmerzen und m\u00fcde Glieder,", "tokens": ["Kopf\u00b7schmer\u00b7zen", "und", "m\u00fc\u00b7de", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Verstaubt und grau sind Blumen und Blatt,", "tokens": ["Ver\u00b7staubt", "und", "grau", "sind", "Blu\u00b7men", "und", "Blatt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Verstummt sind Lachen und Lieder.", "tokens": ["Ver\u00b7stummt", "sind", "La\u00b7chen", "und", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich liege bewegungslos im Gras,", "tokens": ["Ich", "lie\u00b7ge", "be\u00b7we\u00b7gungs\u00b7los", "im", "Gras", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein Leichnam mit F\u00fchlen und Denken \u2013", "tokens": ["Ein", "Leich\u00b7nam", "mit", "F\u00fch\u00b7len", "und", "Den\u00b7ken", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Wann wirst du, launische Dame Natur,", "tokens": ["Wann", "wirst", "du", ",", "lau\u00b7ni\u00b7sche", "Da\u00b7me", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "ADJA", "NN", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Uns Blitz und Regen schenken?", "tokens": ["Uns", "Blitz", "und", "Re\u00b7gen", "schen\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ein abgeflatterter Schmetterling", "tokens": ["Ein", "ab\u00b7ge\u00b7flat\u00b7ter\u00b7ter", "Schmet\u00b7ter\u00b7ling"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zuckt neben mir mit den Schwingen,", "tokens": ["Zuckt", "ne\u00b7ben", "mir", "mit", "den", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich trete ihn tot \u2013 das Leben kann", "tokens": ["Ich", "tre\u00b7te", "ihn", "tot", "\u2013", "das", "Le\u00b7ben", "kann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$(", "ART", "NN", "VMFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ihm doch keine Freude mehr bringen.", "tokens": ["Ihm", "doch", "kei\u00b7ne", "Freu\u00b7de", "mehr", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PIAT", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Ein saurer, fauliger Schwei\u00dfgeruch", "tokens": ["Ein", "sau\u00b7rer", ",", "fau\u00b7li\u00b7ger", "Schwei\u00df\u00b7ge\u00b7ruch"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Steigt auf aus allen Teichen,", "tokens": ["Steigt", "auf", "aus", "al\u00b7len", "Tei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als wollte aus einem entstellten Leib", "tokens": ["Als", "woll\u00b7te", "aus", "ei\u00b7nem", "ent\u00b7stell\u00b7ten", "Leib"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VMFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Das Leichengas entweichen.", "tokens": ["Das", "Lei\u00b7chen\u00b7gas", "ent\u00b7wei\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und G\u00e4hnen durchzieht die Lebewelt,", "tokens": ["Und", "G\u00e4h\u00b7nen", "durch\u00b7zieht", "die", "Le\u00b7be\u00b7welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein Lechzen nach Tod und Ruhe \u2013", "tokens": ["Ein", "Lech\u00b7zen", "nach", "Tod", "und", "Ru\u00b7he", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Jetzt nagle den Deckel auf den Sarg,", "tokens": ["Jetzt", "nag\u00b7le", "den", "De\u00b7ckel", "auf", "den", "Sarg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Natur, und schlie\u00dfe die Truhe.", "tokens": ["Na\u00b7tur", ",", "und", "schlie\u00b7\u00dfe", "die", "Tru\u00b7he", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Den armen Menschen zum mindesten", "tokens": ["Den", "ar\u00b7men", "Men\u00b7schen", "zum", "min\u00b7des\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Darfst traumlose Ruhe du geben,", "tokens": ["Darfst", "traum\u00b7lo\u00b7se", "Ru\u00b7he", "du", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Ein fauler Witz ohne Saft und Kraft", "tokens": ["Ein", "fau\u00b7ler", "Witz", "oh\u00b7ne", "Saft", "und", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ist das ganze, menschliche Leben.", "tokens": ["Ist", "das", "gan\u00b7ze", ",", "menschli\u00b7che", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}