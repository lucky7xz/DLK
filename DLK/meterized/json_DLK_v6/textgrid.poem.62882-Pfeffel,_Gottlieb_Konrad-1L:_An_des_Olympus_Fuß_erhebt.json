{"textgrid.poem.62882": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: An des Olympus Fu\u00df erhebt", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An des Olympus Fu\u00df erhebt", "tokens": ["An", "des", "O\u00b7lym\u00b7pus", "Fu\u00df", "er\u00b7hebt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NE", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein alter Palmbaum sich, den man als g\u00f6ttlich ehret,", "tokens": ["Ein", "al\u00b7ter", "Palm\u00b7baum", "sich", ",", "den", "man", "als", "g\u00f6tt\u00b7lich", "eh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "$,", "PRELS", "PIS", "KOKOM", "ADJD", "VVFIN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Weil eine Dryas in ihm lebt,", "tokens": ["Weil", "ei\u00b7ne", "Dryas", "in", "ihm", "lebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Die jeden reinen Wunsch erh\u00f6ret.", "tokens": ["Die", "je\u00b7den", "rei\u00b7nen", "Wunsch", "er\u00b7h\u00f6\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Philint, der blos ein Herz und nichts von dem besa\u00df,", "tokens": ["Phi\u00b7lint", ",", "der", "blos", "ein", "Herz", "und", "nichts", "von", "dem", "be\u00b7sa\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ADV", "ART", "NN", "KON", "PIS", "APPR", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was man zum Gl\u00fcck des Lebens z\u00e4hlet,", "tokens": ["Was", "man", "zum", "Gl\u00fcck", "des", "Le\u00b7bens", "z\u00e4h\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Schlich traurig vor den Baum: \u00bbO G\u00f6ttin! gieb mir das,", "tokens": ["Schlich", "trau\u00b7rig", "vor", "den", "Baum", ":", "\u00bb", "O", "G\u00f6t\u00b7tin", "!", "gieb", "mir", "das", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "APPR", "ART", "NN", "$.", "$(", "NE", "NE", "$.", "VVIMP", "PPER", "PDS", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Was mir zu meiner Ruhe fehlet.\u00ab", "tokens": ["Was", "mir", "zu", "mei\u00b7ner", "Ru\u00b7he", "feh\u00b7let", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Er schweigt. Das Alles findest du,", "tokens": ["Er", "schweigt", ".", "Das", "Al\u00b7les", "fin\u00b7dest", "du", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ART", "PIS", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wein Sohn, in deiner eignen H\u00fctte;", "tokens": ["Wein", "Sohn", ",", "in", "dei\u00b7ner", "eig\u00b7nen", "H\u00fct\u00b7te", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "So fl\u00fcstert ihm ein holder Zephyr zu.", "tokens": ["So", "fl\u00fcs\u00b7tert", "ihm", "ein", "hol\u00b7der", "Ze\u00b7phyr", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Philint befl\u00fcgelt seine Schritte,", "tokens": ["Phi\u00b7lint", "be\u00b7fl\u00fc\u00b7gelt", "sei\u00b7ne", "Schrit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "St\u00fcrmt keichend in sein kleines Haus,", "tokens": ["St\u00fcrmt", "kei\u00b7chend", "in", "sein", "klei\u00b7nes", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Und von der Hofnung Rausch beseelet", "tokens": ["Und", "von", "der", "Hof\u00b7nung", "Rausch", "be\u00b7see\u00b7let"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Sp\u00fcrt er umsonst den letzten Winkel aus,", "tokens": ["Sp\u00fcrt", "er", "um\u00b7sonst", "den", "letz\u00b7ten", "Win\u00b7kel", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Durchsucht sich endlich selbst und findet was ihm fehlet.", "tokens": ["Durch\u00b7sucht", "sich", "end\u00b7lich", "selbst", "und", "fin\u00b7det", "was", "ihm", "feh\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ADV", "KON", "VVFIN", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Was mochte wohl der guten Dryas Gunst", "tokens": ["Was", "moch\u00b7te", "wohl", "der", "gu\u00b7ten", "Dryas", "Gunst"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.18": {"text": "Dem redlichen Philint bescheren?", "tokens": ["Dem", "red\u00b7li\u00b7chen", "Phi\u00b7lint", "be\u00b7sche\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "O Freund, den gr\u00f6sten Schatz, die seltne Kunst,", "tokens": ["O", "Freund", ",", "den", "gr\u00f6s\u00b7ten", "Schatz", ",", "die", "selt\u00b7ne", "Kunst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Was er nicht hatte, zu entbehren.", "tokens": ["Was", "er", "nicht", "hat\u00b7te", ",", "zu", "ent\u00b7beh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "VAFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}