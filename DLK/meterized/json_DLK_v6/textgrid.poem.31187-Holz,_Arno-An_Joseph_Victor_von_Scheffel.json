{"textgrid.poem.31187": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "An Joseph Victor von Scheffel", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du schwankst als Urbild hin und her", "tokens": ["Du", "schwankst", "als", "Ur\u00b7bild", "hin", "und", "her"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOUS", "NN", "PTKVZ", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Eines s\u00fcffelnden Philosophen,", "tokens": ["Ei\u00b7nes", "s\u00fcf\u00b7feln\u00b7den", "Phi\u00b7lo\u00b7so\u00b7phen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Im Magen liegen uns centnerschwer", "tokens": ["Im", "Ma\u00b7gen", "lie\u00b7gen", "uns", "cent\u00b7ner\u00b7schwer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Deine vorsintfluthlichen Strophen.", "tokens": ["Dei\u00b7ne", "vor\u00b7sint\u00b7fluth\u00b7li\u00b7chen", "Stro\u00b7phen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Jahrzehntelang lagen sie uns zur Last,", "tokens": ["Jahr\u00b7zehn\u00b7te\u00b7lang", "la\u00b7gen", "sie", "uns", "zur", "Last", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Deine altdeutsch jodelnden Leute,", "tokens": ["Dei\u00b7ne", "alt\u00b7deutsch", "jo\u00b7deln\u00b7den", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Doch dass du den Ekkhart geschrieben hast,", "tokens": ["Doch", "dass", "du", "den", "E\u00b7kkhart", "ge\u00b7schrie\u00b7ben", "hast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das danken wir dir noch heute!", "tokens": ["Das", "dan\u00b7ken", "wir", "dir", "noch", "heu\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PPER", "ADV", "ADV", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}