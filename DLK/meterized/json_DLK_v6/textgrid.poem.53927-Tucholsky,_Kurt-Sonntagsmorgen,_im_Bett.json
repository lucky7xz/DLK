{"textgrid.poem.53927": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Sonntagsmorgen, im Bett", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was \u2013 was ist?", "tokens": ["Was", "\u2013", "was", "ist", "?"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PWS", "$(", "PWS", "VAFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Ach so. Heute ist Sonntag. Da kann ich noch liegen.", "tokens": ["Ach", "so", ".", "Heu\u00b7te", "ist", "Sonn\u00b7tag", ".", "Da", "kann", "ich", "noch", "lie\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$.", "ADV", "VAFIN", "NN", "$.", "ADV", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Mit den Schultern kuscheln. Mich ans Kopfkissen schmiegen \u2013", "tokens": ["Mit", "den", "Schul\u00b7tern", "ku\u00b7scheln", ".", "Mich", "ans", "Kopf\u00b7kis\u00b7sen", "schmie\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$.", "PPER", "APPRART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Aus alter Gewohnheit wacht man sonntags immer", "tokens": ["Aus", "al\u00b7ter", "Ge\u00b7wohn\u00b7heit", "wacht", "man", "sonn\u00b7tags", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PIS", "ADV", "ADV"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "so fr\u00fch auf wie wochentags \u2013 das kommt vielleicht von dem Schimmer", "tokens": ["so", "fr\u00fch", "auf", "wie", "wo\u00b7chen\u00b7tags", "\u2013", "das", "kommt", "viel\u00b7leicht", "von", "dem", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PTKVZ", "KOKOM", "ADV", "$(", "PDS", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "da von den Jalousien \u2013 was ist denn das f\u00fcr ein Geratter und Gebraus?", "tokens": ["da", "von", "den", "Ja\u00b7lou\u00b7si\u00b7en", "\u2013", "was", "ist", "denn", "das", "f\u00fcr", "ein", "Ge\u00b7rat\u00b7ter", "und", "Ge\u00b7braus", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$(", "PWS", "VAFIN", "ADV", "ART", "APPR", "ART", "NN", "KON", "NN", "$."], "meter": "-+--+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.7": {"text": "Na, jedenfalls heute mu\u00df ich nicht raus.", "tokens": ["Na", ",", "je\u00b7den\u00b7falls", "heu\u00b7te", "mu\u00df", "ich", "nicht", "raus", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "ADV", "VMFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich kann heute ganz stille liegen und ruhn.", "tokens": ["Ich", "kann", "heu\u00b7te", "ganz", "stil\u00b7le", "lie\u00b7gen", "und", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "ADJA", "VVFIN", "KON", "VVINF", "$."], "meter": "-+---+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und mu\u00df gar nichts. Und hier kann mir keiner was tun.", "tokens": ["Und", "mu\u00df", "gar", "nichts", ".", "Und", "hier", "kann", "mir", "kei\u00b7ner", "was", "tun", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "PIS", "$.", "KON", "ADV", "VMFIN", "PPER", "PIS", "PWS", "VVINF", "$."], "meter": "++-+----+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "So ein Bett ist eigentlich eine sch\u00f6ne Sache \u2013", "tokens": ["So", "ein", "Bett", "ist", "ei\u00b7gent\u00b7lich", "ei\u00b7ne", "sch\u00f6\u00b7ne", "Sa\u00b7che", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "da m\u00fc\u00dfte noch so eine Sonnenplache", "tokens": ["da", "m\u00fc\u00df\u00b7te", "noch", "so", "ei\u00b7ne", "Son\u00b7nen\u00b7pla\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "dr\u00fcber sein, und dann f\u00e4hrt man damit \u00fcberall hin.", "tokens": ["dr\u00fc\u00b7ber", "sein", ",", "und", "dann", "f\u00e4hrt", "man", "da\u00b7mit", "\u00fc\u00b7be\u00b7rall", "hin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAINF", "$,", "KON", "ADV", "VVFIN", "PIS", "PAV", "ADV", "PTKVZ", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Woher kommt das, da\u00df ich heute so furchtbar m\u00fcde bin \u2013?", "tokens": ["Wo\u00b7her", "kommt", "das", ",", "da\u00df", "ich", "heu\u00b7te", "so", "furcht\u00b7bar", "m\u00fc\u00b7de", "bin", "\u2013", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VVFIN", "PDS", "$,", "KOUS", "PPER", "ADV", "ADV", "ADJD", "ADJD", "VAFIN", "$(", "$."], "meter": "-+-+-+---+-+-+", "measure": "unknown.measure.hexa"}}, "stanza.3": {"line.1": {"text": "Gestern abend haben wir wesentlich zu viel Schwedenpunsch getrunken,", "tokens": ["Ge\u00b7stern", "a\u00b7bend", "ha\u00b7ben", "wir", "we\u00b7sent\u00b7lich", "zu", "viel", "Schwe\u00b7den\u00b7punsch", "ge\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "VAFIN", "PPER", "ADJD", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+--+-+-+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Paul war zum Schlu\u00df ganz in seinen Sessel versunken;", "tokens": ["Paul", "war", "zum", "Schlu\u00df", "ganz", "in", "sei\u00b7nen", "Ses\u00b7sel", "ver\u00b7sun\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "APPRART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "ich habe auch noch so einen komischen Geschmack im Mund", "tokens": ["ich", "ha\u00b7be", "auch", "noch", "so", "ei\u00b7nen", "ko\u00b7mi\u00b7schen", "Ge\u00b7schmack", "im", "Mund"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADV", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "und \u2013 \u2013", "tokens": ["und", "\u2013", "\u2013"], "token_info": ["word", "punct", "punct"], "pos": ["KON", "$(", "$("], "meter": "-", "measure": "single.down"}}, "stanza.4": {"line.1": {"text": "Halb neun! Da mu\u00df ich richtig wieder eingeschlafen sein.", "tokens": ["Halb", "neun", "!", "Da", "mu\u00df", "ich", "rich\u00b7tig", "wie\u00b7der", "ein\u00b7ge\u00b7schla\u00b7fen", "sein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "CARD", "$.", "ADV", "VMFIN", "PPER", "ADJD", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Sonntagsmorgen im Bett, das ist fein.", "tokens": ["Sonn\u00b7tags\u00b7mor\u00b7gen", "im", "Bett", ",", "das", "ist", "fein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "PDS", "VAFIN", "ADJD", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Das hei\u00dft: Was nun noch kommt, ist weniger sch\u00f6n . . .", "tokens": ["Das", "hei\u00dft", ":", "Was", "nun", "noch", "kommt", ",", "ist", "we\u00b7ni\u00b7ger", "sch\u00f6n", ".", ".", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PDS", "VVFIN", "$.", "PWS", "ADV", "ADV", "VVFIN", "$,", "VAFIN", "ADV", "ADJD", "$.", "$.", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Heute mu\u00df ich zu Onkel Otto und Tante Frieda gehn \u2013", "tokens": ["Heu\u00b7te", "mu\u00df", "ich", "zu", "On\u00b7kel", "Ot\u00b7to", "und", "Tan\u00b7te", "Frie\u00b7da", "gehn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "NN", "NE", "KON", "NN", "NE", "VVINF", "$("], "meter": "+-+--+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Margot ist auch da, die keusche Lilie . . .", "tokens": ["Mar\u00b7got", "ist", "auch", "da", ",", "die", "keu\u00b7sche", "Li\u00b7lie", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NE", "VAFIN", "ADV", "ADV", "$,", "ART", "ADJA", "NN", "$.", "$.", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.6": {"text": "Warum, lieber Gott, ist man sonntags stets in Familie?", "tokens": ["Wa\u00b7rum", ",", "lie\u00b7ber", "Gott", ",", "ist", "man", "sonn\u00b7tags", "stets", "in", "Fa\u00b7mi\u00b7lie", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "ADV", "NN", "$,", "VAFIN", "PIS", "ADV", "ADV", "APPR", "NN", "$."], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "Vor Tisch sind sie beleidigt, und nach Tisch sind sie satt \u2013", "tokens": ["Vor", "Tisch", "sind", "sie", "be\u00b7lei\u00b7digt", ",", "und", "nach", "Tisch", "sind", "sie", "satt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "VVPP", "$,", "KON", "APPR", "NN", "VAFIN", "PPER", "ADJD", "$("], "meter": "-+-+-+---+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "wenn ich dran denke, wird mir jetzt schon ganz matt.", "tokens": ["wenn", "ich", "dran", "den\u00b7ke", ",", "wird", "mir", "jetzt", "schon", "ganz", "matt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "VVFIN", "$,", "VAFIN", "PPER", "ADV", "ADV", "ADV", "ADJD", "$."], "meter": "+-+--+----+", "measure": "trochaic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Abends ist Theater . . . morgen mu\u00df ich unbedingt mal mit Kempner telefonieren:", "tokens": ["A\u00b7bends", "ist", "The\u00b7a\u00b7ter", ".", ".", ".", "mor\u00b7gen", "mu\u00df", "ich", "un\u00b7be\u00b7dingt", "mal", "mit", "Kemp\u00b7ner", "te\u00b7le\u00b7fo\u00b7nie\u00b7ren", ":"], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "$.", "$.", "$.", "ADV", "VMFIN", "PPER", "ADV", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+--+-+--+-", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "Er mu\u00df mir die Diele billiger tapezieren \u2013", "tokens": ["Er", "mu\u00df", "mir", "die", "Die\u00b7le", "bil\u00b7li\u00b7ger", "ta\u00b7pe\u00b7zie\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ART", "NN", "ADJD", "VVINF", "$("], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "achtzig ist zu viel \u2013 der Junge ist wohl nicht ganz gesund!", "tokens": ["acht\u00b7zig", "ist", "zu", "viel", "\u2013", "der", "Jun\u00b7ge", "ist", "wohl", "nicht", "ganz", "ge\u00b7sund", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "VAFIN", "PTKA", "PIS", "$(", "ART", "NN", "VAFIN", "ADV", "PTKNEG", "ADV", "ADJD", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "und \u2013 \u2013", "tokens": ["und", "\u2013", "\u2013"], "token_info": ["word", "punct", "punct"], "pos": ["KON", "$(", "$("], "meter": "-", "measure": "single.down"}}, "stanza.6": {"line.1": {"text": "Halb zehn!", "tokens": ["Halb", "zehn", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "CARD", "$."], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "\u00bbwilli! Aufstehn! Aufstehn!\u00ab", "tokens": ["\u00bb", "wil\u00b7li", "!", "Auf\u00b7stehn", "!", "Auf\u00b7stehn", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "FM", "$.", "VVINF", "$.", "VVINF", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ja doch, ja!", "tokens": ["Ja", "doch", ",", "ja", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PTKANT", "ADV", "$,", "ADV", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Ich stehe ja schon auf, Mama \u2013", "tokens": ["Ich", "ste\u00b7he", "ja", "schon", "auf", ",", "Ma\u00b7ma", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PTKVZ", "$,", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Jetzt geht der Sonntag los! Nein: eigentlich ist er jetzt vorbei.", "tokens": ["Jetzt", "geht", "der", "Sonn\u00b7tag", "los", "!", "Nein", ":", "ei\u00b7gent\u00b7lich", "ist", "er", "jetzt", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$.", "PTKANT", "$.", "ADV", "VAFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Jetzt kommen die Zeitungen und Briefe und Telefon und Geschrei.", "tokens": ["Jetzt", "kom\u00b7men", "die", "Zei\u00b7tun\u00b7gen", "und", "Brie\u00b7fe", "und", "Te\u00b7le\u00b7fon", "und", "Ge\u00b7schrei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+--+---+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Das ist nun weniger geruhsam und labend . . .", "tokens": ["Das", "ist", "nun", "we\u00b7ni\u00b7ger", "ge\u00b7ruh\u00b7sam", "und", "la\u00b7bend", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "ADJD", "KON", "ADJD", "$.", "$.", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.8": {"line.1": {"text": "Aber so ist das im Leben:", "tokens": ["A\u00b7ber", "so", "ist", "das", "im", "Le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "APPRART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Das Sch\u00f6nste vom Sonntag ist der Sonnabend Abend.", "tokens": ["Das", "Sch\u00f6ns\u00b7te", "vom", "Sonn\u00b7tag", "ist", "der", "Sonn\u00b7a\u00b7bend", "A\u00b7bend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}}}}}