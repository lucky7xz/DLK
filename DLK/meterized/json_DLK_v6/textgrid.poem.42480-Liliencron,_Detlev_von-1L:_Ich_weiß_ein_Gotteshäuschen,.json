{"textgrid.poem.42480": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich wei\u00df ein Gottesh\u00e4uschen,", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df ein Gottesh\u00e4uschen,", "tokens": ["Ich", "wei\u00df", "ein", "Got\u00b7tes\u00b7h\u00e4u\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hart hinterm Deich erbaut.", "tokens": ["Hart", "hin\u00b7term", "Deich", "er\u00b7baut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sein Name \u00bbJesusbl\u00f6dlein\u00ab", "tokens": ["Sein", "Na\u00b7me", "\u00bb", "Je\u00b7sus\u00b7bl\u00f6d\u00b7lein", "\u00ab"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PTKANT", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ist keinem leicht vertraut.", "tokens": ["Ist", "kei\u00b7nem", "leicht", "ver\u00b7traut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADJD", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein Bild \u00fcberm Altare", "tokens": ["Ein", "Bild", "\u00fc\u00b7berm", "Al\u00b7ta\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN"], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "H\u00e4ngt da seit alter Zeit;", "tokens": ["H\u00e4ngt", "da", "seit", "al\u00b7ter", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein gro\u00dfer Genter Maler", "tokens": ["Ein", "gro\u00b7\u00dfer", "Gen\u00b7ter", "Ma\u00b7ler"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erschuf es gottbereit.", "tokens": ["Er\u00b7schuf", "es", "gott\u00b7be\u00b7reit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der lautre Christusj\u00fcngling:", "tokens": ["Der", "laut\u00b7re", "Chris\u00b7tus\u00b7j\u00fcng\u00b7ling", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sein Auge strahlt ins Feld.", "tokens": ["Sein", "Au\u00b7ge", "strahlt", "ins", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So ging in erster Jugend", "tokens": ["So", "ging", "in", "ers\u00b7ter", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Herr wohl durch die Welt.", "tokens": ["Der", "Herr", "wohl", "durch", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sein Antlitz ohne Strenge,", "tokens": ["Sein", "Ant\u00b7litz", "oh\u00b7ne", "Stren\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Voll zarter Bl\u00f6digkeit,", "tokens": ["Voll", "zar\u00b7ter", "Bl\u00f6\u00b7dig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Voll innigster Menschenliebe,", "tokens": ["Voll", "in\u00b7nigs\u00b7ter", "Men\u00b7schen\u00b7lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von keinem Arg entweiht.", "tokens": ["Von", "kei\u00b7nem", "Arg", "ent\u00b7weiht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die S\u00fcnden abzub\u00fc\u00dfen,", "tokens": ["Die", "S\u00fcn\u00b7den", "ab\u00b7zu\u00b7b\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hat es das Volk bestellt", "tokens": ["Hat", "es", "das", "Volk", "be\u00b7stellt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bei jenem gro\u00dfen Meister", "tokens": ["Bei", "je\u00b7nem", "gro\u00b7\u00dfen", "Meis\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcr eine F\u00fclle Geld.", "tokens": ["F\u00fcr", "ei\u00b7ne", "F\u00fcl\u00b7le", "Geld", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Weit vor dem heutigen Deiche", "tokens": ["Weit", "vor", "dem", "heu\u00b7ti\u00b7gen", "Dei\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Lag Stadt und Dorf im Land.", "tokens": ["Lag", "Stadt", "und", "Dorf", "im", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann kamen wilde Fluten,", "tokens": ["Dann", "ka\u00b7men", "wil\u00b7de", "Flu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Worin die Marsch verschwand.", "tokens": ["Wo\u00b7rin", "die", "Marsch", "ver\u00b7schwand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und Alles war verschwunden,", "tokens": ["Und", "Al\u00b7les", "war", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im Wellenkamf zerw\u00fchlt.", "tokens": ["Im", "Wel\u00b7len\u00b7kamf", "zer\u00b7w\u00fchlt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Bild allein schwamm oben", "tokens": ["Das", "Bild", "al\u00b7lein", "schwamm", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und ist hierher gesp\u00fclt.", "tokens": ["Und", "ist", "hier\u00b7her", "ge\u00b7sp\u00fclt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PAV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Da haben sie von neuem,", "tokens": ["Da", "ha\u00b7ben", "sie", "von", "neu\u00b7em", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dicht hinterm Winterdeich,", "tokens": ["Dicht", "hin\u00b7term", "Win\u00b7ter\u00b7deich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Kirchlein aufgerichtet,", "tokens": ["Ein", "Kirch\u00b7lein", "auf\u00b7ge\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da h\u00e4ngt das Bild zugleich.", "tokens": ["Da", "h\u00e4ngt", "das", "Bild", "zu\u00b7gleich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Von Wettern oft umdunkelt,", "tokens": ["Von", "Wet\u00b7tern", "oft", "um\u00b7dun\u00b7kelt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Ebbe, Sturm und Flut:", "tokens": ["In", "Eb\u00b7be", ",", "Sturm", "und", "Flut", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Bildnis leuchtet ruhig", "tokens": ["Das", "Bild\u00b7nis", "leuch\u00b7tet", "ru\u00b7hig"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In hoher Himmelshut.", "tokens": ["In", "ho\u00b7her", "Him\u00b7mels\u00b7hut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Einst auf dem Deich, im Fr\u00fchling,", "tokens": ["Einst", "auf", "dem", "Deich", ",", "im", "Fr\u00fch\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sah ich hinaus aufs Meer,", "tokens": ["Sah", "ich", "hin\u00b7aus", "aufs", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APZR", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das wie der Friede feiert \u2013", "tokens": ["Das", "wie", "der", "Frie\u00b7de", "fei\u00b7ert", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "KOKOM", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Herz war w\u00fcst und schwer.", "tokens": ["Mein", "Herz", "war", "w\u00fcst", "und", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Ich wandte mich ins Kirchlein,", "tokens": ["Ich", "wand\u00b7te", "mich", "ins", "Kirch\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Weit offen klafft das Tor,", "tokens": ["Weit", "of\u00b7fen", "klafft", "das", "Tor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und schaute auf den Heiland,", "tokens": ["Und", "schau\u00b7te", "auf", "den", "Hei\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Stand tief erregt davor.", "tokens": ["Stand", "tief", "er\u00b7regt", "da\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVFIN", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Und seiner Augen Klarheit", "tokens": ["Und", "sei\u00b7ner", "Au\u00b7gen", "Klar\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sank mir ins Herz herein.", "tokens": ["Sank", "mir", "ins", "Herz", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich bog ihm meine Stirne:", "tokens": ["Ich", "bog", "ihm", "mei\u00b7ne", "Stir\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du sollst mein H\u00fcter sein.", "tokens": ["Du", "sollst", "mein", "H\u00fc\u00b7ter", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}