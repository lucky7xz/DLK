{"textgrid.poem.36249": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Sterne", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du staunest, o Mensch, was heilig wir strahlen?", "tokens": ["Du", "stau\u00b7nest", ",", "o", "Mensch", ",", "was", "hei\u00b7lig", "wir", "strah\u00b7len", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "FM", "NN", "$,", "PRELS", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "O folgtest du nur den himmlischen Winken,", "tokens": ["O", "folg\u00b7test", "du", "nur", "den", "himm\u00b7li\u00b7schen", "Win\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Vern\u00e4hmest du besser, was freundlich wir blinken,", "tokens": ["Ver\u00b7n\u00e4h\u00b7mest", "du", "bes\u00b7ser", ",", "was", "freund\u00b7lich", "wir", "blin\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "PRELS", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Wie w\u00e4ren verschwunden die irdischen Qualen!", "tokens": ["Wie", "w\u00e4\u00b7ren", "ver\u00b7schwun\u00b7den", "die", "ir\u00b7di\u00b7schen", "Qua\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "VVPP", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Dann fl\u00f6sse die Liebe aus ewigen Schalen,", "tokens": ["Dann", "fl\u00f6s\u00b7se", "die", "Lie\u00b7be", "aus", "e\u00b7wi\u00b7gen", "Scha\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Es atmeten alle in reinen Azuren,", "tokens": ["Es", "at\u00b7me\u00b7ten", "al\u00b7le", "in", "rei\u00b7nen", "A\u00b7zu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Das lichtblaue Meer umschwebte die Fluren,", "tokens": ["Das", "licht\u00b7blau\u00b7e", "Meer", "um\u00b7schweb\u00b7te", "die", "Flu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Es funkelten Stern' auf den heimischen Talen.", "tokens": ["Es", "fun\u00b7kel\u00b7ten", "Stern'", "auf", "den", "hei\u00b7mi\u00b7schen", "Ta\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Aus g\u00f6ttlicher Quelle sind alle genommen,", "tokens": ["Aus", "g\u00f6tt\u00b7li\u00b7cher", "Quel\u00b7le", "sind", "al\u00b7le", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PIS", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ist jegliches Wesen nicht Eines im Chore?", "tokens": ["Ist", "jeg\u00b7li\u00b7ches", "We\u00b7sen", "nicht", "Ei\u00b7nes", "im", "Cho\u00b7re", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "PTKNEG", "PIS", "APPRART", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Nun sind ja ge\u00f6ffnet die himmlischen Tore,", "tokens": ["Nun", "sind", "ja", "ge\u00b7\u00f6ff\u00b7net", "die", "himm\u00b7li\u00b7schen", "To\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Was soll denn das bange Verzagen noch frommen?", "tokens": ["Was", "soll", "denn", "das", "ban\u00b7ge", "Ver\u00b7za\u00b7gen", "noch", "from\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ART", "ADJA", "NN", "ADV", "ADJA", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "O w\u00e4ret ihr schon zur Tiefe geklommen,", "tokens": ["O", "w\u00e4\u00b7ret", "ihr", "schon", "zur", "Tie\u00b7fe", "ge\u00b7klom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "So s\u00e4het das Haupt ihr von Sternen umflogen", "tokens": ["So", "s\u00e4\u00b7het", "das", "Haupt", "ihr", "von", "Ster\u00b7nen", "um\u00b7flo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "APPR", "NN", "VVINF"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Und spielend ums Herz die kindlichen Wogen,", "tokens": ["Und", "spie\u00b7lend", "ums", "Herz", "die", "kind\u00b7li\u00b7chen", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Zu denen die St\u00fcrme des Lebens nicht kommen.", "tokens": ["Zu", "de\u00b7nen", "die", "St\u00fcr\u00b7me", "des", "Le\u00b7bens", "nicht", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+--+--++-+-", "measure": "iambic.penta.relaxed"}}}}}