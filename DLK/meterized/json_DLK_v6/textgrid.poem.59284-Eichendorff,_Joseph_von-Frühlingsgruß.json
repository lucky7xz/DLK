{"textgrid.poem.59284": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingsgru\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es steht ein Berg in Feuer,", "tokens": ["Es", "steht", "ein", "Berg", "in", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In feurigem Morgenbrand,", "tokens": ["In", "feu\u00b7ri\u00b7gem", "Mor\u00b7gen\u00b7brand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und auf des Berges Spitze", "tokens": ["Und", "auf", "des", "Ber\u00b7ges", "Spit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Tannbaum \u00fcberm Land.", "tokens": ["Ein", "Tann\u00b7baum", "\u00fc\u00b7berm", "Land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und auf dem h\u00f6chsten Wipfel", "tokens": ["Und", "auf", "dem", "h\u00f6chs\u00b7ten", "Wip\u00b7fel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Steh ich und schau vom Baum,", "tokens": ["Steh", "ich", "und", "schau", "vom", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O Welt, du sch\u00f6ne Welt, du,", "tokens": ["O", "Welt", ",", "du", "sch\u00f6\u00b7ne", "Welt", ",", "du", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "ADJA", "NN", "$,", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Man sieht dich vor Bl\u00fcten kaum!", "tokens": ["Man", "sieht", "dich", "vor", "Bl\u00fc\u00b7ten", "kaum", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "APPR", "NN", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}