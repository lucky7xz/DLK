{"textgrid.poem.53606": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Wider die Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die brave Hausfrau liest im Bl\u00e4ttchen", "tokens": ["Die", "bra\u00b7ve", "Haus\u00b7frau", "liest", "im", "Bl\u00e4tt\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "von Lastern selten dustrer Art,", "tokens": ["von", "Las\u00b7tern", "sel\u00b7ten", "dust\u00b7rer", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "vom Marktpreis flei\u00dfiger Erzkokettchen,", "tokens": ["vom", "Markt\u00b7preis", "flei\u00b7\u00dfi\u00b7ger", "Erz\u00b7ko\u00b7kett\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "vom Lustgreis auch mit Fu\u00dfsackbart.", "tokens": ["vom", "Lust\u00b7greis", "auch", "mit", "Fu\u00df\u00b7sack\u00b7bart", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mein Gott, denkt sich die junge Gattin,", "tokens": ["Mein", "Gott", ",", "denkt", "sich", "die", "jun\u00b7ge", "Gat\u00b7tin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "mein Gott! welch ein Spektakulum!", "tokens": ["mein", "Gott", "!", "welch", "ein", "Spek\u00b7ta\u00b7ku\u00b7lum", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PWAT", "ART", "NE", "$."], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "\u00bbdas schlanke Frauenzimmer hat ihn . . . \u00ab", "tokens": ["\u00bb", "das", "schlan\u00b7ke", "Frau\u00b7en\u00b7zim\u00b7mer", "hat", "ihn", ".", ".", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "VAFIN", "PPER", "$.", "$.", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ja was? Sie bringt sich reinweg um.", "tokens": ["Ja", "was", "?", "Sie", "bringt", "sich", "rein\u00b7weg", "um", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PWS", "$.", "PPER", "VVFIN", "PRF", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O Frau! Die Phantasie hat Grenzen,", "tokens": ["O", "Frau", "!", "Die", "Phan\u00b7ta\u00b7sie", "hat", "Gren\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ART", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "sie ist so eng \u2013 es gibt nicht viel.", "tokens": ["sie", "ist", "so", "eng", "\u2013", "es", "gibt", "nicht", "viel", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$(", "PPER", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nach wenigen Touren, wenigen T\u00e4nzen", "tokens": ["Nach", "we\u00b7ni\u00b7gen", "Tou\u00b7ren", ",", "we\u00b7ni\u00b7gen", "T\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "PIAT", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "ists stets das alte, gleiche Spiel.", "tokens": ["ists", "stets", "das", "al\u00b7te", ",", "glei\u00b7che", "Spiel", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der liebt die Knaben. Dieser Ziegen.", "tokens": ["Der", "liebt", "die", "Kna\u00b7ben", ".", "Die\u00b7ser", "Zie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$.", "PDAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die will die M\u00e4nner laut und fett.", "tokens": ["Die", "will", "die", "M\u00e4n\u00b7ner", "laut", "und", "fett", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die mag bei Seeoffizieren liegen.", "tokens": ["Die", "mag", "bei", "See\u00b7of\u00b7fi\u00b7zie\u00b7ren", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und der geht nur mit sich ins Bett.", "tokens": ["Und", "der", "geht", "nur", "mit", "sich", "ins", "Bett", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "ADV", "APPR", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Hausbacken schminkt sich selbst das Laster.", "tokens": ["Haus\u00b7ba\u00b7cken", "schminkt", "sich", "selbst", "das", "Las\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ADV", "ART", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Sieh hin \u2013 und Illusionen fliehn.", "tokens": ["Sieh", "hin", "\u2013", "und", "Il\u00b7lu\u00b7si\u00b7o\u00b7nen", "fliehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$(", "KON", "NN", "VVINF", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Es gr\u00fcnden noch die P\u00e4deraster", "tokens": ["Es", "gr\u00fcn\u00b7den", "noch", "die", "P\u00e4\u00b7de\u00b7ras\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u203averein f\u00fcr Unzucht, Sitz Berlin\u2039.", "tokens": ["\u203a", "ver\u00b7ein", "f\u00fcr", "Un\u00b7zucht", ",", "Sitz", "Ber\u00b7lin", "\u2039", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "APPR", "NN", "$,", "NN", "NE", "$(", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Was kann der Mensch denn mit sich machen!", "tokens": ["Was", "kann", "der", "Mensch", "denn", "mit", "sich", "ma\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "ADV", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie er sich anstellt und verrenkt:", "tokens": ["Wie", "er", "sich", "an\u00b7stellt", "und", "ver\u00b7renkt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was Neues kann er nicht entfachen.", "tokens": ["Was", "Neu\u00b7es", "kann", "er", "nicht", "ent\u00b7fa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es sind doch stets dieselben Sachen . . .", "tokens": ["Es", "sind", "doch", "stets", "die\u00b7sel\u00b7ben", "Sa\u00b7chen", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PDAT", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Geschenkt! Geschenkt!", "tokens": ["Ge\u00b7schenkt", "!", "Ge\u00b7schenkt", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVPP", "$.", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}