{"textgrid.poem.55974": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Die Marien-Prozession", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus allen T\u00fcrmen st\u00fcrzt sich, Flu\u00df um Flu\u00df,", "tokens": ["Aus", "al\u00b7len", "T\u00fcr\u00b7men", "st\u00fcrzt", "sich", ",", "Flu\u00df", "um", "Flu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PRF", "$,", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "hinwallendes Metall in solchen Massen", "tokens": ["hin\u00b7wal\u00b7len\u00b7des", "Me\u00b7tall", "in", "sol\u00b7chen", "Mas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "als sollte drunten in der Form der Gassen", "tokens": ["als", "soll\u00b7te", "drun\u00b7ten", "in", "der", "Form", "der", "Gas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VMFIN", "ADV", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "ein blanker Tag erstehn aus Bronzegu\u00df,", "tokens": ["ein", "blan\u00b7ker", "Tag", "er\u00b7stehn", "aus", "Bron\u00b7ze\u00b7gu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "an dessen Rand, geh\u00e4mmert und erhaben,", "tokens": ["an", "des\u00b7sen", "Rand", ",", "ge\u00b7h\u00e4m\u00b7mert", "und", "er\u00b7ha\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "$,", "VVPP", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "zu sehen ist der buntgebundne Zug", "tokens": ["zu", "se\u00b7hen", "ist", "der", "bunt\u00b7ge\u00b7bund\u00b7ne", "Zug"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "der leichten M\u00e4dchen und der neuen Knaben,", "tokens": ["der", "leich\u00b7ten", "M\u00e4d\u00b7chen", "und", "der", "neu\u00b7en", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und wie er Wellen schlug und trieb und trug,", "tokens": ["und", "wie", "er", "Wel\u00b7len", "schlug", "und", "trieb", "und", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "NN", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "hinabgehalten von dem ungewissen", "tokens": ["hin\u00b7ab\u00b7ge\u00b7hal\u00b7ten", "von", "dem", "un\u00b7ge\u00b7wis\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Gewicht der Fahnen und von Hindernissen", "tokens": ["Ge\u00b7wicht", "der", "Fah\u00b7nen", "und", "von", "Hin\u00b7der\u00b7nis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "gehemmt, unsichtbar wie die Hand des Herrn;", "tokens": ["ge\u00b7hemmt", ",", "un\u00b7sicht\u00b7bar", "wie", "die", "Hand", "des", "Herrn", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "KOKOM", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "und dr\u00fcben pl\u00f6tzlich beinah mitgerissen", "tokens": ["und", "dr\u00fc\u00b7ben", "pl\u00f6tz\u00b7lich", "bei\u00b7nah", "mit\u00b7ge\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "vom Aufstieg aufgescheuchter R\u00e4ucherbecken,", "tokens": ["vom", "Auf\u00b7stieg", "auf\u00b7ge\u00b7scheuch\u00b7ter", "R\u00e4u\u00b7cher\u00b7be\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "die fliegend, alle sieben, wie im Schrecken", "tokens": ["die", "flie\u00b7gend", ",", "al\u00b7le", "sie\u00b7ben", ",", "wie", "im", "Schre\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJD", "$,", "PIAT", "CARD", "$,", "PWAV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "an ihren Silberketten zerrn.", "tokens": ["an", "ih\u00b7ren", "Sil\u00b7ber\u00b7ket\u00b7ten", "zerrn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die B\u00f6schung Schauender umschlie\u00dft die Schiene,", "tokens": ["Die", "B\u00f6\u00b7schung", "Schau\u00b7en\u00b7der", "um\u00b7schlie\u00dft", "die", "Schie\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "in der das alles stockt und rauscht und rollt:", "tokens": ["in", "der", "das", "al\u00b7les", "stockt", "und", "rauscht", "und", "rollt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "PIS", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "das Kommende, das Chryselephantine,", "tokens": ["das", "Kom\u00b7men\u00b7de", ",", "das", "Chry\u00b7se\u00b7le\u00b7phan\u00b7ti\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+---+--+--", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "aus dem sich zu Balkonen Baldachine", "tokens": ["aus", "dem", "sich", "zu", "Bal\u00b7ko\u00b7nen", "Bal\u00b7da\u00b7chi\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PRF", "APPR", "NN", "NE"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "aufb\u00e4umen, schwankend im Behang von Gold.", "tokens": ["auf\u00b7b\u00e4u\u00b7men", ",", "schwan\u00b7kend", "im", "Be\u00b7hang", "von", "Gold", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "APPRART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und sie erkennen \u00fcber all dem Wei\u00dfen,", "tokens": ["Und", "sie", "er\u00b7ken\u00b7nen", "\u00fc\u00b7ber", "all", "dem", "Wei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "APPR", "PIAT", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "getragen und im spanischen Gewand,", "tokens": ["ge\u00b7tra\u00b7gen", "und", "im", "spa\u00b7ni\u00b7schen", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "das alte Standbild mit dem kleinen hei\u00dfen", "tokens": ["das", "al\u00b7te", "Stand\u00b7bild", "mit", "dem", "klei\u00b7nen", "hei\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gesichte und dem Kinde auf der Hand", "tokens": ["Ge\u00b7sich\u00b7te", "und", "dem", "Kin\u00b7de", "auf", "der", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "und knieen hin, je mehr es naht und naht,", "tokens": ["und", "kni\u00b7e\u00b7en", "hin", ",", "je", "mehr", "es", "naht", "und", "naht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$,", "ADV", "ADV", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+--+---+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "in seiner Krone ahnungslos veraltend", "tokens": ["in", "sei\u00b7ner", "Kro\u00b7ne", "ah\u00b7nungs\u00b7los", "ver\u00b7al\u00b7tend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "und immer noch das Segnen h\u00f6lzern haltend", "tokens": ["und", "im\u00b7mer", "noch", "das", "Seg\u00b7nen", "h\u00f6l\u00b7zern", "hal\u00b7tend"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "ART", "NN", "VVINF", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "aus dem sich gro\u00df geb\u00e4rdenden Brokat.", "tokens": ["aus", "dem", "sich", "gro\u00df", "ge\u00b7b\u00e4r\u00b7den\u00b7den", "Bro\u00b7kat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PRF", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+--++", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Da aber wie es an den Hingeknieten", "tokens": ["Da", "a\u00b7ber", "wie", "es", "an", "den", "Hin\u00b7ge\u00b7knie\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "KOKOM", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "vor\u00fcberkommt, die scheu von unten schaun,", "tokens": ["vor\u00b7\u00fc\u00b7ber\u00b7kommt", ",", "die", "scheu", "von", "un\u00b7ten", "schaun", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "ADJD", "APPR", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "da scheint es seinen Tr\u00e4gern zu gebieten", "tokens": ["da", "scheint", "es", "sei\u00b7nen", "Tr\u00e4\u00b7gern", "zu", "ge\u00b7bie\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "mit einem Hochziehn seiner Augenbraun,", "tokens": ["mit", "ei\u00b7nem", "Hoch\u00b7ziehn", "sei\u00b7ner", "Au\u00b7gen\u00b7braun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "hochm\u00fctig, ungehalten und bestimmt:", "tokens": ["hoch\u00b7m\u00fc\u00b7tig", ",", "un\u00b7ge\u00b7hal\u00b7ten", "und", "be\u00b7stimmt", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJA", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "so da\u00df sie staunen, stehn und \u00fcberlegen", "tokens": ["so", "da\u00df", "sie", "stau\u00b7nen", ",", "stehn", "und", "\u00fc\u00b7berl\u00b7e\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "VVFIN", "$,", "VVFIN", "KON", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "und schlie\u00dflich z\u00f6gernd gehn. Sie aber nimmt,", "tokens": ["und", "schlie\u00df\u00b7lich", "z\u00f6\u00b7gernd", "gehn", ".", "Sie", "a\u00b7ber", "nimmt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "VVINF", "$.", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "in sich die Schritte dieses ganzen Stromes", "tokens": ["in", "sich", "die", "Schrit\u00b7te", "die\u00b7ses", "gan\u00b7zen", "Stro\u00b7mes"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRF", "ART", "NN", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und geht, allein, wie auf erkannten Wegen", "tokens": ["und", "geht", ",", "al\u00b7lein", ",", "wie", "auf", "er\u00b7kann\u00b7ten", "We\u00b7gen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "ADV", "$,", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "dem Glockendonnern des gro\u00dfoffnen Domes", "tokens": ["dem", "Glo\u00b7cken\u00b7don\u00b7nern", "des", "gro\u00df\u00b7off\u00b7nen", "Do\u00b7mes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "auf hundert Schultern frauenhaft entgegen.", "tokens": ["auf", "hun\u00b7dert", "Schul\u00b7tern", "frau\u00b7en\u00b7haft", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}