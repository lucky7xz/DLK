{"textgrid.poem.61932": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcber die zackigen Giebel der Stadt h\u00e4ngt br\u00fctender Nebel", "tokens": ["\u00dc\u00b7ber", "die", "za\u00b7cki\u00b7gen", "Gie\u00b7bel", "der", "Stadt", "h\u00e4ngt", "br\u00fc\u00b7ten\u00b7der", "Ne\u00b7bel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "ADJA", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "D\u00fcster herab, es erschlie\u00dft kaum noch die Wimpern der Tag.", "tokens": ["D\u00fcs\u00b7ter", "her\u00b7ab", ",", "es", "er\u00b7schlie\u00dft", "kaum", "noch", "die", "Wim\u00b7pern", "der", "Tag", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PPER", "VVFIN", "ADV", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Drunten, ged\u00e4mpft vom Schnee, wogt sacht das Getriebe der Gasse;", "tokens": ["Drun\u00b7ten", ",", "ge\u00b7d\u00e4mpft", "vom", "Schnee", ",", "wogt", "sacht", "das", "Ge\u00b7trie\u00b7be", "der", "Gas\u00b7se", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVPP", "APPRART", "NN", "$,", "PWAV", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Nur undeutlich herauf dringt der verschleierte Laut.", "tokens": ["Nur", "un\u00b7deut\u00b7lich", "her\u00b7auf", "dringt", "der", "ver\u00b7schlei\u00b7er\u00b7te", "Laut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-++--+---+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Selbst die metallene Stimme des Turms ruft heiser die Stunden,", "tokens": ["Selbst", "die", "me\u00b7tal\u00b7le\u00b7ne", "Stim\u00b7me", "des", "Turms", "ruft", "hei\u00b7ser", "die", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Stockend, als schickte die Zeit stille zu stehen sich an.", "tokens": ["Sto\u00b7ckend", ",", "als", "schick\u00b7te", "die", "Zeit", "stil\u00b7le", "zu", "ste\u00b7hen", "sich", "an", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "VVFIN", "ART", "NN", "VVFIN", "PTKZU", "VVFIN", "PRF", "PTKVZ", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Trauriges Zwielicht rings! Auf, Knab', und entz\u00fcnde die Lampe!", "tokens": ["Trau\u00b7ri\u00b7ges", "Zwie\u00b7licht", "rings", "!", "Auf", ",", "Knab'", ",", "und", "ent\u00b7z\u00fcn\u00b7de", "die", "Lam\u00b7pe", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "$.", "APPR", "$,", "NN", "$,", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+--+---+--+--+-", "measure": "dactylic.di.plus"}, "line.8": {"text": "Kommt, ihr B\u00fccher, die Welt dunkelt, so fl\u00fccht' ich zu euch.", "tokens": ["Kommt", ",", "ihr", "B\u00fc\u00b7cher", ",", "die", "Welt", "dun\u00b7kelt", ",", "so", "fl\u00fccht'", "ich", "zu", "euch", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "Dich heut w\u00e4hl' ich vor allen, Horaz; mit l\u00e4chelnder Weisheit", "tokens": ["Dich", "heut", "w\u00e4hl'", "ich", "vor", "al\u00b7len", ",", "Ho\u00b7raz", ";", "mit", "l\u00e4\u00b7cheln\u00b7der", "Weis\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "VVIMP", "PPER", "APPR", "PIAT", "$,", "NE", "$.", "APPR", "ADJA", "NN"], "meter": "-+---+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Hast du des Tr\u00fcbsinns Bann oft mir gel\u00f6st wie ein Freund.", "tokens": ["Hast", "du", "des", "Tr\u00fcb\u00b7sinns", "Bann", "oft", "mir", "ge\u00b7l\u00f6st", "wie", "ein", "Freund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "NN", "ADV", "PPER", "VVPP", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+---+--+", "measure": "iambic.penta.chol"}, "line.11": {"text": "Gr\u00f6\u00dfere kenn' ich als dich; doch gerecht f\u00fcr jegliche Stimmung,", "tokens": ["Gr\u00f6\u00b7\u00dfe\u00b7re", "kenn'", "ich", "als", "dich", ";", "doch", "ge\u00b7recht", "f\u00fcr", "jeg\u00b7li\u00b7che", "Stim\u00b7mung", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "KOUS", "PPER", "$.", "ADV", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "+--+-+-+-+-+--+-", "measure": "iambic.septa.invert"}, "line.12": {"text": "Wie du den Knaben erfreut, bliebst du dem Alten getreu.", "tokens": ["Wie", "du", "den", "Kna\u00b7ben", "er\u00b7freut", ",", "bliebst", "du", "dem", "Al\u00b7ten", "ge\u00b7treu", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$,", "VVFIN", "PPER", "ART", "NN", "ADJD", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Wie dem parnassischen Fels zwei H\u00e4upter entragen, so gipfeln", "tokens": ["Wie", "dem", "par\u00b7nas\u00b7si\u00b7schen", "Fels", "zwei", "H\u00e4up\u00b7ter", "ent\u00b7ra\u00b7gen", ",", "so", "gip\u00b7feln"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "CARD", "NN", "VVPP", "$,", "ADV", "VVINF"], "meter": "+-++--+-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "\u00dcber dem Epos Homers Lyrik und Drama sich auf.", "tokens": ["\u00dc\u00b7ber", "dem", "E\u00b7pos", "Ho\u00b7mers", "Ly\u00b7rik", "und", "Dra\u00b7ma", "sich", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "NE", "KON", "NN", "PRF", "PTKVZ", "$."], "meter": "+--+-+--+-+--+", "measure": "iambic.hexa.invert"}}, "stanza.3": {"line.1": {"text": "Ob dich viele geschm\u00e4ht, Euripides, neben den Besten", "tokens": ["Ob", "dich", "vie\u00b7le", "ge\u00b7schm\u00e4ht", ",", "Eu\u00b7ri\u00b7pi\u00b7des", ",", "ne\u00b7ben", "den", "Bes\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PIS", "VVPP", "$,", "NE", "$,", "APPR", "ART", "NN"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Sei mir im bakchischen Kranz, m\u00e4chtig Erregter, gegr\u00fc\u00dft.", "tokens": ["Sei", "mir", "im", "bak\u00b7chi\u00b7schen", "Kranz", ",", "m\u00e4ch\u00b7tig", "Er\u00b7reg\u00b7ter", ",", "ge\u00b7gr\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "ADJA", "NN", "$,", "ADJD", "NN", "$,", "VVPP", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Preis' ich gewaltiger \u00c4schylus auch und Sophokles sch\u00f6ner:", "tokens": ["Preis'", "ich", "ge\u00b7wal\u00b7ti\u00b7ger", "\u00c4\u00b7schy\u00b7lus", "auch", "und", "So\u00b7phok\u00b7les", "sch\u00f6\u00b7ner", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADJA", "NN", "ADV", "KON", "NE", "ADJA", "$."], "meter": "-+-+--++-++-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "Dein Zeitalter des Kampfs spiegelte keiner wie du.", "tokens": ["Dein", "Zei\u00b7tal\u00b7ter", "des", "Kampfs", "spie\u00b7gel\u00b7te", "kei\u00b7ner", "wie", "du", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVFIN", "PIS", "KOKOM", "PPER", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Nimmer gelingt's dir, Freund, uns Pindars Lied zu beleben,", "tokens": ["Nim\u00b7mer", "ge\u00b7lingt's", "dir", ",", "Freund", ",", "uns", "Pin\u00b7dars", "Lied", "zu", "be\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NN", "$,", "PPER", "NE", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Wie's in Olympias Hain einst die Hellenen ergriff.", "tokens": ["Wie's", "in", "O\u00b7lym\u00b7pias", "Hain", "einst", "die", "Hel\u00b7le\u00b7nen", "er\u00b7griff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NE", "NE", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "Zwar wir erbaun uns noch heut an dem Tiefsinn seiner Gedanken,", "tokens": ["Zwar", "wir", "er\u00b7baun", "uns", "noch", "heut", "an", "dem", "Tief\u00b7sinn", "sei\u00b7ner", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Sp\u00fcren des Fittichs Schwung, der den Begeisterten trug,", "tokens": ["Sp\u00fc\u00b7ren", "des", "Fit\u00b7tichs", "Schwung", ",", "der", "den", "Be\u00b7geis\u00b7ter\u00b7ten", "trug", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Ahnen die Rhythmengewalt der sich k\u00fchn auft\u00fcrmenden Worte,", "tokens": ["Ah\u00b7nen", "die", "Rhyth\u00b7men\u00b7ge\u00b7walt", "der", "sich", "k\u00fchn", "auf\u00b7t\u00fcr\u00b7men\u00b7den", "Wor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "PRF", "ADJD", "ADJA", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Aber der reine Genu\u00df bleibt uns auf ewig versagt.", "tokens": ["A\u00b7ber", "der", "rei\u00b7ne", "Ge\u00b7nu\u00df", "bleibt", "uns", "auf", "e\u00b7wig", "ver\u00b7sagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PPER", "APPR", "ADJD", "VVPP", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.7": {"text": "Was ein lebendiger Schatz ihm war und ein Born der Empfindung,", "tokens": ["Was", "ein", "le\u00b7ben\u00b7di\u00b7ger", "Schatz", "ihm", "war", "und", "ein", "Born", "der", "Emp\u00b7fin\u00b7dung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "PPER", "VAFIN", "KON", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Ward zum dunklen Geweb' frostiger Namen f\u00fcr uns;", "tokens": ["Ward", "zum", "dunk\u00b7len", "Ge\u00b7web'", "fros\u00b7ti\u00b7ger", "Na\u00b7men", "f\u00fcr", "uns", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "ADJA", "NN", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Pfl\u00fcckt' er doch seinen Gesang vom ", "tokens": ["Pfl\u00fcckt'", "er", "doch", "sei\u00b7nen", "Ge\u00b7sang", "vom"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "APPRART"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.10": {"text": "Und kein forschender Flei\u00df weckt den ", "tokens": ["Und", "kein", "for\u00b7schen\u00b7der", "Flei\u00df", "weckt", "den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN", "VVFIN", "ART"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Milton deucht mir der Briten Poet; der gewaltige Shakspeare", "tokens": ["Mil\u00b7ton", "deucht", "mir", "der", "Bri\u00b7ten", "Po\u00b7et", ";", "der", "ge\u00b7wal\u00b7ti\u00b7ge", "Shak\u00b7spea\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ART", "NN", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-----+--+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ist der germanischen Welt eigen, so weit sie sich dehnt.", "tokens": ["Ist", "der", "ger\u00b7ma\u00b7ni\u00b7schen", "Welt", "ei\u00b7gen", ",", "so", "weit", "sie", "sich", "dehnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "ADJD", "$,", "ADV", "ADJD", "PPER", "PRF", "VVFIN", "$."], "meter": "-+--+-++--+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Wollt ihr den S\u00e4nger Armins mir trostlos schelten und bitter?", "tokens": ["Wollt", "ihr", "den", "S\u00e4n\u00b7ger", "Ar\u00b7mins", "mir", "trost\u00b7los", "schel\u00b7ten", "und", "bit\u00b7ter", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "NE", "PPER", "ADJD", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Scheltet die bittere Zeit, welche das Lied ihn gelehrt.", "tokens": ["Schel\u00b7tet", "die", "bit\u00b7te\u00b7re", "Zeit", ",", "wel\u00b7che", "das", "Lied", "ihn", "ge\u00b7lehrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Gern als erquickender Tau auf Lilien w\u00e4r' es gefallen,", "tokens": ["Gern", "als", "er\u00b7qui\u00b7cken\u00b7der", "Tau", "auf", "Li\u00b7li\u00b7en", "w\u00e4r'", "es", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ADJA", "NN", "APPR", "NE", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+--+--+-+-+-+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Aber ins d\u00fcrre Gezweig' schlug es als Hagelgew\u00f6lk.", "tokens": ["A\u00b7ber", "ins", "d\u00fcr\u00b7re", "Ge\u00b7zweig'", "schlug", "es", "als", "Ha\u00b7gel\u00b7ge\u00b7w\u00f6lk", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "VVFIN", "PPER", "KOUS", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}}, "stanza.7": {"line.1": {"text": "Gern auch kost' ich einmal von Byrons hei\u00dfem Gew\u00fcrztrank,", "tokens": ["Gern", "auch", "kost'", "ich", "ein\u00b7mal", "von", "By\u00b7rons", "hei\u00b7\u00dfem", "Ge\u00b7w\u00fcrz\u00b7trank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Aber den t\u00e4glichen Krug reiche mir Vater Homer.", "tokens": ["A\u00b7ber", "den", "t\u00e4g\u00b7li\u00b7chen", "Krug", "rei\u00b7che", "mir", "Va\u00b7ter", "Ho\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PPER", "NN", "NE", "$."], "meter": "+--+--++--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Nennt Epigonen uns immer! Ein Tor nur sch\u00e4mt sich des Namens,", "tokens": ["Nennt", "E\u00b7pi\u00b7go\u00b7nen", "uns", "im\u00b7mer", "!", "Ein", "Tor", "nur", "sch\u00e4mt", "sich", "des", "Na\u00b7mens", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PPER", "ADV", "$.", "ART", "NN", "ADV", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Der an die Pflicht ihn mahnt, w\u00fcrdig der V\u00e4ter zu sein.", "tokens": ["Der", "an", "die", "Pflicht", "ihn", "mahnt", ",", "w\u00fcr\u00b7dig", "der", "V\u00e4\u00b7ter", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word