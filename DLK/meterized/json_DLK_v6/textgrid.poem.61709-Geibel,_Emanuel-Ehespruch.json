{"textgrid.poem.61709": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Ehespruch", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das ist die rechte Ehe,", "tokens": ["Das", "ist", "die", "rech\u00b7te", "E\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo zweie sind gemeint,", "tokens": ["Wo", "zwei\u00b7e", "sind", "ge\u00b7meint", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Durch alles Gl\u00fcck und Wehe", "tokens": ["Durch", "al\u00b7les", "Gl\u00fcck", "und", "We\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu pilgern treu vereint:", "tokens": ["Zu", "pil\u00b7gern", "treu", "ver\u00b7eint", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Eine Stab des Andern", "tokens": ["Der", "Ei\u00b7ne", "Stab", "des", "An\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und liebe Last zugleich,", "tokens": ["Und", "lie\u00b7be", "Last", "zu\u00b7gleich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Gemeinsam Rast und Wandern", "tokens": ["Ge\u00b7mein\u00b7sam", "Rast", "und", "Wan\u00b7dern"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und Ziel das Himmelreich.", "tokens": ["Und", "Ziel", "das", "Him\u00b7mel\u00b7reich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}