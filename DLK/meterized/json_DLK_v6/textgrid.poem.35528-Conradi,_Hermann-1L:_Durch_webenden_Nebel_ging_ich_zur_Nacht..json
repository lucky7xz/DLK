{"textgrid.poem.35528": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Durch webenden Nebel ging ich zur Nacht.", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch webenden Nebel ging ich zur Nacht.", "tokens": ["Durch", "we\u00b7ben\u00b7den", "Ne\u00b7bel", "ging", "ich", "zur", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da kam mir, Christus, dein Wort in den Sinn \u2013", "tokens": ["Da", "kam", "mir", ",", "Chris\u00b7tus", ",", "dein", "Wort", "in", "den", "Sinn", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NE", "$,", "PPOSAT", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dein Wort am Kreuze: Es ist vollbracht! \u2013", "tokens": ["Dein", "Wort", "am", "Kreu\u00b7ze", ":", "Es", "ist", "voll\u00b7bracht", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$.", "PPER", "VAFIN", "VVPP", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und seine Tiefe nahm mich hin ...", "tokens": ["Und", "sei\u00b7ne", "Tie\u00b7fe", "nahm", "mich", "hin", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich ri\u00df dich zu mir! \u2013 Scharf klang mein Schritt \u2013", "tokens": ["Ich", "ri\u00df", "dich", "zu", "mir", "!", "\u2013", "Scharf", "klang", "mein", "Schritt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPER", "$.", "$(", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ich ri\u00df dich zu mir \u2013 ein gl\u00e4ubiges Kind! \u2013", "tokens": ["Ich", "ri\u00df", "dich", "zu", "mir", "\u2013", "ein", "gl\u00e4u\u00b7bi\u00b7ges", "Kind", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPER", "$(", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und was ich in Lebens\u00e4ngsten litt:", "tokens": ["Und", "was", "ich", "in", "Le\u00b7ben\u00b7s\u00e4ngs\u00b7ten", "litt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Hinstarb es wie fl\u00fcsternder Abendwind ...", "tokens": ["Hins\u00b7tarb", "es", "wie", "fl\u00fcs\u00b7tern\u00b7der", "Ab\u00b7end\u00b7wind", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "ADJA", "NN", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Bunt kreuzte der Nebel phantastischer Kreis.", "tokens": ["Bunt", "kreuz\u00b7te", "der", "Ne\u00b7bel", "phan\u00b7tas\u00b7ti\u00b7scher", "Kreis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ich ging durch die stille, die atmende Nacht \u2013", "tokens": ["Ich", "ging", "durch", "die", "stil\u00b7le", ",", "die", "at\u00b7men\u00b7de", "Nacht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Da kam es von meinen Lippen leis:", "tokens": ["Da", "kam", "es", "von", "mei\u00b7nen", "Lip\u00b7pen", "leis", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}