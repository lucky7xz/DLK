{"textgrid.poem.63702": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00dcberm Moorgrund still und schaurig", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcberm Moorgrund still und schaurig", "tokens": ["\u00dc\u00b7berm", "Mo\u00b7or\u00b7grund", "still", "und", "schau\u00b7rig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PTKVZ", "KON", "ADJD"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wie der Tag so rot vergl\u00fcht!", "tokens": ["Wie", "der", "Tag", "so", "rot", "ver\u00b7gl\u00fcht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fern ein Vogel pfeift noch traurig,", "tokens": ["Fern", "ein", "Vo\u00b7gel", "pfeift", "noch", "trau\u00b7rig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NE", "VVFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Heimwehbange, wanderm\u00fcd.", "tokens": ["Heim\u00b7weh\u00b7ban\u00b7ge", ",", "wan\u00b7der\u00b7m\u00fcd", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "PWAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nun die bleichen Nebel geisten", "tokens": ["Nun", "die", "blei\u00b7chen", "Ne\u00b7bel", "geis\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie Gespenster heimatlos,", "tokens": ["Wie", "Ge\u00b7spens\u00b7ter", "hei\u00b7mat\u00b7los", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eilen nestw\u00e4rts all die dreisten", "tokens": ["Ei\u00b7len", "nest\u00b7w\u00e4rts", "all", "die", "dreis\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "PIAT", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Waldestiere klein und gro\u00df.", "tokens": ["Wal\u00b7des\u00b7tie\u00b7re", "klein", "und", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nur der Hirsch, so scheu am Tage,", "tokens": ["Nur", "der", "Hirsch", ",", "so", "scheu", "am", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tritt hervor am Waldeshang,", "tokens": ["Tritt", "her\u00b7vor", "am", "Wal\u00b7des\u00b7hang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In dem ernsten Aug die Frage:", "tokens": ["In", "dem", "erns\u00b7ten", "Aug", "die", "Fra\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird denn ", "tokens": ["Wird", "denn"], "token_info": ["word", "word"], "pos": ["VAFIN", "ADV"], "meter": "++", "measure": "spondeus"}}, "stanza.4": {"line.1": {"text": "Wei\u00dft du nicht, da\u00df jetzt in diesen", "tokens": ["Wei\u00dft", "du", "nicht", ",", "da\u00df", "jetzt", "in", "die\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "ADV", "APPR", "PDAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weiten b\u00f6ser Spuk beginnt?", "tokens": ["Wei\u00b7ten", "b\u00f6\u00b7ser", "Spuk", "be\u00b7ginnt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wagst du's mit den Schattenriesen,", "tokens": ["Wagst", "du's", "mit", "den", "Schat\u00b7ten\u00b7rie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aberwitzig Menschenkind?", "tokens": ["A\u00b7berw\u00b7it\u00b7zig", "Men\u00b7schen\u00b7kind", "?"], "token_info": ["word", "word", "punct"], "pos": ["CARD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Sieh, ich selbst, der F\u00fcrst der Heide,", "tokens": ["Sieh", ",", "ich", "selbst", ",", "der", "F\u00fcrst", "der", "Hei\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "ADV", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ducke schauernd mein Geweih,", "tokens": ["Du\u00b7cke", "schau\u00b7ernd", "mein", "Ge\u00b7weih", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "St\u00fcrmt im grauen Zottenkleide", "tokens": ["St\u00fcrmt", "im", "grau\u00b7en", "Zot\u00b7ten\u00b7klei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nachts der Nebelwolf vorbei.", "tokens": ["Nachts", "der", "Ne\u00b7bel\u00b7wolf", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Schl\u00fcrfend trinkt er aus den Lachen,", "tokens": ["Schl\u00fcr\u00b7fend", "trinkt", "er", "aus", "den", "La\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trabt dahin auf dunkler Spur,", "tokens": ["Trabt", "da\u00b7hin", "auf", "dunk\u00b7ler", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die F\u00f6hren\u00e4ste krachen,", "tokens": ["Und", "die", "F\u00f6h\u00b7re\u00b7n\u00e4s\u00b7te", "kra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und es bebt die Kreatur.", "tokens": ["Und", "es", "bebt", "die", "Kre\u00b7a\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Wehe, wer ihm kreuzt die Pfade!", "tokens": ["We\u00b7he", ",", "wer", "ihm", "kreuzt", "die", "Pfa\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eisig pfaucht sein Schlund ihn an.", "tokens": ["Ei\u00b7sig", "pfaucht", "sein", "Schlund", "ihn", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Siehst du? \u2013 dort! \u2013 da\u00df Gott dir gnade! \u2013 \u2013", "tokens": ["Siehst", "du", "?", "\u2013", "dort", "!", "\u2013", "da\u00df", "Gott", "dir", "gna\u00b7de", "!", "\u2013", "\u2013"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "PPER", "$.", "$(", "ADV", "$.", "$(", "KOUS", "NN", "PPER", "VVFIN", "$.", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Pfeilschnell flieht der Hirsch vondann.", "tokens": ["Pfeil\u00b7schnell", "flieht", "der", "Hirsch", "von\u00b7dann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}