{"textgrid.poem.43337": {"metadata": {"author": {"name": "Leopardi, Giacomo", "birth": "N.A.", "death": "N.A."}, "title": "Iv. Zur Hochzeit der Schwester Paolina.", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun du so bald den Frieden", "tokens": ["Nun", "du", "so", "bald", "den", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des stillen Vaterhauses wirst vermissen", "tokens": ["Des", "stil\u00b7len", "Va\u00b7ter\u00b7hau\u00b7ses", "wirst", "ver\u00b7mis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und weit von deiner Jugend Trug und Wahn,", "tokens": ["Und", "weit", "von", "dei\u00b7ner", "Ju\u00b7gend", "Trug", "und", "Wahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PPOSAT", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die unser \u00f6des Land versch\u00f6nt, geschieden", "tokens": ["Die", "un\u00b7ser", "\u00f6\u00b7des", "Land", "ver\u00b7sch\u00f6nt", ",", "ge\u00b7schie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVFIN", "$,", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dich in des Lebens Staub und L\u00e4rm fortan", "tokens": ["Dich", "in", "des", "Le\u00b7bens", "Staub", "und", "L\u00e4rm", "for\u00b7tan"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "NN", "KON", "NN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Dein Schicksal ruft, nun, Schwester, sollst du wissen,", "tokens": ["Dein", "Schick\u00b7sal", "ruft", ",", "nun", ",", "Schwes\u00b7ter", ",", "sollst", "du", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "ADV", "$,", "NN", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Zu welcher Schmach der Himmel uns verdammt.", "tokens": ["Zu", "wel\u00b7cher", "Schmach", "der", "Him\u00b7mel", "uns", "ver\u00b7dammt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Sollst du ja selbst in schweren", "tokens": ["Sollst", "du", "ja", "selbst", "in", "schwe\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "APPR", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Nothjahren voller Leid", "tokens": ["Noth\u00b7jah\u00b7ren", "vol\u00b7ler", "Leid"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Des ungl\u00fccksel'gen Vaterlands unselig", "tokens": ["Des", "un\u00b7gl\u00fcck\u00b7sel'\u00b7gen", "Va\u00b7ter\u00b7lands", "un\u00b7se\u00b7lig"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Geschlecht vermehren. St\u00e4hle drum beizeit", "tokens": ["Ge\u00b7schlecht", "ver\u00b7meh\u00b7ren", ".", "St\u00e4h\u00b7le", "drum", "bei\u00b7zeit"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVINF", "$.", "NN", "PAV", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "An hohen Mustern deine S\u00f6hne. Wehren", "tokens": ["An", "ho\u00b7hen", "Mus\u00b7tern", "dei\u00b7ne", "S\u00f6h\u00b7ne", ".", "Weh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "NN", "$.", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Die G\u00f6tter doch ein fr\u00f6hlich", "tokens": ["Die", "G\u00f6t\u00b7ter", "doch", "ein", "fr\u00f6h\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ART", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Gedeihn heut jeder Kraft,", "tokens": ["Ge\u00b7deihn", "heut", "je\u00b7der", "Kraft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Und kein verz\u00e4rtelt Herz bleibt tugendhaft.", "tokens": ["Und", "kein", "ver\u00b7z\u00e4r\u00b7telt", "Herz", "bleibt", "tu\u00b7gend\u00b7haft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "VVFIN", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Elende \u2013 oder Feige", "tokens": ["E\u00b7len\u00b7de", "\u2013", "o\u00b7der", "Fei\u00b7ge"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$(", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wirst du geb\u00e4ren. La\u00df sie elend werden!", "tokens": ["Wirst", "du", "ge\u00b7b\u00e4\u00b7ren", ".", "La\u00df", "sie", "e\u00b7lend", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVINF", "$.", "VVIMP", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Denn einen Abgrund zwischen Gl\u00fcck und Werth", "tokens": ["Denn", "ei\u00b7nen", "Ab\u00b7grund", "zwi\u00b7schen", "Gl\u00fcck", "und", "Werth"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Schuf diese Zeit. Zu sp\u00e4t, da schon zur Neige", "tokens": ["Schuf", "die\u00b7se", "Zeit", ".", "Zu", "sp\u00e4t", ",", "da", "schon", "zur", "Nei\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PDAT", "NN", "$.", "PTKA", "ADJD", "$,", "KOUS", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die menschlichen Geschicke sich gekehrt,", "tokens": ["Die", "menschli\u00b7chen", "Ge\u00b7schi\u00b7cke", "sich", "ge\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Erwacht, wer heut geboren wird auf Erden.", "tokens": ["Er\u00b7wacht", ",", "wer", "heut", "ge\u00b7bo\u00b7ren", "wird", "auf", "Er\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "ADV", "VVPP", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Das \u00fcberla\u00df dem Himmel. Dir am Herzen", "tokens": ["Das", "\u00fc\u00b7berl\u00b7a\u00df", "dem", "Him\u00b7mel", ".", "Dir", "am", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "$.", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Liege die Sorge blo\u00df,", "tokens": ["Lie\u00b7ge", "die", "Sor\u00b7ge", "blo\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.9": {"text": "Nicht zu der Jagd nach Gl\u00fcck", "tokens": ["Nicht", "zu", "der", "Jagd", "nach", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Die S\u00f6hne zu erziehn, und nimmer auch", "tokens": ["Die", "S\u00f6h\u00b7ne", "zu", "er\u00b7ziehn", ",", "und", "nim\u00b7mer", "auch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,", "KON", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Zu Narr'n der Furcht und Hoffnung. Ihr Geschick", "tokens": ["Zu", "Narr'n", "der", "Furcht", "und", "Hoff\u00b7nung", ".", "Ihr", "Ge\u00b7schick"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NE", "ART", "NN", "KON", "NN", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "R\u00fchmt dann die k\u00fcnft'ge Zeit als sch\u00f6n und gro\u00df,", "tokens": ["R\u00fchmt", "dann", "die", "k\u00fcnft'\u00b7ge", "Zeit", "als", "sch\u00f6n", "und", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "KOKOM", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Da wir \u2013 nach feigem Brauch", "tokens": ["Da", "wir", "\u2013", "nach", "fei\u00b7gem", "Brauch"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "$(", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Der heuchlerischen Weisen \u2013", "tokens": ["Der", "heuch\u00b7le\u00b7ri\u00b7schen", "Wei\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Lebend'ge Tugend schm\u00e4hn und todte preisen.", "tokens": ["Le\u00b7ben\u00b7d'\u00b7ge", "Tu\u00b7gend", "schm\u00e4hn", "und", "tod\u00b7te", "prei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.3": {"line.1": {"text": "Viel hofft von euch, ihr Frauen,", "tokens": ["Viel", "hofft", "von", "euch", ",", "ihr", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Vaterland; und nicht zu Schimpf und Schaden", "tokens": ["Das", "Va\u00b7ter\u00b7land", ";", "und", "nicht", "zu", "Schimpf", "und", "Scha\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "KON", "PTKNEG", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Menschens\u00f6hne ward dem sanften Strahl", "tokens": ["Der", "Men\u00b7schen\u00b7s\u00f6h\u00b7ne", "ward", "dem", "sanf\u00b7ten", "Strahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Aus euren Augen Macht, wohin sie schauen,", "tokens": ["Aus", "eu\u00b7ren", "Au\u00b7gen", "Macht", ",", "wo\u00b7hin", "sie", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,", "PWAV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Zu b\u00e4nd'gen Feu'r und Schwert. Ihr lenkt zumal", "tokens": ["Zu", "b\u00e4n\u00b7d'\u00b7gen", "Feu'r", "und", "Schwert", ".", "Ihr", "lenkt", "zu\u00b7mal"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$.", "PPER", "VVFIN", "ADV"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Den Weisen wie den Starken klug am Faden,", "tokens": ["Den", "Wei\u00b7sen", "wie", "den", "Star\u00b7ken", "klug", "am", "Fa\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "ART", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und was die Sonn' umkreiset, neigt sich euch.", "tokens": ["Und", "was", "die", "Sonn'", "um\u00b7krei\u00b7set", ",", "neigt", "sich", "euch", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVFIN", "$,", "VVFIN", "PRF", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Drum sollt f\u00fcr diese Zeit", "tokens": ["Drum", "sollt", "f\u00fcr", "die\u00b7se", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Ihr Rechenschaft mir geben.", "tokens": ["Ihr", "Re\u00b7chen\u00b7schaft", "mir", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Der Jugend heil'ge Glut \u2013 lie\u00df ", "tokens": ["Der", "Ju\u00b7gend", "heil'\u00b7ge", "Glut", "\u2013", "lie\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Sie denn erl\u00f6schen? Ward denn unser Leben", "tokens": ["Sie", "denn", "er\u00b7l\u00f6\u00b7schen", "?", "Ward", "denn", "un\u00b7ser", "Le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVINF", "$.", "VAFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Marklos und morsch durch ", "tokens": ["Mark\u00b7los", "und", "morsch", "durch"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "APPR"], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Und Schlafsucht uns entmannt", "tokens": ["Und", "Schlaf\u00b7sucht", "uns", "ent\u00b7mannt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "PPER", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Und Nerv' und Muskel missen", "tokens": ["Und", "Ner\u00b7v'", "und", "Mus\u00b7kel", "mis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "Die alte Kraft, \u2013 habt ", "tokens": ["Die", "al\u00b7te", "Kraft", ",", "\u2013", "habt"], "token_info": ["word", "word", "word", "punct", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$,", "$(", "VAFIN"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Ein Sporn zu edlen Thaten", "tokens": ["Ein", "Sporn", "zu", "ed\u00b7len", "Tha\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist Liebe, recht erkannt, und hohes Streben", "tokens": ["Ist", "Lie\u00b7be", ",", "recht", "er\u00b7kannt", ",", "und", "ho\u00b7hes", "Stre\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "NN", "$,", "ADJD", "VVPP", "$,", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erweckt die Sch\u00f6nheit. Der ist liebeleer,", "tokens": ["Er\u00b7weckt", "die", "Sch\u00f6n\u00b7heit", ".", "Der", "ist", "lie\u00b7be\u00b7leer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "PDS", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der nicht frohlockend f\u00fchlt das Herz erbeben", "tokens": ["Der", "nicht", "froh\u00b7lo\u00b7ckend", "f\u00fchlt", "das", "Herz", "er\u00b7be\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PTKNEG", "ADJD", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "In tiefster Brust, wenn an den Felsengraten", "tokens": ["In", "tiefs\u00b7ter", "Brust", ",", "wenn", "an", "den", "Fel\u00b7sen\u00b7gra\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die St\u00fcrme toben, wenn gewitterschwer", "tokens": ["Die", "St\u00fcr\u00b7me", "to\u00b7ben", ",", "wenn", "ge\u00b7wit\u00b7ter\u00b7schwer"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "ADJA"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der Himmel sich umw\u00f6lkt und Flutgebraus", "tokens": ["Der", "Him\u00b7mel", "sich", "um\u00b7w\u00f6lkt", "und", "Flut\u00b7ge\u00b7braus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PRF", "VVPP", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Die Berge peitscht. Ihr Br\u00e4ute", "tokens": ["Die", "Ber\u00b7ge", "peitscht", ".", "Ihr", "Br\u00e4u\u00b