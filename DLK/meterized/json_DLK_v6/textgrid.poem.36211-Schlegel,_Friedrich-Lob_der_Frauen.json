{"textgrid.poem.36211": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Lob der Frauen", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein g\u00f6ttlich Spielwerk str\u00f6mt die sch\u00f6ne Welt", "tokens": ["Ein", "g\u00f6tt\u00b7lich", "Spiel\u00b7werk", "str\u00f6mt", "die", "sch\u00f6\u00b7ne", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In lichter Lebensf\u00fclle,", "tokens": ["In", "lich\u00b7ter", "Le\u00b7bens\u00b7f\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Des sch\u00f6nsten Wesens Hauch in alle Sinne;", "tokens": ["Des", "sch\u00f6ns\u00b7ten", "We\u00b7sens", "Hauch", "in", "al\u00b7le", "Sin\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das ew'ge Bild gl\u00e4nzt neu in jeder H\u00fclle,", "tokens": ["Das", "ew'\u00b7ge", "Bild", "gl\u00e4nzt", "neu", "in", "je\u00b7der", "H\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Gie\u00dft Kraft ins Herz, und h\u00e4lt", "tokens": ["Gie\u00dft", "Kraft", "ins", "Herz", ",", "und", "h\u00e4lt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "NN", "APPRART", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Das trunkne, da\u00df in Freud' es nicht zerrinne.", "tokens": ["Das", "trunk\u00b7ne", ",", "da\u00df", "in", "Freud'", "es", "nicht", "zer\u00b7rin\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "KOUS", "APPR", "NN", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Du heil'ge, lockst den Geist zu ew'ger Minne,", "tokens": ["Du", "heil'\u00b7ge", ",", "lockst", "den", "Geist", "zu", "ew'\u00b7ger", "Min\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Natur! im Abgrund sch\u00f6n, wie in den Funken", "tokens": ["Na\u00b7tur", "!", "im", "Ab\u00b7grund", "sch\u00f6n", ",", "wie", "in", "den", "Fun\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "APPRART", "NN", "ADJD", "$,", "PWAV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Des Lichts, im Tod und in des Lebens Welle;", "tokens": ["Des", "Lichts", ",", "im", "Tod", "und", "in", "des", "Le\u00b7bens", "Wel\u00b7le", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPRART", "NN", "KON", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Du aller Sch\u00f6nheit Quelle,", "tokens": ["Du", "al\u00b7ler", "Sch\u00f6n\u00b7heit", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Aus deren \u00fcpp'gem Scho\u00dfe sonnentrunken", "tokens": ["Aus", "de\u00b7ren", "\u00fcpp'\u00b7gem", "Scho\u00b7\u00dfe", "son\u00b7nen\u00b7trun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Das mut'ge Tier entquillt, die holde Pflanze,", "tokens": ["Das", "mut'\u00b7ge", "Tier", "ent\u00b7quillt", ",", "die", "hol\u00b7de", "Pflan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Der vollen Erde Brust zum bunten Kranze.", "tokens": ["Der", "vol\u00b7len", "Er\u00b7de", "Brust", "zum", "bun\u00b7ten", "Kran\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch m\u00fcssen alle Erdenkinder weichen", "tokens": ["Doch", "m\u00fcs\u00b7sen", "al\u00b7le", "Er\u00b7den\u00b7kin\u00b7der", "wei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem hohen Menschenbilde,", "tokens": ["Dem", "ho\u00b7hen", "Men\u00b7schen\u00b7bil\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Aus dessen Aug' das All sich selbst beschaut,", "tokens": ["Aus", "des\u00b7sen", "Aug'", "das", "All", "sich", "selbst", "be\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ART", "NN", "PRF", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Des k\u00fchnes Haupt am himmlischen Gefilde", "tokens": ["Des", "k\u00fch\u00b7nes", "Haupt", "am", "himm\u00b7li\u00b7schen", "Ge\u00b7fil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Sterne mag vergleichen,", "tokens": ["Die", "Ster\u00b7ne", "mag", "ver\u00b7glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und deuten, was im fernen Morgen graut.", "tokens": ["Und", "deu\u00b7ten", ",", "was", "im", "fer\u00b7nen", "Mor\u00b7gen", "graut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "PRELS", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Aus allen Zeiten, Zungen flie\u00dft Ein Laut,", "tokens": ["Aus", "al\u00b7len", "Zei\u00b7ten", ",", "Zun\u00b7gen", "flie\u00dft", "Ein", "Laut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wie Sonn' und Erde Eins im Lichte strahlen,", "tokens": ["Wie", "Sonn'", "und", "Er\u00b7de", "Eins", "im", "Lich\u00b7te", "strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Vergangne, k\u00fcnft'ge, jetz'ge Geister bindend,", "tokens": ["Ver\u00b7gang\u00b7ne", ",", "k\u00fcnft'\u00b7ge", ",", "jetz'\u00b7ge", "Geis\u00b7ter", "bin\u00b7dend", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Die heil'ge Kunst erfindend,", "tokens": ["Die", "heil'\u00b7ge", "Kunst", "er\u00b7fin\u00b7dend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Und bildet ew'ger Liebe s\u00fc\u00dfe Qualen.", "tokens": ["Und", "bil\u00b7det", "ew'\u00b7ger", "Lie\u00b7be", "s\u00fc\u00b7\u00dfe", "Qua\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Der Mensch nur l\u00e4chelt, selbst sein holder Sp\u00f6tter;", "tokens": ["Der", "Mensch", "nur", "l\u00e4\u00b7chelt", ",", "selbst", "sein", "hol\u00b7der", "Sp\u00f6t\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Aus seinem Haupt entsprangen alle G\u00f6tter.", "tokens": ["Aus", "sei\u00b7nem", "Haupt", "ent\u00b7spran\u00b7gen", "al\u00b7le", "G\u00f6t\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Das Urbild solcher Bildung bl\u00fcht im Weibe;", "tokens": ["Das", "Ur\u00b7bild", "sol\u00b7cher", "Bil\u00b7dung", "bl\u00fcht", "im", "Wei\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es ist der Menschheit Blume,", "tokens": ["Es", "ist", "der", "Menschheit", "Blu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die selig duftet stille Liebesflammen.", "tokens": ["Die", "se\u00b7lig", "duf\u00b7tet", "stil\u00b7le", "Lie\u00b7bes\u00b7flam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Frauen Reiz nur gl\u00e4nzt im lichten Ruhme;", "tokens": ["Der", "Frau\u00b7en", "Reiz", "nur", "gl\u00e4nzt", "im", "lich\u00b7ten", "Ruh\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Aus ihrem s\u00fc\u00dfen Leibe", "tokens": ["Aus", "ih\u00b7rem", "s\u00fc\u00b7\u00dfen", "Lei\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Blitzt Kraft in jene, die vom Himmel stammen.", "tokens": ["Blitzt", "Kraft", "in", "je\u00b7ne", ",", "die", "vom", "Him\u00b7mel", "stam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "PDS", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Schmilz aller M\u00e4nner Macht und Geist zusammen;", "tokens": ["Schmilz", "al\u00b7ler", "M\u00e4n\u00b7ner", "Macht", "und", "Geist", "zu\u00b7sam\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Was gro\u00df und w\u00fcrdig, m\u00f6gen sie erringen,", "tokens": ["Was", "gro\u00df", "und", "w\u00fcr\u00b7dig", ",", "m\u00f6\u00b7gen", "sie", "er\u00b7rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "KON", "ADJD", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Zur Sch\u00f6nheit wird die Freud'gen Lieb' entz\u00fcnden.", "tokens": ["Zur", "Sch\u00f6n\u00b7heit", "wird", "die", "Freu\u00b7d'\u00b7gen", "Lieb'", "ent\u00b7z\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Den Gott im Werk verk\u00fcnden,", "tokens": ["Den", "Gott", "im", "Werk", "ver\u00b7k\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Lehrt Lieb' und auch durch Tat zu ihm sich schwingen;", "tokens": ["Lehrt", "Lieb'", "und", "auch", "durch", "Tat", "zu", "ihm", "sich", "schwin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "ADV", "APPR", "NN", "APPR", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Und Liebe kann der Milden Hand nur geben,", "tokens": ["Und", "Lie\u00b7be", "kann", "der", "Mil\u00b7den", "Hand", "nur", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VMFIN", "ART", "NN", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die kindlich der Natur im Scho\u00df noch leben.", "tokens": ["Die", "kind\u00b7lich", "der", "Na\u00b7tur", "im", "Scho\u00df", "noch", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "NN", "APPRART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nie hat so treu der Freund den Freund gefunden,", "tokens": ["Nie", "hat", "so", "treu", "der", "Freund", "den", "Freund", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADJD", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als sanfte Frau'n oft waren,", "tokens": ["Als", "sanf\u00b7te", "Frau'n", "oft", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn's mutig galt, ans Herz des Liebsten hin", "tokens": ["Wenn's", "mu\u00b7tig", "galt", ",", "ans", "Herz", "des", "Liebs\u00b7ten", "hin"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "VVFIN", "$,", "APPRART", "NN", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Zu dringen durch den Tod und durch Gefahren;", "tokens": ["Zu", "drin\u00b7gen", "durch", "den", "Tod", "und", "durch", "Ge\u00b7fah\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dem Einz'gen fest verbunden,", "tokens": ["Dem", "Einz'\u00b7gen", "fest", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Nichts achtend allen Glanz und Weltgewinn.", "tokens": ["Nichts", "ach\u00b7tend", "al\u00b7len", "Glanz", "und", "Welt\u00b7ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Aus tiefer Lieb' erzeugt und zartem Sinn,", "tokens": ["Aus", "tie\u00b7fer", "Lieb'", "er\u00b7zeugt", "und", "zar\u00b7tem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Bl\u00fcht sch\u00f6n in Frau'n der Tugend milde Frucht,", "tokens": ["Bl\u00fcht", "sch\u00f6n", "in", "Frau'n", "der", "Tu\u00b7gend", "mil\u00b7de", "Frucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Verstand und Frieden gl\u00e4nzt vom Angesichte,", "tokens": ["Ver\u00b7stand", "und", "Frie\u00b7den", "gl\u00e4nzt", "vom", "An\u00b7ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Das Aug' in heiterm Lichte", "tokens": ["Das", "Aug'", "in", "hei\u00b7term", "Lich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Blickt freundlich l\u00e4chelnd auf des