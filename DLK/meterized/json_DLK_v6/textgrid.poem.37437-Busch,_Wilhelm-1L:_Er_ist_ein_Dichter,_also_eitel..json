{"textgrid.poem.37437": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Er ist ein Dichter, also eitel.", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er ist ein Dichter, also eitel.", "tokens": ["Er", "ist", "ein", "Dich\u00b7ter", ",", "al\u00b7so", "ei\u00b7tel", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und, bitte, nehmt es ihm nicht krumm,", "tokens": ["Und", ",", "bit\u00b7te", ",", "nehmt", "es", "ihm", "nicht", "krumm", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PTKANT", "$,", "VVFIN", "PPER", "PPER", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zieht er aus seinem L\u00fcgenbeutel", "tokens": ["Zieht", "er", "aus", "sei\u00b7nem", "L\u00fc\u00b7gen\u00b7beu\u00b7tel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So allerlei Brimborium.", "tokens": ["So", "al\u00b7ler\u00b7lei", "Brim\u00b7bo\u00b7ri\u00b7um", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Juwelen, Gold und stolze Namen,", "tokens": ["Ju\u00b7we\u00b7len", ",", "Gold", "und", "stol\u00b7ze", "Na\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "ADJA", "NN", "$,"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Ein hohes Schlo\u00df im Mondenschein", "tokens": ["Ein", "ho\u00b7hes", "Schlo\u00df", "im", "Mon\u00b7den\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sch\u00f6ne, h\u00f6chstverliebte Damen,", "tokens": ["Und", "sch\u00f6\u00b7ne", ",", "h\u00f6chst\u00b7ver\u00b7lieb\u00b7te", "Da\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dies alles nennt der Dichter sein.", "tokens": ["Dies", "al\u00b7les", "nennt", "der", "Dich\u00b7ter", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Indessen ist ein enges St\u00fcbchen", "tokens": ["In\u00b7des\u00b7sen", "ist", "ein", "en\u00b7ges", "St\u00fcb\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sein ungeheizter Aufenthalt.", "tokens": ["Sein", "un\u00b7ge\u00b7heiz\u00b7ter", "Auf\u00b7ent\u00b7halt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er hat kein Geld, er hat kein Liebchen,", "tokens": ["Er", "hat", "kein", "Geld", ",", "er", "hat", "kein", "Lieb\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,", "PPER", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und seine F\u00fc\u00dfe werden kalt.", "tokens": ["Und", "sei\u00b7ne", "F\u00fc\u00b7\u00dfe", "wer\u00b7den", "kalt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}