{"dta.poem.19059": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Xl.  Uber die verkehrte Anwendung des  \n achten Gebots.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Wje kommt es immermehr? wenn man des Teufels ist,", "tokens": ["Wie", "kommt", "es", "im\u00b7mer\u00b7mehr", "?", "wenn", "man", "des", "Teu\u00b7fels", "ist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "$.", "KOUS", "PIS", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So gilts Entschuldigen, und alls zum Besten kehren; ", "tokens": ["So", "gilts", "Ent\u00b7schul\u00b7di\u00b7gen", ",", "und", "alls", "zum", "Bes\u00b7ten", "keh\u00b7ren", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$,", "KON", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie, da\u00df man diese Pflicht gleich gegen uns vergi\u00dft,", "tokens": ["Wie", ",", "da\u00df", "man", "die\u00b7se", "Pflicht", "gleich", "ge\u00b7gen", "uns", "ver\u00b7gi\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "KOUS", "PIS", "PDAT", "NN", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn wir zu GOtt bekehrt, und JEsu zugeh\u00f6ren?", "tokens": ["Wenn", "wir", "zu", "Gott", "be\u00b7kehrt", ",", "und", "Je\u00b7su", "zu\u00b7ge\u00b7h\u00f6\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVPP", "$,", "KON", "NE", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie, da\u00df man einen Klotz im Auge nicht ersieht,", "tokens": ["Wie", ",", "da\u00df", "man", "ei\u00b7nen", "Klotz", "im", "Au\u00b7ge", "nicht", "er\u00b7sieht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "KOUS", "PIS", "ART", "NN", "APPRART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und zum Pr\u00e4servativ an allen ", "tokens": ["Und", "zum", "Pr\u00e4\u00b7ser\u00b7va\u00b7tiv", "an", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "APPR", "PIAT"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Gewi\u00df, ein Christe hat viel Ungelegenheit.", "tokens": ["Ge\u00b7wi\u00df", ",", "ein", "Chris\u00b7te", "hat", "viel", "Un\u00b7ge\u00b7le\u00b7gen\u00b7heit", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vor ein nat\u00fcrlich Mensch bleibt immer gute Meynung,", "tokens": ["Vor", "ein", "na\u00b7t\u00fcr\u00b7lich", "Mensch", "bleibt", "im\u00b7mer", "gu\u00b7te", "Mey\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und ist doch ein Gef\u00e4\u00df ", "tokens": ["Und", "ist", "doch", "ein", "Ge\u00b7f\u00e4\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da jener Hofnung hat zur seligen Erscheinung.", "tokens": ["Da", "je\u00b7ner", "Hof\u00b7nung", "hat", "zur", "se\u00b7li\u00b7gen", "Er\u00b7schei\u00b7nung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "VAFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die kleinste Neu-Geburt geh in das Hau\u00df der Ruh;", "tokens": ["Die", "kleins\u00b7te", "Neu\u00b7Ge\u00b7burt", "geh", "in", "das", "Hau\u00df", "der", "Ruh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}}, "stanza.3": {"line.1": {"text": "Die Kinder sind nicht gleich, was alte Leute sind.", "tokens": ["Die", "Kin\u00b7der", "sind", "nicht", "gleich", ",", "was", "al\u00b7te", "Leu\u00b7te", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADV", "$,", "PWS", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch spielt der kleine Sohn inzwischen mit der Docke,", "tokens": ["Doch", "spielt", "der", "klei\u00b7ne", "Sohn", "in\u00b7zwi\u00b7schen", "mit", "der", "Do\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df die erwachsne Magd an ihrem Wercke spinnt.", "tokens": ["Da\u00df", "die", "er\u00b7wachs\u00b7ne", "Magd", "an", "ih\u00b7rem", "Wer\u00b7cke", "spinnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Kleine erbt das Hau\u00df, die Magd bekommt zum Rocke.", "tokens": ["Der", "Klei\u00b7ne", "erbt", "das", "Hau\u00df", ",", "die", "Magd", "be\u00b7kommt", "zum", "Ro\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der gr\u00f6ste Todte schweigt, und liegt, und steht nicht auf:", "tokens": ["Der", "gr\u00f6s\u00b7te", "Tod\u00b7te", "schweigt", ",", "und", "liegt", ",", "und", "steht", "nicht", "auf", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "KON", "VVFIN", "$,", "KON", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das Kind lallt, bi\u00df es redt, und taumelt bi\u00df zum Lauf.", "tokens": ["Das", "Kind", "lallt", ",", "bi\u00df", "es", "redt", ",", "und", "tau\u00b7melt", "bi\u00df", "zum", "Lauf", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,", "KON", "VVFIN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wer sich kein neues Hertz und neuen Geist geschaft,", "tokens": ["Wer", "sich", "kein", "neu\u00b7es", "Hertz", "und", "neu\u00b7en", "Geist", "ge\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "PIAT", "ADJA", "NN", "KON", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der liegt (und w\u00e4r er gleich der Fr\u00f6mmste) noch im Tode;", "tokens": ["Der", "liegt", "(", "und", "w\u00e4r", "er", "gleich", "der", "Fr\u00f6mms\u00b7te", ")", "noch", "im", "To\u00b7de", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$(", "KON", "VAFIN", "PPER", "ADV", "ART", "NN", "$(", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ist einer noch so gut, und fehlt ihm nichts als Kraft:", "tokens": ["Ist", "ei\u00b7ner", "noch", "so", "gut", ",", "und", "fehlt", "ihm", "nichts", "als", "Kraft", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADV", "ADV", "ADJD", "$,", "KON", "VVFIN", "PPER", "PIS", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So bleibt der Hof des HErrn bey seiner alten Mode.", "tokens": ["So", "bleibt", "der", "Hof", "des", "Herrn", "bey", "sei\u00b7ner", "al\u00b7ten", "Mo\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das R\u00fchmen ist umsonst; da l\u00e4st man keinen ein,", "tokens": ["Das", "R\u00fch\u00b7men", "ist", "um\u00b7sonst", ";", "da", "l\u00e4st", "man", "kei\u00b7nen", "ein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$.", "ADV", "VVFIN", "PIS", "PIAT", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Er mu\u00df von oben her aus Geist gezeuget seyn.", "tokens": ["Er", "mu\u00df", "von", "o\u00b7ben", "her", "aus", "Geist", "ge\u00b7zeu\u00b7get", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ADV", "ADV", "APPR", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wer gleich nicht gar verstockt, und ohne F\u00fchlung ist;", "tokens": ["Wer", "gleich", "nicht", "gar", "ver\u00b7stockt", ",", "und", "oh\u00b7ne", "F\u00fch\u00b7lung", "ist", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PTKNEG", "ADV", "VVPP", "$,", "KON", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dergleichen Leute kaum errettet werden k\u00f6nnen,", "tokens": ["Derg\u00b7lei\u00b7chen", "Leu\u00b7te", "kaum", "er\u00b7ret\u00b7tet", "wer\u00b7den", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "NN", "ADV", "VVPP", "VAINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil sich ihr frecher Stoltz auch gegen GOtt vermi\u00dft,", "tokens": ["Weil", "sich", "ihr", "fre\u00b7cher", "Stoltz", "auch", "ge\u00b7gen", "Gott", "ver\u00b7mi\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "ADJA", "NN", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und sie mit Sprung und Streich in ihr Verderben rennen,", "tokens": ["Und", "sie", "mit", "Sprung", "und", "Streich", "in", "ihr", "Ver\u00b7der\u00b7ben", "ren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der ist doch oftermals tief in das Netz verstrickt,", "tokens": ["Der", "ist", "doch", "of\u00b7ter\u00b7mals", "tief", "in", "das", "Netz", "ver\u00b7strickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "ADJD", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Das unser Feind mit Geld, und Ehr und Lust gespickt.", "tokens": ["Das", "un\u00b7ser", "Feind", "mit", "Geld", ",", "und", "Ehr", "und", "Lust", "ge\u00b7spickt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "APPR", "NN", "$,", "KON", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Weil Satan seine Brut in Finsternissen heckt;", "tokens": ["Weil", "Sa\u00b7tan", "sei\u00b7ne", "Brut", "in", "Fins\u00b7ter\u00b7nis\u00b7sen", "heckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So sorgt er, da\u00df sie ja die Furcht der Nacht nicht f\u00fchlen;", "tokens": ["So", "sorgt", "er", ",", "da\u00df", "sie", "ja", "die", "Furcht", "der", "Nacht", "nicht", "f\u00fch\u00b7len", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bald wird man in den Schlaf der Sicherheit gestreckt;", "tokens": ["Bald", "wird", "man", "in", "den", "Schlaf", "der", "Si\u00b7cher\u00b7heit", "ge\u00b7streckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bald aber mu\u00df man sich im Schlamm der L\u00fcste s\u00fchlen.", "tokens": ["Bald", "a\u00b7ber", "mu\u00df", "man", "sich", "im", "Schlamm", "der", "L\u00fcs\u00b7te", "s\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PIS", "PRF", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Kurtz, da sey einer tod, verstrickt in Fleischlichkeit,", "tokens": ["Kurtz", ",", "da", "sey", "ei\u00b7ner", "tod", ",", "ver\u00b7strickt", "in", "Fleischlich\u00b7keit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADV", "VAFIN", "ART", "NN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Demselben ist kein Ort bey Christo zubereit.", "tokens": ["Dem\u00b7sel\u00b7ben", "ist", "kein", "Ort", "bey", "Chris\u00b7to", "zu\u00b7be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIAT", "NN", "APPR", "NE", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}