{"textgrid.poem.51011": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Land im Herbste", "genre": "verse", "period": "N.A.", "pub_year": 1879, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die alte Heimat seh ich wieder,", "tokens": ["Die", "al\u00b7te", "Hei\u00b7mat", "seh", "ich", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Geh\u00fcllt in herbstlich feuchten Duft;", "tokens": ["Ge\u00b7h\u00fcllt", "in", "herbst\u00b7lich", "feuch\u00b7ten", "Duft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er tr\u00e4ufelt von den B\u00e4umen nieder,", "tokens": ["Er", "tr\u00e4u\u00b7felt", "von", "den", "B\u00e4u\u00b7men", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und weithin d\u00e4mmert grau die Luft.", "tokens": ["Und", "weit\u00b7hin", "d\u00e4m\u00b7mert", "grau", "die", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und grau ragt eine Flur im Grauen,", "tokens": ["Und", "grau", "ragt", "ei\u00b7ne", "Flur", "im", "Grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Drauf geht ein Mann mit weitem Schritt", "tokens": ["Drauf", "geht", "ein", "Mann", "mit", "wei\u00b7tem", "Schritt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und streut, ein Schatten nur zu schauen,", "tokens": ["Und", "streut", ",", "ein", "Schat\u00b7ten", "nur", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein graues Zeug, wohin er tritt.", "tokens": ["Ein", "grau\u00b7es", "Zeug", ",", "wo\u00b7hin", "er", "tritt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ist es der Geist verschollner Ahnen,", "tokens": ["Ist", "es", "der", "Geist", "ver\u00b7scholl\u00b7ner", "Ah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der kaum erstrittnes Land bes\u00e4t,", "tokens": ["Der", "kaum", "er\u00b7stritt\u00b7nes", "Land", "be\u00b7s\u00e4t", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Indes zu Seiten seiner Bahnen", "tokens": ["In\u00b7des", "zu", "Sei\u00b7ten", "sei\u00b7ner", "Bah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Speer in brauner Erde steht?", "tokens": ["Der", "Speer", "in", "brau\u00b7ner", "Er\u00b7de", "steht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der aus vom Kampf noch blut'gen H\u00e4nden", "tokens": ["Der", "aus", "vom", "Kampf", "noch", "blut'\u00b7gen", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "APPRART", "NN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die K\u00f6rner in die Furche wirft,", "tokens": ["Die", "K\u00f6r\u00b7ner", "in", "die", "Fur\u00b7che", "wirft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So mit dem Pflug von End zu Enden", "tokens": ["So", "mit", "dem", "Pflug", "von", "End", "zu", "En\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein j\u00fcngst vertriebnes Volk gesch\u00fcrft?", "tokens": ["Ein", "j\u00fcngst", "ver\u00b7trieb\u00b7nes", "Volk", "ge\u00b7sch\u00fcrft", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Nein, den Genossen meines Blutes", "tokens": ["Nein", ",", "den", "Ge\u00b7nos\u00b7sen", "mei\u00b7nes", "Blu\u00b7tes"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ART", "NN", "PPOSAT", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Erkenn ich, da ich ihm genaht,", "tokens": ["Er\u00b7kenn", "ich", ",", "da", "ich", "ihm", "ge\u00b7naht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der langsam schreitend, schweren Mutes", "tokens": ["Der", "lang\u00b7sam", "schrei\u00b7tend", ",", "schwe\u00b7ren", "Mu\u00b7tes"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJD", "ADJD", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Flur best\u00e4ubt mit Aschensaat.", "tokens": ["Die", "Flur", "be\u00b7st\u00e4ubt", "mit", "A\u00b7schen\u00b7saat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die m\u00fcde Scholle neu zu st\u00e4rken,", "tokens": ["Die", "m\u00fc\u00b7de", "Schol\u00b7le", "neu", "zu", "st\u00e4r\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "L\u00e4\u00dft er den toten Staub verwehn;", "tokens": ["L\u00e4\u00dft", "er", "den", "to\u00b7ten", "Staub", "ver\u00b7wehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So seh ich ihn in seinen Werken", "tokens": ["So", "seh", "ich", "ihn", "in", "sei\u00b7nen", "Wer\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gedankenvoll und einsam gehn.", "tokens": ["Ge\u00b7dan\u00b7ken\u00b7voll", "und", "ein\u00b7sam", "gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Grau ist der Schuh an seinem Fu\u00dfe,", "tokens": ["Grau", "ist", "der", "Schuh", "an", "sei\u00b7nem", "Fu\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Grau Hut und Kleid, wie Luft und Land;", "tokens": ["Grau", "Hut", "und", "Kleid", ",", "wie", "Luft", "und", "Land", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "$,", "PWAV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nun reicht er mir die Hand zum Gru\u00dfe", "tokens": ["Nun", "reicht", "er", "mir", "die", "Hand", "zum", "Gru\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und f\u00e4rbt mit Asche mir die Hand.", "tokens": ["Und", "f\u00e4rbt", "mit", "A\u00b7sche", "mir", "die", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Das alte Lied, wo ich auch bliebe,", "tokens": ["Das", "al\u00b7te", "Lied", ",", "wo", "ich", "auch", "blie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von M\u00fchsal und Verg\u00e4nglichkeit!", "tokens": ["Von", "M\u00fch\u00b7sal", "und", "Ver\u00b7g\u00e4ng\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein wenig Freiheit, wenig Liebe,", "tokens": ["Ein", "we\u00b7nig", "Frei\u00b7heit", ",", "we\u00b7nig", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Und um das Wie der arme Streit!", "tokens": ["Und", "um", "das", "Wie", "der", "ar\u00b7me", "Streit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Wohl h\u00f6r ich gr\u00fcne Halme fl\u00fcstern", "tokens": ["Wohl", "h\u00f6r", "ich", "gr\u00fc\u00b7ne", "Hal\u00b7me", "fl\u00fcs\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und ahne froher Lenze Licht;", "tokens": ["Und", "ah\u00b7ne", "fro\u00b7her", "Len\u00b7ze", "Licht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wohl blinkt ein Sichelglanz im D\u00fcstern,", "tokens": ["Wohl", "blinkt", "ein", "Si\u00b7chel\u00b7glanz", "im", "D\u00fcs\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch binden ", "tokens": ["Doch", "bin\u00b7den"], "token_info": ["word", "word"], "pos": ["KON", "VVINF"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.10": {"line.1": {"text": "Wir d\u00fcrfen selbst das Korn nicht messen,", "tokens": ["Wir", "d\u00fcr\u00b7fen", "selbst", "das", "Korn", "nicht", "mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das wir ges\u00e4t aus toter Hand;", "tokens": ["Das", "wir", "ge\u00b7s\u00e4t", "aus", "to\u00b7ter", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir gehn und werden bald vergessen,", "tokens": ["Wir", "gehn", "und", "wer\u00b7den", "bald", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und unsre Asche fliegt im Land!", "tokens": ["Und", "uns\u00b7re", "A\u00b7sche", "fliegt", "im", "Land", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}