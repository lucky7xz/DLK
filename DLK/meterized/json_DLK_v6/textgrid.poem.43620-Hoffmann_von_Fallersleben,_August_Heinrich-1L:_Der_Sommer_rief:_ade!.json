{"textgrid.poem.43620": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Sommer rief: ade!", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Sommer rief: ade!", "tokens": ["Der", "Som\u00b7mer", "rief", ":", "a\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ADV", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Das th\u00e4t dem Stieglitz weh;", "tokens": ["Das", "th\u00e4t", "dem", "Stieg\u00b7litz", "weh", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er hing das K\u00f6pflein nieder,", "tokens": ["Er", "hing", "das", "K\u00f6\u00b7pflein", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verga\u00df all seine Lieder.", "tokens": ["Ver\u00b7ga\u00df", "all", "sei\u00b7ne", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wie oft ich ihm auch rief,", "tokens": ["Wie", "oft", "ich", "ihm", "auch", "rief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Es schien, als ob er schlief.", "tokens": ["Es", "schien", ",", "als", "ob", "er", "schlief", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOKOM", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Jetzt war der Winter da,", "tokens": ["Jetzt", "war", "der", "Win\u00b7ter", "da", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wie ging's dem Stieglitz nah!", "tokens": ["Wie", "ging's", "dem", "Stieg\u00b7litz", "nah", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er zog sein buntes Kleid aus", "tokens": ["Er", "zog", "sein", "bun\u00b7tes", "Kleid", "aus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und sah voll Gram und Leid aus.", "tokens": ["Und", "sah", "voll", "Gram", "und", "Leid", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er sa\u00df so still und stumm", "tokens": ["Er", "sa\u00df", "so", "still", "und", "stumm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und sah sich nicht mal um.", "tokens": ["Und", "sah", "sich", "nicht", "mal", "um", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und endlich schmolz der Schnee,", "tokens": ["Und", "end\u00b7lich", "schmolz", "der", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der Stieglitz rief: juchhe!", "tokens": ["Der", "Stieg\u00b7litz", "rief", ":", "juch\u00b7he", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "VVFIN", "$."], "meter": "-+-++-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Die Sonne schien aufs Bauer,", "tokens": ["Die", "Son\u00b7ne", "schien", "aufs", "Bau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da war hinweg die Trauer.", "tokens": ["Da", "war", "hin\u00b7weg", "die", "Trau\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Stieglitz sprang und sang,", "tokens": ["Der", "Stieg\u00b7litz", "sprang", "und", "sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Da\u00df es gar lieblich klang.", "tokens": ["Da\u00df", "es", "gar", "lieb\u00b7lich", "klang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er sprang voll Freud' und Lust", "tokens": ["Er", "sprang", "voll", "Freud'", "und", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und sang aus voller Brust:", "tokens": ["Und", "sang", "aus", "vol\u00b7ler", "Brust", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbjetzt kommt die sch\u00f6ne Zeit an;", "tokens": ["\u00bb", "jetzt", "kommt", "die", "sch\u00f6\u00b7ne", "Zeit", "an", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.4": {"text": "Ich zieh' mein buntes Kleid an.", "tokens": ["Ich", "zieh'", "mein", "bun\u00b7tes", "Kleid", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Willkommen, Sonnenschein!", "tokens": ["Will\u00b7kom\u00b7men", ",", "Son\u00b7nen\u00b7schein", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Jetzt will ich lustig sein.\u00ab", "tokens": ["Jetzt", "will", "ich", "lus\u00b7tig", "sein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VAINF", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Herr Stieglitz, kannst du sein", "tokens": ["Herr", "Stieg\u00b7litz", ",", "kannst", "du", "sein"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NE", "$,", "VMFIN", "PPER", "VAINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Schon froh beim Sonnenschein \u2013", "tokens": ["Schon", "froh", "beim", "Son\u00b7nen\u00b7schein", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So will ich tanzen, springen,", "tokens": ["So", "will", "ich", "tan\u00b7zen", ",", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So will ich fr\u00f6hlich singen!", "tokens": ["So", "will", "ich", "fr\u00f6h\u00b7lich", "sin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nicht nur der Sonnenschein,", "tokens": ["Nicht", "nur", "der", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der ganze Lenz ist mein.", "tokens": ["Der", "gan\u00b7ze", "Lenz", "ist", "mein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}