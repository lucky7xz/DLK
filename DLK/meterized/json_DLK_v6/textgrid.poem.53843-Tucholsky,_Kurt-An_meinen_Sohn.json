{"textgrid.poem.53843": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "An meinen Sohn", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn du mal gro\u00df bist, Leopold,", "tokens": ["Wenn", "du", "mal", "gro\u00df", "bist", ",", "Leo\u00b7pold", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$,", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "dann sieh dich um in Deutschland-Preu\u00dfen,", "tokens": ["dann", "sieh", "dich", "um", "in", "Deut\u00b7schlan\u00b7dPreu\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "wo eure Flagge Schwarz-Rot-Gold", "tokens": ["wo", "eu\u00b7re", "Flag\u00b7ge", "Schwa\u00b7rz\u00b7Rot\u00b7Gold"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "im Wind weht \u00fcber lauter Preu\u00dfen.", "tokens": ["im", "Wind", "weht", "\u00fc\u00b7ber", "lau\u00b7ter", "Preu\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Stell dich auf einen Aussichtsstand,", "tokens": ["Stell", "dich", "auf", "ei\u00b7nen", "Aus\u00b7sichts\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und vor dir liegt dein Vaterland:", "tokens": ["und", "vor", "dir", "liegt", "dein", "Va\u00b7ter\u00b7land", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ganz oben thront die Schicht mit Geld,", "tokens": ["Ganz", "o\u00b7ben", "thront", "die", "Schicht", "mit", "Geld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "die hat die Kohlen, Stahl und R\u00fcben;", "tokens": ["die", "hat", "die", "Koh\u00b7len", ",", "Stahl", "und", "R\u00fc\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "die lenkt den Lauf der deutschen Welt,", "tokens": ["die", "lenkt", "den", "Lauf", "der", "deut\u00b7schen", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "die l\u00e4\u00dft die Reichswehr kr\u00e4ftig \u00fcben.", "tokens": ["die", "l\u00e4\u00dft", "die", "Reichs\u00b7wehr", "kr\u00e4f\u00b7tig", "\u00fc\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Augen gradeaus!", "tokens": ["Au\u00b7gen", "gra\u00b7de\u00b7aus", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Gehorsam harret ihres Winks", "tokens": ["Ge\u00b7hor\u00b7sam", "har\u00b7ret", "ih\u00b7res", "Winks"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "das Korps der Rache in Talaren:", "tokens": ["das", "Korps", "der", "Ra\u00b7che", "in", "Ta\u00b7la\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "die segnen rechts und w\u00fcten links,", "tokens": ["die", "seg\u00b7nen", "rechts", "und", "w\u00fc\u00b7ten", "links", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "KON", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "so lernten sies auf Seminaren.", "tokens": ["so", "lern\u00b7ten", "sies", "auf", "Se\u00b7mi\u00b7na\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Im Namen des Volkes \u2013!", "tokens": ["Im", "Na\u00b7men", "des", "Vol\u00b7kes", "\u2013", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$(", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Da schw\u00e4tzt der Reichstag, lieber Gott!", "tokens": ["Da", "schw\u00e4tzt", "der", "Reichs\u00b7tag", ",", "lie\u00b7ber", "Gott", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "H\u00f6rst du den alten Breitscheid reden?", "tokens": ["H\u00f6rst", "du", "den", "al\u00b7ten", "Breit\u00b7scheid", "re\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er ist voll Ironie und Spott \u2013", "tokens": ["Er", "ist", "voll", "I\u00b7ro\u00b7nie", "und", "Spott", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "zum Schlu\u00df bewilligen sie dann jeden", "tokens": ["zum", "Schlu\u00df", "be\u00b7wil\u00b7li\u00b7gen", "sie", "dann", "je\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV", "PIAT"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Etat.", "tokens": ["E\u00b7tat", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.5": {"line.1": {"text": "Und unter allen den Gewalten", "tokens": ["Und", "un\u00b7ter", "al\u00b7len", "den", "Ge\u00b7wal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "da kannst du, Leopold mein Sohn,", "tokens": ["da", "kannst", "du", ",", "Leo\u00b7pold", "mein", "Sohn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "NE", "PPOSAT", "NN", "$,"], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "dein Leben lang die Schnauze halten \u2013", "tokens": ["dein", "Le\u00b7ben", "lang", "die", "Schnau\u00b7ze", "hal\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "von wegen Subordination.", "tokens": ["von", "we\u00b7gen", "Sub\u00b7or\u00b7di\u00b7na\u00b7ti\u00b7on", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "APPR", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Aber lauter Republikaner,", "tokens": ["A\u00b7ber", "lau\u00b7ter", "Re\u00b7pub\u00b7li\u00b7ka\u00b7ner", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "lauter Republikaner!", "tokens": ["lau\u00b7ter", "Re\u00b7pub\u00b7li\u00b7ka\u00b7ner", "!"], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.6": {"line.1": {"text": "Und willst du wissen, wem du das", "tokens": ["Und", "willst", "du", "wis\u00b7sen", ",", "wem", "du", "das"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "VVINF", "$,", "PWS", "PPER", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "verdankst, dies Reich von kleinen Strebern:", "tokens": ["ver\u00b7dankst", ",", "dies", "Reich", "von", "klei\u00b7nen", "Stre\u00b7bern", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PDS", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "dann wein dir nicht die \u00c4uglein na\u00df \u2013", "tokens": ["dann", "wein", "dir", "nicht", "die", "\u00c4ug\u00b7lein", "na\u00df", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PPER", "PTKNEG", "ART", "NN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "dann wandle du zu deutschen Gr\u00e4bern.", "tokens": ["dann", "wand\u00b7le", "du", "zu", "deut\u00b7schen", "Gr\u00e4\u00b7bern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Auf jedem ein Gedenkstein:", "tokens": ["Auf", "je\u00b7dem", "ein", "Ge\u00b7denk\u00b7stein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Da liegen, die zu meiner Zeit", "tokens": ["Da", "lie\u00b7gen", ",", "die", "zu", "mei\u00b7ner", "Zeit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "PRELS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "aus Angst vorm Volk die eignen Ziele", "tokens": ["aus", "Angst", "vorm", "Volk", "die", "eig\u00b7nen", "Zie\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "verrieten \u2013 taktisch so gescheit!", "tokens": ["ver\u00b7rie\u00b7ten", "\u2013", "tak\u00b7tisch", "so", "ge\u00b7scheit", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "ADJD", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und klug! und \u00fcberhaupt Schlemihle.", "tokens": ["und", "klug", "!", "und", "\u00fc\u00b7ber\u00b7haupt", "Schle\u00b7mih\u00b7le", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$.", "KON", "ADV", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Sie machten schon im Umsturz schlapp", "tokens": ["Sie", "mach\u00b7ten", "schon", "im", "Um\u00b7sturz", "schlapp"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und sa\u00dfen \u00e4ngstlich auf der Banke.", "tokens": ["und", "sa\u00b7\u00dfen", "\u00e4ngst\u00b7lich", "auf", "der", "Ban\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Charakter war bei denen knapp . . .", "tokens": ["Cha\u00b7rak\u00b7ter", "war", "bei", "de\u00b7nen", "knapp", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "VAFIN", "APPR", "PRELS", "ADJD", "$.", "$.", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Leg einen Kranz auf jedes Grab", "tokens": ["Leg", "ei\u00b7nen", "Kranz", "auf", "je\u00b7des", "Grab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "und dann sag leise, leise:", "tokens": ["und", "dann", "sag", "lei\u00b7se", ",", "lei\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADJD", "$,", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Danke.", "tokens": ["Dan\u00b7ke", "."], "token_info": ["word", "punct"], "pos": ["PTKANT", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}