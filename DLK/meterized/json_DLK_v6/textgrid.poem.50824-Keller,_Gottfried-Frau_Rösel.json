{"textgrid.poem.50824": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Frau R\u00f6sel", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frau R\u00f6sel ist eine gute Frau, wie liebt sie ihren K\u00f6nig!", "tokens": ["Frau", "R\u00f6\u00b7sel", "ist", "ei\u00b7ne", "gu\u00b7te", "Frau", ",", "wie", "liebt", "sie", "ih\u00b7ren", "K\u00f6\u00b7nig", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "ART", "ADJA", "NN", "$,", "PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Den K\u00f6nig und sein ganzes Haus! und i\u00dft und trinkt so wenig!", "tokens": ["Den", "K\u00f6\u00b7nig", "und", "sein", "gan\u00b7zes", "Haus", "!", "und", "i\u00dft", "und", "trinkt", "so", "we\u00b7nig", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPOSAT", "ADJA", "NN", "$.", "KON", "VVFIN", "KON", "VVFIN", "ADV", "PIS", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Die gute arme Frau R\u00f6sel!", "tokens": ["Die", "gu\u00b7te", "ar\u00b7me", "Frau", "R\u00f6\u00b7sel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Frau R\u00f6sel hat ihren einz'gen Sohn dem K\u00f6nig \u00fcbergeben,", "tokens": ["Frau", "R\u00f6\u00b7sel", "hat", "ih\u00b7ren", "einz'\u00b7gen", "Sohn", "dem", "K\u00f6\u00b7nig", "\u00fc\u00b7ber\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Er steht und gafft am Schilderhaus, sie n\u00e4hrt mit Spinnen ihr Leben!", "tokens": ["Er", "steht", "und", "gafft", "am", "Schil\u00b7der\u00b7haus", ",", "sie", "n\u00e4hrt", "mit", "Spin\u00b7nen", "ihr", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPRART", "NN", "$,", "PPER", "VVFIN", "APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Die gute arme Frau R\u00f6sel!", "tokens": ["Die", "gu\u00b7te", "ar\u00b7me", "Frau", "R\u00f6\u00b7sel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und als es hie\u00df, der liebe Prinz wird seine Braut heimf\u00fchren,", "tokens": ["Und", "als", "es", "hie\u00df", ",", "der", "lie\u00b7be", "Prinz", "wird", "sei\u00b7ne", "Braut", "heim\u00b7f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Da sprach der Vogt: \u00bbFrau R\u00f6sel mein, ihr m\u00fc\u00dft euer Haus verzieren!\u00ab", "tokens": ["Da", "sprach", "der", "Vogt", ":", "\u00bb", "Frau", "R\u00f6\u00b7sel", "mein", ",", "ihr", "m\u00fc\u00dft", "eu\u00b7er", "Haus", "ver\u00b7zie\u00b7ren", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "$(", "NN", "NN", "PPOSAT", "$,", "PPER", "VMFIN", "PPOSAT", "NN", "VVINF", "$.", "$("], "meter": "-+--+---+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die gute arme Frau R\u00f6sel!", "tokens": ["Die", "gu\u00b7te", "ar\u00b7me", "Frau", "R\u00f6\u00b7sel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Nun hat Frau R\u00f6sel dick zu tun, wie trippelt sie und wie lauft sie!", "tokens": ["Nun", "hat", "Frau", "R\u00f6\u00b7sel", "dick", "zu", "tun", ",", "wie", "trip\u00b7pelt", "sie", "und", "wie", "lauft", "sie", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "NN", "ADJD", "PTKZU", "VVINF", "$,", "PWAV", "VVFIN", "PPER", "KON", "PWAV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Baumwollenfahnen und Goldpapier und frische Rosen kauft sie,", "tokens": ["Baum\u00b7wol\u00b7len\u00b7fah\u00b7nen", "und", "Gold\u00b7pa\u00b7pier", "und", "fri\u00b7sche", "Ro\u00b7sen", "kauft", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "+--+--+-+-+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Die gute arme Frau R\u00f6sel.", "tokens": ["Die", "gu\u00b7te", "ar\u00b7me", "Frau", "R\u00f6\u00b7sel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Sie geht zu Wald und sammelt Moos, beim Nachbar bettelt sie Schn\u00fcre", "tokens": ["Sie", "geht", "zu", "Wald", "und", "sam\u00b7melt", "Moos", ",", "beim", "Nach\u00b7bar", "bet\u00b7telt", "sie", "Schn\u00fc\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "VVFIN", "NN", "$,", "APPRART", "NN", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Und alte N\u00e4gel und solchen Quark, beim Schuster Kleister und Schmiere!", "tokens": ["Und", "al\u00b7te", "N\u00e4\u00b7gel", "und", "sol\u00b7chen", "Quark", ",", "beim", "Schus\u00b7ter", "Kleis\u00b7ter", "und", "Schmie\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "PIAT", "NN", "$,", "APPRART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Die gute arme Frau R\u00f6sel.", "tokens": ["Die", "gu\u00b7te", "ar\u00b7me", "Frau", "R\u00f6\u00b7sel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Dann keucht und schafft sie den ganzen Tag und sinnt und klopft und klittert,", "tokens": ["Dann", "keucht", "und", "schafft", "sie", "den", "gan\u00b7zen", "Tag", "und", "sinnt", "und", "klopft", "und", "klit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "KON", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+--+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Bis da\u00df das H\u00e4uslein um und um behangen und beflittert.", "tokens": ["Bis", "da\u00df", "das", "H\u00e4us\u00b7lein", "um", "und", "um", "be\u00b7han\u00b7gen", "und", "be\u00b7flit\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "NN", "APPR", "KON", "APPR", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Die gute arme Frau R\u00f6sel!", "tokens": ["Die", "gu\u00b7te", "ar\u00b7me", "Frau", "R\u00f6\u00b7sel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Herr Bunzelmann, der alles kann, hilft ihr studieren und kleben,", "tokens": ["Herr", "Bun\u00b7zel\u00b7mann", ",", "der", "al\u00b7les", "kann", ",", "hilft", "ihr", "stu\u00b7die\u00b7ren", "und", "kle\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PRELS", "PIS", "VMFIN", "$,", "VVFIN", "PPER", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Macht Wappen und Kron und Namenszug, sauft zehn Ma\u00df Bier daneben.", "tokens": ["Macht", "Wap\u00b7pen", "und", "Kron", "und", "Na\u00b7mens\u00b7zug", ",", "sauft", "zehn", "Ma\u00df", "Bier", "da\u00b7ne\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "KON", "NN", "$,", "VVFIN", "CARD", "NN", "NN", "PAV", "$."], "meter": "-+--+-+--+-++-+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Die gute arme Frau R\u00f6sel.", "tokens": ["Die", "gu\u00b7te", "ar\u00b7me", "Frau", "R\u00f6\u00b7sel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und aus dem letzten Groschen kauft sie Brot und frische Butter", "tokens": ["Und", "aus", "dem", "letz\u00b7ten", "Gro\u00b7schen", "kauft", "sie", "Brot", "und", "fri\u00b7sche", "But\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Und sitzt vergn\u00fcgt und harrt in Ruh auf die neue Landesmutter!", "tokens": ["Und", "sitzt", "ver\u00b7gn\u00fcgt", "und", "harrt", "in", "Ruh", "auf", "die", "neu\u00b7e", "Lan\u00b7des\u00b7mut\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVPP", "KON", "VVFIN", "APPR", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Die gute arme Frau R\u00f6sel!", "tokens": ["Die", "gu\u00b7te", "ar\u00b7me", "Frau", "R\u00f6\u00b7sel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Wie wird es mir so weinerlich, wenn ich dies H\u00e4uslein schaue,", "tokens": ["Wie", "wird", "es", "mir", "so", "wei\u00b7ner\u00b7lich", ",", "wenn", "ich", "dies", "H\u00e4us\u00b7lein", "schau\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PPER", "ADV", "ADJD", "$,", "KOUS", "PPER", "PDS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "So flimmerlich und schimmerlich, und drin die alte graue,", "tokens": ["So", "flim\u00b7mer\u00b7lich", "und", "schim\u00b7mer\u00b7lich", ",", "und", "drin", "die", "al\u00b7te", "grau\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,", "KON", "ADV", "ART", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Die gute arme Frau R\u00f6sel!", "tokens": ["Die", "gu\u00b7te", "ar\u00b7me", "Frau", "R\u00f6\u00b7sel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Und eh sie sich recht umgeschaut, sind schon vorbei die Wagen!", "tokens": ["Und", "eh", "sie", "sich", "recht", "um\u00b7ge\u00b7schaut", ",", "sind", "schon", "vor\u00b7bei", "die", "Wa\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "ADJD", "VVPP", "$,", "VAFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Und wie das P\u00e4rlein ausgesehn, mu\u00df sie die Nachbarn fragen,", "tokens": ["Und", "wie", "das", "P\u00e4r\u00b7lein", "aus\u00b7ge\u00b7sehn", ",", "mu\u00df", "sie", "die", "Nach\u00b7barn", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVINF", "$,", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Die kurzsichtige Frau R\u00f6sel!", "tokens": ["Die", "kurz\u00b7sich\u00b7ti\u00b7ge", "Frau", "R\u00f6\u00b7sel", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.11": {"line.1": {"text": "So schlage doch der Teufel drein! ich kann nicht mehr spa\u00dfen und narren!", "tokens": ["So", "schla\u00b7ge", "doch", "der", "Teu\u00b7fel", "drein", "!", "ich", "kann", "nicht", "mehr", "spa\u00b7\u00dfen", "und", "nar\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$.", "PPER", "VMFIN", "PTKNEG", "ADV", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+--+--+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Wie lange willst du stettig noch in deiner Blindheit verharren,", "tokens": ["Wie", "lan\u00b7ge", "willst", "du", "stet\u00b7tig", "noch", "in", "dei\u00b7ner", "Blind\u00b7heit", "ver\u00b7har\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "ADJD", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Du dummes Weib! du Frau R\u00f6sel?", "tokens": ["Du", "dum\u00b7mes", "Weib", "!", "du", "Frau", "R\u00f6\u00b7sel", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "PPER", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}