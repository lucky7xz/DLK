{"dta.poem.3647": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Vater .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es wird aus den Zeitungen vernommen,               ", "tokens": ["Es", "wird", "aus", "den", "Zei\u00b7tun\u00b7gen", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+--++--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da\u00df der heilige Sankt Niklaus werde kommen,", "tokens": ["Da\u00df", "der", "hei\u00b7li\u00b7ge", "Sankt", "Nik\u00b7laus", "wer\u00b7de", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "VVFIN", "NE", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Aus Moskau, wo er gehalten werth,", "tokens": ["Aus", "Mos\u00b7kau", ",", "wo", "er", "ge\u00b7hal\u00b7ten", "werth", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "PWAV", "PPER", "VVPP", "ADJD", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Und als ein Heilger wird geehrt;", "tokens": ["Und", "als", "ein", "Heil\u00b7ger", "wird", "ge\u00b7ehrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er ist bereits schon auf der Fahrt,", "tokens": ["Er", "ist", "be\u00b7reits", "schon", "auf", "der", "Fahrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zu besuchen die Schuljugend zart,", "tokens": ["Zu", "be\u00b7su\u00b7chen", "die", "Schul\u00b7ju\u00b7gend", "zart", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Zu sehn, was die kleinen M\u00e4gdlein und Knaben", "tokens": ["Zu", "sehn", ",", "was", "die", "klei\u00b7nen", "M\u00e4gd\u00b7lein", "und", "Kna\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "$,", "PRELS", "ART", "ADJA", "NN", "KON", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "In diesem Jahre gelernet haben,", "tokens": ["In", "die\u00b7sem", "Jah\u00b7re", "ge\u00b7ler\u00b7net", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVPP", "VAINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "In Beten, Schreiben, Singen und Lesen,", "tokens": ["In", "Be\u00b7ten", ",", "Schrei\u00b7ben", ",", "Sin\u00b7gen", "und", "Le\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Auch ob sie sind h\u00fcbsch fromm gewesen.", "tokens": ["Auch", "ob", "sie", "sind", "h\u00fcbsch", "fromm", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "VAFIN", "ADJD", "ADJD", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Er hat auch in seinem Sack verschlossen,", "tokens": ["Er", "hat", "auch", "in", "sei\u00b7nem", "Sack", "ver\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Sch\u00f6ne Puppen aus Zucker gegossen,", "tokens": ["Sch\u00f6\u00b7ne", "Pup\u00b7pen", "aus", "Zu\u00b7cker", "ge\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.13": {"text": "Den Kindern, welche h\u00fcbsch fromm w\u00e4ren,", "tokens": ["Den", "Kin\u00b7dern", ",", "wel\u00b7che", "h\u00fcbsch", "fromm", "w\u00e4\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Will er solche sch\u00f6ne Sachen verehren.", "tokens": ["Will", "er", "sol\u00b7che", "sch\u00f6\u00b7ne", "Sa\u00b7chen", "ver\u00b7eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich bitte dich Sankt Niklaus sehr,", "tokens": ["Ich", "bit\u00b7te", "dich", "Sankt", "Nik\u00b7laus", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In meinem Hause auch einkehr,", "tokens": ["In", "mei\u00b7nem", "Hau\u00b7se", "auch", "ein\u00b7kehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bring B\u00fccher, Kleider und auch Schuh,", "tokens": ["Bring", "B\u00fc\u00b7cher", ",", "Klei\u00b7der", "und", "auch", "Schuh", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "NN", "KON", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und noch viel sch\u00f6ne gute Sachen dazu,", "tokens": ["Und", "noch", "viel", "sch\u00f6\u00b7ne", "gu\u00b7te", "Sa\u00b7chen", "da\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "ADJA", "ADJA", "NN", "PAV", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "So will ich lernen wohl,", "tokens": ["So", "will", "ich", "ler\u00b7nen", "wohl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und fromm seyn, wie ich soll.", "tokens": ["Und", "fromm", "seyn", ",", "wie", "ich", "soll", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAINF", "$,", "PWAV", "PPER", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Amen.", "tokens": ["A\u00b7men", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.3": {"line.1": {"text": "Gott gr\u00fc\u00df euch lieben Kinderlein,", "tokens": ["Gott", "gr\u00fc\u00df", "euch", "lie\u00b7ben", "Kin\u00b7der\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr sollt Vater und Mutter gehorsam seyn,", "tokens": ["Ihr", "sollt", "Va\u00b7ter", "und", "Mut\u00b7ter", "ge\u00b7hor\u00b7sam", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "KON", "NN", "ADJD", "VAINF", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "So soll euch was Sch\u00f6nes beschehret seyn;", "tokens": ["So", "soll", "euch", "was", "Sch\u00f6\u00b7nes", "be\u00b7scheh\u00b7ret", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIS", "ADJA", "VVPP", "VAINF", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Wenn ihr aber dasselbige nicht thut,", "tokens": ["Wenn", "ihr", "a\u00b7ber", "das\u00b7sel\u00b7bi\u00b7ge", "nicht", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PDS", "PTKNEG", "VVFIN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "So bringe ich euch den Stecken und die Ruth.", "tokens": ["So", "brin\u00b7ge", "ich", "euch", "den", "Ste\u00b7cken", "und", "die", "Ruth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Amen.", "tokens": ["A\u00b7men", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}