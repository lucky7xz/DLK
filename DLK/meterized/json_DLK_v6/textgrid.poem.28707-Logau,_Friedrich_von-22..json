{"textgrid.poem.28707": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "22.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ists, worauff ihr Ziel gesetzt hat alle Welt?", "tokens": ["Was", "ists", ",", "wo\u00b7rauff", "ihr", "Ziel", "ge\u00b7setzt", "hat", "al\u00b7le", "Welt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "PWAV", "PPOSAT", "NN", "VVPP", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Befrag ein Echo drum; was sagt sie? H\u00f6re! Geld.", "tokens": ["Be\u00b7frag", "ein", "E\u00b7cho", "drum", ";", "was", "sagt", "sie", "?", "H\u00f6\u00b7re", "!", "Geld", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "NN", "PAV", "$.", "PWS", "VVFIN", "PPER", "$.", "NE", "$.", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was ist dann wol das Geld, das solche Liebe stifft?", "tokens": ["Was", "ist", "dann", "wol", "das", "Geld", ",", "das", "sol\u00b7che", "Lie\u00b7be", "stifft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADV", "ART", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Geh! frag ein Echo drum; was sagt sie? H\u00f6re! Gifft.", "tokens": ["Geh", "!", "frag", "ein", "E\u00b7cho", "drum", ";", "was", "sagt", "sie", "?", "H\u00f6\u00b7re", "!", "Gifft", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$.", "VVIMP", "ART", "NN", "PAV", "$.", "PWS", "VVFIN", "PPER", "$.", "NE", "$.", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ach ja! wer diese Gifft nimmt unbesunnen ein,", "tokens": ["Ach", "ja", "!", "wer", "die\u00b7se", "Gifft", "nimmt", "un\u00b7be\u00b7sun\u00b7nen", "ein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "$.", "PWS", "PDAT", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wird ehstes Seelen-arm und Sinnen-lose seyn.", "tokens": ["Wird", "ehs\u00b7tes", "See\u00b7len\u00b7arm", "und", "Sin\u00b7nen\u00b7lo\u00b7se", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}