{"textgrid.poem.64235": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie friedlich gl\u00e4nzen Flur und H\u00fcgel", "genre": "verse", "period": "N.A.", "pub_year": 1866, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie friedlich gl\u00e4nzen Flur und H\u00fcgel", "tokens": ["Wie", "fried\u00b7lich", "gl\u00e4n\u00b7zen", "Flur", "und", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im warmen Herbstes-Abendstrahl!", "tokens": ["Im", "war\u00b7men", "Herbs\u00b7tes\u00b7A\u00b7bend\u00b7strahl", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Schlug wirklich denn die schwarzen Fl\u00fcgel", "tokens": ["Schlug", "wirk\u00b7lich", "denn", "die", "schwar\u00b7zen", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Bruderkrieg um dieses Tal?", "tokens": ["Der", "Bru\u00b7der\u00b7krieg", "um", "die\u00b7ses", "Tal", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und diese gr\u00fcnen Rebgel\u00e4nde,", "tokens": ["Und", "die\u00b7se", "gr\u00fc\u00b7nen", "Reb\u00b7ge\u00b7l\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Darin die Sonne l\u00e4chelnd spielt,", "tokens": ["Da\u00b7rin", "die", "Son\u00b7ne", "l\u00e4\u00b7chelnd", "spielt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sah'n wirklich sie, wie deutsche H\u00e4nde", "tokens": ["Sah'n", "wirk\u00b7lich", "sie", ",", "wie", "deut\u00b7sche", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "PPER", "$,", "PWAV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nach deutschen Herzen scharf gezielt?", "tokens": ["Nach", "deut\u00b7schen", "Her\u00b7zen", "scharf", "ge\u00b7zielt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Als j\u00fcngst ich schritt durch dies Gefilde,", "tokens": ["Als", "j\u00fcngst", "ich", "schritt", "durch", "dies", "Ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "VVFIN", "APPR", "PDS", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da war es nicht von ", "tokens": ["Da", "war", "es", "nicht", "von"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "APPR"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Jetzt rief herbei des Herbstes Milde", "tokens": ["Jetzt", "rief", "her\u00b7bei", "des", "Herbs\u00b7tes", "Mil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein duftig Blumenaufgebot.", "tokens": ["Ein", "duf\u00b7tig", "Blu\u00b7men\u00b7auf\u00b7ge\u00b7bot", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "O habet Dank, ihr friedereichen,", "tokens": ["O", "ha\u00b7bet", "Dank", ",", "ihr", "frie\u00b7de\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VAFIN", "NN", "$,", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die ihr die Toten sanft umschlie\u00dft:", "tokens": ["Die", "ihr", "die", "To\u00b7ten", "sanft", "um\u00b7schlie\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich gr\u00fc\u00df' euch als Verhei\u00dfungszeichen,", "tokens": ["Ich", "gr\u00fc\u00df'", "euch", "als", "Ver\u00b7hei\u00b7\u00dfungs\u00b7zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df aus den Gr\u00e4bern \u2013 Eintracht sprie\u00dft.", "tokens": ["Da\u00df", "aus", "den", "Gr\u00e4\u00b7bern", "\u2013", "Ein\u00b7tracht", "sprie\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "$(", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}