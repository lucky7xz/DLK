{"textgrid.poem.30323": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "37.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wilstu dein Verh\u00e4ngn\u00fc\u00df trotzen? ey, so wil nur, was es wil!", "tokens": ["Wils\u00b7tu", "dein", "Ver\u00b7h\u00e4ng\u00b7n\u00fc\u00df", "trot\u00b7zen", "?", "ey", ",", "so", "wil", "nur", ",", "was", "es", "wil", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VVINF", "$.", "NE", "$,", "ADV", "VMFIN", "ADV", "$,", "PWS", "PPER", "VMFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Ungeduld, Schreyn, Heulen, Schelten endert doch nicht dessen Ziel,", "tokens": ["Un\u00b7ge\u00b7duld", ",", "Schreyn", ",", "Heu\u00b7len", ",", "Schel\u00b7ten", "en\u00b7dert", "doch", "nicht", "des\u00b7sen", "Ziel", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "VVFIN", "ADV", "PTKNEG", "PRELAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Macht vielmehr, was arg ist, \u00e4rger; macht au\u00df vielem noch so viel.", "tokens": ["Macht", "viel\u00b7mehr", ",", "was", "arg", "ist", ",", "\u00e4r\u00b7ger", ";", "macht", "au\u00df", "vie\u00b7lem", "noch", "so", "viel", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PRELS", "ADJD", "VAFIN", "$,", "ADJD", "$.", "VVFIN", "APPR", "PIS", "ADV", "ADV", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}