{"textgrid.poem.35208": {"metadata": {"author": {"name": "Harsd\u00f6rffer, Georg Philipp", "birth": "N.A.", "death": "N.A."}, "title": "[du Sch\u00e4ferorgelwerk/ das Pan erk\u00fcnstelt hat]", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Sch\u00e4ferorgelwerk/ das Pan erk\u00fcnstelt hat/", "tokens": ["Du", "Sch\u00e4\u00b7fer\u00b7or\u00b7gel\u00b7werk", "/", "das", "Pan", "er\u00b7k\u00fcns\u00b7telt", "hat", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$(", "ART", "NN", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Ladon/ als er stahl die Nymphe/ hat gebohren/", "tokens": ["Das", "La\u00b7don", "/", "als", "er", "stahl", "die", "Nym\u00b7phe", "/", "hat", "ge\u00b7boh\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "KOUS", "PPER", "VVFIN", "ART", "NN", "$(", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dein T\u00f6n\u0113 macht/ da\u00df oft von uns der blasse Kummer trat/", "tokens": ["Dein", "T\u00f6n\u0113", "macht", "/", "da\u00df", "oft", "von", "uns", "der", "blas\u00b7se", "Kum\u00b7mer", "trat", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$(", "KOUS", "ADV", "APPR", "PPER", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.4": {"text": "Du labest Hirt\u0113/ Heerd und Heid/ f\u00fcllst Eiter und die Ohr\u0113.", "tokens": ["Du", "la\u00b7best", "Hir\u00b7t\u0113", "/", "Heerd", "und", "Heid", "/", "f\u00fcllst", "Ei\u00b7ter", "und", "die", "Ohr\u0113", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "$(", "NN", "KON", "NN", "$(", "VVFIN", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Pan hat heut dich uns verehret/ ehret uns mit hoher Gnad:", "tokens": ["Pan", "hat", "heut", "dich", "uns", "ver\u00b7eh\u00b7ret", "/", "eh\u00b7ret", "uns", "mit", "ho\u00b7her", "Gnad", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "PPER", "PPER", "VVFIN", "$(", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Wir sollen und wollen dich/ Chore der Rohre/ so n\u00fctzen/", "tokens": ["Wir", "sol\u00b7len", "und", "wol\u00b7len", "dich", "/", "Cho\u00b7re", "der", "Roh\u00b7re", "/", "so", "n\u00fct\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KON", "VMFIN", "PPER", "$(", "NN", "ART", "NN", "$(", "ADV", "VVINF", "$("], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.7": {"text": "Da\u00df unsrer Gedichte Ger\u00fcchte von fernen bey Sternen soll blitzen.", "tokens": ["Da\u00df", "uns\u00b7rer", "Ge\u00b7dich\u00b7te", "Ge\u00b7r\u00fcch\u00b7te", "von", "fer\u00b7nen", "bey", "Ster\u00b7nen", "soll", "blit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "APPR", "ADJA", "APPR", "NN", "VMFIN", "VVINF", "$."], "meter": "-+--+--+--+--+--+-", "measure": "amphibrach.penta.plus"}}}}}