{"dta.poem.15053": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Von den vortrefflichen Umst\u00e4nden  \n f\u00fcr die Poesie unter den Kaisern aus  \n dem schw\u00e4bischen Hause.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198476", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Leb du in tugentlicher Aht", "tokens": ["Leb", "du", "in", "tu\u00b7gent\u00b7li\u00b7cher", "Aht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und las den kranken also leben, als im von Arte", "tokens": ["Und", "las", "den", "kran\u00b7ken", "al\u00b7so", "le\u00b7ben", ",", "als", "im", "von", "Ar\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "VVINF", "$,", "KOUS", "APPRART", "APPR", "NN"], "meter": "-+-+-+-+-++-+-", "measure": "unknown.measure.septa"}, "line.3": {"text": "ist geslaht.", "tokens": ["ist", "ges\u00b7laht", "."], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}