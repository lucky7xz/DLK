{"textgrid.poem.31686": {"metadata": {"author": {"name": "Weerth, Georg", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die goldne Morgenr\u00f6te fliegt", "tokens": ["Die", "gold\u00b7ne", "Mor\u00b7gen\u00b7r\u00f6\u00b7te", "fliegt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schon \u00fcber Berg und Wald.", "tokens": ["Schon", "\u00fc\u00b7ber", "Berg", "und", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Steh auf, wer bei Feinsliebchen liegt,", "tokens": ["Steh", "auf", ",", "wer", "bei", "Feins\u00b7lieb\u00b7chen", "liegt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PWS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der junge Tag kommt bald!", "tokens": ["Der", "jun\u00b7ge", "Tag", "kommt", "bald", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Doch wer ein sch\u00f6nes Weibchen hat,", "tokens": ["Doch", "wer", "ein", "sch\u00f6\u00b7nes", "Weib\u00b7chen", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der eile nicht so sehr \u2013", "tokens": ["Der", "ei\u00b7le", "nicht", "so", "sehr", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKNEG", "ADV", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich wollt, da\u00df ich an seiner Statt", "tokens": ["Ich", "wollt", ",", "da\u00df", "ich", "an", "sei\u00b7ner", "Statt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein halbes St\u00fcndchen w\u00e4r!", "tokens": ["Ein", "hal\u00b7bes", "St\u00fcnd\u00b7chen", "w\u00e4r", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}