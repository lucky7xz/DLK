{"textgrid.poem.58987": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "206. Haben, als nicht haben", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer Gott allein zum Schatz will haben,", "tokens": ["Wer", "Gott", "al\u00b7lein", "zum", "Schatz", "will", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "ADV", "APPRART", "NN", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ruht nicht in Leib's- noch Seelengaben;", "tokens": ["Ruht", "nicht", "in", "Leib'\u00b7s", "noch", "See\u00b7len\u00b7ga\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "TRUNC", "KON", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Lieb alles, doch lieb alles so,", "tokens": ["Lieb", "al\u00b7les", ",", "doch", "lieb", "al\u00b7les", "so", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "$,", "ADV", "ADJD", "PIS", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df du mit nichts seist gleich so froh.", "tokens": ["Da\u00df", "du", "mit", "nichts", "seist", "gleich", "so", "froh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PIS", "VAFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}