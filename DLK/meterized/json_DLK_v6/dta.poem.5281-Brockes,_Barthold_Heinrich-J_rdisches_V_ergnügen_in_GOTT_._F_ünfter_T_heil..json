{"dta.poem.5281": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Diese Welt, die gleichsam mein,", "tokens": ["Die\u00b7se", "Welt", ",", "die", "gleich\u00b7sam", "mein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PRELS", "ADJD", "PPOSAT", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Will ich, in vergn\u00fcgtem Dencken,", "tokens": ["Will", "ich", ",", "in", "ver\u00b7gn\u00fcg\u00b7tem", "Den\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dir, o ", "tokens": ["Dir", ",", "o"], "token_info": ["word", "punct", "word"], "pos": ["PPER", "$,", "FM"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "La\u00df sie dir gef\u00e4llig seyn!", "tokens": ["La\u00df", "sie", "dir", "ge\u00b7f\u00e4l\u00b7lig", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Geb ich dir nun gleich was dein,", "tokens": ["Geb", "ich", "dir", "nun", "gleich", "was", "dein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "ADV", "PWS", "PPOSAT", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "(denn wer kann dir sonst was geben!)", "tokens": ["(", "denn", "wer", "kann", "dir", "sonst", "was", "ge\u00b7ben", "!", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "PWS", "VMFIN", "PPER", "ADV", "PIS", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wird doch die recht nach dem Leben", "tokens": ["Wird", "doch", "die", "recht", "nach", "dem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wol gerahtene Copie", "tokens": ["Wol", "ge\u00b7rah\u00b7te\u00b7ne", "Co\u00b7pie"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Besser, als ein Opfer-Vieh,", "tokens": ["Bes\u00b7ser", ",", "als", "ein", "Op\u00b7fer\u00b7Vieh", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Dir, o ", "tokens": ["Dir", ",", "o"], "token_info": ["word", "punct", "word"], "pos": ["PPER", "$,", "FM"], "meter": "+-", "measure": "trochaic.single"}}}}}