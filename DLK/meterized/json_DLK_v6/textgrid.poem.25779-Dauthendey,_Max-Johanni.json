{"textgrid.poem.25779": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Johanni", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Himmel, Erde schaffenstrunken.", "tokens": ["Him\u00b7mel", ",", "Er\u00b7de", "schaf\u00b7fen\u00b7strun\u00b7ken", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch die N\u00e4chte schl\u00fcrfen lechzend", "tokens": ["Noch", "die", "N\u00e4ch\u00b7te", "schl\u00fcr\u00b7fen", "lech\u00b7zend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Des ersch\u00f6pften Tages Helle,", "tokens": ["Des", "er\u00b7sch\u00f6pf\u00b7ten", "Ta\u00b7ges", "Hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bleiches Dunkel atmet Funken,", "tokens": ["Blei\u00b7ches", "Dun\u00b7kel", "at\u00b7met", "Fun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und das Sp\u00e4tlicht schleppt sich \u00e4chzend", "tokens": ["Und", "das", "Sp\u00e4t\u00b7licht", "schleppt", "sich", "\u00e4ch\u00b7zend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PRF", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Durch die Mittnacht,", "tokens": ["Durch", "die", "Mitt\u00b7nacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Zu des jungen Tages Schwelle.", "tokens": ["Zu", "des", "jun\u00b7gen", "Ta\u00b7ges", "Schwel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sonnenfeuer kochen S\u00e4fte.", "tokens": ["Son\u00b7nen\u00b7feu\u00b7er", "ko\u00b7chen", "S\u00e4f\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Bl\u00fctenzarte dort versengt.", "tokens": ["Bl\u00fc\u00b7ten\u00b7zar\u00b7te", "dort", "ver\u00b7sengt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Aus dem weichen Maienkosen", "tokens": ["Aus", "dem", "wei\u00b7chen", "Mai\u00b7en\u00b7ko\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Dr\u00e4ngen willenstarke Kr\u00e4fte,", "tokens": ["Dr\u00e4n\u00b7gen", "wil\u00b7len\u00b7star\u00b7ke", "Kr\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Und die Sommerreife senkt", "tokens": ["Und", "die", "Som\u00b7mer\u00b7rei\u00b7fe", "senkt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Sinnend ihre ernsten Rosen.", "tokens": ["Sin\u00b7nend", "ih\u00b7re", "erns\u00b7ten", "Ro\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Satt zerrann das Fr\u00fchlingsgirren,", "tokens": ["Satt", "zer\u00b7rann", "das", "Fr\u00fch\u00b7lings\u00b7gir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Grimme Sensenhiebe klirren,", "tokens": ["Grim\u00b7me", "Sen\u00b7sen\u00b7hie\u00b7be", "klir\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Halme seufzen, in der Luft,", "tokens": ["Hal\u00b7me", "seuf\u00b7zen", ",", "in", "der", "Luft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Von Verg\u00e4nglichkeit umwittert,", "tokens": ["Von", "Ver\u00b7g\u00e4ng\u00b7lich\u00b7keit", "um\u00b7wit\u00b7tert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Wanket schwermutweher Duft,", "tokens": ["Wan\u00b7ket", "schwer\u00b7mut\u00b7we\u00b7her", "Duft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "\u2013 und das stolze Leben zittert.", "tokens": ["\u2013", "und", "das", "stol\u00b7ze", "Le\u00b7ben", "zit\u00b7tert", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}