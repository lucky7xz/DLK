rd", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPR", "ADJA", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und bebten scheu vor ihrer Glut zur\u00fcck;", "tokens": ["Und", "beb\u00b7ten", "scheu", "vor", "ih\u00b7rer", "Glut", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die H\u00e4nde kaum in leisem Druck sich fanden,", "tokens": ["Die", "H\u00e4n\u00b7de", "kaum", "in", "lei\u00b7sem", "Druck", "sich", "fan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Blick, Ein Wort war unser letztes Gl\u00fcck.", "tokens": ["Ein", "Blick", ",", "Ein", "Wort", "war", "un\u00b7ser", "letz\u00b7tes", "Gl\u00fcck", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wir liebten uns mit jener m\u00fcden Liebe ...", "tokens": ["Wir", "lieb\u00b7ten", "uns", "mit", "je\u00b7ner", "m\u00fc\u00b7den", "Lie\u00b7be", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PDAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}