{"dta.poem.10038": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Undanckbarkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Indem ich eben, voller Lust,", "tokens": ["In\u00b7dem", "ich", "e\u00b7ben", ",", "vol\u00b7ler", "Lust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bey diesem Blumen-Topff, voll ", "tokens": ["Bey", "die\u00b7sem", "Blu\u00b7men\u00b7Topff", ",", "voll"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "ADJD"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und mit, zugleich durch Nas\u2019 und Aug, erquickter Brust,", "tokens": ["Und", "mit", ",", "zu\u00b7gleich", "durch", "Nas'", "und", "Aug", ",", "er\u00b7quick\u00b7ter", "Brust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "$,", "ADV", "APPR", "NN", "KON", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den holden Balsam riech\u2019 und ihren Glantz besehe;", "tokens": ["Den", "hol\u00b7den", "Bal\u00b7sam", "riech'", "und", "ih\u00b7ren", "Glantz", "be\u00b7se\u00b7he", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Trug diese sch\u00f6ne Bl\u00fcht\u2019 der Andacht Frucht in mir.", "tokens": ["Trug", "die\u00b7se", "sch\u00f6\u00b7ne", "Bl\u00fcht'", "der", "An\u00b7dacht", "Frucht", "in", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "ADJA", "NN", "ART", "NN", "NN", "APPR", "PPER", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "Es kam mir ungerecht, und fast unleidlich f\u00fcr,", "tokens": ["Es", "kam", "mir", "un\u00b7ge\u00b7recht", ",", "und", "fast", "un\u00b7leid\u00b7lich", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,", "KON", "ADV", "ADJD", "APPR", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df man des Sch\u00f6pfers Werck, das doch so sch\u00f6n, nicht", "tokens": ["Da\u00df", "man", "des", "Sch\u00f6p\u00b7fers", "Werck", ",", "das", "doch", "so", "sch\u00f6n", ",", "nicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PIS", "ART", "NN", "NN", "$,", "PRELS", "ADV", "ADV", "ADJD", "$,", "PTKNEG"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df man es obenhin, ja gar nicht einst, betrachtet.", "tokens": ["Da\u00df", "man", "es", "o\u00b7ben\u00b7hin", ",", "ja", "gar", "nicht", "einst", ",", "be\u00b7trach\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADV", "$,", "ADV", "ADV", "PTKNEG", "ADV", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein stiller und geheimer Schmertz", "tokens": ["Ein", "stil\u00b7ler", "und", "ge\u00b7hei\u00b7mer", "Schmertz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schlich sich in mein vergn\u00fcgtes Hertz.", "tokens": ["Schlich", "sich", "in", "mein", "ver\u00b7gn\u00fcg\u00b7tes", "Hertz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Recht mitten in der Lust, fieng ich mit seufzen, an:", "tokens": ["Recht", "mit\u00b7ten", "in", "der", "Lust", ",", "fi\u00b7eng", "ich", "mit", "seuf\u00b7zen", ",", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "NN", "$,", "VVFIN", "PPER", "APPR", "VVINF", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Ach! liebster Vater, sollt Du dann", "tokens": ["Ach", "!", "liebs\u00b7ter", "Va\u00b7ter", ",", "sollt", "Du", "dann"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "ADJA", "NN", "$,", "VMFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr solch ein sch\u00f6n Gesch\u00f6pff, f\u00fcr alle Deine Gaben,", "tokens": ["F\u00fcr", "solch", "ein", "sch\u00f6n", "Ge\u00b7sch\u00f6pff", ",", "f\u00fcr", "al\u00b7le", "Dei\u00b7ne", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "ADJD", "NN", "$,", "APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "F\u00fcr so viel Proben Deiner G\u00fcte,", "tokens": ["F\u00fcr", "so", "viel", "Pro\u00b7ben", "Dei\u00b7ner", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PIAT", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von uns nicht einst ein fr\u00f6hliches Gem\u00fcthe,", "tokens": ["Von", "uns", "nicht", "einst", "ein", "fr\u00f6h\u00b7li\u00b7ches", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKNEG", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Richt einst ein danckbar Hertze haben?", "tokens": ["Richt", "einst", "ein", "dan\u00b7ck\u00b7bar", "Hert\u00b7ze", "ha\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJD", "VVFIN", "VAINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}