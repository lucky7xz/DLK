{"dta.poem.8506": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Das verlorne Paradies.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als noch das Paradies erschlossen war", "tokens": ["Als", "noch", "das", "Pa\u00b7ra\u00b7dies", "er\u00b7schlos\u00b7sen", "war"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "VVPP", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dem ersten s\u00fcndelosen Menschenpaar,", "tokens": ["Dem", "ers\u00b7ten", "s\u00fcn\u00b7de\u00b7lo\u00b7sen", "Men\u00b7schen\u00b7paar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kein Gift die Viper kannte, keinen Dorn", "tokens": ["Kein", "Gift", "die", "Vi\u00b7per", "kann\u00b7te", ",", "kei\u00b7nen", "Dorn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "ART", "NN", "VVFIN", "$,", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Strauch, der Leu und Tiger keinen Zorn,", "tokens": ["Der", "Strauch", ",", "der", "Leu", "und", "Ti\u00b7ger", "kei\u00b7nen", "Zorn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Noch fr\u00f6hlich scholl der Nachtigallen Fl\u00f6te;", "tokens": ["Noch", "fr\u00f6h\u00b7lich", "scholl", "der", "Nach\u00b7ti\u00b7gal\u00b7len", "Fl\u00f6\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da schlief an jedem Abend Eva ein", "tokens": ["Da", "schlief", "an", "je\u00b7dem", "A\u00b7bend", "E\u00b7va", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PIAT", "NN", "NN", "ART"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "An einem Rosenstrauche, und der Schein", "tokens": ["An", "ei\u00b7nem", "Ro\u00b7sen\u00b7strau\u00b7che", ",", "und", "der", "Schein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Von ihrer unschuldsvollen Wangenr\u00f6the", "tokens": ["Von", "ih\u00b7rer", "un\u00b7schulds\u00b7vol\u00b7len", "Wan\u00b7gen\u00b7r\u00f6\u00b7the"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Spielt\u2019 lieblich um der Blume lichten Ball;", "tokens": ["Spielt'", "lieb\u00b7lich", "um", "der", "Blu\u00b7me", "lich\u00b7ten", "Ball", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Denn damals waren wei\u00df die Rosen all\u2019", "tokens": ["Denn", "da\u00b7mals", "wa\u00b7ren", "wei\u00df", "die", "Ro\u00b7sen", "all'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "VVFIN", "ART", "NN", "PIAT"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und dornenlos. \u2014 Umnickt vom duft\u2019gen Kranz,", "tokens": ["Und", "dor\u00b7nen\u00b7los", ".", "Um\u00b7nickt", "vom", "duft'\u00b7gen", "Kranz", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$.", "$(", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Der \u00fcber\u2019m Haupte f\u00fchrte lichten Tanz,", "tokens": ["Der", "\u00fc\u00b7ber'm", "Haup\u00b7te", "f\u00fchr\u00b7te", "lich\u00b7ten", "Tanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Ruhte das erste Weib, Gedanken sinnend.", "tokens": ["Ruh\u00b7te", "das", "ers\u00b7te", "Weib", ",", "Ge\u00b7dan\u00b7ken", "sin\u00b7nend", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "NN", "VVPP", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.14": {"text": "Die Embrhone schon der Gottheit Siegel", "tokens": ["Die", "E\u00b7mbrho\u00b7ne", "schon", "der", "Got\u00b7theit", "Sie\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ART", "NN", "NN"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.15": {"text": "Am Haupte trugen, schon im Keime minnend", "tokens": ["Am", "Haup\u00b7te", "tru\u00b7gen", ",", "schon", "im", "Kei\u00b7me", "min\u00b7nend"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "$,", "ADV", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Bewegten halberschloss\u2019ne Seraphsfl\u00fcgel;", "tokens": ["Be\u00b7weg\u00b7ten", "hal\u00b7ber\u00b7schloss'\u00b7ne", "Se\u00b7raphs\u00b7fl\u00fc\u00b7gel", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Sie lag den Zweig an ihre Brust gedr\u00fcckt;", "tokens": ["Sie", "lag", "den", "Zweig", "an", "ih\u00b7re", "Brust", "ge\u00b7dr\u00fcckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Denn keine Blume wurde noch gepfl\u00fcckt,", "tokens": ["Denn", "kei\u00b7ne", "Blu\u00b7me", "wur\u00b7de", "noch", "ge\u00b7pfl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Bis leise sich die Wimper niederlie\u00df", "tokens": ["Bis", "lei\u00b7se", "sich", "die", "Wim\u00b7per", "nie\u00b7der\u00b7lie\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "PRF", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Und in die Tr\u00e4ume schlich das Paradies;", "tokens": ["Und", "in", "die", "Tr\u00e4u\u00b7me", "schlich", "das", "Pa\u00b7ra\u00b7dies", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "O heilig war das Weib; wer sie geseh\u2019n,", "tokens": ["O", "hei\u00b7lig", "war", "das", "Weib", ";", "wer", "sie", "ge\u00b7seh'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VAFIN", "ART", "NN", "$.", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Nicht denken h\u00e4tt\u2019 er k\u00f6nnen, ob sie sch\u00f6n,", "tokens": ["Nicht", "den\u00b7ken", "h\u00e4tt'", "er", "k\u00f6n\u00b7nen", ",", "ob", "sie", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VAFIN", "PPER", "VMFIN", "$,", "KOUS", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Nur da\u00df sie rein wie Thau, und Gottes Spiegel.", "tokens": ["Nur", "da\u00df", "sie", "rein", "wie", "Thau", ",", "und", "Got\u00b7tes", "Spie\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADJD", "KOKOM", "NN", "$,", "KON", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}