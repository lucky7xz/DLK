{"textgrid.poem.66270": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Vater Unser", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jehovah! den mit Zittern", "tokens": ["Je\u00b7ho\u00b7vah", "!", "den", "mit", "Zit\u00b7tern"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "ART", "APPR", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Das Heer der Geister ehrt,", "tokens": ["Das", "Heer", "der", "Geis\u00b7ter", "ehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und den aus Nachtgewittern", "tokens": ["Und", "den", "aus", "Nacht\u00b7ge\u00b7wit\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der S\u00fcnder donnern h\u00f6rt,", "tokens": ["Der", "S\u00fcn\u00b7der", "don\u00b7nern", "h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Den Erd' und Himmel kennen,", "tokens": ["Den", "Erd'", "und", "Him\u00b7mel", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dich darf ich Vater nennen,", "tokens": ["Dich", "darf", "ich", "Va\u00b7ter", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Dein Sohn hat mich's gelehrt.", "tokens": ["Dein", "Sohn", "hat", "mich's", "ge\u00b7lehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PIS", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mein Vater! Himmelswonne", "tokens": ["Mein", "Va\u00b7ter", "!", "Him\u00b7mels\u00b7won\u00b7ne"], "token_info": ["word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "$.", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Liegt in dem Namen! Dich,", "tokens": ["Liegt", "in", "dem", "Na\u00b7men", "!", "Dich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$.", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Sch\u00f6pfer dieser Sonne,", "tokens": ["Den", "Sch\u00f6p\u00b7fer", "die\u00b7ser", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dich, Welterhalter, Dich", "tokens": ["Dich", ",", "Wel\u00b7ter\u00b7hal\u00b7ter", ",", "Dich"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["PPER", "$,", "NN", "$,", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Darf ich als Vater loben,", "tokens": ["Darf", "ich", "als", "Va\u00b7ter", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "KOUS", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie deine Geister droben;", "tokens": ["Wie", "dei\u00b7ne", "Geis\u00b7ter", "dro\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Als Kind erh\u00f6rst du mich!", "tokens": ["Als", "Kind", "er\u00b7h\u00f6rst", "du", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ihr Kinder, so versammelt", "tokens": ["Ihr", "Kin\u00b7der", ",", "so", "ver\u00b7sam\u00b7melt"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Euch um des Vaters Thron;", "tokens": ["Euch", "um", "des", "Va\u00b7ters", "Thron", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Gebete, die ihr stammelt,", "tokens": ["Ge\u00b7be\u00b7te", ",", "die", "ihr", "stam\u00b7melt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sind ihm ein s\u00fc\u00dfer Ton.", "tokens": ["Sind", "ihm", "ein", "s\u00fc\u00b7\u00dfer", "Ton", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ja, Vater! h\u00f6r uns singen;", "tokens": ["Ja", ",", "Va\u00b7ter", "!", "h\u00f6r", "uns", "sin\u00b7gen", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "$.", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wann wir mit Ohnmacht ringen,", "tokens": ["Wann", "wir", "mit", "Ohn\u00b7macht", "rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "So sieh auf deinen Sohn.", "tokens": ["So", "sieh", "auf", "dei\u00b7nen", "Sohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "La\u00df deines Namens Ehre", "tokens": ["La\u00df", "dei\u00b7nes", "Na\u00b7mens", "Eh\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Uns Menschen heilig sein;", "tokens": ["Uns", "Men\u00b7schen", "hei\u00b7lig", "sein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihn m\u00fcsse falsche Lehre", "tokens": ["Ihn", "m\u00fcs\u00b7se", "fal\u00b7sche", "Leh\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und Laster nie entweihn.", "tokens": ["Und", "Las\u00b7ter", "nie", "ent\u00b7weihn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O, unser Vater! fl\u00f6\u00dfe", "tokens": ["O", ",", "un\u00b7ser", "Va\u00b7ter", "!", "fl\u00f6\u00b7\u00dfe"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["NE", "$,", "PPOSAT", "NN", "$.", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Erkenntni\u00df deiner Gr\u00f6\u00dfe", "tokens": ["Er\u00b7kennt\u00b7ni\u00df", "dei\u00b7ner", "Gr\u00f6\u00b7\u00dfe"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "In unsre Herzen ein!", "tokens": ["In", "uns\u00b7re", "Her\u00b7zen", "ein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "O la\u00df es kommen, kommen", "tokens": ["O", "la\u00df", "es", "kom\u00b7men", ",", "kom\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NE", "VVFIN", "PPER", "VVINF", "$,", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dein Reich voll Recht und Licht,", "tokens": ["Dein", "Reich", "voll", "Recht", "und", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zur Rettung deiner Frommen,", "tokens": ["Zur", "Ret\u00b7tung", "dei\u00b7ner", "From\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Frevlern zum Gericht,", "tokens": ["Den", "Frev\u00b7lern", "zum", "Ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vertilge bald die Rotte,", "tokens": ["Ver\u00b7til\u00b7ge", "bald", "die", "Rot\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die mit verfluchtem Spotte", "tokens": ["Die", "mit", "ver\u00b7fluch\u00b7tem", "Spot\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Von deinem Sohne spricht.", "tokens": ["Von", "dei\u00b7nem", "Soh\u00b7ne", "spricht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Herr! es gescheh' dein Wille,", "tokens": ["Herr", "!", "es", "ge\u00b7scheh'", "dein", "Wil\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Wie dort, so in der Zeit;", "tokens": ["Wie", "dort", ",", "so", "in", "der", "Zeit", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit Demuth, Herzensstille,", "tokens": ["Mit", "De\u00b7muth", ",", "Her\u00b7zens\u00b7stil\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und Engelschnelligkeit;", "tokens": ["Und", "En\u00b7gel\u00b7schnel\u00b7lig\u00b7keit", ";"], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Erkl\u00e4r' es unsern Seelen,", "tokens": ["Er\u00b7kl\u00e4r'", "es", "un\u00b7sern", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wann wir aus Schwachheit fehlen,", "tokens": ["Wann", "wir", "aus", "Schwach\u00b7heit", "feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Was uns dein Wort gebeut.", "tokens": ["Was", "uns", "dein", "Wort", "ge\u00b7beut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Still' unsre Erdensorgen;", "tokens": ["Still'", "uns\u00b7re", "Er\u00b7den\u00b7sor\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gib H\u00fclle, Trank und Brod,", "tokens": ["Gib", "H\u00fcl\u00b7le", ",", "Trank", "und", "Brod", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nur heute; denn der Morgen", "tokens": ["Nur", "heu\u00b7te", ";", "denn", "der", "Mor\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "$.", "KON", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Find't uns vielleicht schon todt.", "tokens": ["Find't", "uns", "viel\u00b7leicht", "schon", "todt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Wann Noth und Mangel dr\u00fccken,", "tokens": ["Wann", "Noth", "und", "Man\u00b7gel", "dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Lehr' auf zu dir uns blicken,", "tokens": ["Lehr'", "auf", "zu", "dir", "uns", "bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPR", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Dem Stiller jeder Noth.", "tokens": ["Dem", "Stil\u00b7ler", "je\u00b7der", "Noth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Vergib uns unsre S\u00fcnden,", "tokens": ["Ver\u00b7gib", "uns", "uns\u00b7re", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du bist ja voll Geduld;", "tokens": ["Du", "bist", "ja", "voll", "Ge\u00b7duld", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kein Engel kann ergr\u00fcnden", "tokens": ["Kein", "En\u00b7gel", "kann", "er\u00b7gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Tiefe deiner Huld.", "tokens": ["Die", "Tie\u00b7fe", "dei\u00b7ner", "Huld", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Schenkst du uns Schuld und Leben,", "tokens": ["Schenkst", "du", "uns", "Schuld", "und", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So la\u00df uns auch vergeben", "tokens": ["So", "la\u00df", "uns", "auch", "ver\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Den Br\u00fcdern ihre Schuld.", "tokens": ["Den", "Br\u00fc\u00b7dern", "ih\u00b7re", "Schuld", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Schwingst du die Vaterruthe,", "tokens": ["Schwingst", "du", "die", "Va\u00b7ter\u00b7ru\u00b7the", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Versucht uns H\u00f6ll' und Welt,", "tokens": ["Ver\u00b7sucht", "uns", "H\u00f6ll'", "und", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So r\u00fcst' uns mit dem Muthe,", "tokens": ["So", "r\u00fcst'", "uns", "mit", "dem", "Mu\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der uns im Kampf erh\u00e4lt;", "tokens": ["Der", "uns", "im", "Kampf", "er\u00b7h\u00e4lt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sei du des Schwachen St\u00fctze,", "tokens": ["Sei", "du", "des", "Schwa\u00b7chen", "St\u00fct\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wann in des Kampfes Hitze", "tokens": ["Wann", "in", "des", "Kamp\u00b7fes", "Hit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ihm Muth und Kraft entf\u00e4llt.", "tokens": ["Ihm", "Muth", "und", "Kraft", "ent\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Erl\u00f6s uns von dem B\u00f6sen", "tokens": ["Er\u00b7l\u00f6s", "uns", "von", "dem", "B\u00f6\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch sanften Christentod;", "tokens": ["Durch", "sanf\u00b7ten", "Chris\u00b7ten\u00b7tod", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer kann uns sonst erl\u00f6sen,", "tokens": ["Wer", "kann", "uns", "sonst", "er\u00b7l\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als du, aus aller Noth?", "tokens": ["Als", "du", ",", "aus", "al\u00b7ler", "Noth", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Aus Armuth, Krankheit, Banden,", "tokens": ["Aus", "Ar\u00b7muth", ",", "Krank\u00b7heit", ",", "Ban\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Verf\u00fchrung, Schmach und Schanden,", "tokens": ["Ver\u00b7f\u00fch\u00b7rung", ",", "Schmach", "und", "Schan\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und was uns Armen droht.", "tokens": ["Und", "was", "uns", "Ar\u00b7men", "droht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Dein ist das Reich, die Ehre,", "tokens": ["Dein", "ist", "das", "Reich", ",", "die", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Macht, Kraft und Herrlichkeit!", "tokens": ["Macht", ",", "Kraft", "und", "Herr\u00b7lich\u00b7keit", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dir jauchzen Engelheere,", "tokens": ["Dir", "jauch\u00b7zen", "En\u00b7gel\u00b7hee\u00b7re", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dir t\u00f6nt das Lied der Zeit!", "tokens": ["Dir", "t\u00f6nt", "das", "Lied", "der", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Preis deinem gro\u00dfen Namen,", "tokens": ["Preis", "dei\u00b7nem", "gro\u00b7\u00dfen", "Na\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Jehovah! Amen! Amen", "tokens": ["Je\u00b7ho\u00b7vah", "!", "A\u00b7men", "!", "A\u00b7men"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NE", "$.", "NN", "$.", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Jetzt, und in Ewigkeit!", "tokens": ["Jetzt", ",", "und", "in", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KON", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}