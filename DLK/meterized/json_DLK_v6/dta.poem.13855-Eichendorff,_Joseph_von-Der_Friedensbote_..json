{"dta.poem.13855": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Friedensbote .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Schlaf ein, mein Liebchen, schlaf' ein,", "tokens": ["Schlaf", "ein", ",", "mein", "Lieb\u00b7chen", ",", "schlaf'", "ein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Leis durch die Blumen am Gitter", "tokens": ["Leis", "durch", "die", "Blu\u00b7men", "am", "Git\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "APPRART", "NN"], "meter": "++-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "S\u00e4uselt des Laubes Gezitter,", "tokens": ["S\u00e4u\u00b7selt", "des", "Lau\u00b7bes", "Ge\u00b7zit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Rauschen die Quellen herein;", "tokens": ["Rau\u00b7schen", "die", "Quel\u00b7len", "her\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Gesenkt auf den schneewei\u00dfen Arm,", "tokens": ["Ge\u00b7senkt", "auf", "den", "schnee\u00b7wei\u00b7\u00dfen", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Schlaf ein, mein Liebchen, schlaf' ein,", "tokens": ["Schlaf", "ein", ",", "mein", "Lieb\u00b7chen", ",", "schlaf'", "ein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Wie athmest du lieblich und warm!", "tokens": ["Wie", "ath\u00b7mest", "du", "lieb\u00b7lich", "und", "warm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}