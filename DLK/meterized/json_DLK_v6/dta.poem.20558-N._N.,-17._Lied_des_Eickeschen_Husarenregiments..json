{"dta.poem.20558": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "17.  \n  Lied des Eickeschen Husarenregiments.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1848", "urn": "urn:nbn:de:kobv:b4-25129-1", "language": ["de:0.99"], "booktitle": "[N. N.]: Erinnerungen eines freiwilligen reitenden J\u00e4gers aus den Kriegsjahren 1813\u20131815. Berlin, 1848."}, "poem": {"stanza.1": {"line.1": {"text": "Den Franzmann will ich schlagen,", "tokens": ["Den", "Franz\u00b7mann", "will", "ich", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit Franzfrau mich vertragen,", "tokens": ["Mit", "Franz\u00b7frau", "mich", "ver\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mir hilft, fehlt auch das Geld,", "tokens": ["Mir", "hilft", ",", "fehlt", "auch", "das", "Geld", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mein S\u00e4bel durch die Welt.", "tokens": ["Mein", "S\u00e4\u00b7bel", "durch", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wir hier, wir Herr\u2019n Husaren", "tokens": ["Wir", "hier", ",", "wir", "Herr'n", "Hu\u00b7sa\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "$,", "PPER", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sind fr\u00f6hlich bei Gefahren.", "tokens": ["Sind", "fr\u00f6h\u00b7lich", "bei", "Ge\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Stets wechselt Noth mit Gl\u00fcck.", "tokens": ["Stets", "wech\u00b7selt", "Noth", "mit", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Es herrscht der Augenblick.", "tokens": ["Es", "herrscht", "der", "Au\u00b7gen\u00b7blick", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wir sind die sch\u00f6nsten M\u00e4nner,", "tokens": ["Wir", "sind", "die", "sch\u00f6ns\u00b7ten", "M\u00e4n\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das sagen alle Kenner.", "tokens": ["Das", "sa\u00b7gen", "al\u00b7le", "Ken\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ja, unsern alten Herrn", "tokens": ["Ja", ",", "un\u00b7sern", "al\u00b7ten", "Herrn"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Freit\u2019 selbst die J\u00fcngste gern!", "tokens": ["Freit'", "selbst", "die", "J\u00fcngs\u00b7te", "gern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Herr Oberst, die nicht hassen", "tokens": ["Herr", "O\u00b7berst", ",", "die", "nicht", "has\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "$,", "PRELS", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Damen, doch das Prassen,", "tokens": ["Die", "Da\u00b7men", ",", "doch", "das", "Pras\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie trinken Wein statt Thee", "tokens": ["Sie", "trin\u00b7ken", "Wein", "statt", "Thee"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und lieben Schlachtquarr", "tokens": ["Und", "lie\u00b7ben", "Schlacht\u00b7quarr"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Herzvater sagt beim Scheiden:", "tokens": ["Herz\u00b7va\u00b7ter", "sagt", "beim", "Schei\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Christoph, mu\u00dft Weibsvolk meiden;", "tokens": ["Chris\u00b7toph", ",", "mu\u00dft", "Weibs\u00b7volk", "mei\u00b7den", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Merk\u2019s Dir, der Dirnen Ku\u00df", "tokens": ["Merk's", "Dir", ",", "der", "Dir\u00b7nen", "Ku\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PPER", "$,", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Schadet dem Reiterschlu\u00df.", "tokens": ["Scha\u00b7det", "dem", "Rei\u00b7ter\u00b7schlu\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "Papa, Uhlan, dann Bauer,", "tokens": ["Pa\u00b7pa", ",", "Uh\u00b7lan", ",", "dann", "Bau\u00b7er", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "ADV", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kannt\u2019 Welt wohl viel genauer", "tokens": ["Kannt'", "Welt", "wohl", "viel", "ge\u00b7nau\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "NN", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als mancher Sekretair.", "tokens": ["Als", "man\u00b7cher", "Se\u00b7kre\u00b7tair", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der gr\u00fcbelt freilich mehr.", "tokens": ["Der", "gr\u00fc\u00b7belt", "frei\u00b7lich", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Horch, Donnern der Haubitzen!", "tokens": ["Horch", ",", "Don\u00b7nern", "der", "Hau\u00b7bit\u00b7zen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Trompeter bl\u00e4st: Aufsitzen. \u2014", "tokens": ["Trom\u00b7pe\u00b7ter", "bl\u00e4st", ":", "Auf\u00b7sit\u00b7zen", "."], "token_info": ["word", "word", "punct", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "$.", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dem Alten schl\u00e4gt voll Lust", "tokens": ["Dem", "Al\u00b7ten", "schl\u00e4gt", "voll", "Lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Seht nur Husarenbrust!", "tokens": ["Seht", "nur", "Hu\u00b7sa\u00b7ren\u00b7brust", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}}, "stanza.8": {"line.1": {"text": "So la\u00dft uns wacker reiten,", "tokens": ["So", "la\u00dft", "uns", "wa\u00b7cker", "rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und wer in sp\u00e4ten Zeiten", "tokens": ["Und", "wer", "in", "sp\u00e4\u00b7ten", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vom Oberst ", "tokens": ["Vom", "O\u00b7berst"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Vergi\u00dft auch unsrer nicht.", "tokens": ["Ver\u00b7gi\u00dft", "auch", "uns\u00b7rer", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}