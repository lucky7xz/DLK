{"dta.poem.1376": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Marini.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Der ungl\u00fcckselge Mensch kan kaum die Welt begr\u00fcssen/", "tokens": ["Der", "un\u00b7gl\u00fcck\u00b7sel\u00b7ge", "Mensch", "kan", "kaum", "die", "Welt", "be\u00b7gr\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df nicht ein Thr\u00e4nen-Flu\u00df/ eh das noch schwache", "tokens": ["Da\u00df", "nicht", "ein", "Thr\u00e4\u00b7nen\u00b7Flu\u00df", "/", "eh", "das", "noch", "schwa\u00b7che"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "$(", "KOUS", "PDS", "ADV", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Licht", "tokens": ["Licht"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Den hellen Tag erkennt/ aus seinen Augen bricht:", "tokens": ["Den", "hel\u00b7len", "Tag", "er\u00b7kennt", "/", "aus", "sei\u00b7nen", "Au\u00b7gen", "bricht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$(", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wird frey und l\u00e4sset sich in neue Bande schl\u00fcssen.", "tokens": ["Wird", "frey", "und", "l\u00e4s\u00b7set", "sich", "in", "neu\u00b7e", "Ban\u00b7de", "schl\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "VVFIN", "PRF", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ist er der zarten Milch und ersten Spei\u00df entrissen/", "tokens": ["Ist", "er", "der", "zar\u00b7ten", "Milch", "und", "ers\u00b7ten", "Spei\u00df", "ent\u00b7ris\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "KON", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So f\u00e4sselt seinen Mutt der Zucht gezwungne Pflicht/", "tokens": ["So", "f\u00e4s\u00b7selt", "sei\u00b7nen", "Mutt", "der", "Zucht", "ge\u00b7zwung\u00b7ne", "Pflicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Befreyet ihn die Zeit/ wie mu\u00df sein Hertze nicht", "tokens": ["Be\u00b7fre\u00b7yet", "ihn", "die", "Zeit", "/", "wie", "mu\u00df", "sein", "Hert\u00b7ze", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "$(", "PWAV", "VMFIN", "PPOSAT", "VVFIN", "PTKNEG"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sich lebend offt und tod von Gl\u00fcck und Liebe wissen!", "tokens": ["Sich", "le\u00b7bend", "offt", "und", "tod", "von", "Gl\u00fcck", "und", "Lie\u00b7be", "wis\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ADV", "KON", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Was hat er denn f\u00fcr Sorg\u2019 und Kummer auszustehn/", "tokens": ["Was", "hat", "er", "denn", "f\u00fcr", "Sor\u00b7g'", "und", "Kum\u00b7mer", "aus\u00b7zu\u00b7stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Was mu\u00df ihm nicht f\u00fcr Schmertz und Leid zu handen gehn/", "tokens": ["Was", "mu\u00df", "ihm", "nicht", "f\u00fcr", "Schmertz", "und", "Leid", "zu", "han\u00b7den", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PTKNEG", "APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Bi\u00df er geb\u00fcckt und matt ergreifft den schwachen Stab.", "tokens": ["Bi\u00df", "er", "ge\u00b7b\u00fcckt", "und", "matt", "er\u00b7greifft", "den", "schwa\u00b7chen", "Stab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Zulezt entflieht der Geist/ der Leib wird hingetragen/", "tokens": ["Zu\u00b7lezt", "ent\u00b7flieht", "der", "Geist", "/", "der", "Leib", "wird", "hin\u00b7ge\u00b7tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$(", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So pl\u00f6tzlich/ da\u00df ich mu\u00df mit tieffem Seuffzen sagen:", "tokens": ["So", "pl\u00f6tz\u00b7lich", "/", "da\u00df", "ich", "mu\u00df", "mit", "tief\u00b7fem", "Seuff\u00b7zen", "sa\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$(", "KOUS", "PPER", "VMFIN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wie nahe grentzen doch die Wieg\u2019 und unser Grab.", "tokens": ["Wie", "na\u00b7he", "grent\u00b7zen", "doch", "die", "Wieg'", "und", "un\u00b7ser", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ADV", "ART", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}