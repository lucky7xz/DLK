{"textgrid.poem.38862": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn das Gew\u00fchl der Welt mit tausend Banden", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn das Gew\u00fchl der Welt mit tausend Banden", "tokens": ["Wenn", "das", "Ge\u00b7w\u00fchl", "der", "Welt", "mit", "tau\u00b7send", "Ban\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Um Auge, Sinn und Herz sich wollte stricken,", "tokens": ["Um", "Au\u00b7ge", ",", "Sinn", "und", "Herz", "sich", "woll\u00b7te", "stri\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "$,", "NN", "KON", "NN", "PRF", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So durft' ich nur in deine Augen blicken", "tokens": ["So", "durft'", "ich", "nur", "in", "dei\u00b7ne", "Au\u00b7gen", "bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und alle Zweifel, alle R\u00e4thsel schwanden.", "tokens": ["Und", "al\u00b7le", "Zwei\u00b7fel", ",", "al\u00b7le", "R\u00e4th\u00b7sel", "schwan\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich sah wie sich die giftgen Schlangen wanden,", "tokens": ["Ich", "sah", "wie", "sich", "die", "gift\u00b7gen", "Schlan\u00b7gen", "wan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "PRF", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Vater samt den Kindern zu erdr\u00fccken,", "tokens": ["Den", "Va\u00b7ter", "samt", "den", "Kin\u00b7dern", "zu", "er\u00b7dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wie kein Gott wollt' H\u00fclfe niederschicken,", "tokens": ["Und", "wie", "kein", "Gott", "wollt'", "H\u00fcl\u00b7fe", "nie\u00b7der\u00b7schi\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIAT", "NN", "VMFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Fast unbewu\u00dft die Armen h\u00fclflos standen.", "tokens": ["Fast", "un\u00b7be\u00b7wu\u00dft", "die", "Ar\u00b7men", "h\u00fcl\u00b7flos", "stan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So wird der Mensch von Angst und Pein getrieben,", "tokens": ["So", "wird", "der", "Mensch", "von", "Angst", "und", "Pein", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der stolz und zornig der, in L\u00fcsten gl\u00fchend,", "tokens": ["Der", "stolz", "und", "zor\u00b7nig", "der", ",", "in", "L\u00fcs\u00b7ten", "gl\u00fc\u00b7hend", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "ART", "$,", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von Habsucht der erstickt, von giftgem Neide:", "tokens": ["Von", "Hab\u00b7sucht", "der", "er\u00b7stickt", ",", "von", "gift\u00b7gem", "Nei\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "VVPP", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dann sah ich dich in stiller frommer Freude", "tokens": ["Dann", "sah", "ich", "dich", "in", "stil\u00b7ler", "from\u00b7mer", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im ewigen Gebete niederknieend", "tokens": ["Im", "e\u00b7wi\u00b7gen", "Ge\u00b7be\u00b7te", "nie\u00b7der\u00b7kni\u00b7e\u00b7end"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Einsam Natur und Gott und Himmel lieben.", "tokens": ["Ein\u00b7sam", "Na\u00b7tur", "und", "Gott", "und", "Him\u00b7mel", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "KON", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}}}}