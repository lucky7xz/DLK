{"textgrid.poem.50951": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Schlafwandel am Tage", "genre": "verse", "period": "N.A.", "pub_year": 1850, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im afrikanischen Felsental", "tokens": ["Im", "af\u00b7ri\u00b7ka\u00b7ni\u00b7schen", "Fel\u00b7sen\u00b7tal"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Marschiert ein Bataillon,", "tokens": ["Mar\u00b7schiert", "ein", "Ba\u00b7tail\u00b7lon", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Sich selber fremd, eine braune Schar", "tokens": ["Sich", "sel\u00b7ber", "fremd", ",", "ei\u00b7ne", "brau\u00b7ne", "Schar"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "ADV", "ADJD", "$,", "ART", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Fremdenlegion;", "tokens": ["Der", "Frem\u00b7den\u00b7le\u00b7gi\u00b7on", ";"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Lang ist ihr wildes Lied verhallt", "tokens": ["Lang", "ist", "ihr", "wil\u00b7des", "Lied", "ver\u00b7hallt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In Sprachen mancherlei,", "tokens": ["In", "Spra\u00b7chen", "man\u00b7cher\u00b7lei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "PIS", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Stumm gl\u00fcht der r\u00f6mische Schutt am Weg,", "tokens": ["Stumm", "gl\u00fcht", "der", "r\u00f6\u00b7mi\u00b7sche", "Schutt", "am", "Weg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Schlafend ziehn sie vorbei.", "tokens": ["Schla\u00b7fend", "ziehn", "sie", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Unter der Trommel vorgebeugt", "tokens": ["Un\u00b7ter", "der", "Trom\u00b7mel", "vor\u00b7ge\u00b7beugt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVPP"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Der schlafende Tambour geht,", "tokens": ["Der", "schla\u00b7fen\u00b7de", "Tam\u00b7bour", "geht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es nickt der Kommandant zu Ro\u00df,", "tokens": ["Es", "nickt", "der", "Kom\u00b7man\u00b7dant", "zu", "Ro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von webender Glut umweht;", "tokens": ["Von", "we\u00b7ben\u00b7der", "Glut", "um\u00b7weht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Es schl\u00e4ft die Truppe, Haupt f\u00fcr Haupt", "tokens": ["Es", "schl\u00e4ft", "die", "Trup\u00b7pe", ",", "Haupt", "f\u00fcr", "Haupt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Unter der Sonne gesenkt,", "tokens": ["Un\u00b7ter", "der", "Son\u00b7ne", "ge\u00b7senkt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "Von der Gewohnheit Eisenfaust", "tokens": ["Von", "der", "Ge\u00b7wohn\u00b7heit", "Ei\u00b7sen\u00b7faust"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In Schritt und Tritt gelenkt.", "tokens": ["In", "Schritt", "und", "Tritt", "ge\u00b7lenkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und was sonst in der dunklen Nacht", "tokens": ["Und", "was", "sonst", "in", "der", "dunk\u00b7len", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Das enge Zelt nur sieht,", "tokens": ["Das", "en\u00b7ge", "Zelt", "nur", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wird unterm offnen Himmelblau", "tokens": ["Wird", "un\u00b7term", "off\u00b7nen", "Him\u00b7mel\u00b7blau"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom W\u00fcstenlicht durchgl\u00fcht.", "tokens": ["Vom", "W\u00fcs\u00b7ten\u00b7licht", "durch\u00b7gl\u00fcht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es spielt das schmerzliche Mienenspiel", "tokens": ["Es", "spielt", "das", "schmerz\u00b7li\u00b7che", "Mie\u00b7nen\u00b7spiel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ungl\u00fccklichen Manns, der tr\u00e4umt,", "tokens": ["Un\u00b7gl\u00fcck\u00b7li\u00b7chen", "Manns", ",", "der", "tr\u00e4umt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Von Gram und Leid und Bitterkeit", "tokens": ["Von", "Gram", "und", "Leid", "und", "Bit\u00b7ter\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ist jeglicher Mund ums\u00e4umt.", "tokens": ["Ist", "jeg\u00b7li\u00b7cher", "Mund", "um\u00b7s\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Es zuckt die Lippe, es zuckt das Aug,", "tokens": ["Es", "zuckt", "die", "Lip\u00b7pe", ",", "es", "zuckt", "das", "Aug", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Auf d\u00fcrre Wangen quillt", "tokens": ["Auf", "d\u00fcr\u00b7re", "Wan\u00b7gen", "quillt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die unbemeisterte Tr\u00e4ne hin,", "tokens": ["Die", "un\u00b7be\u00b7meis\u00b7ter\u00b7te", "Tr\u00e4\u00b7ne", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Vom Sonnenbrand gestillt.", "tokens": ["Vom", "Son\u00b7nen\u00b7brand", "ge\u00b7stillt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie schaun ein reizend Spiegelbild", "tokens": ["Sie", "schaun", "ein", "rei\u00b7zend", "Spie\u00b7gel\u00b7bild"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vom k\u00fchlen Heimatstrand,", "tokens": ["Vom", "k\u00fch\u00b7len", "Hei\u00b7mat\u00b7strand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das gr\u00fcne Kleefeld, rot bebl\u00fcmt,", "tokens": ["Das", "gr\u00fc\u00b7ne", "Klee\u00b7feld", ",", "rot", "be\u00b7bl\u00fcmt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Mutter, die einst den Sohn ger\u00fchmt,", "tokens": ["Die", "Mut\u00b7ter", ",", "die", "einst", "den", "Sohn", "ge\u00b7r\u00fchmt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Verlornes Vaterland!", "tokens": ["Ver\u00b7lor\u00b7nes", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ein Schu\u00df \u2013 da flattert's wei\u00df heran,", "tokens": ["Ein", "Schu\u00df", "\u2013", "da", "flat\u00b7tert's", "wei\u00df", "he\u00b7ran", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "KOUS", "NE", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und schon steht das Quarr\u00e9", "tokens": ["Und", "schon", "steht", "das", "Quarr\u00e9"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Schlagfertig und munter, und keiner sah", "tokens": ["Schlag\u00b7fer\u00b7tig", "und", "mun\u00b7ter", ",", "und", "kei\u00b7ner", "sah"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "$,", "KON", "PIS", "VVFIN"], "meter": "+---+--+-+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Des andern Reu und Weh;", "tokens": ["Des", "an\u00b7dern", "Reu", "und", "Weh", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nur zorniger ist jeder Mann", "tokens": ["Nur", "zor\u00b7ni\u00b7ger", "ist", "je\u00b7der", "Mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und ihm willkommen der Streit;", "tokens": ["Und", "ihm", "will\u00b7kom\u00b7men", "der", "Streit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Doch wie er kam, zerstiebt der Feind,", "tokens": ["Doch", "wie", "er", "kam", ",", "zer\u00b7stiebt", "der", "Feind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wie Traum und Reu so weit!", "tokens": ["Wie", "Traum", "und", "Reu", "so", "weit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NE", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}