{"textgrid.poem.53670": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Neujahrsgru\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Blickt her!", "tokens": ["Blickt", "her", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$."], "meter": "--", "measure": "unknown.measure.zero"}, "line.2": {"text": "Ihr kamt ins leise Gleiten \u2013", "tokens": ["Ihr", "kamt", "ins", "lei\u00b7se", "Glei\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "die alte Zeit, sie winkt und winkt . . .", "tokens": ["die", "al\u00b7te", "Zeit", ",", "sie", "winkt", "und", "winkt", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "KON", "VVFIN", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr d\u00fcrft euch \u00fcber Stile streiten,", "tokens": ["Ihr", "d\u00fcrft", "euch", "\u00fc\u00b7ber", "Sti\u00b7le", "strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "indes ihr immer tiefer sinkt.", "tokens": ["in\u00b7des", "ihr", "im\u00b7mer", "tie\u00b7fer", "sinkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Im Schrank h\u00e4ngt noch ein guter Sakko,", "tokens": ["Im", "Schrank", "h\u00e4ngt", "noch", "ein", "gu\u00b7ter", "Sak\u00b7ko", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+---+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "im Bord steht noch ein Lederband.", "tokens": ["im", "Bord", "steht", "noch", "ein", "Le\u00b7der\u00b7band", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Einst saht ihr noch die Sadda Yacco,", "tokens": ["Einst", "saht", "ihr", "noch", "die", "Sad\u00b7da", "Y\u00b7ac\u00b7co", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NE", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "ihr wu\u00dftet, wo Mentone stand.", "tokens": ["ihr", "wu\u00df\u00b7tet", ",", "wo", "Men\u00b7to\u00b7ne", "stand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und immer kleiner wird die Wohnung,", "tokens": ["Und", "im\u00b7mer", "klei\u00b7ner", "wird", "die", "Woh\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "und immer kleiner wird der Kreis.", "tokens": ["und", "im\u00b7mer", "klei\u00b7ner", "wird", "der", "Kreis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Uns alle fleddert ohne Schonung", "tokens": ["Uns", "al\u00b7le", "fled\u00b7dert", "oh\u00b7ne", "Scho\u00b7nung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "PIS", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "des Unternehmers Hungerpreis.", "tokens": ["des", "Un\u00b7ter\u00b7neh\u00b7mers", "Hun\u00b7ger\u00b7preis", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wann habt ihr aus den stickigen L\u00fcften", "tokens": ["Wann", "habt", "ihr", "aus", "den", "sti\u00b7cki\u00b7gen", "L\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "zum letzen Male ausgesp\u00e4ht?", "tokens": ["zum", "let\u00b7zen", "Ma\u00b7le", "aus\u00b7ge\u00b7sp\u00e4ht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was wissen wir von fremden D\u00fcften,", "tokens": ["Was", "wis\u00b7sen", "wir", "von", "frem\u00b7den", "D\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "von dem, was drau\u00dfen vor sich geht?", "tokens": ["von", "dem", ",", "was", "drau\u00b7\u00dfen", "vor", "sich", "geht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "ADV", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Kommi\u00df. Kommi\u00df. Und B\u00fcrokraten.", "tokens": ["Kom\u00b7mi\u00df", ".", "Kom\u00b7mi\u00df", ".", "Und", "B\u00fc\u00b7ro\u00b7kra\u00b7ten", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er hats geschafft, der Milit\u00e4r:", "tokens": ["Er", "hats", "ge\u00b7schafft", ",", "der", "Mi\u00b7li\u00b7t\u00e4r", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vom Volk der Denker und Soldaten", "tokens": ["Vom", "Volk", "der", "Den\u00b7ker", "und", "Sol\u00b7da\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "nimmt keiner einen Knochen mehr.", "tokens": ["nimmt", "kei\u00b7ner", "ei\u00b7nen", "Kno\u00b7chen", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ihr repetiert die alten Lieder", "tokens": ["Ihr", "re\u00b7pe\u00b7tiert", "die", "al\u00b7ten", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "zum \u00dcberdru\u00df. Die Muse schielt.", "tokens": ["zum", "\u00dc\u00b7berd\u00b7ru\u00df", ".", "Die", "Mu\u00b7se", "schielt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein sanfter Balkan senkt sich nieder,", "tokens": ["Ein", "sanf\u00b7ter", "Bal\u00b7kan", "senkt", "sich", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "in dem ihr keine Rolle spielt.", "tokens": ["in", "dem", "ihr", "kei\u00b7ne", "Rol\u00b7le", "spielt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Der starke H\u00e4ndler sitzt am Ruder,", "tokens": ["Der", "star\u00b7ke", "H\u00e4nd\u00b7ler", "sitzt", "am", "Ru\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "die Finger dick, den Nacken feist.", "tokens": ["die", "Fin\u00b7ger", "dick", ",", "den", "Na\u00b7cken", "feist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du bist ein, bleibst ein armes Luder,", "tokens": ["Du", "bist", "ein", ",", "bleibst", "ein", "ar\u00b7mes", "Lu\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKVZ", "$,", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "auch wenn du h\u00fcbsch zu schreiben wei\u00dft.", "tokens": ["auch", "wenn", "du", "h\u00fcbsch", "zu", "schrei\u00b7ben", "wei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADJD", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und Frauen, Blumen, Weltenr\u00e4ume,", "tokens": ["Und", "Frau\u00b7en", ",", "Blu\u00b7men", ",", "Wel\u00b7ten\u00b7r\u00e4u\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "sie bl\u00fchn f\u00fcr den, der st\u00e4rker war.", "tokens": ["sie", "bl\u00fchn", "f\u00fcr", "den", ",", "der", "st\u00e4r\u00b7ker", "war", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "$,", "PRELS", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schlag, K\u00fcnstler, deine Purzelb\u00e4ume!", "tokens": ["Schlag", ",", "K\u00fcnst\u00b7ler", ",", "dei\u00b7ne", "Pur\u00b7zel\u00b7b\u00e4u\u00b7me", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du bist nicht mehr. Es fliehn die Tr\u00e4ume . . .", "tokens": ["Du", "bist", "nicht", "mehr", ".", "Es", "fliehn", "die", "Tr\u00e4u\u00b7me", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "$.", "PPER", "VVFIN", "ART", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "In diesem Sinn:", "tokens": ["In", "die\u00b7sem", "Sinn", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Ein frohes Jahr \u2013!", "tokens": ["Ein", "fro\u00b7hes", "Jahr", "\u2013", "!"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}