{"dta.poem.10743": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Ich thu ein Rose loben/", "tokens": ["Ich", "thu", "ein", "Ro\u00b7se", "lo\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Rose tugent voll.", "tokens": ["Ein", "Ro\u00b7se", "tu\u00b7gent", "voll", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wolt mich mit jhr verloben/", "tokens": ["Wolt", "mich", "mit", "jhr", "ver\u00b7lo\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "APPR", "PPER", "VVINF", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Wans jhr gefiehle wol.", "tokens": ["Wans", "jhr", "ge\u00b7fieh\u00b7le", "wol", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ihrs gleichen find man nicht", "tokens": ["Ihrs", "glei\u00b7chen", "find", "man", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "VVFIN", "PIS", "PTKNEG"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "In Schwaben vnd in Francken:", "tokens": ["In", "Schwa\u00b7ben", "vnd", "in", "Fran\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Mich Schwachen vnd sehr Krancken", "tokens": ["Mich", "Schwa\u00b7chen", "vnd", "sehr", "Kran\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "KON", "ADV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sie Tag vnd nacht anficht.", "tokens": ["Sie", "Tag", "vnd", "nacht", "an\u00b7ficht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}