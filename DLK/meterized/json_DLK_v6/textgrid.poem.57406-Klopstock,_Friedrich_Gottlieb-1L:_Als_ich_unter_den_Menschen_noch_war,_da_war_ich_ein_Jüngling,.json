{"textgrid.poem.57406": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als ich unter den Menschen noch war, da war ich ein J\u00fcngling,", "genre": "verse", "period": "N.A.", "pub_year": 1749, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ich unter den Menschen noch war, da war ich ein J\u00fcngling,", "tokens": ["Als", "ich", "un\u00b7ter", "den", "Men\u00b7schen", "noch", "war", ",", "da", "war", "ich", "ein", "J\u00fcng\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "ADV", "VAFIN", "$,", "ADV", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Weiblich und zart von Gef\u00fchl,", "tokens": ["Weib\u00b7lich", "und", "zart", "von", "Ge\u00b7f\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "APPR", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Ganz zur Empfindung der Liebe geschaffen. So z\u00e4rtlich und f\u00fchlend", "tokens": ["Ganz", "zur", "Emp\u00b7fin\u00b7dung", "der", "Lie\u00b7be", "ge\u00b7schaf\u00b7fen", ".", "So", "z\u00e4rt\u00b7lich", "und", "f\u00fch\u00b7lend"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "VVPP", "$.", "ADV", "ADJD", "KON", "VVPP"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "War kein Sterblicher mehr.", "tokens": ["War", "kein", "Sterb\u00b7li\u00b7cher", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADV", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Also sah ich ein g\u00f6ttliches M\u00e4dchen; so z\u00e4rtlich und f\u00fchlend", "tokens": ["Al\u00b7so", "sah", "ich", "ein", "g\u00f6tt\u00b7li\u00b7ches", "M\u00e4d\u00b7chen", ";", "so", "z\u00e4rt\u00b7lich", "und", "f\u00fch\u00b7lend"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$.", "ADV", "ADJD", "KON", "VVPP"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.6": {"text": "War keine Sterbliche mehr.", "tokens": ["War", "kei\u00b7ne", "Sterb\u00b7li\u00b7che", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Aber ein unerbittliches Schicksal, ein eisernes Schicksal", "tokens": ["A\u00b7ber", "ein", "un\u00b7er\u00b7bitt\u00b7li\u00b7ches", "Schick\u00b7sal", ",", "ein", "ei\u00b7ser\u00b7nes", "Schick\u00b7sal"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "+--+-+---+-+--+-", "measure": "iambic.hexa.invert"}, "line.8": {"text": "Gab mir ein hartes Gesetz,", "tokens": ["Gab", "mir", "ein", "har\u00b7tes", "Ge\u00b7setz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.9": {"text": "Ewig zu schweigen, und einsam zu weinen. So z\u00e4rtlich und elend", "tokens": ["E\u00b7wig", "zu", "schwei\u00b7gen", ",", "und", "ein\u00b7sam", "zu", "wei\u00b7nen", ".", "So", "z\u00e4rt\u00b7lich", "und", "e\u00b7lend"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "PTKZU", "VVINF", "$,", "KON", "ADJD", "PTKZU", "VVINF", "$.", "ADV", "ADJD", "KON", "ADJD"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.10": {"text": "War kein Sterblicher mehr.", "tokens": ["War", "kein", "Sterb\u00b7li\u00b7cher", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADV", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.11": {"text": "Einst sah ich sie im Haine; da ging ich seitw\u00e4rts und weinte", "tokens": ["Einst", "sah", "ich", "sie", "im", "Hai\u00b7ne", ";", "da", "ging", "ich", "seit\u00b7w\u00e4rts", "und", "wein\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$.", "ADV", "VVFIN", "PPER", "ADV", "KON", "VVFIN"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Seitw\u00e4rts ins Einsame hin,", "tokens": ["Seit\u00b7w\u00e4rts", "ins", "Ein\u00b7sa\u00b7me", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.13": {"text": "Tief in den dunkelsten Hain, der den b\u00e4ngsten Schmerzen geweiht war,", "tokens": ["Tief", "in", "den", "dun\u00b7kels\u00b7ten", "Hain", ",", "der", "den", "b\u00e4ngs\u00b7ten", "Schmer\u00b7zen", "ge\u00b7weiht", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$,", "PRELS", "ART", "ADJA", "NN", "VVPP", "VAFIN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.14": {"text": "Und dem erbebenden Geist.", "tokens": ["Und", "dem", "er\u00b7be\u00b7ben\u00b7den", "Geist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.15": {"text": "Ach, vergebens erschaffne \u2013 wenn jene, die die Natur dir", "tokens": ["Ach", ",", "ver\u00b7ge\u00b7bens", "er\u00b7schaff\u00b7ne", "\u2013", "wenn", "je\u00b7ne", ",", "die", "die", "Na\u00b7tur", "dir"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "$(", "KOUS", "PDS", "$,", "PRELS", "ART", "NN", "PPER"], "meter": "+-+--+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.16": {"text": "Gleich schuf, ewig dich flieht \u2013", "tokens": ["Gleich", "schuf", ",", "e\u00b7wig", "dich", "flieht", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADJD", "PPER", "VVFIN", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.17": {"text": "Ach, vergebens unsterbliche Seele! wenn ewig einsam", "tokens": ["Ach", ",", "ver\u00b7ge\u00b7bens", "uns\u00b7terb\u00b7li\u00b7che", "See\u00b7le", "!", "wenn", "e\u00b7wig", "ein\u00b7sam"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "ADJA", "NN", "$.", "KOUS", "ADJD", "ADJD"], "meter": "+-+--+--+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.18": {"text": "Dir die Unsterblichkeit ist.", "tokens": ["Dir", "die", "U\u00b7nsterb\u00b7lich\u00b7keit", "ist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Wenn du, da du die Seelen erschufst, zwo Seelen von vielen,", "tokens": ["Wenn", "du", ",", "da", "du", "die", "See\u00b7len", "er\u00b7schufst", ",", "zwo", "See\u00b7len", "von", "vie\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "CARD", "NN", "APPR", "PIAT", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.20": {"text": "M\u00fctterliche Natur,", "tokens": ["M\u00fct\u00b7ter\u00b7li\u00b7che", "Na\u00b7tur", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.21": {"text": "Z\u00e4rtlicher und sich \u00e4hnlich erschufst, und gleichwohl sie trenntest,", "tokens": ["Z\u00e4rt\u00b7li\u00b7cher", "und", "sich", "\u00e4hn\u00b7lich", "er\u00b7schufst", ",", "und", "gleich\u00b7wohl", "sie", "trenn\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PRF", "ADJD", "VVFIN", "$,", "KON", "ADV", "PPER", "VVFIN", "$,"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.22": {"text": "Sage, was dachtest du da,", "tokens": ["Sa\u00b7ge", ",", "was", "dach\u00b7test", "du", "da", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VVFIN", "PPER", "ADV", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.23": {"text": "M\u00fctterliche Natur? Sonst immer weise, mir aber", "tokens": ["M\u00fct\u00b7ter\u00b7li\u00b7che", "Na\u00b7tur", "?", "Sonst", "im\u00b7mer", "wei\u00b7se", ",", "mir", "a\u00b7ber"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$.", "ADV", "ADV", "VVFIN", "$,", "PPER", "ADV"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.24": {"text": "Hier nicht weise genug,", "tokens": ["Hier", "nicht", "wei\u00b7se", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADV", "ADV", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.25": {"text": "Hier nicht z\u00e4rtlich genug! nicht mehr die liebende Mutter,", "tokens": ["Hier", "nicht", "z\u00e4rt\u00b7lich", "ge\u00b7nug", "!", "nicht", "mehr", "die", "lie\u00b7ben\u00b7de", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADJD", "ADV", "$.", "PTKNEG", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.26": {"text": "Die du immer sonst warst!", "tokens": ["Die", "du", "im\u00b7mer", "sonst", "warst", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VAFIN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.27": {"text": "Ach, wenn dich noch Tr\u00e4nen erweichten! und wenn ein vor Wehmut", "tokens": ["Ach", ",", "wenn", "dich", "noch", "Tr\u00e4\u00b7nen", "er\u00b7weich\u00b7ten", "!", "und", "wenn", "ein", "vor", "Weh\u00b7mut"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KOUS", "PPER", "ADV", "NN", "VVINF", "$.", "KON", "KOUS", "ART", "APPR", "NN"], "meter": "-+--+--+---+-+-", "measure": "amphibrach.tri.plus"}, "line.28": {"text": "Bang erbebendes Herz", "tokens": ["Bang", "er\u00b7be\u00b7ben\u00b7des", "Herz"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.29": {"text": "Dich und dein eisernes Schicksal und seine Donner vers\u00f6hnte,", "tokens": ["Dich", "und", "dein", "ei\u00b7ser\u00b7nes", "Schick\u00b7sal", "und", "sei\u00b7ne", "Don\u00b7ner", "ver\u00b7s\u00f6hn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "PPOSAT", "ADJA", "NN", "KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+--+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.30": {"text": "Wenn du Mutter noch w\u00e4rst!", "tokens": ["Wenn", "du", "Mut\u00b7ter", "noch", "w\u00e4rst", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "ADV", "VAFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.31": {"text": "Wenn, wie vormals, dein Ohr, zur Zeit des goldenen Alters,", "tokens": ["Wenn", ",", "wie", "vor\u00b7mals", ",", "dein", "Ohr", ",", "zur", "Zeit", "des", "gol\u00b7de\u00b7nen", "Al\u00b7ters", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWAV", "ADV", "$,", "PPOSAT", "NN", "$,", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.32": {"text": "Stammelnde Seufzer vern\u00e4hm'!", "tokens": ["Stam\u00b7meln\u00b7de", "Seuf\u00b7zer", "ver\u00b7n\u00e4hm'", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.33": {"text": "Aber du bleibst unerbittlich und ernst. So sei es denn ewig!", "tokens": ["A\u00b7ber", "du", "bleibst", "un\u00b7er\u00b7bitt\u00b7lich", "und", "ernst", ".", "So", "sei", "es", "denn", "e\u00b7wig", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "KON", "ADJD", "$.", "ADV", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "+-+-+-+--+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.34": {"text": "Sei's! nicht mehr Mutter, Natur!", "tokens": ["Sei's", "!", "nicht", "mehr", "Mut\u00b7ter", ",", "Na\u00b7tur", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$.", "PTKNEG", "PIAT", "NN", "$,", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.35": {"text": "Warum hast du mich nicht, wie diesen Hain hier, erschaffen,", "tokens": ["Wa\u00b7rum", "hast", "du", "mich", "nicht", ",", "wie", "die\u00b7sen", "Hain", "hier", ",", "er\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PRF", "PTKNEG", "$,", "PWAV", "PDAT", "NN", "ADV", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.36": {"text": "Ruhig und ohne Gef\u00fchl?", "tokens": ["Ru\u00b7hig", "und", "oh\u00b7ne", "Ge\u00b7f\u00fchl", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "APPR", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.37": {"text": "Warum nicht, wie den S\u00e4nger des Hains? Er f\u00fchlt sich vielleicht nicht,", "tokens": ["Wa\u00b7rum", "nicht", ",", "wie", "den", "S\u00e4n\u00b7ger", "des", "Hains", "?", "Er", "f\u00fchlt", "sich", "viel\u00b7leicht", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "$,", "PWAV", "ART", "NN", "ART", "NN", "$.", "PPER", "VVFIN", "PRF", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.38": {"text": "Oder ist es Gef\u00fchl,", "tokens": ["O\u00b7der", "ist", "es", "Ge\u00b7f\u00fchl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.39": {"text": "Was er t\u00f6net; sinds z\u00e4rtliche Klagen, die seufzend sein Mund singt,", "tokens": ["Was", "er", "t\u00f6\u00b7net", ";", "sinds", "z\u00e4rt\u00b7li\u00b7che", "Kla\u00b7gen", ",", "die", "seuf\u00b7zend", "sein", "Mund", "singt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$.", "VAFIN", "ADJA", "NN", "$,", "PRELS", "ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "--+--+--+--+--++", "measure": "anapaest.tetra.plus"}, "line.40": {"text": "Ach, so wird er geh\u00f6rt!", "tokens": ["Ach", ",", "so", "wird", "er", "ge\u00b7h\u00f6rt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VAFIN", "PPER", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.41": {"text": "Ach, so lieben ihn S\u00e4ngerinnen! so donnert kein Schicksal", "tokens": ["Ach", ",", "so", "lie\u00b7ben", "ihn", "S\u00e4n\u00b7ge\u00b7rin\u00b7nen", "!", "so", "don\u00b7nert", "kein", "Schick\u00b7sal"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PPER", "NN", "$.", "ADV", "VVFIN", "PIAT", "NN"], "meter": "+-+--+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.42": {"text": "Sie zu trennen daher!", "tokens": ["Sie", "zu", "tren\u00b7nen", "da\u00b7her", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "PAV", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.43": {"text": "Ach, so f\u00fchlt er kein menschliches Elend! \u2013 Auf, la\u00df mich wie er sein!", "tokens": ["Ach", ",", "so", "f\u00fchlt", "er", "kein", "menschli\u00b7ches", "E\u00b7lend", "!", "\u2013", "Auf", ",", "la\u00df", "mich", "wie", "er", "sein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PPER", "PIAT", "ADJA", "NN", "$.", "$(", "APPR", "$,", "VVIMP", "PPER", "PWAV", "PPER", "VAINF", "$."], "meter": "+-+--+-+----+-+", "measure": "trochaic.hexa.relaxed"}, "line.44": {"text": "Nicht mehr Mutter, Natur,", "tokens": ["Nicht", "mehr", "Mut\u00b7ter", ",", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "PIAT", "NN", "$,", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.45": {"text": "Schaffe zur Nachtigall mich! doch la\u00df mir die menschliche Seele,", "tokens": ["Schaf\u00b7fe", "zur", "Nach\u00b7ti\u00b7gall", "mich", "!", "doch", "la\u00df", "mir", "die", "menschli\u00b7che", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PPER", "$.", "KON", "VVIMP", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+--+--+-+-", "measure": "iambic.hexa.invert"}, "line.46": {"text": "Diese Seele nicht mehr!", "tokens": ["Die\u00b7se", "See\u00b7le", "nicht", "mehr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PTKNEG", "ADV", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.47": {"text": "Also sagt' ich, und wurde verwandelt, doch blieb mir die Seele", "tokens": ["Al\u00b7so", "sagt'", "ich", ",", "und", "wur\u00b7de", "ver\u00b7wan\u00b7delt", ",", "doch", "blieb", "mir", "die", "See\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "VAFIN", "VVPP", "$,", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.48": {"text": "Und mein zu f\u00fchlendes Herz;", "tokens": ["Und", "mein", "zu", "f\u00fch\u00b7len\u00b7des", "Herz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.49": {"text": "Und, nicht gl\u00fccklicher, klag' ich noch einsam, und weine die Nacht durch", "tokens": ["Und", ",", "nicht", "gl\u00fcck\u00b7li\u00b7cher", ",", "klag'", "ich", "noch", "ein\u00b7sam", ",", "und", "wei\u00b7ne", "die", "Nacht", "durch"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "PTKNEG", "ADJD", "$,", "VVFIN", "PPER", "ADV", "ADJD", "$,", "KON", "VVFIN", "ART", "NN", "APPR"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.50": {"text": "Und den mir n\u00e4chtlichen Tag.", "tokens": ["Und", "den", "mir", "n\u00e4cht\u00b7li\u00b7chen", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.51": {"text": "Wenn der Morgen dahertaut, wenn gl\u00fccklichern V\u00f6geln und Menschen", "tokens": ["Wenn", "der", "Mor\u00b7gen", "da\u00b7her\u00b7taut", ",", "wenn", "gl\u00fcck\u00b7li\u00b7chern", "V\u00f6\u00b7geln", "und", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,", "KOUS", "ADJA", "NN", "KON", "NN"], "meter": "--+---+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.52": {"text": "Du, o Abendstern, winkst,", "tokens": ["Du", ",", "o", "A\u00b7bends\u00b7tern", ",", "winkst", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "FM", "NN", "$,", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.53": {"text": "Geht, die ich lieb', im Haine daher; dann sing' ich ihr Klagen,", "tokens": ["Geht", ",", "die", "ich", "lieb'", ",", "im", "Hai\u00b7ne", "da\u00b7her", ";", "dann", "sing'", "ich", "ihr", "Kla\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$,", "APPRART", "NN", "PAV", "$.", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.54": {"text": "Aber sie h\u00f6ret mich nicht.", "tokens": ["A\u00b7ber", "sie", "h\u00f6\u00b7ret", "mich", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.55": {"text": "O so h\u00f6re mich, Jupiter, dann, du, des hohen Olympus", "tokens": ["O", "so", "h\u00f6\u00b7re", "mich", ",", "Ju\u00b7pi\u00b7ter", ",", "dann", ",", "du", ",", "des", "ho\u00b7hen", "O\u00b7lym\u00b7pus"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADV", "VVFIN", "PPER", "$,", "NN", "$,", "ADV", "$,", "PPER", "$,", "ART", "ADJA", "NE"], "meter": "+-+--+-+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.56": {"text": "Donnerer, h\u00f6re du mich:", "tokens": ["Don\u00b7ne\u00b7rer", ",", "h\u00f6\u00b7re", "du", "mich", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "PRF", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.57": {"text": "Schaffe zum Adler mich um, la\u00df deinen Donner mich tragen,", "tokens": ["Schaf\u00b7fe", "zum", "Ad\u00b7ler", "mich", "um", ",", "la\u00df", "dei\u00b7nen", "Don\u00b7ner", "mich", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PPER", "PTKVZ", "$,", "VVIMP", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "+--+---+--+--+-", "measure": "dactylic.di.plus"}, "line.58": {"text": "Da\u00df sein kriegrischer Schall", "tokens": ["Da\u00df", "sein", "krie\u00b7gri\u00b7scher", "Schall"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.59": {"text": "Hart und f\u00fchllos mich mache, da\u00df in den hohen Gewittern", "tokens": ["Hart", "und", "f\u00fchl\u00b7los", "mich", "ma\u00b7che", ",", "da\u00df", "in", "den", "ho\u00b7hen", "Ge\u00b7wit\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "VVFIN", "PPER", "VVFIN", "$,", "KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.60": {"text": "Z\u00e4rtlich mein Herz nicht mehr bebt,", "tokens": ["Z\u00e4rt\u00b7lich", "mein", "Herz", "nicht", "mehr", "bebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.61": {"text": "Da\u00df ich die ehernen donnernden Wagen des Zeus nur erblicke,", "tokens": ["Da\u00df", "ich", "die", "e\u00b7her\u00b7nen", "don\u00b7nern\u00b7den", "Wa\u00b7gen", "des", "Zeus", "nur", "er\u00b7bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "ADJA", "NN", "ART", "NE", "ADV", "VVFIN", "$,"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.62": {"text": "Aber kein bl\u00fchend Gesicht,", "tokens": ["A\u00b7ber", "kein", "bl\u00fc\u00b7hend", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJD", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.63": {"text": "Und kein l\u00e4chelndes Auge, das seelenvoll redt, und die Sprache", "tokens": ["Und", "kein", "l\u00e4\u00b7cheln\u00b7des", "Au\u00b7ge", ",", "das", "see\u00b7len\u00b7voll", "redt", ",", "und", "die", "Spra\u00b7che"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,", "KON", "ART", "NN"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.64": {"text": "Der Unsterblichen spricht.\u2013", "tokens": ["Der", "U\u00b7nsterb\u00b7li\u00b7chen", "spricht", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$("], "meter": "-+---+", "measure": "dactylic.init"}, "line.65": {"text": "Also sang er und wurde zum Adler, und an dem Olympus", "tokens": ["Al\u00b7so", "sang", "er", "und", "wur\u00b7de", "zum", "Ad\u00b7ler", ",", "und", "an", "dem", "O\u00b7lym\u00b7pus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VAFIN", "APPRART", "NN", "$,", "KON", "APPR", "ART", "NE"], "meter": "+-+--+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.66": {"text": "Zog sich ein Wetter herauf.", "tokens": ["Zog", "sich", "ein", "Wet\u00b7ter", "her\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}