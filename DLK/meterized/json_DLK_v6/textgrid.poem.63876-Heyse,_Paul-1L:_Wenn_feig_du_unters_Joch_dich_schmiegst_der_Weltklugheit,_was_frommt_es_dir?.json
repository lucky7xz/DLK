{"textgrid.poem.63876": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn feig du unters Joch dich schmiegst der Weltklugheit, was frommt es dir?", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn feig du unters Joch dich schmiegst der Weltklugheit, was frommt es dir?", "tokens": ["Wenn", "feig", "du", "un\u00b7ters", "Joch", "dich", "schmiegst", "der", "Welt\u00b7klug\u00b7heit", ",", "was", "frommt", "es", "dir", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "APPRART", "NN", "PPER", "VVFIN", "ART", "NN", "$,", "PWS", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Man wei\u00df, wenn du den R\u00fccken biegst, 's ist nicht dein Ernst. Was frommt es dir?", "tokens": ["Man", "wei\u00df", ",", "wenn", "du", "den", "R\u00fc\u00b7cken", "biegst", ",", "'s", "ist", "nicht", "dein", "Ernst", ".", "Was", "frommt", "es", "dir", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "PPER", "VAFIN", "PTKNEG", "PPOSAT", "NN", "$.", "PWS", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+--+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "Man f\u00fchlt' an deinem Ton und Blick und wie du auf dem Nacken tr\u00e4gst", "tokens": ["Man", "f\u00fchlt'", "an", "dei\u00b7nem", "Ton", "und", "Blick", "und", "wie", "du", "auf", "dem", "Na\u00b7cken", "tr\u00e4gst"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "PPOSAT", "NN", "KON", "NN", "KON", "PWAV", "PPER", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Den Kopf, den du so h\u00f6flich wiegst, du heuchelst nur. Was frommt es dir?", "tokens": ["Den", "Kopf", ",", "den", "du", "so", "h\u00f6f\u00b7lich", "wiegst", ",", "du", "heu\u00b7chelst", "nur", ".", "Was", "frommt", "es", "dir", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVFIN", "$,", "PPER", "ADV", "ADV", "$.", "PWS", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.5": {"text": "Nein, sage lieber frei heraus dem Schelm, dem Sch\u00e4cher ins Gesicht,", "tokens": ["Nein", ",", "sa\u00b7ge", "lie\u00b7ber", "frei", "he\u00b7raus", "dem", "Schelm", ",", "dem", "Sch\u00e4\u00b7cher", "ins", "Ge\u00b7sicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "ADV", "ADJD", "ADV", "ART", "NN", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.6": {"text": "Wie dir zu Mut. Denn wenn du schwiegst als Diplomat, was frommt es dir?", "tokens": ["Wie", "dir", "zu", "Mut", ".", "Denn", "wenn", "du", "schwiegst", "als", "Dip\u00b7lo\u00b7mat", ",", "was", "frommt", "es", "dir", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "$.", "KON", "KOUS", "PPER", "VVFIN", "KOUS", "NN", "$,", "PWS", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.7": {"text": "So dumm ist selbst der d\u00fcmmste nicht, da\u00df er nicht merk', aus Mitleid nur", "tokens": ["So", "dumm", "ist", "selbst", "der", "d\u00fcmms\u00b7te", "nicht", ",", "da\u00df", "er", "nicht", "merk'", ",", "aus", "Mit\u00b7leid", "nur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "ADV", "ART", "ADJA", "PTKNEG", "$,", "KOUS", "PPER", "PTKNEG", "VVFIN", "$,", "APPR", "NN", "ADV"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.8": {"text": "Gescheh's, da\u00df du ihn nicht bekriegst, und schonst du ihn, was frommt es dir?", "tokens": ["Ge\u00b7scheh's", ",", "da\u00df", "du", "ihn", "nicht", "be\u00b7kriegst", ",", "und", "schonst", "du", "ihn", ",", "was", "frommt", "es", "dir", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "PPER", "PTKNEG", "VVFIN", "$,", "KON", "VVFIN", "PPER", "PPER", "$,", "PWS", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.9": {"text": "Dies macht verha\u00dft, allein was liegt am Hasse der Ver\u00e4chtlichen?", "tokens": ["Dies", "macht", "ver\u00b7ha\u00dft", ",", "al\u00b7lein", "was", "liegt", "am", "Has\u00b7se", "der", "Ver\u00b7\u00e4cht\u00b7li\u00b7chen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "$,", "ADV", "PWS", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.10": {"text": "Wenn du in ihrer Achtung stiegst um solchen Preis, was frommt' es dir?", "tokens": ["Wenn", "du", "in", "ih\u00b7rer", "Ach\u00b7tung", "stiegst", "um", "sol\u00b7chen", "Preis", ",", "was", "frommt'", "es", "dir", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "APPR", "PIAT", "NN", "$,", "PWS", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.11": {"text": "Gef\u00fcrchtet sein als St\u00f6renfried, als unbequemer Narr verlacht,", "tokens": ["Ge\u00b7f\u00fcrch\u00b7tet", "sein", "als", "St\u00f6\u00b7ren\u00b7fried", ",", "als", "un\u00b7be\u00b7que\u00b7mer", "Narr", "ver\u00b7lacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAINF", "KOKOM", "NN", "$,", "KOUS", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.12": {"text": "Ist mehr, als, wenn im Grab du liegst, ein Lobgesang; was frommt' er dir?", "tokens": ["Ist", "mehr", ",", "als", ",", "wenn", "im", "Grab", "du", "liegst", ",", "ein", "Lob\u00b7ge\u00b7sang", ";", "was", "frommt'", "er", "dir", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "KOUS", "$,", "KOUS", "APPRART", "NN", "PPER", "VVFIN", "$,", "ART", "NN", "$.", "PWS", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.13": {"text": "Am besten freilich bleib f\u00fcr dich. Denn wenn du zehnmal Recht beh\u00e4ltst", "tokens": ["Am", "bes\u00b7ten", "frei\u00b7lich", "bleib", "f\u00fcr", "dich", ".", "Denn", "wenn", "du", "zehn\u00b7mal", "Recht", "be\u00b7h\u00e4ltst"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "ADV", "VVFIN", "APPR", "PPER", "$.", "KON", "KOUS", "PPER", "ADV", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.14": {"text": "Und allen Unverstand besiegst \u2013 ein solcher Sieg, was frommt' er dir?", "tokens": ["Und", "al\u00b7len", "Un\u00b7ver\u00b7stand", "be\u00b7siegst", "\u2013", "ein", "sol\u00b7cher", "Sieg", ",", "was", "frommt'", "er", "dir", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$(", "ART", "PIAT", "NN", "$,", "PWS", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}}}}