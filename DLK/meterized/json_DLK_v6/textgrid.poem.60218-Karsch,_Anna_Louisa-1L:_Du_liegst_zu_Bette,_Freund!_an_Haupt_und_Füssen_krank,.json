{"textgrid.poem.60218": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du liegst zu Bette, Freund! an Haupt und F\u00fcssen krank,", "genre": "verse", "period": "N.A.", "pub_year": 1762, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du liegst zu Bette, Freund! an Haupt und F\u00fcssen krank,", "tokens": ["Du", "liegst", "zu", "Bet\u00b7te", ",", "Freund", "!", "an", "Haupt", "und", "F\u00fcs\u00b7sen", "krank", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "NN", "$.", "APPR", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Itzt, da von allen Patrioten", "tokens": ["Itzt", ",", "da", "von", "al\u00b7len", "Pat\u00b7ri\u00b7o\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Geredet wird:", "tokens": ["Ge\u00b7re\u00b7det", "wird", ":"], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Zu seyn bey Friedrichs Volk, bis matt zu Boden sank", "tokens": ["Zu", "seyn", "bey", "Fried\u00b7richs", "Volk", ",", "bis", "matt", "zu", "Bo\u00b7den", "sank"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "APPR", "NE", "NN", "$,", "KOUS", "ADJD", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der stolzgekommne Feind, der jene Vestung wieder", "tokens": ["Der", "stolz\u00b7ge\u00b7komm\u00b7ne", "Feind", ",", "der", "je\u00b7ne", "Ves\u00b7tung", "wie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PDAT", "NN", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dem Sieger \u00fcberlassen soll?", "tokens": ["Dem", "Sie\u00b7ger", "\u00fc\u00b7ber\u00b7las\u00b7sen", "soll", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Bald st\u00fcrzt Theresiens geth\u00fcrmte Hoffnung nieder!", "tokens": ["Bald", "st\u00fcrzt", "The\u00b7re\u00b7si\u00b7ens", "ge\u00b7th\u00fcrm\u00b7te", "Hoff\u00b7nung", "nie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Darius baute so, von stolzer Hoffnung voll,", "tokens": ["Da\u00b7ri\u00b7us", "bau\u00b7te", "so", ",", "von", "stol\u00b7zer", "Hoff\u00b7nung", "voll", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$,", "APPR", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Acht grosse Schl\u00f6sser hin in eine grosse W\u00fcste;", "tokens": ["Acht", "gros\u00b7se", "Schl\u00f6s\u00b7ser", "hin", "in", "ei\u00b7ne", "gros\u00b7se", "W\u00fcs\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und keine Sorge fiel ihm ein,", "tokens": ["Und", "kei\u00b7ne", "Sor\u00b7ge", "fiel", "ihm", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df er den Bau verlassen m\u00fcste,", "tokens": ["Da\u00df", "er", "den", "Bau", "ver\u00b7las\u00b7sen", "m\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn halb heraufgef\u00fchrt die W\u00e4nde w\u00fcrden seyn!", "tokens": ["Wenn", "halb", "her\u00b7auf\u00b7ge\u00b7f\u00fchrt", "die", "W\u00e4n\u00b7de", "w\u00fcr\u00b7den", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VVPP", "ART", "NN", "VAFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So gieng sein stolzer Sohn mit pr\u00e4chtigem Ger\u00fcste,", "tokens": ["So", "gieng", "sein", "stol\u00b7zer", "Sohn", "mit", "pr\u00e4ch\u00b7ti\u00b7gem", "Ge\u00b7r\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und gl\u00e4nzend, gleich dem Sonnenschein,", "tokens": ["Und", "gl\u00e4n\u00b7zend", ",", "gleich", "dem", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ans freye Griechenland, hie\u00df ungez\u00e4hlte Heere", "tokens": ["Ans", "frey\u00b7e", "Grie\u00b7chen\u00b7land", ",", "hie\u00df", "un\u00b7ge\u00b7z\u00e4hl\u00b7te", "Hee\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vor sich vor\u00fcber gehn, und gab dem wilden Meere", "tokens": ["Vor", "sich", "vor\u00b7\u00fc\u00b7ber", "gehn", ",", "und", "gab", "dem", "wil\u00b7den", "Mee\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRF", "ADV", "VVINF", "$,", "KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Bestrafung, wie ein ernster Mann", "tokens": ["Be\u00b7stra\u00b7fung", ",", "wie", "ein", "erns\u00b7ter", "Mann"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dem wilden Knaben giebt, den er nicht zwingen kann;", "tokens": ["Dem", "wil\u00b7den", "Kna\u00b7ben", "giebt", ",", "den", "er", "nicht", "zwin\u00b7gen", "kann", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "PRELS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Den Wellen warf er Fesseln an.", "tokens": ["Den", "Wel\u00b7len", "warf", "er", "Fes\u00b7seln", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Fesseln aber, Freund, verschluckten erst das Eisen,", "tokens": ["Die", "Fes\u00b7seln", "a\u00b7ber", ",", "Freund", ",", "ver\u00b7schluck\u00b7ten", "erst", "das", "Ei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "NN", "$,", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und dann vier hundert Schiffe nach,", "tokens": ["Und", "dann", "vier", "hun\u00b7dert", "Schif\u00b7fe", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "CARD", "CARD", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Um einig mit dem Sturm, vollm\u00e4chtig zu beweisen,", "tokens": ["Um", "ei\u00b7nig", "mit", "dem", "Sturm", ",", "voll\u00b7m\u00e4ch\u00b7tig", "zu", "be\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "APPR", "ART", "NN", "$,", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wem Xerxes trotzig wiedersprach.", "tokens": ["Wem", "Xe\u00b7rxes", "trot\u00b7zig", "wie\u00b7der\u00b7sprach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Gott regieret noch, dem bey dem Hellesponte", "tokens": ["Der", "Gott", "re\u00b7gie\u00b7ret", "noch", ",", "dem", "bey", "dem", "Hel\u00b7les\u00b7pon\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Perser Hohn gesprochen hat;", "tokens": ["Der", "Per\u00b7ser", "Hohn", "ge\u00b7spro\u00b7chen", "hat", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df unser Friederich nicht matt", "tokens": ["Da\u00df", "un\u00b7ser", "Frie\u00b7de\u00b7rich", "nicht", "matt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PTKNEG", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geworden ist, o Freund! da\u00df ihn nichts st\u00fcrzen konnte,", "tokens": ["Ge\u00b7wor\u00b7den", "ist", ",", "o", "Freund", "!", "da\u00df", "ihn", "nichts", "st\u00fcr\u00b7zen", "konn\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "FM", "NN", "$.", "KOUS", "PPER", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das wollte dieser Gott, den Griech' und Perser blind", "tokens": ["Das", "woll\u00b7te", "die\u00b7ser", "Gott", ",", "den", "Griech'", "und", "Per\u00b7ser", "blind"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PDAT", "NN", "$,", "ART", "NN", "KON", "NN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "In ihrem Jupiter mit Hecatomben ehrten.", "tokens": ["In", "ih\u00b7rem", "Ju\u00b7pi\u00b7ter", "mit", "He\u00b7ca\u00b7tom\u00b7ben", "ehr\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wir aber die sein Wort und seinen Willen h\u00f6rten,", "tokens": ["Wir", "a\u00b7ber", "die", "sein", "Wort", "und", "sei\u00b7nen", "Wil\u00b7len", "h\u00f6r\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wir gl\u00fcckliche Gesch\u00f6pfe, sind", "tokens": ["Wir", "gl\u00fcck\u00b7li\u00b7che", "Ge\u00b7sch\u00f6p\u00b7fe", ",", "sind"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Im Herzen \u00fcberzeugt, da\u00df aus dem Vaterlande", "tokens": ["Im", "Her\u00b7zen", "\u00fc\u00b7berz\u00b7eugt", ",", "da\u00df", "aus", "dem", "Va\u00b7ter\u00b7lan\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVPP", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der Feind getrieben wird, und Deutschland nicht die Bande", "tokens": ["Der", "Feind", "ge\u00b7trie\u00b7ben", "wird", ",", "und", "Deutschland", "nicht", "die", "Ban\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "VAFIN", "$,", "KON", "NE", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Gedrohter Knechtschaft tragen darf;", "tokens": ["Ge\u00b7droh\u00b7ter", "Knecht\u00b7schaft", "tra\u00b7gen", "darf", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "So wie der Perser nicht mit seinen Millionen", "tokens": ["So", "wie", "der", "Per\u00b7ser", "nicht", "mit", "sei\u00b7nen", "Mil\u00b7lion\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NN", "PTKNEG", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "An Griechenland die Fesseln warf;", "tokens": ["An", "Grie\u00b7chen\u00b7land", "die", "Fes\u00b7seln", "warf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "So werfen sie an uns nicht beyde Kayser-Cronen.", "tokens": ["So", "wer\u00b7fen", "sie", "an", "uns", "nicht", "bey\u00b7de", "Kay\u00b7ser\u00b7Cro\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "PTKNEG", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}