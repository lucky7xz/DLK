{"dta.poem.11355": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf das absterben eben derselben,  \n im nahmen anderer.  \n  G. S.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr, die ihr euch den wahn der welt verblenden lasset,", "tokens": ["Ihr", ",", "die", "ihr", "euch", "den", "wahn", "der", "welt", "ver\u00b7blen\u00b7den", "las\u00b7set", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "PRF", "ART", "NN", "ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und den verkehrten schlu\u00df in die gedancken fasset,", "tokens": ["Und", "den", "ver\u00b7kehr\u00b7ten", "schlu\u00df", "in", "die", "ge\u00b7dan\u00b7cken", "fas\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die wahre gottesfurcht sey lauter fantasey,", "tokens": ["Die", "wah\u00b7re", "got\u00b7tes\u00b7furcht", "sey", "lau\u00b7ter", "fan\u00b7ta\u00b7sey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Schaut unsre Stryckin an! Jhr tugend-volles leben", "tokens": ["Schaut", "uns\u00b7re", "Stry\u00b7ck\u00b7in", "an", "!", "Ihr", "tu\u00b7gen\u00b7dvol\u00b7les", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$.", "PPOSAT", "NN", "VVINF"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Kan euch in dieser nacht ein helles licht abgeben,", "tokens": ["Kan", "euch", "in", "die\u00b7ser", "nacht", "ein", "hel\u00b7les", "licht", "ab\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PDAT", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-++--", "measure": "unknown.measure.hexa"}, "line.6": {"text": "Ich wei\u00df, es f\u00e4llt euch dann ein ander urtheil bey.", "tokens": ["Ich", "wei\u00df", ",", "es", "f\u00e4llt", "euch", "dann", "ein", "an\u00b7der", "ur\u00b7theil", "bey", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wir geben gerne zu: Da\u00df in Egyptens w\u00fcsten", "tokens": ["Wir", "ge\u00b7ben", "ger\u00b7ne", "zu", ":", "Da\u00df", "in", "E\u00b7gyp\u00b7tens", "w\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "$.", "KOUS", "APPR", "NE", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mehr molch und crocodil, als fromme tauben, nisten:", "tokens": ["Mehr", "molch", "und", "cro\u00b7co\u00b7dil", ",", "als", "from\u00b7me", "tau\u00b7ben", ",", "nis\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "$,", "KOUS", "ADJA", "ADJA", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und da\u00df die fr\u00f6mmigkeit ein seltner vogel ist.", "tokens": ["Und", "da\u00df", "die", "fr\u00f6m\u00b7mig\u00b7keit", "ein", "selt\u00b7ner", "vo\u00b7gel", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wir geben gerne nach: Da\u00df menschen ihre flecken", "tokens": ["Wir", "ge\u00b7ben", "ger\u00b7ne", "nach", ":", "Da\u00df", "men\u00b7schen", "ih\u00b7re", "fle\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "$.", "KOUS", "PIS", "PPOSAT", "NN"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Mit einem blossen schein, und sich mit masquen decken,", "tokens": ["Mit", "ei\u00b7nem", "blos\u00b7sen", "schein", ",", "und", "sich", "mit", "mas\u00b7quen", "de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "KON", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So da\u00df man offt den wolff selbst vor ein schaf erkiest.", "tokens": ["So", "da\u00df", "man", "offt", "den", "wolff", "selbst", "vor", "ein", "schaf", "er\u00b7kiest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PIS", "ADV", "ART", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Doch falscher purpur kan nicht \u00e4chtem purpur gleichen:", "tokens": ["Doch", "fal\u00b7scher", "pur\u00b7pur", "kan", "nicht", "\u00e4ch\u00b7tem", "pur\u00b7pur", "glei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "VMFIN", "PTKNEG", "ADJA", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Es wird kein schlechter stein den diamant erreichen,", "tokens": ["Es", "wird", "kein", "schlech\u00b7ter", "stein", "den", "di\u00b7a\u00b7mant", "er\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "ADJA", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "So sehr ihn auch die kunst mit folgen helffen kan.", "tokens": ["So", "sehr", "ihn", "auch", "die", "kunst", "mit", "fol\u00b7gen", "helf\u00b7fen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "ART", "NN", "APPR", "VVINF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Man mu\u00df die geister nur recht auf die probe stellen.", "tokens": ["Man", "mu\u00df", "die", "geis\u00b7ter", "nur", "recht", "auf", "die", "pro\u00b7be", "stel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "ADJA", "ADV", "ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Man kennt die worte schon, die aus den hertzen quellen,", "tokens": ["Man", "kennt", "die", "wor\u00b7te", "schon", ",", "die", "aus", "den", "hert\u00b7zen", "quel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "ADV", "$,", "PRELS", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die wercke geben bald den Pharis\u00e4er an.", "tokens": ["Die", "wer\u00b7cke", "ge\u00b7ben", "bald", "den", "Pha\u00b7ri\u00b7s\u00e4\u00b7er", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Der Stryckin, deren leib hier auf dem sarge lieget,", "tokens": ["Der", "Stry\u00b7ck\u00b7in", ",", "de\u00b7ren", "leib", "hier", "auf", "dem", "sar\u00b7ge", "lie\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELAT", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "Der Stryckin, deren geist die h\u00f6chste ruh vergn\u00fcget,", "tokens": ["Der", "Stry\u00b7ck\u00b7in", ",", "de\u00b7ren", "geist", "die", "h\u00f6chs\u00b7te", "ruh", "ver\u00b7gn\u00fc\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "Der Stryckin gottesfurcht hielt alle proben aus.", "tokens": ["Der", "Stry\u00b7ck\u00b7in", "got\u00b7tes\u00b7furcht", "hielt", "al\u00b7le", "pro\u00b7ben", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "PIS", "VVFIN", "PTKVZ", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "Erwegt, wie sie gelebt! Jhr wandel liegt am tage.", "tokens": ["Er\u00b7wegt", ",", "wie", "sie", "ge\u00b7lebt", "!", "Ihr", "wan\u00b7del", "liegt", "am", "ta\u00b7ge", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWAV", "PPER", "VVPP", "$.", "PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Der weisen urtheil cr\u00f6nt hier die gemeine sage:", "tokens": ["Der", "wei\u00b7sen", "ur\u00b7theil", "cr\u00f6nt", "hier", "die", "ge\u00b7mei\u00b7ne", "sa\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Der Stryckin hertze war kein \u00fcbert\u00fcnchtes haus.", "tokens": ["Der", "Stry\u00b7ck\u00b7in", "hert\u00b7ze", "war", "kein", "\u00fc\u00b7ber\u00b7t\u00fcnch\u00b7tes", "haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.25": {"text": "Bi\u00dfweilen h\u00fcllt die noth uns in das kleid der tugend;", "tokens": ["Bi\u00df\u00b7wei\u00b7len", "h\u00fcllt", "die", "noth", "uns", "in", "das", "kleid", "der", "tu\u00b7gend", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Jhr schien des gl\u00fcckes gunst von ihrer gr\u00fcnen jugend,", "tokens": ["Ihr", "schien", "des", "gl\u00fc\u00b7ckes", "gunst", "von", "ih\u00b7rer", "gr\u00fc\u00b7nen", "ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Von ihrer wiegen an, bi\u00df in die finstre grufft.", "tokens": ["Von", "ih\u00b7rer", "wie\u00b7gen", "an", ",", "bi\u00df", "in", "die", "finst\u00b7re", "grufft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "VVFIN", "PTKVZ", "$,", "KOUS", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Die mittel fehlten nicht, der welt sich zu bedienen,", "tokens": ["Die", "mit\u00b7tel", "fehl\u00b7ten", "nicht", ",", "der", "welt", "sich", "zu", "be\u00b7die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "$,", "ART", "NN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Man sah der ehren krantz um ihre schl\u00e4fe gr\u00fcnen.", "tokens": ["Man", "sah", "der", "eh\u00b7ren", "krantz", "um", "ih\u00b7re", "schl\u00e4\u00b7fe", "gr\u00fc\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Indessen d\u00e4mpfft ihr geist der eitlen regung dufft.", "tokens": ["In\u00b7des\u00b7sen", "d\u00e4mpfft", "ihr", "geist", "der", "eit\u00b7len", "re\u00b7gung", "dufft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Der hochmuth ist ihr nie in ihren kopff gestiegen,", "tokens": ["Der", "hoch\u00b7muth", "ist", "ihr", "nie", "in", "ih\u00b7ren", "kopff", "ge\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Sie hie\u00df die prahlerey zu ihren f\u00fcssen liegen,", "tokens": ["Sie", "hie\u00df", "die", "prah\u00b7le\u00b7rey", "zu", "ih\u00b7ren", "f\u00fcs\u00b7sen", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Und stieg durch niedrigkeit des geistes himmel-w\u00e4rts.", "tokens": ["Und", "stieg", "durch", "nied\u00b7rig\u00b7keit", "des", "geis\u00b7tes", "him\u00b7mel\u00b7w\u00e4rts", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.34": {"text": "Gebet und m\u00e4\u00dfigkeit verstattete den l\u00fcsten", "tokens": ["Ge\u00b7bet", "und", "m\u00e4\u00b7\u00dfig\u00b7keit", "ver\u00b7stat\u00b7te\u00b7te", "den", "l\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Hier keinen eingang nicht. Sie gieng die bahn der Christen:", "tokens": ["Hier", "kei\u00b7nen", "ein\u00b7gang", "nicht", ".", "Sie", "gieng", "die", "bahn", "der", "Chris\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "PTKNEG", "$.", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Jhr mund wie\u00df jederzeit ein GOtt geweihtes hertz.", "tokens": ["Ihr", "mund", "wie\u00df", "je\u00b7der\u00b7zeit", "ein", "Gott", "ge\u00b7weih\u00b7tes", "hertz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Sie pflegte keine zeit mit karten zu verspielen;", "tokens": ["Sie", "pfleg\u00b7te", "kei\u00b7ne", "zeit", "mit", "kar\u00b7ten", "zu", "ver\u00b7spie\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPR", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Ein andacht-volles buch, das itzund bey so vielen", "tokens": ["Ein", "an\u00b7dacht\u00b7vol\u00b7les", "buch", ",", "das", "it\u00b7zund", "bey", "so", "vie\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "APPR", "ADV", "PIAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Verworffen liegen mu\u00df, war ihre gr\u00f6ste lust.", "tokens": ["Ver\u00b7worf\u00b7fen", "lie\u00b7gen", "mu\u00df", ",", "war", "ih\u00b7re", "gr\u00f6s\u00b7te", "lust", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VMFIN", "$,", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Es hat kein neid und geitz ihr hertze zugeschlossen.", "tokens": ["Es", "hat", "kein", "neid", "und", "geitz", "ihr", "hert\u00b7ze", "zu\u00b7ge\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "KON", "VVIMP", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Was hat das armuth nicht von ihrer hand genossen?", "tokens": ["Was", "hat", "das", "ar\u00b7muth", "nicht", "von", "ih\u00b7rer", "hand", "ge\u00b7nos\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "PTKNEG", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Wem ist die mildigkeit der Stryckin unbewust?", "tokens": ["Wem", "ist", "die", "mil\u00b7dig\u00b7keit", "der", "Stry\u00b7ck\u00b7in", "un\u00b7be\u00b7wust", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.43": {"text": "Zeigt dieser wercke glantz nicht ihren wahren glauben?", "tokens": ["Zeigt", "die\u00b7ser", "wer\u00b7cke", "glantz", "nicht", "ih\u00b7ren", "wah\u00b7ren", "glau\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "NN", "PTKNEG", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Wer kan die gottesfurcht wohl einer seele rauben,", "tokens": ["Wer", "kan", "die", "got\u00b7tes\u00b7furcht", "wohl", "ei\u00b7ner", "see\u00b7le", "rau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Die alle reitzungen der schn\u00f6den welt besiegt?", "tokens": ["Die", "al\u00b7le", "reit\u00b7zun\u00b7gen", "der", "schn\u00f6\u00b7den", "welt", "be\u00b7siegt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.46": {"text": "Kommt! die ihr fromme sucht, wo ihr sie finden wollet!", "tokens": ["Kommt", "!", "die", "ihr", "from\u00b7me", "sucht", ",", "wo", "ihr", "sie", "fin\u00b7den", "wol\u00b7let", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "ART", "PPOSAT", "ADJA", "VVFIN", "$,", "PWAV", "PPER", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Der leib, um welchen itzt so manche thr\u00e4ne rollet,", "tokens": ["Der", "leib", ",", "um", "wel\u00b7chen", "itzt", "so", "man\u00b7che", "thr\u00e4\u00b7ne", "rol\u00b7let", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUI", "PWAT", "ADV", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Bewohnt ein solcher geist, der sich an GOtt vergn\u00fcgt.", "tokens": ["Be\u00b7wohnt", "ein", "sol\u00b7cher", "geist", ",", "der", "sich", "an", "Gott", "ver\u00b7gn\u00fcgt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "PIAT", "NN", "$,", "PRELS", "PRF", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "La\u00dft, feige seelen! la\u00dft euch dieses beyspiel dienen,", "tokens": ["La\u00dft", ",", "fei\u00b7ge", "see\u00b7len", "!", "la\u00dft", "euch", "die\u00b7ses", "bey\u00b7spiel", "die\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "ADJA", "VVINF", "$.", "VVIMP", "PPER", "PDAT", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Geht unsrer Stryckin nach, bi\u00df ihr die hohen b\u00fchnen", "tokens": ["Geht", "uns\u00b7rer", "Stry\u00b7ck\u00b7in", "nach", ",", "bi\u00df", "ihr", "die", "ho\u00b7hen", "b\u00fch\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$,", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+--++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.51": {"text": "Des Christenthums erreicht, die man so ferne sieht.", "tokens": ["Des", "Chris\u00b7ten\u00b7thums", "er\u00b7reicht", ",", "die", "man", "so", "fer\u00b7ne", "sieht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PRELS", "PIS", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Du aber, grosser Stryck! der du ihr vorgegangen,", "tokens": ["Du", "a\u00b7ber", ",", "gros\u00b7ser", "Stryck", "!", "der", "du", "ihr", "vor\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ADJA", "NN", "$.", "ART", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Um dessen weises haupt der Themis perlen prangen,", "tokens": ["Um", "des\u00b7sen", "wei\u00b7ses", "haupt", "der", "The\u00b7mis", "per\u00b7len", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Sey nicht bey ihrer grufft mit tr\u00fcber angst bem\u00fcht.", "tokens": ["Sey", "nicht", "bey", "ih\u00b7rer", "grufft", "mit", "tr\u00fc\u00b7ber", "angst", "be\u00b7m\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Dein schatz, den du bedaurst, singt itzund freuden-lieder,", "tokens": ["Dein", "schatz", ",", "den", "du", "be\u00b7daurst", ",", "singt", "it\u00b7zund", "freu\u00b7den\u00b7lie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Du siehest sie dereinst in jenem reiche wieder.", "tokens": ["Du", "sie\u00b7hest", "sie", "de\u00b7reinst", "in", "je\u00b7nem", "rei\u00b7che", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "PDAT", "ADJA", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Indessen, weil der HErr dich noch der erde g\u00f6nnt,", "tokens": ["In\u00b7des\u00b7sen", ",", "weil", "der", "Herr", "dich", "noch", "der", "er\u00b7de", "g\u00f6nnt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "So g\u00f6nne, da\u00df sie dich nicht vor der zeit verliehre,", "tokens": ["So", "g\u00f6n\u00b7ne", ",", "da\u00df", "sie", "dich", "nicht", "vor", "der", "zeit", "ver\u00b7lieh\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "PPER", "PRF", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Damit dein hoher witz Fridricianen ziere,", "tokens": ["Da\u00b7mit", "dein", "ho\u00b7her", "witz", "Frid\u00b7ri\u00b7ci\u00b7a\u00b7nen", "zie\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Den recht und billigkeit ihr wahres kleinod nennt!", "tokens": ["Den", "recht", "und", "bil\u00b7lig\u00b7keit", "ihr", "wah\u00b7res", "klei\u00b7nod", "nennt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}