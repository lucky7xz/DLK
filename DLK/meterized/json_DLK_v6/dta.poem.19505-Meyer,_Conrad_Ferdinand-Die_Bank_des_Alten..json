{"dta.poem.19505": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Die Bank des Alten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin einmal in einem Thal gegangen,", "tokens": ["Ich", "bin", "ein\u00b7mal", "in", "ei\u00b7nem", "Thal", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das fern der Welt, dem Himmel nahe war,", "tokens": ["Das", "fern", "der", "Welt", ",", "dem", "Him\u00b7mel", "na\u00b7he", "war", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "ART", "NN", "$,", "ART", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Durch das Gel\u00e4nde seiner Wiesen klangen", "tokens": ["Durch", "das", "Ge\u00b7l\u00e4n\u00b7de", "sei\u00b7ner", "Wie\u00b7sen", "klan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Sensen rings der zweiten Mahd im Jahr.", "tokens": ["Die", "Sen\u00b7sen", "rings", "der", "zwei\u00b7ten", "Mahd", "im", "Jahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich schritt durch eines D\u00f6rfchens stille Gassen.", "tokens": ["Ich", "schritt", "durch", "ei\u00b7nes", "D\u00f6rf\u00b7chens", "stil\u00b7le", "Gas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kein Laut. Vor einer H\u00fctte sa\u00df allein", "tokens": ["Kein", "Laut", ".", "Vor", "ei\u00b7ner", "H\u00fct\u00b7te", "sa\u00df", "al\u00b7lein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "$.", "APPR", "ART", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein alter Mann, von seiner Kraft verlassen,", "tokens": ["Ein", "al\u00b7ter", "Mann", ",", "von", "sei\u00b7ner", "Kraft", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und schaute feiernd auf den Firneschein.", "tokens": ["Und", "schau\u00b7te", "fei\u00b7ernd", "auf", "den", "Fir\u00b7ne\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Zuweilen, in die Hand gelegt die Stirne,", "tokens": ["Zu\u00b7wei\u00b7len", ",", "in", "die", "Hand", "ge\u00b7legt", "die", "Stir\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ART", "NN", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Seh' ich den Himmel jenes Thales blaun,", "tokens": ["Seh'", "ich", "den", "Him\u00b7mel", "je\u00b7nes", "Tha\u00b7les", "blaun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den M\u00fcden seh' ich wieder auf die Firne,", "tokens": ["Den", "M\u00fc\u00b7den", "seh'", "ich", "wie\u00b7der", "auf", "die", "Fir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die nahen, selig klaren Firne schaun.", "tokens": ["Die", "na\u00b7hen", ",", "se\u00b7lig", "kla\u00b7ren", "Fir\u00b7ne", "schaun", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJD", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "S'ist nur ein Traum. Wohl ist der Greis geschieden", "tokens": ["S'\u00b7ist", "nur", "ein", "Traum", ".", "Wohl", "ist", "der", "Greis", "ge\u00b7schie\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "NN", "$.", "ADV", "VAFIN", "ART", "NN", "VVPP"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Aus dieser Sonne Licht, von Jahren schwer;", "tokens": ["Aus", "die\u00b7ser", "Son\u00b7ne", "Licht", ",", "von", "Jah\u00b7ren", "schwer", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NN", "$,", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er schlummert wohl in seines Grabes Frieden", "tokens": ["Er", "schlum\u00b7mert", "wohl", "in", "sei\u00b7nes", "Gra\u00b7bes", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und seine Bank steht vor der H\u00fctte leer.", "tokens": ["Und", "sei\u00b7ne", "Bank", "steht", "vor", "der", "H\u00fct\u00b7te", "leer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Noch pulst mein Leben feurig. Wie den Andern", "tokens": ["Noch", "pulst", "mein", "Le\u00b7ben", "feu\u00b7rig", ".", "Wie", "den", "An\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADJD", "$.", "PWAV", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "kommt mir ein Tag, da mich die Kraft verr\u00e4t;", "tokens": ["kommt", "mir", "ein", "Tag", ",", "da", "mich", "die", "Kraft", "ver\u00b7r\u00e4t", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dann will ich langsam in die Berge wandern", "tokens": ["Dann", "will", "ich", "lang\u00b7sam", "in", "die", "Ber\u00b7ge", "wan\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und suchen wo die Bank des Alten steht.", "tokens": ["Und", "su\u00b7chen", "wo", "die", "Bank", "des", "Al\u00b7ten", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}