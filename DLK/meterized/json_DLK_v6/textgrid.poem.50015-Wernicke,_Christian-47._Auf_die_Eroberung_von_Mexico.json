{"textgrid.poem.50015": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "47. Auf die Eroberung von Mexico", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man fand in Indien der ", "tokens": ["Man", "fand", "in", "In\u00b7di\u00b7en", "der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "NE", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Volck, das ohne Zwang ", "tokens": ["Ein", "Volck", ",", "das", "oh\u00b7ne", "Zwang"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das einen ", "tokens": ["Das", "ei\u00b7nen"], "token_info": ["word", "word"], "pos": ["PDS", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "So dass hier jederman erkante mit erfreuen,", "tokens": ["So", "dass", "hier", "je\u00b7der\u00b7man", "er\u00b7kan\u00b7te", "mit", "er\u00b7freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ADV", "PIS", "VVFIN", "APPR", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es sey die ", "tokens": ["Es", "sey", "die"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}}}}}