{"dta.poem.9546": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Uber die conterfeyen der sch\u00f6nsten von Engel-  \n land/ die ietziger Churf\u00fcrstin zu Brandenburg  \n von daher geschicket worden.  \n \u2020 \u2020 \u2020", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die sch\u00f6nen Engell\u00e4nderinnen/", "tokens": ["Die", "sch\u00f6\u00b7nen", "En\u00b7gel\u00b7l\u00e4n\u00b7de\u00b7rin\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die man zum wunder \u00fcberschickt/", "tokens": ["Die", "man", "zum", "wun\u00b7der", "\u00fc\u00b7bersc\u00b7hickt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Entsatzten sich ob dem beginnen/", "tokens": ["Ent\u00b7satz\u00b7ten", "sich", "ob", "dem", "be\u00b7gin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "KOUS", "ART", "VVINF", "$("], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Als sie Charlotten angeblickt.", "tokens": ["Als", "sie", "Char\u00b7lot\u00b7ten", "an\u00b7ge\u00b7blickt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was habt ihr/ alle K\u00f6nigreich/", "tokens": ["Was", "habt", "ihr", "/", "al\u00b7le", "K\u00f6\u00b7nig\u00b7reich", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$(", "PIAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "(sprach iede) dieser F\u00fcrstin gleich?", "tokens": ["(", "sprach", "ie\u00b7de", ")", "die\u00b7ser", "F\u00fcrs\u00b7tin", "gleich", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PIAT", "$(", "PDAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Schickt ihr auch eure K\u00f6niginnen/", "tokens": ["Schickt", "ihr", "auch", "eu\u00b7re", "K\u00f6\u00b7ni\u00b7gin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Jhr werdet dennoch nicht gewinnen.", "tokens": ["Ihr", "wer\u00b7det", "den\u00b7noch", "nicht", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}