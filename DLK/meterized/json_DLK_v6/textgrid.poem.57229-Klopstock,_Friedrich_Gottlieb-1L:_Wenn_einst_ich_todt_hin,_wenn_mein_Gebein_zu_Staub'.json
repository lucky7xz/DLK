{"textgrid.poem.57229": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn einst ich todt hin, wenn mein Gebein zu Staub'", "genre": "verse", "period": "N.A.", "pub_year": 1748, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn einst ich todt hin, wenn mein Gebein zu Staub'", "tokens": ["Wenn", "einst", "ich", "todt", "hin", ",", "wenn", "mein", "Ge\u00b7bein", "zu", "Staub'"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPER", "ADJD", "PTKVZ", "$,", "KOUS", "PPOSAT", "NN", "APPR", "NN"], "meter": "----+-+-+-+", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Ist eingesunken, wenn du, mein Auge, nun", "tokens": ["Ist", "ein\u00b7ge\u00b7sun\u00b7ken", ",", "wenn", "du", ",", "mein", "Au\u00b7ge", ",", "nun"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VAFIN", "VVINF", "$,", "KOUS", "PPER", "$,", "PPOSAT", "NN", "$,", "ADV"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Lang' \u00fcber meines Lebens Schicksal,", "tokens": ["Lang'", "\u00fc\u00b7ber", "mei\u00b7nes", "Le\u00b7bens", "Schick\u00b7sal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Brechend im Tode, nun ausgeweint hast,", "tokens": ["Bre\u00b7chend", "im", "To\u00b7de", ",", "nun", "aus\u00b7ge\u00b7weint", "hast", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$,", "ADV", "VVPP", "VAFIN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Und stillanbetend da, wo die Zukunft ist,", "tokens": ["Und", "stil\u00b7lan\u00b7be\u00b7tend", "da", ",", "wo", "die", "Zu\u00b7kunft", "ist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKVZ", "$,", "PWAV", "ART", "NN", "VAFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nicht mehr hinauf blickst, wenn mein ersungner Ruhm,", "tokens": ["Nicht", "mehr", "hin\u00b7auf", "blickst", ",", "wenn", "mein", "er\u00b7sung\u00b7ner", "Ruhm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "VVFIN", "$,", "KOUS", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die Frucht von meiner J\u00fcnglingsthr\u00e4ne,", "tokens": ["Die", "Frucht", "von", "mei\u00b7ner", "J\u00fcng\u00b7lings\u00b7thr\u00e4\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und von der Liebe zu dir, Messias!", "tokens": ["Und", "von", "der", "Lie\u00b7be", "zu", "dir", ",", "Mes\u00b7si\u00b7as", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "PPER", "$,", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nun auch verweht ist, oder von wenigen", "tokens": ["Nun", "auch", "ver\u00b7weht", "ist", ",", "o\u00b7der", "von", "we\u00b7ni\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "VVPP", "VAFIN", "$,", "KON", "APPR", "PIAT"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In jene Welt hin\u00fcber gerettet ward:", "tokens": ["In", "je\u00b7ne", "Welt", "hin\u00b7\u00fc\u00b7ber", "ge\u00b7ret\u00b7tet", "ward", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wenn du alsdann auch, meine Fanny,", "tokens": ["Wenn", "du", "als\u00b7dann", "auch", ",", "mei\u00b7ne", "Fan\u00b7ny", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Lange schon todt bist, und deines Auges", "tokens": ["Lan\u00b7ge", "schon", "todt", "bist", ",", "und", "dei\u00b7nes", "Au\u00b7ges"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "VAFIN", "$,", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Stillheitres L\u00e4cheln, und sein beseelter Blick", "tokens": ["Still\u00b7heit\u00b7res", "L\u00e4\u00b7cheln", ",", "und", "sein", "be\u00b7seel\u00b7ter", "Blick"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "KON", "PPOSAT", "ADJA", "NN"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Auch ist verloschen, wenn du, vom Volke nicht", "tokens": ["Auch", "ist", "ver\u00b7lo\u00b7schen", ",", "wenn", "du", ",", "vom", "Vol\u00b7ke", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "VVPP", "$,", "KOUS", "PPER", "$,", "APPRART", "NN", "PTKNEG"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Bemerket, deines ganzen Lebens", "tokens": ["Be\u00b7mer\u00b7ket", ",", "dei\u00b7nes", "gan\u00b7zen", "Le\u00b7bens"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Edlere Thaten nunmehr gethan hast,", "tokens": ["Ed\u00b7le\u00b7re", "Tha\u00b7ten", "nun\u00b7mehr", "ge\u00b7than", "hast", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VVPP", "VAFIN", "$,"], "meter": "+--+-++-+-", "measure": "iambic.penta.invert"}}, "stanza.5": {"line.1": {"text": "Des Nachruhms werther, als ein unsterblich Lied,", "tokens": ["Des", "Nach\u00b7ruhms", "wert\u00b7her", ",", "als", "ein", "uns\u00b7terb\u00b7lich", "Lied", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "KOUS", "ART", "ADJD", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ach wenn du dann auch einen begl\u00fcckteren", "tokens": ["Ach", "wenn", "du", "dann", "auch", "ei\u00b7nen", "be\u00b7gl\u00fcck\u00b7te\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "KOUS", "PPER", "ADV", "ADV", "ART", "ADJA"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Als mich geliebt hast, lass den Stolz mir,", "tokens": ["Als", "mich", "ge\u00b7liebt", "hast", ",", "lass", "den", "Stolz", "mir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$,", "VVFIN", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Einen Begl\u00fcckteren, doch nicht edlern!", "tokens": ["Ei\u00b7nen", "Be\u00b7gl\u00fcck\u00b7te\u00b7ren", ",", "doch", "nicht", "ed\u00b7lern", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PTKNEG", "VVINF", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.6": {"line.1": {"text": "Dann wird ein Tag seyn, den werd ich auferstehn!", "tokens": ["Dann", "wird", "ein", "Tag", "seyn", ",", "den", "werd", "ich", "auf\u00b7er\u00b7stehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VAINF", "$,", "ART", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dann wird ein Tag seyn, den wirst du auferstehn!", "tokens": ["Dann", "wird", "ein", "Tag", "seyn", ",", "den", "wirst", "du", "auf\u00b7er\u00b7stehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VAINF", "$,", "ART", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dann trennt kein Schicksal mehr die Seelen,", "tokens": ["Dann", "trennt", "kein", "Schick\u00b7sal", "mehr", "die", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die du einander, Natur, bestimtest.", "tokens": ["Die", "du", "ein\u00b7an\u00b7der", ",", "Na\u00b7tur", ",", "be\u00b7stim\u00b7test", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "PRF", "$,", "NN", "$,", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Dann w\u00e4gt, die Wagschaal in der gehobnen Hand,", "tokens": ["Dann", "w\u00e4gt", ",", "die", "Wag\u00b7schaal", "in", "der", "ge\u00b7hob\u00b7nen", "Hand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Gott Gl\u00fcck und Tugend gegen einander gleich;", "tokens": ["Gott", "Gl\u00fcck", "und", "Tu\u00b7gend", "ge\u00b7gen", "ein\u00b7an\u00b7der", "gleich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "APPR", "PRF", "ADV", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Was in der Dinge Lauf jetzt misklingt,", "tokens": ["Was", "in", "der", "Din\u00b7ge", "Lauf", "jetzt", "mis\u00b7klingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "T\u00f6net in ewigen Harmonicen!", "tokens": ["T\u00f6\u00b7net", "in", "e\u00b7wi\u00b7gen", "Har\u00b7mo\u00b7ni\u00b7cen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.8": {"line.1": {"text": "Wenn dann du dastehst jugendlich auferweckt,", "tokens": ["Wenn", "dann", "du", "das\u00b7tehst", "ju\u00b7gend\u00b7lich", "au\u00b7fer\u00b7weckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Dann eil' ich zu dir! s\u00e4ume nicht, bis mich erst", "tokens": ["Dann", "eil'", "ich", "zu", "dir", "!", "s\u00e4u\u00b7me", "nicht", ",", "bis", "mich", "erst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "$.", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ein Seraph bey der rechten fasse,", "tokens": ["Ein", "Se\u00b7raph", "bey", "der", "rech\u00b7ten", "fas\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und mich, Unsterbliche, zu dir f\u00fchre.", "tokens": ["Und", "mich", ",", "U\u00b7nsterb\u00b7li\u00b7che", ",", "zu", "dir", "f\u00fch\u00b7re", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "NN", "$,", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Dann soll dein Bruder, innig von mir umarmt,", "tokens": ["Dann", "soll", "dein", "Bru\u00b7der", ",", "in\u00b7nig", "von", "mir", "um\u00b7armt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "$,", "ADJD", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Zu dir auch eilen! dann will ich thr\u00e4nenvoll,", "tokens": ["Zu", "dir", "auch", "ei\u00b7len", "!", "dann", "will", "ich", "thr\u00e4\u00b7nen\u00b7voll", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VVFIN", "$.", "ADV", "VMFIN", "PPER", "ADJD", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Voll froher Thr\u00e4nen jenes Lebens", "tokens": ["Voll", "fro\u00b7her", "Thr\u00e4\u00b7nen", "je\u00b7nes", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Neben dir stehn, dich mit Namen nennen,", "tokens": ["Ne\u00b7ben", "dir", "stehn", ",", "dich", "mit", "Na\u00b7men", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVINF", "$,", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "+--+-+-+--", "measure": "iambic.tetra.invert"}}, "stanza.10": {"line.1": {"text": "Und dich umarmen! Dann, o Unsterblichkeit,", "tokens": ["Und", "dich", "um\u00b7ar\u00b7men", "!", "Dann", ",", "o", "U\u00b7nsterb\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "$.", "ADV", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Geh\u00f6rst du ganz uns! Komt, die das Lied nicht singt,", "tokens": ["Ge\u00b7h\u00f6rst", "du", "ganz", "uns", "!", "Komt", ",", "die", "das", "Lied", "nicht", "singt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPER", "$.", "VVFIN", "$,", "PRELS", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Komt, unaussprechlich s\u00fcsse Freuden!", "tokens": ["Komt", ",", "un\u00b7aus\u00b7sprech\u00b7lich", "s\u00fcs\u00b7se", "Freu\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So unaussprechlich, als jetzt mein Schmerz ist.", "tokens": ["So", "un\u00b7aus\u00b7sprech\u00b7lich", ",", "als", "jetzt", "mein", "Schmerz", "ist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "ADV", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.11": {"line.1": {"text": "Rinn unterdess, o Leben. Sie komt gewiss", "tokens": ["Rinn", "un\u00b7ter\u00b7dess", ",", "o", "Le\u00b7ben", ".", "Sie", "komt", "ge\u00b7wiss"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "FM", "NN", "$.", "PPER", "VVFIN", "NE"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Stunde, die uns nach der Zypresse ruft!", "tokens": ["Die", "Stun\u00b7de", ",", "die", "uns", "nach", "der", "Zyp\u00b7res\u00b7se", "ruft", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Ihr andern, seyd der schwermuthsvollen", "tokens": ["Ihr", "an\u00b7dern", ",", "seyd", "der", "schwer\u00b7muths\u00b7vol\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "$,", "VAFIN", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Liebe geweiht! und umw\u00f6lkt und dunkel!", "tokens": ["Lie\u00b7be", "ge\u00b7weiht", "!", "und", "um\u00b7w\u00f6lkt", "und", "dun\u00b7kel", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$.", "KON", "VVFIN", "KON", "ADJD", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}