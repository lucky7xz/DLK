{"textgrid.poem.58078": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "116. Der Weg nach Haus", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Halt stets dein Ziel im Aug'! Es hindert nur im Gehen,", "tokens": ["Halt", "stets", "dein", "Ziel", "im", "Aug'", "!", "Es", "hin\u00b7dert", "nur", "im", "Ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PPOSAT", "NN", "APPRART", "NN", "$.", "PPER", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O Pilger, wenn du viel auf dies und das willst sehen.", "tokens": ["O", "Pil\u00b7ger", ",", "wenn", "du", "viel", "auf", "dies", "und", "das", "willst", "se\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "KOUS", "PPER", "ADV", "APPR", "PDS", "KON", "PDS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Merk, wie du reisen mu\u00dft: aus dir und allem aus", "tokens": ["Merk", ",", "wie", "du", "rei\u00b7sen", "mu\u00dft", ":", "aus", "dir", "und", "al\u00b7lem", "aus"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "PPER", "VVINF", "VMFIN", "$.", "APPR", "PPER", "KON", "PIS", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit deiner ganzen Lieb'! So hei\u00dft der Weg nach Haus.", "tokens": ["Mit", "dei\u00b7ner", "gan\u00b7zen", "Lieb'", "!", "So", "hei\u00dft", "der", "Weg", "nach", "Haus", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$.", "ADV", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}