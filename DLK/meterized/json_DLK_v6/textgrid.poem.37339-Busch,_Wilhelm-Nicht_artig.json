{"textgrid.poem.37339": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Nicht artig", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man ist ja von Natur kein Engel,", "tokens": ["Man", "ist", "ja", "von", "Na\u00b7tur", "kein", "En\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "APPR", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vielmehr ein Welt- und Menschenkind,", "tokens": ["Viel\u00b7mehr", "ein", "Welt", "und", "Men\u00b7schen\u00b7kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und rings umher ist ein Gedr\u00e4ngel", "tokens": ["Und", "rings", "um\u00b7her", "ist", "ein", "Ge\u00b7dr\u00e4n\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PTKVZ", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von solchen, die dasselbe sind.", "tokens": ["Von", "sol\u00b7chen", ",", "die", "das\u00b7sel\u00b7be", "sind", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "$,", "PRELS", "PDAT", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "In diesem Reich geborner Flegel,", "tokens": ["In", "die\u00b7sem", "Reich", "ge\u00b7bor\u00b7ner", "Fle\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wer k\u00f6nnte sich des Lebens freun,", "tokens": ["Wer", "k\u00f6nn\u00b7te", "sich", "des", "Le\u00b7bens", "freun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "W\u00fcrd' es vers\u00e4umt, schon fr\u00fch die Regel", "tokens": ["W\u00fcrd'", "es", "ver\u00b7s\u00e4umt", ",", "schon", "fr\u00fch", "die", "Re\u00b7gel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "ADV", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der R\u00fccksicht kr\u00e4ftig einzubleun.", "tokens": ["Der", "R\u00fcck\u00b7sicht", "kr\u00e4f\u00b7tig", "ein\u00b7zu\u00b7bleun", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es saust der Stock, es schwirrt die Rute.", "tokens": ["Es", "saust", "der", "Stock", ",", "es", "schwirrt", "die", "Ru\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du darfst nicht zeigen, was du bist.", "tokens": ["Du", "darfst", "nicht", "zei\u00b7gen", ",", "was", "du", "bist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$,", "PWS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie schad, o Mensch, da\u00df dir das Gute", "tokens": ["Wie", "schad", ",", "o", "Mensch", ",", "da\u00df", "dir", "das", "Gu\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "$,", "FM", "NN", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Grunde so zuwider ist.", "tokens": ["Im", "Grun\u00b7de", "so", "zu\u00b7wi\u00b7der", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}