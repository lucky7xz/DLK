{"textgrid.poem.67088": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Hymne an die Freiheit", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wonne s\u00e4ng ich an des Orkus Toren,", "tokens": ["Won\u00b7ne", "s\u00e4ng", "ich", "an", "des", "Or\u00b7kus", "To\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und die Schatten lehrt ich Trunkenheit,", "tokens": ["Und", "die", "Schat\u00b7ten", "lehrt", "ich", "Trun\u00b7ken\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Denn ich sah, vor Tausenden erkoren,", "tokens": ["Denn", "ich", "sah", ",", "vor", "Tau\u00b7sen\u00b7den", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Meiner G\u00f6ttin ganze G\u00f6ttlichkeit;", "tokens": ["Mei\u00b7ner", "G\u00f6t\u00b7tin", "gan\u00b7ze", "G\u00f6tt\u00b7lich\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Wie nach dumpfer Nacht im Purpurscheine", "tokens": ["Wie", "nach", "dum\u00b7pfer", "Nacht", "im", "Pur\u00b7pur\u00b7schei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Der Pilote seinen Ozean,", "tokens": ["Der", "Pi\u00b7lo\u00b7te", "sei\u00b7nen", "O\u00b7ze\u00b7an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Wie die Seligen Elysens Haine,", "tokens": ["Wie", "die", "Se\u00b7li\u00b7gen", "E\u00b7ly\u00b7sens", "Hai\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NE", "NE", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Staun ich dich, geliebtes Wunder! an.", "tokens": ["Staun", "ich", "dich", ",", "ge\u00b7lieb\u00b7tes", "Wun\u00b7der", "!", "an", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "ADJA", "NN", "$.", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Ehrerbietig senkten ihre Fl\u00fcgel,", "tokens": ["Ehr\u00b7er\u00b7bie\u00b7tig", "senk\u00b7ten", "ih\u00b7re", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ihres Raubs vergessen, Falk und Aar,", "tokens": ["Ih\u00b7res", "Raubs", "ver\u00b7ges\u00b7sen", ",", "Falk", "und", "Aar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,", "NE", "KON", "NE", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und getreu dem diamantnen Z\u00fcgel", "tokens": ["Und", "ge\u00b7treu", "dem", "di\u00b7a\u00b7mant\u00b7nen", "Z\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Schritt vor ihr ein trotzig L\u00f6wenpaar;", "tokens": ["Schritt", "vor", "ihr", "ein", "trot\u00b7zig", "L\u00f6\u00b7wen\u00b7paar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Jugendliche wilde Str\u00f6me standen,", "tokens": ["Ju\u00b7gend\u00b7li\u00b7che", "wil\u00b7de", "Str\u00f6\u00b7me", "stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Wie mein Herz, vor banger Wonne stumm;", "tokens": ["Wie", "mein", "Herz", ",", "vor", "ban\u00b7ger", "Won\u00b7ne", "stumm", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "$,", "APPR", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Selbst die k\u00fchnen Boreasse schwanden,", "tokens": ["Selbst", "die", "k\u00fch\u00b7nen", "Bo\u00b7re\u00b7as\u00b7se", "schwan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Und die Erde ward zum Heiligtum.", "tokens": ["Und", "die", "Er\u00b7de", "ward", "zum", "Hei\u00b7lig\u00b7tum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Ha! zum Lohne treuer Huldigungen", "tokens": ["Ha", "!", "zum", "Loh\u00b7ne", "treu\u00b7er", "Hul\u00b7di\u00b7gun\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "APPRART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Bot die K\u00f6nigin die Rechte mir,", "tokens": ["Bot", "die", "K\u00f6\u00b7ni\u00b7gin", "die", "Rech\u00b7te", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "PPER", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und von zauberischer Kraft durchdrungen", "tokens": ["Und", "von", "zau\u00b7be\u00b7ri\u00b7scher", "Kraft", "durch\u00b7drun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Jauchzte Sinn und Herz versch\u00f6nert ihr;", "tokens": ["Jauchz\u00b7te", "Sinn", "und", "Herz", "ver\u00b7sch\u00f6\u00b7nert", "ihr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Was sie sprach, die Richterin der Kronen,", "tokens": ["Was", "sie", "sprach", ",", "die", "Rich\u00b7te\u00b7rin", "der", "Kro\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Ewig t\u00f6nts in dieser Seele nach,", "tokens": ["E\u00b7wig", "t\u00f6nts", "in", "die\u00b7ser", "See\u00b7le", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PDAT", "NN", "APPR", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Ewig in der Sch\u00f6pfung Regionen \u2013", "tokens": ["E\u00b7wig", "in", "der", "Sch\u00f6p\u00b7fung", "Re\u00b7gi\u00b7o\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "H\u00f6rt, o Geister, was die Mutter sprach!", "tokens": ["H\u00f6rt", ",", "o", "Geis\u00b7ter", ",", "was", "die", "Mut\u00b7ter", "sprach", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "FM", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "\u00bbtaumelnd in des alten Chaos Wogen,", "tokens": ["\u00bb", "tau\u00b7melnd", "in", "des", "al\u00b7ten", "Chaos", "Wo\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Froh und wild, wie Evans Priesterin,", "tokens": ["Froh", "und", "wild", ",", "wie", "E\u00b7vans", "Pries\u00b7te\u00b7rin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,", "PWAV", "NE", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Von der Jugend k\u00fchner Lust betrogen,", "tokens": ["Von", "der", "Ju\u00b7gend", "k\u00fch\u00b7ner", "Lust", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Nannt ich mich der Freiheit K\u00f6nigin;", "tokens": ["Nannt", "ich", "mich", "der", "Frei\u00b7heit", "K\u00f6\u00b7ni\u00b7gin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Doch es winkte der Vernichtungsstunde", "tokens": ["Doch", "es", "wink\u00b7te", "der", "Ver\u00b7nich\u00b7tungs\u00b7stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Z\u00fcgelloser Elemente Streit;", "tokens": ["Z\u00fc\u00b7gel\u00b7lo\u00b7ser", "E\u00b7le\u00b7men\u00b7te", "Streit", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Da berief zu br\u00fcderlichem Bunde", "tokens": ["Da", "be\u00b7rief", "zu", "br\u00fc\u00b7der\u00b7li\u00b7chem", "Bun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Mein Gesetz die Unerme\u00dflichkeit.", "tokens": ["Mein", "Ge\u00b7setz", "die", "Un\u00b7er\u00b7me\u00df\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Mein Gesetz, es t\u00f6tet zartes Leben,", "tokens": ["Mein", "Ge\u00b7setz", ",", "es", "t\u00f6\u00b7tet", "zar\u00b7tes", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "K\u00fchnen Mut, und bunte Freude nicht,", "tokens": ["K\u00fch\u00b7nen", "Mut", ",", "und", "bun\u00b7te", "Freu\u00b7de", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "ADJA", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Jedem ward der Liebe Recht gegeben,", "tokens": ["Je\u00b7dem", "ward", "der", "Lie\u00b7be", "Recht", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "VAFIN", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Jedes \u00fcbt der Liebe s\u00fc\u00dfe Pflicht;", "tokens": ["Je\u00b7des", "\u00fcbt", "der", "Lie\u00b7be", "s\u00fc\u00b7\u00dfe", "Pflicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Froh und stolz im ungest\u00f6rten Gange", "tokens": ["Froh", "und", "stolz", "im", "un\u00b7ge\u00b7st\u00f6r\u00b7ten", "Gan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Wandelt Riesenkraft die weite Bahn,", "tokens": ["Wan\u00b7delt", "Rie\u00b7sen\u00b7kraft", "die", "wei\u00b7te", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Sicher schmiegt in s\u00fc\u00dfem Liebesdrange", "tokens": ["Si\u00b7cher", "schmiegt", "in", "s\u00fc\u00b7\u00dfem", "Lie\u00b7bes\u00b7dran\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Schw\u00e4cheres der gro\u00dfen Welt sich an.", "tokens": ["Schw\u00e4\u00b7che\u00b7res", "der", "gro\u00b7\u00dfen", "Welt", "sich", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Kann ein Riese meinen Aar entmannen?", "tokens": ["Kann", "ein", "Rie\u00b7se", "mei\u00b7nen", "Aar", "ent\u00b