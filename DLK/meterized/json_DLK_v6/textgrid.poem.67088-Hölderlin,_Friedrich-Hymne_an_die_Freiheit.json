n", "tokens": ["Und", "des", "Va\u00b7ter\u00b7lan\u00b7des", "R\u00e4\u00b7cher", "wal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Im Triumphe nach der bessern Welt.", "tokens": ["Im", "Tri\u00b7um\u00b7phe", "nach", "der", "bes\u00b7sern", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.16": {"line.1": {"text": "Lange schon vom engen Haus umschlossen,", "tokens": ["Lan\u00b7ge", "schon", "vom", "en\u00b7gen", "Haus", "um\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schlummre dann im Frieden mein Gebein! \u2013", "tokens": ["Schlumm\u00b7re", "dann", "im", "Frie\u00b7den", "mein", "Ge\u00b7bein", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Hab ich doch der Hoffnung Kelch genossen,", "tokens": ["Hab", "ich", "doch", "der", "Hoff\u00b7nung", "Kelch", "ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Mich gelabt am holden D\u00e4mmerschein!", "tokens": ["Mich", "ge\u00b7labt", "am", "hol\u00b7den", "D\u00e4m\u00b7mer\u00b7schein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Ha! und dort in wolkenloser Ferne", "tokens": ["Ha", "!", "und", "dort", "in", "wol\u00b7ken\u00b7lo\u00b7ser", "Fer\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "KON", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Winkt auch mir der Freiheit heilig Ziel!", "tokens": ["Winkt", "auch", "mir", "der", "Frei\u00b7heit", "hei\u00b7lig", "Ziel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "ART", "NN", "ADJD", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Dort, mit euch, ihr k\u00f6niglichen Sterne,", "tokens": ["Dort", ",", "mit", "euch", ",", "ihr", "k\u00f6\u00b7nig\u00b7li\u00b7chen", "Ster\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Klinge festlicher mein Saitenspiel!", "tokens": ["Klin\u00b7ge", "fest\u00b7li\u00b7cher", "mein", "Sai\u00b7ten\u00b7spiel", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}