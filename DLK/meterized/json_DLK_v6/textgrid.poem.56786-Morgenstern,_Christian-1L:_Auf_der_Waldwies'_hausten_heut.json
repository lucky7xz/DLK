{"textgrid.poem.56786": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf der Waldwies' hausten heut", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf der Waldwies' hausten heut", "tokens": ["Auf", "der", "Wald\u00b7wies'", "haus\u00b7ten", "heut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "sonderbare Br\u00fcder,", "tokens": ["son\u00b7der\u00b7ba\u00b7re", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "sangen, sprangen um die Wett'", "tokens": ["san\u00b7gen", ",", "spran\u00b7gen", "um", "die", "Wett'"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "zu eines Alten Fiedel, \u2013", "tokens": ["zu", "ei\u00b7nes", "Al\u00b7ten", "Fie\u00b7del", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Goldfuchs, rund wie ein Banquier,", "tokens": ["Gold\u00b7fuchs", ",", "rund", "wie", "ein", "Ban\u00b7qui\u00b7er", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00fcrze, zart und wei\u00df wie Schnee,", "tokens": ["Sch\u00fcr\u00b7ze", ",", "zart", "und", "wei\u00df", "wie", "Schnee", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "VVFIN", "KOKOM", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Flasche, grau wie Asche.", "tokens": ["Fla\u00b7sche", ",", "grau", "wie", "A\u00b7sche", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KOKOM", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Sang der Goldfuchs:", "tokens": ["Sang", "der", "Gold\u00b7fuchs", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Alles dreht", "tokens": ["Al\u00b7les", "dreht"], "token_info": ["word", "word"], "pos": ["PIS", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "sich um mich", "tokens": ["sich", "um", "mich"], "token_info": ["word", "word", "word"], "pos": ["PRF", "APPR", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "fr\u00fch und sp\u00e4t,", "tokens": ["fr\u00fch", "und", "sp\u00e4t", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "rum didl dum,", "tokens": ["rum", "didl", "dum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "rum didl dauz,", "tokens": ["rum", "didl", "dauz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "bum bum bum,", "tokens": ["bum", "bum", "bum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["XY", "XY", "XY", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "bauz!", "tokens": ["bauz", "!"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-", "measure": "single.down"}}, "stanza.4": {"line.1": {"text": "Sang die Sch\u00fcrze:", "tokens": ["Sang", "die", "Sch\u00fcr\u00b7ze", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Alles dreht", "tokens": ["Al\u00b7les", "dreht"], "token_info": ["word", "word"], "pos": ["PIS", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "sich um mich", "tokens": ["sich", "um", "mich"], "token_info": ["word", "word", "word"], "pos": ["PRF", "APPR", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "fr\u00fch und sp\u00e4t,", "tokens": ["fr\u00fch", "und", "sp\u00e4t", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "rum didl dum,", "tokens": ["rum", "didl", "dum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "rum didl dauz,", "tokens": ["rum", "didl", "dauz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "bum bum bum,", "tokens": ["bum", "bum", "bum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["XY", "XY", "XY", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "bauz!", "tokens": ["bauz", "!"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-", "measure": "single.down"}}, "stanza.5": {"line.1": {"text": "Sang die Flasche:", "tokens": ["Sang", "die", "Fla\u00b7sche", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Alles dreht", "tokens": ["Al\u00b7les", "dreht"], "token_info": ["word", "word"], "pos": ["PIS", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "sich um mich", "tokens": ["sich", "um", "mich"], "token_info": ["word", "word", "word"], "pos": ["PRF", "APPR", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "fr\u00fch und sp\u00e4t,", "tokens": ["fr\u00fch", "und", "sp\u00e4t", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "rum didl dum,", "tokens": ["rum", "didl", "dum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "rum didl dauz,", "tokens": ["rum", "didl", "dauz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "bum bum bum,", "tokens": ["bum", "bum", "bum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["XY", "XY", "XY", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "bauz!", "tokens": ["bauz", "!"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-", "measure": "single.down"}}, "stanza.6": {"line.1": {"text": "Warf der Alt' die Fiedel weg,", "tokens": ["Warf", "der", "Alt'", "die", "Fie\u00b7del", "weg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "kriegt' den Fuchs zu fassen,", "tokens": ["kriegt'", "den", "Fuchs", "zu", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NE", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "schickt' ihn wie 'nen Schlitterstein", "tokens": ["schickt'", "ihn", "wie", "'nen", "Schlit\u00b7ter\u00b7stein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "KOKOM", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "weit hinaus aufs Wasser,", "tokens": ["weit", "hin\u00b7aus", "aufs", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "griff die Sch\u00fcrze, stopfte sie", "tokens": ["griff", "die", "Sch\u00fcr\u00b7ze", ",", "stopf\u00b7te", "sie"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "zwischen Ripp' und G\u00fcrtel,", "tokens": ["zwi\u00b7schen", "Ripp'", "und", "G\u00fcr\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "schmi\u00df die leere Flasch' zu Boden,", "tokens": ["schmi\u00df", "die", "lee\u00b7re", "Fla\u00b7sch'", "zu", "Bo\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "da\u00df sie gell zerklirrte ...", "tokens": ["da\u00df", "sie", "gell", "zer\u00b7klirr\u00b7te", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Wandte sich, das Buschwerk schlug", "tokens": ["Wand\u00b7te", "sich", ",", "das", "Busc\u00b7hwerk", "schlug"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PRF", "$,", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "hinter ihm zusammen,", "tokens": ["hin\u00b7ter", "ihm", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "aber lang noch h\u00f6rt' man ihn", "tokens": ["a\u00b7ber", "lang", "noch", "h\u00f6rt'", "man", "ihn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADV", "VVFIN", "PIS", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "fernher brummen:", "tokens": ["fern\u00b7her", "brum\u00b7men", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Alles dreht", "tokens": ["Al\u00b7les", "dreht"], "token_info": ["word", "word"], "pos": ["PIS", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "sich um mich", "tokens": ["sich", "um", "mich"], "token_info": ["word", "word", "word"], "pos": ["PRF", "APPR", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "fr\u00fch und sp\u00e4t,", "tokens": ["fr\u00fch", "und", "sp\u00e4t", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "rum didl dum,", "tokens": ["rum", "didl", "dum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "rum didl dauz,", "tokens": ["rum", "didl", "dauz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.10": {"text": "bum bum bum,", "tokens": ["bum", "bum", "bum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["XY", "XY", "XY", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "bauz!", "tokens": ["bauz", "!"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-", "measure": "single.down"}}}}}