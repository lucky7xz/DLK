{"textgrid.poem.50628": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Neujahrsw\u00fcnsche", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es spricht Marcolphus mir am neuen Jahrstag zu", "tokens": ["Es", "spricht", "Mar\u00b7col\u00b7phus", "mir", "am", "neu\u00b7en", "Jahr\u00b7stag", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NE", "PPER", "APPRART", "ADJA", "NN", "PTKZU"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und w\u00fcnscht mir, was er mir entwendet", "tokens": ["Und", "w\u00fcnscht", "mir", ",", "was", "er", "mir", "ent\u00b7wen\u00b7det"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "PWS", "PPER", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er w\u00fcnscht mir lange Jahr' und raubt mir meine Zeit,", "tokens": ["Er", "w\u00fcnscht", "mir", "lan\u00b7ge", "Jahr'", "und", "raubt", "mir", "mei\u00b7ne", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "NN", "KON", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Veranla\u00dft mich zum Fluch und w\u00fcnscht mir Seligkeit;", "tokens": ["Ver\u00b7an\u00b7la\u00dft", "mich", "zum", "Fluch", "und", "w\u00fcnscht", "mir", "Se\u00b7lig\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "KON", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "In seinem Antlitz sitzt das Merkmal b\u00f6ser Tage,", "tokens": ["In", "sei\u00b7nem", "Ant\u00b7litz", "sitzt", "das", "Merk\u00b7mal", "b\u00f6\u00b7ser", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und sein ", "tokens": ["Und", "sein"], "token_info": ["word", "word"], "pos": ["KON", "PPOSAT"], "meter": "-+", "measure": "iambic.single"}}}}}