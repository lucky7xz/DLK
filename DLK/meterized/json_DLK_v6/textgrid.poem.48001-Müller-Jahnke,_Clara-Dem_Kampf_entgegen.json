{"textgrid.poem.48001": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Dem Kampf entgegen", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So schlaf in Frieden, armes Lamm . . .", "tokens": ["So", "schlaf", "in", "Frie\u00b7den", ",", "ar\u00b7mes", "Lamm", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$,", "ADJA", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00df einsam mich auf steinigen Wegen", "tokens": ["La\u00df", "ein\u00b7sam", "mich", "auf", "stei\u00b7ni\u00b7gen", "We\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "im Stra\u00dfenstaube f\u00fcrba\u00df ziehn", "tokens": ["im", "Stra\u00b7\u00dfen\u00b7stau\u00b7be", "f\u00fcr\u00b7ba\u00df", "ziehn"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "des Tages gro\u00dfem Kampf entgegen.", "tokens": ["des", "Ta\u00b7ges", "gro\u00b7\u00dfem", "Kampf", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es geht ein Brausen durch die Luft", "tokens": ["Es", "geht", "ein", "Brau\u00b7sen", "durch", "die", "Luft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "wie eines starken Sturmes Wehen:", "tokens": ["wie", "ei\u00b7nes", "star\u00b7ken", "Stur\u00b7mes", "We\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Aus Tr\u00fcmmern tausendj\u00e4hrigen Wahns", "tokens": ["Aus", "Tr\u00fcm\u00b7mern", "tau\u00b7send\u00b7j\u00e4h\u00b7ri\u00b7gen", "Wahns"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "will eine neue Welt erstehen.", "tokens": ["will", "ei\u00b7ne", "neu\u00b7e", "Welt", "er\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Des Wertes Wage schwankt nicht mehr,", "tokens": ["Des", "Wer\u00b7tes", "Wa\u00b7ge", "schwankt", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "schon neigt sich tief die goldene Schale \u2013", "tokens": ["schon", "neigt", "sich", "tief", "die", "gol\u00b7de\u00b7ne", "Scha\u00b7le", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADJD", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "des neuen Glaubens M\u00e4rtyrer", "tokens": ["des", "neu\u00b7en", "Glau\u00b7bens", "M\u00e4r\u00b7ty\u00b7rer"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "empfingen ihre Wundenmale.", "tokens": ["emp\u00b7fin\u00b7gen", "ih\u00b7re", "Wun\u00b7den\u00b7ma\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "In deinem Tempel knieen sie,", "tokens": ["In", "dei\u00b7nem", "Tem\u00b7pel", "kni\u00b7e\u00b7en", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "von Schmerz durchloht und edlem Grimme,", "tokens": ["von", "Schmerz", "durch\u00b7loht", "und", "ed\u00b7lem", "Grim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "du dreimal heilige Natur,", "tokens": ["du", "drei\u00b7mal", "hei\u00b7li\u00b7ge", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und h\u00f6ren der Verhei\u00dfung Stimme:", "tokens": ["und", "h\u00f6\u00b7ren", "der", "Ver\u00b7hei\u00b7\u00dfung", "Stim\u00b7me", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bbraum hat die Erde allerw\u00e4rts,", "tokens": ["\u00bb", "raum", "hat", "die", "Er\u00b7de", "al\u00b7ler\u00b7w\u00e4rts", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "der Himmel Luft f\u00fcr Millionen \u2013", "tokens": ["der", "Him\u00b7mel", "Luft", "f\u00fcr", "Mil\u00b7lion\u00b7en", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "der Aermste soll auf eigenem Grund", "tokens": ["der", "A\u00b7erms\u00b7te", "soll", "auf", "ei\u00b7ge\u00b7nem", "Grund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "APPR", "ADJA", "NN"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "im Schatten seines Daches wohnen!", "tokens": ["im", "Schat\u00b7ten", "sei\u00b7nes", "Da\u00b7ches", "woh\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und trinken soll mit vollem Zug,", "tokens": ["Und", "trin\u00b7ken", "soll", "mit", "vol\u00b7lem", "Zug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "wer nach dem Born der Wahrheit d\u00fcrstet, \u2013", "tokens": ["wer", "nach", "dem", "Born", "der", "Wahr\u00b7heit", "d\u00fcrs\u00b7tet", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "und wem der Geist die Krone reicht,", "tokens": ["und", "wem", "der", "Geist", "die", "Kro\u00b7ne", "reicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "die g\u00f6ttliche, der sei gef\u00fcrstet!", "tokens": ["die", "g\u00f6tt\u00b7li\u00b7che", ",", "der", "sei", "ge\u00b7f\u00fcrs\u00b7tet", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Fortan soll keine Mutter mehr", "tokens": ["For\u00b7tan", "soll", "kei\u00b7ne", "Mut\u00b7ter", "mehr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "PIAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "ihr Kind in tausendfachen Schmerzen", "tokens": ["ihr", "Kind", "in", "tau\u00b7send\u00b7fa\u00b7chen", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "verleugnen m\u00fcssen, das sie tr\u00e4gt", "tokens": ["ver\u00b7leug\u00b7nen", "m\u00fcs\u00b7sen", ",", "das", "sie", "tr\u00e4gt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVINF", "VMINF", "$,", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "in heiligster Liebe unterm Herzen.", "tokens": ["in", "hei\u00b7ligs\u00b7ter", "Lie\u00b7be", "un\u00b7term", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Das reine Antlitz der Natur,", "tokens": ["Das", "rei\u00b7ne", "Ant\u00b7litz", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "wer wagt, mit Schmach es zu bewerfen? \u2013", "tokens": ["wer", "wagt", ",", "mit", "Schmach", "es", "zu", "be\u00b7wer\u00b7fen", "?", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "$,", "APPR", "NN", "PPER", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das Schwert der siegenden Vernunft,", "tokens": ["Das", "Schwert", "der", "sie\u00b7gen\u00b7den", "Ver\u00b7nunft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "zum letzten Kampfe sollt ihr's sch\u00e4rfen! \u2013\u00ab", "tokens": ["zum", "letz\u00b7ten", "Kamp\u00b7fe", "sollt", "ih\u00b7r's", "sch\u00e4r\u00b7fen", "!", "\u2013", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPRART", "ADJA", "NN", "VMFIN", "PIS", "VVINF", "$.", "$(", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Und gl\u00fchend st\u00fcrmen sie zum Streit,", "tokens": ["Und", "gl\u00fc\u00b7hend", "st\u00fcr\u00b7men", "sie", "zum", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "laut gellend schreit die Schlachttrompete, \u2013", "tokens": ["laut", "gel\u00b7lend", "schreit", "die", "Schlacht\u00b7trom\u00b7pe\u00b7te", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "VVPP", "VVFIN", "ART", "NN", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "hoch \u00fcber ihren H\u00e4uptern flammt", "tokens": ["hoch", "\u00fc\u00b7ber", "ih\u00b7ren", "H\u00e4up\u00b7tern", "flammt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "des neuen Tages Morgenr\u00f6te.", "tokens": ["des", "neu\u00b7en", "Ta\u00b7ges", "Mor\u00b7gen\u00b7r\u00f6\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Aus Ketten schmieden sie den Stahl,", "tokens": ["Aus", "Ket\u00b7ten", "schmie\u00b7den", "sie", "den", "Stahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "von Herzblut rot die Banner wehen . . .", "tokens": ["von", "Herz\u00b7blut", "rot", "die", "Ban\u00b7ner", "we\u00b7hen", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "NN", "ADJD", "ART", "NN", "VVFIN", "$.", "$.", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mich aber la\u00dft mit nackter Brust", "tokens": ["Mich", "a\u00b7ber", "la\u00dft", "mit", "nack\u00b7ter", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "in ihren ersten Reihen stehen!", "tokens": ["in", "ih\u00b7ren", "ers\u00b7ten", "Rei\u00b7hen", "ste\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}