{"textgrid.poem.43317": {"metadata": {"author": {"name": "Gerstenberg, Heinrich Wilhelm von", "birth": "N.A.", "death": "N.A."}, "title": "1L: An jungen misgerathnen St\u00f6cken,", "genre": "verse", "period": "N.A.", "pub_year": 1780, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An jungen misgerathnen St\u00f6cken,", "tokens": ["An", "jun\u00b7gen", "mis\u00b7ge\u00b7rath\u00b7nen", "St\u00f6\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und noch von keiner Traube schwer,", "tokens": ["Und", "noch", "von", "kei\u00b7ner", "Trau\u00b7be", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Geht Bacchus wirthschaftlich daher,", "tokens": ["Geht", "Bac\u00b7chus", "wirth\u00b7schaft\u00b7lich", "da\u00b7her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ADJD", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sucht itzt ein Auge zu verstecken,", "tokens": ["Sucht", "itzt", "ein", "Au\u00b7ge", "zu", "ver\u00b7ste\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Itzt eins den Stralen aufzudecken,", "tokens": ["Itzt", "eins", "den", "Stra\u00b7len", "auf\u00b7zu\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Arbeitet, knickt, zerrei\u00dft und bricht,", "tokens": ["Ar\u00b7bei\u00b7tet", ",", "knickt", ",", "zer\u00b7rei\u00dft", "und", "bricht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und keicht, und schont der H\u00e4nde nicht.", "tokens": ["Und", "keicht", ",", "und", "schont", "der", "H\u00e4n\u00b7de", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KON", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der kleine Gott, der sonst nur trank,", "tokens": ["Der", "klei\u00b7ne", "Gott", ",", "der", "sonst", "nur", "trank", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sonst nur vom Rausch zu Boden sank,", "tokens": ["Sonst", "nur", "vom", "Rausch", "zu", "Bo\u00b7den", "sank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sinkt itzt vor Amors Pfeil zu Boden,", "tokens": ["Sinkt", "itzt", "vor", "A\u00b7mors", "Pfeil", "zu", "Bo\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NE", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und geistig Blut trieft in den Boden;", "tokens": ["Und", "geis\u00b7tig", "Blut", "trieft", "in", "den", "Bo\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und Bacchus weint, da er die Wunde f\u00fchlt,", "tokens": ["Und", "Bac\u00b7chus", "weint", ",", "da", "er", "die", "Wun\u00b7de", "f\u00fchlt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df Amor ihm so mitgespielt.", "tokens": ["Da\u00df", "A\u00b7mor", "ihm", "so", "mit\u00b7ge\u00b7spielt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Von diesem Most, den ich noch keinem zugebracht,", "tokens": ["Von", "die\u00b7sem", "Most", ",", "den", "ich", "noch", "kei\u00b7nem", "zu\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "PPER", "ADV", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df er ihn nicht gleich f\u00fchlbarer gemacht,", "tokens": ["Da\u00df", "er", "ihn", "nicht", "gleich", "f\u00fchl\u00b7ba\u00b7rer", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von diesem Moste will ich trinken.", "tokens": ["Von", "die\u00b7sem", "Mos\u00b7te", "will", "ich", "trin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "O Phillis! siehst du ihn nicht winken?", "tokens": ["O", "Phil\u00b7lis", "!", "siehst", "du", "ihn", "nicht", "win\u00b7ken", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "VVFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "O liebe Phillis, la\u00df uns trinken!", "tokens": ["O", "lie\u00b7be", "Phil\u00b7lis", ",", "la\u00df", "uns", "trin\u00b7ken", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NE", "$,", "VVIMP", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}