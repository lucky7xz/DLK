{"textgrid.poem.56687": {"metadata": {"author": {"name": "Sack, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Einsam", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe niemals Du zu euch gesagt \u2013", "tokens": ["Ich", "ha\u00b7be", "nie\u00b7mals", "Du", "zu", "euch", "ge\u00b7sagt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPER", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "wohin ich kam und wen ich immer sprach", "tokens": ["wo\u00b7hin", "ich", "kam", "und", "wen", "ich", "im\u00b7mer", "sprach"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "KON", "PWS", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und wenn er auch in meinen Armen lag,", "tokens": ["und", "wenn", "er", "auch", "in", "mei\u00b7nen", "Ar\u00b7men", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "ich habe niemals Du zu ihm gesagt.", "tokens": ["ich", "ha\u00b7be", "nie\u00b7mals", "Du", "zu", "ihm", "ge\u00b7sagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und doch hab ich mich nie darob beklagt", "tokens": ["Und", "doch", "hab", "ich", "mich", "nie", "da\u00b7rob", "be\u00b7klagt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "PRF", "ADV", "PAV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und wenn die Sehnsucht auch mit wildem Schlag", "tokens": ["und", "wenn", "die", "Sehn\u00b7sucht", "auch", "mit", "wil\u00b7dem", "Schlag"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "mein selbstgew\u00e4hltes Klausnertum durchbrach,", "tokens": ["mein", "selbst\u00b7ge\u00b7w\u00e4hl\u00b7tes", "Klaus\u00b7ner\u00b7tum", "durch\u00b7brach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "hab ich doch niemals Du zu euch gesagt", "tokens": ["hab", "ich", "doch", "nie\u00b7mals", "Du", "zu", "euch", "ge\u00b7sagt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "PPER", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "und hatte immerdar an mir genug,", "tokens": ["und", "hat\u00b7te", "im\u00b7mer\u00b7dar", "an", "mir", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "APPR", "PPER", "ADV", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "berg ich doch ewig in mir Gott und Tier", "tokens": ["berg", "ich", "doch", "e\u00b7wig", "in", "mir", "Gott", "und", "Tier"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "APPR", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und Licht und Kot und heiligste Begier,", "tokens": ["und", "Licht", "und", "Kot", "und", "hei\u00b7ligs\u00b7te", "Be\u00b7gier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "und hielt mein stilles Zwiegespr\u00e4ch mit mir,", "tokens": ["und", "hielt", "mein", "stil\u00b7les", "Zwie\u00b7ge\u00b7spr\u00e4ch", "mit", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "bis \u00fcber mir gleich einem Totentuch", "tokens": ["bis", "\u00fc\u00b7ber", "mir", "gleich", "ei\u00b7nem", "To\u00b7ten\u00b7tuch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "der Schrei der Einsamkeit zusammenschlug.", "tokens": ["der", "Schrei", "der", "Ein\u00b7sam\u00b7keit", "zu\u00b7sam\u00b7men\u00b7schlug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}