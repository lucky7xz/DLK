{"dta.poem.11589": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "An   Friedr. Vischer", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Oft hat mich der Freund vertheidigt,", "tokens": ["Oft", "hat", "mich", "der", "Freund", "ver\u00b7thei\u00b7digt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Oft sogar gelobt, doch nun?", "tokens": ["Oft", "so\u00b7gar", "ge\u00b7lobt", ",", "doch", "nun", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$,", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der Professor ist beeidigt", "tokens": ["Der", "Pro\u00b7fes\u00b7sor", "ist", "be\u00b7ei\u00b7digt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und da hilft kein Traulich-thun.", "tokens": ["Und", "da", "hilft", "kein", "Trau\u00b7lich\u00b7thun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Also geht, ihr braven Lieder,", "tokens": ["Al\u00b7so", "geht", ",", "ihr", "bra\u00b7ven", "Lie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df man euch die K\u00f6pfe wascht!", "tokens": ["Da\u00df", "man", "euch", "die", "K\u00f6p\u00b7fe", "wascht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seht auch, da\u00df ihr hin und wieder", "tokens": ["Seht", "auch", ",", "da\u00df", "ihr", "hin", "und", "wie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "KOUS", "PPER", "PTKVZ", "KON", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einen guten Blick erhascht.", "tokens": ["Ei\u00b7nen", "gu\u00b7ten", "Blick", "er\u00b7hascht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Er ist Vater: um so minder", "tokens": ["Er", "ist", "Va\u00b7ter", ":", "um", "so", "min\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "$.", "APPR", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denk' ich ihn euch abgeneigt;", "tokens": ["Denk'", "ich", "ihn", "euch", "ab\u00b7ge\u00b7neigt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sind doch seine eignen Kinder", "tokens": ["Sind", "doch", "sei\u00b7ne", "eig\u00b7nen", "Kin\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf der Schulbank nicht gezeugt!", "tokens": ["Auf", "der", "Schul\u00b7bank", "nicht", "ge\u00b7zeugt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}