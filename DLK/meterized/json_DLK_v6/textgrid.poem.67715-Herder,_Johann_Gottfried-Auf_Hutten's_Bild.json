{"textgrid.poem.67715": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Auf Hutten's Bild", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du streckst die Nase weidlich vor;", "tokens": ["Du", "streckst", "die", "Na\u00b7se", "weid\u00b7lich", "vor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das wird Dir schlecht bekommen, Thor!", "tokens": ["Das", "wird", "Dir", "schlecht", "be\u00b7kom\u00b7men", ",", "Thor", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADJD", "VVINF", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer stets will ha'n das Schwert zur Hand,", "tokens": ["Wer", "stets", "will", "ha'n", "das", "Schwert", "zur", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VMFIN", "NE", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der wird bald Ritter ohne Land.", "tokens": ["Der", "wird", "bald", "Rit\u00b7ter", "oh\u00b7ne", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}