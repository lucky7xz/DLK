{"textgrid.poem.41269": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "An den Marschall von Frankreich, Grafen von S.", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gemeiner Tugenden kann nur ein Held entrathen:", "tokens": ["Ge\u00b7mei\u00b7ner", "Tu\u00b7gen\u00b7den", "kann", "nur", "ein", "Held", "ent\u00b7ra\u00b7then", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VMFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Glanz von seinem Ruhm strahlt aus erhabnen Thaten,", "tokens": ["Der", "Glanz", "von", "sei\u00b7nem", "Ruhm", "strahlt", "aus", "er\u00b7hab\u00b7nen", "Tha\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Aus dem, was andern schwer und unerreichlich f\u00e4llt.", "tokens": ["Aus", "dem", ",", "was", "an\u00b7dern", "schwer", "und", "un\u00b7er\u00b7reich\u00b7lich", "f\u00e4llt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "PIS", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Niedern m\u00fcssen sich ein leichtres Lob erlesen;", "tokens": ["Die", "Nie\u00b7dern", "m\u00fcs\u00b7sen", "sich", "ein", "leicht\u00b7res", "Lob", "er\u00b7le\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch Scipio verbleibt ein Held,", "tokens": ["Doch", "Sci\u00b7pio", "ver\u00b7bleibt", "ein", "Held", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "W\u00e4r' er in Spanien auch nicht so keusch gewesen.", "tokens": ["W\u00e4r'", "er", "in", "Spa\u00b7ni\u00b7en", "auch", "nicht", "so", "keusch", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NE", "ADV", "PTKNEG", "ADV", "ADJD", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}