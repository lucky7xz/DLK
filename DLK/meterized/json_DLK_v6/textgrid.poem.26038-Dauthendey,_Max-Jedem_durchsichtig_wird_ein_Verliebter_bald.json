{"textgrid.poem.26038": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Jedem durchsichtig wird ein Verliebter bald", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist wie ein Apfel mit Feuer bemalt,", "tokens": ["Du", "bist", "wie", "ein", "Ap\u00b7fel", "mit", "Feu\u00b7er", "be\u00b7malt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Bringst mir Gl\u00fcck wie das Vierblatt im Klee;", "tokens": ["Bringst", "mir", "Gl\u00fcck", "wie", "das", "Vier\u00b7blatt", "im", "Klee", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KOKOM", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Bist mir hoch wie der Wolke hohe Gestalt,", "tokens": ["Bist", "mir", "hoch", "wie", "der", "Wol\u00b7ke", "ho\u00b7he", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "KOKOM", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Nie vermi\u00df ich die Sonne in deiner N\u00e4h'.", "tokens": ["Nie", "ver\u00b7mi\u00df", "ich", "die", "Son\u00b7ne", "in", "dei\u00b7ner", "N\u00e4h'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}}, "stanza.2": {"line.1": {"text": "Kein Schlaf will mir Verliebtem mehr taugen,", "tokens": ["Kein", "Schlaf", "will", "mir", "Ver\u00b7lieb\u00b7tem", "mehr", "tau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Bin wach wie die Fliege mit tausend Augen;", "tokens": ["Bin", "wach", "wie", "die", "Flie\u00b7ge", "mit", "tau\u00b7send", "Au\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KOKOM", "ART", "NN", "APPR", "CARD", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "H\u00f6r' Tauben gurren in jedem Wald,", "tokens": ["H\u00f6r'", "Tau\u00b7ben", "gur\u00b7ren", "in", "je\u00b7dem", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und das Herz f\u00e4llt mir schwach in das Gras;", "tokens": ["Und", "das", "Herz", "f\u00e4llt", "mir", "schwach", "in", "das", "Gras", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Jedem durchsichtig wird ein Verliebter bald", "tokens": ["Je\u00b7dem", "durch\u00b7sich\u00b7tig", "wird", "ein", "Ver\u00b7lieb\u00b7ter", "bald"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJD", "VAFIN", "ART", "NN", "ADV"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "Und zerbrechlich wie wimmerndes Glas.", "tokens": ["Und", "zer\u00b7brech\u00b7lich", "wie", "wim\u00b7mern\u00b7des", "Glas", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", 