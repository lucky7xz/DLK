{"textgrid.poem.24666": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "1L: F\u00fchlst Du durch die Winternacht", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fchlst Du durch die Winternacht", "tokens": ["F\u00fchlst", "Du", "durch", "die", "Win\u00b7ter\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch der kalten Sternlein Zittern", "tokens": ["Durch", "der", "kal\u00b7ten", "Stern\u00b7lein", "Zit\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch der Eiskristalle Pracht", "tokens": ["Durch", "der", "Eis\u00b7kris\u00b7tal\u00b7le", "Pracht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie sie flimmern und zersplittern,", "tokens": ["Wie", "sie", "flim\u00b7mern", "und", "zer\u00b7split\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "KON", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "F\u00fchlst nicht nahen laue Mahnung,", "tokens": ["F\u00fchlst", "nicht", "na\u00b7hen", "lau\u00b7e", "Mah\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Keimen leise Fr\u00fchlingsahnung?", "tokens": ["Kei\u00b7men", "lei\u00b7se", "Fr\u00fch\u00b7lin\u00b7gsah\u00b7nung", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Drunten schl\u00e4ft der Fr\u00fchlingsmorgen", "tokens": ["Drun\u00b7ten", "schl\u00e4ft", "der", "Fr\u00fch\u00b7lings\u00b7mor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Quillt in g\u00e4hrenden Gewalten", "tokens": ["Quillt", "in", "g\u00e4h\u00b7ren\u00b7den", "Ge\u00b7wal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und, ob heute noch verborgen,", "tokens": ["Und", ",", "ob", "heu\u00b7te", "noch", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "ADV", "ADV", "VVPP", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Sprengt er rings das Eis in Spalten:", "tokens": ["Sprengt", "er", "rings", "das", "Eis", "in", "Spal\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und in wirbelnd lauem Wehen", "tokens": ["Und", "in", "wir\u00b7belnd", "lau\u00b7em", "We\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Braust er denen, die's verstehen.", "tokens": ["Braust", "er", "de\u00b7nen", ",", "die's", "ver\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "$,", "PDS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "H\u00f6rst Du aus der Worte Hall,", "tokens": ["H\u00f6rst", "Du", "aus", "der", "Wor\u00b7te", "Hall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie sie k\u00fchn und trotzig klettern", "tokens": ["Wie", "sie", "k\u00fchn", "und", "trot\u00b7zig", "klet\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADJD", "KON", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mit jugendlichem Prall", "tokens": ["Und", "mit", "ju\u00b7gend\u00b7li\u00b7chem", "Prall"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Klirrend eine Welt zerschmettern:", "tokens": ["Klir\u00b7rend", "ei\u00b7ne", "Welt", "zer\u00b7schmet\u00b7tern", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "H\u00f6rst Du nicht die leise Mahnung,", "tokens": ["H\u00f6rst", "Du", "nicht", "die", "lei\u00b7se", "Mah\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Warmen Lebensfr\u00fchlings Ahnung?", "tokens": ["War\u00b7men", "Le\u00b7bens\u00b7fr\u00fch\u00b7lings", "Ah\u00b7nung", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}