{"textgrid.poem.39850": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df du bei Sinnenglut und Witzesgaben", "tokens": ["Da\u00df", "du", "bei", "Sin\u00b7nen\u00b7glut", "und", "Wit\u00b7zes\u00b7ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Phantasie, in fremder Form Gewand,", "tokens": ["Und", "Phan\u00b7ta\u00b7sie", ",", "in", "frem\u00b7der", "Form", "Ge\u00b7wand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Entbehren glaubst zu k\u00f6nnen den Verstand,", "tokens": ["Ent\u00b7beh\u00b7ren", "glaubst", "zu", "k\u00f6n\u00b7nen", "den", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKZU", "VMINF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und Geist, Beth\u00f6rter, w\u00e4hnest schon zu haben,", "tokens": ["Und", "Geist", ",", "Be\u00b7th\u00f6r\u00b7ter", ",", "w\u00e4h\u00b7nest", "schon", "zu", "ha\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "NN", "$,", "VVFIN", "ADV", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mein Hohn wird sich daran f\u00fcrwahr nicht laben;", "tokens": ["Mein", "Hohn", "wird", "sich", "da\u00b7ran", "f\u00fcr\u00b7wahr", "nicht", "la\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PRF", "PAV", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mich dauert's, wenn ein anvertrautes Pfand", "tokens": ["Mich", "dau\u00b7ert's", ",", "wenn", "ein", "an\u00b7ver\u00b7trau\u00b7tes", "Pfand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Verschleudert wird in Uebermut und Tand,", "tokens": ["Ver\u00b7schleu\u00b7dert", "wird", "in", "Ue\u00b7ber\u00b7mut", "und", "Tand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und wenn ein junges Ro\u00df zu Tod' will traben.", "tokens": ["Und", "wenn", "ein", "jun\u00b7ges", "Ro\u00df", "zu", "Tod'", "will", "tra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "APPR", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Auch hoff' ich's ja, du lernst dereinst erkennen", "tokens": ["Auch", "hoff'", "ich's", "ja", ",", "du", "lernst", "de\u00b7reinst", "er\u00b7ken\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "$,", "PPER", "VVFIN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Schranke deiner Kraft und wirst, geschult,", "tokens": ["Die", "Schran\u00b7ke", "dei\u00b7ner", "Kraft", "und", "wirst", ",", "ge\u00b7schult", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "KON", "VAFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit Lust und Ernst auf rechter Bahn dich treiben:", "tokens": ["Mit", "Lust", "und", "Ernst", "auf", "rech\u00b7ter", "Bahn", "dich", "trei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch Andrer Freundschaft kann f\u00fcr dich entbrennen,", "tokens": ["Doch", "A\u00b7ndrer", "Freund\u00b7schaft", "kann", "f\u00fcr", "dich", "ent\u00b7bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VMFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erst wenn dein Ich nicht mit sich selbst mehr buhlt:", "tokens": ["Erst", "wenn", "dein", "Ich", "nicht", "mit", "sich", "selbst", "mehr", "buhlt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPOSAT", "PPER", "PTKNEG", "APPR", "PRF", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So lang la\u00df uns geschiedne Leute bleiben.", "tokens": ["So", "lang", "la\u00df", "uns", "ge\u00b7schied\u00b7ne", "Leu\u00b7te", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVIMP", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}