{"textgrid.poem.56329": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Der neue Diogenes", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was pressen sich die dichten Massen", "tokens": ["Was", "pres\u00b7sen", "sich", "die", "dich\u00b7ten", "Mas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Volkes in den engen Raum?", "tokens": ["Des", "Vol\u00b7kes", "in", "den", "en\u00b7gen", "Raum", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es fassen, Amiens, deine Stra\u00dfen", "tokens": ["Es", "fas\u00b7sen", ",", "A\u00b7miens", ",", "dei\u00b7ne", "Stra\u00b7\u00dfen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "VVINF", "$,", "NE", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das wogende Gedr\u00e4nge kaum. \u2013", "tokens": ["Das", "wo\u00b7gen\u00b7de", "Ge\u00b7dr\u00e4n\u00b7ge", "kaum", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$.", "$("], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.5": {"text": "Der Kaiser naht, der Herr der Welt;", "tokens": ["Der", "Kai\u00b7ser", "naht", ",", "der", "Herr", "der", "Welt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hebt Siegeslieder an zu singen!", "tokens": ["Hebt", "Sie\u00b7ges\u00b7lie\u00b7der", "an", "zu", "sin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APZR", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Er hat der Feinde Macht zerschellt,", "tokens": ["Er", "hat", "der", "Fein\u00b7de", "Macht", "zer\u00b7schellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Er naht, den Seinen Heil zu bringen! \u2013", "tokens": ["Er", "naht", ",", "den", "Sei\u00b7nen", "Heil", "zu", "brin\u00b7gen", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "PPOSAT", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Freudenrausch, der sich ergossen,", "tokens": ["Der", "Freu\u00b7den\u00b7rausch", ",", "der", "sich", "er\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er l\u00e4\u00dft den Einen unber\u00fchrt:", "tokens": ["Er", "l\u00e4\u00dft", "den", "Ei\u00b7nen", "un\u00b7be\u00b7r\u00fchrt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ART", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Steinmetz ist's, der unverdrossen", "tokens": ["Ein", "Stein\u00b7metz", "ist's", ",", "der", "un\u00b7ver\u00b7dros\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VAFIN", "$,", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Mei\u00dfel und den Hammer f\u00fchrt;", "tokens": ["Den", "Mei\u00b7\u00dfel", "und", "den", "Ham\u00b7mer", "f\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der l\u00e4\u00dft den Zug vor\u00fcbergehn", "tokens": ["Der", "l\u00e4\u00dft", "den", "Zug", "vor\u00b7\u00fc\u00b7ber\u00b7gehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und nicht im Tagewerk sich st\u00f6ren,", "tokens": ["Und", "nicht", "im", "Ta\u00b7ge\u00b7werk", "sich", "st\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPRART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Als hab er Augen nicht, zu sehn,", "tokens": ["Als", "hab", "er", "Au\u00b7gen", "nicht", ",", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "NN", "PTKNEG", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Als hab er Ohren nicht, zu h\u00f6ren.", "tokens": ["Als", "hab", "er", "Oh\u00b7ren", "nicht", ",", "zu", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "NN", "PTKNEG", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Vom Ro\u00df herab bemerkt von ferne", "tokens": ["Vom", "Ro\u00df", "her\u00b7ab", "be\u00b7merkt", "von", "fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "VVPP", "APPR", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Kaiser dort den r\u00fcst'gen Mann;", "tokens": ["Der", "Kai\u00b7ser", "dort", "den", "r\u00fcst'\u00b7gen", "Mann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es reizt ihn, da\u00df er kennen lerne,", "tokens": ["Es", "reizt", "ihn", ",", "da\u00df", "er", "ken\u00b7nen", "ler\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wer so von ihm sich sondern kann.", "tokens": ["Wer", "so", "von", "ihm", "sich", "son\u00b7dern", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "PPER", "PRF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er hat sich ihm genaht, er fragt:", "tokens": ["Er", "hat", "sich", "ihm", "ge\u00b7naht", ",", "er", "fragt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "PPER", "VVPP", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u00bbwas schaffst du da?\u00ab \u2013 \u00bbDen Stein behauen!\u00ab", "tokens": ["\u00bb", "was", "schaffst", "du", "da", "?", "\u00ab", "\u2013", "\u00bb", "Den", "Stein", "be\u00b7hau\u00b7en", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "ADV", "$.", "$(", "$(", "$(", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Entgegnet der, und wie er's sagt,", "tokens": ["Ent\u00b7geg\u00b7net", "der", ",", "und", "wie", "er's", "sagt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "$,", "KON", "PWAV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Er kann ihm scharf ins Antlitz schauen.", "tokens": ["Er", "kann", "ihm", "scharf", "ins", "Ant\u00b7litz", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbich sah dich bei den Pyramiden,", "tokens": ["\u00bb", "ich", "sah", "dich", "bei", "den", "Py\u00b7ra\u00b7mi\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du schlugst dich gut, du warst Sergeant;", "tokens": ["Du", "schlugst", "dich", "gut", ",", "du", "warst", "Ser\u00b7ge\u00b7ant", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,", "PPER", "VAFIN", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wie kam's, da\u00df du den Dienst gemieden,", "tokens": ["Wie", "kam's", ",", "da\u00df", "du", "den", "Dienst", "ge\u00b7mie\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vergessen hier und unbekannt?\u00ab", "tokens": ["Ver\u00b7ges\u00b7sen", "hier", "und", "un\u00b7be\u00b7kannt", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ADV", "KON", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u00bbich habe meine Schuldigkeit", "tokens": ["\u00bb", "ich", "ha\u00b7be", "mei\u00b7ne", "Schul\u00b7dig\u00b7keit"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Getan, o Herr, zu allen Stunden,", "tokens": ["Ge\u00b7tan", ",", "o", "Herr", ",", "zu", "al\u00b7len", "Stun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "FM", "NN", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und ward nach ausgedienter Zeit", "tokens": ["Und", "ward", "nach", "aus\u00b7ge\u00b7dien\u00b7ter", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Von Eid und Kriegespflicht entbunden!\u00ab \u2013", "tokens": ["Von", "Eid", "und", "Krie\u00b7ge\u00b7spflicht", "ent\u00b7bun\u00b7den", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$.", "$(", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bbes tut mir leid, im Heer zu missen,", "tokens": ["\u00bb", "es", "tut", "mir", "leid", ",", "im", "Heer", "zu", "mis\u00b7sen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ADJD", "$,", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wer brav sich hielt im Kriegeslauf;", "tokens": ["Wer", "brav", "sich", "hielt", "im", "Krie\u00b7ges\u00b7lauf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "PRF", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00df deinen k\u00fchnsten Wunsch mich wissen,", "tokens": ["La\u00df", "dei\u00b7nen", "k\u00fchns\u00b7ten", "Wunsch", "mich", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Kaisers Gnade sucht dich auf!\u00ab \u2013", "tokens": ["Des", "Kai\u00b7sers", "Gna\u00b7de", "sucht", "dich", "auf", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "PTKVZ", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u00bbich brauche nichts, die H\u00e4nde mein", "tokens": ["\u00bb", "ich", "brau\u00b7che", "nichts", ",", "die", "H\u00e4n\u00b7de", "mein"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "PIS", "$,", "ART", "NN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gen\u00fcgen noch, mich zu ern\u00e4hren;", "tokens": ["Ge\u00b7n\u00fc\u00b7gen", "noch", ",", "mich", "zu", "er\u00b7n\u00e4h\u00b7ren", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "La\u00df mich behauen meinen Stein,", "tokens": ["La\u00df", "mich", "be\u00b7hau\u00b7en", "mei\u00b7nen", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und deiner Gnade nicht begehren.\u00ab", "tokens": ["Und", "dei\u00b7ner", "Gna\u00b7de", "nicht", "be\u00b7geh\u00b7ren", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKNEG", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}