{"dta.poem.13565": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die zwei Gesellen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Es zogen zwei r\u00fcst'ge Gesellen", "tokens": ["Es", "zo\u00b7gen", "zwei", "r\u00fcst'\u00b7ge", "Ge\u00b7sel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "CARD", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Zum ersten Mal von Haus,", "tokens": ["Zum", "ers\u00b7ten", "Mal", "von", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So jubelnd recht in die hellen", "tokens": ["So", "ju\u00b7belnd", "recht", "in", "die", "hel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADJD", "APPR", "ART", "ADJA"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Klingenden, singenden Wellen", "tokens": ["Klin\u00b7gen\u00b7den", ",", "sin\u00b7gen\u00b7den", "Wel\u00b7len"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Des vollen Fr\u00fchlings hinaus.", "tokens": ["Des", "vol\u00b7len", "Fr\u00fch\u00b7lings", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Die strebten nach hohen Dingen,", "tokens": ["Die", "streb\u00b7ten", "nach", "ho\u00b7hen", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die wollten, trotz Lust und Schmerz,", "tokens": ["Die", "woll\u00b7ten", ",", "trotz", "Lust", "und", "Schmerz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Was Rechts in der Welt vollbringen,", "tokens": ["Was", "Rechts", "in", "der", "Welt", "voll\u00b7brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und wem sie vor\u00fcber gingen,", "tokens": ["Und", "wem", "sie", "vor\u00b7\u00fc\u00b7ber", "gin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Dem lachten Sinnen und Herz. \u2014", "tokens": ["Dem", "lach\u00b7ten", "Sin\u00b7nen", "und", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Der Erste, der fand ein Liebchen,", "tokens": ["Der", "Ers\u00b7te", ",", "der", "fand", "ein", "Lieb\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die Schwieger kauft' Hof und Haus;", "tokens": ["Die", "Schwie\u00b7ger", "kauft'", "Hof", "und", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der wiegte gar bald ein B\u00fcbchen,", "tokens": ["Der", "wieg\u00b7te", "gar", "bald", "ein", "B\u00fcb\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und sah aus heimlichem St\u00fcbchen", "tokens": ["Und", "sah", "aus", "heim\u00b7li\u00b7chem", "St\u00fcb\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Behaglich in's Feld hinaus.", "tokens": ["Be\u00b7hag\u00b7lich", "in's", "Feld", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Dem Zweiten sangen und logen", "tokens": ["Dem", "Zwei\u00b7ten", "san\u00b7gen", "und", "lo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die tausend Stimmen im Grund,", "tokens": ["Die", "tau\u00b7send", "Stim\u00b7men", "im", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Verlockend' Syrenen, und zogen", "tokens": ["Ver\u00b7lo\u00b7ckend'", "Sy\u00b7re\u00b7nen", ",", "und", "zo\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "NE", "$,", "KON", "VVFIN"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Ihn in der buhlenden Wogen", "tokens": ["Ihn", "in", "der", "buh\u00b7len\u00b7den", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Farbig klingenden Schlund.", "tokens": ["Far\u00b7big", "klin\u00b7gen\u00b7den", "Schlund", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Und wie er auftaucht' vom Schlunde,", "tokens": ["Und", "wie", "er", "auf\u00b7taucht'", "vom", "Schlun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+----+-", "measure": "dactylic.init"}, "line.2": {"text": "Da war er m\u00fcde und alt,", "tokens": ["Da", "war", "er", "m\u00fc\u00b7de", "und", "alt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Sein Schifflein das lag im Grunde,", "tokens": ["Sein", "Schif\u00b7flein", "das", "lag", "im", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PDS", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "So still war's rings in die Runde", "tokens": ["So", "still", "wa\u00b7r's", "rings", "in", "die", "Run\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Und \u00fcber die Wasser weht's kalt.", "tokens": ["Und", "\u00fc\u00b7ber", "die", "Was\u00b7ser", "weht's", "kalt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Es singen und klingen die Wellen", "tokens": ["Es", "sin\u00b7gen", "und", "klin\u00b7gen", "die", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Des Fr\u00fchlings wohl \u00fcber mir;", "tokens": ["Des", "Fr\u00fch\u00b7lings", "wohl", "\u00fc\u00b7ber", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und seh' ich so kecke Gesellen,", "tokens": ["Und", "seh'", "ich", "so", "ke\u00b7cke", "Ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die Thr\u00e4nen im Auge mir schwellen \u2014", "tokens": ["Die", "Thr\u00e4\u00b7nen", "im", "Au\u00b7ge", "mir", "schwel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PPER", "VVINF", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Ach Gott, f\u00fchr' uns liebreich zu Dir!", "tokens": ["Ach", "Gott", ",", "f\u00fchr'", "uns", "lieb\u00b7reich", "zu", "Dir", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "VVFIN", "PPER", "ADJD", "APPR", "PPER", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}