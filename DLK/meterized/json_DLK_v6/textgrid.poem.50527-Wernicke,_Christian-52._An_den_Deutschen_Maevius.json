{"textgrid.poem.50527": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "52. An den Deutschen Maevius", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freund hast du ", "tokens": ["Freund", "hast", "du"], "token_info": ["word", "word", "word"], "pos": ["NN", "VAFIN", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Dass dem ", "tokens": ["Dass", "dem"], "token_info": ["word", "word"], "pos": ["KOUS", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "So habe ", "tokens": ["So", "ha\u00b7be"], "token_info": ["word", "word"], "pos": ["ADV", "VAFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Und schimpf' ein ", "tokens": ["Und", "schimpf'", "ein"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "ART"], "meter": "+--", "measure": "dactylic.init"}, "line.5": {"text": "Lass offt ", "tokens": ["Lass", "offt"], "token_info": ["word", "word"], "pos": ["NE", "ADV"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Sprich was die ", "tokens": ["Sprich", "was", "die"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "PWS", "ART"], "meter": "+--", "measure": "dactylic.init"}, "line.8": {"text": "Und dein ", "tokens": ["Und", "dein"], "token_info": ["word", "word"], "pos": ["KON", "PPOSAT"], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Sprich einem ", "tokens": ["Sprich", "ei\u00b7nem"], "token_info": ["word", "word"], "pos": ["VVIMP", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.11": {"text": "Gib einem ", "tokens": ["Gib", "ei\u00b7nem"], "token_info": ["word", "word"], "pos": ["VVIMP", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.12": {"text": "Damit es scheinen m\u00f6g' als \u2013 \u2013 Ja, ", "tokens": ["Da\u00b7mit", "es", "schei\u00b7nen", "m\u00f6g'", "als", "\u2013", "\u2013", "Ja", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "KOKOM", "$(", "$(", "PTKANT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Such' eine ", "tokens": ["Such'", "ei\u00b7ne"], "token_info": ["word", "word"], "pos": ["VVFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "Und zeige, wie ", "tokens": ["Und", "zei\u00b7ge", ",", "wie"], "token_info": ["word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "$,", "PWAV"], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "Du siehst mein Raht ist gutt, und plagt die", "tokens": ["Du", "siehst", "mein", "Raht", "ist", "gutt", ",", "und", "plagt", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "VAFIN", "ADJD", "$,", "KON", "VVFIN", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "So tadle wo du wilst, ", "tokens": ["So", "tad\u00b7le", "wo", "du", "wilst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PWAV", "PPER", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}