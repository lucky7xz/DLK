{"textgrid.poem.66487": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Den Waldweg abw\u00e4rts, von den Felsenstufen", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den Waldweg abw\u00e4rts, von den Felsenstufen", "tokens": ["Den", "Wald\u00b7weg", "ab\u00b7w\u00e4rts", ",", "von", "den", "Fel\u00b7sen\u00b7stu\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In Spr\u00fcngen, tollk\u00fchn \u00fcber Steinkolosse,", "tokens": ["In", "Spr\u00fcn\u00b7gen", ",", "toll\u00b7k\u00fchn", "\u00fc\u00b7ber", "Stein\u00b7ko\u00b7los\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kam's mit Gel\u00e4chter, Jauchzen und mit Rufen.", "tokens": ["Kam's", "mit", "Ge\u00b7l\u00e4ch\u00b7ter", ",", "Jauch\u00b7zen", "und", "mit", "Ru\u00b7fen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "$,", "NN", "KON", "APPR", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Die junge Schaar, wie ungez\u00e4hmte Rosse,", "tokens": ["Die", "jun\u00b7ge", "Schaar", ",", "wie", "un\u00b7ge\u00b7z\u00e4hm\u00b7te", "Ros\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gab sich, am Sommertag kathederfl\u00fcchtig,", "tokens": ["Gab", "sich", ",", "am", "Som\u00b7mer\u00b7tag", "ka\u00b7the\u00b7der\u00b7fl\u00fcch\u00b7tig", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit Lust der halsgef\u00e4hrlich wilden Posse.", "tokens": ["Mit", "Lust", "der", "hals\u00b7ge\u00b7f\u00e4hr\u00b7lich", "wil\u00b7den", "Pos\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Zum Wagni\u00df auf einander eifers\u00fcchtig,", "tokens": ["Zum", "Wag\u00b7ni\u00df", "auf", "ein\u00b7an\u00b7der", "ei\u00b7fer\u00b7s\u00fcch\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit Schwung und Kraft, erwiesen Bein' und Glieder,", "tokens": ["Mit", "Schwung", "und", "Kraft", ",", "er\u00b7wie\u00b7sen", "Bein'", "und", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vom Gl\u00fcck beg\u00fcnstigt, sich doch heil und t\u00fcchtig.", "tokens": ["Vom", "Gl\u00fcck", "be\u00b7g\u00fcns\u00b7tigt", ",", "sich", "doch", "heil", "und", "t\u00fcch\u00b7tig", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "PRF", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und als vom tollen Weg die letzten nieder", "tokens": ["Und", "als", "vom", "tol\u00b7len", "Weg", "die", "letz\u00b7ten", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPRART", "ADJA", "NN", "ART", "ADJA", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Getobt, sucht man mit ungeheurem Lachen", "tokens": ["Ge\u00b7tobt", ",", "sucht", "man", "mit", "un\u00b7ge\u00b7heu\u00b7rem", "La\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "VVFIN", "PIS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu ordnen das verschobene Gefieder.", "tokens": ["Zu", "ord\u00b7nen", "das", "ver\u00b7scho\u00b7be\u00b7ne", "Ge\u00b7fie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und in des Jubels festlichem Erwachen", "tokens": ["Und", "in", "des", "Ju\u00b7bels", "fest\u00b7li\u00b7chem", "Er\u00b7wa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.2": {"text": "Nach Epheuranken greift ein blonder Springer,", "tokens": ["Nach", "E\u00b7pheu\u00b7ran\u00b7ken", "greift", "ein", "blon\u00b7der", "Sprin\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit einem Kranz sein H\u00fctlein sch\u00f6n zu machen.", "tokens": ["Mit", "ei\u00b7nem", "Kranz", "sein", "H\u00fct\u00b7lein", "sch\u00f6n", "zu", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Mit Epheuranken bald sind alle Finger", "tokens": ["Mit", "E\u00b7pheu\u00b7ran\u00b7ken", "bald", "sind", "al\u00b7le", "Fin\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gesch\u00e4ftig, und mit Kr\u00e4nzen ist phantastisch", "tokens": ["Ge\u00b7sch\u00e4f\u00b7tig", ",", "und", "mit", "Kr\u00e4n\u00b7zen", "ist", "phan\u00b7tas\u00b7tisch"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "KON", "APPR", "NN", "VAFIN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Man auch gestimmt zum Ruf der Thyrsusschwinger.", "tokens": ["Man", "auch", "ge\u00b7stimmt", "zum", "Ruf", "der", "Thyr\u00b7sus\u00b7schwin\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VVPP", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "In T\u00f6nen, gleich verwegen als elastisch,", "tokens": ["In", "T\u00f6\u00b7nen", ",", "gleich", "ver\u00b7we\u00b7gen", "als", "e\u00b7las\u00b7tisch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADV", "ADJD", "KOKOM", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Evo\u00eb Bacchos! schallt's aus Kehl' und Lippen,", "tokens": ["E\u00b7vo\u00eb", "Bac\u00b7chos", "!", "schallt's", "aus", "Kehl'", "und", "Lip\u00b7pen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und neues Wagen hebt sich an gymnastisch.", "tokens": ["Und", "neu\u00b7es", "Wa\u00b7gen", "hebt", "sich", "an", "gym\u00b7nas\u00b7tisch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PRF", "APPR", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Ein Felsenblock, mit Moos und Wurzelrippen", "tokens": ["Ein", "Fel\u00b7sen\u00b7block", ",", "mit", "Moos", "und", "Wur\u00b7zel\u00b7rip\u00b7pen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Buchenstamms bedeckt, der dr\u00fcber thronte,", "tokens": ["Des", "Bu\u00b7chen\u00b7stamms", "be\u00b7deckt", ",", "der", "dr\u00fc\u00b7ber", "thron\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PRELS", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ward ausersehn, zum Trotz der steilen Klippen.", "tokens": ["Ward", "au\u00b7ser\u00b7sehn", ",", "zum", "Trotz", "der", "stei\u00b7len", "Klip\u00b7pen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVINF", "$,", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Hinauf! Als ob ein K\u00f6nigreich es lohnte,", "tokens": ["Hin\u00b7auf", "!", "Als", "ob", "ein", "K\u00f6\u00b7nig\u00b7reich", "es", "lohn\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "KOUS", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Begiebt ein Klettern sich, ein Rutschen, Ringen,", "tokens": ["Be\u00b7giebt", "ein", "Klet\u00b7tern", "sich", ",", "ein", "Rut\u00b7schen", ",", "Rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PRF", "$,", "ART", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das weder Kraft noch Sommerkleider schonte.", "tokens": ["Das", "we\u00b7der", "Kraft", "noch", "Som\u00b7mer\u00b7klei\u00b7der", "schon\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "KON", "NN", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Genommen wird der Posten. Und mit Singen,", "tokens": ["Ge\u00b7nom\u00b7men", "wird", "der", "Pos\u00b7ten", ".", "Und", "mit", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$.", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Noch halb des Athems bar, mu\u00df in die Runde", "tokens": ["Noch", "halb", "des", "A\u00b7thems", "bar", ",", "mu\u00df", "in", "die", "Run\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "NN", "ADJD", "$,", "VMFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Siegestanz noch um den Baum gelingen.", "tokens": ["Der", "Sie\u00b7ge\u00b7stanz", "noch", "um", "den", "Baum", "ge\u00b7lin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Evo\u00eb! gellt's hinab zum Thalesgrunde,", "tokens": ["E\u00b7vo\u00eb", "!", "gellt's", "hin\u00b7ab", "zum", "Tha\u00b7les\u00b7grun\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NE", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und Sonnenstrahlen durch der Wipfel Lichtung", "tokens": ["Und", "Son\u00b7nen\u00b7strah\u00b7len", "durch", "der", "Wip\u00b7fel", "Lich\u00b7tung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Schau'n lachend nieder auf das Fest der Stunde.", "tokens": ["Schau'n", "la\u00b7chend", "nie\u00b7der", "auf", "das", "Fest", "der", "Stun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PTKVZ", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Dann schwindet von der H\u00f6h' in andrer Richtung", "tokens": ["Dann", "schwin\u00b7det", "von", "der", "H\u00f6h'", "in", "an\u00b7drer", "Rich\u00b7tung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Schaar, das Evo\u00eb verklingt, gestaltet", "tokens": ["Die", "Schaar", ",", "das", "E\u00b7vo\u00eb", "ver\u00b7klingt", ",", "ge\u00b7stal\u00b7tet"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "PRELS", "NE", "VVFIN", "$,", "VVPP"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Zum Juvivallera moderner Dichtung. \u2013", "tokens": ["Zum", "Ju\u00b7vi\u00b7val\u00b7le\u00b7ra", "mo\u00b7der\u00b7ner", "Dich\u00b7tung", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NE", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.13": {"line.1": {"text": "Der aber achtsam, und noch unerkaltet", "tokens": ["Der", "a\u00b7ber", "acht\u00b7sam", ",", "und", "noch", "un\u00b7er\u00b7kal\u00b7tet"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADV", "ADJD", "$,", "KON", "ADV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fcr solchen Waldesspuk, ihn angesehen,", "tokens": ["F\u00fcr", "sol\u00b7chen", "Wal\u00b7des\u00b7spuk", ",", "ihn", "an\u00b7ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Mann sah mehr noch seinem Sinn entfaltet.", "tokens": ["Der", "Mann", "sah", "mehr", "noch", "sei\u00b7nem", "Sinn", "ent\u00b7fal\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.14": {"line.1": {"text": "Vom Boden hob er im Vor\u00fcbergehen", "tokens": ["Vom", "Bo\u00b7den", "hob", "er", "im", "Vor\u00b7\u00fc\u00b7ber\u00b7ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Epheuranken eine, die, zum Kranze", "tokens": ["Der", "E\u00b7pheu\u00b7ran\u00b7ken", "ei\u00b7ne", ",", "die", ",", "zum", "Kran\u00b7ze"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ART", "$,", "PRELS", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gef\u00fcgt, verloren lief im Windeswehen.", "tokens": ["Ge\u00b7f\u00fcgt", ",", "ver\u00b7lo\u00b7ren", "lief", "im", "Win\u00b7des\u00b7we\u00b7hen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.15": {"line.1": {"text": "Du hast gedient, so dacht' er, bei dem Tanze", "tokens": ["Du", "hast", "ge\u00b7dient", ",", "so", "dacht'", "er", ",", "bei", "dem", "Tan\u00b7ze"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "ADV", "VVFIN", "PPER", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der wilden Kraft heut, wie ein paar Jahrtausend", "tokens": ["Der", "wil\u00b7den", "Kraft", "heut", ",", "wie", "ein", "paar", "Jahr\u00b7tau\u00b7send"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "$,", "PWAV", "ART", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zuvor schon die Geschlechter deiner Pflanze!", "tokens": ["Zu\u00b7vor", "schon", "die", "Ge\u00b7schlech\u00b7ter", "dei\u00b7ner", "Pflan\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.16": {"line.1": {"text": "Sein Haupt mit deinem starken Laub umkrausend", "tokens": ["Sein", "Haupt", "mit", "dei\u00b7nem", "star\u00b7ken", "Laub", "um\u00b7krau\u00b7send"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erschien Ly\u00e4us selbst, kam \u00fcberm\u00fcthig", "tokens": ["Er\u00b7schien", "Ly\u00b7\u00e4us", "selbst", ",", "kam", "\u00fc\u00b7ber\u00b7m\u00fct\u00b7hig"], "token_info": ["word", "