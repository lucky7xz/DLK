{"dta.poem.2817": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "18.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Leben ist ein solch unsch\u00e4tzbar Gut, mein Kind,", "tokens": ["Das", "Le\u00b7ben", "ist", "ein", "solch", "un\u00b7sch\u00e4tz\u00b7bar", "Gut", ",", "mein", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "PIAT", "ADJD", "ADJD", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Weil alle G\u00fcter mit darin begriffen sind.", "tokens": ["Weil", "al\u00b7le", "G\u00fc\u00b7ter", "mit", "da\u00b7rin", "be\u00b7grif\u00b7fen", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "APPR", "PAV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Denn Theil an allen hat, wer Theil am Leben nimmt,", "tokens": ["Denn", "Theil", "an", "al\u00b7len", "hat", ",", "wer", "Theil", "am", "Le\u00b7ben", "nimmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PIS", "VAFIN", "$,", "PWS", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob ihm ein gr\u00f6\u00dfrer Theil, ein kleinrer sei bestimmt.", "tokens": ["Ob", "ihm", "ein", "gr\u00f6\u00df\u00b7rer", "Theil", ",", "ein", "klein\u00b7rer", "sei", "be\u00b7stimmt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Des Ganzen Mitgef\u00fchl ist ganz im kleinsten Theil,", "tokens": ["Des", "Gan\u00b7zen", "Mit\u00b7ge\u00b7f\u00fchl", "ist", "ganz", "im", "kleins\u00b7ten", "Theil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dein besondres Gl\u00fcck das allgemeine Heil,", "tokens": ["Und", "dein", "be\u00b7sond\u00b7res", "Gl\u00fcck", "das", "all\u00b7ge\u00b7mei\u00b7ne", "Heil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Zu f\u00fchlen rings um dich, stets aus sich selbst erneut,", "tokens": ["Zu", "f\u00fch\u00b7len", "rings", "um", "dich", ",", "stets", "aus", "sich", "selbst", "er\u00b7neut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADV", "APPR", "PPER", "$,", "ADV", "APPR", "PRF", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Leben tausendfach, das sich des Lebens freut.", "tokens": ["Ein", "Le\u00b7ben", "tau\u00b7send\u00b7fach", ",", "das", "sich", "des", "Le\u00b7bens", "freut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wer dieses lebhaft f\u00fchlt in jedem Augenblick,", "tokens": ["Wer", "die\u00b7ses", "leb\u00b7haft", "f\u00fchlt", "in", "je\u00b7dem", "Au\u00b7gen\u00b7blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "ADJD", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dankt f\u00fcr sein Leben Gott und segnet sein Geschick.", "tokens": ["Dankt", "f\u00fcr", "sein", "Le\u00b7ben", "Gott", "und", "seg\u00b7net", "sein", "Ge\u00b7schick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "NN", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}