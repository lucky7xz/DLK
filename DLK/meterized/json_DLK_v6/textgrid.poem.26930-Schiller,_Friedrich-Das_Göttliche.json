{"textgrid.poem.26930": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Das G\u00f6ttliche", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4re sie unverwelklich, die Sch\u00f6nheit, ihr k\u00f6nnte nichts gleichen,", "tokens": ["W\u00e4\u00b7re", "sie", "un\u00b7ver\u00b7wel\u00b7klich", ",", "die", "Sch\u00f6n\u00b7heit", ",", "ihr", "k\u00f6nn\u00b7te", "nichts", "glei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "ART", "NN", "$,", "PPER", "VMFIN", "PIS", "VVINF", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Nichts, wo die G\u00f6ttliche bl\u00fcht, wei\u00df ich der G\u00f6ttlichen gleich.", "tokens": ["Nichts", ",", "wo", "die", "G\u00f6tt\u00b7li\u00b7che", "bl\u00fcht", ",", "wei\u00df", "ich", "der", "G\u00f6tt\u00b7li\u00b7chen", "gleich", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PWAV", "ART", "NN", "VVFIN", "$,", "VVFIN", "PPER", "ART", "NN", "ADV", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Ein Unendliches ahndet, ein H\u00f6chstes erschafft die Vernunft sich,", "tokens": ["Ein", "Un\u00b7end\u00b7li\u00b7ches", "ahn\u00b7det", ",", "ein", "H\u00f6chs\u00b7tes", "er\u00b7schafft", "die", "Ver\u00b7nunft", "sich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$,", "ART", "NN", "VVFIN", "ART", "NN", "PRF", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "In der sch\u00f6nen Gestalt lebt es dem Herzen, dem Blick.", "tokens": ["In", "der", "sch\u00f6\u00b7nen", "Ge\u00b7stalt", "lebt", "es", "dem", "Her\u00b7zen", ",", "dem", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}}}}}