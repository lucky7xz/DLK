{"dta.poem.11604": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "An den Schlaf.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Schlaf! sanfter Schlaf! obwohl dem Tod wie du Nichts", "tokens": ["Schlaf", "!", "sanf\u00b7ter", "Schlaf", "!", "ob\u00b7wohl", "dem", "Tod", "wie", "du", "Nichts"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "ADJA", "NN", "$.", "KOUS", "ART", "NN", "KOKOM", "PPER", "PIS"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Komm! theilen wir dies Lager br\u00fcderlich!", "tokens": ["Komm", "!", "thei\u00b7len", "wir", "dies", "La\u00b7ger", "br\u00fc\u00b7der\u00b7lich", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "VVFIN", "PPER", "PDS", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So ohne Leben, ach wie lieblich lebt es sich!", "tokens": ["So", "oh\u00b7ne", "Le\u00b7ben", ",", "ach", "wie", "lieb\u00b7lich", "lebt", "es", "sich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "ADV", "KOKOM", "ADJD", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So ohne Tod, wie stirbt es sich so leicht!", "tokens": ["So", "oh\u00b7ne", "Tod", ",", "wie", "stirbt", "es", "sich", "so", "leicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "PWAV", "VVFIN", "PPER", "PRF", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}