{"dta.poem.11053": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die neue Art durch Connexiones  \n der Schl\u00fcsse zu \u00fcberf\u00fchren, eben so  \n wohl widersprechlich.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Aneinanderhang der Schl\u00fcsse, wovon wir so viel\nWunder-Sachen,", "tokens": ["Ein", "An\u00b7ein\u00b7an\u00b7der\u00b7hang", "der", "Schl\u00fcs\u00b7se", ",", "wo\u00b7von", "wir", "so", "viel", "Wun\u00b7der\u00b7Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PWAV", "PPER", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+--+---+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Und ein unwidersprechlich Mittel, die Wahrheit zu erfinden,", "tokens": ["Und", "ein", "un\u00b7wi\u00b7der\u00b7sprech\u00b7lich", "Mit\u00b7tel", ",", "die", "Wahr\u00b7heit", "zu", "er\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJD", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "--+--+-+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Ist lange nicht so unbetrieglich, als wie es etwan anfangs", "tokens": ["Ist", "lan\u00b7ge", "nicht", "so", "un\u00b7be\u00b7trieg\u00b7lich", ",", "als", "wie", "es", "et\u00b7wan", "an\u00b7fangs"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PTKNEG", "ADV", "ADJD", "$,", "KOUS", "PWAV", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Und wie man nach der neuen Art, die jetzo Mode worden, meynt.", "tokens": ["Und", "wie", "man", "nach", "der", "neu\u00b7en", "Art", ",", "die", "jet\u00b7zo", "Mo\u00b7de", "wor\u00b7den", ",", "meynt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PWAV", "PIS", "APPR", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "NN", "VAPP", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.5": {"text": "Ein Beyspiel wird es deutlich zeigen. Zween Menschen sol-", "tokens": ["Ein", "Bey\u00b7spiel", "wird", "es", "deut\u00b7lich", "zei\u00b7gen", ".", "Zween", "Men\u00b7schen", "sol"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "VVINF", "$.", "NN", "NN", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Dasselbige Principium zu ihrem Grundsatz anzunehmen;", "tokens": ["Das\u00b7sel\u00b7bi\u00b7ge", "Prin\u00b7ci\u00b7pium", "zu", "ih\u00b7rem", "Grund\u00b7satz", "an\u00b7zu\u00b7neh\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPOSAT", "NN", "VVIZU", "$."], "meter": "-+--+-+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.7": {"text": "Sie sollen beyde b\u00fcndig schliessen; sie sollen beyde sich bem\u00fchn,", "tokens": ["Sie", "sol\u00b7len", "bey\u00b7de", "b\u00fcn\u00b7dig", "schlies\u00b7sen", ";", "sie", "sol\u00b7len", "bey\u00b7de", "sich", "be\u00b7m\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "ADJD", "VVINF", "$.", "PPER", "VMFIN", "PIS", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.8": {"text": "Und eine lange feste Kette, von Schl\u00fcssen, die vern\u00fcnftig, ziehn.", "tokens": ["Und", "ei\u00b7ne", "lan\u00b7ge", "fes\u00b7te", "Ket\u00b7te", ",", "von", "Schl\u00fcs\u00b7sen", ",", "die", "ver\u00b7n\u00fcnf\u00b7tig", ",", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "ADV", "ADJA", "NN", "$,", "APPR", "NN", "$,", "PRELS", "ADJD", "$,", "VVINF", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.9": {"text": "Allein es darf der eine nur, von seinem Centro, Westwerts gehn,", "tokens": ["Al\u00b7lein", "es", "darf", "der", "ei\u00b7ne", "nur", ",", "von", "sei\u00b7nem", "Cent\u00b7ro", ",", "West\u00b7werts", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "ART", "ART", "ADV", "$,", "APPR", "PPOSAT", "NE", "$,", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.10": {"text": "Der andere, mit seiner Kette, aus seinem Punkt sich Ostwerts", "tokens": ["Der", "an\u00b7de\u00b7re", ",", "mit", "sei\u00b7ner", "Ket\u00b7te", ",", "aus", "sei\u00b7nem", "Punkt", "sich", "Ost\u00b7werts"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "PRF", "NE"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.11": {"text": "So wird man beyde feste Ketten, die doch aus einem Punkt", "tokens": ["So", "wird", "man", "bey\u00b7de", "fes\u00b7te", "Ket\u00b7ten", ",", "die", "doch", "aus", "ei\u00b7nem", "Punkt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIS", "PIAT", "ADJA", "NN", "$,", "PRELS", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.12": {"text": "Und gnugsam an einander hangen, bald weit genug entfernet", "tokens": ["Und", "gnug\u00b7sam", "an", "ein\u00b7an\u00b7der", "han\u00b7gen", ",", "bald", "weit", "ge\u00b7nug", "ent\u00b7fer\u00b7net"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "PRF", "VVINF", "$,", "ADV", "ADJD", "ADV", "VVFIN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.13": {"text": "Indem nicht nur aus der Mathesi, nein, auch aus der Natur", "tokens": ["In\u00b7dem", "nicht", "nur", "aus", "der", "Ma\u00b7the\u00b7si", ",", "nein", ",", "auch", "aus", "der", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ADV", "APPR", "ART", "NE", "$,", "PTKANT", "$,", "ADV", "APPR", "ART", "NN"], "meter": "-+-+---+-+-+--+", "measure": "iambic.hexa.chol"}, "line.14": {"text": "Es ganz unwidersprechlich wahr:", "tokens": ["Es", "ganz", "un\u00b7wi\u00b7der\u00b7sprech\u00b7lich", "wahr", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Da\u00df eines Centri Radii, nach ganz untrieglich wahren Schl\u00fcssen,", "tokens": ["Da\u00df", "ei\u00b7nes", "Cent\u00b7ri", "Ra\u00b7dii", ",", "nach", "ganz", "un\u00b7trieg\u00b7lich", "wah\u00b7ren", "Schl\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NE", "NE", "$,", "APPR", "ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.16": {"text": "Wie gleich sie im Zusammenhang, nothwendig divergiren m\u00fcssen.", "tokens": ["Wie", "gleich", "sie", "im", "Zu\u00b7sam\u00b7men\u00b7hang", ",", "noth\u00b7wen\u00b7dig", "di\u00b7ver\u00b7gi\u00b7ren", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "APPRART", "NN", "$,", "ADJD", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}}}}}