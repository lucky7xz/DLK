{"textgrid.poem.44296": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Der Abend", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit balsamischem Gefieder", "tokens": ["Mit", "bal\u00b7sa\u00b7mi\u00b7schem", "Ge\u00b7fie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Deckt der Abend nun die Flur,", "tokens": ["Deckt", "der", "A\u00b7bend", "nun", "die", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sanfte K\u00fchlung weht hernieder,", "tokens": ["Sanf\u00b7te", "K\u00fch\u00b7lung", "weht", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stille sind der V\u00f6gel Lieder,", "tokens": ["Stil\u00b7le", "sind", "der", "V\u00f6\u00b7gel", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Feiernd schweiget die Natur.", "tokens": ["Fei\u00b7ernd", "schwei\u00b7get", "die", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ruhe tr\u00e4uft von seinem Fl\u00fcgel,", "tokens": ["Ru\u00b7he", "tr\u00e4uft", "von", "sei\u00b7nem", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und er spendet Labung aus.", "tokens": ["Und", "er", "spen\u00b7det", "La\u00b7bung", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dort, an jenem grauen H\u00fcgel,", "tokens": ["Dort", ",", "an", "je\u00b7nem", "grau\u00b7en", "H\u00fc\u00b7gel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kehrt vom Feld, mit losem Z\u00fcgel,", "tokens": ["Kehrt", "vom", "Feld", ",", "mit", "lo\u00b7sem", "Z\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Froh der Ackersmann nach Haus.", "tokens": ["Froh", "der", "A\u00b7ckers\u00b7mann", "nach", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Stille deckt die d\u00fcstre Erde,", "tokens": ["Stil\u00b7le", "deckt", "die", "d\u00fcst\u00b7re", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alles ruht auf Berg und Tal,", "tokens": ["Al\u00b7les", "ruht", "auf", "Berg", "und", "Tal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zu den H\u00fcrden eilt die Herde,", "tokens": ["Zu", "den", "H\u00fcr\u00b7den", "eilt", "die", "Her\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Abgemattet ziehn die Pferde", "tokens": ["Ab\u00b7ge\u00b7mat\u00b7tet", "ziehn", "die", "Pfer\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In den langentbehrten Stall.", "tokens": ["In", "den", "lan\u00b7gent\u00b7behr\u00b7ten", "Stall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Purpurr\u00f6te \u00fcberwebet", "tokens": ["Pur\u00b7pur\u00b7r\u00f6\u00b7te", "\u00fc\u00b7ber\u00b7we\u00b7bet"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hell das blaue Firmament,", "tokens": ["Hell", "das", "blau\u00b7e", "Fir\u00b7ma\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und das Laub im Walde bebet,", "tokens": ["Und", "das", "Laub", "im", "Wal\u00b7de", "be\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von des Zephirs Hauch belebet,", "tokens": ["Von", "des", "Ze\u00b7phirs", "Hauch", "be\u00b7le\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und der ferne Westen brennt.", "tokens": ["Und", "der", "fer\u00b7ne", "Wes\u00b7ten", "brennt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.5": {"line.1": {"text": "Rotes Gold gl\u00fcht in den L\u00fcften,", "tokens": ["Ro\u00b7tes", "Gold", "gl\u00fcht", "in", "den", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spiegelt sich im nahen Teich.", "tokens": ["Spie\u00b7gelt", "sich", "im", "na\u00b7hen", "Teich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dunkles Grau umh\u00fcllt die Triften,", "tokens": ["Dunk\u00b7les", "Grau", "um\u00b7h\u00fcllt", "die", "Trif\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Berg und Tal und Wiese d\u00fcften,", "tokens": ["Berg", "und", "Tal", "und", "Wie\u00b7se", "d\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dampfenden Alt\u00e4ren gleich.", "tokens": ["Damp\u00b7fen\u00b7den", "Al\u00b7t\u00e4\u00b7ren", "gleich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Hinter jener Berge R\u00fccken", "tokens": ["Hin\u00b7ter", "je\u00b7ner", "Ber\u00b7ge", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sinket Ph\u00f6bus hehr hinab,", "tokens": ["Sin\u00b7ket", "Ph\u00f6\u00b7bus", "hehr", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So st\u00fcrzt, Hoheit in den Blicken,", "tokens": ["So", "st\u00fcrzt", ",", "Ho\u00b7heit", "in", "den", "Bli\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich der Brave mit Entz\u00fccken", "tokens": ["Sich", "der", "Bra\u00b7ve", "mit", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "F\u00fcr das Recht ins offne Grab.", "tokens": ["F\u00fcr", "das", "Recht", "ins", "off\u00b7ne", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Nichts kann seinen Mut ermatten,", "tokens": ["Nichts", "kann", "sei\u00b7nen", "Mut", "er\u00b7mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn die Pflicht den Edlen ruft,", "tokens": ["Wenn", "die", "Pflicht", "den", "Ed\u00b7len", "ruft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Er z\u00e4hlt fr\u00f6hlich seine Taten,", "tokens": ["Er", "z\u00e4hlt", "fr\u00f6h\u00b7lich", "sei\u00b7ne", "Ta\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Fliehet in des Grabes Schatten,", "tokens": ["Flie\u00b7het", "in", "des", "Gra\u00b7bes", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sinket heiter in die Gruft.", "tokens": ["Sin\u00b7ket", "hei\u00b7ter", "in", "die", "Gruft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Denn warum sollt er auch beben,", "tokens": ["Denn", "wa\u00b7rum", "sollt", "er", "auch", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Bald entweicht des Grabes Nacht,", "tokens": ["Bald", "ent\u00b7weicht", "des", "Gra\u00b7bes", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bald kehrt er zu besserm Leben,", "tokens": ["Bald", "kehrt", "er", "zu", "bes\u00b7serm", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Bald wird er sich neu erheben,", "tokens": ["Bald", "wird", "er", "sich", "neu", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wie die Sonn in hehrer Pracht.", "tokens": ["Wie", "die", "Sonn", "in", "heh\u00b7rer", "Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}