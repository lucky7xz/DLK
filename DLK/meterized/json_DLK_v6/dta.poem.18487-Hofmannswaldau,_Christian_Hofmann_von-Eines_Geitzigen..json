{"dta.poem.18487": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Eines Geitzigen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich a\u00df und tranck nicht gnung/ ich ruhte niemals aus/", "tokens": ["Ich", "a\u00df", "und", "tranck", "nicht", "gnung", "/", "ich", "ruh\u00b7te", "nie\u00b7mals", "aus", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PTKNEG", "ADV", "$(", "PPER", "VVFIN", "ADV", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit brodt und wasser hab ich meistens mich ern\u00e4hret/", "tokens": ["Mit", "brodt", "und", "was\u00b7ser", "hab", "ich", "meis\u00b7tens", "mich", "er\u00b7n\u00e4h\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "KON", "PWS", "VAFIN", "PPER", "ADV", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gieng brummen wie ein b\u00e4r/ bey tag nacht und im hau\u00df/", "tokens": ["Gieng", "brum\u00b7men", "wie", "ein", "b\u00e4r", "/", "bey", "tag", "nacht", "und", "im", "hau\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KOKOM", "ART", "NN", "$(", "APPR", "NN", "NN", "KON", "APPRART", "NN", "$("], "meter": "-+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Jtzt wird mein gut mit lust/ durch w\u00fcrmer ich/ verzehret.", "tokens": ["Jtzt", "wird", "mein", "gut", "mit", "lust", "/", "durch", "w\u00fcr\u00b7mer", "ich", "/", "ver\u00b7zeh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJD", "APPR", "NN", "$(", "APPR", "NN", "PPER", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}