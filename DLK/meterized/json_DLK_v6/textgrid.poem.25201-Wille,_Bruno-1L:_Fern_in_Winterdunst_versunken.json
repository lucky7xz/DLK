{"textgrid.poem.25201": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Fern in Winterdunst versunken", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fern in Winterdunst versunken", "tokens": ["Fern", "in", "Win\u00b7ter\u00b7dunst", "ver\u00b7sun\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liegt die graue Stadt. \u2013", "tokens": ["Liegt", "die", "grau\u00b7e", "Stadt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Auf bereifter Wiese", "tokens": ["Auf", "be\u00b7reif\u00b7ter", "Wie\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Tr\u00e4umt ein Fr\u00fchgeborener", "tokens": ["Tr\u00e4umt", "ein", "Fr\u00fch\u00b7ge\u00b7bo\u00b7re\u00b7ner"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.5": {"text": "Von einer Stadt des Lichtes. \u2013 \u2013 \u2013", "tokens": ["Von", "ei\u00b7ner", "Stadt", "des", "Lich\u00b7tes", ".", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$.", "$(", "$(", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "In frostigen D\u00fcnsten, die zum Himmel qualmen,", "tokens": ["In", "fros\u00b7ti\u00b7gen", "D\u00fcns\u00b7ten", ",", "die", "zum", "Him\u00b7mel", "qual\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "APPRART", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Verblutet die Sonne.", "tokens": ["Ver\u00b7blu\u00b7tet", "die", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Ein wei\u00dfes Birkenkind mit bebenden Reisern", "tokens": ["Ein", "wei\u00b7\u00dfes", "Bir\u00b7ken\u00b7kind", "mit", "be\u00b7ben\u00b7den", "Rei\u00b7sern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Starrt bang in die Blutung:", "tokens": ["Starrt", "bang", "in", "die", "Blu\u00b7tung", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "O stirb nicht, M\u00fctterchen Sonne!", "tokens": ["O", "stirb", "nicht", ",", "M\u00fct\u00b7ter\u00b7chen", "Son\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "$,", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Im zarten Gezweige h\u00e4ngt", "tokens": ["Im", "zar\u00b7ten", "Ge\u00b7zwei\u00b7ge", "h\u00e4ngt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Rotkehlchen mit blutiger Brust,", "tokens": ["Rot\u00b7kehl\u00b7chen", "mit", "blu\u00b7ti\u00b7ger", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "++--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Das Gefieder schaudernd gestr\u00e4ubt:", "tokens": ["Das", "Ge\u00b7fie\u00b7der", "schau\u00b7dernd", "ge\u00b7str\u00e4ubt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Die Sonne stirbt, \u2013", "tokens": ["Die", "Son\u00b7ne", "stirbt", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Wie Bl\u00e4tter und M\u00fccken starben!", "tokens": ["Wie", "Bl\u00e4t\u00b7ter", "und", "M\u00fc\u00b7cken", "star\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Ein karges Weilchen am Nachmittag", "tokens": ["Ein", "kar\u00b7ges", "Weil\u00b7chen", "am", "Nach\u00b7mit\u00b7tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Erhob sie sich und schaute matt", "tokens": ["Er\u00b7hob", "sie", "sich", "und", "schau\u00b7te", "matt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "KON", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und schr\u00e4ge \u00fcber die Wiese;", "tokens": ["Und", "schr\u00e4\u00b7ge", "\u00fc\u00b7ber", "die", "Wie\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dann ward sie ein verweintes Auge,", "tokens": ["Dann", "ward", "sie", "ein", "ver\u00b7wein\u00b7tes", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und nun ein Tropfen Blut ...", "tokens": ["Und", "nun", "ein", "Trop\u00b7fen", "Blut", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Sie stirbt, \u2013 wie j\u00fcngst die Bl\u00e4tter starben.", "tokens": ["Sie", "stirbt", ",", "\u2013", "wie", "j\u00fcngst", "die", "Bl\u00e4t\u00b7ter", "star\u00b7ben", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "$(", "KOKOM", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Lebwohl, lebwohl!", "tokens": ["Leb\u00b7wohl", ",", "leb\u00b7wohl", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Deine Kinder behalten dich lieb.", "tokens": ["Dei\u00b7ne", "Kin\u00b7der", "be\u00b7hal\u00b7ten", "dich", "lieb", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Sieh, dr\u00fcben das H\u00e4uschen,", "tokens": ["Sieh", ",", "dr\u00fc\u00b7ben", "das", "H\u00e4usc\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Das oft du bel\u00e4chelt,", "tokens": ["Das", "oft", "du", "be\u00b7l\u00e4\u00b7chelt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Gr\u00fc\u00dft dich wehm\u00fctig", "tokens": ["Gr\u00fc\u00dft", "dich", "weh\u00b7m\u00fc\u00b7tig"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Mit gl\u00fchender Fensterscheibe ...", "tokens": ["Mit", "gl\u00fc\u00b7hen\u00b7der", "Fens\u00b7ter\u00b7schei\u00b7be", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Und dicker qualmen die frostigen D\u00fcnste.", "tokens": ["Und", "di\u00b7cker", "qual\u00b7men", "die", "fros\u00b7ti\u00b7gen", "D\u00fcns\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Anfangs m\u00fcssen sie leuchten wie Nordlicht;", "tokens": ["An\u00b7fangs", "m\u00fcs\u00b7sen", "sie", "leuch\u00b7ten", "wie", "Nord\u00b7licht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "KOKOM", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Doch ihr rauchiger Schleier siegt,", "tokens": ["Doch", "ihr", "rau\u00b7chi\u00b7ger", "Schlei\u00b7er", "siegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und d\u00fcster blutend,", "tokens": ["Und", "d\u00fcs\u00b7ter", "blu\u00b7tend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Gleich vergl\u00fchender Kohle,", "tokens": ["Gleich", "ver\u00b7gl\u00fc\u00b7hen\u00b7der", "Koh\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Erstickt im Qualme die Sonne.", "tokens": ["Er\u00b7stickt", "im", "Qual\u00b7me", "die", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Russige Wolken ragen empor,", "tokens": ["Rus\u00b7si\u00b7ge", "Wol\u00b7ken", "ra\u00b7gen", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Die auf riesigen Rumpfen", "tokens": ["Die", "auf", "rie\u00b7si\u00b7gen", "Rump\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.3": {"text": "Unendliche Flockenlasten zusammentragen,", "tokens": ["Un\u00b7end\u00b7li\u00b7che", "Flo\u00b7cken\u00b7las\u00b7ten", "zu\u00b7sam\u00b7men\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die Welt zu versch\u00fctten.", "tokens": ["Die", "Welt", "zu", "ver\u00b7sch\u00fct\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.8": {"line.1": {"text": "D\u00e4mmrung st\u00fcrzt lawinengleich", "tokens": ["D\u00e4mm\u00b7rung", "st\u00fcrzt", "la\u00b7wi\u00b7nen\u00b7gleich"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVFIN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Von Wolkengebirgen;", "tokens": ["Von", "Wol\u00b7ken\u00b7ge\u00b7bir\u00b7gen", ";"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Aus Wolkenkl\u00fcften haucht der Frost", "tokens": ["Aus", "Wol\u00b7ken\u00b7kl\u00fcf\u00b7ten", "haucht", "der", "Frost"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schneidend \u00fcber frierende Gr\u00e4ser.", "tokens": ["Schnei\u00b7dend", "\u00fc\u00b7ber", "frie\u00b7ren\u00b7de", "Gr\u00e4\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Kr\u00e4chzend und fl\u00fcgelklatschend", "tokens": ["Kr\u00e4ch\u00b7zend", "und", "fl\u00fc\u00b7gel\u00b7klat\u00b7schend"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "VVPP"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Hastet die Kr\u00e4he hinweg;", "tokens": ["Has\u00b7tet", "die", "Kr\u00e4\u00b7he", "hin\u00b7weg", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "Rotkehlchen ist fort, wie sturmverweht;", "tokens": ["Rot\u00b7kehl\u00b7chen", "ist", "fort", ",", "wie", "sturm\u00b7ver\u00b7weht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PTKVZ", "$,", "PWAV", "VVFIN", "$."], "meter": "++--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Die verwaiste Birke erschauert,", "tokens": ["Die", "ver\u00b7wais\u00b7te", "Bir\u00b7ke", "er\u00b7schau\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "An den Wimpern erfrorene Thr\u00e4nen ...", "tokens": ["An", "den", "Wim\u00b7pern", "er\u00b7fro\u00b7re\u00b7ne", "Thr\u00e4\u00b7nen", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$("], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.10": {"text": "Die Sonne ist tot! \u2013 \u2013 \u2013", "tokens": ["Die", "Son\u00b7ne", "ist", "tot", "!", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "$(", "$(", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.9": {"line.1": {"text": "In Finsternis versunken liegt die ferne Stadt.", "tokens": ["In", "Fins\u00b7ter\u00b7nis", "ver\u00b7sun\u00b7ken", "liegt", "die", "fer\u00b7ne", "Stadt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVINF", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf erfrorener Wiese", "tokens": ["Auf", "er\u00b7fro\u00b7re\u00b7ner", "Wie\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Tr\u00e4umt ein Fr\u00fchgeborener", "tokens": ["Tr\u00e4umt", "ein", "Fr\u00fch\u00b7ge\u00b7bo\u00b7re\u00b7ner"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Von einer Stadt des Lichtes.", "tokens": ["Von", "ei\u00b7ner", "Stadt", "des", "Lich\u00b7tes", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}