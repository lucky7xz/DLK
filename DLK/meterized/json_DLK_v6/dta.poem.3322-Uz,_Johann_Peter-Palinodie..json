{"dta.poem.3322": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Palinodie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "La\u00dft ab von mir, ich will mich selbst verdammen;", "tokens": ["La\u00dft", "ab", "von", "mir", ",", "ich", "will", "mich", "selbst", "ver\u00b7dam\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "APPR", "PPER", "$,", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gespenster! ach! die ihr mit Klauen dr\u00e4ut,", "tokens": ["Ge\u00b7spens\u00b7ter", "!", "ach", "!", "die", "ihr", "mit", "Klau\u00b7en", "dr\u00e4ut", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "XY", "$.", "PRELS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Um Gr\u00e4ber sp\u00fckt und Kindern oder Ammen", "tokens": ["Um", "Gr\u00e4\u00b7ber", "sp\u00fckt", "und", "Kin\u00b7dern", "o\u00b7der", "Am\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "NN", "VVFIN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Am liebsten sichtbar seyd!", "tokens": ["Am", "liebs\u00b7ten", "sicht\u00b7bar", "seyd", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich glaubte sonst: der Todte kommt nicht wieder;", "tokens": ["Ich", "glaub\u00b7te", "sonst", ":", "der", "Tod\u00b7te", "kommt", "nicht", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$.", "ART", "NN", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein eisern Band h\u00e4lt seine F\u00fcsse fest:", "tokens": ["Ein", "ei\u00b7sern", "Band", "h\u00e4lt", "sei\u00b7ne", "F\u00fcs\u00b7se", "fest", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo ist ein Grab, das die vermorschten Glieder", "tokens": ["Wo", "ist", "ein", "Grab", ",", "das", "die", "ver\u00b7morschten", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Aus kalten Armen l\u00e4\u00dft?", "tokens": ["Aus", "kal\u00b7ten", "Ar\u00b7men", "l\u00e4\u00dft", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Jm Grabe schl\u00e4ft Uly\u00df, nach langen Reisen;", "tokens": ["Jm", "Gra\u00b7be", "schl\u00e4ft", "U\u00b7ly\u00df", ",", "nach", "lan\u00b7gen", "Rei\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "NE", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Da schl\u00e4ft Achill, nur lebend im Gedicht:", "tokens": ["Da", "schl\u00e4ft", "A\u00b7chill", ",", "nur", "le\u00b7bend", "im", "Ge\u00b7dicht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "$,", "ADV", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da k\u00fcmmern sich die Narren, wie die Weisen,", "tokens": ["Da", "k\u00fcm\u00b7mern", "sich", "die", "Nar\u00b7ren", ",", "wie", "die", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um andre Narren nicht.", "tokens": ["Um", "and\u00b7re", "Nar\u00b7ren", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "So schwatzt Vernunft, die immer n\u00e4rrschgewesen:", "tokens": ["So", "schwatzt", "Ver\u00b7nunft", ",", "die", "im\u00b7mer", "n\u00e4rrschge\u00b7we\u00b7sen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$,", "PRELS", "ADV", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich glaub inde\u00df, was mein Balbier beze\u00fcgt,", "tokens": ["Ich", "glaub", "in\u00b7de\u00df", ",", "was", "mein", "Bal\u00b7bier", "be\u00b7ze\u00fcgt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Was wir im Faust und im Kalender lesen;", "tokens": ["Was", "wir", "im", "Faust", "und", "im", "Ka\u00b7len\u00b7der", "le\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "KON", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und kein Kalender leugt.", "tokens": ["Und", "kein", "Ka\u00b7len\u00b7der", "leugt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Ich glaube nun die kl\u00e4gliche Geschichte", "tokens": ["Ich", "glau\u00b7be", "nun", "die", "kl\u00e4g\u00b7li\u00b7che", "Ge\u00b7schich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom schwarzen M\u00f6nch, der n\u00e4chtlich wachen mu\u00df;", "tokens": ["Vom", "schwar\u00b7zen", "M\u00f6nch", ",", "der", "n\u00e4cht\u00b7lich", "wa\u00b7chen", "mu\u00df", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PRELS", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den Hexen-Tanz und Marthens Nacht-Gesichte,", "tokens": ["Den", "He\u00b7xen\u00b7Tanz", "und", "Mar\u00b7thens", "Nacht\u00b7Ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Selbst Satans Pferdefu\u00df.", "tokens": ["Selbst", "Sa\u00b7tans", "Pfer\u00b7de\u00b7fu\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Was Aberglaub im Finstern ausgebr\u00fctet,", "tokens": ["Was", "A\u00b7berg\u00b7laub", "im", "Fins\u00b7tern", "aus\u00b7ge\u00b7br\u00fc\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "H\u00f6rt itzt mein Ohr, von banger Lust entz\u00fcckt,", "tokens": ["H\u00f6rt", "itzt", "mein", "Ohr", ",", "von", "ban\u00b7ger", "Lust", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PPOSAT", "NN", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Seit \u00fcber mich der Hypochonder w\u00fcthet,", "tokens": ["Seit", "\u00fc\u00b7ber", "mich", "der", "Hy\u00b7po\u00b7chon\u00b7der", "w\u00fct\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und mein Gehirn verr\u00fcckt.", "tokens": ["Und", "mein", "Ge\u00b7hirn", "ver\u00b7r\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Der Jugend Roth flieht meine blassen Wangen:", "tokens": ["Der", "Ju\u00b7gend", "Roth", "flieht", "mei\u00b7ne", "blas\u00b7sen", "Wan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich seh, erstaunt, mein schwarzes Haar gebleicht,", "tokens": ["Ich", "seh", ",", "er\u00b7staunt", ",", "mein", "schwar\u00b7zes", "Haar", "ge\u00b7bleicht", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVPP", "$,", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und welke Haut um meine Knochen hangen:", "tokens": ["Und", "wel\u00b7ke", "Haut", "um", "mei\u00b7ne", "Kno\u00b7chen", "han\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mein schwerer Odem keicht.", "tokens": ["Mein", "schwe\u00b7rer", "O\u00b7dem", "keicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Jhr Larven, schont! verschont mein einsam Bette,", "tokens": ["Ihr", "Lar\u00b7ven", ",", "schont", "!", "ver\u00b7schont", "mein", "ein\u00b7sam", "Bet\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "$.", "VVFIN", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo ich allein und ohne M\u00e4dchen bin!", "tokens": ["Wo", "ich", "al\u00b7lein", "und", "oh\u00b7ne", "M\u00e4d\u00b7chen", "bin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "KON", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was rasselt ihr mit nachgeschleppter Kette", "tokens": ["Was", "ras\u00b7selt", "ihr", "mit", "nach\u00b7ge\u00b7schlepp\u00b7ter", "Ket\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vor meinen Ohren hin?", "tokens": ["Vor", "mei\u00b7nen", "Oh\u00b7ren", "hin", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Will ein Gespenst bey meinem Bett erscheinen,", "tokens": ["Will", "ein", "Ge\u00b7spenst", "bey", "mei\u00b7nem", "Bett", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So sey es Fleisch und f\u00e4hig schlauer Lust,", "tokens": ["So", "sey", "es", "Fleisch", "und", "f\u00e4\u00b7hig", "schlau\u00b7er", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "KON", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "(versteht mich recht!) mit runden weissen Beinen", "tokens": ["(", "ver\u00b7steht", "mich", "recht", "!", ")", "mit", "run\u00b7den", "weis\u00b7sen", "Bei\u00b7nen"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "PPER", "ADJD", "$.", "$(", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und einer weissen Brust.", "tokens": ["Und", "ei\u00b7ner", "weis\u00b7sen", "Brust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}