{"textgrid.poem.26005": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Du bist mehr als ein Fr\u00fchling", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der s\u00fc\u00dfe Flieder steht nur einmal im Jahr auf dem Baum,", "tokens": ["Der", "s\u00fc\u00b7\u00dfe", "Flie\u00b7der", "steht", "nur", "ein\u00b7mal", "im", "Jahr", "auf", "dem", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ADV", "APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Deine Br\u00fcste bl\u00fchen mir jahraus, jahrein, du bist mehr als ein Fr\u00fchling.", "tokens": ["Dei\u00b7ne", "Br\u00fcs\u00b7te", "bl\u00fc\u00b7hen", "mir", "ja\u00b7hraus", ",", "ja\u00b7hrein", ",", "du", "bist", "mehr", "als", "ein", "Fr\u00fch\u00b7ling", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "PTKANT", "$,", "PPER", "VAFIN", "PIAT", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Meine W\u00fcnsche gl\u00e4nzten wie die Sprossen der Kastanie,", "tokens": ["Mei\u00b7ne", "W\u00fcn\u00b7sche", "gl\u00e4nz\u00b7ten", "wie", "die", "Spros\u00b7sen", "der", "Kas\u00b7ta\u00b7nie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "KOKOM", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+--+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Du zogst sie alle an die Sonne, wir sitzen in einem Laubdach", "tokens": ["Du", "zogst", "sie", "al\u00b7le", "an", "die", "Son\u00b7ne", ",", "wir", "sit\u00b7zen", "in", "ei\u00b7nem", "Laub\u00b7dach"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "APPR", "ART", "NN", "$,", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+--+--+-+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Und lachen uns zu im satten Schatten.", "tokens": ["Und", "la\u00b7chen", "uns", "zu", "im", "sat\u00b7ten", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Wie einen Baum, den der Blitz \u00fcberfiel, hatte mich die Sehnsucht gezeichnet,", "tokens": ["Wie", "ei\u00b7nen", "Baum", ",", "den", "der", "Blitz", "\u00fc\u00b7berf\u00b7iel", ",", "hat\u00b7te", "mich", "die", "Sehn\u00b7sucht", "ge\u00b7zeich\u00b7net", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--++--+-+-+--+-", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Jetzt wohnen deine Bienen bei mir, und meine Augen flie\u00dfen \u00fcber von deinem Honig.", "tokens": ["Jetzt", "woh\u00b7nen", "dei\u00b7ne", "Bie\u00b7nen", "bei", "mir", ",", "und", "mei\u00b7ne", "Au\u00b7gen", "flie\u00b7\u00dfen", "\u00fc\u00b7ber", "von", "dei\u00b7nem", "Ho\u00b7nig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "PPER", "$,", "KON", "PPOSAT", "NN", "VVFIN", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+-+-+--+-+-", "measure": "iambic.octa.plus.relaxed"}}}}}