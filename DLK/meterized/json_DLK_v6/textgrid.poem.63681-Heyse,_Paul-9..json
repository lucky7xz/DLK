{"textgrid.poem.63681": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heut hab' ich redlich mir mein Mittagessen", "tokens": ["Heut", "hab'", "ich", "red\u00b7lich", "mir", "mein", "Mit\u00b7ta\u00b7ges\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verdient; mein Doktor wird zufrieden sein.", "tokens": ["Ver\u00b7di\u00b7ent", ";", "mein", "Dok\u00b7tor", "wird", "zu\u00b7frie\u00b7den", "sein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$.", "PPOSAT", "NN", "VAFIN", "ADJD", "VAINF", "$."], "meter": "+---+-+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "Fr\u00fch streift' ich lang umher im Buchenhain", "tokens": ["Fr\u00fch", "streift'", "ich", "lang", "um\u00b7her", "im", "Bu\u00b7chen\u00b7hain"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ADJD", "PTKVZ", "APPRART", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Und hab' auf jeder zweiten Bank gesessen.", "tokens": ["Und", "hab'", "auf", "je\u00b7der", "zwei\u00b7ten", "Bank", "ge\u00b7ses\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PIAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "An nichts zu denken m\u00fcht' ich mich. Indessen", "tokens": ["An", "nichts", "zu", "den\u00b7ken", "m\u00fcht'", "ich", "mich", ".", "In\u00b7des\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PIS", "PTKZU", "VVINF", "VMFIN", "PPER", "PRF", "$.", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sprang vor mir durchs Ge\u00e4st ein Eichk\u00e4tzlein;", "tokens": ["Sprang", "vor", "mir", "durchs", "Ge\u00b7\u00e4st", "ein", "Eich\u00b7k\u00e4tz\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "APPRART", "NN", "ART", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Ich raucht' ein wenig, sah ins Blau hinein", "tokens": ["Ich", "raucht'", "ein", "we\u00b7nig", ",", "sah", "ins", "Blau", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "PIS", "$,", "VVFIN", "APPRART", "NN", "APZR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und suchte mich halbschlummernd zu vergessen.", "tokens": ["Und", "such\u00b7te", "mich", "halb\u00b7schlum\u00b7mernd", "zu", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Hernach im W\u00e4ldchen hinterm Haus am Bronnen", "tokens": ["Her\u00b7nach", "im", "W\u00e4ld\u00b7chen", "hin\u00b7term", "Haus", "am", "Bron\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "APPRART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erquickt ich mich schlich so hin und her,", "tokens": ["Er\u00b7quickt", "ich", "mich", "schlich", "so", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "ADV", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Bis endlich diese Stunden auch verronnen.", "tokens": ["Bis", "end\u00b7lich", "die\u00b7se", "Stun\u00b7den", "auch", "ver\u00b7ron\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PDAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wenn nur das s\u00fc\u00dfe Nichtstun leichter w\u00e4r!", "tokens": ["Wenn", "nur", "das", "s\u00fc\u00b7\u00dfe", "Nichts\u00b7tun", "leich\u00b7ter", "w\u00e4r", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wohl ist \u00bbstill liegen und sich einsam sonnen", "tokens": ["Wohl", "ist", "\u00bb", "still", "lie\u00b7gen", "und", "sich", "ein\u00b7sam", "son\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "$(", "ADJD", "VVFIN", "KON", "PRF", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "'ne tapfre Kunst\u00ab \u2013 allein verteufelt schwer!", "tokens": ["'ne", "tapf\u00b7re", "Kunst", "\u00ab", "\u2013", "al\u00b7lein", "ver\u00b7teu\u00b7felt", "schwer", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "$(", "ADV", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}