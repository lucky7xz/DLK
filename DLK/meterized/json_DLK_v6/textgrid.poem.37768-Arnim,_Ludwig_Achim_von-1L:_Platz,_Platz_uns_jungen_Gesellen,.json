{"textgrid.poem.37768": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Platz, Platz uns jungen Gesellen,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Platz, Platz uns jungen Gesellen,", "tokens": ["Platz", ",", "Platz", "uns", "jun\u00b7gen", "Ge\u00b7sel\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wir wollen zum Tanze uns stellen,", "tokens": ["Wir", "wol\u00b7len", "zum", "Tan\u00b7ze", "uns", "stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "PPER", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Wer reicht mir den Kranz,", "tokens": ["Wer", "reicht", "mir", "den", "Kranz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Ich f\u00fchre den Tanz.", "tokens": ["Ich", "f\u00fch\u00b7re", "den", "Tanz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Ich bin ein Geschlechter,", "tokens": ["Ich", "bin", "ein", "Ge\u00b7schlech\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Ein stattlicher Fechter,", "tokens": ["Ein", "statt\u00b7li\u00b7cher", "Fech\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Ich kann Euch besch\u00fctzen", "tokens": ["Ich", "kann", "Euch", "be\u00b7sch\u00fct\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "VVINF"], "meter": "++--+-", "measure": "trochaic.tri.relaxed"}, "line.8": {"text": "Mit Messern und Witzen,", "tokens": ["Mit", "Mes\u00b7sern", "und", "Wit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Will einer Euch kr\u00e4nken,", "tokens": ["Will", "ei\u00b7ner", "Euch", "kr\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "PPER", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Ich will's ihm nicht schenken.", "tokens": ["Ich", "will's", "ihm", "nicht", "schen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Kann schweben und schwanken", "tokens": ["Kann", "schwe\u00b7ben", "und", "schwan\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "VVINF", "KON", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Mit Herz und Gedanken,", "tokens": ["Mit", "Herz", "und", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Kann treten und springen,", "tokens": ["Kann", "tre\u00b7ten", "und", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "Wie Pfeifen erklingen,", "tokens": ["Wie", "Pfei\u00b7fen", "er\u00b7klin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.15": {"text": "Kann drehen und wenden", "tokens": ["Kann", "dre\u00b7hen", "und", "wen\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "VVINF", "KON", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.16": {"text": "Mit dr\u00fcckenden H\u00e4nden,", "tokens": ["Mit", "dr\u00fc\u00b7cken\u00b7den", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.17": {"text": "Mit klopfendem Herzen,", "tokens": ["Mit", "klop\u00b7fen\u00b7dem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.18": {"text": "Mit jauchzenden Scherzen;", "tokens": ["Mit", "jauch\u00b7zen\u00b7den", "Scher\u00b7zen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.19": {"text": "Es folgen mir alle", "tokens": ["Es", "fol\u00b7gen", "mir", "al\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PIAT"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.20": {"text": "Mit freudigem Schalle,", "tokens": ["Mit", "freu\u00b7di\u00b7gem", "Schal\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.21": {"text": "Schnell spielen die Geigen", "tokens": ["Schnell", "spie\u00b7len", "die", "Gei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.22": {"text": "Den freudigen Reigen,", "tokens": ["Den", "freu\u00b7di\u00b7gen", "Rei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.23": {"text": "Es schwanken die Dielen", "tokens": ["Es", "schwan\u00b7ken", "die", "Die\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.24": {"text": "Je h\u00f6her sie spielen,", "tokens": ["Je", "h\u00f6\u00b7her", "sie", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.25": {"text": "Es st\u00e4ubet das Haus,", "tokens": ["Es", "st\u00e4u\u00b7bet", "das", "Haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.26": {"text": "Da geht es zum Schmaus,", "tokens": ["Da", "geht", "es", "zum", "Schmaus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.27": {"text": "Da geht es zum Wein:", "tokens": ["Da", "geht", "es", "zum", "Wein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.28": {"text": "Nun Liebchen schenk ein!", "tokens": ["Nun", "Lieb\u00b7chen", "schenk", "ein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}