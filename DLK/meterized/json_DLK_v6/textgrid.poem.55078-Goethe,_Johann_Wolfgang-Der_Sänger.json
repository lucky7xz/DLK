{"textgrid.poem.55078": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Der S\u00e4nger", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwas h\u00f6r ich drau\u00dfen vor dem Tor,", "tokens": ["\u00bb", "was", "h\u00f6r", "ich", "drau\u00b7\u00dfen", "vor", "dem", "Tor", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was auf der Br\u00fccke schallen?", "tokens": ["Was", "auf", "der", "Br\u00fc\u00b7cke", "schal\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df den Gesang vor unserm Ohr", "tokens": ["La\u00df", "den", "Ge\u00b7sang", "vor", "un\u00b7serm", "Ohr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Im Saale widerhallen!\u00ab", "tokens": ["Im", "Saa\u00b7le", "wi\u00b7der\u00b7hal\u00b7len", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der K\u00f6nig sprach's, der Page lief;", "tokens": ["Der", "K\u00f6\u00b7nig", "sprach's", ",", "der", "Pa\u00b7ge", "lief", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Knabe kam, der K\u00f6nig rief:", "tokens": ["Der", "Kna\u00b7be", "kam", ",", "der", "K\u00f6\u00b7nig", "rief", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "\u00bbla\u00dft mir herein den Alten!\u00ab", "tokens": ["\u00bb", "la\u00dft", "mir", "her\u00b7ein", "den", "Al\u00b7ten", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVIMP", "PPER", "PTKVZ", "ART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbgegr\u00fc\u00dfet seid mir, edle Herrn,", "tokens": ["\u00bb", "ge\u00b7gr\u00fc\u00b7\u00dfet", "seid", "mir", ",", "ed\u00b7le", "Herrn", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVPP", "VAFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gegr\u00fc\u00dft ihr, sch\u00f6ne Damen!", "tokens": ["Ge\u00b7gr\u00fc\u00dft", "ihr", ",", "sch\u00f6\u00b7ne", "Da\u00b7men", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Welch reicher Himmel! Stern bei Stern!", "tokens": ["Welch", "rei\u00b7cher", "Him\u00b7mel", "!", "Stern", "bei", "Stern", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$.", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wer kennet ihre Namen?", "tokens": ["Wer", "ken\u00b7net", "ih\u00b7re", "Na\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Im Saal voll Pracht und Herrlichkeit", "tokens": ["Im", "Saal", "voll", "Pracht", "und", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJD", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Schlie\u00dft, Augen, euch; hier ist nicht Zeit,", "tokens": ["Schlie\u00dft", ",", "Au\u00b7gen", ",", "euch", ";", "hier", "ist", "nicht", "Zeit", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "PPER", "$.", "ADV", "VAFIN", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sich staunend zu ergetzen.\u00ab", "tokens": ["Sich", "stau\u00b7nend", "zu", "er\u00b7get\u00b7zen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PRF", "ADJD", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der S\u00e4nger dr\u00fcckt' die Augen ein", "tokens": ["Der", "S\u00e4n\u00b7ger", "dr\u00fcckt'", "die", "Au\u00b7gen", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und schlug in vollen T\u00f6nen;", "tokens": ["Und", "schlug", "in", "vol\u00b7len", "T\u00f6\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Ritter schauten mutig drein", "tokens": ["Die", "Rit\u00b7ter", "schau\u00b7ten", "mu\u00b7tig", "drein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und in den Scho\u00df die Sch\u00f6nen.", "tokens": ["Und", "in", "den", "Scho\u00df", "die", "Sch\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der K\u00f6nig, dem das Lied gefiel,", "tokens": ["Der", "K\u00f6\u00b7nig", ",", "dem", "das", "Lied", "ge\u00b7fiel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Lie\u00df, ihn zu ehren f\u00fcr sein Spiel,", "tokens": ["Lie\u00df", ",", "ihn", "zu", "eh\u00b7ren", "f\u00fcr", "sein", "Spiel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "PTKZU", "VVINF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Eine goldne Kette holen.", "tokens": ["Ei\u00b7ne", "gold\u00b7ne", "Ket\u00b7te", "ho\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbdie goldne Kette gib mir nicht,", "tokens": ["\u00bb", "die", "gold\u00b7ne", "Ket\u00b7te", "gib", "mir", "nicht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "VVIMP", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Kette gib den Rittern,", "tokens": ["Die", "Ket\u00b7te", "gib", "den", "Rit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIMP", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vor deren k\u00fchnem Angesicht", "tokens": ["Vor", "de\u00b7ren", "k\u00fch\u00b7nem", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Feinde Lanzen splittern;", "tokens": ["Der", "Fein\u00b7de", "Lan\u00b7zen", "split\u00b7tern", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Gib sie dem Kanzler, den du hast,", "tokens": ["Gib", "sie", "dem", "Kanz\u00b7ler", ",", "den", "du", "hast", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und la\u00df ihn noch die goldne Last", "tokens": ["Und", "la\u00df", "ihn", "noch", "die", "gold\u00b7ne", "Last"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zu andern Lasten tragen.", "tokens": ["Zu", "an\u00b7dern", "Las\u00b7ten", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ich singe, wie der Vogel singt,", "tokens": ["Ich", "sin\u00b7ge", ",", "wie", "der", "Vo\u00b7gel", "singt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der in den Zweigen wohnet;", "tokens": ["Der", "in", "den", "Zwei\u00b7gen", "woh\u00b7net", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Lied, das aus der Kehle dringt,", "tokens": ["Das", "Lied", ",", "das", "aus", "der", "Keh\u00b7le", "dringt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist Lohn, der reichlich lohnet.", "tokens": ["Ist", "Lohn", ",", "der", "reich\u00b7lich", "loh\u00b7net", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Doch darf ich bitten, bitt ich eins:", "tokens": ["Doch", "darf", "ich", "bit\u00b7ten", ",", "bitt", "ich", "eins", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "VVINF", "$,", "VVFIN", "PPER", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "La\u00df mir den besten Becher Weins", "tokens": ["La\u00df", "mir", "den", "bes\u00b7ten", "Be\u00b7cher", "Weins"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In purem Golde reichen.\u00ab", "tokens": ["In", "pu\u00b7rem", "Gol\u00b7de", "rei\u00b7chen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Er setzt' ihn an, er trank ihn aus:", "tokens": ["Er", "setzt'", "ihn", "an", ",", "er", "trank", "ihn", "aus", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbo Trank voll s\u00fc\u00dfer Labe!", "tokens": ["\u00bb", "o", "Trank", "voll", "s\u00fc\u00b7\u00dfer", "La\u00b7be", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O wohl dem hochbegl\u00fcckten Haus,", "tokens": ["O", "wohl", "dem", "hoch\u00b7be\u00b7gl\u00fcck\u00b7ten", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo das ist kleine Gabe!", "tokens": ["Wo", "das", "ist", "klei\u00b7ne", "Ga\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ergeht's euch wohl, so denkt an mich,", "tokens": ["Er\u00b7geht's", "euch", "wohl", ",", "so", "denkt", "an", "mich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "$,", "ADV", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und danket Gott so warm, als ich", "tokens": ["Und", "dan\u00b7ket", "Gott", "so", "warm", ",", "als", "ich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "NN", "ADV", "ADJD", "$,", "KOUS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "F\u00fcr diesen Trunk euch danke.\u00ab", "tokens": ["F\u00fcr", "die\u00b7sen", "Trunk", "euch", "dan\u00b7ke", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PDAT", "NN", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}