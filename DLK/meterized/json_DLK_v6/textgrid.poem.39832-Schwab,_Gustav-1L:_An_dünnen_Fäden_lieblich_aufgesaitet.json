{"textgrid.poem.39832": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: An d\u00fcnnen F\u00e4den lieblich aufgesaitet", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An d\u00fcnnen F\u00e4den lieblich aufgesaitet", "tokens": ["An", "d\u00fcn\u00b7nen", "F\u00e4\u00b7den", "lieb\u00b7lich", "auf\u00b7ge\u00b7sai\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "H\u00e4ngt eine Leier unter Blumenduft.", "tokens": ["H\u00e4ngt", "ei\u00b7ne", "Lei\u00b7er", "un\u00b7ter", "Blu\u00b7men\u00b7duft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es braust der Sturm hervor aus seiner Kluft,", "tokens": ["Es", "braust", "der", "Sturm", "her\u00b7vor", "aus", "sei\u00b7ner", "Kluft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Felder m\u00e4ht und mit den Eichen streitet.", "tokens": ["Der", "Fel\u00b7der", "m\u00e4ht", "und", "mit", "den", "Ei\u00b7chen", "strei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du schwache Leier, dir ist Tod bereitet,", "tokens": ["Du", "schwa\u00b7che", "Lei\u00b7er", ",", "dir", "ist", "Tod", "be\u00b7rei\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PPER", "VAFIN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie magst du trotzen keck in freier Luft?", "tokens": ["Wie", "magst", "du", "trot\u00b7zen", "keck", "in", "frei\u00b7er", "Luft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "VVFIN", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch horch, mit bangen Klaget\u00f6nen ruft", "tokens": ["Doch", "horch", ",", "mit", "ban\u00b7gen", "Kla\u00b7ge\u00b7t\u00f6\u00b7nen", "ruft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "$,", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sie schon dem Sturme, der gewaltig schreitet!", "tokens": ["Sie", "schon", "dem", "Stur\u00b7me", ",", "der", "ge\u00b7wal\u00b7tig", "schrei\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Jetzt r\u00fchrt er an die Saiten, voll erklingen", "tokens": ["Jetzt", "r\u00fchrt", "er", "an", "die", "Sai\u00b7ten", ",", "voll", "er\u00b7klin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und voller sie; doch ist nicht Flehn ihr Laut:", "tokens": ["Und", "vol\u00b7ler", "sie", ";", "doch", "ist", "nicht", "Flehn", "ihr", "Laut", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "PPER", "$.", "ADV", "VAFIN", "PTKNEG", "VVFIN", "PPER", "APPR", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein selig Brautlied singen sie dem Winde.", "tokens": ["Ein", "se\u00b7lig", "Braut\u00b7lied", "sin\u00b7gen", "sie", "dem", "Win\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "So wei\u00dft auch du des Mannes Sturm zu zwingen;", "tokens": ["So", "wei\u00dft", "auch", "du", "des", "Man\u00b7nes", "Sturm", "zu", "zwin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wild ist sein Hauch: doch l\u00f6set er gelinde", "tokens": ["Wild", "ist", "sein", "Hauch", ":", "doch", "l\u00f6\u00b7set", "er", "ge\u00b7lin\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "$.", "ADV", "VVFIN", "PPER", "ADJA"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "In deinen Ton sich auf, du zarte Braut!", "tokens": ["In", "dei\u00b7nen", "Ton", "sich", "auf", ",", "du", "zar\u00b7te", "Braut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}