{"dta.poem.8864": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XlIII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.85", "af:0.14"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn sie auch die Strenge tadelt, hat's die Liebe je", "tokens": ["Wenn", "sie", "auch", "die", "Stren\u00b7ge", "ta\u00b7delt", ",", "hat's", "die", "Lie\u00b7be", "je"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$,", "VAFIN", "ART", "NN", "ADV"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "La\u00dft das schelmische Get\u00e4ndel schmeicheln sich in eure", "tokens": ["La\u00dft", "das", "schel\u00b7mi\u00b7sche", "Ge\u00b7t\u00e4n\u00b7del", "schmei\u00b7cheln", "sich", "in", "eu\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ART", "ADJA", "NN", "VVFIN", "PRF", "APPR", "PPOSAT"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.3": {"text": "M\u00f6ge der Verstand es schelten, wenn das Herz euch nur", "tokens": ["M\u00f6\u00b7ge", "der", "Ver\u00b7stand", "es", "schel\u00b7ten", ",", "wenn", "das", "Herz", "euch", "nur"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "PPER", "ADV"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.4": {"text": "Dachtet ihr an weise Lehren, wenn das Liebchen euch", "tokens": ["Dach\u00b7tet", "ihr", "an", "wei\u00b7se", "Leh\u00b7ren", ",", "wenn", "das", "Lieb\u00b7chen", "euch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,", "KOUS", "ART", "NN", "PPER"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.5": {"text": "Fragtet ihr um Rath die Sitte, wenn ihr an den Rosen", "tokens": ["Frag\u00b7tet", "ihr", "um", "Rath", "die", "Sit\u00b7te", ",", "wenn", "ihr", "an", "den", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ART", "NN", "$,", "KOUS", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.6": {"text": "Andre Gaben w\u00fcrd' ich pflegen, wenn sie mir das Loos", "tokens": ["And\u00b7re", "Ga\u00b7ben", "w\u00fcrd'", "ich", "pfle\u00b7gen", ",", "wenn", "sie", "mir", "das", "Loos"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VAFIN", "PPER", "VVINF", "$,", "KOUS", "PPER", "PPER", "ART", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.7": {"text": "Doch nur Sch\u00f6nes sezt in Flammen meines Lebens", "tokens": ["Doch", "nur", "Sch\u00f6\u00b7nes", "sezt", "in", "Flam\u00b7men", "mei\u00b7nes", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJA", "ADV", "APPR", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.8": {"text": "Denn mir ward ein Sinn gegeben, den ich selbst mir", "tokens": ["Denn", "mir", "ward", "ein", "Sinn", "ge\u00b7ge\u00b7ben", ",", "den", "ich", "selbst", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "VVPP", "$,", "PRELS", "PPER", "ADV", "PPER"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Stolz und trotzig gegen Alles, doch vom Sch\u00f6nen unter-", "tokens": ["Stolz", "und", "trot\u00b7zig", "ge\u00b7gen", "Al\u00b7les", ",", "doch", "vom", "Sch\u00f6\u00b7nen", "un\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "APPR", "PIS", "$,", "ADV", "APPRART", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.10": {"text": "Das nur ist es, was mich fesselt, ob ich wandle durch", "tokens": ["Das", "nur", "ist", "es", ",", "was", "mich", "fes\u00b7selt", ",", "ob", "ich", "wand\u00b7le", "durch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "VAFIN", "PPER", "$,", "PWS", "PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "APPR"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.11": {"text": "Ob mir holde Blicke l\u00e4cheln, ob der Wein im Becher", "tokens": ["Ob", "mir", "hol\u00b7de", "Bli\u00b7cke", "l\u00e4\u00b7cheln", ",", "ob", "der", "Wein", "im", "Be\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVFIN", "$,", "KOUS", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.12": {"text": "Das nur ist's, wof\u00fcr ich athme, das nur, was mich", "tokens": ["Das", "nur", "ist's", ",", "wo\u00b7f\u00fcr", "ich", "ath\u00b7me", ",", "das", "nur", ",", "was", "mich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PDS", "ADV", "VAFIN", "$,", "PWAV", "PPER", "VVFIN", "$,", "PRELS", "ADV", "$,", "PWS", "PPER"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Wenn ich liebender Entsagung ehrenvolle K\u00e4mpfe focht.", "tokens": ["Wenn", "ich", "lie\u00b7ben\u00b7der", "Ent\u00b7sa\u00b7gung", "eh\u00b7ren\u00b7vol\u00b7le", "K\u00e4mp\u00b7fe", "focht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}