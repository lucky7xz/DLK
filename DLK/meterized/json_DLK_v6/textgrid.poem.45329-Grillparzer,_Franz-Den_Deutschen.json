{"textgrid.poem.45329": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Den Deutschen", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da eure Phantasie, verwildert,", "tokens": ["Da", "eu\u00b7re", "Phan\u00b7ta\u00b7sie", ",", "ver\u00b7wil\u00b7dert", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Statt zu bilden, denn doch nur bildert,", "tokens": ["Statt", "zu", "bil\u00b7den", ",", "denn", "doch", "nur", "bil\u00b7dert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,", "KON", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Und euer Verstand, wenn ihrs nicht ver\u00fcbelt,", "tokens": ["Und", "eu\u00b7er", "Ver\u00b7stand", ",", "wenn", "ihrs", "nicht", "ver\u00b7\u00fc\u00b7belt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "KOUS", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Statt zu denken vielmehr nur gr\u00fcbelt,", "tokens": ["Statt", "zu", "den\u00b7ken", "viel\u00b7mehr", "nur", "gr\u00fc\u00b7belt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Machen sie aus euch, was Menschen nie noch kannten,", "tokens": ["Ma\u00b7chen", "sie", "aus", "euch", ",", "was", "Men\u00b7schen", "nie", "noch", "kann\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "$,", "PWS", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Ein Monstrum von phantastischen Pedanten.", "tokens": ["Ein", "Monst\u00b7rum", "von", "phan\u00b7tas\u00b7ti\u00b7schen", "Pe\u00b7dan\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}