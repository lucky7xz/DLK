{"textgrid.poem.60679": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Esel, der die Haut des L\u00f6wen angelegt,", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Esel, der die Haut des L\u00f6wen angelegt,", "tokens": ["Der", "E\u00b7sel", ",", "der", "die", "Haut", "des", "L\u00f6\u00b7wen", "an\u00b7ge\u00b7legt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat Angst und Schrecken \u00fcberall erregt,", "tokens": ["Hat", "Angst", "und", "Schre\u00b7cken", "\u00fc\u00b7be\u00b7rall", "er\u00b7regt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und vielen tugendlosen Tieren gleich", "tokens": ["Und", "vie\u00b7len", "tu\u00b7gend\u00b7lo\u00b7sen", "Tie\u00b7ren", "gleich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Lie\u00df er erzittern rings das ganze Reich.", "tokens": ["Lie\u00df", "er", "er\u00b7zit\u00b7tern", "rings", "das", "gan\u00b7ze", "Reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein Zipfelchen von seinem Ohr", "tokens": ["Ein", "Zip\u00b7fel\u00b7chen", "von", "sei\u00b7nem", "Ohr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sah ungl\u00fccklicherweis hervor,", "tokens": ["Sah", "un\u00b7gl\u00fcck\u00b7li\u00b7cher\u00b7weis", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Verriet des Esels List, der anderen Versehen.", "tokens": ["Ver\u00b7riet", "des", "E\u00b7sels", "List", ",", "der", "an\u00b7de\u00b7ren", "Ver\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NE", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Schnell trat der Stock in seine Rechte.", "tokens": ["Schnell", "trat", "der", "Stock", "in", "sei\u00b7ne", "Rech\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Uneingeweihte staunten sehr und fragten,", "tokens": ["Un\u00b7ein\u00b7ge\u00b7weih\u00b7te", "staun\u00b7ten", "sehr", "und", "frag\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Wie k\u00f6nne das geschehen,", "tokens": ["Wie", "k\u00f6n\u00b7ne", "das", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PDS", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Da\u00df stockbewehrte Knechte", "tokens": ["Da\u00df", "stock\u00b7be\u00b7wehr\u00b7te", "Knech\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "L\u00f6wen zur M\u00fchle jagten?", "tokens": ["L\u00f6\u00b7wen", "zur", "M\u00fch\u00b7le", "jag\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "In Frankreich sind der L\u00e4rmer viel zu Haus,", "tokens": ["In", "Fran\u00b7kreich", "sind", "der", "L\u00e4r\u00b7mer", "viel", "zu", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "ART", "NN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf die dies Gleichnis angebracht:", "tokens": ["Auf", "die", "dies", "Gleich\u00b7nis", "an\u00b7ge\u00b7bracht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PDS", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dreiviertel ihres Wertes macht", "tokens": ["Drei\u00b7vier\u00b7tel", "ih\u00b7res", "Wer\u00b7tes", "macht"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Prunk und ihre Kleidung aus.", "tokens": ["Ihr", "Prunk", "und", "ih\u00b7re", "Klei\u00b7dung", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}