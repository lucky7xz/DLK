{"textgrid.poem.51205": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: An der Br\u00fccke zu Lausanne", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An der Br\u00fccke zu Lausanne", "tokens": ["An", "der", "Br\u00fc\u00b7cke", "zu", "Lau\u00b7san\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00e4ngt der Wappenschild von Waadt,", "tokens": ["H\u00e4ngt", "der", "Wap\u00b7pen\u00b7schild", "von", "Waadt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Darauf \u00bbVaterland und Freiheit\u00ab", "tokens": ["Da\u00b7rauf", "\u00bb", "Va\u00b7ter\u00b7land", "und", "Frei\u00b7heit", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "$(", "NN", "KON", "NN", "$("], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Froh das Volk geschrieben hat.", "tokens": ["Froh", "das", "Volk", "ge\u00b7schrie\u00b7ben", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Erzgegossen gl\u00e4nzt das Wappen,", "tokens": ["Erz\u00b7ge\u00b7gos\u00b7sen", "gl\u00e4nzt", "das", "Wap\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In der Sonne strahlt die Schrift;", "tokens": ["In", "der", "Son\u00b7ne", "strahlt", "die", "Schrift", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Also schrieb man in Helvetien,", "tokens": ["Al\u00b7so", "schrieb", "man", "in", "Hel\u00b7ve\u00b7ti\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "NE", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Und von Eisen war der Stift!", "tokens": ["Und", "von", "Ei\u00b7sen", "war", "der", "Stift", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sieh! im regen Br\u00fcckenwandel", "tokens": ["Sieh", "!", "im", "re\u00b7gen", "Br\u00fc\u00b7cken\u00b7wan\u00b7del"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "APPRART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Malet sich ein sch\u00f6nes Bild:", "tokens": ["Ma\u00b7let", "sich", "ein", "sch\u00f6\u00b7nes", "Bild", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Liebend hebt ein kleines Dirnchen", "tokens": ["Lie\u00b7bend", "hebt", "ein", "klei\u00b7nes", "Dirn\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seinen Bruder vor den Schild,", "tokens": ["Sei\u00b7nen", "Bru\u00b7der", "vor", "den", "Schild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lehrt ihn schreiben jene Worte:", "tokens": ["Lehrt", "ihn", "schrei\u00b7ben", "je\u00b7ne", "Wor\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "\u00bbfreiheit und das Vaterland!\u00ab", "tokens": ["\u00bb", "frei\u00b7heit", "und", "das", "Va\u00b7ter\u00b7land", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "KON", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und sie f\u00fchrt des Kn\u00e4bleins Finger", "tokens": ["Und", "sie", "f\u00fchrt", "des", "Kn\u00e4\u00b7bleins", "Fin\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mit der wenig gr\u00f6\u00dfern Hand.", "tokens": ["Mit", "der", "we\u00b7nig", "gr\u00f6\u00b7\u00dfern", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und sie lenkt den zarten Finger", "tokens": ["Und", "sie", "lenkt", "den", "zar\u00b7ten", "Fin\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Am Metall hinauf, hinab,", "tokens": ["Am", "Me\u00b7tall", "hin\u00b7auf", ",", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$,", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "An den sonndurchgl\u00fchten Zeichen,", "tokens": ["An", "den", "sonn\u00b7durch\u00b7gl\u00fch\u00b7ten", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die das gro\u00dfe Rom uns gab.", "tokens": ["Die", "das", "gro\u00b7\u00dfe", "Rom", "uns", "gab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NE", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und wie von der Kinder Locken", "tokens": ["Und", "wie", "von", "der", "Kin\u00b7der", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "ART", "NN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Gold in Gold zusammenflie\u00dft,", "tokens": ["Gold", "in", "Gold", "zu\u00b7sam\u00b7men\u00b7flie\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Von der Wangen Freudenr\u00f6te", "tokens": ["Von", "der", "Wan\u00b7gen", "Freu\u00b7den\u00b7r\u00f6\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ros' an Rose bl\u00fchend sprie\u00dft.", "tokens": ["Ros'", "an", "Ro\u00b7se", "bl\u00fc\u00b7hend", "sprie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NE", "VVPP", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aber auf derselben Br\u00fccke", "tokens": ["A\u00b7ber", "auf", "der\u00b7sel\u00b7ben", "Br\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Geht ein einsam fremder Mann,", "tokens": ["Geht", "ein", "ein\u00b7sam", "frem\u00b7der", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wandelt mit ergrautem Haare", "tokens": ["Wan\u00b7delt", "mit", "er\u00b7grau\u00b7tem", "Haa\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Still und k\u00fchl in Acht und Bann;", "tokens": ["Still", "und", "k\u00fchl", "in", "Acht", "und", "Bann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "APPR", "CARD", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Er gewahrt das Spiel der Kleinen,", "tokens": ["Er", "ge\u00b7wahrt", "das", "Spiel", "der", "Klei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Rascher flie\u00dft sogleich sein Blut,", "tokens": ["Ra\u00b7scher", "flie\u00dft", "sog\u00b7leich", "sein", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch um schmerzlich nur zu klagen", "tokens": ["Doch", "um", "schmerz\u00b7lich", "nur", "zu", "kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUI", "ADJD", "ADV", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Um verlornes h\u00f6chstes Gut:", "tokens": ["Um", "ver\u00b7lor\u00b7nes", "h\u00f6chs\u00b7tes", "Gut", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bbwelche Worte seh ich schreiben", "tokens": ["\u00bb", "wel\u00b7che", "Wor\u00b7te", "seh", "ich", "schrei\u00b7ben"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PWAT", "NN", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hier die Unschuld und das Gl\u00fcck!", "tokens": ["Hier", "die", "Un\u00b7schuld", "und", "das", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wehvoll wenden sie mein Sehnen,", "tokens": ["Weh\u00b7voll", "wen\u00b7den", "sie", "mein", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Frankenland! zu dir zur\u00fcck:", "tokens": ["Fran\u00b7ken\u00b7land", "!", "zu", "dir", "zu\u00b7r\u00fcck", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Was mir dort in Blut und Greuel,", "tokens": ["Was", "mir", "dort", "in", "Blut", "und", "Greu\u00b7el", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "APPR", "NN", "KON", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Im Verrat zusammenbrach,", "tokens": ["Im", "Ver\u00b7rat", "zu\u00b7sam\u00b7men\u00b7brach", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Lehret hier ein Kind das andre,", "tokens": ["Leh\u00b7ret", "hier", "ein", "Kind", "das", "and\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Singt der Vogel auf dem Dach!", "tokens": ["Singt", "der", "Vo\u00b7gel", "auf", "dem", "Dach", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ist denn euer Himmel blauer,", "tokens": ["Ist", "denn", "eu\u00b7er", "Him\u00b7mel", "blau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schweizer! goldner euer Korn?", "tokens": ["Schwei\u00b7zer", "!", "gold\u00b7ner", "eu\u00b7er", "Korn", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADJA", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sind denn lautrer eure Brunnen,", "tokens": ["Sind", "denn", "laut\u00b7rer", "eu\u00b7re", "Brun\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eure Rosen ohne Dorn?", "tokens": ["Eu\u00b7re", "Ro\u00b7sen", "oh\u00b7ne", "Dorn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gl\u00fcck und Unschuld, ach! sie bauen", "tokens": ["Gl\u00fcck", "und", "Un\u00b7schuld", ",", "ach", "!", "sie", "bau\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "ITJ", "$.", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wohl allein der Freiheit Reich!", "tokens": ["Wohl", "al\u00b7lein", "der", "Frei\u00b7heit", "Reich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "NN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}, "line.7": {"text": "Ob ihr schuldlos seid \u2013 nicht wei\u00df ich's \u2013", "tokens": ["Ob", "ihr", "schuld\u00b7los", "seid", "\u2013", "nicht", "wei\u00df", "ich's", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$(", "PTKNEG", "VVFIN", "PIS", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Doch gesegnet seh ich euch!\u00ab", "tokens": ["Doch", "ge\u00b7seg\u00b7net", "seh", "ich", "euch", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVPP", "VVFIN", "PPER", "PPER", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}