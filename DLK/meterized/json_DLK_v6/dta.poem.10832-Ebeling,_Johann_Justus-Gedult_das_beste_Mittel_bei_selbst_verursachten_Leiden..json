{"dta.poem.10832": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Gedult das beste Mittel  \n bei  \n selbst verursachten Leiden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198774", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Menschen klagen oft bei ihrem Leid\nund Schmerzen,", "tokens": ["Die", "Men\u00b7schen", "kla\u00b7gen", "oft", "bei", "ih\u00b7rem", "Leid", "und", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und murren \u00fcber ", "tokens": ["Und", "mur\u00b7ren", "\u00fc\u00b7ber"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "APPR"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Woher ihr Leiden r\u00fchrt. Der H\u00f6ch-", "tokens": ["Wo\u00b7her", "ihr", "Lei\u00b7den", "r\u00fchrt", ".", "Der", "H\u00f6ch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "$.", "ART", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von der empfundnen Noth, die Quell und Ursach", "tokens": ["Von", "der", "emp\u00b7fund\u00b7nen", "Noth", ",", "die", "Quell", "und", "Ur\u00b7sach"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "J\u00fcngst h\u00f6rt ich einen schrein: Er h\u00e4tt es an den", "tokens": ["J\u00fcngst", "h\u00f6rt", "ich", "ei\u00b7nen", "schrein", ":", "Er", "h\u00e4tt", "es", "an", "den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$.", "PPER", "VAFIN", "PPER", "APPR", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dies leidend Ungemach erwekt ihm Klag und Thr\u00e4-", "tokens": ["Dies", "lei\u00b7dend", "Un\u00b7ge\u00b7mach", "er\u00b7wekt", "ihm", "Klag", "und", "Thr\u00e4"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADJD", "NN", "VVFIN", "PPER", "NN", "KON", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich sprach woher kommt es; Freund! gib die Ur-", "tokens": ["Ich", "sprach", "wo\u00b7her", "kommt", "es", ";", "Freund", "!", "gib", "die", "Ur"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PWAV", "VVFIN", "PPER", "$.", "NN", "$.", "VVIMP", "ART", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Er sprach: Es qu\u00e4let mich ein faul und hohler", "tokens": ["Er", "sprach", ":", "Es", "qu\u00e4\u00b7let", "mich", "ein", "faul", "und", "hoh\u00b7ler"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "ART", "ADJD", "KON", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ich habe es vers\u00e4umt, da\u00df ich mit Tobaks Aschen,", "tokens": ["Ich", "ha\u00b7be", "es", "ver\u00b7s\u00e4umt", ",", "da\u00df", "ich", "mit", "To\u00b7baks", "A\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$,", "KOUS", "PPER", "APPR", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Nicht flei\u00dfig meinen Zahn gerieben und gewaschen.", "tokens": ["Nicht", "flei\u00b7\u00dfig", "mei\u00b7nen", "Zahn", "ge\u00b7rie\u00b7ben", "und", "ge\u00b7wa\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "PPOSAT", "NN", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Nun war die Antwort drauf: Du bist es selbsten", "tokens": ["Nun", "war", "die", "Ant\u00b7wort", "drauf", ":", "Du", "bist", "es", "selbs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKVZ", "$.", "PPER", "VAFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Was ist die Arzenei? Das Kr\u00e4utlein, die Ge-", "tokens": ["Was", "ist", "die", "Ar\u00b7ze\u00b7nei", "?", "Das", "Kr\u00e4ut\u00b7lein", ",", "die", "Ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "NN", "$.", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Es kommt das Uebel her von der entstandnen F\u00e4u-", "tokens": ["Es", "kommt", "das", "Ue\u00b7bel", "her", "von", "der", "ent\u00b7stand\u00b7nen", "F\u00e4u"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die von der Faulheit kommt; die selbst geschlagne", "tokens": ["Die", "von", "der", "Faul\u00b7heit", "kommt", ";", "die", "selbst", "ge\u00b7schlag\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$.", "ART", "ADV", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Ertr\u00e4gt man mit Gedult, bis da\u00df sie ist curirt:", "tokens": ["Er\u00b7tr\u00e4gt", "man", "mit", "Ge\u00b7dult", ",", "bis", "da\u00df", "sie", "ist", "cu\u00b7rirt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "NN", "$,", "KON", "KOUS", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Drum \u00fcbe solche aus, bis sich der Schmerz ver-", "tokens": ["Drum", "\u00fc\u00b7be", "sol\u00b7che", "aus", ",", "bis", "sich", "der", "Schmerz", "ver"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PIS", "PTKVZ", "$,", "KOUS", "PRF", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}