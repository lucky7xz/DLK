{"dta.poem.123": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als seine  Blondine  sehr kranckwurde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Es zeigen jetzt der Welt die Seuffzer und die Z\u00e4hren/", "tokens": ["Es", "zei\u00b7gen", "jetzt", "der", "Welt", "die", "Seuff\u00b7zer", "und", "die", "Z\u00e4h\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "KON", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df sich mein Canaan in ", "tokens": ["Da\u00df", "sich", "mein", "Ca\u00b7naan", "in"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Zucker vorger Lust wird jetzt zu Wermuth-Schleim/", "tokens": ["Das", "Zu\u00b7cker", "vor\u00b7ger", "Lust", "wird", "jetzt", "zu", "Wer\u00b7muth\u00b7Schleim", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "ADV", "APPR", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Vor spei\u00dfte mich das Gl\u00fcck mit gleissenden Pasteten/", "tokens": ["Vor", "spei\u00df\u00b7te", "mich", "das", "Gl\u00fcck", "mit", "gleis\u00b7sen\u00b7den", "Pas\u00b7te\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nun aber m\u00fcht es sich nur meine Lust zu t\u00f6dten/", "tokens": ["Nun", "a\u00b7ber", "m\u00fcht", "es", "sich", "nur", "mei\u00b7ne", "Lust", "zu", "t\u00f6d\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PRF", "ADV", "PPOSAT", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und zeigt den Drachen-Schwantz vor die ", "tokens": ["Und", "zeigt", "den", "Dra\u00b7chen\u00b7Schwantz", "vor", "die"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Indem es mir entzieht die Nahrung meiner Lust.", "tokens": ["In\u00b7dem", "es", "mir", "ent\u00b7zieht", "die", "Nah\u00b7rung", "mei\u00b7ner", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Es wird sein Freuden-Fest zu einer Marter-Wochen/", "tokens": ["Es", "wird", "sein", "Freu\u00b7den\u00b7Fest", "zu", "ei\u00b7ner", "Mar\u00b7ter\u00b7Wo\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und meine Sonn\u2019 hat sich in Wolcken gantz verkrochen", "tokens": ["Und", "mei\u00b7ne", "Sonn'", "hat", "sich", "in", "Wol\u00b7cken", "gantz", "ver\u00b7kro\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PRF", "APPR", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Hat es mir seinen Wein im Anfang gleich geschenckt/", "tokens": ["Hat", "es", "mir", "sei\u00b7nen", "Wein", "im", "An\u00b7fang", "gleich", "ge\u00b7schenckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "PPOSAT", "NN", "APPRART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Doch werd\u2019 ich nun mit Gifft und Schierlings-Safft getr\u00e4uckt/", "tokens": ["Doch", "werd'", "ich", "nun", "mit", "Gifft", "und", "Schier\u00b7lings\u00b7\u00b7S\u00b7afft", "ge\u00b7tr\u00e4uckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.13": {"text": "So schmeichelt mir das Gl\u00fcck mit falschen Judas-K\u00fcssen/", "tokens": ["So", "schmei\u00b7chelt", "mir", "das", "Gl\u00fcck", "mit", "fal\u00b7schen", "Ju\u00b7das\u00b7K\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und l\u00e4st mir Pilsen-Safft in seinem ", "tokens": ["Und", "l\u00e4st", "mir", "Pil\u00b7sen\u00b7\u00b7S\u00b7afft", "in", "sei\u00b7nem"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "NN", "APPR", "PPOSAT"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Es dr\u00fcckt mich an die Brust/ und stellt sich freundlich an", "tokens": ["Es", "dr\u00fcckt", "mich", "an", "die", "Brust", "/", "und", "stellt", "sich", "freund\u00b7lich", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$(", "KON", "VVFIN", "PRF", "ADJD", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Doch es die Klauen nicht dabey verbergen kan.", "tokens": ["Doch", "es", "die", "Klau\u00b7en", "nicht", "da\u00b7bey", "ver\u00b7ber\u00b7gen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "PTKNEG", "PAV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ich sehe den Betrug und f\u00fchle seine Klauen/", "tokens": ["Ich", "se\u00b7he", "den", "Be\u00b7trug", "und", "f\u00fch\u00b7le", "sei\u00b7ne", "Klau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die es in meine Brust so t\u00f6dtlich eingehauen/", "tokens": ["Die", "es", "in", "mei\u00b7ne", "Brust", "so", "t\u00f6dt\u00b7lich", "ein\u00b7ge\u00b7hau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Da\u00df mich der herbe Schmertz durch Wehmuth fast entseelt/", "tokens": ["Da\u00df", "mich", "der", "her\u00b7be", "Schmertz", "durch", "Weh\u00b7muth", "fast", "ent\u00b7seelt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Weil mein Vergn\u00fcgungs-Licht die letzte Stunde z\u00e4hlt.", "tokens": ["Weil", "mein", "Ver\u00b7gn\u00fc\u00b7gungs\u00b7Licht", "die", "letz\u00b7te", "Stun\u00b7de", "z\u00e4hlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ich k\u00fcsse auch den Tod wenn meine Sch\u00f6ne stirbet/", "tokens": ["Ich", "k\u00fcs\u00b7se", "auch", "den", "Tod", "wenn", "mei\u00b7ne", "Sch\u00f6\u00b7ne", "stir\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "KOUS", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Blondinens Untergang vor mich ein Grab erwirbet/", "tokens": ["Blon\u00b7di\u00b7nens", "Un\u00b7ter\u00b7gang", "vor", "mich", "ein", "Grab", "er\u00b7wir\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "PRF", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die Rosen wandeln sich in eine Dornen Bahn", "tokens": ["Die", "Ro\u00b7sen", "wan\u00b7deln", "sich", "in", "ei\u00b7ne", "Dor\u00b7nen", "Bahn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und vor Erg\u00f6tzlichkeit greifft mich ein Schaudern an.", "tokens": ["Und", "vor", "Er\u00b7g\u00f6tz\u00b7lich\u00b7keit", "greifft", "mich", "ein", "Schau\u00b7dern", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Das R\u00f6cheln ihrer Brust beklemmet mir das Hertze", "tokens": ["Das", "R\u00f6\u00b7cheln", "ih\u00b7rer", "Brust", "be\u00b7klem\u00b7met", "mir", "das", "Hert\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "PPER", "PDS", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ich f\u00fchle ebenfals die Pein/ ach herber Schmertze!", "tokens": ["Ich", "f\u00fch\u00b7le", "e\u00b7ben\u00b7fals", "die", "Pein", "/", "ach", "her\u00b7ber", "Schmert\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$(", "XY", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "So dieses Engels-Bild/ Ha Ungerechtigkeit!", "tokens": ["So", "die\u00b7ses", "En\u00b7gels\u00b7Bild", "/", "Ha", "Un\u00b7ge\u00b7rech\u00b7tig\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "$(", "NE", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Erb\u00e4rmlich t\u00f6dten will vor der gesetzten Zeit.", "tokens": ["Er\u00b7b\u00e4rm\u00b7lich", "t\u00f6d\u00b7ten", "will", "vor", "der", "ge\u00b7setz\u00b7ten", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "VMFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ach Himmel giebst du zn das Sterben dieser Sch\u00f6nen?", "tokens": ["Ach", "Him\u00b7mel", "giebst", "du", "zn", "das", "Ster\u00b7ben", "die\u00b7ser", "Sch\u00f6\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "VVFIN", "PPER", "NE", "ART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Kan denn kein Seuffzer nicht den harten Zorn vers\u00f6hnen.", "tokens": ["Kan", "denn", "kein", "Seuff\u00b7zer", "nicht", "den", "har\u00b7ten", "Zorn", "ver\u00b7s\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PIAT", "NN", "PTKNEG", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ach! zin\u00dft so mancher Mund die Farren gantz umsonst?", "tokens": ["Ach", "!", "zin\u00dft", "so", "man\u00b7cher", "Mund", "die", "Far\u00b7ren", "gantz", "um\u00b7sonst", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "ADV", "PIAT", "NN", "ART", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und finder keiner nicht vor deinem Throne Gunst?", "tokens": ["Und", "fin\u00b7der", "kei\u00b7ner", "nicht", "vor", "dei\u00b7nem", "Thro\u00b7ne", "Gunst", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PTKNEG", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Grausahmer Himmel/ ach! ach la\u00df dich doch erbitten!", "tokens": ["Grau\u00b7sah\u00b7mer", "Him\u00b7mel", "/", "ach", "!", "ach", "la\u00df", "dich", "doch", "er\u00b7bit\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$(", "XY", "$.", "XY", "VVIMP", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und h\u00f6re auf in Grimm die Hoffnung zu versch\u00fctten/", "tokens": ["Und", "h\u00f6\u00b7re", "auf", "in", "Grimm", "die", "Hoff\u00b7nung", "zu", "ver\u00b7sch\u00fct\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "APPR", "NE", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Schmei\u00df Donner-Keile her/ ihr Wolcken blitzt und kracht", "tokens": ["Schmei\u00df", "Don\u00b7ner\u00b7Kei\u00b7le", "her", "/", "ihr", "Wol\u00b7cken", "blitzt", "und", "kracht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "PTKVZ", "$(", "PPOSAT", "NN", "VVFIN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Gewaltig auf mich zu/ da\u00df nur ", "tokens": ["Ge\u00b7wal\u00b7tig", "auf", "mich", "zu", "/", "da\u00df", "nur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "APPR", "PPER", "PTKZU", "$(", "KOUS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Doch wo gedenckt ihr hin/ ihr Sorgen reiche Sinnen?", "tokens": ["Doch", "wo", "ge\u00b7denckt", "ihr", "hin", "/", "ihr", "Sor\u00b7gen", "rei\u00b7che", "Sin\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "PPER", "PTKVZ", "$(", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Was stosset ihr heraus/ was wollet ihr beginnen?", "tokens": ["Was", "stos\u00b7set", "ihr", "he\u00b7raus", "/", "was", "wol\u00b7let", "ihr", "be\u00b7gin\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PTKVZ", "$(", "PWS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Der Himmel ist gerecht/ verzaget nicht sogleich/", "tokens": ["Der", "Him\u00b7mel", "ist", "ge\u00b7recht", "/", "ver\u00b7za\u00b7get", "nicht", "sog\u00b7leich", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$(", "VVFIN", "PTKNEG", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Er wird vielleicht noch wol durch meine Seuffzer weich.", "tokens": ["Er", "wird", "viel\u00b7leicht", "noch", "wol", "durch", "mei\u00b7ne", "Seuff\u00b7zer", "weich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADV", "APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Was aber hilfft der Trost? einjeder will verzagen", "tokens": ["Was", "a\u00b7ber", "hilfft", "der", "Trost", "?", "ein\u00b7je\u00b7der", "will", "ver\u00b7za\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "ADV", "VVFIN", "ART", "NN", "$.", "ADV", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Den Himmel und das Gl\u00fcck k\u00f6nnt ihr mit recht verklagen", "tokens": ["Den", "Him\u00b7mel", "und", "das", "Gl\u00fcck", "k\u00f6nnt", "ihr", "mit", "recht", "ver\u00b7kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVFIN", "PPER", "APPR", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Auf/ fluchet dem Geschi