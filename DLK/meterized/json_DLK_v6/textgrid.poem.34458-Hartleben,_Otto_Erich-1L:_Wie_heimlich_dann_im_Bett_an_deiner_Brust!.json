{"textgrid.poem.34458": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie heimlich dann im Bett an deiner Brust!", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie heimlich dann im Bett an deiner Brust!", "tokens": ["Wie", "heim\u00b7lich", "dann", "im", "Bett", "an", "dei\u00b7ner", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "APPRART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Aus Morgentr\u00e4umen Arm in Arm erwacht,", "tokens": ["Aus", "Mor\u00b7gen\u00b7tr\u00e4u\u00b7men", "Arm", "in", "Arm", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "bestaunen wir den lustigen Sonnenstrahl,", "tokens": ["be\u00b7stau\u00b7nen", "wir", "den", "lus\u00b7ti\u00b7gen", "Son\u00b7nen\u00b7strahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "der keck zu solchen Heimlichkeiten drang.", "tokens": ["der", "keck", "zu", "sol\u00b7chen", "Heim\u00b7lich\u00b7kei\u00b7ten", "drang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Behaglich recken wir die schlafgest\u00e4rkten", "tokens": ["Be\u00b7hag\u00b7lich", "re\u00b7cken", "wir", "die", "schlaf\u00b7ge\u00b7st\u00e4rk\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "und schon von neuer Lust durchbebten Glieder,", "tokens": ["und", "schon", "von", "neu\u00b7er", "Lust", "durch\u00b7beb\u00b7ten", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "und selig l\u00e4chelnd schauen wir uns stumm", "tokens": ["und", "se\u00b7lig", "l\u00e4\u00b7chelnd", "schau\u00b7en", "wir", "uns", "stumm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJD", "VVFIN", "PPER", "PRF", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "in Augen, die der Schlaf noch kaum verliess.", "tokens": ["in", "Au\u00b7gen", ",", "die", "der", "Schlaf", "noch", "kaum", "ver\u00b7liess", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ART", "NN", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "O meine s\u00fcsse, weisse Hede, komm \u2013", "tokens": ["O", "mei\u00b7ne", "s\u00fcs\u00b7se", ",", "weis\u00b7se", "He\u00b7de", ",", "komm", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$,", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "lass deine Haare fliessen! Diese Spitzen \u2013", "tokens": ["lass", "dei\u00b7ne", "Haa\u00b7re", "flies\u00b7sen", "!", "Die\u00b7se", "Spit\u00b7zen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "VVINF", "$.", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "o lass mich \u2013 lass mich: du bist sch\u00f6ner so,", "tokens": ["o", "lass", "mich", "\u2013", "lass", "mich", ":", "du", "bist", "sch\u00f6\u00b7ner", "so", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM", "VVFIN", "PPER", "$(", "VVFIN", "PPER", "$.", "PPER", "VAFIN", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "und freier schweifen meine K\u00fcsse \u2013 ah!", "tokens": ["und", "frei\u00b7er", "schwei\u00b7fen", "mei\u00b7ne", "K\u00fcs\u00b7se", "\u2013", "ah", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN", "$(", "ITJ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Zieh deine H\u00e4nde von den Augen, Kind:", "tokens": ["Zieh", "dei\u00b7ne", "H\u00e4n\u00b7de", "von", "den", "Au\u00b7gen", ",", "Kind", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "was sch\u00e4mst du dich? Der Sonnenstrahl ist keusch \u2013", "tokens": ["was", "sch\u00e4mst", "du", "dich", "?", "Der", "Son\u00b7nen\u00b7strahl", "ist", "keusch", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "$.", "ART", "NN", "VAFIN", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}