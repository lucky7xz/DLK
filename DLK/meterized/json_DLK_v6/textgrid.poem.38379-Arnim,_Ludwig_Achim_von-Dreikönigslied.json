{"textgrid.poem.38379": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Dreik\u00f6nigslied", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott so wollen wir loben und ehrn,", "tokens": ["Gott", "so", "wol\u00b7len", "wir", "lo\u00b7ben", "und", "ehrn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VMFIN", "PPER", "VVINF", "KON", "VVINF", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Die heiligen drei K\u00f6nig mit ihrem Stern,", "tokens": ["Die", "hei\u00b7li\u00b7gen", "drei", "K\u00f6\u00b7nig", "mit", "ih\u00b7rem", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "CARD", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sie reiten daher in aller Eil", "tokens": ["Sie", "rei\u00b7ten", "da\u00b7her", "in", "al\u00b7ler", "Eil"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PAV", "APPR", "PIAT", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In dreisig Tagen vierhundert Meil,", "tokens": ["In", "drei\u00b7sig", "Ta\u00b7gen", "vier\u00b7hun\u00b7dert", "Meil", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "CARD", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Sie kamen in Herodis Haus,", "tokens": ["Sie", "ka\u00b7men", "in", "He\u00b7ro\u00b7dis", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Herodes sahe zum Fenster raus:", "tokens": ["He\u00b7ro\u00b7des", "sa\u00b7he", "zum", "Fens\u00b7ter", "raus", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Ihr meine liebe Herrn, wo wollt ihr hin?", "tokens": ["Ihr", "mei\u00b7ne", "lie\u00b7be", "Herrn", ",", "wo", "wollt", "ihr", "hin", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "$,", "PWAV", "VMFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Nach Bethlehem steht unser Sinn.", "tokens": ["Nach", "Beth\u00b7le\u00b7hem", "steht", "un\u00b7ser", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Da ist geboren ohn' alles Leid", "tokens": ["Da", "ist", "ge\u00b7bo\u00b7ren", "ohn'", "al\u00b7les", "Leid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "VVPP", "APPR", "PIAT", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Ein Kindlein von einer reinen Maid.", "tokens": ["Ein", "Kin\u00b7dlein", "von", "ei\u00b7ner", "rei\u00b7nen", "Maid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Herodes sprach aus gro\u00dfem Trotz:", "tokens": ["He\u00b7ro\u00b7des", "sprach", "aus", "gro\u00b7\u00dfem", "Trotz", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Ey warum ist der hinder so schwarz?", "tokens": ["Ey", "wa\u00b7rum", "ist", "der", "hin\u00b7der", "so", "schwarz", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWAV", "VAFIN", "ART", "ADV", "ADV", "ADJD", "$."], "meter": "---+-+--+", "measure": "iambic.tri.chol"}, "line.13": {"text": "O lieber Herr, er ist uns wohl bekannt,", "tokens": ["O", "lie\u00b7ber", "Herr", ",", "er", "ist", "uns", "wohl", "be\u00b7kannt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "NN", "$,", "PPER", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Er ist ein K\u00f6nig im Mohrenland,", "tokens": ["Er", "ist", "ein", "K\u00f6\u00b7nig", "im", "Moh\u00b7ren\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Und w\u00f6llend ihr uns recht erkennen,", "tokens": ["Und", "w\u00f6l\u00b7lend", "ihr", "uns", "recht", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Wir d\u00f6rffend uns gar wohl nennen.", "tokens": ["Wir", "d\u00f6rf\u00b7fend", "uns", "gar", "wohl", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.17": {"text": "Wir seynd die K\u00f6nig vom finstern Stern,", "tokens": ["Wir", "seynd", "die", "K\u00f6\u00b7nig", "vom", "fins\u00b7tern", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Und br\u00e4chten dem Kindlein ein Opfer gern,", "tokens": ["Und", "br\u00e4ch\u00b7ten", "dem", "Kin\u00b7dlein", "ein", "Op\u00b7fer", "gern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.19": {"text": "Myrrhen, Weihrauch und rothes Gold,", "tokens": ["Myr\u00b7rhen", ",", "Weih\u00b7rauch", "und", "ro\u00b7thes", "Gold", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.20": {"text": "Wir seynd dem Kindlein ins Herz nein hold.", "tokens": ["Wir", "seynd", "dem", "Kin\u00b7dlein", "ins", "Herz", "nein", "hold", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPRART", "NN", "PTKANT", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "Herodes sprach aus Uebermuth,", "tokens": ["He\u00b7ro\u00b7des", "sprach", "aus", "Ue\u00b7ber\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Bleibend bei mir, und nehmt f\u00fcr gut,", "tokens": ["Blei\u00b7bend", "bei", "mir", ",", "und", "nehmt", "f\u00fcr", "gut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPER", "$,", "KON", "VVFIN", "APPR", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.23": {"text": "Ich will euch geben Heu und Streu,", "tokens": ["Ich", "will", "euch", "ge\u00b7ben", "Heu", "und", "Streu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Ich will euch halten Zehrung frey.", "tokens": ["Ich", "will", "euch", "hal\u00b7ten", "Zeh\u00b7rung", "frey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Die heiligen drei K\u00f6nig th\u00e4ten sich besinnen,", "tokens": ["Die", "hei\u00b7li\u00b7gen", "drei", "K\u00f6\u00b7nig", "th\u00e4\u00b7ten", "sich", "be\u00b7sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "CARD", "NN", "VVFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "F\u00fcrwahr, wir wollen jezt von hinnen.", "tokens": ["F\u00fcr\u00b7wahr", ",", "wir", "wol\u00b7len", "jezt", "von", "hin\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VMFIN", "ADV", "APPR", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Herodes sprach aus trutzigem Sinn,", "tokens": ["He\u00b7ro\u00b7des", "sprach", "aus", "trut\u00b7zi\u00b7gem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.28": {"text": "Wollt ihr nicht bleiben, so fahret hin.", "tokens": ["Wollt", "ihr", "nicht", "blei\u00b7ben", ",", "so", "fah\u00b7ret", "hin", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVINF", "$,", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.29": {"text": "Sie zogen \u00fcber den Berg hinaus,", "tokens": ["Sie", "zo\u00b7gen", "\u00fc\u00b7ber", "den", "Berg", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APZR", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.30": {"text": "Sie funden den Stern ob dem Haus,", "tokens": ["Sie", "fun\u00b7den", "den", "Stern", "ob", "dem", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KOUS", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.31": {"text": "Sie traten in das Haus hinein,", "tokens": ["Sie", "tra\u00b7ten", "in", "das", "Haus", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Sie funden Jesum in dem Krippelein.", "tokens": ["Sie", "fun\u00b7den", "Je\u00b7sum", "in", "dem", "Krip\u00b7pe\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Sie gaben ihm ein reichen Sold,", "tokens": ["Sie", "ga\u00b7ben", "ihm", "ein", "rei\u00b7chen", "Sold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Myrrhen, Weyhrauch und rothes Gold.", "tokens": ["Myr\u00b7rhen", ",", "Wey\u00b7hrauch", "und", "ro\u00b7thes", "Gold", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.35": {"text": "Joseph bei dem Kripplein sa\u00df,", "tokens": ["Jo\u00b7se\u00b7ph", "bei", "dem", "Krip\u00b7plein", "sa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.36": {"text": "Bis da\u00df er schier erfroren was.", "tokens": ["Bis", "da\u00df", "er", "schier", "er\u00b7fro\u00b7ren", "was", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "ADJD", "VVFIN", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Joseph nahm ein Pf\u00e4nnelein,", "tokens": ["Jo\u00b7se\u00b7ph", "nahm", "ein", "Pf\u00e4n\u00b7nel\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.38": {"text": "Und macht dem Kind ein M\u00fc\u00dfelein.", "tokens": ["Und", "macht", "dem", "Kind", "ein", "M\u00fc\u00b7\u00dfe\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Joseph, der zog seine H\u00f6selein aus,", "tokens": ["Jo\u00b7se\u00b7ph", ",", "der", "zog", "sei\u00b7ne", "H\u00f6\u00b7se\u00b7lein", "aus", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.40": {"text": "Und macht dem Kindlein zwey Windelein d'raus.", "tokens": ["Und", "macht", "dem", "Kin\u00b7dlein", "zwey", "Win\u00b7de\u00b7lein", "d'raus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "CARD", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.41": {"text": "Joseph, lieber Joseph mein,", "tokens": ["Jo\u00b7se\u00b7ph", ",", "lie\u00b7ber", "Jo\u00b7se\u00b7ph", "mein", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "NE", "PPOSAT", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.42": {"text": "Hilf mir wiegen mein Kindelein.", "tokens": ["Hilf", "mir", "wie\u00b7gen", "mein", "Kin\u00b7de\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.43": {"text": "Es waren da zwey unvern\u00fcnftige Thier,", "tokens": ["Es", "wa\u00b7ren", "da", "zwey", "un\u00b7ver\u00b7n\u00fcnf\u00b7ti\u00b7ge", "Thier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.44": {"text": "Sie fielen nieder auf ihre Knie.", "tokens": ["Sie", "fie\u00b7len", "nie\u00b7der", "auf", "ih\u00b7re", "Knie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.45": {"text": "Das Oechselein und das Eselein,", "tokens": ["Das", "O\u00b7ech\u00b7sel\u00b7ein", "und", "das", "E\u00b7sel\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.46": {"text": "Die kannten Gott den Herren rein.", "tokens": ["Die", "kann\u00b7ten", "Gott", "den", "Her\u00b7ren", "rein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}