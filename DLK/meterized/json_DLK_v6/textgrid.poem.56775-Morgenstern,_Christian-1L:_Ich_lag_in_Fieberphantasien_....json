{"textgrid.poem.56775": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich lag in Fieberphantasien ...", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich lag in Fieberphantasien ...", "tokens": ["Ich", "lag", "in", "Fie\u00b7ber\u00b7phan\u00b7ta\u00b7si\u00b7en", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Aus allen Ecken wuchs es her ...", "tokens": ["Aus", "al\u00b7len", "E\u00b7cken", "wuchs", "es", "her", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wohin ich sah, ich sah nur Ihn,", "tokens": ["Wo\u00b7hin", "ich", "sah", ",", "ich", "sah", "nur", "Ihn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "wohin ich tastete, war Er ...", "tokens": ["wo\u00b7hin", "ich", "tas\u00b7te\u00b7te", ",", "war", "Er", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die T\u00fccher, die Tapeten liehn", "tokens": ["Die", "T\u00fc\u00b7cher", ",", "die", "Ta\u00b7pe\u00b7ten", "liehn"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "ihm ihrer Muster Fratzenmeer ...", "tokens": ["ihm", "ih\u00b7rer", "Mus\u00b7ter", "Frat\u00b7zen\u00b7meer", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und schlo\u00df ich fest die Lider, schien", "tokens": ["Und", "schlo\u00df", "ich", "fest", "die", "Li\u00b7der", ",", "schien"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "PPER", "VVFIN", "ART", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "sein Aug' in meines weit und leer.", "tokens": ["sein", "Aug'", "in", "mei\u00b7nes", "weit", "und", "leer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein Opfer wilder Bilderreihn", "tokens": ["Ein", "Op\u00b7fer", "wil\u00b7der", "Bil\u00b7der\u00b7reihn"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "entschlief ich endlich. Mich umspann,", "tokens": ["ent\u00b7schlief", "ich", "end\u00b7lich", ".", "Mich", "um\u00b7spann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$.", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "mich spornte rittlings sein Gebein", "tokens": ["mich", "sporn\u00b7te", "ritt\u00b7lings", "sein", "Ge\u00b7bein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "durch Felsenw\u00fcsten glutwindan ...", "tokens": ["durch", "Fel\u00b7sen\u00b7w\u00fcs\u00b7ten", "glut\u00b7win\u00b7dan", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NE", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Verzehrend fra\u00df sein Frost sich ein,", "tokens": ["Ver\u00b7zeh\u00b7rend", "fra\u00df", "sein", "Frost", "sich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPOSAT", "NN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "indes mich Blutschwei\u00df \u00fcberrann,", "tokens": ["in\u00b7des", "mich", "Blut\u00b7schwei\u00df", "\u00fc\u00b7ber\u00b7rann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "und auf Ger\u00f6ll und spitzem Stein", "tokens": ["und", "auf", "Ge\u00b7r\u00f6ll", "und", "spit\u00b7zem", "Stein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "der wunde Fu\u00df nicht Weg gewann.", "tokens": ["der", "wun\u00b7de", "Fu\u00df", "nicht", "Weg", "ge\u00b7wann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch nicht ein Fristchen durft' ich ruhn.", "tokens": ["Doch", "nicht", "ein", "Frist\u00b7chen", "durft'", "ich", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbwir m\u00fcssen\u00ab \u2013 stachelte sein Hohn \u2013", "tokens": ["\u00bb", "wir", "m\u00fcs\u00b7sen", "\u00ab", "\u2013", "sta\u00b7chel\u00b7te", "sein", "Hohn", "\u2013"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "$(", "$(", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbzum Richter \u00fcber all dein Tun,", "tokens": ["\u00bb", "zum", "Rich\u00b7ter", "\u00fc\u00b7ber", "all", "dein", "Tun", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPRART", "NN", "APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "der Weg ist weit nach seinem Thron.", "tokens": ["der", "Weg", "ist", "weit", "nach", "sei\u00b7nem", "Thron", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gebucht, in klaftertiefen Truhn,", "tokens": ["Ge\u00b7bucht", ",", "in", "klaf\u00b7ter\u00b7tie\u00b7fen", "Truhn", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "erharrt dich dort, wof\u00fcr dich Lohn", "tokens": ["er\u00b7harrt", "dich", "dort", ",", "wo\u00b7f\u00fcr", "dich", "Lohn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PWAV", "PPER", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "und Strafe wird ereilen nun:", "tokens": ["und", "Stra\u00b7fe", "wird", "er\u00b7ei\u00b7len", "nun", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "VVINF", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Bereite dich, verlorner Sohn!\u00ab", "tokens": ["Be\u00b7rei\u00b7te", "dich", ",", "ver\u00b7lor\u00b7ner", "Sohn", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da ging die Stubent\u00fcr, und leis", "tokens": ["Da", "ging", "die", "Stu\u00b7ben\u00b7t\u00fcr", ",", "und", "leis"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "umklang mein Bett ein sanfter Schritt,", "tokens": ["um\u00b7klang", "mein", "Bett", "ein", "sanf\u00b7ter", "Schritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und eines Stirnbands k\u00fchlend Eis", "tokens": ["und", "ei\u00b7nes", "Stirn\u00b7bands", "k\u00fch\u00b7lend", "Eis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "erl\u00f6ste mich vom grausen Ritt.", "tokens": ["er\u00b7l\u00f6s\u00b7te", "mich", "vom", "grau\u00b7sen", "Ritt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch ehe noch ein Wort dem Kreis", "tokens": ["Doch", "e\u00b7he", "noch", "ein", "Wort", "dem", "Kreis"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "der Wirrgedanken sich entstritt,", "tokens": ["der", "Wirr\u00b7ge\u00b7dan\u00b7ken", "sich", "ent\u00b7stritt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "verschob schon wieder sich das Gleis", "tokens": ["ver\u00b7schob", "schon", "wie\u00b7der", "sich", "das", "Gleis"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "PRF", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "und neuer Traumgang ri\u00df mich mit.", "tokens": ["und", "neu\u00b7er", "Traum\u00b7gang", "ri\u00df", "mich", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wie anders aber war das Bild,", "tokens": ["Wie", "an\u00b7ders", "a\u00b7ber", "war", "das", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "das nun mein Fiebergeist entband!", "tokens": ["das", "nun", "mein", "Fie\u00b7berg\u00b7eist", "ent\u00b7band", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein liebster Freund umfing mich mild", "tokens": ["Mein", "liebs\u00b7ter", "Freund", "um\u00b7fing", "mich", "mild"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und hob mich von des Lagers Rand.", "tokens": ["und", "hob", "mich", "von", "des", "La\u00b7gers", "Rand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Aus Zweigen harrte mein ein Schild:", "tokens": ["Aus", "Zwei\u00b7gen", "harr\u00b7te", "mein", "ein", "Schild", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPOSAT", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Drauf trug mich vierer Fremden Hand", "tokens": ["Drauf", "trug", "mich", "vie\u00b7rer", "Frem\u00b7den", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "wie ein erbeutet Edelwild", "tokens": ["wie", "ein", "er\u00b7beu\u00b7tet", "E\u00b7del\u00b7wild"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "VVPP", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "hinaus ins sommerliche Land.", "tokens": ["hin\u00b7aus", "ins", "som\u00b7mer\u00b7li\u00b7che", "Land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wer waren sie? wo lief ihr Pfad?", "tokens": ["Wer", "wa\u00b7ren", "sie", "?", "wo", "lief", "ihr", "Pfad", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$.", "PWAV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie st\u00fcrmten voll erhabner Wucht ...", "tokens": ["Sie", "st\u00fcrm\u00b7ten", "voll", "er\u00b7hab\u00b7ner", "Wucht", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "bis, wo ein L\u00e4rm vollbrachter Mahd", "tokens": ["bis", ",", "wo", "ein", "L\u00e4rm", "voll\u00b7brach\u00b7ter", "Mahd"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "herklang aus stiller Waldesbucht.", "tokens": ["her\u00b7klang", "aus", "stil\u00b7ler", "Wal\u00b7des\u00b7bucht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Noch rollte hoch das Sonnenrad,", "tokens": ["Noch", "roll\u00b7te", "hoch", "das", "Son\u00b7nen\u00b7rad", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "doch schon geschnitten lag die Frucht;", "tokens": ["doch", "schon", "ge\u00b7schnit\u00b7ten", "lag", "die", "Frucht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "denn Wolken drohten Blitz und Bad:", "tokens": ["denn", "Wol\u00b7ken", "droh\u00b7ten", "Blitz", "und", "Bad", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "NN", "KON", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und alles war schon helle Flucht.", "tokens": ["Und", "al\u00b7les", "war", "schon", "hel\u00b7le", "Flucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Dort setzten sie aufs hohe Korn", "tokens": ["Dort", "setz\u00b7ten", "sie", "aufs", "ho\u00b7he", "Korn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "die Bahre ab. Noch stand sie nicht:", "tokens": ["die", "Bah\u00b7re", "ab", ".", "Noch", "stand", "sie", "nicht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$.", "ADV", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da scho\u00df schon goldner Wetterzorn:", "tokens": ["Da", "scho\u00df", "schon", "gold\u00b7ner", "Wet\u00b7ter\u00b7zorn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Glutsto\u00df stob die \u00c4hrenschicht.", "tokens": ["Ein", "Glut\u00b7sto\u00df", "stob", "die", "\u00c4h\u00b7ren\u00b7schicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mein Herz stand still vom scharfen Dorn.", "tokens": ["Mein", "Herz", "stand", "still", "vom", "schar\u00b7fen", "Dorn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es sank der Erde h\u00f6chst Gedicht,", "tokens": ["Es", "sank", "der", "Er\u00b7de", "h\u00f6chst", "Ge\u00b7dicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "der Mensch, zur\u00fcck in ihren Born,", "tokens": ["der", "Mensch", ",", "zu\u00b7r\u00fcck", "in", "ih\u00b7ren", "Born", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "als Asche, Wasser, Luft und Licht.", "tokens": ["als", "A\u00b7sche", ",", "Was\u00b7ser", ",", "Luft", "und", "Licht", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}