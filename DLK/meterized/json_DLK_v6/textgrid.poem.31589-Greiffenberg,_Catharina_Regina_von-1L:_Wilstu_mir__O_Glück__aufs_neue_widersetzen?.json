{"textgrid.poem.31589": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wilstu mir / O Gl\u00fcck / aufs neue widersetzen?", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wilstu mir / O Gl\u00fcck / aufs neue widersetzen?", "tokens": ["Wils\u00b7tu", "mir", "/", "O", "Gl\u00fcck", "/", "aufs", "neu\u00b7e", "wi\u00b7der\u00b7set\u00b7zen", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$(", "NE", "NN", "$(", "APPRART", "ADJA", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "der Anfang ist schon recht / auf alte Feindsal-Wei\u00df.", "tokens": ["der", "An\u00b7fang", "ist", "schon", "recht", "/", "auf", "al\u00b7te", "Feind\u00b7sa\u00b7lWei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$(", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "denkst zu besiegen mich? es ist umsonst dein Flei\u00df.", "tokens": ["denkst", "zu", "be\u00b7sie\u00b7gen", "mich", "?", "es", "ist", "um\u00b7sonst", "dein", "Flei\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "PPER", "$.", "PPER", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "die Tugend l\u00e4st sich / auch gedrucket / nicht verletzen!", "tokens": ["die", "Tu\u00b7gend", "l\u00e4st", "sich", "/", "auch", "ge\u00b7dru\u00b7cket", "/", "nicht", "ver\u00b7let\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$(", "ADV", "VVPP", "$(", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wann deiner qualen Heer die Degen auf sie wetzen /", "tokens": ["Wann", "dei\u00b7ner", "qua\u00b7len", "Heer", "die", "De\u00b7gen", "auf", "sie", "wet\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "ART", "NN", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "wann du mich schon umgiebst mit engem Aengsten-Krei\u00df:", "tokens": ["wann", "du", "mich", "schon", "um\u00b7giebst", "mit", "en\u00b7gem", "A\u00b7engs\u00b7ten\u00b7Krei\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADV", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-----+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "will Tugend-tapffer ich erhalten doch den Prei\u00df.", "tokens": ["will", "Tu\u00b7gen\u00b7dtapf\u00b7fer", "ich", "er\u00b7hal\u00b7ten", "doch", "den", "Prei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ein schwer-erlangter Sieg kan doppelt-hoch ergetzen.", "tokens": ["Ein", "schwer\u00b7er\u00b7lang\u00b7ter", "Sieg", "kan", "dop\u00b7pel\u00b7thoch", "er\u00b7get\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Vergie\u00df' ich weises Blut; die Thr\u00e4nen trennen nicht", "tokens": ["Ver\u00b7gie\u00df'", "ich", "wei\u00b7ses", "Blut", ";", "die", "Thr\u00e4\u00b7nen", "tren\u00b7nen", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADJA", "NN", "$.", "ART", "NN", "VVFIN", "PTKNEG"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "vom Tugend-Rennen ab / sie seynd vielmehr die Sporen /", "tokens": ["vom", "Tu\u00b7gen\u00b7dRen\u00b7nen", "ab", "/", "sie", "seynd", "viel\u00b7mehr", "die", "Spo\u00b7ren", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$(", "PPER", "VAFIN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "dadurch ein traurigs Aug das Helden-Herz ansticht.", "tokens": ["da\u00b7durch", "ein", "trau\u00b7rigs", "Aug", "das", "Hel\u00b7den\u00b7Herz", "an\u00b7sticht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Sie werden zu Entsatz der Herzen-B\u00fcrd / gebohren /", "tokens": ["Sie", "wer\u00b7den", "zu", "Ent\u00b7satz", "der", "Her\u00b7zen\u00b7B\u00fcrd", "/", "ge\u00b7boh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "ART", "NN", "$(", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "das so entlastet dann viel mehr mit Ehr verricht.", "tokens": ["das", "so", "ent\u00b7las\u00b7tet", "dann", "viel", "mehr", "mit", "Ehr", "ver\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "ADV", "ADV", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Bekriegst mich auf das neu / so hast aufs neu verlohren.", "tokens": ["Be\u00b7kriegst", "mich", "auf", "das", "neu", "/", "so", "hast", "aufs", "neu", "ver\u00b7loh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "ADJD", "$(", "ADV", "VAFIN", "APPRART", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}