{"textgrid.poem.57038": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Die Geruchs-Orgel", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Palmstr\u00f6m baut sich eine Geruchs-Orgel", "tokens": ["Palm\u00b7str\u00f6m", "baut", "sich", "ei\u00b7ne", "Ge\u00b7ruchs\u00b7Or\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PRF", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "und spielt drauf v. Korfs Nie\u00dfwurz-Sonate.", "tokens": ["und", "spielt", "drauf", "v.", "Korfs", "Nie\u00df\u00b7wurz\u00b7So\u00b7na\u00b7te", "."], "token_info": ["word", "word", "word", "abbreviation", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "APPR", "NE", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Diese beginnt mit Alpenkr\u00e4uter-Triolen", "tokens": ["Die\u00b7se", "be\u00b7ginnt", "mit", "Al\u00b7pen\u00b7kr\u00e4u\u00b7ter\u00b7Tri\u00b7o\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "NN"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "und erfreut durch eine Akazien-Arie.", "tokens": ["und", "er\u00b7freut", "durch", "ei\u00b7ne", "A\u00b7ka\u00b7zi\u00b7en\u00b7\u00b7A\u00b7rie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch im Scherzo, pl\u00f6tzlich und unerwartet,", "tokens": ["Doch", "im", "Scher\u00b7zo", ",", "pl\u00f6tz\u00b7lich", "und", "un\u00b7er\u00b7war\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "zwischen Tuberosen und Eukalyptus,", "tokens": ["zwi\u00b7schen", "Tu\u00b7be\u00b7ro\u00b7sen", "und", "Eu\u00b7ka\u00b7lyp\u00b7tus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NE", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "folgen die drei ber\u00fchmten Nie\u00dfwurz-Stellen,", "tokens": ["fol\u00b7gen", "die", "drei", "be\u00b7r\u00fchm\u00b7ten", "Nie\u00df\u00b7wurz\u00b7Stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "CARD", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "welche der Sonate den Namen geben.", "tokens": ["wel\u00b7che", "der", "So\u00b7na\u00b7te", "den", "Na\u00b7men", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}}, "stanza.5": {"line.1": {"text": "Palmstr\u00f6m f\u00e4llt bei diesen Ha-Cis-Synkopen", "tokens": ["Palm\u00b7str\u00f6m", "f\u00e4llt", "bei", "die\u00b7sen", "Ha\u00b7Cis\u00b7Syn\u00b7ko\u00b7pen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "jedesmal beinahe vom Sessel, w\u00e4hrend", "tokens": ["je\u00b7des\u00b7mal", "bei\u00b7na\u00b7he", "vom", "Ses\u00b7sel", ",", "w\u00e4h\u00b7rend"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ADV", "APPRART", "NN", "$,", "KOUS"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}}, "stanza.6": {"line.1": {"text": "Korf daheim, am sichern Schreibtisch sitzend,", "tokens": ["Korf", "da\u00b7heim", ",", "am", "si\u00b7chern", "Schreib\u00b7tisch", "sit\u00b7zend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Opus hinter Opus aufs Papier wirft ...", "tokens": ["O\u00b7pus", "hin\u00b7ter", "O\u00b7pus", "aufs", "Pa\u00b7pier", "wirft", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NE", "APPRART", "NN", "VVFIN", "$("], "meter": "+-+-++-+-+", "measure": "unknown.measure.hexa"}}}}}