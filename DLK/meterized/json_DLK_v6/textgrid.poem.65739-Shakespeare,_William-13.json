{"textgrid.poem.65739": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "13", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O w\u00e4rest du dein eigen! Aber du", "tokens": ["O", "w\u00e4\u00b7rest", "du", "dein", "ei\u00b7gen", "!", "A\u00b7ber", "du"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "PPOSAT", "ADJD", "$.", "KON", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Geh\u00f6rest nur in diesem Leben dir.", "tokens": ["Ge\u00b7h\u00f6\u00b7rest", "nur", "in", "die\u00b7sem", "Le\u00b7ben", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PDAT", "NN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Darum bereit' auf diesen Schlu\u00df dich zu,", "tokens": ["Da\u00b7rum", "be\u00b7reit'", "auf", "die\u00b7sen", "Schlu\u00df", "dich", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "APPR", "PDAT", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gib einem andern deine holde Zier.", "tokens": ["Gib", "ei\u00b7nem", "an\u00b7dern", "dei\u00b7ne", "hol\u00b7de", "Zier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So geht der Sch\u00f6nheit Lehen, das du hast,", "tokens": ["So", "geht", "der", "Sch\u00f6n\u00b7heit", "Le\u00b7hen", ",", "das", "du", "hast", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$,", "PRELS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Zu Ende nicht: du bist derselbe wieder,", "tokens": ["Zu", "En\u00b7de", "nicht", ":", "du", "bist", "der\u00b7sel\u00b7be", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKNEG", "$.", "PPER", "VAFIN", "PDAT", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wenn sich dein sch\u00f6nes Bild, nachdem du selbst erbla\u00dft,", "tokens": ["Wenn", "sich", "dein", "sch\u00f6\u00b7nes", "Bild", ",", "nach\u00b7dem", "du", "selbst", "er\u00b7bla\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "ADJA", "NN", "$,", "KOUS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Einst fortgepflanzt auf sch\u00f6ne Kindesglieder,", "tokens": ["Einst", "fort\u00b7ge\u00b7pflanzt", "auf", "sch\u00f6\u00b7ne", "Kin\u00b7des\u00b7glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Wer lie\u00df verfallen ein so edles Haus,", "tokens": ["Wer", "lie\u00df", "ver\u00b7fal\u00b7len", "ein", "so", "ed\u00b7les", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "VVINF", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Das Wirtlichkeit in Ehren halten k\u00f6nnte,", "tokens": ["Das", "Wirt\u00b7lich\u00b7keit", "in", "Eh\u00b7ren", "hal\u00b7ten", "k\u00f6nn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Gesichert gegen Wintersturmes Graus", "tokens": ["Ge\u00b7si\u00b7chert", "ge\u00b7gen", "Win\u00b7ter\u00b7stur\u00b7mes", "Graus"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "APPR", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und ew'gen Todeskampf der Elemente?", "tokens": ["Und", "ew'\u00b7gen", "To\u00b7des\u00b7kampf", "der", "E\u00b7le\u00b7men\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "O nur Verschwender! \u2013 ", "tokens": ["O", "nur", "Ver\u00b7schwen\u00b7der", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NE", "ADV", "NN", "$.", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "Du, Freund: gib, da\u00df dein Sohn auch so den seinen preist.", "tokens": ["Du", ",", "Freund", ":", "gib", ",", "da\u00df", "dein", "Sohn", "auch", "so", "den", "sei\u00b7nen", "preist", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$.", "VVIMP", "$,", "KOUS", "PPOSAT", "NN", "ADV", "ADV", "ART", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}