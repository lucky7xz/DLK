{"textgrid.poem.55936": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Der Stifter", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das war der Auftrag an die Malergilde.", "tokens": ["Das", "war", "der", "Auf\u00b7trag", "an", "die", "Ma\u00b7ler\u00b7gil\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vielleicht da\u00df ihm der Heiland nie erschien;", "tokens": ["Viel\u00b7leicht", "da\u00df", "ihm", "der", "Hei\u00b7land", "nie", "er\u00b7schien", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "vielleicht trat auch kein heiliger Bischof milde", "tokens": ["viel\u00b7leicht", "trat", "auch", "kein", "hei\u00b7li\u00b7ger", "Bi\u00b7schof", "mil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PIAT", "ADJA", "NN", "NE"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "an seine Seite wie in diesem Bilde", "tokens": ["an", "sei\u00b7ne", "Sei\u00b7te", "wie", "in", "die\u00b7sem", "Bil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "KOKOM", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "und legte leise seine Hand auf ihn.", "tokens": ["und", "leg\u00b7te", "lei\u00b7se", "sei\u00b7ne", "Hand", "auf", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPOSAT", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Vielleicht war dieses alles: ", "tokens": ["Viel\u00b7leicht", "war", "die\u00b7ses", "al\u00b7les", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PDAT", "PIS", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "(so wie es alles ist was wir erfuhren):", "tokens": ["(", "so", "wie", "es", "al\u00b7les", "ist", "was", "wir", "er\u00b7fuh\u00b7ren", ")", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "KOKOM", "PPER", "PIS", "VAFIN", "PWS", "PPER", "VVFIN", "$(", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "zu knien: da\u00df man die eigenen Konturen,", "tokens": ["zu", "kni\u00b7en", ":", "da\u00df", "man", "die", "ei\u00b7ge\u00b7nen", "Kon\u00b7tu\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVFIN", "$.", "KOUS", "PIS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "die ausw\u00e4rtswollenden, ganz angespannt", "tokens": ["die", "aus\u00b7w\u00e4rts\u00b7wol\u00b7len\u00b7den", ",", "ganz", "an\u00b7ge\u00b7spannt"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADV", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "im Herzen h\u00e4lt, wie Pferde in der Hand.", "tokens": ["im", "Her\u00b7zen", "h\u00e4lt", ",", "wie", "Pfer\u00b7de", "in", "der", "Hand", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "PWAV", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da\u00df wenn ein Ungeheueres gesch\u00e4he,", "tokens": ["Da\u00df", "wenn", "ein", "Un\u00b7ge\u00b7heu\u00b7e\u00b7res", "ge\u00b7sch\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "das nicht versprochen ist und nieverbrieft,", "tokens": ["das", "nicht", "ver\u00b7spro\u00b7chen", "ist", "und", "nie\u00b7ver\u00b7brieft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "VVPP", "VAFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "wir hoffen k\u00f6nnten, da\u00df es uns nicht s\u00e4he", "tokens": ["wir", "hof\u00b7fen", "k\u00f6nn\u00b7ten", ",", "da\u00df", "es", "uns", "nicht", "s\u00e4\u00b7he"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVINF", "VMFIN", "$,", "KOUS", "PPER", "PRF", "PTKNEG", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und n\u00e4her k\u00e4me, ganz in unsre N\u00e4he,", "tokens": ["und", "n\u00e4\u00b7her", "k\u00e4\u00b7me", ",", "ganz", "in", "uns\u00b7re", "N\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$,", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "mit sich besch\u00e4ftigt und in sich vertieft.", "tokens": ["mit", "sich", "be\u00b7sch\u00e4f\u00b7tigt", "und", "in", "sich", "ver\u00b7tieft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "VVPP", "KON", "APPR", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}