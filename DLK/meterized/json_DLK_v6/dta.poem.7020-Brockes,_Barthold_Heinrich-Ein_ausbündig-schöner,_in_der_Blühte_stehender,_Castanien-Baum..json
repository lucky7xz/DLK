{"dta.poem.7020": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ein ausb\u00fcndig-sch\u00f6ner, in der  \n Bl\u00fchte stehender, Castanien-Baum.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So mancher Grad vom Licht, so mancher Grad vom Schatten", "tokens": ["So", "man\u00b7cher", "Grad", "vom", "Licht", ",", "so", "man\u00b7cher", "Grad", "vom", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "APPRART", "NN", "$,", "ADV", "PIAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist in den dunckel-gr\u00fcnen Gr\u00fcnden,", "tokens": ["Ist", "in", "den", "dun\u00b7ckel\u00b7gr\u00fc\u00b7nen", "Gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Worinn sie sich bald scheiden, bald sich gatten.", "tokens": ["Wo\u00b7rinn", "sie", "sich", "bald", "schei\u00b7den", ",", "bald", "sich", "gat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADV", "VVINF", "$,", "ADV", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hier nimmt ein dunckler Zweig, von vielen Bl\u00e4ttern kraus,", "tokens": ["Hier", "nimmt", "ein", "dunck\u00b7ler", "Zweig", ",", "von", "vie\u00b7len", "Bl\u00e4t\u00b7tern", "kraus", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sich dort von einer lichten Stelle,", "tokens": ["Sich", "dort", "von", "ei\u00b7ner", "lich\u00b7ten", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die durch der Sonnen Feur bestrahlt und helle,", "tokens": ["Die", "durch", "der", "Son\u00b7nen", "Feur", "be\u00b7strahlt", "und", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "VVPP", "KON", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Recht unvergleichlich aus.", "tokens": ["Recht", "un\u00b7ver\u00b7gleich\u00b7lich", "aus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Dort sticht ein heller Platz, der gantz im Lichten steht,", "tokens": ["Dort", "sticht", "ein", "hel\u00b7ler", "Platz", ",", "der", "gantz", "im", "Lich\u00b7ten", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Indem des Himmels Gluth auf ihn herab", "tokens": ["In\u00b7dem", "des", "Him\u00b7mels", "Gluth", "auf", "ihn", "her\u00b7ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN", "APPR", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Mit vollen Strahlen f\u00e4llt; sich ebenm\u00e4\u00dfig ab", "tokens": ["Mit", "vol\u00b7len", "Strah\u00b7len", "f\u00e4llt", ";", "sich", "e\u00b7ben\u00b7m\u00e4\u00b7\u00dfig", "ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$.", "PRF", "ADJD", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Von einer Schattenreichen Tieffe,", "tokens": ["Von", "ei\u00b7ner", "Schat\u00b7ten\u00b7rei\u00b7chen", "Tief\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Die sein Licht-helles Gr\u00fcn als eine Fulg\u2019 erh\u00f6ht:", "tokens": ["Die", "sein", "Licht\u00b7hel\u00b7les", "Gr\u00fcn", "als", "ei\u00b7ne", "Ful\u00b7g'", "er\u00b7h\u00f6ht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADJD", "KOKOM", "ART", "NN", "VVPP", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.13": {"text": "Wenn nun der g\u00fcldnen Sonnen Schein", "tokens": ["Wenn", "nun", "der", "g\u00fcld\u00b7nen", "Son\u00b7nen", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Sich allgem\u00e4hlich abwerts senckt,", "tokens": ["Sich", "all\u00b7ge\u00b7m\u00e4h\u00b7lich", "ab\u00b7werts", "senckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Und sich, dem Schein nach, von uns lenckt;", "tokens": ["Und", "sich", ",", "dem", "Schein", "nach", ",", "von", "uns", "lenckt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "$,", "ART", "NN", "PTKVZ", "$,", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Zieht in dies gr\u00fcne Zelt, bey gantzen Schaaren, ein", "tokens": ["Zieht", "in", "dies", "gr\u00fc\u00b7ne", "Zelt", ",", "bey", "gant\u00b7zen", "Schaa\u00b7ren", ",", "ein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "APPR", "PDS", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,", "ART"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Das leichte V\u00f6gel-Volck: es h\u00fcpfft, es schlupfft, es springt,", "tokens": ["Das", "leich\u00b7te", "V\u00f6\u00b7gel\u00b7Volck", ":", "es", "h\u00fcpfft", ",", "es", "schlupfft", ",", "es", "springt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Es gurgelt, zwitschert, pfeifft und singt.", "tokens": ["Es", "gur\u00b7gelt", ",", "zwit\u00b7schert", ",", "pfeifft", "und", "singt", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Des Gipffels gantzen Raum", "tokens": ["Des", "Gipf\u00b7fels", "gant\u00b7zen", "Raum"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.20": {"text": "Erf\u00fcllt ein heller Klang: es lebt der gantze Baum.", "tokens": ["Er\u00b7f\u00fcllt", "ein", "hel\u00b7ler", "Klang", ":", "es", "lebt", "der", "gant\u00b7ze", "Baum", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}