{"textgrid.poem.36371": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[schweigt, ihr L\u00fcfte! in den Blumeng\u00e4ngen]", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schweigt, ihr L\u00fcfte! in den Blumeng\u00e4ngen,", "tokens": ["Schweigt", ",", "ihr", "L\u00fcf\u00b7te", "!", "in", "den", "Blu\u00b7men\u00b7g\u00e4n\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$.", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Denn entflohen ist des Fr\u00fchlings Traum;", "tokens": ["Denn", "ent\u00b7flo\u00b7hen", "ist", "des", "Fr\u00fch\u00b7lings", "Traum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "S\u00fc\u00df dem Jugendspiele nachzuh\u00e4ngen,", "tokens": ["S\u00fc\u00df", "dem", "Ju\u00b7gend\u00b7spie\u00b7le", "nach\u00b7zu\u00b7h\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVIZU", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Gibt der ernste Tag uns keinen Raum.", "tokens": ["Gibt", "der", "erns\u00b7te", "Tag", "uns", "kei\u00b7nen", "Raum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Ihr Ges\u00e4nge in der V\u00e4ter Hallen,", "tokens": ["Ihr", "Ge\u00b7s\u00e4n\u00b7ge", "in", "der", "V\u00e4\u00b7ter", "Hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Von der Ahnen hohem Heldenruhm,", "tokens": ["Von", "der", "Ah\u00b7nen", "ho\u00b7hem", "Hel\u00b7den\u00b7ruhm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "M\u00fc\u00dft den Strom der Zeit hinunterwallen,", "tokens": ["M\u00fc\u00dft", "den", "Strom", "der", "Zeit", "hin\u00b7un\u00b7ter\u00b7wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wo des Todes graue Scharen ruhn.", "tokens": ["Wo", "des", "To\u00b7des", "grau\u00b7e", "Scha\u00b7ren", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Andre Zeiten heischen andre Lieder,", "tokens": ["And\u00b7re", "Zei\u00b7ten", "hei\u00b7schen", "and\u00b7re", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Neue Dinge spricht der Welten-Greis;", "tokens": ["Neu\u00b7e", "Din\u00b7ge", "spricht", "der", "Wel\u00b7ten\u00b7Greis", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Zum verborgnen Ursprung wendet wieder", "tokens": ["Zum", "ver\u00b7borg\u00b7nen", "Ur\u00b7sprung", "wen\u00b7det", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ADV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sich zur\u00fcck der gro\u00dfe Sch\u00f6pfungskreis.", "tokens": ["Sich", "zu\u00b7r\u00fcck", "der", "gro\u00b7\u00dfe", "Sch\u00f6p\u00b7fungs\u00b7kreis", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Mag noch tr\u00fcbe Finsternis uns decken,", "tokens": ["Mag", "noch", "tr\u00fc\u00b7be", "Fins\u00b7ter\u00b7nis", "uns", "de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "In den Wogen dieser \u00f6den Welt;", "tokens": ["In", "den", "Wo\u00b7gen", "die\u00b7ser", "\u00f6\u00b7den", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "La\u00df den Geist die Seelenfl\u00fcgel strecken,", "tokens": ["La\u00df", "den", "Geist", "die", "See\u00b7len\u00b7fl\u00fc\u00b7gel", "stre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Rauschend zu der Morgenr\u00f6te Zelt!", "tokens": ["Rau\u00b7schend", "zu", "der", "Mor\u00b7gen\u00b7r\u00f6\u00b7te", "Zelt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Einsam auf der stillen Felsenwarte", "tokens": ["Ein\u00b7sam", "auf", "der", "stil\u00b7len", "Fel\u00b7sen\u00b7war\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Blickt der Seher in den Sturm hinaus;", "tokens": ["Blickt", "der", "Se\u00b7her", "in", "den", "Sturm", "hin\u00b7aus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Sp\u00e4hend an der lichten Sternencharte,", "tokens": ["Sp\u00e4\u00b7hend", "an", "der", "lich\u00b7ten", "Ster\u00b7nen\u00b7char\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Bis die Sonne teilt der Wolken Haus.", "tokens": ["Bis", "die", "Son\u00b7ne", "teilt", "der", "Wol\u00b7ken", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Wie der greise Ahnherr einst in Fluten", "tokens": ["Wie", "der", "grei\u00b7se", "Ahn\u00b7herr", "einst", "in", "Flu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "ADV", "APPR", "NN"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Sich das heil'ge Schiff der Rettung baut;", "tokens": ["Sich", "das", "heil'\u00b7ge", "Schiff", "der", "Ret\u00b7tung", "baut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wo verschlossen die Gesch\u00f6pfe ruhten,", "tokens": ["Wo", "ver\u00b7schlos\u00b7sen", "die", "Ge\u00b7sch\u00f6p\u00b7fe", "ruh\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Die dem starken F\u00e4hrmann Gott vertraut.", "tokens": ["Die", "dem", "star\u00b7ken", "F\u00e4hr\u00b7mann", "Gott", "ver\u00b7traut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "NN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Wohlgeordnet in den sichern Kammern", "tokens": ["Wohl\u00b7ge\u00b7ord\u00b7net", "in", "den", "si\u00b7chern", "Kam\u00b7mern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Stehn die Kreaturen Paar und Paar;", "tokens": ["Stehn", "die", "Kre\u00b7a\u00b7tu\u00b7ren", "Paar", "und", "Paar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Fern zu halten der Vertilgung Jammern", "tokens": ["Fern", "zu", "hal\u00b7ten", "der", "Ver\u00b7til\u00b7gung", "Jam\u00b7mern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKZU", "VVINF", "ART", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Von der Arche auserw\u00e4hlter Schar.", "tokens": ["Von", "der", "Ar\u00b7che", "au\u00b7ser\u00b7w\u00e4hl\u00b7ter", "Schar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Nieder st\u00fcrzen all die Riesengeister", "tokens": ["Nie\u00b7der", "st\u00fcr\u00b7zen", "all", "die", "Rie\u00b7sen\u00b7geis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIAT", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Nach Jahrhunderten voll \u00dcbermut;", "tokens": ["Nach", "Jahr\u00b7hun\u00b7der\u00b7ten", "voll", "\u00dc\u00b7ber\u00b7mut", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Aufw\u00e4rts forschend blickt des Schiffes Meister", "tokens": ["Auf\u00b7w\u00e4rts", "for\u00b7schend", "blickt", "des", "Schif\u00b7fes", "Meis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Zu dem Morgenstern in dunkler Flut.", "tokens": ["Zu", "dem", "Mor\u00b7gens\u00b7tern", "in", "dunk\u00b7ler", "Flut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}}, "stanza.9": {"line.1": {"text": "In den Wogen stirbt die letzte Klage,", "tokens": ["In", "den", "Wo\u00b7gen", "stirbt", "die", "letz\u00b7te", "Kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Stumm zum Grabe der Vergangenheit;", "tokens": ["Stumm", "zum", "Gra\u00b7be", "der", "Ver\u00b7gan\u00b7gen\u00b7heit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Festgesetzt, gez\u00e4hlt sind schon die Tage,", "tokens": ["Fest\u00b7ge\u00b7setzt", ",", "ge\u00b7z\u00e4hlt", "sind", "schon", "die", "Ta\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVPP", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Jede Stunde der Zerst\u00f6rungszeit.", "tokens": ["Je\u00b7de", "Stun\u00b7de", "der", "Zer\u00b7st\u00f6\u00b7rungs\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "In der \u00d6de, ob den w\u00fcsten Wellen", "tokens": ["In", "der", "\u00d6\u00b7de", ",", "ob", "den", "w\u00fcs\u00b7ten", "Wel\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Bringt die Taube den Olivenzweig;", "tokens": ["Bringt", "die", "Tau\u00b7be", "den", "O\u00b7liv\u00b7en\u00b7zweig", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Mutig steigt der Ahnherr nun im Hellen", "tokens": ["Mu\u00b7tig", "steigt", "der", "Ahn\u00b7herr", "nun", "im", "Hel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Auf das neue, gr\u00fcne Erdenreich.", "tokens": ["Auf", "das", "neu\u00b7e", ",", "gr\u00fc\u00b7ne", "Er\u00b7den\u00b7reich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.11": {"line.1": {"text": "Dankend nach dem gro\u00dfen Trauerjahre", 