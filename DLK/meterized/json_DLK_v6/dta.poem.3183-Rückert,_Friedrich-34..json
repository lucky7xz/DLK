{"dta.poem.3183": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "34.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich unterhalte mich so oft in meinen Liedern", "tokens": ["Ich", "un\u00b7ter\u00b7hal\u00b7te", "mich", "so", "oft", "in", "mei\u00b7nen", "Lie\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit Freunden, die darauf so wenig mir erwiedern.", "tokens": ["Mit", "Freun\u00b7den", ",", "die", "da\u00b7rauf", "so", "we\u00b7nig", "mir", "er\u00b7wie\u00b7dern", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PAV", "ADV", "PIS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Als ob nicht jedes Lied, dem keinen Namen bei", "tokens": ["Als", "ob", "nicht", "je\u00b7des", "Lied", ",", "dem", "kei\u00b7nen", "Na\u00b7men", "bei"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PTKNEG", "PIAT", "NN", "$,", "PRELS", "PIAT", "NN", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich schrieb, an jeden, dem's gef\u00e4llt, gerichtet sei.", "tokens": ["Ich", "schrieb", ",", "an", "je\u00b7den", ",", "dem's", "ge\u00b7f\u00e4llt", ",", "ge\u00b7rich\u00b7tet", "sei", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "PIAT", "$,", "PRELS", "VVPP", "$,", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch mit dem Dank darauf will keiner sich befassen,", "tokens": ["Doch", "mit", "dem", "Dank", "da\u00b7rauf", "will", "kei\u00b7ner", "sich", "be\u00b7fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PAV", "VMFIN", "PIS", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das bleibt dem Kritiker, wie billig, \u00fcberlassen;", "tokens": ["Das", "bleibt", "dem", "Kri\u00b7ti\u00b7ker", ",", "wie", "bil\u00b7lig", ",", "\u00fc\u00b7ber\u00b7las\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$,", "PWAV", "ADJD", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der wie ein Sekret\u00e4r schreibt in des G\u00f6nners Namen,", "tokens": ["Der", "wie", "ein", "Se\u00b7kre\u00b7t\u00e4r", "schreibt", "in", "des", "G\u00f6n\u00b7ners", "Na\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df deine Opfer zur Beh\u00f6rde richtig kamen.", "tokens": ["Da\u00df", "dei\u00b7ne", "Op\u00b7fer", "zur", "Be\u00b7h\u00f6r\u00b7de", "rich\u00b7tig", "ka\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPRART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}