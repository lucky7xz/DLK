{"textgrid.poem.52584": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mit der Kraft von tausend Herzen", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit der Kraft von tausend Herzen", "tokens": ["Mit", "der", "Kraft", "von", "tau\u00b7send", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liebst du mich, ich wei\u00df es wohl,", "tokens": ["Liebst", "du", "mich", ",", "ich", "wei\u00df", "es", "wohl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Darum auch von tausend Schmerzen", "tokens": ["Da\u00b7rum", "auch", "von", "tau\u00b7send", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "APPR", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stehet deine Seele voll.", "tokens": ["Ste\u00b7het", "dei\u00b7ne", "See\u00b7le", "voll", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ist auf Erden Alles m\u00f6glich,", "tokens": ["Ist", "auf", "Er\u00b7den", "Al\u00b7les", "m\u00f6g\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "PIS", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Macht mir Eines doch nicht bang,", "tokens": ["Macht", "mir", "Ei\u00b7nes", "doch", "nicht", "bang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PIS", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur das Eine ist unm\u00f6glich,", "tokens": ["Nur", "das", "Ei\u00b7ne", "ist", "un\u00b7m\u00f6g\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "PIS", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unsrer Liebe Untergang!", "tokens": ["Uns\u00b7rer", "Lie\u00b7be", "Un\u00b7ter\u00b7gang", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}