{"textgrid.poem.58435": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "473. Lust ohne Last", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was heut der Lust gel\u00fcst't, das l\u00e4\u00dft sie morgen fahren,", "tokens": ["Was", "heut", "der", "Lust", "ge\u00b7l\u00fcst't", ",", "das", "l\u00e4\u00dft", "sie", "mor\u00b7gen", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VVPP", "$,", "PDS", "VVFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch schau und liebe Gott, des wirst du nimmer satt;", "tokens": ["Doch", "schau", "und", "lie\u00b7be", "Gott", ",", "des", "wirst", "du", "nim\u00b7mer", "satt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "VVFIN", "NN", "$,", "PDS", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Frag nur den Seraphim nach so viel tausend Jahren,", "tokens": ["Frag", "nur", "den", "Se\u00b7ra\u00b7phim", "nach", "so", "viel", "tau\u00b7send", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "APPR", "ADV", "ADV", "CARD", "NN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Womit er seine Zeit so lang vertrieben hat!", "tokens": ["Wo\u00b7mit", "er", "sei\u00b7ne", "Zeit", "so", "lang", "ver\u00b7trie\u00b7ben", "hat", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "ADV", "ADJD", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}