{"textgrid.poem.54428": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Vanitas; Vanitatum; et Omnia Vanitas", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich seh' wohin ich seh/ nur Eitelkeit auff Erden/", "tokens": ["Ich", "seh'", "wo\u00b7hin", "ich", "seh", "/", "nur", "Ei\u00b7tel\u00b7keit", "auff", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PWAV", "PPER", "VVFIN", "$(", "ADV", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was dieser heute bawt/ reist jener morgen ein/", "tokens": ["Was", "die\u00b7ser", "heu\u00b7te", "bawt", "/", "reist", "je\u00b7ner", "mor\u00b7gen", "ein", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "ADV", "VVFIN", "$(", "VVFIN", "PDS", "ADV", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo jtzt die St\u00e4dte stehn so herrlich/ hoch vnd fein/", "tokens": ["Wo", "jtzt", "die", "St\u00e4d\u00b7te", "stehn", "so", "herr\u00b7lich", "/", "hoch", "vnd", "fein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "ADV", "ADJD", "$(", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da wird in kurtzem gehn ein Hirt mit seinen Herden:", "tokens": ["Da", "wird", "in", "kurt\u00b7zem", "gehn", "ein", "Hirt", "mit", "sei\u00b7nen", "Her\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "ADJA", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was jtzt so pr\u00e4chtig bl\u00fcht/ wird bald zutretten werden:", "tokens": ["Was", "jtzt", "so", "pr\u00e4ch\u00b7tig", "bl\u00fcht", "/", "wird", "bald", "zu\u00b7tret\u00b7ten", "wer\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "ADJD", "VVFIN", "$(", "VAFIN", "ADV", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der jtzt so pocht vnd trotzt/ l\u00e4st vbrig Asch vnd Bein/", "tokens": ["Der", "jtzt", "so", "pocht", "vnd", "trotzt", "/", "l\u00e4st", "vb\u00b7rig", "Asch", "vnd", "Bein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VVFIN", "KON", "VVFIN", "$(", "VVFIN", "ADJD", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Nichts ist/ da\u00df auff der Welt k\u00f6nt vnverg\u00e4nglich seyn/", "tokens": ["Nichts", "ist", "/", "da\u00df", "auff", "der", "Welt", "k\u00f6nt", "vn\u00b7ver\u00b7g\u00e4ng\u00b7lich", "seyn", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "$(", "KOUS", "APPR", "ART", "NN", "VVFIN", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Jtzt scheint des Gl\u00fcckes Sonn/ bald donnerts mit beschwerden.", "tokens": ["Jtzt", "scheint", "des", "Gl\u00fc\u00b7ckes", "Sonn", "/", "bald", "don\u00b7nerts", "mit", "be\u00b7schwer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$(", "ADV", "ADV", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der Thaten Herrligkeit mu\u00df wie ein Traum vergehn:", "tokens": ["Der", "Tha\u00b7ten", "Herr\u00b7lig\u00b7keit", "mu\u00df", "wie", "ein", "Traum", "ver\u00b7gehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VMFIN", "KOKOM", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Solt denn die Wasserbla\u00df/ der leichte Mensch bestehn", "tokens": ["Solt", "denn", "die", "Was\u00b7ser\u00b7bla\u00df", "/", "der", "leich\u00b7te", "Mensch", "be\u00b7stehn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ART", "NN", "$(", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ach! was ist alles di\u00df/ was wir vor k\u00f6stlich achten!", "tokens": ["Ach", "!", "was", "ist", "al\u00b7les", "di\u00df", "/", "was", "wir", "vor", "k\u00f6st\u00b7lich", "ach\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWS", "VAFIN", "PIS", "PDS", "$(", "PWS", "PPER", "APPR", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Al\u00df schlechte Nichtigkeit? als hew/ staub/ asch vnnd wind?", "tokens": ["Al\u00df", "schlech\u00b7te", "Nich\u00b7tig\u00b7keit", "?", "als", "hew", "/", "staub", "/", "asch", "vnnd", "wind", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$.", "KOUS", "NE", "$(", "VVFIN", "$(", "ADJD", "KON", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Als eine Wiesenblum/ die man nicht widerfind.", "tokens": ["Als", "ei\u00b7ne", "Wie\u00b7sen\u00b7blum", "/", "die", "man", "nicht", "wi\u00b7der\u00b7find", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NE", "$(", "PRELS", "PIS", "PTKNEG", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Noch wil/ was ewig ist/ kein einig Mensch betrachten!", "tokens": ["Noch", "wil", "/", "was", "e\u00b7wig", "ist", "/", "kein", "ei\u00b7nig", "Mensch", "be\u00b7trach\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "$(", "PWS", "ADJD", "VAFIN", "$(", "PIAT", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}