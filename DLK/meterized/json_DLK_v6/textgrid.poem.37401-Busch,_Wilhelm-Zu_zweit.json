{"textgrid.poem.37401": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Zu zweit", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frau Urschel teilte Freud und Leid", "tokens": ["Frau", "Ur\u00b7schel", "teil\u00b7te", "Freud", "und", "Leid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit ihrer lieben Kuh;", "tokens": ["Mit", "ih\u00b7rer", "lie\u00b7ben", "Kuh", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie lebten in Herzeinigkeit", "tokens": ["Sie", "leb\u00b7ten", "in", "Her\u00b7zei\u00b7nig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ganz wie auf Du und Du.", "tokens": ["Ganz", "wie", "auf", "Du", "und", "Du", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "APPR", "PPER", "KON", "PPER", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Wie war der Winter doch so lang,", "tokens": ["Wie", "war", "der", "Win\u00b7ter", "doch", "so", "lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie knapp ward da das Heu;", "tokens": ["Wie", "knapp", "ward", "da", "das", "Heu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Frau Urschel rief und seufzte bang:", "tokens": ["Frau", "Ur\u00b7schel", "rief", "und", "seufz\u00b7te", "bang", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O komm, du sch\u00f6ner Mai!", "tokens": ["O", "komm", ",", "du", "sch\u00f6\u00b7ner", "Mai", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Komm schnell und lindre unsre Not,", "tokens": ["Komm", "schnell", "und", "lind\u00b7re", "uns\u00b7re", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der du die Krippe f\u00fcllst;", "tokens": ["Der", "du", "die", "Krip\u00b7pe", "f\u00fcllst", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn ich und meine Kuh erst tot,", "tokens": ["Wenn", "ich", "und", "mei\u00b7ne", "Kuh", "erst", "tot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KON", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dann komme, wann du willst.", "tokens": ["Dann", "kom\u00b7me", ",", "wann", "du", "willst", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWAV", "PPER", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}