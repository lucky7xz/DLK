{"textgrid.poem.28254": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "69.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Rabe wil Noha zum Bothen nicht t\u00fcgen;", "tokens": ["Ein", "Ra\u00b7be", "wil", "No\u00b7ha", "zum", "Bo\u00b7then", "nicht", "t\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "NE", "APPRART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Doch bringt dem Elias ein Rabe vergn\u00fcgen.", "tokens": ["Doch", "bringt", "dem", "E\u00b7lias", "ein", "Ra\u00b7be", "ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der Himmel kan morgen vil Gunsten verleyen", "tokens": ["Der", "Him\u00b7mel", "kan", "mor\u00b7gen", "vil", "Guns\u00b7ten", "ver\u00b7le\u00b7yen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "ADV", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dem schlechsten, den heute die Grossen verspeyen.", "tokens": ["Dem", "schlechs\u00b7ten", ",", "den", "heu\u00b7te", "die", "Gros\u00b7sen", "ver\u00b7spe\u00b7yen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PRELS", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}}}}