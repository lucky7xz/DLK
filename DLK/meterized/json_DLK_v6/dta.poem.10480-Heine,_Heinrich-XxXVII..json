{"dta.poem.10480": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XxXVII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Die heil'gen drei K\u00f6nige aus Morgenland,", "tokens": ["Die", "heil'\u00b7gen", "drei", "K\u00f6\u00b7ni\u00b7ge", "aus", "Mor\u00b7gen\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "CARD", "NN", "APPR", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sie frugen in jedem St\u00e4dtchen:", "tokens": ["Sie", "fru\u00b7gen", "in", "je\u00b7dem", "St\u00e4dt\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wo geht der Weg nach Bethlehem,", "tokens": ["Wo", "geht", "der", "Weg", "nach", "Beth\u00b7le\u00b7hem", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr lieben Buben und M\u00e4dchen?", "tokens": ["Ihr", "lie\u00b7ben", "Bu\u00b7ben", "und", "M\u00e4d\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Jungen und Alten, sie wu\u00dften es nicht,", "tokens": ["Die", "Jun\u00b7gen", "und", "Al\u00b7ten", ",", "sie", "wu\u00df\u00b7ten", "es", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die K\u00f6nige zogen weiter;", "tokens": ["Die", "K\u00f6\u00b7ni\u00b7ge", "zo\u00b7gen", "wei\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie folgten einem goldenen Stern,", "tokens": ["Sie", "folg\u00b7ten", "ei\u00b7nem", "gol\u00b7de\u00b7nen", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Der leuchtete lieblich und heiter.", "tokens": ["Der", "leuch\u00b7te\u00b7te", "lieb\u00b7lich", "und", "hei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Der Stern blieb stehn \u00fcber ", "tokens": ["Der", "Stern", "blieb", "stehn", "\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "APPR"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Da sind sie hineingegangen;", "tokens": ["Da", "sind", "sie", "hin\u00b7ein\u00b7ge\u00b7gan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.3": {"text": "Das Oechslein br\u00fcllte, das Kindlein schrie,", "tokens": ["Das", "O\u00b7echs\u00b7lein", "br\u00fcll\u00b7te", ",", "das", "Kin\u00b7dlein", "schrie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Die heil'gen drei K\u00f6nige sangen.", "tokens": ["Die", "heil'\u00b7gen", "drei", "K\u00f6\u00b7ni\u00b7ge", "san\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "CARD", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}