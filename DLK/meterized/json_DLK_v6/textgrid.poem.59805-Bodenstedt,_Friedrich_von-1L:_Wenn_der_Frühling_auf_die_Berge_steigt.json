ext": "Bis zum Tale weit:", "tokens": ["Bis", "zum", "Ta\u00b7le", "weit", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.10": {"text": "O wie wundersch\u00f6n", "tokens": ["O", "wie", "wun\u00b7der\u00b7sch\u00f6n"], "token_info": ["word", "word", "word"], "pos": ["NE", "KOKOM", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Ist die Fr\u00fchlingszeit.", "tokens": ["Ist", "die", "Fr\u00fch\u00b7lings\u00b7zeit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "War's nicht auch zur jungen Fr\u00fchlingszeit,", "tokens": ["Wa\u00b7r's", "nicht", "auch", "zur", "jun\u00b7gen", "Fr\u00fch\u00b7lings\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Als ", "tokens": ["Als"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Als von dir, du wunders\u00fc\u00dfe Maid,", "tokens": ["Als", "von", "dir", ",", "du", "wun\u00b7der\u00b7s\u00fc\u00b7\u00dfe", "Maid", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Ich den ersten langen Ku\u00df geno\u00df!", "tokens": ["Ich", "den", "ers\u00b7ten", "lan\u00b7gen", "Ku\u00df", "ge\u00b7no\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Durch den Hain erklang", "tokens": ["Durch", "den", "Hain", "er\u00b7klang"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Heller Lustgesang,", "tokens": ["Hel\u00b7ler", "Lust\u00b7ge\u00b7sang", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Und die Quelle von den Bergen sprang \u2013", "tokens": ["Und", "die", "Quel\u00b7le", "von", "den", "Ber\u00b7gen", "sprang", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Scholl es von den H\u00f6hn", "tokens": ["Scholl", "es", "von", "den", "H\u00f6hn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Bis zum Tale weit:", "tokens": ["Bis", "zum", "Ta\u00b7le", "weit", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.10": {"text": "O wie wundersch\u00f6n", "tokens": ["O", "wie", "wun\u00b7der\u00b7sch\u00f6n"], "token_info": ["word", "word", "word"], "pos": ["NE", "KOKOM", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Ist die Fr\u00fchlingszeit!", "tokens": ["Ist", "die", "Fr\u00fch\u00b7lings\u00b7zeit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}