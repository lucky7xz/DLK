{"textgrid.poem.59805": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn der Fr\u00fchling auf die Berge steigt", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn der Fr\u00fchling auf die Berge steigt", "tokens": ["Wenn", "der", "Fr\u00fch\u00b7ling", "auf", "die", "Ber\u00b7ge", "steigt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Und im Sonnenstrahl der Schnee zerflie\u00dft,", "tokens": ["Und", "im", "Son\u00b7nen\u00b7strahl", "der", "Schnee", "zer\u00b7flie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wenn das erste Gr\u00fcn am Baum sich zeigt", "tokens": ["Wenn", "das", "ers\u00b7te", "Gr\u00fcn", "am", "Baum", "sich", "zeigt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPRART", "NN", "PRF", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und im Gras das erste Bl\u00fcmlein sprie\u00dft \u2013", "tokens": ["Und", "im", "Gras", "das", "ers\u00b7te", "Bl\u00fcm\u00b7lein", "sprie\u00dft", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Wenn vorbei im Tal", "tokens": ["Wenn", "vor\u00b7bei", "im", "Tal"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPRART", "NN"], "meter": "----+", "measure": "unknown.measure.single"}, "line.6": {"text": "Nun mit einemmal", "tokens": ["Nun", "mit", "ei\u00b7nem\u00b7mal"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPR", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Alle Regenzeit und Winterqual,", "tokens": ["Al\u00b7le", "Re\u00b7gen\u00b7zeit", "und", "Win\u00b7ter\u00b7qual", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Schallt es von den H\u00f6hn", "tokens": ["Schallt", "es", "von", "den", "H\u00f6hn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Bis zum Tale weit:", "tokens": ["Bis", "zum", "Ta\u00b7le", "weit", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.10": {"text": "O wie wundersch\u00f6n", "tokens": ["O", "wie", "wun\u00b7der\u00b7sch\u00f6n"], "token_info": ["word", "word", "word"], "pos": ["NE", "KOKOM", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Ist die Fr\u00fchlingszeit!", "tokens": ["Ist", "die", "Fr\u00fch\u00b7lings\u00b7zeit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Wenn am Gletscher hei\u00df die Sonne leckt,", "tokens": ["Wenn", "am", "Glet\u00b7scher", "hei\u00df", "die", "Son\u00b7ne", "leckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Wenn die Quelle von den Bergen springt,", "tokens": ["Wenn", "die", "Quel\u00b7le", "von", "den", "Ber\u00b7gen", "springt", ","], "tok