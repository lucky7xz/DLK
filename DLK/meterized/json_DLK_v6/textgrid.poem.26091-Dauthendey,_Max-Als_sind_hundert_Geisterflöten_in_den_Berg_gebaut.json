{"textgrid.poem.26091": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Als sind hundert Geisterfl\u00f6ten in den Berg gebaut", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fr\u00fchd\u00e4mmerung steht am Fenster grau schwach,", "tokens": ["Fr\u00fch\u00b7d\u00e4m\u00b7me\u00b7rung", "steht", "am", "Fens\u00b7ter", "grau", "schwach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Ein Heer von Amseln ruft drau\u00dfen die Sonne wach.", "tokens": ["Ein", "Heer", "von", "Am\u00b7seln", "ruft", "drau\u00b7\u00dfen", "die", "Son\u00b7ne", "wach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Es ist, als zieht mein Fenster den fr\u00fchen Liedern nach.", "tokens": ["Es", "ist", ",", "als", "zieht", "mein", "Fens\u00b7ter", "den", "fr\u00fc\u00b7hen", "Lie\u00b7dern", "nach", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Flink wie ein Spielwerk plaudert der Liederlaut,", "tokens": ["Flink", "wie", "ein", "Spiel\u00b7werk", "plau\u00b7dert", "der", "Lie\u00b7der\u00b7laut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Als sind hundert Geisterfl\u00f6ten in den Berg gebaut,", "tokens": ["Als", "sind", "hun\u00b7dert", "Geis\u00b7ter\u00b7fl\u00f6\u00b7ten", "in", "den", "Berg", "ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "CARD", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.6": {"text": "Und hundert Seelen erscheinen im Liede, ehe der Tag noch graut.", "tokens": ["Und", "hun\u00b7dert", "See\u00b7len", "er\u00b7schei\u00b7nen", "im", "Lie\u00b7de", ",", "e\u00b7he", "der", "Tag", "noch", "graut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "VVFIN", "APPRART", "NN", "$,", "KOUS", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+--+--+-+--+-+", "measure": "iambic.septa.relaxed"}, "line.7": {"text": "Wer hat die Amselkehlen geweckt, eh' die Sonne aufstand?", "tokens": ["Wer", "hat", "die", "Am\u00b7sel\u00b7keh\u00b7len", "ge\u00b7weckt", ",", "eh'", "die", "Son\u00b7ne", "auf\u00b7stand", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "VVPP", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+--+--+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Wer ist es, der ihnen den Lustgesang im Dunkel erfand?", "tokens": ["Wer", "ist", "es", ",", "der", "ih\u00b7nen", "den", "Lust\u00b7ge\u00b7sang", "im", "Dun\u00b7kel", "er\u00b7fand", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "PRELS", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+-+--+", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "Verliebtes Herzblut, das \u00fcbersch\u00e4umt \u00fcber den Rand.", "tokens": ["Ver\u00b7lieb\u00b7tes", "Herz\u00b7blut", ",", "das", "\u00fc\u00b7bersc\u00b7h\u00e4umt", "\u00fc\u00b7ber", "den", "Rand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}}}}}