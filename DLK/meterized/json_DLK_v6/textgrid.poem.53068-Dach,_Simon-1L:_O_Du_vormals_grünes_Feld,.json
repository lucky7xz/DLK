{"textgrid.poem.53068": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Du vormals gr\u00fcnes Feld,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Du vormals gr\u00fcnes Feld,", "tokens": ["O", "Du", "vor\u00b7mals", "gr\u00fc\u00b7nes", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "O jhr P\u00fcsch und Awen,", "tokens": ["O", "jhr", "P\u00fcsch", "und", "A\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Vor mein Pallast vnd Gezelt,", "tokens": ["Vor", "mein", "Pal\u00b7last", "vnd", "Ge\u00b7zelt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jetzt ein \u00f6des grawen,", "tokens": ["Jetzt", "ein", "\u00f6\u00b7des", "gra\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "O jhr B\u00e4che, die jhr klar", "tokens": ["O", "jhr", "B\u00e4\u00b7che", ",", "die", "jhr", "klar"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "$,", "PRELS", "PPER", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Hin-zu-rauschen pflaget,", "tokens": ["Hin\u00b7zu\u00b7rau\u00b7schen", "pfla\u00b7get", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Da wo Pan der Nymphen Schar", "tokens": ["Da", "wo", "Pan", "der", "Nym\u00b7phen", "Schar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PWAV", "NN", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Offtmals hat verjaget.", "tokens": ["Offt\u00b7mals", "hat", "ver\u00b7ja\u00b7get", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Meine Phyllis zwingt mich euch", "tokens": ["Mei\u00b7ne", "Phyl\u00b7lis", "zwingt", "mich", "euch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NE", "VVFIN", "PPER", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gutte Nacht zu geben,", "tokens": ["Gut\u00b7te", "Nacht", "zu", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ihr seyd trawrig, tod vnd bleich,", "tokens": ["Ihr", "seyd", "traw\u00b7rig", ",", "tod", "vnd", "bleich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "NN", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie ist gantz mein Leben,", "tokens": ["Sie", "ist", "gantz", "mein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+---+-", "measure": "dactylic.init"}, "line.5": {"text": "Euch ist durch des Herbstes noht", "tokens": ["Euch", "ist", "durch", "des", "Herbs\u00b7tes", "noht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Aller Pracht vergangen,", "tokens": ["Al\u00b7ler", "Pracht", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Sie ist wei\u00df vnd Sonnen-roht", "tokens": ["Sie", "ist", "wei\u00df", "vnd", "Son\u00b7nen\u00b7roht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Auff den frischen Wangen.", "tokens": ["Auff", "den", "fri\u00b7schen", "Wan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Bey euch st\u00fcrmt es ohne ruh,", "tokens": ["Bey", "euch", "st\u00fcrmt", "es", "oh\u00b7ne", "ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Vnd in allen H\u00f6len,", "tokens": ["Vnd", "in", "al\u00b7len", "H\u00f6\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Phyllis weht ein Theil mir zu", "tokens": ["Phyl\u00b7lis", "weht", "ein", "Theil", "mir", "zu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN", "PPER", "PTKZU"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihrer edlen Seelen,", "tokens": ["Ih\u00b7rer", "ed\u00b7len", "See\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Bey euch mu\u00df ohn vnterla\u00df", "tokens": ["Bey", "euch", "mu\u00df", "ohn", "vn\u00b7ter\u00b7la\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VMFIN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sich die Lufft ergiessen,", "tokens": ["Sich", "die", "Lufft", "er\u00b7gies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Sie wird nur von Thr\u00e4nen na\u00df", "tokens": ["Sie", "wird", "nur", "von", "Thr\u00e4\u00b7nen", "na\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "NN", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Vmb die Nacht-Zeit fliessen.", "tokens": ["Vmb", "die", "Nacht\u00b7Zeit", "flies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Keine Sonne lacht euch an,", "tokens": ["Kei\u00b7ne", "Son\u00b7ne", "lacht", "euch", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihr Gesicht von fernen", "tokens": ["Ihr", "Ge\u00b7sicht", "von", "fer\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ist, was mich ergetzen kan", "tokens": ["Ist", ",", "was", "mich", "er\u00b7get\u00b7zen", "kan"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "$,", "PWS", "PPER", "VVINF", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Trotz den lichten Sternen.", "tokens": ["Trotz", "den", "lich\u00b7ten", "Ster\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Ich wil in der Phyllis Scho\u00df", "tokens": ["Ich", "wil", "in", "der", "Phyl\u00b7lis", "Scho\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NE", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Steten fr\u00fcling f\u00fchren,", "tokens": ["Ste\u00b7ten", "fr\u00fc\u00b7ling", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Bey euch m\u00f6cht' ich Nackt vnd Blo\u00df", "tokens": ["Bey", "euch", "m\u00f6cht'", "ich", "Nackt", "vnd", "Blo\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VMFIN", "PPER", "VVFIN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Vnd vor K\u00e4ltt' erfrieren.", "tokens": ["Vnd", "vor", "K\u00e4ltt'", "er\u00b7frie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Darumb sol nur sie allein", "tokens": ["Da\u00b7rumb", "sol", "nur", "sie", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "ADV", "PPER", "ADV"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Mir an stat der Felder", "tokens": ["Mir", "an", "stat", "der", "Fel\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Vnd an stat der Berge seyn,", "tokens": ["Vnd", "an", "stat", "der", "Ber\u00b7ge", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "APPR", "ART", "NN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hie sind meine W\u00e4lder,", "tokens": ["Hie", "sind", "mei\u00b7ne", "W\u00e4l\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Meine Brunnen sind allhie,", "tokens": ["Mei\u00b7ne", "Brun\u00b7nen", "sind", "all\u00b7hie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo ich ohne leiden", "tokens": ["Wo", "ich", "oh\u00b7ne", "lei\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Meine Seele spat vnd fr\u00fce", "tokens": ["Mei\u00b7ne", "See\u00b7le", "spat", "vnd", "fr\u00fce"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Sicher Werde weiden.", "tokens": ["Si\u00b7cher", "Wer\u00b7de", "wei\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Kein betr\u00fcbtes Sinnen-weh", "tokens": ["Kein", "be\u00b7tr\u00fcb\u00b7tes", "Sin\u00b7nen\u00b7weh"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sol mich hie erschrecken,", "tokens": ["Sol", "mich", "hie", "er\u00b7schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ihrer weissen Arme Schnee", "tokens": ["Ih\u00b7rer", "weis\u00b7sen", "Ar\u00b7me", "Schnee"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird mich trewlich decken,", "tokens": ["Wird", "mich", "trew\u00b7lich", "de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Mein verliebtes Hertze sol", "tokens": ["Mein", "ver\u00b7lieb\u00b7tes", "Hert\u00b7ze", "sol"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "VVFIN", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Zwischen jhren Br\u00fcsten", "tokens": ["Zwi\u00b7schen", "jhren", "Br\u00fcs\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Als den H\u00fcgeln, welche vol", "tokens": ["Als", "den", "H\u00fc\u00b7geln", ",", "wel\u00b7che", "vol"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "S\u00fcsser frewde, nisten.", "tokens": ["S\u00fcs\u00b7ser", "frew\u00b7de", ",", "nis\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["FM.la", "FM.la", "$,", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Dieses ist mein Keyserthumb,", "tokens": ["Die\u00b7ses", "ist", "mein", "Key\u00b7ser\u00b7thumb", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Di\u00df sind meine Sch\u00e4tze,", "tokens": ["Di\u00df", "sind", "mei\u00b7ne", "Sch\u00e4t\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Was hat sonst bey mir den Ruhm,", "tokens": ["Was", "hat", "sonst", "bey", "mir", "den", "Ruhm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Das es mich ergetze?", "tokens": ["Das", "es", "mich", "er\u00b7get\u00b7ze", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PRF", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Dieses ist das rechte Ziel", "tokens": ["Die\u00b7ses", "ist", "das", "rech\u00b7te", "Ziel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Meiner M\u00fch auff Erden,", "tokens": ["Mei\u00b7ner", "M\u00fch", "auff", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Was mein Hertze denckt vnd wil", "tokens": ["Was", "mein", "Hert\u00b7ze", "denckt", "vnd", "wil"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPOSAT", "VVFIN", "VVFIN", "KON", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Mu\u00df mir Phyllis werden.", "tokens": ["Mu\u00df", "mir", "Phyl\u00b7lis", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NE", "VAINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Zeucht ein Kauffmann hin vnd her,", "tokens": ["Zeucht", "ein", "Kauf\u00b7fmann", "hin", "vnd", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Vber Stock vnd Steine,", "tokens": ["Vber", "Stock", "vnd", "Stei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Durch die Klippen, durch das Meer,", "tokens": ["Durch", "die", "Klip\u00b7pen", ",", "durch", "das", "Meer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch die w\u00fcsten Haine,", "tokens": ["Durch", "die", "w\u00fcs\u00b7ten", "Hai\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Was er suchet f\u00fcr vnd f\u00fcr,", "tokens": ["Was", "er", "su\u00b7chet", "f\u00fcr", "vnd", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "APPR", "KON", "APPR", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Vnd ich mag gedencken,", "tokens": ["Vnd", "ich", "mag", "ge\u00b7den\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Kan mir meiner Phyllis zier", "tokens": ["Kan", "mir", "mei\u00b7ner", "Phyl\u00b7lis", "zier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPOSAT", "NE", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Reicher vorraht schencken.", "tokens": ["Rei\u00b7cher", "vor\u00b7raht", "schen\u00b7cken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure":