{"dta.poem.8229": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "I   .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Liebestrunken sitzt der Ritter;", "tokens": ["Lie\u00b7be\u00b7strun\u00b7ken", "sitzt", "der", "Rit\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sprechend blitzen seine Augen,", "tokens": ["Spre\u00b7chend", "blit\u00b7zen", "sei\u00b7ne", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schweigend ruhen seine Lippen.", "tokens": ["Schwei\u00b7gend", "ru\u00b7hen", "sei\u00b7ne", "Lip\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Am Balkone sitzt die Dame,", "tokens": ["Am", "Bal\u00b7ko\u00b7ne", "sitzt", "die", "Da\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Eine goldne Sch\u00e4rpe wirkt sie;", "tokens": ["Ei\u00b7ne", "gold\u00b7ne", "Sch\u00e4r\u00b7pe", "wirkt", "sie", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf den Ritter blickt sie l\u00e4chelnd,", "tokens": ["Auf", "den", "Rit\u00b7ter", "blickt", "sie", "l\u00e4\u00b7chelnd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und mit hellem Klange spricht sie:", "tokens": ["Und", "mit", "hel\u00b7lem", "Klan\u00b7ge", "spricht", "sie", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "\u201edenket ihr auf Tod und Schlachten,", "tokens": ["\u201e", "den\u00b7ket", "ihr", "auf", "Tod", "und", "Schlach\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Oder sinnt ihr Minnelieder?", "tokens": ["O\u00b7der", "sinnt", "ihr", "Min\u00b7ne\u00b7lie\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wahrlich, eure stumme Weise", "tokens": ["Wahr\u00b7lich", ",", "eu\u00b7re", "stum\u00b7me", "Wei\u00b7se"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bleibt mir unerkl\u00e4rlich, Ritter!", "tokens": ["Bleibt", "mir", "un\u00b7er\u00b7kl\u00e4r\u00b7lich", ",", "Rit\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Schw\u00f6rt ihr erst in tausend Briefen,", "tokens": ["Schw\u00f6rt", "ihr", "erst", "in", "tau\u00b7send", "Brie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tausend unerh\u00f6rte Dinge", "tokens": ["Tau\u00b7send", "un\u00b7er\u00b7h\u00f6r\u00b7te", "Din\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["CARD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00e4ttet ihr f\u00fcr meine Ohren,", "tokens": ["H\u00e4t\u00b7tet", "ihr", "f\u00fcr", "mei\u00b7ne", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und das Herz sei voll zum Springen!", "tokens": ["Und", "das", "Herz", "sei", "voll", "zum", "Sprin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Fleht ihr erst in tausend Briefen", "tokens": ["Fleht", "ihr", "erst", "in", "tau\u00b7send", "Brie\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um ein heimlich einsam St\u00fcndchen!", "tokens": ["Um", "ein", "heim\u00b7lich", "ein\u00b7sam", "St\u00fcnd\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJD", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wohl, die Stunde ist gekommen \u2014", "tokens": ["Wohl", ",", "die", "Stun\u00b7de", "ist", "ge\u00b7kom\u00b7men"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Redet jetzt von tausend Dingen!\u201c", "tokens": ["Re\u00b7det", "jetzt", "von", "tau\u00b7send", "Din\u00b7gen", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADV", "APPR", "CARD", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und der Ritter bricht das Schweigen:", "tokens": ["Und", "der", "Rit\u00b7ter", "bricht", "das", "Schwei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u201ez\u00fcrnt mir nicht, o Wonnemilde;", "tokens": ["\u201e", "z\u00fcrnt", "mir", "nicht", ",", "o", "Won\u00b7ne\u00b7mil\u00b7de", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "PTKNEG", "$,", "FM", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wisset, da\u00df geheimer Zauber", "tokens": ["Wis\u00b7set", ",", "da\u00df", "ge\u00b7hei\u00b7mer", "Zau\u00b7ber"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bleiern mir die Zunge bindet.", "tokens": ["Blei\u00b7ern", "mir", "die", "Zun\u00b7ge", "bin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Nur ein Wink aus euren Augen,", "tokens": ["Nur", "ein", "Wink", "aus", "eu\u00b7ren", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur ein Wort von euren Lippen,", "tokens": ["Nur", "ein", "Wort", "von", "eu\u00b7ren", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur ihr selbst, o meine Herrin,", "tokens": ["Nur", "ihr", "selbst", ",", "o", "mei\u00b7ne", "Her\u00b7rin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "$,", "FM", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "K\u00f6nnt den argen Bann bezwingen.\u201c", "tokens": ["K\u00f6nnt", "den", "ar\u00b7gen", "Bann", "be\u00b7zwin\u00b7gen", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}