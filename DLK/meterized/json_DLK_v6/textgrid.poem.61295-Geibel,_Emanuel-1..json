{"textgrid.poem.61295": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es hat das Meer mit seinem Wogenschlage,", "tokens": ["Es", "hat", "das", "Meer", "mit", "sei\u00b7nem", "Wo\u00b7gen\u00b7schla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es hat der Wald mit seinen gr\u00fcnen Zungen", "tokens": ["Es", "hat", "der", "Wald", "mit", "sei\u00b7nen", "gr\u00fc\u00b7nen", "Zun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bis diesen Tag dasselbe Lied gesungen,", "tokens": ["Bis", "die\u00b7sen", "Tag", "das\u00b7sel\u00b7be", "Lied", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das einst sie angestimmt am Sch\u00f6pfungstage.", "tokens": ["Das", "einst", "sie", "an\u00b7ge\u00b7stimmt", "am", "Sch\u00f6p\u00b7fungs\u00b7ta\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie sich auch wandeln mocht' in Kampf und Plage", "tokens": ["Wie", "sich", "auch", "wan\u00b7deln", "mocht'", "in", "Kampf", "und", "Pla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "ADV", "VVFIN", "VMFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Welt umher, vom Menschenwitz bezwungen:", "tokens": ["Die", "Welt", "um\u00b7her", ",", "vom", "Men\u00b7schen\u00b7witz", "be\u00b7zwun\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Noch klingt der Gru\u00df, der dermaleinst erklungen,", "tokens": ["Noch", "klingt", "der", "Gru\u00df", ",", "der", "der\u00b7ma\u00b7leinst", "er\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von Flut zu Flut, von Blatt zu Blatt im Hage.", "tokens": ["Von", "Flut", "zu", "Flut", ",", "von", "Blatt", "zu", "Blatt", "im", "Ha\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$,", "APPR", "NN", "APPR", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Drum, wenn ich sinnen will von ew'gen Dingen,", "tokens": ["Drum", ",", "wenn", "ich", "sin\u00b7nen", "will", "von", "ew'\u00b7gen", "Din\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Such' ich den alten Forst an hoher K\u00fcste,", "tokens": ["Such'", "ich", "den", "al\u00b7ten", "Forst", "an", "ho\u00b7her", "K\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo Meer und Wald ihr rauschend Wort verschlingen;", "tokens": ["Wo", "Meer", "und", "Wald", "ihr", "rau\u00b7schend", "Wort", "ver\u00b7schlin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "PPER", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Mir ist es, wenn ich dort zum Werk mich r\u00fcste,", "tokens": ["Mir", "ist", "es", ",", "wenn", "ich", "dort", "zum", "Werk", "mich", "r\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als ob des Weltgeists Stimme zu mir dringen", "tokens": ["Als", "ob", "des", "Welt\u00b7geists", "Stim\u00b7me", "zu", "mir", "drin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "ART", "NN", "NN", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und mich sein Odem nah durchschauern m\u00fc\u00dfte.", "tokens": ["Und", "mich", "sein", "O\u00b7dem", "nah", "durch\u00b7schau\u00b7ern", "m\u00fc\u00df\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPOSAT", "NN", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}