{"textgrid.poem.56871": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Den Nacken hoch, Germane!", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den Nacken hoch, Germane!", "tokens": ["Den", "Na\u00b7cken", "hoch", ",", "Ger\u00b7ma\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Diese Gassen", "tokens": ["Die\u00b7se", "Gas\u00b7sen"], "token_info": ["word", "word"], "pos": ["PDAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "trat deines Ahns", "tokens": ["trat", "dei\u00b7nes", "Ahns"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "geschienter Herrenfu\u00df.", "tokens": ["ge\u00b7schien\u00b7ter", "Her\u00b7ren\u00b7fu\u00df", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hier eben, wo ich schreite,", "tokens": ["Hier", "e\u00b7ben", ",", "wo", "ich", "schrei\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "schritt auch er,", "tokens": ["schritt", "auch", "er", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "$,"], "meter": "+--", "measure": "dactylic.init"}, "line.7": {"text": "geehrt vom Italer,", "tokens": ["ge\u00b7ehrt", "vom", "I\u00b7ta\u00b7ler", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "und seiner Weiber", "tokens": ["und", "sei\u00b7ner", "Wei\u00b7ber"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Gebet und Furcht.", "tokens": ["Ge\u00b7bet", "und", "Furcht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Ich blonder Enkel bin", "tokens": ["Ich", "blon\u00b7der", "En\u00b7kel", "bin"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "kein Fremder hier;", "tokens": ["kein", "Frem\u00b7der", "hier", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "der Bursch dort teilt vielleicht", "tokens": ["der", "Bursch", "dort", "teilt", "viel\u00b7leicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "uralte Vaterschaft", "tokens": ["ur\u00b7al\u00b7te", "Va\u00b7ter\u00b7schaft"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "mit meinem Blut.", "tokens": ["mit", "mei\u00b7nem", "Blut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "Wie mir das Herz schl\u00e4gt,", "tokens": ["Wie", "mir", "das", "Herz", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.16": {"text": "t\u00f6richt laut und stark!", "tokens": ["t\u00f6\u00b7richt", "laut", "und", "stark", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "----+", "measure": "unknown.measure.single"}, "line.17": {"text": "Es ist ein Stolz", "tokens": ["Es", "ist", "ein", "Stolz"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.18": {"text": "um alte Volkheit doch, \u2013", "tokens": ["um", "al\u00b7te", "Volk\u00b7heit", "doch", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "$,", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "und warens B\u00e4ren auch,", "tokens": ["und", "wa\u00b7rens", "B\u00e4\u00b7ren", "auch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.20": {"text": "die hier als G\u00e4ste", "tokens": ["die", "hier", "als", "G\u00e4s\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "KOUS", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.21": {"text": "des sch\u00f6nsten Reichs gehaust \u2013\u2013", "tokens": ["des", "sch\u00f6ns\u00b7ten", "Reichs", "ge\u00b7haust", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.22": {"text": "der Enkel hegt,", "tokens": ["der", "En\u00b7kel", "hegt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.23": {"text": "nicht ihren Grimm,", "tokens": ["nicht", "ih\u00b7ren", "Grimm", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NE", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.24": {"text": "doch ihre Kraft noch heut.", "tokens": ["doch", "ih\u00b7re", "Kraft", "noch", "heut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.25": {"text": "Den Nacken hoch, Germane!", "tokens": ["Den", "Na\u00b7cken", "hoch", ",", "Ger\u00b7ma\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Felskastelle", "tokens": ["Fels\u00b7kas\u00b7tel\u00b7le"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.27": {"text": "des Berner Dietrich", "tokens": ["des", "Ber\u00b7ner", "Diet\u00b7rich"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NE"], "meter": "-+-+-", "measure": "iambic.di"}, "line.28": {"text": "und des gro\u00dfen Karl", "tokens": ["und", "des", "gro\u00b7\u00dfen", "Karl"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NE"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.29": {"text": "erz\u00e4hlen heut", "tokens": ["er\u00b7z\u00e4h\u00b7len", "heut"], "token_info": ["word", "word"], "pos": ["VVFIN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.30": {"text": "von alten Siegen noch,", "tokens": ["von", "al\u00b7ten", "Sie\u00b7gen", "noch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.31": {"text": "und schwarze Augen", "tokens": ["und", "schwar\u00b7ze", "Au\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.32": {"text": "brenne heut noch hei\u00dfer,", "tokens": ["bren\u00b7ne", "heut", "noch", "hei\u00b7\u00dfer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.33": {"text": "wenn sie des Nordens", "tokens": ["wenn", "sie", "des", "Nor\u00b7dens"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.34": {"text": "blauer Blitz versengt.", "tokens": ["blau\u00b7er", "Blitz", "ver\u00b7sengt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}