{"dta.poem.4290": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Wolfs-Jagd.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Handlungen, die selbst in der Natur", "tokens": ["Die", "Hand\u00b7lun\u00b7gen", ",", "die", "selbst", "in", "der", "Na\u00b7tur"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "APPR", "ART", "NN"], "meter": "-++--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nur einen Augenblick bestehen,", "tokens": ["Nur", "ei\u00b7nen", "Au\u00b7gen\u00b7blick", "be\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Befestigt ", "tokens": ["Be\u00b7fes\u00b7tigt"], "token_info": ["word"], "pos": ["VVPP"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "In dieser k\u00fcnstlichen Figur.", "tokens": ["In", "die\u00b7ser", "k\u00fcnst\u00b7li\u00b7chen", "Fi\u00b7gur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der umgeri\u00dfne Wolf wird umgerissen,", "tokens": ["Der", "um\u00b7ge\u00b7ri\u00df\u00b7ne", "Wolf", "wird", "um\u00b7ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Er siehet seinem Feind, im Umsturz, ins Gesicht,", "tokens": ["Er", "sie\u00b7het", "sei\u00b7nem", "Feind", ",", "im", "Um\u00b7sturz", ",", "ins", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "APPRART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-++--+", "measure": "iambic.hexa.chol"}, "line.7": {"text": "Er f\u00e4llt, und dennoch f\u00e4llt er nicht.", "tokens": ["Er", "f\u00e4llt", ",", "und", "den\u00b7noch", "f\u00e4llt", "er", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "ADV", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wer wird die\u00df nicht bewundern m\u00fcssen!", "tokens": ["Wer", "wird", "die\u00df", "nicht", "be\u00b7wun\u00b7dern", "m\u00fcs\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PDS", "PTKNEG", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Man sieht, was sich nicht regt, sich regen,", "tokens": ["Man", "sieht", ",", "was", "sich", "nicht", "regt", ",", "sich", "re\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PRELS", "PRF", "PTKNEG", "VVFIN", "$,", "PRF", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und stille Striche sich bewegen.", "tokens": ["Und", "stil\u00b7le", "Stri\u00b7che", "sich", "be\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Durch seinen Geist, den er im Kupfer gr\u00e4bt,", "tokens": ["Durch", "sei\u00b7nen", "Geist", ",", "den", "er", "im", "Kup\u00b7fer", "gr\u00e4bt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Ist hier Papier und Erz belebt.", "tokens": ["Ist", "hier", "Pa\u00b7pier", "und", "Erz", "be\u00b7lebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Man kann, der Geister Quell\u2019 zum Ruhm, hieraus erse-", "tokens": ["Man", "kann", ",", "der", "Geis\u00b7ter", "Quell'", "zum", "Ruhm", ",", "hier\u00b7aus", "er\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PIS", "VMFIN", "$,", "ART", "NN", "NE", "APPRART", "NN", "$,", "PAV", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "hen,", "tokens": ["hen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.15": {"text": "Wie weit schon auf der Welt des Geistes Kr\u00e4fte gehen.", "tokens": ["Wie", "weit", "schon", "auf", "der", "Welt", "des", "Geis\u00b7tes", "Kr\u00e4f\u00b7te", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "APPR", "ART", "NN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}