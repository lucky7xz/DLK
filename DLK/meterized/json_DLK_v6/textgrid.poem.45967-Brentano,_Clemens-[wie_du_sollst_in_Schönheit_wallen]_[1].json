{"textgrid.poem.45967": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[wie du sollst in Sch\u00f6nheit wallen] [1]", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie du sollst in Sch\u00f6nheit wallen", "tokens": ["Wie", "du", "sollst", "in", "Sch\u00f6n\u00b7heit", "wal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VMFIN", "APPR", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und dem Herrn doch wohlgefallen?", "tokens": ["Und", "dem", "Herrn", "doch", "wohl\u00b7ge\u00b7fal\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Frag die Wiesenbl\u00fcmelein", "tokens": ["Frag", "die", "Wie\u00b7sen\u00b7bl\u00fc\u00b7me\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die nicht ihrer Sch\u00f6nheit denken", "tokens": ["Die", "nicht", "ih\u00b7rer", "Sch\u00f6n\u00b7heit", "den\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PTKNEG", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sich der Sonne heben \u2013 senken", "tokens": ["Sich", "der", "Son\u00b7ne", "he\u00b7ben", "\u2013", "sen\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PRF", "ART", "NN", "VVINF", "$(", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Einsam duften und allein,", "tokens": ["Ein\u00b7sam", "duf\u00b7ten", "und", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wo sie spro\u00dften in dem Garten", "tokens": ["Wo", "sie", "spro\u00df\u00b7ten", "in", "dem", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ruhig auch den Tod erwarten,", "tokens": ["Ru\u00b7hig", "auch", "den", "Tod", "er\u00b7war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Ihrer Sch\u00f6nheit ew'gen Samen,", "tokens": ["Ih\u00b7rer", "Sch\u00f6n\u00b7heit", "ew'\u00b7gen", "Sa\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Gottes L\u00fcften gern vertrauen", "tokens": ["Got\u00b7tes", "L\u00fcf\u00b7ten", "gern", "ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Freudig sterben und nicht schauen,", "tokens": ["Freu\u00b7dig", "ster\u00b7ben", "und", "nicht", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "KON", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Wo der Herr sie aus will s\u00e4en in seinem Namen.", "tokens": ["Wo", "der", "Herr", "sie", "aus", "will", "s\u00e4\u00b7en", "in", "sei\u00b7nem", "Na\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "APPR", "VMFIN", "VVINF", "APPR", "PPOSAT", "NN", "$."], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Nichts vergehet nichts entstehet,", "tokens": ["Nichts", "ver\u00b7ge\u00b7het", "nichts", "ent\u00b7ste\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Alles ist unendlich da", "tokens": ["Al\u00b7les", "ist", "un\u00b7end\u00b7lich", "da"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "ADJD", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Denn der Herr ist O und A!", "tokens": ["Denn", "der", "Herr", "ist", "O", "und", "A", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "NE", "KON", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Doch die armen Augen taugen", "tokens": ["Doch", "die", "ar\u00b7men", "Au\u00b7gen", "tau\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Nur den ird'schen Tod zu sehn,", "tokens": ["Nur", "den", "ird'\u00b7schen", "Tod", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Dichter, du sollst eingestehn,", "tokens": ["Dich\u00b7ter", ",", "du", "sollst", "ein\u00b7ge\u00b7stehn", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Da\u00df die Rose, die verblichen", "tokens": ["Da\u00df", "die", "Ro\u00b7se", ",", "die", "ver\u00b7bli\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ART", "NN", "$,", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Du der Sterblichkeit verglichen", "tokens": ["Du", "der", "Sterb\u00b7lich\u00b7keit", "ver\u00b7gli\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Eh' sie war, und da sie gl\u00fchte", "tokens": ["Eh'", "sie", "war", ",", "und", "da", "sie", "gl\u00fch\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VAFIN", "$,", "KON", "KOUS", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Und nachdem sie l\u00e4ngst verbl\u00fchte,", "tokens": ["Und", "nach\u00b7dem", "sie", "l\u00e4ngst", "ver\u00b7bl\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.23": {"text": "Da\u00df die Rose eh und je,", "tokens": ["Da\u00df", "die", "Ro\u00b7se", "eh", "und", "je", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "KOUS", "KON", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Die ich hier erblassen seh',", "tokens": ["Die", "ich", "hier", "er\u00b7blas\u00b7sen", "seh'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Ewiglich in Gott florieret,", "tokens": ["E\u00b7wig\u00b7lich", "in", "Gott", "flo\u00b7rie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Und wer dieses recht verstehet,", "tokens": ["Und", "wer", "die\u00b7ses", "recht", "ver\u00b7ste\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PDAT", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Triumphieret", "tokens": ["Tri\u00b7um\u00b7phie\u00b7ret"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.28": {"text": "Nichts vergehet, nichts entstehet", "tokens": ["Nichts", "ver\u00b7ge\u00b7het", ",", "nichts", "ent\u00b7ste\u00b7het"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "PIS", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Alles ist unendlich da,", "tokens": ["Al\u00b7les", "ist", "un\u00b7end\u00b7lich", "da", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.30": {"text": "Denn der Herr ist O und A.", "tokens": ["Denn", "der", "Herr", "ist", "O", "und", "A."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "ART", "NN", "VAFIN", "NE", "KON", "APPRART"], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}