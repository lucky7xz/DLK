{"dta.poem.1006": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "T RAURIGE   T \u00c4NZE", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Mir ist kein weg zu steil zu weit               ", "tokens": ["Mir", "ist", "kein", "weg", "zu", "steil", "zu", "weit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "ADV", "PTKA", "ADJD", "PTKA", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den ich nicht ginge mein geleit               ", "tokens": ["Den", "ich", "nicht", "gin\u00b7ge", "mein", "ge\u00b7leit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "PTKNEG", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit dir &#8212; uns &#228;ngstet keine kluft               ", "tokens": ["Mit", "dir", "&#8212;", "uns", "&#228;", "ngs\u00b7tet", "kei\u00b7ne", "kluft"], "token_info": ["word", "word", "XML_entity", "word", "XML_entity", "word", "word", "word"], "pos": ["APPR", "PPER", "$(", "PPER", "$(", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und s&#252;hne steht auf jeder gruft             ", "tokens": ["Und", "s", "&#252;", "hne", "steht", "auf", "je\u00b7der", "gruft"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word", "word"], "pos": ["KON", "FM", "FM", "FM", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So kreuzen wir in wehmut nur               ", "tokens": ["So", "kreu\u00b7zen", "wir", "in", "weh\u00b7mut", "nur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der freudlos grauen aschen flur               ", "tokens": ["Der", "freud\u00b7los", "grau\u00b7en", "asc\u00b7hen", "flur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit ihrem d&#252;rren gras und dorn               ", "tokens": ["Mit", "ih\u00b7rem", "d", "&#252;", "rren", "gras", "und", "dorn"], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "XY", "$(", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch rein von reue rein von zorn             ", "tokens": ["Doch", "rein", "von", "reu\u00b7e", "rein", "von", "zorn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "VVFIN", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mein feuchtes auge sp&#228;ht nur fern               ", "tokens": ["Mein", "feuch\u00b7tes", "au\u00b7ge", "sp", "&#228;", "ht", "nur", "fern"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "NE", "$(", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Nach diesem Einen aus der gern               ", "tokens": ["Nach", "die\u00b7sem", "Ei\u00b7nen", "aus", "der", "gern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ART", "APPR", "ART", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die harfe reich und wol gestimmt               ", "tokens": ["Die", "har\u00b7fe", "reich", "und", "wol", "ge\u00b7stimmt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "KON", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der unsre goldne harfe nimmt.             ", "tokens": ["Der", "uns\u00b7re", "gold\u00b7ne", "har\u00b7fe", "nimmt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}