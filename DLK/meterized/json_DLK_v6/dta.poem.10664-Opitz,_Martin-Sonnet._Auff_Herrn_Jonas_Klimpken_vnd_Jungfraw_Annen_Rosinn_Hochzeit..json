{"dta.poem.10664": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet.  \n  Auff Herrn Jonas Klimpken vnd Jungfraw Annen  \n Rosinn Hochzeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Der sehr gew\u00fcnschte Lentz die kalte Lufft verdringet/", "tokens": ["Der", "sehr", "ge\u00b7w\u00fcnschte", "Lentz", "die", "kal\u00b7te", "Lufft", "ver\u00b7drin\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die gantze Welt zeigt an ein newes gr\u00fcnes Kleyd/", "tokens": ["Die", "gant\u00b7ze", "Welt", "zeigt", "an", "ein", "ne\u00b7wes", "gr\u00fc\u00b7nes", "Kleyd", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der zarten Blumen glantz Hertz/ Muth vnd Sinn erfrewet/", "tokens": ["Der", "zar\u00b7ten", "Blu\u00b7men", "glantz", "Hertz", "/", "Muth", "vnd", "Sinn", "er\u00b7fre\u00b7wet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "NN", "$(", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.4": {"text": "Das Vieh geht wider au\u00df/ der V\u00f6gel schar sich schwinget.", "tokens": ["Das", "Vieh", "geht", "wi\u00b7der", "au\u00df", "/", "der", "V\u00f6\u00b7gel", "schar", "sich", "schwin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PTKVZ", "$(", "ART", "NN", "VVFIN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Venus selber auch mit jhren Nymphen singet/", "tokens": ["Die", "Ve\u00b7nus", "sel\u00b7ber", "auch", "mit", "jhren", "Nym\u00b7phen", "sin\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Da\u00df jhre Stimm im Waldt erschallet weit vnd breit/", "tokens": ["Da\u00df", "jhre", "Stimm", "im", "Waldt", "er\u00b7schal\u00b7let", "weit", "vnd", "breit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.7": {"text": "Auch jhr/ Herr Breutigam/ bequemet euch der Zeit/", "tokens": ["Auch", "jhr", "/", "Herr", "Breu\u00b7ti\u00b7gam", "/", "be\u00b7que\u00b7met", "euch", "der", "Zeit", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "$(", "NN", "NE", "$(", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Vnd vnsrem Schlesien ein edle Rose bringet/", "tokens": ["Vnd", "vns\u00b7rem", "Schle\u00b7si\u00b7en", "ein", "ed\u00b7le", "Ro\u00b7se", "brin\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Mi\u00dfg\u00f6nn vns/ G\u00f6mlitz/ doch nit diese deine Zierdt/", "tokens": ["Mi\u00df\u00b7g\u00f6nn", "vns", "/", "G\u00f6m\u00b7litz", "/", "doch", "nit", "die\u00b7se", "dei\u00b7ne", "Zierdt", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$(", "NE", "$(", "ADV", "PTKNEG", "PDS", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Bey vns wird gleich so wol sehr tr\u00e4chtig Land gesp\u00fcrt.", "tokens": ["Bey", "vns", "wird", "gleich", "so", "wol", "sehr", "tr\u00e4ch\u00b7tig", "Land", "ge\u00b7sp\u00fcrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "ADV", "ADV", "ADV", "ADV", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Vielleichte wirst du noch ins k\u00fcnfftig selber sagen:", "tokens": ["Viel\u00b7leich\u00b7te", "wirst", "du", "noch", "ins", "k\u00fcnff\u00b7tig", "sel\u00b7ber", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "APPRART", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da so ein junger Baum wird anderwerts verf\u00fchrt/", "tokens": ["Da", "so", "ein", "jun\u00b7ger", "Baum", "wird", "an\u00b7der\u00b7werts", "ver\u00b7f\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "VAFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Viel eh\u2019/ als wo er stund zu erste/ Frucht gebiert/", "tokens": ["Viel", "eh'", "/", "als", "wo", "er", "stund", "zu", "ers\u00b7te", "/", "Frucht", "ge\u00b7biert", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "KOUS", "$(", "KOUS", "PWAV", "PPER", "ADJD", "PTKZU", "VVFIN", "$(", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wen diese Rose wird viel sch\u00f6ne Rosen tragen.", "tokens": ["Wen", "die\u00b7se", "Ro\u00b7se", "wird", "viel", "sch\u00f6\u00b7ne", "Ro\u00b7sen", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "NN", "VAFIN", "PIAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}