{"dta.poem.1433": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Seelen-Ermunterung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Ich soll nunmehr die Schrecken-reiche Bahn/", "tokens": ["Ich", "soll", "nun\u00b7mehr", "die", "Schre\u00b7cken\u00b7rei\u00b7che", "Bahn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die keiner k\u00f6mmt zur\u00fccke/ treten an/", "tokens": ["Die", "kei\u00b7ner", "k\u00f6mmt", "zu\u00b7r\u00fc\u00b7cke", "/", "tre\u00b7ten", "an", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PTKVZ", "$(", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Seele bebt/ das Hertze will erkalten/", "tokens": ["Die", "See\u00b7le", "bebt", "/", "das", "Hert\u00b7ze", "will", "er\u00b7kal\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "PDS", "VVFIN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch JEsus wird mich auch im Tod erhalten.", "tokens": ["Doch", "Je\u00b7sus", "wird", "mich", "auch", "im", "Tod", "er\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VAFIN", "PPER", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein schmaler Weg voll Dornen/ Sand und Stein", "tokens": ["Ein", "schma\u00b7ler", "Weg", "voll", "Dor\u00b7nen", "/", "Sand", "und", "Stein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD", "NN", "$(", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fchrt mich durch Berg und Thal in Himmel ein/", "tokens": ["F\u00fchrt", "mich", "durch", "Berg", "und", "Thal", "in", "Him\u00b7mel", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "KON", "NN", "APPR", "NN", "ART", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch JEsus hat mir schon die Bahn gebrochen/", "tokens": ["Doch", "Je\u00b7sus", "hat", "mir", "schon", "die", "Bahn", "ge\u00b7bro\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Viel V\u00e4ter sind ihm freudig nachgekrochen.", "tokens": ["Viel", "V\u00e4\u00b7ter", "sind", "ihm", "freu\u00b7dig", "nach\u00b7ge\u00b7kro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Entgleitet mir der Ohnmachts-volle Fu\u00df/", "tokens": ["Ent\u00b7glei\u00b7tet", "mir", "der", "Ohn\u00b7machts\u00b7vol\u00b7le", "Fu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der allbereit von unten sterben mu\u00df/", "tokens": ["Der", "all\u00b7be\u00b7reit", "von", "un\u00b7ten", "ster\u00b7ben", "mu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ADV", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich falle nicht/ weil ich zum Stul und Stabe", "tokens": ["Ich", "fal\u00b7le", "nicht", "/", "weil", "ich", "zum", "Stul", "und", "Sta\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$(", "KOUS", "PPER", "APPRART", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des HErren Creutz und offne Seiten habe.", "tokens": ["Des", "Her\u00b7ren", "Creutz", "und", "off\u00b7ne", "Sei\u00b7ten", "ha\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Er ist der Arzt/ der mit durchgrabner Hand/", "tokens": ["Er", "ist", "der", "Arzt", "/", "der", "mit", "durch\u00b7grab\u00b7ner", "Hand", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$(", "ART", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bef\u00fchlet wie mein schwacher Pul\u00df bewand/", "tokens": ["Be\u00b7f\u00fch\u00b7let", "wie", "mein", "schwa\u00b7cher", "Pul\u00df", "be\u00b7wand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "PPOSAT", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sein theures Blutt/ das er mir selbst verschreibet/", "tokens": ["Sein", "theu\u00b7res", "Blutt", "/", "das", "er", "mir", "selbst", "ver\u00b7schrei\u00b7bet", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "PRELS", "PPER", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Macht/ da\u00df man auch im Tode lebend bleibet.", "tokens": ["Macht", "/", "da\u00df", "man", "auch", "im", "To\u00b7de", "le\u00b7bend", "blei\u00b7bet", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOUS", "PIS", "ADV", "APPRART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Sein Liebes-Brand entz\u00fcndt der Glieder Ei\u00df/", "tokens": ["Sein", "Lie\u00b7bes\u00b7Brand", "ent\u00b7z\u00fcndt", "der", "Glie\u00b7der", "Ei\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sein k\u00fchler Trost wischt ab den Todes-Schwei\u00df/", "tokens": ["Sein", "k\u00fch\u00b7ler", "Trost", "wischt", "ab", "den", "To\u00b7des\u00b7Schwei\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sein s\u00fcsses Wort bestillt mein kl\u00e4glich Aechzen/", "tokens": ["Sein", "s\u00fcs\u00b7ses", "Wort", "be\u00b7stillt", "mein", "kl\u00e4g\u00b7lich", "A\u00b7ech\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPOSAT", "ADJD", "NN", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Sein Myrrhen-Tranck der matten Seelen Lechzen.", "tokens": ["Sein", "Myr\u00b7rhen\u00b7Tranck", "der", "mat\u00b7ten", "See\u00b7len", "Lech\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Die Zunge schweigt/ das Hertze glaubt und hofft/", "tokens": ["Die", "Zun\u00b7ge", "schweigt", "/", "das", "Hert\u00b7ze", "glaubt", "und", "hofft", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "PDS", "VVFIN", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich h\u00f6re nichts/ als wie mir JEsus rufft/", "tokens": ["Ich", "h\u00f6\u00b7re", "nichts", "/", "als", "wie", "mir", "Je\u00b7sus", "rufft", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$(", "KOUS", "PWAV", "PPER", "NE", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich sehe nichts/ als wie mich JEsus f\u00fchret/", "tokens": ["Ich", "se\u00b7he", "nichts", "/", "als", "wie", "mich", "Je\u00b7sus", "f\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$(", "KOUS", "PWAV", "PPER", "NE", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo ewge Lust und Wonne wird gesp\u00fcret.", "tokens": ["Wo", "ew\u00b7ge", "Lust", "und", "Won\u00b7ne", "wird", "ge\u00b7sp\u00fc\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "KON", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Wer wolte nicht mit Freuden folgen nach?", "tokens": ["Wer", "wol\u00b7te", "nicht", "mit", "Freu\u00b7den", "fol\u00b7gen", "nach", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PTKNEG", "APPR", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Besucht der Leib sein kaltes Schlaff-Gemach/", "tokens": ["Be\u00b7sucht", "der", "Leib", "sein", "kal\u00b7tes", "Schlaff\u00b7Ge\u00b7mach", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er wird auch da des HErren Erde finden/", "tokens": ["Er", "wird", "auch", "da", "des", "Her\u00b7ren", "Er\u00b7de", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und ihn der HErr/ wenn alle Welt mu\u00df schwinden.", "tokens": ["Und", "ihn", "der", "Herr", "/", "wenn", "al\u00b7le", "Welt", "mu\u00df", "schwin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "$(", "KOUS", "PIAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Drum ob der Tod mich auff die Folter zieht/", "tokens": ["Drum", "ob", "der", "Tod", "mich", "auff", "die", "Fol\u00b7ter", "zieht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "KOUS", "ART", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ob Schmertz und Weh durchgehen iedes Glied/", "tokens": ["Ob", "Schmertz", "und", "Weh", "durch\u00b7ge\u00b7hen", "ie\u00b7des", "Glied", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VVFIN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ob man den Leib zur W\u00fcrme Kost begr\u00e4bet/", "tokens": ["Ob", "man", "den", "Leib", "zur", "W\u00fcr\u00b7me", "Kost", "be\u00b7gr\u00e4\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "APPRART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So wei\u00df ich doch/ da\u00df mein Erl\u00f6ser lebet.", "tokens": ["So", "wei\u00df", "ich", "doch", "/", "da\u00df", "mein", "Er\u00b7l\u00f6\u00b7ser", "le\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$(", "KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Ich bin gewi\u00df/ da\u00df mein Erl\u00f6ser lebt/", "tokens": ["Ich", "bin", "ge\u00b7wi\u00df", "/", "da\u00df", "mein", "Er\u00b7l\u00f6\u00b7ser", "lebt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$(", "KOUS", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vor dessen Thron die reine Seele schwebt/", "tokens": ["Vor", "des\u00b7sen", "Thron", "die", "rei\u00b7ne", "See\u00b7le", "schwebt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die durch sein Blutt gewaschen ist von S\u00fcnden/", "tokens": ["Die", "durch", "sein", "Blutt", "ge\u00b7wa\u00b7schen", "ist", "von", "S\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "ADJD", "VVPP", "VAFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-", "measure":