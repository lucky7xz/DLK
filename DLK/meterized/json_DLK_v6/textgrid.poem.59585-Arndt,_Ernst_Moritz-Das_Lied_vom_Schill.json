{"textgrid.poem.59585": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Das Lied vom Schill", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es zog aus Berlin ein tapferer Held,", "tokens": ["Es", "zog", "aus", "Ber\u00b7lin", "ein", "tap\u00b7fe\u00b7rer", "Held", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er f\u00fchrte sechshundert Reiter ins Feld,", "tokens": ["Er", "f\u00fchr\u00b7te", "sechs\u00b7hun\u00b7dert", "Rei\u00b7ter", "ins", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sechshundert Reiter mit redlichem Mut,", "tokens": ["Sechs\u00b7hun\u00b7dert", "Rei\u00b7ter", "mit", "red\u00b7li\u00b7chem", "Mut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Die d\u00fcrsteten alle Franzosenblut.", "tokens": ["Die", "d\u00fcrs\u00b7te\u00b7ten", "al\u00b7le", "Fran\u00b7zo\u00b7sen\u00b7blut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PIAT", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Auch zogen mit Reitern und Rossen im Schritt", "tokens": ["Auch", "zo\u00b7gen", "mit", "Rei\u00b7tern", "und", "Ros\u00b7sen", "im", "Schritt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "KON", "NN", "APPRART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wohl tausend der tapfersten Sch\u00fctzen mit,", "tokens": ["Wohl", "tau\u00b7send", "der", "tap\u00b7fers\u00b7ten", "Sch\u00fct\u00b7zen", "mit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Ihr Sch\u00fctzen gesegn' euch Gott jeglichen Schu\u00df,", "tokens": ["Ihr", "Sch\u00fct\u00b7zen", "ge\u00b7segn'", "euch", "Gott", "jeg\u00b7li\u00b7chen", "Schu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "NN", "PIAT", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Durch welchen ein Franzmann erblassen mu\u00df!", "tokens": ["Durch", "wel\u00b7chen", "ein", "Franz\u00b7mann", "er\u00b7blas\u00b7sen", "mu\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "So zieht der tapfre, der mutige Schill,", "tokens": ["So", "zieht", "der", "tapf\u00b7re", ",", "der", "mu\u00b7ti\u00b7ge", "Schill", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der mit den Franzosen sich schlagen will;", "tokens": ["Der", "mit", "den", "Fran\u00b7zo\u00b7sen", "sich", "schla\u00b7gen", "will", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PRF", "VVINF", "VMFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Ihn sendet kein Kaiser, kein K\u00f6nig aus,", "tokens": ["Ihn", "sen\u00b7det", "kein", "Kai\u00b7ser", ",", "kein", "K\u00f6\u00b7nig", "aus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Ihn sendet die Freiheit, das Vaterland aus.", "tokens": ["Ihn", "sen\u00b7det", "die", "Frei\u00b7heit", ",", "das", "Va\u00b7ter\u00b7land", "aus", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "PTKVZ", "$."], "meter": "----+--+--+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Bei Dodendorf f\u00e4rbten die M\u00e4nner gut", "tokens": ["Bei", "Do\u00b7den\u00b7dorf", "f\u00e4rb\u00b7ten", "die", "M\u00e4n\u00b7ner", "gut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "VVFIN", "ART", "NN", "ADJD"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Das Magdeburger Land mit franz\u00f6sischem Blut,", "tokens": ["Das", "Mag\u00b7de\u00b7bur\u00b7ger", "Land", "mit", "fran\u00b7z\u00f6\u00b7si\u00b7schem", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Zweitausend zerhieben die S\u00e4bel blank,", "tokens": ["Zweit\u00b7au\u00b7send", "zer\u00b7hie\u00b7ben", "die", "S\u00e4\u00b7bel", "blank", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Die \u00fcbrigen machten die Beine lang.", "tokens": ["Die", "\u00fcb\u00b7ri\u00b7gen", "mach\u00b7ten", "die", "Bei\u00b7ne", "lang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.5": {"line.1": {"text": "Drauf st\u00fcrmten sie D\u00f6mitz, das feste Haus,", "tokens": ["Drauf", "st\u00fcrm\u00b7ten", "sie", "D\u00f6\u00b7mitz", ",", "das", "fes\u00b7te", "Haus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--++-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und jagten die Schelmenfranzosen heraus,", "tokens": ["Und", "jag\u00b7ten", "die", "Schel\u00b7men\u00b7fran\u00b7zo\u00b7sen", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Dann zogen sie lustig ins Pommerland ein,", "tokens": ["Dann", "zo\u00b7gen", "sie", "lus\u00b7tig", "ins", "Pom\u00b7mer\u00b7land", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Da soll kein Franzose sein Kiwi! mehr schrein.", "tokens": ["Da", "soll", "kein", "Fran\u00b7zo\u00b7se", "sein", "Ki\u00b7wi", "!", "mehr", "schrein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIAT", "NN", "PPOSAT", "NN", "$.", "ADV", "ADJD", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Auf Stralsund st\u00fcrmte der reisige Zug \u2013", "tokens": ["Auf", "Stral\u00b7sund", "st\u00fcrm\u00b7te", "der", "rei\u00b7si\u00b7ge", "Zug", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "O Franzosen, verst\u00e4ndet ihr Vogelflug!", "tokens": ["O", "Fran\u00b7zo\u00b7sen", ",", "ver\u00b7st\u00e4n\u00b7det", "ihr", "Vo\u00b7gel\u00b7flug", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "O w\u00fcchsen euch Federn und Fl\u00fcgel geschwind!", "tokens": ["O", "w\u00fcch\u00b7sen", "euch", "Fe\u00b7dern", "und", "Fl\u00fc\u00b7gel", "ge\u00b7schwind", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Es nahet der Schill, und er reitet wie Wind.", "tokens": ["Es", "na\u00b7het", "der", "Schill", ",", "und", "er", "rei\u00b7tet", "wie", "Wind", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KON", "PPER", "VVFIN", "KOKOM", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.7": {"line.1": {"text": "Er reitet wie Wetter hinein in die Stadt,", "tokens": ["Er", "rei\u00b7tet", "wie", "Wet\u00b7ter", "hin\u00b7ein", "in", "die", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die der Wallenstein weiland belagert hat,", "tokens": ["Die", "der", "Wal\u00b7len\u00b7stein", "wei\u00b7land", "be\u00b7la\u00b7gert", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Wo der zw\u00f6lfte Karolus im Tore schlief.", "tokens": ["Wo", "der", "zw\u00f6lf\u00b7te", "Ka\u00b7ro\u00b7lus", "im", "To\u00b7re", "schlief", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Jetzt liegen ihre Mauern und T\u00fcrme tief.", "tokens": ["Jetzt", "lie\u00b7gen", "ih\u00b7re", "Mau\u00b7ern", "und", "T\u00fcr\u00b7me", "tief", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.8": {"line.1": {"text": "O weh euch, Franzosen! Jetzt seid ihr tot,", "tokens": ["O", "weh", "euch", ",", "Fran\u00b7zo\u00b7sen", "!", "Jetzt", "seid", "ihr", "tot", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "NN", "$.", "ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ihr f\u00e4rbet die S\u00e4bel der Reiter rot,", "tokens": ["Ihr", "f\u00e4r\u00b7bet", "die", "S\u00e4\u00b7bel", "der", "Rei\u00b7ter", "rot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Die Reiter sie f\u00fchlen das deutsche Blut,", "tokens": ["Die", "Rei\u00b7ter", "sie", "f\u00fch\u00b7len", "das", "deut\u00b7sche", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Franzosen zu s\u00e4beln, das deucht ihnen gut.", "tokens": ["Fran\u00b7zo\u00b7sen", "zu", "s\u00e4\u00b7beln", ",", "das", "deucht", "ih\u00b7nen", "gut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,", "PDS", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.9": {"line.1": {"text": "O Schill, o Schill, du tapferer Held!", "tokens": ["O", "Schill", ",", "o", "Schill", ",", "du", "tap\u00b7fe\u00b7rer", "Held", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "FM", "NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Was sind dir f\u00fcr b\u00fcbische Netze gestellt!", "tokens": ["Was", "sind", "dir", "f\u00fcr", "b\u00fc\u00b7bi\u00b7sche", "Net\u00b7ze", "ge\u00b7stellt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Viele ziehen zu Lande, es schleichet vom Meer", "tokens": ["Vie\u00b7le", "zie\u00b7hen", "zu", "Lan\u00b7de", ",", "es", "schlei\u00b7chet", "vom", "Meer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "NN", "$,", "PPER", "VVFIN", "APPRART", "NN"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Der D\u00e4ne, die t\u00fcckische Schlange, daher.", "tokens": ["Der", "D\u00e4\u00b7ne", ",", "die", "t\u00fc\u00b7cki\u00b7sche", "Schlan\u00b7ge", ",", "da\u00b7her", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,", "PAV", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.10": {"line.1": {"text": "O Schill, o Schill, du tapferer Held!", "tokens": ["O", "Schill", ",", "o", "Schill", ",", "du", "tap\u00b7fe\u00b7rer", "Held", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "FM", "NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Was sprengst du nicht mit den Reitern ins Feld?", "tokens": ["Was", "sprengst", "du", "nicht", "mit", "den", "Rei\u00b7tern", "ins", "Feld", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PTKNEG", "APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Was schlie\u00dfest in Mauern die Tapferkeit ein?", "tokens": ["Was", "schlie\u00b7\u00dfest", "in", "Mau\u00b7ern", "die", "Tap\u00b7fer\u00b7keit", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "In Stralsund, da sollst du begraben sein.", "tokens": ["In", "Stral\u00b7sund", ",", "da", "sollst", "du", "be\u00b7gra\u00b7ben", "sein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "KOUS", "VMFIN", "PPER", "VVPP", "VAINF", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.11": {"line.1": {"text": "O Stralsund, du trauriges Stralesund!", "tokens": ["O", "Stral\u00b7sund", ",", "du", "trau\u00b7ri\u00b7ges", "Stra\u00b7le\u00b7sund", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "In dir geht das tapferste Herz zugrund',", "tokens": ["In", "dir", "geht", "das", "tap\u00b7fers\u00b7te", "Herz", "zu\u00b7grund'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Eine Kugel durchbohret das treueste Herz,", "tokens": ["Ei\u00b7ne", "Ku\u00b7gel", "durch\u00b7boh\u00b7ret", "das", "treu\u00b7es\u00b7te", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Und Buben sie treiben mit Helden Scherz.", "tokens": ["Und", "Bu\u00b7ben", "sie", "trei\u00b7ben", "mit", "Hel\u00b7den", "Scherz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPER", "VVFIN", "APPR", "NN", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.12": {"line.1": {"text": "Da schreiet ein frecher Franzosenmund:", "tokens": ["Da", "schrei\u00b7et", "ein", "fre\u00b7cher", "Fran\u00b7zo\u00b7sen\u00b7mund", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "\u00bbman soll ihn begraben wie einen Hund,", "tokens": ["\u00bb", "man", "soll", "ihn", "be\u00b7gra\u00b7ben", "wie", "ei\u00b7nen", "Hund", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "VMFIN", "PPER", "VVPP", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Wie einen Schelm, der an Galgen und Rad", "tokens": ["Wie", "ei\u00b7nen", "Schelm", ",", "der", "an", "Gal\u00b7gen", "und", "Rad"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "APPR", "NN", "KON", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Schon f\u00fctterte Kr\u00e4hen und Raben satt.\u00ab", "tokens": ["Schon", "f\u00fct\u00b7ter\u00b7te", "Kr\u00e4\u00b7hen", "und", "Ra\u00b7ben", "satt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "NN", "ADJD", "$.", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.13": {"line.1": {"text": "So trugen sie ihn ohne Sang und Klang,", "tokens": ["So", "tru\u00b7gen", "sie", "ihn", "oh\u00b7ne", "Sang", "und", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ohne Pfeifenspiel und ohne Trommelklang,", "tokens": ["Oh\u00b7ne", "Pfei\u00b7fen\u00b7spiel", "und", "oh\u00b7ne", "Trom\u00b7mel\u00b7klang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Ohne Kanonenmusik und Flintengru\u00df,", "tokens": ["Oh\u00b7ne", "Ka\u00b7no\u00b7nen\u00b7mu\u00b7sik", "und", "Flin\u00b7ten\u00b7gru\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Womit man die Tapfern begraben mu\u00df.", "tokens": ["Wo\u00b7mit", "man", "die", "Tap\u00b7fern", "be\u00b7gra\u00b7ben", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.14": {"line.1": {"text": "Sie schnitten den Kopf von dem Rumpf ihm ab", "tokens": ["Sie", "schnit\u00b7ten", "den", "Kopf", "von", "dem", "Rumpf", "ihm", "ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "PPER", "PTKVZ"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und warfen den Leib in ein schlechtes Grab,", "tokens": ["Und", "war\u00b7fen", "den", "Leib", "in", "ein", "schlech\u00b7tes", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Da schl\u00e4ft er nun bis an den J\u00fcngsten Tag,", "tokens": ["Da", "schl\u00e4ft", "er", "nun", "bis", "an", "den", "J\u00fcng\u00b7sten", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wo Gott ihn zu Freuden erwecken mag.", "tokens": ["Wo", "Gott", "ihn", "zu", "Freu\u00b7den", "er\u00b7we\u00b7cken", "mag."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["PWAV", "NN", "PPER", "APPR", "NN", "VVINF", "VMFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.15": {"line.1": {"text": "Da schl\u00e4ft der fromme, der tapfre Held,", "tokens": ["Da", "schl\u00e4ft", "der", "from\u00b7me", ",", "der", "tapf\u00b7re", "Held", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ihm ward kein Stein zum Ged\u00e