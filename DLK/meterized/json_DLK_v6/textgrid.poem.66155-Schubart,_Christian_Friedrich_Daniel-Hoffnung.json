{"textgrid.poem.66155": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Hoffnung", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn mich aus meinen Banden", "tokens": ["Wenn", "mich", "aus", "mei\u00b7nen", "Ban\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Herr befreit;", "tokens": ["Der", "Herr", "be\u00b7freit", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und hab' ich \u00fcberstanden", "tokens": ["Und", "hab'", "ich", "\u00fc\u00b7bers\u00b7tan\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Hohn der Zeit;", "tokens": ["Den", "Hohn", "der", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Dann werd' ich wie erwachen", "tokens": ["Dann", "werd'", "ich", "wie", "er\u00b7wa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "KOKOM", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Aus einem Traumgesicht;", "tokens": ["Aus", "ei\u00b7nem", "Traum\u00b7ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Mund, Heil mir! voll Lachen,", "tokens": ["Der", "Mund", ",", "Heil", "mir", "!", "voll", "La\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "PPER", "$.", "ADJD", "NN", "$,"], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Das Herz bestr\u00f6mt mit Licht!", "tokens": ["Das", "Herz", "be\u00b7str\u00f6mt", "mit", "Licht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dann jauchz' ich: Gott hat Gro\u00dfes", "tokens": ["Dann", "jauch\u00b7z'", "ich", ":", "Gott", "hat", "Gro\u00b7\u00dfes"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "NN", "VAFIN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "An mir gethan;", "tokens": ["An", "mir", "ge\u00b7than", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "De\u00df bin ich fr\u00f6hlich, Gro\u00dfes", "tokens": ["De\u00df", "bin", "ich", "fr\u00f6h\u00b7lich", ",", "Gro\u00b7\u00dfes"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PDS", "VAFIN", "PPER", "ADJD", "$,", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hat Gott an mir gethan!", "tokens": ["Hat", "Gott", "an", "mir", "ge\u00b7than", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Preis' ihn in Lobges\u00e4ngen,", "tokens": ["Preis'", "ihn", "in", "Lob\u00b7ge\u00b7s\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Die Fesseln sind entzwei;", "tokens": ["Die", "Fes\u00b7seln", "sind", "ent\u00b7zwei", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Eisenriegel sprengen", "tokens": ["Die", "Ei\u00b7sen\u00b7rie\u00b7gel", "spren\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und ich bin frei, bin frei!", "tokens": ["Und", "ich", "bin", "frei", ",", "bin", "frei", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "$,", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "So wende mein Gef\u00e4ngnis,", "tokens": ["So", "wen\u00b7de", "mein", "Ge\u00b7f\u00e4ng\u00b7nis", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Herr! mach' mich los!", "tokens": ["Herr", "!", "mach'", "mich", "los", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Mein trauriges Verh\u00e4ngnis", "tokens": ["Mein", "trau\u00b7ri\u00b7ges", "Ver\u00b7h\u00e4ng\u00b7nis"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Scheint mir zu gro\u00df!", "tokens": ["Scheint", "mir", "zu", "gro\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKA", "ADJD", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Bald sind des Lebens Fluten", "tokens": ["Bald", "sind", "des", "Le\u00b7bens", "Flu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vertrocknet, wie ein Bach;", "tokens": ["Ver\u00b7trock\u00b7net", ",", "wie", "ein", "Bach", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "O la\u00df es nicht verbluten,", "tokens": ["O", "la\u00df", "es", "nicht", "ver\u00b7blu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dies Herz im Ungemach!", "tokens": ["Dies", "Herz", "im", "Un\u00b7ge\u00b7mach", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Doch die mit Thr\u00e4nen s\u00e4en,", "tokens": ["Doch", "die", "mit", "Thr\u00e4\u00b7nen", "s\u00e4\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gefangne, l\u00e4\u00dft", "tokens": ["Ge\u00b7fang\u00b7ne", ",", "l\u00e4\u00dft"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Der Herr mit Jauchzen gehen", "tokens": ["Der", "Herr", "mit", "Jauch\u00b7zen", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Aufs Erntefest.", "tokens": ["Aufs", "Ern\u00b7te\u00b7fest", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Sie gehen hin und weinen,", "tokens": ["Sie", "ge\u00b7hen", "hin", "und", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und tragen edle Frucht,", "tokens": ["Und", "tra\u00b7gen", "ed\u00b7le", "Frucht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Am Tage, wenn die Seinen", "tokens": ["Am", "Ta\u00b7ge", ",", "wenn", "die", "Sei\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Herr der Ernte sucht.", "tokens": ["Der", "Herr", "der", "Ern\u00b7te", "sucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die ihr mit Fesselnarben", "tokens": ["Die", "ihr", "mit", "Fes\u00b7sel\u00b7nar\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Kerkern weint,", "tokens": ["In", "Ker\u00b7kern", "weint", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Einst bringt ihr eure Garben", "tokens": ["Einst", "bringt", "ihr", "eu\u00b7re", "Gar\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dem Menschenfreund!", "tokens": ["Dem", "Men\u00b7schen\u00b7freund", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "O Tag der Wonn' und Psalmen,", "tokens": ["O", "Tag", "der", "Wonn'", "und", "Psal\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "O Erntetag brich an!", "tokens": ["O", "Ern\u00b7te\u00b7tag", "brich", "an", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df ich auch meine Halmen", "tokens": ["Da\u00df", "ich", "auch", "mei\u00b7ne", "Hal\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Frohlockend bringen kann!", "tokens": ["Froh\u00b7lo\u00b7ckend", "brin\u00b7gen", "kann", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}