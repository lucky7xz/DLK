{"textgrid.poem.67461": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Die zehnte Muse", "genre": "verse", "period": "N.A.", "pub_year": 1789, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hohe Lehrerin, Noth, und treffliche Sch\u00fclerin, Armuth,", "tokens": ["Ho\u00b7he", "Leh\u00b7re\u00b7rin", ",", "Noth", ",", "und", "treff\u00b7li\u00b7che", "Sch\u00fc\u00b7le\u00b7rin", ",", "Ar\u00b7muth", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "$,", "KON", "ADJA", "NN", "$,", "NN", "$,"], "meter": "+-+--+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Zehnte Muse der Welt, o Du erfandest so viel.", "tokens": ["Zehn\u00b7te", "Mu\u00b7se", "der", "Welt", ",", "o", "Du", "er\u00b7fan\u00b7dest", "so", "viel", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,", "FM", "PPER", "VVFIN", "ADV", "ADV", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Nicht nur sch\u00e4rfetest Du den Witz der Pflegebefohlnen;", "tokens": ["Nicht", "nur", "sch\u00e4r\u00b7fe\u00b7test", "Du", "den", "Witz", "der", "Pfle\u00b7ge\u00b7be\u00b7fohl\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Noch eine sch\u00f6nere Kunst, M\u00e4\u00dfigung, lehrtest Du sie.", "tokens": ["Noch", "ei\u00b7ne", "sch\u00f6\u00b7ne\u00b7re", "Kunst", ",", "M\u00e4\u00b7\u00dfi\u00b7gung", ",", "lehr\u00b7test", "Du", "sie", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "NN", "$,", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Und die M\u00e4\u00dfigung ward ihr Gewohnheit, Gewohnheit zur Freude;", "tokens": ["Und", "die", "M\u00e4\u00b7\u00dfi\u00b7gung", "ward", "ihr", "Ge\u00b7wohn\u00b7heit", ",", "Ge\u00b7wohn\u00b7heit", "zur", "Freu\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$,", "NN", "APPRART", "NN", "$."], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.6": {"text": "Freude machte sie dann \u00fcber den Reichesten reich.", "tokens": ["Freu\u00b7de", "mach\u00b7te", "sie", "dann", "\u00fc\u00b7ber", "den", "Rei\u00b7ches\u00b7ten", "reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}}}}}