{"dta.poem.21317": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput V.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Hosen sind roth und nicht mehr wei\u00df,", "tokens": ["Die", "Ho\u00b7sen", "sind", "roth", "und", "nicht", "mehr", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie haben auch andere Kn\u00f6pfe,", "tokens": ["Sie", "ha\u00b7ben", "auch", "an\u00b7de\u00b7re", "Kn\u00f6p\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Sie singen nicht mehr, sie springen nicht mehr,", "tokens": ["Sie", "sin\u00b7gen", "nicht", "mehr", ",", "sie", "sprin\u00b7gen", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "$,", "PPER", "VVFIN", "PTKNEG", "ADV", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie senken nachdenklich die K\u00f6pfe.", "tokens": ["Sie", "sen\u00b7ken", "nach\u00b7denk\u00b7lich", "die", "K\u00f6p\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Sie philosophiren und sprechen jetzt", "tokens": ["Sie", "phi\u00b7lo\u00b7so\u00b7phi\u00b7ren", "und", "spre\u00b7chen", "jetzt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVINF", "KON", "VVFIN", "ADV"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Von Kant, von Fischte und Hegel,", "tokens": ["Von", "Kant", ",", "von", "Fischte", "und", "He\u00b7gel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie rauchen Tabak, sie trinken Bier,", "tokens": ["Sie", "rau\u00b7chen", "Ta\u00b7bak", ",", "sie", "trin\u00b7ken", "Bier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "PPER", "VVFIN", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und manche schieben auch Kegel.", "tokens": ["Und", "man\u00b7che", "schie\u00b7ben", "auch", "Ke\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADV", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Sie werden Philister ganz wie wir", "tokens": ["Sie", "wer\u00b7den", "Phi\u00b7lis\u00b7ter", "ganz", "wie", "wir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "ADV", "KOKOM", "PPER"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und treiben es endlich noch \u00e4rger;", "tokens": ["Und", "trei\u00b7ben", "es", "end\u00b7lich", "noch", "\u00e4r\u00b7ger", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Sie sind keine Voltairianer mehr,", "tokens": ["Sie", "sind", "kei\u00b7ne", "Vol\u00b7tai\u00b7ri\u00b7a\u00b7ner", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "ADV", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Sie werden Hengstenberger.", "tokens": ["Sie", "wer\u00b7den", "Hengs\u00b7ten\u00b7ber\u00b7ger", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Alphred de M\u00fcsset, das ist wahr,", "tokens": ["Der", "Al\u00b7ph\u00b7red", "de", "M\u00fcs\u00b7set", ",", "das", "ist", "wahr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NE", "NE", "$,", "PDS", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ist noch ein Gassenjunge;", "tokens": ["Ist", "noch", "ein", "Gas\u00b7sen\u00b7jun\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch f\u00fcrchte nichts, wir fesseln ihm", "tokens": ["Doch", "f\u00fcrch\u00b7te", "nichts", ",", "wir", "fes\u00b7seln", "ihm"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIS", "$,", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die sch\u00e4ndliche Sp\u00f6tterzunge.", "tokens": ["Die", "sch\u00e4nd\u00b7li\u00b7che", "Sp\u00f6t\u00b7ter\u00b7zun\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}