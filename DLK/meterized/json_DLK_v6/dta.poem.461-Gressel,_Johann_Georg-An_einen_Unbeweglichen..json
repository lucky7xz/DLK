{"dta.poem.461": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An einen Unbeweglichen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn Purpur und Coral/ der Lippen Wunder-Sachen", "tokens": ["Wenn", "Pur\u00b7pur", "und", "Co\u00b7ral", "/", "der", "Lip\u00b7pen", "Wun\u00b7der\u00b7Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "$(", "ART", "NN", "NN"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Der angenehme Ku\u00df das holde W\u00f6rter-machen", "tokens": ["Der", "an\u00b7ge\u00b7neh\u00b7me", "Ku\u00df", "das", "hol\u00b7de", "W\u00f6r\u00b7ter\u00b7ma\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein Ernst und Freundlich-seyn/ ein \u00fcber-zuckter Mund", "tokens": ["Ein", "Ernst", "und", "Freund\u00b7lich\u00b7seyn", "/", "ein", "\u00fc\u00b7ber\u00b7zuck\u00b7ter", "Mund"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dein Auge nicht entz\u00fcndt und deinen Sinn verwundt/", "tokens": ["Dein", "Au\u00b7ge", "nicht", "ent\u00b7z\u00fcndt", "und", "dei\u00b7nen", "Sinn", "ver\u00b7wundt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "VVPP", "KON", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wenn Fleisch das Sammet ist/ und Adern die ", "tokens": ["Wenn", "Fleisch", "das", "Sam\u00b7met", "ist", "/", "und", "A\u00b7dern", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "NN", "ART", "NN", "VAFIN", "$(", "KON", "NN", "ART"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die Haut/ so weisser Schnee/ dein Hertze nicht verf\u00fchren?", "tokens": ["Die", "Haut", "/", "so", "weis\u00b7ser", "Schnee", "/", "dein", "Hert\u00b7ze", "nicht", "ver\u00b7f\u00fch\u00b7ren", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "ADJA", "NN", "$(", "PPOSAT", "VVFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So bist du gar kein Mensch ein Stock und harter Stein.", "tokens": ["So", "bist", "du", "gar", "kein", "Mensch", "ein", "Stock", "und", "har\u00b7ter", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PIAT", "NN", "ART", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dem keiner Sch\u00f6nheit-Pracht/ erreget Liebes-Pein.", "tokens": ["Dem", "kei\u00b7ner", "Sch\u00f6n\u00b7heit\u00b7Pracht", "/", "er\u00b7re\u00b7get", "Lie\u00b7bes\u00b7Pein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$(", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}