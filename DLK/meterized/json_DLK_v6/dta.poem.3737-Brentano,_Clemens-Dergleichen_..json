{"dta.poem.3737": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Dergleichen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gickes gackes Eyermu\u00df,               ", "tokens": ["Gi\u00b7ckes", "ga\u00b7ckes", "Ey\u00b7er\u00b7mu\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "G\u00e4nse laufen barfu\u00df,", "tokens": ["G\u00e4n\u00b7se", "lau\u00b7fen", "bar\u00b7fu\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Hinterm Ofen steht sie,", "tokens": ["Hin\u00b7term", "O\u00b7fen", "steht", "sie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Vor dem Ofen geht sie,", "tokens": ["Vor", "dem", "O\u00b7fen", "geht", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Hat sie Schuh,", "tokens": ["Hat", "sie", "Schuh", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Sie legt sie an.", "tokens": ["Sie", "legt", "sie", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Hat sie keine,", "tokens": ["Hat", "sie", "kei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "So kauft sie ein Paar.", "tokens": ["So", "kauft", "sie", "ein", "Paar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}