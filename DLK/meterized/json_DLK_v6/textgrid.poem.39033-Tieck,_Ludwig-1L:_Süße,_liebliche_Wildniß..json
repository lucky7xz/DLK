{"textgrid.poem.39033": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: S\u00fc\u00dfe, liebliche Wildni\u00df.", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "S\u00fc\u00dfe, liebliche Wildni\u00df.", "tokens": ["S\u00fc\u00b7\u00dfe", ",", "lieb\u00b7li\u00b7che", "Wild\u00b7ni\u00df", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Deine Berge, dein Kloster oben,", "tokens": ["Dei\u00b7ne", "Ber\u00b7ge", ",", "dein", "Klos\u00b7ter", "o\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "ADV", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Die Cypressen, die Th\u00e4ler hier,", "tokens": ["Die", "Cyp\u00b7res\u00b7sen", ",", "die", "Th\u00e4\u00b7ler", "hier", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Werden nie aus meiner Seele schwinden.", "tokens": ["Wer\u00b7den", "nie", "aus", "mei\u00b7ner", "See\u00b7le", "schwin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Hoher Gedanken, sch\u00f6ner Bilder Erinnerung,", "tokens": ["Ho\u00b7her", "Ge\u00b7dan\u00b7ken", ",", "sch\u00f6\u00b7ner", "Bil\u00b7der", "E\u00b7rin\u00b7ne\u00b7rung", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "NN", "$,"], "meter": "+--+-+-+--+--", "measure": "iambic.penta.invert"}, "line.6": {"text": "Wunder der Natur,", "tokens": ["Wun\u00b7der", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Weile gastlich in meiner Phantasie,", "tokens": ["Wei\u00b7le", "gast\u00b7lich", "in", "mei\u00b7ner", "Phan\u00b7ta\u00b7sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Wenn wieder Sand und F\u00f6hren", "tokens": ["Wenn", "wie\u00b7der", "Sand", "und", "F\u00f6h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Mich geistlos umstarren,", "tokens": ["Mich", "geist\u00b7los", "um\u00b7star\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Und ein L\u00e4cheln zweifelnd", "tokens": ["Und", "ein", "L\u00e4\u00b7cheln", "zwei\u00b7felnd"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.11": {"text": "Bei Schilderungen der Natur", "tokens": ["Bei", "Schil\u00b7de\u00b7run\u00b7gen", "der", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Des Entz\u00fcckten spotten m\u00f6chte.", "tokens": ["Des", "Ent\u00b7z\u00fcck\u00b7ten", "spot\u00b7ten", "m\u00f6ch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Hier dichtet die Erde,", "tokens": ["Hier", "dich\u00b7tet", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Dort schl\u00e4ft sie kaum,", "tokens": ["Dort", "schl\u00e4ft", "sie", "kaum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Befangen, angstvoll,", "tokens": ["Be\u00b7fan\u00b7gen", ",", "angst\u00b7voll", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Ringt sie nur nach Dasein:", "tokens": ["Ringt", "sie", "nur", "nach", "Da\u00b7sein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "++-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und goldne Abendwolken", "tokens": ["Und", "gold\u00b7ne", "Ab\u00b7end\u00b7wol\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und gl\u00fchend Morgenroth", "tokens": ["Und", "gl\u00fc\u00b7hend", "Mor\u00b7gen\u00b7roth"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Schweben nur als Kranken-Tr\u00e4ume", "tokens": ["Schwe\u00b7ben", "nur", "als", "Kran\u00b7ken\u00b7Tr\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "KOUS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ob der Verscheidenden.", "tokens": ["Ob", "der", "Ver\u00b7schei\u00b7den\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$."], "meter": "+--+--", "measure": "dactylic.di.plus"}}}}}