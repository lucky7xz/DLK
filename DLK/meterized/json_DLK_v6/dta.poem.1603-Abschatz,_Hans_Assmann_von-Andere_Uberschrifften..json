{"dta.poem.1603": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Andere Uberschrifften.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Die Sonne weiset mir den Weg zur finstern Grufft/", "tokens": ["Die", "Son\u00b7ne", "wei\u00b7set", "mir", "den", "Weg", "zur", "fins\u00b7tern", "Grufft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich scheide/ da ich kaum gesch\u00f6pffet frische Lufft/", "tokens": ["Ich", "schei\u00b7de", "/", "da", "ich", "kaum", "ge\u00b7sch\u00f6pf\u00b7fet", "fri\u00b7sche", "Lufft", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPER", "ADV", "VVPP", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Erd umwindelt mich/ der Sarg ist meine Wiege/", "tokens": ["Die", "Erd", "um\u00b7win\u00b7delt", "mich", "/", "der", "Sarg", "ist", "mei\u00b7ne", "Wie\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$(", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch wohl mir/ da\u00df ich hier von St\u00fcrmen sicher liege!", "tokens": ["Doch", "wohl", "mir", "/", "da\u00df", "ich", "hier", "von", "St\u00fcr\u00b7men", "si\u00b7cher", "lie\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "$(", "KOUS", "PPER", "ADV", "APPR", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich schlaff in GOttes Hutt/ von Angst und Sorge frey/", "tokens": ["Ich", "schlaff", "in", "Got\u00b7tes", "Hutt", "/", "von", "Angst", "und", "Sor\u00b7ge", "frey", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "NN", "$(", "APPR", "NN", "KON", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Denckt/ ob ich wachend nicht/ eur Leben schlaffend sey.", "tokens": ["Denckt", "/", "ob", "ich", "wa\u00b7chend", "nicht", "/", "eur", "Le\u00b7ben", "schlaf\u00b7fend", "sey", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "KOUS", "PPER", "ADJD", "PTKNEG", "$(", "PPOSAT", "NN", "VVPP", "VAFIN", "$."], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}}}}}