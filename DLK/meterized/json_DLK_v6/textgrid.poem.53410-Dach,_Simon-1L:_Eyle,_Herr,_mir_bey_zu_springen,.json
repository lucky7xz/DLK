{"textgrid.poem.53410": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Eyle, Herr, mir bey zu springen,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eyle, Herr, mir bey zu springen,", "tokens": ["Ey\u00b7le", ",", "Herr", ",", "mir", "bey", "zu", "sprin\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PPER", "APPR", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir zu helffen nun o Gott!", "tokens": ["Mir", "zu", "helf\u00b7fen", "nun", "o", "Gott", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "ADV", "FM", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die nach meiner Seelen ringen,", "tokens": ["Die", "nach", "mei\u00b7ner", "See\u00b7len", "rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00fcssen tragen Schmach und Spott.", "tokens": ["M\u00fcs\u00b7sen", "tra\u00b7gen", "Schmach", "und", "Spott", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Schamroht m\u00fcssen alle stehn,", "tokens": ["Scham\u00b7roht", "m\u00fcs\u00b7sen", "al\u00b7le", "stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Welcher Wunsch ist und Verlangen,", "tokens": ["Wel\u00b7cher", "Wunsch", "ist", "und", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "VAFIN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df es mir sol \u00fcbel gehn,", "tokens": ["Da\u00df", "es", "mir", "sol", "\u00fc\u00b7bel", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schande m\u00fcsse Sie umbfangen,", "tokens": ["Schan\u00b7de", "m\u00fcs\u00b7se", "Sie", "umb\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Da\u00df sie wiederumb auf Erden,", "tokens": ["Da\u00df", "sie", "wie\u00b7de\u00b7rumb", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stets vernichtet m\u00fcssen seyn,", "tokens": ["Stets", "ver\u00b7nich\u00b7tet", "m\u00fcs\u00b7sen", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VMFIN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vnd durchaus geh\u00f6net werden,", "tokens": ["Vnd", "durc\u00b7haus", "ge\u00b7h\u00f6\u00b7net", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die mir h\u00e4uffen meine Pein,", "tokens": ["Die", "mir", "h\u00e4uf\u00b7fen", "mei\u00b7ne", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mich verachten da und hier,", "tokens": ["Mich", "ver\u00b7ach\u00b7ten", "da", "und", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KON", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Die mein Vnfall mu\u00df erfrewen,", "tokens": ["Die", "mein", "Vn\u00b7fall", "mu\u00df", "er\u00b7fre\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Die sich k\u00fctzeln \u00fcber mir,", "tokens": ["Die", "sich", "k\u00fct\u00b7zeln", "\u00fc\u00b7ber", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "VVFIN", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Vnd: da da! ohn Abla\u00df schreyen.", "tokens": ["Vnd", ":", "da", "da", "!", "ohn", "Ab\u00b7la\u00df", "schre\u00b7yen", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$.", "ADV", "ADV", "$.", "APPR", "NN", "VVINF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Frewd und Fr\u00f6ligkeit hergegen", "tokens": ["Frewd", "und", "Fr\u00f6\u00b7lig\u00b7keit", "her\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kr\u00f6ne die, so flei\u00dfig sich", "tokens": ["Kr\u00f6\u00b7ne", "die", ",", "so", "flei\u00b7\u00dfig", "sich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "$,", "ADV", "ADJD", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vmbthun, Herr, nach deinen Wegen,", "tokens": ["Vmb\u00b7thun", ",", "Herr", ",", "nach", "dei\u00b7nen", "We\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nach dir fragen inniglich,", "tokens": ["Nach", "dir", "fra\u00b7gen", "in\u00b7nig\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Vnd die, Gott, dein Heyl ergetzt,", "tokens": ["Vnd", "die", ",", "Gott", ",", "dein", "Heyl", "er\u00b7getzt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "NN", "$,", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dieser Lied mu\u00df immer bleiben:", "tokens": ["Die\u00b7ser", "Lied", "mu\u00df", "im\u00b7mer", "blei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VMFIN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Gottes Lob sey hoch gesch\u00e4tzt,", "tokens": ["Got\u00b7tes", "Lob", "sey", "hoch", "ge\u00b7sch\u00e4tzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Weil sich Tag und N\u00e4chte treiben.", "tokens": ["Weil", "sich", "Tag", "und", "N\u00e4ch\u00b7te", "trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Aber ich bin gantz verlassen,", "tokens": ["A\u00b7ber", "ich", "bin", "gantz", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vnd mein Elend dr\u00fcckt mich sehr,", "tokens": ["Vnd", "mein", "E\u00b7lend", "dr\u00fcckt", "mich", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Komm, dich meiner anzumassen,", "tokens": ["Komm", ",", "dich", "mei\u00b7ner", "an\u00b7zu\u00b7mas\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "PPOSAT", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auff, gewehr mir H\u00fclff und Ehr,", "tokens": ["Auff", ",", "ge\u00b7wehr", "mir", "H\u00fclff", "und", "Ehr", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "ADV", "PPER", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mein Erretter, Gott, bist du,", "tokens": ["Mein", "Er\u00b7ret\u00b7ter", ",", "Gott", ",", "bist", "du", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "VAFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Schaw wo ich sey hin gediehen,", "tokens": ["Schaw", "wo", "ich", "sey", "hin", "ge\u00b7die\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWAV", "PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Eyle, sey mein Schutz und Ruh,", "tokens": ["Ey\u00b7le", ",", "sey", "mein", "Schutz", "und", "Ruh", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Fleuch, mein Gott, doch zu verziehen.", "tokens": ["Fleuch", ",", "mein", "Gott", ",", "doch", "zu", "ver\u00b7zie\u00b7hen", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}