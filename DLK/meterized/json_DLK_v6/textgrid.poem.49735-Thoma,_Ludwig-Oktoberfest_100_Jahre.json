{"textgrid.poem.49735": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Oktoberfest 100 Jahre", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lasset uns, ihr edlen Bayern", "tokens": ["Las\u00b7set", "uns", ",", "ihr", "ed\u00b7len", "Bay\u00b7ern"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u2013 Untertanen! Publikum! \u2013,", "tokens": ["\u2013", "Un\u00b7ter\u00b7ta\u00b7nen", "!", "Pub\u00b7li\u00b7kum", "!", "\u2013", ","], "token_info": ["punct", "word", "punct", "word", "punct", "punct", "punct"], "pos": ["$(", "NN", "$.", "NN", "$.", "$(", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit gehobnen Herzen feiern", "tokens": ["Mit", "ge\u00b7hob\u00b7nen", "Her\u00b7zen", "fei\u00b7ern"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieses stolze saeculum!", "tokens": ["Die\u00b7ses", "stol\u00b7ze", "sae\u00b7cu\u00b7lum", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "H\u00fchner-, G\u00e4ns- und Heringsbrater,", "tokens": ["H\u00fch\u00b7ner", ",", "G\u00e4ns", "und", "He\u00b7rings\u00b7bra\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["TRUNC", "$,", "TRUNC", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heute seid ihr zentenar!", "tokens": ["Heu\u00b7te", "seid", "ihr", "zen\u00b7te\u00b7nar", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dank so manchem Landesvater,", "tokens": ["Dank", "so", "man\u00b7chem", "Lan\u00b7des\u00b7va\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der euch mild gewogen war.", "tokens": ["Der", "euch", "mild", "ge\u00b7wo\u00b7gen", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Stier- und Sau- und Ochsentreiber,", "tokens": ["Stier", "und", "Sau", "und", "Och\u00b7sen\u00b7trei\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["TRUNC", "KON", "TRUNC", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heute f\u00fchlt euch vaterl\u00e4nd'sch!", "tokens": ["Heu\u00b7te", "f\u00fchlt", "euch", "va\u00b7ter\u00b7l\u00e4n\u00b7d'sch", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Brezelfrauen! Radiweiber!", "tokens": ["Bre\u00b7zel\u00b7frau\u00b7en", "!", "Ra\u00b7di\u00b7wei\u00b7ber", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$.", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jedes alte Kuchelmensch!", "tokens": ["Je\u00b7des", "al\u00b7te", "Ku\u00b7chel\u00b7mensch", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Schweige jeder Widersacher!", "tokens": ["Schwei\u00b7ge", "je\u00b7der", "Wi\u00b7der\u00b7sa\u00b7cher", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn noch bl\u00fchen sie uns frisch:", "tokens": ["Denn", "noch", "bl\u00fc\u00b7hen", "sie", "uns", "frisch", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Treue f\u00fcr die Wittelsbacher,", "tokens": ["Treu\u00b7e", "f\u00fcr", "die", "Wit\u00b7tels\u00b7ba\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wiesenma\u00df und Steckerlfisch!", "tokens": ["Wie\u00b7sen\u00b7ma\u00df", "und", "Ste\u00b7ckerl\u00b7fisch", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Nein! Noch ist es keine L\u00fcge,", "tokens": ["Nein", "!", "Noch", "ist", "es", "kei\u00b7ne", "L\u00fc\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ADV", "VAFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df man treu und bieder denkt!", "tokens": ["Da\u00df", "man", "treu", "und", "bie\u00b7der", "denkt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hebet hoch die Literkr\u00fcge,", "tokens": ["He\u00b7bet", "hoch", "die", "Li\u00b7ter\u00b7kr\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit drei Quarteln eingeschenkt!", "tokens": ["Mit", "drei", "Quar\u00b7teln", "ein\u00b7ge\u00b7schenkt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}