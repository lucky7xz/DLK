{"textgrid.poem.45752": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[dankverse an K\u00f6nig Ludwig II. von Bayern]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein hoher F\u00fcrst w\u00fcnscht einem Dichter Gl\u00fcck!", "tokens": ["Ein", "ho\u00b7her", "F\u00fcrst", "w\u00fcnscht", "ei\u00b7nem", "Dich\u00b7ter", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ist das erh\u00f6rt in unserm deutschen Lande?", "tokens": ["Ist", "das", "er\u00b7h\u00f6rt", "in", "un\u00b7serm", "deut\u00b7schen", "Lan\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zwar, denk an deine V\u00e4ter ich zur\u00fcck,", "tokens": ["Zwar", ",", "denk", "an", "dei\u00b7ne", "V\u00e4\u00b7ter", "ich", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PRELS", "APPR", "PPOSAT", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So hielten sie sichs auch f\u00fcr keine Schande.", "tokens": ["So", "hiel\u00b7ten", "sie", "sichs", "auch", "f\u00fcr", "kei\u00b7ne", "Schan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Kunstliebe ist ein sch\u00f6nes Morgenrot", "tokens": ["Kunst\u00b7lie\u00b7be", "ist", "ein", "sch\u00f6\u00b7nes", "Mor\u00b7gen\u00b7rot"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "F\u00fcr einen Arbeitstag im Sonnenbrande.", "tokens": ["F\u00fcr", "ei\u00b7nen", "Ar\u00b7beits\u00b7tag", "im", "Son\u00b7nen\u00b7bran\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Machst du einst wahr, was edle Dichtung bot,", "tokens": ["Machst", "du", "einst", "wahr", ",", "was", "ed\u00b7le", "Dich\u00b7tung", "bot", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "$,", "PWS", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "So w\u00fcnsch ich Gl\u00fcck nicht dir nur, deinem Lande.", "tokens": ["So", "w\u00fcnsch", "ich", "Gl\u00fcck", "nicht", "dir", "nur", ",", "dei\u00b7nem", "Lan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "PTKNEG", "PPER", "ADV", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}