{"dta.poem.7788": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Die Geschichte des Davids,  \n K\u00f6nigs in Juda.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198429", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht lang nach dieser Zeit ward Saul dazu erw\u00e4hlt.", "tokens": ["Nicht", "lang", "nach", "die\u00b7ser", "Zeit", "ward", "Saul", "da\u00b7zu", "er\u00b7w\u00e4hlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "APPR", "PDAT", "NN", "VAFIN", "NE", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nun ware doch mein Stand, ob schon erh\u00f6ht, gequ\u00e4lt,", "tokens": ["Nun", "wa\u00b7re", "doch", "mein", "Stand", ",", "ob", "schon", "er\u00b7h\u00f6ht", ",", "ge\u00b7qu\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPOSAT", "NN", "$,", "KOUS", "ADV", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil Adriel mir stets in meinen Sinnen schwebte,", "tokens": ["Weil", "Ad\u00b7riel", "mir", "stets", "in", "mei\u00b7nen", "Sin\u00b7nen", "schweb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und weil ich nichts von ihm erfuhre, traurig lebte.", "tokens": ["Und", "weil", "ich", "nichts", "von", "ihm", "er\u00b7fuh\u00b7re", ",", "trau\u00b7rig", "leb\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PIS", "APPR", "PPER", "VVFIN", "$,", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Von Joel war ich frey, der mit dem Richterstand", "tokens": ["Von", "Joel", "war", "ich", "frey", ",", "der", "mit", "dem", "Rich\u00b7ter\u00b7stand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NE", "VAFIN", "PPER", "ADJD", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "Zugleich sein Leben lie\u00df. Doch bliebe mir entwandt", "tokens": ["Zu\u00b7gleich", "sein", "Le\u00b7ben", "lie\u00df", ".", "Doch", "blie\u00b7be", "mir", "ent\u00b7wandt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "$.", "KON", "VVFIN", "PPER", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Ruhe wie zuvor. Ich lebt in steten Sorgen,", "tokens": ["Die", "Ru\u00b7he", "wie", "zu\u00b7vor", ".", "Ich", "lebt", "in", "ste\u00b7ten", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "ADV", "$.", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und da die\u00df Liebesfeur war aller Welt verborgen,", "tokens": ["Und", "da", "die\u00df", "Lie\u00b7bes\u00b7feur", "war", "al\u00b7ler", "Welt", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PDS", "NN", "VAFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So brannt es drum vielmehr in meinem innern Sinn.", "tokens": ["So", "brannt", "es", "drum", "viel\u00b7mehr", "in", "mei\u00b7nem", "in\u00b7nern", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PAV", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ich bin nicht in mir selbst, was ich bey Leuten bin.", "tokens": ["Ich", "bin", "nicht", "in", "mir", "selbst", ",", "was", "ich", "bey", "Leu\u00b7ten", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "APPR", "PPER", "ADV", "$,", "PWS", "PPER", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "In solcher Trauerzeit ward unser Land geehret", "tokens": ["In", "sol\u00b7cher", "Trau\u00b7er\u00b7zeit", "ward", "un\u00b7ser", "Land", "ge\u00b7eh\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Mit deiner Gegenwart; Doch anfangs ward vermehret", "tokens": ["Mit", "dei\u00b7ner", "Ge\u00b7gen\u00b7wart", ";", "Doch", "an\u00b7fangs", "ward", "ver\u00b7meh\u00b7ret"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$.", "KON", "ADV", "VAFIN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Mit dir mein qu\u00e4lend Hertz, weil Adriel mit kam,", "tokens": ["Mit", "dir", "mein", "qu\u00e4\u00b7lend", "Hertz", ",", "weil", "Ad\u00b7riel", "mit", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPOSAT", "ADJD", "NN", "$,", "KOUS", "NE", "APPR", "VVFIN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.14": {"text": "Und sich also erwie\u00df, da\u00df ich den Eifer nahm", "tokens": ["Und", "sich", "al\u00b7so", "er\u00b7wie\u00df", ",", "da\u00df", "ich", "den", "Ei\u00b7fer", "nahm"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PRF", "ADV", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "F\u00fcr Liebe in den Sinn. Ich dacht ich bin verlassen,", "tokens": ["F\u00fcr", "Lie\u00b7be", "in", "den", "Sinn", ".", "Ich", "dacht", "ich", "bin", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$.", "PPER", "VVFIN", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Thalmais liebet er, wohlan, ich will ihn hassen;", "tokens": ["Thal\u00b7mais", "lie\u00b7bet", "er", ",", "wo\u00b7hlan", ",", "ich", "will", "ihn", "has\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "PWAV", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.17": {"text": "Die\u00df hassen aber kam mir saurer an als ihm.", "tokens": ["Die\u00df", "has\u00b7sen", "a\u00b7ber", "kam", "mir", "sau\u00b7rer", "an", "als", "ihm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "VVFIN", "PPER", "ADJD", "APPR", "KOUS", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ich f\u00fchlte nur die Qual, empfande einen Grimm,", "tokens": ["Ich", "f\u00fchl\u00b7te", "nur", "die", "Qual", ",", "emp\u00b7fan\u00b7de", "ei\u00b7nen", "Grimm", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,", "VVFIN", "ART", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Der mir mein Hertz abfra\u00df. Und wie nun dein begehrte", "tokens": ["Der", "mir", "mein", "Hertz", "ab\u00b7fra\u00df", ".", "Und", "wie", "nun", "dein", "be\u00b7gehr\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VVFIN", "$.", "KON", "PWAV", "ADV", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Mein Bruder Jonathan, ich ihn hiemit verfehrte,", "tokens": ["Mein", "Bru\u00b7der", "Jo\u00b7na\u00b7than", ",", "ich", "ihn", "hie\u00b7mit", "ver\u00b7fehr\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "$,", "PPER", "PPER", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Da\u00df ich ihm bildte ein, dich liebte Adriel.", "tokens": ["Da\u00df", "ich", "ihm", "bild\u00b7te", "ein", ",", "dich", "lieb\u00b7te", "Ad\u00b7riel", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Das macht die Eifersucht, die die verliebte Seel", "tokens": ["Das", "macht", "die", "Ei\u00b7fer\u00b7sucht", ",", "die", "die", "ver\u00b7lieb\u00b7te", "Seel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Von deinem Jonathan empfand mit solchem W\u00fcten.", "tokens": ["Von", "dei\u00b7nem", "Jo\u00b7na\u00b7than", "emp\u00b7fand", "mit", "sol\u00b7chem", "W\u00fc\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Ich war allein geschickt die\u00df Ungl\u00fcck auszubr\u00fcten,", "tokens": ["Ich", "war", "al\u00b7lein", "ge\u00b7schickt", "die\u00df", "Un\u00b7gl\u00fcck", "aus\u00b7zu\u00b7br\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "PDS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Mir hastu nur den Danck zu geben f\u00fcr die Pein,", "tokens": ["Mir", "has\u00b7tu", "nur", "den", "Danck", "zu", "ge\u00b7ben", "f\u00fcr", "die", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "PTKZU", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Die meine Eifersucht dir Schwester schenckte ein.", "tokens": ["Die", "mei\u00b7ne", "Ei\u00b7fer\u00b7sucht", "dir", "Schwes\u00b7ter", "schenck\u00b7te", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PPER", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Doch als ich endlich sah da\u00df ich mich hatt betrogen,", "tokens": ["Doch", "als", "ich", "end\u00b7lich", "sah", "da\u00df", "ich", "mich", "hatt", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVFIN", "KOUS", "PPER", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und deine Heurath mir die Decke abgezogen,", "tokens": ["Und", "dei\u00b7ne", "Heu\u00b7rath", "mir", "die", "De\u00b7cke", "ab\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Kam durch Beroa Flei\u00df die Sach in solchen Stand,", "tokens": ["Kam", "durch", "Be\u00b7roa", "Flei\u00df", "die", "Sach", "in", "sol\u00b7chen", "Stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NE", "NN", "ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "Da\u00df Adriel mein Hertz und ich das seine kannt.", "tokens": ["Da\u00df", "Ad\u00b7riel", "mein", "Hertz", "und", "ich", "das", "sei\u00b7ne", "kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPOSAT", "NN", "KON", "PPER", "PDS", "PPOSAT", "VVFIN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.31": {"text": "Er sagt mir nach der L\u00e4ng wie Bichri ihn betr\u00fcbet,", "tokens": ["Er", "sagt", "mir", "nach", "der", "L\u00e4ng", "wie", "Bich\u00b7ri", "ihn", "be\u00b7tr\u00fc\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "KOKOM", "NE", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und ich erzehlte ihm was Klagen ich ver\u00fcbet;", "tokens": ["Und", "ich", "er\u00b7zehl\u00b7te", "ihm", "was", "Kla\u00b7gen", "ich", "ver\u00b7\u00fc\u00b7bet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PIS", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Doch konnte unsre Lieb, die nun war neu erneut,", "tokens": ["Doch", "konn\u00b7te", "uns\u00b7re", "Lieb", ",", "die", "nun", "war", "neu", "er\u00b7neut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPOSAT", "NN", "$,", "PRELS", "ADV", "VAFIN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Noch brechen nicht herf\u00fcr, es war noch nicht die Zeit.", "tokens": ["Noch", "bre\u00b7chen", "nicht", "her\u00b7f\u00fcr", ",", "es", "war", "noch", "nicht", "die", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "ADV", "$,", "PPER", "VAFIN", "ADV", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Es mag der K\u00f6nig ihn nicht eben gerne leiden,", "tokens": ["Es", "mag", "der", "K\u00f6\u00b7nig", "ihn", "nicht", "e\u00b7ben", "ger\u00b7ne", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PPER", "PTKNEG", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Weil sein gehabtes Gut er Abner wollt bescheiden.", "tokens": ["Weil", "sein", "ge\u00b7hab\u00b7tes", "Gut", "er", "Ab\u00b7ner", "wollt", "be\u00b7schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "PPER", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Und weil die Rede gieng, der Abner liebte mich,", "tokens": ["Und", "weil", "die", "Re\u00b7de", "gieng", ",", "der", "Ab\u00b7ner", "lieb\u00b7te", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Ist er daher geheim, und muste bergen sich.", "tokens": ["Ist", "er", "da\u00b7her", "ge\u00b7heim", ",", "und", "mus\u00b7te", "ber\u00b7gen", "sich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PAV", "ADJD", "$,", "KON", "ADJA", "VVFIN", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Auch hat noch die\u00df den Groll des K\u00f6nigs viel vermehret,", "tokens": ["Auch", "hat", "noch", "die\u00df", "den", "Groll", "des", "K\u00f6\u00b7nigs", "viel", "ver\u00b7meh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PDS", "ART", "NN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Da\u00df er mit Gera sich, den Saul f\u00fcr andern ehret,", "tokens": ["Da\u00df", "er", "mit", "Ge\u00b7ra", "sich", ",", "den", "Saul", "f\u00fcr", "an\u00b7dern", "eh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NE", "PRF", "$,", "ART", "NN", "APPR", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Geschlagen, als er kam hinwieder in das Land;", "tokens": ["Ge\u00b7schla\u00b7gen", ",", "als", "er", "kam", "hin\u00b7wie\u00b7der", "in", "das", "Land", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Da, ob nun zwischen sie zwar wohl ein Freundschaftsband,", "tokens": ["Da", ",", "ob", "nun", "zwi\u00b7schen", "sie", "zwar", "wohl", "ein", "Freund\u00b7schafts\u00b7band", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ADV", "APPR", "PPER", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Denckt es der K\u00f6nig doch. Und hast du so vernommen,", "tokens": ["Denckt", "es", "der", "K\u00f6\u00b7nig", "doch", ".", "Und", "hast", "du", "so", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "$.", "KON", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.44": {"text": "Wie weit mit Adriel und mir es ist gekommen.", "tokens": ["Wie", "weit", "mit", "Ad\u00b7riel", "und", "mir", "es", "ist", "ge\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPR", "NE", "KON", "PPER", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.45": {"text": "Wir harren nur der Zeit, und leben so vergn\u00fcgt,", "tokens": ["Wir", "har\u00b7ren", "nur", "der", "Zeit", ",", "und", "le\u00b7ben", "so", "ver\u00b7gn\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,", "KON", "VVFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Nun steht bey Gott, da\u00df uns die Eh zusammenf\u00fcgt.", "tokens": ["Nun", "steht", "bey", "Gott", ",", "da\u00df", "uns", "die", "Eh", "zu\u00b7sam\u00b7men\u00b7f\u00fcgt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}