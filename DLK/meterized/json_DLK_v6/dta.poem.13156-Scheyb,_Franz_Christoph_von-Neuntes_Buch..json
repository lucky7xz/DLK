{"dta.poem.13156": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Neuntes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201enun zeig\u2019 ich, wie der Plan zu diesem Bau beschieden:", "tokens": ["\u201e", "nun", "zeig'", "ich", ",", "wie", "der", "Plan", "zu", "die\u00b7sem", "Bau", "be\u00b7schie\u00b7den", ":"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "200\u201dDer Sonne Mittel-Punct wirfft einen Strahl nach S\u00fcden,", "tokens": ["\"", "Der", "Son\u00b7ne", "Mit\u00b7tel\u00b7Punct", "wirfft", "ei\u00b7nen", "Strahl", "nach", "S\u00fc\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201ezugleich erstrecket sich ein andrer Strahl nach West;", "tokens": ["\u201e", "zu\u00b7gleich", "er\u00b7stre\u00b7cket", "sich", "ein", "an\u00b7drer", "Strahl", "nach", "West", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PRF", "ART", "ADJA", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201eso da\u00df ein jeder fast bi\u00df an den Himmel st\u00f6\u00dft.", "tokens": ["\u201e", "so", "da\u00df", "ein", "je\u00b7der", "fast", "bi\u00df", "an", "den", "Him\u00b7mel", "st\u00f6\u00dft", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "KOUS", "ART", "PIS", "ADV", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201eden offnen Raum beschlie\u00dft ein Paar von Regen-B\u00f6gen;", "tokens": ["\u201e", "den", "off\u00b7nen", "Raum", "be\u00b7schlie\u00dft", "ein", "Paar", "von", "Re\u00b7gen\u00b7B\u00f6\u00b7gen", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201edie steigen von dem Strahl einander gleich entgegen;", "tokens": ["\u201e", "die", "stei\u00b7gen", "von", "dem", "Strahl", "ein\u00b7an\u00b7der", "gleich", "ent\u00b7ge\u00b7gen", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "APPR", "ART", "NN", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "205\u201dDa\u00df die Gew\u00f6lbung sich hinauf- und abw\u00e4rts biegt,", "tokens": ["\"", "Da\u00df", "die", "Ge\u00b7w\u00f6l\u00b7bung", "sich", "hin\u00b7auf", "und", "ab\u00b7w\u00e4rts", "biegt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "ART", "NN", "PRF", "TRUNC", "KON", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201eder Farben bunter Schmuck sich nach der Mitte f\u00fcgt;", "tokens": ["\u201e", "der", "Far\u00b7ben", "bun\u00b7ter", "Schmuck", "sich", "nach", "der", "Mit\u00b7te", "f\u00fcgt", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "ADJD", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201ealldorten aber sich in einen Winckel bindet:", "tokens": ["\u201e", "all\u00b7dor\u00b7ten", "a\u00b7ber", "sich", "in", "ei\u00b7nen", "Win\u00b7ckel", "bin\u00b7det", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201edas ist wodurch der Raum sich eingeschlossen findet.", "tokens": ["\u201e", "das", "ist", "wo\u00b7durch", "der", "Raum", "sich", "ein\u00b7ge\u00b7schlos\u00b7sen", "fin\u00b7det", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "PWAV", "ART", "NN", "PRF", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201eder innerliche Plan besteht in solcher Pracht,", "tokens": ["\u201e", "der", "in\u00b7ner\u00b7li\u00b7che", "Plan", "be\u00b7steht", "in", "sol\u00b7cher", "Pracht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "210\u201dDie durch die ganze Welt zweyfache Klarheit macht.", "tokens": ["\"", "Die", "durch", "die", "gan\u00b7ze", "Welt", "zwey\u00b7fa\u00b7che", "Klar\u00b7heit", "macht", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "APPR", "ART", "ADJA", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "\u201eein Wunder-voller Glanz beleuchtet dessen Weiten,", "tokens": ["\u201e", "ein", "Wun\u00b7der\u00b7vol\u00b7ler", "Glanz", "be\u00b7leuch\u00b7tet", "des\u00b7sen", "Wei\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "NN", "VVPP", "PRELAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "\u201eer dringt uneingeschr\u00e4nckt durch alle seine Seiten.", "tokens": ["\u201e", "er", "dringt", "un\u00b7ein\u00b7ge\u00b7schr\u00e4nckt", "durch", "al\u00b7le", "sei\u00b7ne", "Sei\u00b7ten", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADJD", "APPR", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "\u201eein jeder Bogen wird durch eine Sonn\u2019 erkl\u00e4rt,", "tokens": ["\u201e", "ein", "je\u00b7der", "Bo\u00b7gen", "wird", "durch", "ei\u00b7ne", "Sonn'", "er\u00b7kl\u00e4rt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "PIAT", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "\u201eum welche sein Bezirck in gleicher Weite f\u00e4hrt.", "tokens": ["\u201e", "um", "wel\u00b7che", "sein", "Be\u00b7zirck", "in", "glei\u00b7cher", "Wei\u00b7te", "f\u00e4hrt", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PRELS", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "215\u201dWas dessen Majest\u00e4t verherrlichet, bekr\u00e4nzet,", "tokens": ["\"", "Was", "des\u00b7sen", "Ma\u00b7jes\u00b7t\u00e4t", "ver\u00b7herr\u00b7li\u00b7chet", ",", "be\u00b7kr\u00e4n\u00b7zet", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PWS", "PDS", "NN", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "\u201eist selbst des Himmels Bau der dessen Raum begr\u00e4nzet.", "tokens": ["\u201e", "ist", "selbst", "des", "Him\u00b7mels", "Bau", "der", "des\u00b7sen", "Raum", "be\u00b7gr\u00e4n\u00b7zet", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ADV", "ART", "NN", "NN", "ART", "PRELAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "\u201eer schwinget \u00fcber ihn ein Sternen-reiches Dach,", "tokens": ["\u201e", "er", "schwin\u00b7get", "\u00fc\u00b7ber", "ihn", "ein", "Ster\u00b7nen\u00b7rei\u00b7ches", "Dach", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "\u201edie\u00df biegt und neiget sich den beyden B\u00f6gen nach", "tokens": ["\u201e", "die\u00df", "biegt", "und", "nei\u00b7get", "sich", "den", "bey\u00b7den", "B\u00f6\u00b7gen", "nach"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PDS", "VVFIN", "KON", "VVFIN", "PRF", "ART", "PIAT", "NN", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "\u201eund schlie\u00dft den Tempel ein. Hier wollte sie sich neigen,", "tokens": ["\u201e", "und", "schlie\u00dft", "den", "Tem\u00b7pel", "ein", ".", "Hier", "woll\u00b7te", "sie", "sich", "nei\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$.", "ADV", "VMFIN", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "220Und von dem Herz-Geb\u00e4u, so sie beschriebe, schweigen,", "tokens": ["von", "dem", "Her\u00b7zGe\u00b7b\u00e4u", ",", "so", "sie", "be\u00b7schrie\u00b7be", ",", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NE", "$,", "ADV", "PPER", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.23": {"text": "Weil sie gest\u00f6ret ward. Die Bau-Kunst regte sich", "tokens": ["Weil", "sie", "ge\u00b7st\u00f6\u00b7ret", "ward", ".", "Die", "Bau\u00b7Kunst", "reg\u00b7te", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$.", "ART", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und sagte: \u201eDas Geb\u00e4u scheint mir verwunderlich;", "tokens": ["Und", "sag\u00b7te", ":", "\u201e", "Das", "Ge\u00b7b\u00e4u", "scheint", "mir", "ver\u00b7wun\u00b7der\u00b7lich", ";"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "ART", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "\u201ees ist von fremder Kunst; wer mag wohl also bauen?", "tokens": ["\u201e", "es", "ist", "von", "frem\u00b7der", "Kunst", ";", "wer", "mag", "wohl", "al\u00b7so", "bau\u00b7en", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "APPR", "ADJA", "NN", "$.", "PWS", "VMFIN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Die Schilderinn versezt\u2019: \u201eEin feuriges Vertrauen!", "tokens": ["Die", "Schil\u00b7de\u00b7rinn", "ver\u00b7sezt'", ":", "\u201e", "Ein", "feu\u00b7ri\u00b7ges", "Ver\u00b7trau\u00b7en", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "225\u201dWer f\u00fchrte dieses auf? Die Mahler-Kunst fiel ein:", "tokens": ["\"", "Wer", "f\u00fchr\u00b7te", "die\u00b7ses", "auf", "?", "Die", "Mah\u00b7ler\u00b7Kunst", "fiel", "ein", ":"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PDS", "PTKVZ", "$.", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "\u201ees mu\u00df doch in dem Bau so Licht als Schatten seyn!", "tokens": ["\u201e", "es", "mu\u00df", "doch", "in", "dem", "Bau", "so", "Licht", "als", "Schat\u00b7ten", "seyn", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "ADV", "APPR", "ART", "NN", "ADV", "NN", "KOUS", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "\u201emithin ist meine Kunst zu dessen Zier vonn\u00f6then.", "tokens": ["\u201e", "mi\u00b7thin", "ist", "mei\u00b7ne", "Kunst", "zu", "des\u00b7sen", "Zier", "von\u00b7n\u00f6\u00b7then", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPOSAT", "NN", "APPR", "PRELAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Sie wiesen, da\u00df sie nur mit Scherz und Mi\u00dfgunst redten.", "tokens": ["Sie", "wie\u00b7sen", ",", "da\u00df", "sie", "nur", "mit", "Scherz", "und", "Mi\u00df\u00b7gunst", "red\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ADV", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}