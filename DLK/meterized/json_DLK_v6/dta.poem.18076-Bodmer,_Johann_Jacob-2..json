{"dta.poem.18076": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198432", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Roth R\u00f6slein wolt ich brechen", "tokens": ["Roth", "R\u00f6s\u00b7lein", "wolt", "ich", "bre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zum h\u00fcbschen Kr\u00e4ntzelein:", "tokens": ["Zum", "h\u00fcb\u00b7schen", "Kr\u00e4nt\u00b7zel\u00b7ein", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mich D\u00f6rner thaten stechen", "tokens": ["Mich", "D\u00f6r\u00b7ner", "tha\u00b7ten", "ste\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "NE", "VVFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hart in die Finger mein.", "tokens": ["Hart", "in", "die", "Fin\u00b7ger", "mein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Noch wolt ich nit lan ab;", "tokens": ["Noch", "wolt", "ich", "nit", "lan", "ab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ich gunt mich weiter stecken", "tokens": ["Ich", "gunt", "mich", "wei\u00b7ter", "ste\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "In Stauden und in Hecken:", "tokens": ["In", "Stau\u00b7den", "und", "in", "He\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Darinn mirs Wunden gab.", "tokens": ["Da\u00b7rinn", "mirs", "Wun\u00b7den", "gab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O D\u00f6rner krumm und zacket,", "tokens": ["O", "D\u00f6r\u00b7ner", "krumm", "und", "za\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie habt ihr mich zerschrunt?", "tokens": ["Wie", "habt", "ihr", "mich", "zer\u00b7schrunt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer unter euch kommt nacket,", "tokens": ["Wer", "un\u00b7ter", "euch", "kommt", "na\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der ist gar bald verwundt.", "tokens": ["Der", "ist", "gar", "bald", "ver\u00b7wundt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sonst zwar k\u00f6nt ihr nichts mehr:", "tokens": ["Sonst", "zwar", "k\u00f6nt", "ihr", "nichts", "mehr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "PIS", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Jhr keiner Haut thut schonen,", "tokens": ["Ihr", "kei\u00b7ner", "Haut", "thut", "scho\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Noch niedlicher Personen,", "tokens": ["Noch", "nied\u00b7li\u00b7cher", "Per\u00b7so\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.8": {"text": "Wanns gleich ein G\u00f6ttin w\u00e4r.", "tokens": ["Wanns", "gleich", "ein", "G\u00f6t\u00b7tin", "w\u00e4r", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie hats wol selbs erfahren,", "tokens": ["Sie", "hats", "wol", "selbs", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die sch\u00f6ne Venus zart,", "tokens": ["Die", "sch\u00f6\u00b7ne", "Ve\u00b7nus", "zart", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als sie stuhnd in Gefahren,", "tokens": ["Als", "sie", "stuhnd", "in", "Ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und so zerritzet ward.", "tokens": ["Und", "so", "zer\u00b7rit\u00b7zet", "ward", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Daher die R\u00f6slein wei\u00df", "tokens": ["Da\u00b7her", "die", "R\u00f6s\u00b7lein", "wei\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Von Bluttrieffenden Nerben", "tokens": ["Von", "Blut\u00b7trief\u00b7fen\u00b7den", "Ner\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Begunten sich zu ferben:", "tokens": ["Be\u00b7gun\u00b7ten", "sich", "zu", "fer\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Den man verjeht den Prei\u00df.", "tokens": ["Den", "man", "ver\u00b7jeht", "den", "Prei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Ich thu ein Rose loben,", "tokens": ["Ich", "thu", "ein", "Ro\u00b7se", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Rose Tugend voll.", "tokens": ["Ein", "Ro\u00b7se", "Tu\u00b7gend", "voll", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wolt mich mit ihr verloben,", "tokens": ["Wolt", "mich", "mit", "ihr", "ver\u00b7lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wanns ihr gefiele wol.", "tokens": ["Wanns", "ihr", "ge\u00b7fie\u00b7le", "wol", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Jhrs gleichen findt man nicht", "tokens": ["Ihrs", "glei\u00b7chen", "findt", "man", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "VVFIN", "PIS", "PTKNEG"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "In Schwaben und in Francken:", "tokens": ["In", "Schwa\u00b7ben", "und", "in", "Fran\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Mich Schwachen und sehr Krancken", "tokens": ["Mich", "Schwa\u00b7chen", "und", "sehr", "Kran\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "KON", "ADV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sie Tag und Nacht anficht.", "tokens": ["Sie", "Tag", "und", "Nacht", "an\u00b7ficht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nach ihr steht mein Verlangen,", "tokens": ["Nach", "ihr", "steht", "mein", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein sehnlich Hertzegird:", "tokens": ["Mein", "sehn\u00b7lich", "Hert\u00b7ze\u00b7gird", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Am Creutz l\u00e4\u00dft sie mich hangen,", "tokens": ["Am", "Creutz", "l\u00e4\u00dft", "sie", "mich", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Meins Lebens nimmer wird.", "tokens": ["Meins", "Le\u00b7bens", "nim\u00b7mer", "wird", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Zwar bald ich todt mu\u00df seyn.", "tokens": ["Zwar", "bald", "ich", "todt", "mu\u00df", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADJD", "VMFIN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Je weiter sie mich meidet,", "tokens": ["Je", "wei\u00b7ter", "sie", "mich", "mei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Je l\u00e4nger mein Hertz leidet.", "tokens": ["Je", "l\u00e4n\u00b7ger", "mein", "Hertz", "lei\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ist das nit schwere Pein?", "tokens": ["Ist", "das", "nit", "schwe\u00b7re", "Pein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "PTKNEG", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ach liebster Schatz auf Erden,", "tokens": ["Ach", "liebs\u00b7ter", "Schatz", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Warum mich qu\u00e4lest so?", "tokens": ["Wa\u00b7rum", "mich", "qu\u00e4\u00b7lest", "so", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu Theil la\u00df dich mir werden,", "tokens": ["Zu", "Theil", "la\u00df", "dich", "mir", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVIMP", "PPER", "PPER", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und mach mich endlich fro.", "tokens": ["Und", "mach", "mich", "end\u00b7lich", "fro", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dein will ich eigen seyn.", "tokens": ["Dein", "will", "ich", "ei\u00b7gen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VMFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "In Lieb und Treu mich binde,", "tokens": ["In", "Lieb", "und", "Treu", "mich", "bin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Mit deiner Hand mir winde", "tokens": ["Mit", "dei\u00b7ner", "Hand", "mir", "win\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ein Rosenkr\u00e4ntzelein.", "tokens": ["Ein", "Ro\u00b7sen\u00b7kr\u00e4nt\u00b7zel\u00b7ein", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}