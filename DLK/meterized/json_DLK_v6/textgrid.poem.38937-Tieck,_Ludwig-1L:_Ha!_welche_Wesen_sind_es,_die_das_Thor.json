{"textgrid.poem.38937": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ha! welche Wesen sind es, die das Thor", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ha! welche Wesen sind es, die das Thor", "tokens": ["Ha", "!", "wel\u00b7che", "We\u00b7sen", "sind", "es", ",", "die", "das", "Thor"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "PWAT", "NN", "VAFIN", "PPER", "$,", "PRELS", "ART", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Der dunkeln Ahndungen entriegeln?", "tokens": ["Der", "dun\u00b7keln", "Ahn\u00b7dun\u00b7gen", "ent\u00b7rie\u00b7geln", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Was hebt den Geist auf goldbeschwingten Fl\u00fcgeln", "tokens": ["Was", "hebt", "den", "Geist", "auf", "gold\u00b7be\u00b7schwing\u00b7ten", "Fl\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zum sternbes\u00e4ten Himmelsplan empor? \u2013", "tokens": ["Zum", "stern\u00b7be\u00b7s\u00e4\u00b7ten", "Him\u00b7mel\u00b7splan", "em\u00b7por", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es schl\u00e4gt der schwarze Vorhang sich zur\u00fccke,", "tokens": ["Es", "schl\u00e4gt", "der", "schwar\u00b7ze", "Vor\u00b7hang", "sich", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wundervolle Scenen thun sich auf,", "tokens": ["Und", "wun\u00b7der\u00b7vol\u00b7le", "Sce\u00b7nen", "thun", "sich", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Seltsame Gruppen meinem starren Blicke:", "tokens": ["Selt\u00b7sa\u00b7me", "Grup\u00b7pen", "mei\u00b7nem", "star\u00b7ren", "Bli\u00b7cke", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Wie Traumerinnrung stehn sie da! mit frischem Gl\u00fccke", "tokens": ["Wie", "Trau\u00b7me\u00b7rinn\u00b7rung", "stehn", "sie", "da", "!", "mit", "fri\u00b7schem", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "NN", "VVFIN", "PPER", "ADV", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Beginn ich froh den neuen Lebenslauf!", "tokens": ["Be\u00b7ginn", "ich", "froh", "den", "neu\u00b7en", "Le\u00b7bens\u00b7lauf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich f\u00fchle mich von jeder Schmach entbunden,", "tokens": ["Ich", "f\u00fch\u00b7le", "mich", "von", "je\u00b7der", "Schmach", "ent\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die uns vom sch\u00f6nen Taumel r\u00fcckw\u00e4rts h\u00e4lt,", "tokens": ["Die", "uns", "vom", "sch\u00f6\u00b7nen", "Tau\u00b7mel", "r\u00fcck\u00b7w\u00e4rts", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die j\u00e4mmerlichen Ketten sind verschwunden,", "tokens": ["Die", "j\u00e4m\u00b7mer\u00b7li\u00b7chen", "Ket\u00b7ten", "sind", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit Freudejauchzen st\u00fcrzen goldne Stunden", "tokens": ["Mit", "Freu\u00b7de\u00b7jauch\u00b7zen", "st\u00fcr\u00b7zen", "gold\u00b7ne", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Rasch auf mich ein, und ziehn mich tanzend durch die Welt.", "tokens": ["Rasch", "auf", "mich", "ein", ",", "und", "ziehn", "mich", "tan\u00b7zend", "durch", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Es sammeln sich aus den verborgnen Kl\u00fcften", "tokens": ["Es", "sam\u00b7meln", "sich", "aus", "den", "ver\u00b7borg\u00b7nen", "Kl\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die Freuden, wie M\u00e4naden um mich her,", "tokens": ["Die", "Freu\u00b7den", ",", "wie", "M\u00e4\u00b7na\u00b7den", "um", "mich", "her", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es klingen ungesehne Lieder in den L\u00fcften,", "tokens": ["Es", "klin\u00b7gen", "un\u00b7ge\u00b7seh\u00b7ne", "Lie\u00b7der", "in", "den", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es wogt um mich ein ungest\u00fcmes Meer,", "tokens": ["Es", "wogt", "um", "mich", "ein", "un\u00b7ge\u00b7st\u00fc\u00b7mes", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und T\u00f6ne, Jauchzen, Wonne schwebt auf Blumend\u00fcften,", "tokens": ["Und", "T\u00f6\u00b7ne", ",", "Jauch\u00b7zen", ",", "Won\u00b7ne", "schwebt", "auf", "Blu\u00b7men\u00b7d\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "NN", "$,", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und alles st\u00fcrmt um mich, ein wildes Heer.", "tokens": ["Und", "al\u00b7les", "st\u00fcrmt", "um", "mich", ",", "ein", "wil\u00b7des", "Heer", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPR", "PPER", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ich steh im glanzgewebten Feenlande,", "tokens": ["Ich", "steh", "im", "glanz\u00b7ge\u00b7web\u00b7ten", "Fe\u00b7en\u00b7lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und sehe nicht zur d\u00fcrren Welt zur\u00fcck,", "tokens": ["Und", "se\u00b7he", "nicht", "zur", "d\u00fcr\u00b7ren", "Welt", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es fesseln mich nicht irdischschwere Bande,", "tokens": ["Es", "fes\u00b7seln", "mich", "nicht", "ir\u00b7dischschwe\u00b7re", "Ban\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ADJA", "NN", "$,"], "meter": "-+---+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Entsprungen bin ich k\u00fchn dem meisternden Verstande,", "tokens": ["Ent\u00b7sprun\u00b7gen", "bin", "ich", "k\u00fchn", "dem", "meis\u00b7tern\u00b7den", "Ver\u00b7stan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und taumelnd von dem neugefundnen Gl\u00fcck! \u2013", "tokens": ["Und", "tau\u00b7melnd", "von", "dem", "neu\u00b7ge\u00b7fund\u00b7nen", "Gl\u00fcck", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Hinweg mit allen leeren Idealen,", "tokens": ["Hin\u00b7weg", "mit", "al\u00b7len", "lee\u00b7ren", "I\u00b7dea\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit Kunstgef\u00fchl und Sch\u00f6nheitssinn,", "tokens": ["Mit", "Kunst\u00b7ge\u00b7f\u00fchl", "und", "Sch\u00f6n\u00b7heits\u00b7sinn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die St\u00fcmper qu\u00e4len sich zu mahlen,", "tokens": ["Die", "St\u00fcm\u00b7per", "qu\u00e4\u00b7len", "sich", "zu", "mah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und nagen an den d\u00fcrren Schaalen", "tokens": ["Und", "na\u00b7gen", "an", "den", "d\u00fcr\u00b7ren", "Schaa\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und stolpern \u00fcber alle Freuden hin.", "tokens": ["Und", "stol\u00b7pern", "\u00fc\u00b7ber", "al\u00b7le", "Freu\u00b7den", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Hinweg mit Kunstgeschw\u00e4tz und allen Musen,", "tokens": ["Hin\u00b7weg", "mit", "Kunst\u00b7ge\u00b7schw\u00e4tz", "und", "al\u00b7len", "Mu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit Bilderwerk, leblosem Puppentand, \u2013", "tokens": ["Mit", "Bil\u00b7der\u00b7werk", ",", "leb\u00b7lo\u00b7sem", "Pup\u00b7pen\u00b7tand", ",", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "$,", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hinweg! ich greife nach der warmen Lebenshand,", "tokens": ["Hin\u00b7weg", "!", "ich", "grei\u00b7fe", "nach", "der", "war\u00b7men", "Le\u00b7bens\u00b7hand", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mich labt der sch\u00f6n geformt lebendge Busen.", "tokens": ["Mich", "labt", "der", "sch\u00f6n", "ge\u00b7formt", "le\u00b7bend\u00b7ge", "Bu\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "VVPP", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Ach, alles flieht wie tr\u00fcbe Nebelschatten", "tokens": ["Ach", ",", "al\u00b7les", "flieht", "wie", "tr\u00fc\u00b7be", "Ne\u00b7bel\u00b7schat\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PIS", "VVFIN", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was ihr mit kargem Sinne schenken wollt;", "tokens": ["Was", "ihr", "mit", "kar\u00b7gem", "Sin\u00b7ne", "schen\u00b7ken", "wollt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ADJA", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nur der besucht Elisiums sch\u00f6ne Matten,", "tokens": ["Nur", "der", "be\u00b7sucht", "E\u00b7lis\u00b7i\u00b7ums", "sch\u00f6\u00b7ne", "Mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "VVFIN", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Nur dem ist jede Gottheit hold,", "tokens": ["Nur", "dem", "ist", "je\u00b7de", "Got\u00b7theit", "hold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VAFIN", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der keinem Sinnentrug sein Leben zollt.", "tokens": ["Der", "kei\u00b7nem", "Sin\u00b7nen\u00b7trug", "sein", "Le\u00b7ben", "zollt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Der nicht in Luftgefilden schweist,", "tokens": ["Der", "nicht", "in", "Luft\u00b7ge\u00b7fil\u00b7den", "schweist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sich an Dunstphantomen weidet,", "tokens": ["Und", "sich", "an", "Dunst\u00b7phan\u00b7to\u00b7men", "wei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Durch kranke Wehmuth und Begeistrung streift, \u2013", "tokens": ["Durch", "kran\u00b7ke", "Weh\u00b7muth", "und", "Be\u00b7geis\u00b7trung", "streift", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nein, der die schlanke Nimphe rasch ergreift,", "tokens": ["Nein", ",", "der", "die", "schlan\u00b7ke", "Nim\u00b7phe", "rasch", "er\u00b7greift", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PRELS", "ART", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die sich zum k\u00fchlen Bad' entkleidet.", "tokens": ["Die", "sich", "zum", "k\u00fch\u00b7len", "Bad'", "ent\u00b7klei\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Ihm ist's verg\u00f6nnt zum Himmel sich zu schwingen.", "tokens": ["Ihm", "ist's", "ver\u00b7g\u00f6nnt", "zum", "Him\u00b7mel", "sich", "zu", "schwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPRART", "NN", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es sinkt auf ihn der G\u00f6tter Flammenschein,", "tokens": ["Es", "sinkt", "auf", "ihn", "der", "G\u00f6t\u00b7ter", "Flam\u00b7men\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er h\u00f6rt das Chor von tausend Sph\u00e4ren klingen,", "tokens": ["Er", "h\u00f6rt", "das", "Chor", "von", "tau\u00b7send", "Sph\u00e4\u00b7ren", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er wagt es zum Olymp hinauf zu dringen,", "tokens": ["Er", "wagt", "es", "zum", "O\u00b7lymp", "hin\u00b7auf", "zu", "drin\u00b7gen", ","], 