{"textgrid.poem.25107": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "31.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch sind die H\u00e4hne alle stumm,", "tokens": ["Noch", "sind", "die", "H\u00e4h\u00b7ne", "al\u00b7le", "stumm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PIAT", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und schwer liegt auf den Augenliden", "tokens": ["Und", "schwer", "liegt", "auf", "den", "Au\u00b7gen\u00b7li\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mir noch der Schlaf der Nacht; warum", "tokens": ["Mir", "noch", "der", "Schlaf", "der", "Nacht", ";", "wa\u00b7rum"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "ADV", "ART", "NN", "ART", "NN", "$.", "PWAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Weckt ihr so \u00fcberfr\u00fch den M\u00fcden?", "tokens": ["Weckt", "ihr", "so", "\u00fc\u00b7berf\u00b7r\u00fch", "den", "M\u00fc\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Kaum um den Himmelsrand spielt fern", "tokens": ["Kaum", "um", "den", "Him\u00b7mels\u00b7rand", "spielt", "fern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Schein, als ob die D\u00e4mmrung graute;", "tokens": ["Ein", "Schein", ",", "als", "ob", "die", "D\u00e4mm\u00b7rung", "grau\u00b7te", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOKOM", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Schlaftrunken gr\u00fc\u00dft den Morgenstern", "tokens": ["Schlaf\u00b7trun\u00b7ken", "gr\u00fc\u00dft", "den", "Mor\u00b7gens\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Die Lerche mit dem ersten Laute.", "tokens": ["Die", "Ler\u00b7che", "mit", "dem", "ers\u00b7ten", "Lau\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und matt im Osten hebt der Tag", "tokens": ["Und", "matt", "im", "Os\u00b7ten", "hebt", "der", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPRART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich halb empor vom Wolkensaume,", "tokens": ["Sich", "halb", "em\u00b7por", "vom", "Wol\u00b7ken\u00b7sau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "PTKVZ", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dann auf den Pf\u00fchl, auf dem es lag,", "tokens": ["Dann", "auf", "den", "Pf\u00fchl", ",", "auf", "dem", "es", "lag", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sinkt neu sein Haupt zur\u00fcck zum Traume.", "tokens": ["Sinkt", "neu", "sein", "Haupt", "zu\u00b7r\u00fcck", "zum", "Trau\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Dr\u00fcck mir die Augen wieder zu!", "tokens": ["Dr\u00fcck", "mir", "die", "Au\u00b7gen", "wie\u00b7der", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Fern von dem lauten Lebensschwarme,", "tokens": ["Fern", "von", "dem", "lau\u00b7ten", "Le\u00b7bens\u00b7schwar\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Allmutter Nacht, verg\u00f6nne du", "tokens": ["A\u00b7llmut\u00b7ter", "Nacht", ",", "ver\u00b7g\u00f6n\u00b7ne", "du"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mir lang noch Rast in deinem Arme!", "tokens": ["Mir", "lang", "noch", "Rast", "in", "dei\u00b7nem", "Ar\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}