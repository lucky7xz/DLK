{"textgrid.poem.53322": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was stehn vnd weinen wir zu hauff", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was stehn vnd weinen wir zu hauff", "tokens": ["Was", "stehn", "vnd", "wei\u00b7nen", "wir", "zu", "hauff"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVINF", "KON", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bey diesem todten Leichnam? auff!", "tokens": ["Bey", "die\u00b7sem", "tod\u00b7ten", "Leich\u00b7nam", "?", "auff", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$.", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gen Himmel schickt die Hertzen,", "tokens": ["Gen", "Him\u00b7mel", "schickt", "die", "Hert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der weise Raht", "tokens": ["Der", "wei\u00b7se", "Raht"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Des Herren hat", "tokens": ["Des", "Her\u00b7ren", "hat"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VAFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Selbs Theil an unsern Schmertzen.", "tokens": ["Selbs", "Theil", "an", "un\u00b7sern", "Schmert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Mensch, sein sch\u00f6nes Meisterrecht,", "tokens": ["Der", "Mensch", ",", "sein", "sch\u00f6\u00b7nes", "Meis\u00b7ter\u00b7recht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Wunsch, sein Nachbild, sein Geschlecht,", "tokens": ["Sein", "Wunsch", ",", "sein", "Nach\u00b7bild", ",", "sein", "Ge\u00b7schlecht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der nicht ohn Ihn kan werben", "tokens": ["Der", "nicht", "ohn", "Ihn", "kan", "wer\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PTKNEG", "APPR", "PPER", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dies Tageliecht,", "tokens": ["Dies", "Ta\u00b7ge\u00b7liecht", ","], "token_info": ["word", "word", "punct"], "pos": ["PDS", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Solt' er auch nicht", "tokens": ["Solt'", "er", "auch", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "PTKNEG"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Nach seinem Willen sterben?", "tokens": ["Nach", "sei\u00b7nem", "Wil\u00b7len", "ster\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wir sind ia V\u00f6geln vorzuziehn,", "tokens": ["Wir", "sind", "i\u00b7a", "V\u00f6\u00b7geln", "vor\u00b7zu\u00b7ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Nun f\u00e4llt kein Sperrlingk hin ohn Ihn,", "tokens": ["Nun", "f\u00e4llt", "kein", "Sperr\u00b7lingk", "hin", "ohn", "Ihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wofern die Schrifft nicht fehlet;", "tokens": ["Wo\u00b7fern", "die", "Schrifft", "nicht", "feh\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nach der Er gar", "tokens": ["Nach", "der", "Er", "gar"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "ADV"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Auch selbs das Haar", "tokens": ["Auch", "selbs", "das", "Haar"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Auff unsern H\u00e4uptern zehlet.", "tokens": ["Auff", "un\u00b7sern", "H\u00e4up\u00b7tern", "zeh\u00b7let", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sol uns so lieb was seyn allhie,", "tokens": ["Sol", "uns", "so", "lieb", "was", "seyn", "all\u00b7hie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "PWS", "PPOSAT", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df Er ohn Wiederred' und M\u00fch", "tokens": ["Da\u00df", "Er", "ohn", "Wie\u00b7der\u00b7red'", "und", "M\u00fch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht solte von uns heben,", "tokens": ["Nicht", "sol\u00b7te", "von", "uns", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VMFIN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der seinen Sohn", "tokens": ["Der", "sei\u00b7nen", "Sohn"], "token_info": ["word", "word", "word"], "pos": ["ART", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Zum Gnaden-Thron", "tokens": ["Zum", "Gna\u00b7den\u00b7Thron"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Vns S\u00fcndern hat gegeben?", "tokens": ["Vns", "S\u00fcn\u00b7dern", "hat", "ge\u00b7ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "So ist auch sein die gantze Welt,", "tokens": ["So", "ist", "auch", "sein", "die", "gant\u00b7ze", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPOSAT", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr uns ist eignes nichts bestellt,", "tokens": ["F\u00fcr", "uns", "ist", "eig\u00b7nes", "nichts", "be\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "ADV", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir selbs sind blohs des Herren,", "tokens": ["Wir", "selbs", "sind", "blohs", "des", "Her\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Greifft Er uns ein,", "tokens": ["Greifft", "Er", "uns", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Er nimt was sein,", "tokens": ["Er", "nimt", "was", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "VAINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Was sollen wir uns sperren?", "tokens": ["Was", "sol\u00b7len", "wir", "uns", "sper\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Vnd ist uns wol dabey zu Muth,", "tokens": ["Vnd", "ist", "uns", "wol", "da\u00b7bey", "zu", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PAV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn Er uns so viel guttes thut,", "tokens": ["Wenn", "Er", "uns", "so", "viel", "gut\u00b7tes", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "PIAT", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sind wir so zahrt zu leiden?", "tokens": ["Sind", "wir", "so", "zahrt", "zu", "lei\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie k\u00f6nnen wir", "tokens": ["Wie", "k\u00f6n\u00b7nen", "wir"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Das B\u00f6s' auch hier,", "tokens": ["Das", "B\u00f6s'", "auch", "hier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "So Er uns zuschickt, meiden?", "tokens": ["So", "Er", "uns", "zu\u00b7schickt", ",", "mei\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PPER", "PPER", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Gib gern hin, was Gott haben will,", "tokens": ["Gib", "gern", "hin", ",", "was", "Gott", "ha\u00b7ben", "will", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PTKVZ", "$,", "PRELS", "NN", "VAINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Halt seinem weisen Raht-Schlu\u00df still,", "tokens": ["Halt", "sei\u00b7nem", "wei\u00b7sen", "Raht\u00b7Schlu\u00df", "still", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihm' haben stets gefallen", "tokens": ["Ihm'", "ha\u00b7ben", "stets", "ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Vnschuld ziert,", "tokens": ["Die", "Vn\u00b7schuld", "ziert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Vnd diese f\u00fchrt", "tokens": ["Vnd", "die\u00b7se", "f\u00fchrt"], "token_info": ["word", "word", "word"], "pos": ["KON", "PDS", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Er auch bald heim f\u00fcr allen.", "tokens": ["Er", "auch", "bald", "heim", "f\u00fcr", "al\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "PTKVZ", "APPR", "PIAT", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wer wei\u00df es wol, vor welcher Noht", "tokens": ["Wer", "wei\u00df", "es", "wol", ",", "vor", "wel\u00b7cher", "Noht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$,", "APPR", "PWAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er sie zur Rhue bringt durch den Todt,", "tokens": ["Er", "sie", "zur", "Rhue", "bringt", "durch", "den", "Todt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "APPRART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir f\u00fcrchten manchen Jammer,", "tokens": ["Wir", "f\u00fcrch\u00b7ten", "man\u00b7chen", "Jam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie wol ist der,", "tokens": ["Wie", "wol", "ist", "der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "ART", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Den kein Beschwehr", "tokens": ["Den", "kein", "Be\u00b7schwehr"], "token_info": ["word", "word", "word"], "pos": ["ART", "PIAT", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Mehr schreckt in seiner Kammer!", "tokens": ["Mehr", "schreckt", "in", "sei\u00b7ner", "Kam\u00b7mer", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "GoTT thut wie ein getrewer Hirt,", "tokens": ["GoTT", "thut", "wie", "ein", "ge\u00b7tre\u00b7wer", "Hirt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der eines Wetters innen wird,", "tokens": ["Der", "ei\u00b7nes", "Wet\u00b7ters", "in\u00b7nen", "wird", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vnd treibt sein Vieh zusammen", "tokens": ["Vnd", "treibt", "sein", "Vieh", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den St\u00e4llen zu", "tokens": ["Den", "St\u00e4l\u00b7len", "zu"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "PTKZU"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "In sichre Rhue,", "tokens": ["In", "sich\u00b7re", "Rhue", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "NE", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "F\u00fcr Hagel, Sturm vnd Flammen.", "tokens": ["F\u00fcr", "Ha\u00b7gel", ",", "Sturm", "vnd", "Flam\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Er wird auch uns zu seiner Zeit", "tokens": ["Er", "wird", "auch", "uns", "zu", "sei\u00b7ner", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Heimholen aus der Sterblicheit", "tokens": ["Heim\u00b7ho\u00b7len", "aus", "der", "Sterb\u00b7li\u00b7cheit"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu seinen HimmelsSchaaren,", "tokens": ["Zu", "sei\u00b7nen", "Him\u00b7mels", "Schaa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er la\u00df uns nur", "tokens": ["Er", "la\u00df", "uns", "nur"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Die Glaubens-Spur", "tokens": ["Die", "Glau\u00b7bens\u00b7Spur"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "In ienes Leben fahren.", "tokens": ["In", "ie\u00b7nes", "Le\u00b7ben", "fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}