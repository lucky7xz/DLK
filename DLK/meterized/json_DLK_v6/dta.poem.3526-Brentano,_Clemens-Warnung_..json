{"dta.poem.3526": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Warnung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Trutschel und die Frau Nachtigall,", "tokens": ["Die", "Trut\u00b7schel", "und", "die", "Frau", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die sa\u00dfen auf einer Linden;", "tokens": ["Die", "sa\u00b7\u00dfen", "auf", "ei\u00b7ner", "Lin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NE", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "\u201each du mein Herzallerliebster Schatz,", "tokens": ["\u201e", "ach", "du", "mein", "Her\u00b7zal\u00b7ler\u00b7liebs\u00b7ter", "Schatz", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "\u201ewo werd ich dich Abends finden?\u201c", "tokens": ["\u201e", "wo", "werd", "ich", "dich", "A\u00b7bends", "fin\u00b7den", "?", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "VAFIN", "PPER", "PRF", "ADV", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wo du mich Abends finden wirst,", "tokens": ["Wo", "du", "mich", "A\u00b7bends", "fin\u00b7den", "wirst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADV", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Morgens wirds dich reuen;", "tokens": ["Des", "Mor\u00b7gens", "wirds", "dich", "reu\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u201each du mein Herzallerliebster Schatz,", "tokens": ["\u201e", "ach", "du", "mein", "Her\u00b7zal\u00b7ler\u00b7liebs\u00b7ter", "Schatz", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "\u201ewas brichst du mir die Treue.", "tokens": ["\u201e", "was", "brichst", "du", "mir", "die", "Treu\u00b7e", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und all dein Treu die mag ich nicht,", "tokens": ["Und", "all", "dein", "Treu", "die", "mag", "ich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PPOSAT", "NN", "ART", "VMFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Will doch viel lieber sterben;", "tokens": ["Will", "doch", "viel", "lie\u00b7ber", "ster\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was soll ich dann mein jung frisch Blut", "tokens": ["Was", "soll", "ich", "dann", "mein", "jung", "frisch", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "PPOSAT", "ADJD", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An einem Knaben verderben.", "tokens": ["An", "ei\u00b7nem", "Kna\u00b7ben", "ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Ach M\u00e4dchen behalt deine Ehre fest,", "tokens": ["Ach", "M\u00e4d\u00b7chen", "be\u00b7halt", "dei\u00b7ne", "Eh\u00b7re", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+--++-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und la\u00df dich nicht betriegen;", "tokens": ["Und", "la\u00df", "dich", "nicht", "be\u00b7trie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Denn Geld und Gut ist bald verzehrt,", "tokens": ["Denn", "Geld", "und", "Gut", "ist", "bald", "ver\u00b7zehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "ADJD", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Deine Ehr ist nimmer zu kriegen.", "tokens": ["Dei\u00b7ne", "Ehr", "ist", "nim\u00b7mer", "zu", "krie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Ach M\u00e4dchen behalt deine Ehre fest,", "tokens": ["Ach", "M\u00e4d\u00b7chen", "be\u00b7halt", "dei\u00b7ne", "Eh\u00b7re", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+--++-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Als wie der Baum sein Aeste;", "tokens": ["Als", "wie", "der", "Baum", "sein", "A\u00b7es\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wenn das Laub herunter f\u00e4llt,", "tokens": ["Und", "wenn", "das", "Laub", "her\u00b7un\u00b7ter", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APZR", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So trauren alle Aestger.", "tokens": ["So", "trau\u00b7ren", "al\u00b7le", "A\u00b7est\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.6": {"line.1": {"text": "Wenn einer dich betrogen hat,", "tokens": ["Wenn", "ei\u00b7ner", "dich", "be\u00b7tro\u00b7gen", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So zieh er aus dem Lande,", "tokens": ["So", "zieh", "er", "aus", "dem", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er steckt die Feder auf sein Hut,", "tokens": ["Er", "steckt", "die", "Fe\u00b7der", "auf", "sein", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "L\u00e4\u00dfts M\u00e4dchen brav in Schande.", "tokens": ["L\u00e4\u00dfts", "M\u00e4d\u00b7chen", "brav", "in", "Schan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}