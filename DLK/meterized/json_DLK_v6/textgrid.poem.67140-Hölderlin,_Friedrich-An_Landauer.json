{"textgrid.poem.67140": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An Landauer", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sei froh! Du hast das gute Los erkoren,", "tokens": ["Sei", "froh", "!", "Du", "hast", "das", "gu\u00b7te", "Los", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$.", "PPER", "VAFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn tief und treu ward eine Seele dir;", "tokens": ["Denn", "tief", "und", "treu", "ward", "ei\u00b7ne", "See\u00b7le", "dir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "VAFIN", "ART", "NN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Freunde Freund zu sein, bist du geboren,", "tokens": ["Der", "Freun\u00b7de", "Freund", "zu", "sein", ",", "bist", "du", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKZU", "VAINF", "$,", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dies zeugen dir am Feste wir.", "tokens": ["Dies", "zeu\u00b7gen", "dir", "am", "Fes\u00b7te", "wir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPRART", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und selig, wer im eignen Hause Frieden,", "tokens": ["Und", "se\u00b7lig", ",", "wer", "im", "eig\u00b7nen", "Hau\u00b7se", "Frie\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWS", "APPRART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie du, und Lieb und F\u00fclle sieht und Ruh;", "tokens": ["Wie", "du", ",", "und", "Lieb", "und", "F\u00fcl\u00b7le", "sieht", "und", "Ruh", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "KON", "NN", "KON", "NN", "VVFIN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Manch Leben ist, wie Licht und Nacht, verschieden,", "tokens": ["Manch", "Le\u00b7ben", "ist", ",", "wie", "Licht", "und", "Nacht", ",", "ver\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "$,", "PWAV", "NN", "KON", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In goldner Mitte wohnest du.", "tokens": ["In", "gold\u00b7ner", "Mit\u00b7te", "woh\u00b7nest", "du", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dir gl\u00e4nzt die Sonn in wohlgebauter Halle,", "tokens": ["Dir", "gl\u00e4nzt", "die", "Sonn", "in", "wohl\u00b7ge\u00b7bau\u00b7ter", "Hal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Am Berge reift die Sonne dir den Wein,", "tokens": ["Am", "Ber\u00b7ge", "reift", "die", "Son\u00b7ne", "dir", "den", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und immer gl\u00fccklich f\u00fchrt die G\u00fcter alle", "tokens": ["Und", "im\u00b7mer", "gl\u00fcck\u00b7lich", "f\u00fchrt", "die", "G\u00fc\u00b7ter", "al\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "ART", "NN", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der kluge Gott dir aus und ein.", "tokens": ["Der", "klu\u00b7ge", "Gott", "dir", "aus", "und", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und Kind gedeiht, und Mutter um den Gatten,", "tokens": ["Und", "Kind", "ge\u00b7deiht", ",", "und", "Mut\u00b7ter", "um", "den", "Gat\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "$,", "KON", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wie den Wald die goldne Wolke kr\u00f6nt,", "tokens": ["Und", "wie", "den", "Wald", "die", "gold\u00b7ne", "Wol\u00b7ke", "kr\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So seid auch ihr um ihn, geliebte Schatten!", "tokens": ["So", "seid", "auch", "ihr", "um", "ihn", ",", "ge\u00b7lieb\u00b7te", "Schat\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "APPR", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihr Seligen, an ihn gew\u00f6hnt!", "tokens": ["Ihr", "Se\u00b7li\u00b7gen", ",", "an", "ihn", "ge\u00b7w\u00f6hnt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "PPER", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "O seid mit ihm! denn Wolk und Winde ziehen", "tokens": ["O", "seid", "mit", "ihm", "!", "denn", "Wolk", "und", "Win\u00b7de", "zie\u00b7hen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "APPR", "PPER", "$.", "KON", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Unruhig \u00f6fters \u00fcber Land und Haus,", "tokens": ["Un\u00b7ru\u00b7hig", "\u00f6f\u00b7ters", "\u00fc\u00b7ber", "Land", "und", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Doch ruht das Herz bei allen Lebensm\u00fchen", "tokens": ["Doch", "ruht", "das", "Herz", "bei", "al\u00b7len", "Le\u00b7bens\u00b7m\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im heilgen Angedenken aus.", "tokens": ["Im", "heil\u00b7gen", "An\u00b7ge\u00b7den\u00b7ken", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und sieh! aus Freude sagen wir von Sorgen;", "tokens": ["Und", "sieh", "!", "aus", "Freu\u00b7de", "sa\u00b7gen", "wir", "von", "Sor\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$.", "APPR", "NN", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie dunkler Wein, erfreut auch ernster Sang;", "tokens": ["Wie", "dunk\u00b7ler", "Wein", ",", "er\u00b7freut", "auch", "erns\u00b7ter", "Sang", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das Fest verhallt, und jedes gehet morgen", "tokens": ["Das", "Fest", "ver\u00b7hallt", ",", "und", "je\u00b7des", "ge\u00b7het", "mor\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "KON", "PIAT", "VVFIN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf schmaler Erde seinen Gang.", "tokens": ["Auf", "schma\u00b7ler", "Er\u00b7de", "sei\u00b7nen", "Gang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}