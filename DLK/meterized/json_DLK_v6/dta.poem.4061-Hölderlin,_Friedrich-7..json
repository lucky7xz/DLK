{"dta.poem.4061": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Aber o Du, die schon am Scheidewege mir damals,", "tokens": ["A\u00b7ber", "o", "Du", ",", "die", "schon", "am", "Schei\u00b7de\u00b7we\u00b7ge", "mir", "da\u00b7mals", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "FM", "PPER", "$,", "PRELS", "ADV", "APPRART", "NN", "PPER", "ADV", "$,"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Da ich versank vor Dir, tr\u00f6stend ein Sch\u00f6neres", "tokens": ["Da", "ich", "ver\u00b7sank", "vor", "Dir", ",", "tr\u00f6s\u00b7tend", "ein", "Sch\u00f6\u00b7ne\u00b7res"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "PPER", "$,", "ADJD", "ART", "NN"], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "wies,", "tokens": ["wies", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Du, die, Gro\u00dfes zu sehn und froher die G\u00f6tter", "tokens": ["Du", ",", "die", ",", "Gro\u00b7\u00dfes", "zu", "sehn", "und", "fro\u00b7her", "die", "G\u00f6t\u00b7ter"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "$,", "NN", "PTKZU", "VVINF", "KON", "ADJD", "ART", "NN"], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "zu singen,", "tokens": ["zu", "sin\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Schweigend, wie sie, mich einst stille begeisternd,", "tokens": ["Schwei\u00b7gend", ",", "wie", "sie", ",", "mich", "einst", "stil\u00b7le", "be\u00b7geis\u00b7ternd", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "PPER", "$,", "PPER", "ADV", "VVFIN", "VVPP", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "gelehrt,", "tokens": ["ge\u00b7lehrt", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "G\u00f6tterkind! erscheinest Du mir, und gr\u00fc\u00dfest, wie", "tokens": ["G\u00f6t\u00b7ter\u00b7kind", "!", "er\u00b7schei\u00b7nest", "Du", "mir", ",", "und", "gr\u00fc\u00b7\u00dfest", ",", "wie"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "$.", "VVFIN", "PPER", "PPER", "$,", "KON", "VVFIN", "$,", "PWAV"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "einst, mich,", "tokens": ["einst", ",", "mich", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADV", "$,", "PPER", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.10": {"text": "Redest wieder, wie einst, h\u00f6here Dinge mir zu?", "tokens": ["Re\u00b7dest", "wie\u00b7der", ",", "wie", "einst", ",", "h\u00f6\u00b7he\u00b7re", "Din\u00b7ge", "mir", "zu", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PWAV", "ADV", "$,", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Siehe! weinen vor Dir und klagen mu\u00df ich, wenn", "tokens": ["Sie\u00b7he", "!", "wei\u00b7nen", "vor", "Dir", "und", "kla\u00b7gen", "mu\u00df", "ich", ",", "wenn"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVIMP", "$.", "VVINF", "APPR", "PPER", "KON", "VVINF", "VMFIN", "PPER", "$,", "KOUS"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.12": {"text": "schon noch", "tokens": ["schon", "noch"], "token_info": ["word", "word"], "pos": ["ADV", "ADV"], "meter": "-+", "measure": "iambic.single"}, "line.13": {"text": "Denkend edlerer Zeit, dessen die Sele sich sch\u00e4mt.", "tokens": ["Den\u00b7kend", "ed\u00b7le\u00b7rer", "Zeit", ",", "des\u00b7sen", "die", "Se\u00b7le", "sich", "sch\u00e4mt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "PRELAT", "ART", "NN", "PRF", "ADJD", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.14": {"text": "Denn so lange, so lang' auf matten Pfaden der", "tokens": ["Denn", "so", "lan\u00b7ge", ",", "so", "lang'", "auf", "mat\u00b7ten", "Pfa\u00b7den", "der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "$,", "ADV", "ADV", "APPR", "ADJA", "NN", "ART"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.15": {"text": "Erde", "tokens": ["Er\u00b7de"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.16": {"text": "Hab' ich, Deiner gewohnt, Dich in der Irre", "tokens": ["Hab'", "ich", ",", "Dei\u00b7ner", "ge\u00b7wohnt", ",", "Dich", "in", "der", "Ir\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "PPOSAT", "VVPP", "$,", "PRF", "APPR", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.17": {"text": "gesucht,", "tokens": ["ge\u00b7sucht", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.18": {"text": "Freudiger Schutzgeist! aber umsonst, und Jahre", "tokens": ["Freu\u00b7di\u00b7ger", "Schutz\u00b7geist", "!", "a\u00b7ber", "um\u00b7sonst", ",", "und", "Jah\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$.", "ADV", "ADV", "$,", "KON", "NN"], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.19": {"text": "zerrannen,", "tokens": ["zer\u00b7ran\u00b7nen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.20": {"text": "Seit wir ahnend um uns gl\u00e4nzen die Abende", "tokens": ["Seit", "wir", "ah\u00b7nend", "um", "uns", "gl\u00e4n\u00b7zen", "die", "A\u00b7ben\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.21": {"text": "sahn.", "tokens": ["sahn", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}}}}}