{"dta.poem.1694": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "F\u00fcnff Frauenzimmer/ f\u00fcnff Sinnen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcnff Sinnen geben sich uns itzund anzuschauen: ", "tokens": ["F\u00fcnff", "Sin\u00b7nen", "ge\u00b7ben", "sich", "uns", "it\u00b7zund", "an\u00b7zu\u00b7schau\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PRF", "PPER", "ADV", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie aber sollen wir dieselben theilen ein?", "tokens": ["Wie", "a\u00b7ber", "sol\u00b7len", "wir", "die\u00b7sel\u00b7ben", "thei\u00b7len", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "PDS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df wir nicht stossen an bey Fr\u00e4ulein oder Frauen/", "tokens": ["Da\u00df", "wir", "nicht", "stos\u00b7sen", "an", "bey", "Fr\u00e4u\u00b7lein", "o\u00b7der", "Frau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "APPR", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So wird des Wirthes Rath darzu vonn\u00f6then seyn.", "tokens": ["So", "wird", "des", "Wirt\u00b7hes", "Rath", "dar\u00b7zu", "von\u00b7n\u00f6\u00b7then", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "PAV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Zwar zeigt sich das Gesicht in hold-beflammten Blicken/", "tokens": ["Zwar", "zeigt", "sich", "das", "Ge\u00b7sicht", "in", "hold\u00b7be\u00b7flamm\u00b7ten", "Bli\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und frisches Feuer ist der Fr\u00e4ulein Eigenthum;", "tokens": ["Und", "fri\u00b7sches", "Feu\u00b7er", "ist", "der", "Fr\u00e4u\u00b7lein", "Ei\u00b7gen\u00b7thum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch will sich noch ein Sinn zu ihrer Anmutt schicken.", "tokens": ["Doch", "will", "sich", "noch", "ein", "Sinn", "zu", "ih\u00b7rer", "An\u00b7mutt", "schi\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wo sch\u00f6ne Blumen bl\u00fchn/ hat der Geruch den Ruhm.", "tokens": ["Wo", "sch\u00f6\u00b7ne", "Blu\u00b7men", "bl\u00fchn", "/", "hat", "der", "Ge\u00b7ruch", "den", "Ruhm", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVINF", "$(", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der Wirthin H\u00f6fligkeit will man sich selbst entdecken/", "tokens": ["Der", "Wirt\u00b7hin", "H\u00f6f\u00b7lig\u00b7keit", "will", "man", "sich", "selbst", "ent\u00b7de\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VMFIN", "PIS", "PRF", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob ihr von jedem Sinn ein sonders Lob geb\u00fchrt/", "tokens": ["Ob", "ihr", "von", "je\u00b7dem", "Sinn", "ein", "son\u00b7ders", "Lob", "ge\u00b7b\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PIAT", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df sie es diesen Tag am meisten sucht vom Schmecken/", "tokens": ["Da\u00df", "sie", "es", "die\u00b7sen", "Tag", "am", "meis\u00b7ten", "sucht", "vom", "Schme\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PDAT", "NN", "APPRART", "PIS", "VVFIN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Indem sie uns zum Gla\u00df und gutter Speise f\u00fchrt.", "tokens": ["In\u00b7dem", "sie", "uns", "zum", "Gla\u00df", "und", "gut\u00b7ter", "Spei\u00b7se", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPRART", "NN", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wo hoher Tugend Prei\u00df mit vollem Glantze spielet/", "tokens": ["Wo", "ho\u00b7her", "Tu\u00b7gend", "Prei\u00df", "mit", "vol\u00b7lem", "Glant\u00b7ze", "spie\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erschallet auch der Klang darvon mit hellem Thon.", "tokens": ["Er\u00b7schal\u00b7let", "auch", "der", "Klang", "dar\u00b7von", "mit", "hel\u00b7lem", "Thon", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PAV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn Auge/ Ruch/ Geschmack und Ohr Vergn\u00fcgun", "tokens": ["Wenn", "Au\u00b7ge", "/", "Ruch", "/", "Ge\u00b7schmack", "und", "Ohr", "Ver\u00b7gn\u00fc\u00b7gun"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "$(", "NN", "$(", "NN", "KON", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So bleibt noch \u00fcber di\u00df der f\u00fcnffte Sinn mein Lohn.", "tokens": ["So", "bleibt", "noch", "\u00fc\u00b7ber", "di\u00df", "der", "f\u00fcnff\u00b7te", "Sinn", "mein", "Lohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "PDS", "ART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}