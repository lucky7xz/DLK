{"textgrid.poem.40567": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie er im raschen Flug", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie er im raschen Flug", "tokens": ["Wie", "er", "im", "ra\u00b7schen", "Flug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Hin durch die Wolken schiffte,", "tokens": ["Hin", "durch", "die", "Wol\u00b7ken", "schiff\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Stumm durch den zwitschernden Zug,", "tokens": ["Stumm", "durch", "den", "zwit\u00b7schern\u00b7den", "Zug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Der Ahasver der L\u00fcfte.", "tokens": ["Der", "A\u00b7has\u00b7ver", "der", "L\u00fcf\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Stumm wie ein irrer Komet", "tokens": ["Stumm", "wie", "ein", "ir\u00b7rer", "Ko\u00b7met"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit gl\u00e4nzendem Leibeskerne,", "tokens": ["Mit", "gl\u00e4n\u00b7zen\u00b7dem", "Lei\u00b7bes\u00b7ker\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die spr\u00fchende Schleppe weht", "tokens": ["Die", "spr\u00fc\u00b7hen\u00b7de", "Schlep\u00b7pe", "weht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ihm nach weithin in die Ferne.", "tokens": ["Ihm", "nach", "weit\u00b7hin", "in", "die", "Fer\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Tod ihn nimmer ruft,", "tokens": ["Der", "Tod", "ihn", "nim\u00b7mer", "ruft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Noch sah kein Aug' ihn modern;", "tokens": ["Noch", "sah", "kein", "Aug'", "ihn", "mo\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vielleicht da\u00df er mag in Duft,", "tokens": ["Viel\u00b7leicht", "da\u00df", "er", "mag", "in", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "VMFIN", "APPR", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wie sterbende Sterne verlodern?", "tokens": ["Wie", "ster\u00b7ben\u00b7de", "Ster\u00b7ne", "ver\u00b7lo\u00b7dern", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Ihn lockt nicht die bl\u00fchende Au,", "tokens": ["Ihn", "lockt", "nicht", "die", "bl\u00fc\u00b7hen\u00b7de", "Au", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Um Nahrung herabzuwallen,", "tokens": ["Um", "Nah\u00b7rung", "her\u00b7ab\u00b7zu\u00b7wal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Aus Wolken pfl\u00fcckt er den Thau", "tokens": ["Aus", "Wol\u00b7ken", "pfl\u00fcckt", "er", "den", "Thau"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Im Flug, wie Blumen im Fallen.", "tokens": ["Im", "Flug", ",", "wie", "Blu\u00b7men", "im", "Fal\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Und weil sie sein Nest im Wald,", "tokens": ["Und", "weil", "sie", "sein", "Nest", "im", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sein Grab nicht sahn auf der Wiese,", "tokens": ["Sein", "Grab", "nicht", "sahn", "auf", "der", "Wie\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Drum hie\u00df er dem Volk alsbald", "tokens": ["Drum", "hie\u00df", "er", "dem", "Volk", "als\u00b7bald"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN", "ADV"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der Vogel vom Paradiese.", "tokens": ["Der", "Vo\u00b7gel", "vom", "Pa\u00b7ra\u00b7die\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "APPRART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Die Sage aber erz\u00e4hlt:", "tokens": ["Die", "Sa\u00b7ge", "a\u00b7ber", "er\u00b7z\u00e4hlt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Als Nachtigall einst geboren,", "tokens": ["Als", "Nach\u00b7ti\u00b7gall", "einst", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Von Rosenliebe beseelt,", "tokens": ["Von", "Ro\u00b7sen\u00b7lie\u00b7be", "be\u00b7seelt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "War er zum Gesang erkoren.", "tokens": ["War", "er", "zum", "Ge\u00b7sang", "er\u00b7ko\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Er sang, da\u00df starres Erz", "tokens": ["Er", "sang", ",", "da\u00df", "star\u00b7res", "Erz"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Selbst Bl\u00fcthentrieb versp\u00fcrte;", "tokens": ["Selbst", "Bl\u00fc\u00b7then\u00b7trieb", "ver\u00b7sp\u00fcr\u00b7te", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O da\u00df er des Lenzes Herz,", "tokens": ["O", "da\u00df", "er", "des", "Len\u00b7zes", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Des fl\u00fccht'gen, zum Bleiben r\u00fchrte!", "tokens": ["Des", "fl\u00fccht'\u00b7gen", ",", "zum", "Blei\u00b7ben", "r\u00fchr\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "APPRART", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Fortzog der Lenz durch das All'", "tokens": ["Fort\u00b7zog", "der", "Lenz", "durch", "das", "All'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Mit Rosen, Liedern und Scherzen,", "tokens": ["Mit", "Ro\u00b7sen", ",", "Lie\u00b7dern", "und", "Scher\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da ahnte die Nachtigall", "tokens": ["Da", "ahn\u00b7te", "die", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Den Tod vom gebrochenen Herzen.", "tokens": ["Den", "Tod", "vom", "ge\u00b7bro\u00b7che\u00b7nen", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.9": {"line.1": {"text": "Sie fleht in der Seele Pein:", "tokens": ["Sie", "fleht", "in", "der", "See\u00b7le", "Pein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "\u00bbherr, heb' empor mich von hinnen!", "tokens": ["\u00bb", "herr", ",", "heb'", "em\u00b7por", "mich", "von", "hin\u00b7nen", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "VVFIN", "PTKVZ", "PRF", "APPR", "ADV", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "La\u00df mich bei dir allein,", "tokens": ["La\u00df", "mich", "bei", "dir", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Dem Unverg\u00e4nglichen, minnen!\u00ab", "tokens": ["Dem", "Un\u00b7ver\u00b7g\u00e4ng\u00b7li\u00b7chen", ",", "min\u00b7nen", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "ADJA", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "Da ging aus des Herren Hand", "tokens": ["Da", "ging", "aus", "des", "Her\u00b7ren", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Als Adler sie neugeboren,", "tokens": ["Als", "Ad\u00b7ler", "sie", "neu\u00b7ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Von Sonnenlieb' entbrannt,", "tokens": ["Von", "Son\u00b7nen\u00b7lieb'", "ent\u00b7brannt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Zum Himmelsflug erkoren.", "tokens": ["Zum", "Him\u00b7mels\u00b7flug", "er\u00b7ko\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Da flog zum Quell des Lichts", "tokens": ["Da", "flog", "zum", "Quell", "des", "Lichts"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Fort, fort durch Wolken und Sterne,", "tokens": ["Fort", ",", "fort", "durch", "Wol\u00b7ken", "und", "Ster\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PTKVZ", "APPR", "NN", "KON", "NN", "$,"], "meter": "++-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Schon schwand ihm die Erd' in Nichts,", "tokens": ["Schon", "schwand", "ihm", "die", "Erd'", "in", "Nichts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "PIS", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die Sonne doch blieb gleich ferne!", "tokens": ["Die", "Son\u00b7ne", "doch", "blieb", "gleich", "fer\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ADV", "ADV", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.12": {"line.1": {"text": "Sein Aug' von Kristall schon brach,", "tokens": ["Sein", "Aug'", "von", "Kris\u00b7tall", "schon", "brach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Schon schmolz ihm die eherne Schwinge;", "tokens": ["Schon", "schmolz", "ihm", "die", "e\u00b7her\u00b7ne", "Schwin\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Im Niedersinken doch sprach", "tokens": ["Im", "Nie\u00b7der\u00b7sin\u00b7ken", "doch", "sprach"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "VVFIN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Er so zum Herrn der Dinge:", "tokens": ["Er", "so", "zum", "Herrn", "der", "Din\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "\u00bbdarf nicht bei dir ich im Licht,", "tokens": ["\u00bb", "darf", "nicht", "bei", "dir", "ich", "im", "Licht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "PTKNEG", "APPR", "PPER", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dem Unverg\u00e4nglichen, wohnen,", "tokens": ["Dem", "Un\u00b7ver\u00b7g\u00e4ng\u00b7li\u00b7chen", ",", "woh\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "O schleudre zur\u00fcck mich nicht", "tokens": ["O", "schleud\u00b7re", "zu\u00b7r\u00fcck", "mich", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PTKVZ", "PPER", "PTKNEG"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Zu niedern Erdenzonen!\u00ab", "tokens": ["Zu", "nie\u00b7dern", "Er\u00b7den\u00b7zo\u00b7nen", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Da bannt' ihn der Herr im Flug", "tokens": ["Da", "bannt'", "ihn", "der", "Herr", "im", "Flug"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und schuf ihn, wie dort er schiffte", "tokens": ["Und", "schuf", "ihn", ",", "wie", "dort", "er", "schiff\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "PWAV", "ADV", "PPER", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Stumm durch den zwitschernden Zug,", "tokens": ["Stumm", "durch", "den", "zwit\u00b7schern\u00b7den", "Zug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Der Ahasver der L\u00fcfte.", "tokens": ["Der", "A\u00b7has\u00b7ver", "der", "L\u00fcf\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Nicht erdw\u00e4rts schwebt er, da\u00df nicht", "tokens": ["Nicht", "erd\u00b7w\u00e4rts", "schwebt", "er", ",", "da\u00df", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "ADV", "VVFIN", "PPER", "$,", "KOUS", "PTKNEG"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Befleckt sein rein Gefieder,", "tokens": ["Be\u00b7fleckt", "sein", "rein", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAINF", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nicht sonnenw\u00e4rts zum Licht,", "tokens": ["Nicht", "son\u00b7nen\u00b7w\u00e4rts", "zum", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Vorm Ziele s\u00e4nk er ja wieder.", "tokens": ["Vorm", "Zie\u00b7le", "s\u00e4nk", "er", "ja", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.16": {"line.1": {"text": "Sein Herz nicht \u00fcberflie\u00dft's", "tokens": ["Sein", "Herz", "nicht", "\u00fc\u00b7ber\u00b7flie\u00dft's"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PTKNEG", "APPRART"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Von Flammen des Liederdranges;", "tokens": ["Von", "Flam\u00b7men", "des", "Lie\u00b7der\u00b7dran\u00b7ges", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Was oben, unsingbar ist's,", "tokens": ["Was", "o\u00b7ben", ",", "un\u00b7sing\u00b7bar", "ist's", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADV", "$,", "ADJD", "VAFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Was unten, nicht werth des Gesanges!", "tokens": ["Was", "un\u00b7ten", ",", "nicht", "werth", "des", "Ge\u00b7san\u00b7ges", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "$,", "PTKNEG", "ADJD", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.17": {"line.1": {"text": "Ein Stern des Himmels, ergl\u00fcht", "tokens": ["Ein", "Stern", "des", "Him\u00b7mels", ",", "er\u00b7gl\u00fcht"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "VVFIN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Er hell den Irdischen h\u00fcben;", "tokens": ["Er", "hell", "den", "Ir\u00b7di\u00b7schen", "h\u00fc\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Eine Blume der Erde, bl\u00fcht", "tokens": ["Ei\u00b7ne", "Blu\u00b7me", "der", "Er\u00b7de", ",", "bl\u00fcht"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "VVFIN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Er bunt den Geistern dr\u00fcben.", "tokens": ["Er", "bunt", "den", "Geis\u00b7tern", "dr\u00fc\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Und wenn er vorbei euch zieht,", "tokens": ["Und", "wenn", "er", "vor\u00b7bei", "euch", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Stumm durch den singenden Reigen,", "tokens": ["Stumm", "durch", "den", "sin\u00b7gen\u00b7den", "Rei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Verstandet ihr einst nicht sein Lied,", "tokens": ["Ver\u00b7stan\u00b7det", "ihr", "einst", "nicht", "sein", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Lernt jetzt verstehn sein Schweigen.", "tokens": ["Lernt", "jetzt", "ver\u00b7stehn", "sein", "Schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}