{"textgrid.poem.58500": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "538. Es soll bleiben beim Verschreiben!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man kann sich nicht zu fest verschreiben", "tokens": ["Man", "kann", "sich", "nicht", "zu", "fest", "ver\u00b7schrei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "PRF", "PTKNEG", "PTKA", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem, des man ewig w\u00fcnscht zu bleiben,", "tokens": ["Dem", ",", "des", "man", "e\u00b7wig", "w\u00fcnscht", "zu", "blei\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "ART", "PIS", "ADJD", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich hab's gesagt Und nie beklagt:", "tokens": ["Ich", "hab's", "ge\u00b7sagt", "Und", "nie", "be\u00b7klagt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "VVPP", "KON", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Herz und ganzes Leben", "tokens": ["Mein", "Herz", "und", "gan\u00b7zes", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sei, Jesu, dir ergeben;", "tokens": ["Sei", ",", "Je\u00b7su", ",", "dir", "er\u00b7ge\u00b7ben", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "$,", "NE", "$,", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hier hast du meine Hand aufs neu,", "tokens": ["Hier", "hast", "du", "mei\u00b7ne", "Hand", "aufs", "neu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "APPRART", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Setz du dein Siegel nur dabei!", "tokens": ["Setz", "du", "dein", "Sie\u00b7gel", "nur", "da\u00b7bei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "ADV", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}