{"textgrid.poem.29755": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "170.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Brennus dienet keinem Herren, hat ihm selbsten zu befehlen,", "tokens": ["Bren\u00b7nus", "die\u00b7net", "kei\u00b7nem", "Her\u00b7ren", ",", "hat", "ihm", "selbs\u00b7ten", "zu", "be\u00b7feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIAT", "NN", "$,", "VAFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Und man wil ihm seinen Herren dennoch unter Narren zehlen.", "tokens": ["Und", "man", "wil", "ihm", "sei\u00b7nen", "Her\u00b7ren", "den\u00b7noch", "un\u00b7ter", "Nar\u00b7ren", "zeh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "PPER", "PPOSAT", "NN", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}