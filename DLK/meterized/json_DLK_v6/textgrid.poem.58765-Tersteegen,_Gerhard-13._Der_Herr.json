{"textgrid.poem.58765": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "13. Der Herr", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist der Herr mit Nachdruck nur,", "tokens": ["Du", "bist", "der", "Herr", "mit", "Nach\u00b7druck", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Selbstherrscher aller Kreatur;", "tokens": ["Selbst\u00b7herr\u00b7scher", "al\u00b7ler", "Kre\u00b7a\u00b7tur", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du denkst, du sprichst, so mu\u00df es werden.", "tokens": ["Du", "denkst", ",", "du", "sprichst", ",", "so", "mu\u00df", "es", "wer\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "ADV", "VMFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die h\u00f6chsten Geister huld'gen dir,", "tokens": ["Die", "h\u00f6chs\u00b7ten", "Geis\u00b7ter", "huld'\u00b7gen", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sonn', Mond, und alles schmiegt sich hier;", "tokens": ["Sonn'", ",", "Mond", ",", "und", "al\u00b7les", "schmiegt", "sich", "hier", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "KON", "PIS", "VVFIN", "PRF", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mensch, rebellierst du, Wurm der Erden?", "tokens": ["Mensch", ",", "re\u00b7bel\u00b7lierst", "du", ",", "Wurm", "der", "Er\u00b7den", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "NN", "ART", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Mein gn\u00e4d'ger Herr, nimm du mich an,", "tokens": ["Mein", "gn\u00e4d'\u00b7ger", "Herr", ",", "nimm", "du", "mich", "an", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VVIMP", "PPER", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mein Alles sei dir untertan!", "tokens": ["Mein", "Al\u00b7les", "sei", "dir", "un\u00b7ter\u00b7tan", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "PIS", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Halleluja! Halleluja!", "tokens": ["Hal\u00b7le\u00b7lu\u00b7ja", "!", "Hal\u00b7le\u00b7lu\u00b7ja", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ITJ", "$.", "ITJ", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}}}}