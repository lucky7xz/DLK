{"textgrid.poem.55175": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Heilige Familie", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O des s\u00fc\u00dfen Kindes und o der gl\u00fccklichen Mutter,", "tokens": ["O", "des", "s\u00fc\u00b7\u00dfen", "Kin\u00b7des", "und", "o", "der", "gl\u00fcck\u00b7li\u00b7chen", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "KON", "FM", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Wie sie sich einzig in ihm, wie es in ihr sich ergetzt!", "tokens": ["Wie", "sie", "sich", "ein\u00b7zig", "in", "ihm", ",", "wie", "es", "in", "ihr", "sich", "er\u00b7getzt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADJD", "APPR", "PPER", "$,", "PWAV", "PPER", "APPR", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Welche Wonne gew\u00e4hrte der Blick auf dies herrliche Bild mir,", "tokens": ["Wel\u00b7che", "Won\u00b7ne", "ge\u00b7w\u00e4hr\u00b7te", "der", "Blick", "auf", "dies", "herr\u00b7li\u00b7che", "Bild", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "VVFIN", "ART", "NN", "APPR", "PDS", "ADJA", "NN", "PPER", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "St\u00fcnd ich Armer nicht so heilig, wie Joseph, dabei!", "tokens": ["St\u00fcnd", "ich", "Ar\u00b7mer", "nicht", "so", "hei\u00b7lig", ",", "wie", "Jo\u00b7se\u00b7ph", ",", "da\u00b7bei", "!"], "token_info": ["word", "word", "word", "word", "wor