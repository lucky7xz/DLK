{"textgrid.poem.41790": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sie, die einst F\u00fcrstin \u2013 eine K\u00f6nigin,", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie, die einst F\u00fcrstin \u2013 eine K\u00f6nigin,", "tokens": ["Sie", ",", "die", "einst", "F\u00fcrs\u00b7tin", "\u2013", "ei\u00b7ne", "K\u00f6\u00b7ni\u00b7gin", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ADV", "NN", "$(", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nun fern der Heimat \u2013 eine Bettlerin!", "tokens": ["Nun", "fern", "der", "Hei\u00b7mat", "\u2013", "ei\u00b7ne", "Bett\u00b7le\u00b7rin", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "$(", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Aus stolzem Mediceer-Blut entsprossen", "tokens": ["Aus", "stol\u00b7zem", "Me\u00b7di\u00b7ce\u00b7er\u00b7Blut", "ent\u00b7spros\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Herrscherin auf Frankreichs hohem Thron,", "tokens": ["Und", "Herr\u00b7sche\u00b7rin", "auf", "Fran\u00b7kreichs", "ho\u00b7hem", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Auf dunklem Lockenhaar die goldne Kron',", "tokens": ["Auf", "dunk\u00b7lem", "Lo\u00b7cken\u00b7haar", "die", "gold\u00b7ne", "Kron'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Vom Purpur wallend die Gestalt umflossen:", "tokens": ["Vom", "Pur\u00b7pur", "wal\u00b7lend", "die", "Ge\u00b7stalt", "um\u00b7flos\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das war Maria in vergangner Zeit \u2013", "tokens": ["Das", "war", "Ma\u00b7ria", "in", "ver\u00b7gang\u00b7ner", "Zeit", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NE", "APPR", "ADJA", "NN", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Doch jetzt \u2013 wo ist die einst'ge Herrlichkeit?", "tokens": ["Doch", "jetzt", "\u2013", "wo", "ist", "die", "einst'\u00b7ge", "Herr\u00b7lich\u00b7keit", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$(", "PWAV", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Jetzt irrt sie obdachlos von Land zu Lande.", "tokens": ["Jetzt", "irrt", "sie", "ob\u00b7dach\u00b7los", "von", "Land", "zu", "Lan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aufscho\u00df der Samen, den sie selbst ges\u00e4t,", "tokens": ["Auf\u00b7scho\u00df", "der", "Sa\u00b7men", ",", "den", "sie", "selbst", "ge\u00b7s\u00e4t", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Zu blut'ger Ernt', von blut'ger Hand gem\u00e4ht,", "tokens": ["Zu", "blut'\u00b7ger", "Ernt'", ",", "von", "blut'\u00b7ger", "Hand", "ge\u00b7m\u00e4ht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die ihr gereift zum Fluche und zur Schande.", "tokens": ["Die", "ihr", "ge\u00b7reift", "zum", "Flu\u00b7che", "und", "zur", "Schan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der eigne Sohn war's, der sie kalt verstie\u00df \u2013", "tokens": ["Der", "eig\u00b7ne", "Sohn", "wa\u00b7r's", ",", "der", "sie", "kalt", "ver\u00b7stie\u00df", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das ist die Schreckensmacht der Nemesis,", "tokens": ["Das", "ist", "die", "Schre\u00b7ckens\u00b7macht", "der", "Ne\u00b7me\u00b7sis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df sie Verbrechen s\u00fchnt stets mit Verbrechen.", "tokens": ["Da\u00df", "sie", "Ver\u00b7bre\u00b7chen", "s\u00fchnt", "stets", "mit", "Ver\u00b7bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Tyrannei st\u00fcrzt fremde Tyrannei,", "tokens": ["Die", "Ty\u00b7ran\u00b7nei", "st\u00fcrzt", "frem\u00b7de", "Ty\u00b7ran\u00b7nei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Schleppt immer neue Ketten nur herbei,", "tokens": ["Schleppt", "im\u00b7mer", "neu\u00b7e", "Ket\u00b7ten", "nur", "her\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJA", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Bis da\u00df ein Volk erstarkt sie zu zerbrechen.", "tokens": ["Bis", "da\u00df", "ein", "Volk", "er\u00b7starkt", "sie", "zu", "zer\u00b7bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "NN", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Gefangen, wo sie einst Regentin war,", "tokens": ["Ge\u00b7fan\u00b7gen", ",", "wo", "sie", "einst", "Re\u00b7gen\u00b7tin", "war", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "ADV", "NN", "VAFIN", "$,"], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.2": {"text": "Und dann verfolgt, verbannt f\u00fcr immerdar;", "tokens": ["Und", "dann", "ver\u00b7folgt", ",", "ver\u00b7bannt", "f\u00fcr", "im\u00b7mer\u00b7dar", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "$,", "VVFIN", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Aus England und aus Holland selbst vertrieben", "tokens": ["Aus", "En\u00b7gland", "und", "aus", "Hol\u00b7land", "selbst", "ver\u00b7trie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "APPR", "NE", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Betritt sie K\u00f6ln, die heil'ge Stadt am Rhein;", "tokens": ["Be\u00b7tritt", "sie", "K\u00f6ln", ",", "die", "heil'\u00b7ge", "Stadt", "am", "Rhein", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NE", "$,", "ART", "ADJA", "NN", "APPRART", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Unwillig schaut der deutsche B\u00fcrger drein,", "tokens": ["Un\u00b7wil\u00b7lig", "schaut", "der", "deut\u00b7sche", "B\u00fcr\u00b7ger", "drein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und ist doch treu dem heil'gen Gastrecht blieben.", "tokens": ["Und", "ist", "doch", "treu", "dem", "heil'\u00b7gen", "Gast\u00b7recht", "blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "K\u00f6ln ist der Greisin sch\u00fctzendes Asyl,", "tokens": ["K\u00f6ln", "ist", "der", "Grei\u00b7sin", "sch\u00fct\u00b7zen\u00b7des", "A\u00b7syl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "++-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Doch manchmal wogt ein z\u00fcrnendes Gew\u00fchl", "tokens": ["Doch", "manch\u00b7mal", "wogt", "ein", "z\u00fcr\u00b7nen\u00b7des", "Ge\u00b7w\u00fchl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mutwillig h\u00f6hnend unter ihrem Fenster.", "tokens": ["Mut\u00b7wil\u00b7lig", "h\u00f6h\u00b7nend", "un\u00b7ter", "ih\u00b7rem", "Fens\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.4": {"text": "Dann flieht erschreckt sie in ihr Schlafgemach \u2013", "tokens": ["Dann", "flieht", "er\u00b7schreckt", "sie", "in", "ihr", "Schlaf\u00b7ge\u00b7mach", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein Heil'genbild, ein reuevolles Ach!", "tokens": ["Ein", "Heil'\u00b7gen\u00b7bild", ",", "ein", "reu\u00b7e\u00b7vol\u00b7les", "Ach", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Soll scheuchen ihrer Thaten Rachgespenster.", "tokens": ["Soll", "scheu\u00b7chen", "ih\u00b7rer", "Tha\u00b7ten", "Rach\u00b7ge\u00b7spens\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Da naht ein Julitag, der sie erl\u00f6st,", "tokens": ["Da", "naht", "ein", "Ju\u00b7li\u00b7tag", ",", "der", "sie", "er\u00b7l\u00f6st", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sie betet, da\u00df der Herr sie nicht verst\u00f6\u00dft,", "tokens": ["Sie", "be\u00b7tet", ",", "da\u00df", "der", "Herr", "sie", "nicht", "ver\u00b7st\u00f6\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie nicht in ihren S\u00fcnden l\u00e4\u00dft verderben;", "tokens": ["Sie", "nicht", "in", "ih\u00b7ren", "S\u00fcn\u00b7den", "l\u00e4\u00dft", "ver\u00b7der\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "An ihrem Lager steht der Nuntius,", "tokens": ["An", "ih\u00b7rem", "La\u00b7ger", "steht", "der", "Nun\u00b7ti\u00b7us", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das Totengl\u00f6ckchen mahnt wie Himmelsgru\u00df,", "tokens": ["Das", "To\u00b7ten\u00b7gl\u00f6\u00b7ck\u00b7chen", "mahnt", "wie", "Him\u00b7mels\u00b7gru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Das heil'ge Oel benetzet sie im Sterben.", "tokens": ["Das", "heil'\u00b7ge", "O\u00b7el", "be\u00b7net\u00b7zet", "sie", "im", "Ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Ob Frankreich auch die Lebende verstie\u00df,", "tokens": ["Ob", "Fran\u00b7kreich", "auch", "die", "Le\u00b7ben\u00b7de", "ver\u00b7stie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+---+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Die K\u00f6nigsleiche fordert doch Paris", "tokens": ["Die", "K\u00f6\u00b7nigs\u00b7lei\u00b7che", "for\u00b7dert", "doch", "Pa\u00b7ris"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und holt sie ein mit k\u00f6niglichen Ehren.", "tokens": ["Und", "holt", "sie", "ein", "mit", "k\u00f6\u00b7nig\u00b7li\u00b7chen", "Eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im K\u00f6lner Dome blieb allein ihr Herz,", "tokens": ["Im", "K\u00f6l\u00b7ner", "Do\u00b7me", "blieb", "al\u00b7lein", "ihr", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "In einem Schrein von wohlgegossnem Erz,", "tokens": ["In", "ei\u00b7nem", "Schrein", "von", "wohl\u00b7ge\u00b7goss\u00b7nem", "Erz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Noch die Erinnerung an sie zu n\u00e4hren.", "tokens": ["Noch", "die", "E\u00b7rin\u00b7ne\u00b7rung", "an", "sie", "zu", "n\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "---+--+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Drauf eingegraben war ein frommer Spruch \u2013", "tokens": ["Drauf", "ein\u00b7ge\u00b7gra\u00b7ben", "war", "ein", "from\u00b7mer", "Spruch", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVPP", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Doch K\u00f6lner B\u00fcrger nannten's eitel Lug", "tokens": ["Doch", "K\u00f6l\u00b7ner", "B\u00fcr\u00b7ger", "nann\u00b7ten's", "ei\u00b7tel", "Lug"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "NN", "NE", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und stahlen weg die Tafel von dem Male,", "tokens": ["Und", "stah\u00b7len", "weg", "die", "Ta\u00b7fel", "von", "dem", "Ma\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es wagte niemand je sie zu erneun:", "tokens": ["Es", "wag\u00b7te", "nie\u00b7mand", "je", "sie", "zu", "er\u00b7neun", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADV", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein Volks-Urteil wie Gottes-Urteil scheun", "tokens": ["Ein", "Volks\u00b7Ur\u00b7teil", "wie", "Got\u00b7tes\u00b7Ur\u00b7teil", "scheun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KOKOM", "NN", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "War Recht in K\u00f6ln, der Stadt vom heil'gen Grale.", "tokens": ["War", "Recht", "in", "K\u00f6ln", ",", "der", "Stadt", "vom", "heil'\u00b7gen", "Gra\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "