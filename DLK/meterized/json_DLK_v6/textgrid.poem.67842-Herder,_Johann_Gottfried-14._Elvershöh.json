{"textgrid.poem.67842": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "14. Elversh\u00f6h", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich legte mein Haupt auf Elversh\u00f6h,", "tokens": ["Ich", "leg\u00b7te", "mein", "Haupt", "auf", "El\u00b7ver\u00b7sh\u00f6h", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "NE", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mein' Augen begannen zu sinken,", "tokens": ["Mein'", "Au\u00b7gen", "be\u00b7gan\u00b7nen", "zu", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Da kamen gegangen zwo Jungfraun sch\u00f6n,", "tokens": ["Da", "ka\u00b7men", "ge\u00b7gan\u00b7gen", "zwo", "Jung\u00b7fraun", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VVPP", "CARD", "NN", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Die th\u00e4ten mir lieblich winken.", "tokens": ["Die", "th\u00e4\u00b7ten", "mir", "lieb\u00b7lich", "win\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Eine, sie strich mein weisses Kinn,", "tokens": ["Die", "Ei\u00b7ne", ",", "sie", "strich", "mein", "weis\u00b7ses", "Kinn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "ADJD", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die zweite lispelt ins Ohr mir:", "tokens": ["Die", "zwei\u00b7te", "lis\u00b7pelt", "ins", "Ohr", "mir", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "APPRART", "NN", "PPER", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Steh auf, du muntrer J\u00fcngling! auf!", "tokens": ["Steh", "auf", ",", "du", "mun\u00b7trer", "J\u00fcng\u00b7ling", "!", "auf", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$.", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erheb', erhebe den Tanz hier!", "tokens": ["Er\u00b7heb'", ",", "er\u00b7he\u00b7be", "den", "Tanz", "hier", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Steh auf, du muntrer J\u00fcngling, auf!", "tokens": ["Steh", "auf", ",", "du", "mun\u00b7trer", "J\u00fcng\u00b7ling", ",", "auf", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erheb', erhebe den Tanz hier!", "tokens": ["Er\u00b7heb'", ",", "er\u00b7he\u00b7be", "den", "Tanz", "hier", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Meine Jungfraun soll'n dir Lieder singen,", "tokens": ["Mei\u00b7ne", "Jung\u00b7fraun", "soll'n", "dir", "Lie\u00b7der", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Die sch\u00f6nsten Lieder zu h\u00f6ren.", "tokens": ["Die", "sch\u00f6ns\u00b7ten", "Lie\u00b7der", "zu", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Die Eine begann zu singen ein Lied,", "tokens": ["Die", "Ei\u00b7ne", "be\u00b7gann", "zu", "sin\u00b7gen", "ein", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PTKZU", "VVINF", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Sch\u00f6nste aller Sch\u00f6nen;", "tokens": ["Die", "Sch\u00f6ns\u00b7te", "al\u00b7ler", "Sch\u00f6\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der brausende Strom, er flo\u00df nicht mehr,", "tokens": ["Der", "brau\u00b7sen\u00b7de", "Strom", ",", "er", "flo\u00df", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "PTKNEG", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und horcht den s\u00fcssen T\u00f6nen.", "tokens": ["Und", "horcht", "den", "s\u00fcs\u00b7sen", "T\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der brausende Strom, er flo\u00df nicht mehr,", "tokens": ["Der", "brau\u00b7sen\u00b7de", "Strom", ",", "er", "flo\u00df", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "PTKNEG", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Stand still und horchte f\u00fchlend,", "tokens": ["Stand", "still", "und", "horch\u00b7te", "f\u00fch\u00b7lend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Fischlein schwammen in heller Fluth,", "tokens": ["Die", "Fisc\u00b7hlein", "schwam\u00b7men", "in", "hel\u00b7ler", "Fluth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit ihren Feinden spielend.", "tokens": ["Mit", "ih\u00b7ren", "Fein\u00b7den", "spie\u00b7lend", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die Fischlein all' in heller Fluth,", "tokens": ["Die", "Fisc\u00b7hlein", "all'", "in", "hel\u00b7ler", "Fluth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie scherzten auf und nieder,", "tokens": ["Sie", "scherz\u00b7ten", "auf", "und", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die V\u00f6glein all' im gr\u00fcnen Wald,", "tokens": ["Die", "V\u00f6\u00b7glein", "all'", "im", "gr\u00fc\u00b7nen", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie h\u00fcpften, zirpten Lieder.", "tokens": ["Sie", "h\u00fcpf\u00b7ten", ",", "zirp\u00b7ten", "Lie\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "\u00bbh\u00f6r an, du muntrer J\u00fcngling, h\u00f6r an,", "tokens": ["\u00bb", "h\u00f6r", "an", ",", "du", "mun\u00b7trer", "J\u00fcng\u00b7ling", ",", "h\u00f6r", "an", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Willt du hier bei uns bleiben?", "tokens": ["Willt", "du", "hier", "bei", "uns", "blei\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir wollen dich lehren das Runenbuch,", "tokens": ["Wir", "wol\u00b7len", "dich", "leh\u00b7ren", "das", "Ru\u00b7nen\u00b7buch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und Zaubereien schreiben.", "tokens": ["Und", "Zau\u00b7be\u00b7rei\u00b7en", "schrei\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ich will dich lehren, den wilden B\u00e4r", "tokens": ["Ich", "will", "dich", "leh\u00b7ren", ",", "den", "wil\u00b7den", "B\u00e4r"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "VVINF", "$,", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zu binden mit Wort und Zeichen;", "tokens": ["Zu", "bin\u00b7den", "mit", "Wort", "und", "Zei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der Drache, der ruht auf rothem Gold,", "tokens": ["Der", "Dra\u00b7che", ",", "der", "ruht", "auf", "ro\u00b7them", "Gold", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Soll schnell dir fliehn und weichen.\u00ab", "tokens": ["Soll", "schnell", "dir", "fliehn", "und", "wei\u00b7chen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "ADJD", "PPER", "VVINF", "KON", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Sie tanzten hin, sie tanzten her;", "tokens": ["Sie", "tanz\u00b7ten", "hin", ",", "sie", "tanz\u00b7ten", "her", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu buhlen ihr Herz begehrt\u00ab.", "tokens": ["Zu", "buh\u00b7len", "ihr", "Herz", "be\u00b7gehrt", "\u00ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "NN", "VVPP", "$(", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der muntre J\u00fcngling, er sa\u00df da,", "tokens": ["Der", "mun\u00b7tre", "J\u00fcng\u00b7ling", ",", "er", "sa\u00df", "da", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Gest\u00fczet auf sein Schwert.", "tokens": ["Ge\u00b7st\u00fc\u00b7zet", "auf", "sein", "Schwert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "\u00bbh\u00f6r an, du muntrer J\u00fcngling, h\u00f6r an:", "tokens": ["\u00bb", "h\u00f6r", "an", ",", "du", "mun\u00b7trer", "J\u00fcng\u00b7ling", ",", "h\u00f6r", "an", ":"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Willt du nicht mit uns sprechen,", "tokens": ["Willt", "du", "nicht", "mit", "uns", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So reissen wir dir, mit Messer und Schwert,", "tokens": ["So", "reis\u00b7sen", "wir", "dir", ",", "mit", "Mes\u00b7ser", "und", "Schwert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das Herz aus, uns zu r\u00e4chen.\u00ab", "tokens": ["Das", "Herz", "aus", ",", "uns", "zu", "r\u00e4\u00b7chen", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "PPER", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Und da mein gutes, gutes Gl\u00fcck!", "tokens": ["Und", "da", "mein", "gu\u00b7tes", ",", "gu\u00b7tes", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Hahn fing an zu kr\u00e4h'n.", "tokens": ["Der", "Hahn", "fing", "an", "zu", "kr\u00e4h'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "PTKZU", "VVFIN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich w\u00e4r sonst blieb'n auf Elversh\u00f6h,", "tokens": ["Ich", "w\u00e4r", "sonst", "blie\u00b7b'n", "auf", "El\u00b7ver\u00b7sh\u00f6h", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVFIN", "APPR", "NE", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Bei Elvers Jungfraun sch\u00f6n.", "tokens": ["Bei", "El\u00b7vers", "Jung\u00b7fraun", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Drum rath ich jedem J\u00fcngling,", "tokens": ["Drum", "rath", "ich", "je\u00b7dem", "J\u00fcng\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der zieht nach Hofe fein,", "tokens": ["Der", "zieht", "nach", "Ho\u00b7fe", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er seze sich nicht auf Elvers H\u00f6h,", "tokens": ["Er", "se\u00b7ze", "sich", "nicht", "auf", "El\u00b7vers", "H\u00f6h", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKNEG", "APPR", "NE", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Allda zu schlummern ein.", "tokens": ["All\u00b7da", "zu", "schlum\u00b7mern", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PTKZU", "VVINF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}