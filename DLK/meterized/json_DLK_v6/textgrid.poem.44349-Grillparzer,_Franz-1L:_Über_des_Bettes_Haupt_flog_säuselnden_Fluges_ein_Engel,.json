{"textgrid.poem.44349": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00dcber des Bettes Haupt flog s\u00e4uselnden Fluges ein Engel,", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcber des Bettes Haupt flog s\u00e4uselnden Fluges ein Engel,", "tokens": ["\u00dc\u00b7ber", "des", "Bet\u00b7tes", "Haupt", "flog", "s\u00e4u\u00b7seln\u00b7den", "Flu\u00b7ges", "ein", "En\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Und des Unsterblichen Blick fiel auf das schlafende Kind.", "tokens": ["Und", "des", "U\u00b7nsterb\u00b7li\u00b7chen", "Blick", "fiel", "auf", "das", "schla\u00b7fen\u00b7de", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "--+---+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Wie sein eigenes Bild im Spiegel silberner Wellen,", "tokens": ["Wie", "sein", "ei\u00b7ge\u00b7nes", "Bild", "im", "Spie\u00b7gel", "sil\u00b7ber\u00b7ner", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "L\u00e4chelt ihn freundlich und hold an die s\u00fc\u00dfe Gestalt.", "tokens": ["L\u00e4\u00b7chelt", "ihn", "freund\u00b7lich", "und", "hold", "an", "die", "s\u00fc\u00b7\u00dfe", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.5": {"text": "Leise sinkt er herab, sich freuend der lieblichen T\u00e4uschung,", "tokens": ["Lei\u00b7se", "sinkt", "er", "her\u00b7ab", ",", "sich", "freu\u00b7end", "der", "lieb\u00b7li\u00b7chen", "T\u00e4u\u00b7schung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "$,", "PRF", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Und tritt luftigen Schritts vor das Schlafende hin.", "tokens": ["Und", "tritt", "luf\u00b7ti\u00b7gen", "Schritts", "vor", "das", "Schla\u00b7fen\u00b7de", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "--+--++-+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Ach, es schlummert so s\u00fc\u00df, und Unschuld und himmlischer Friede", "tokens": ["Ach", ",", "es", "schlum\u00b7mert", "so", "s\u00fc\u00df", ",", "und", "Un\u00b7schuld", "und", "himm\u00b7li\u00b7scher", "Frie\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "ADV", "ADJD", "$,", "KON", "NN", "KON", "ADJA", "NN"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Wehen im Atem des Munds, ruhn auf der silbernen Stirn,", "tokens": ["We\u00b7hen", "im", "A\u00b7tem", "des", "Munds", ",", "ruhn", "auf", "der", "sil\u00b7ber\u00b7nen", "Stirn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "$,", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.9": {"text": "Kr\u00e4useln zum Heiligenschein des Hauptes goldene Locken,", "tokens": ["Kr\u00e4u\u00b7seln", "zum", "Hei\u00b7li\u00b7gen\u00b7schein", "des", "Haup\u00b7tes", "gol\u00b7de\u00b7ne", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Ruhn, wie ein Lilienzweig, in der gefalteten Hand.", "tokens": ["Ruhn", ",", "wie", "ein", "Li\u00b7li\u00b7en\u00b7zweig", ",", "in", "der", "ge\u00b7fal\u00b7te\u00b7ten", "Hand", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.11": {"text": "Freundlich l\u00e4chelt der Engel; doch bald umw\u00f6lkt sich sein Antlitz,", "tokens": ["Freund\u00b7lich", "l\u00e4\u00b7chelt", "der", "En\u00b7gel", ";", "doch", "bald", "um\u00b7w\u00f6lkt", "sich", "sein", "Ant\u00b7litz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "ADV", "ADV", "VVFIN", "PRF", "PPOSAT", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.12": {"text": "Und mit br\u00fctendem Ernst wendet er seufzend sich ab.", "tokens": ["Und", "mit", "br\u00fc\u00b7ten\u00b7dem", "Ernst", "wen\u00b7det", "er", "seuf\u00b7zend", "sich", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "PRF", "PTKVZ", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.13": {"text": "Er \u00fcberschauet im Geist den Sturm der kommenden Tage,", "tokens": ["Er", "\u00fc\u00b7bersc\u00b7hau\u00b7et", "im", "Geist", "den", "Sturm", "der", "kom\u00b7men\u00b7den", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Dem nur die Eiche steht, der die Blume zerknickt;", "tokens": ["Dem", "nur", "die", "Ei\u00b7che", "steht", ",", "der", "die", "Blu\u00b7me", "zer\u00b7knickt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "VVFIN", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Rauschen h\u00f6rt er des Ungl\u00fccks seelenmordende Pfeile,", "tokens": ["Rau\u00b7schen", "h\u00f6rt", "er", "des", "Un\u00b7gl\u00fccks", "see\u00b7len\u00b7mor\u00b7den\u00b7de", "Pfei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.16": {"text": "Wider die Unschuld und Recht nur ein zerbrechlicher Schild;", "tokens": ["Wi\u00b7der", "die", "Un\u00b7schuld", "und", "Recht", "nur", "ein", "zer\u00b7brech\u00b7li\u00b7cher", "Schild", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.17": {"text": "Tr\u00e4nend sieht er das Aug, das weich die Wimper bedecket,", "tokens": ["Tr\u00e4\u00b7nend", "sieht", "er", "das", "Aug", ",", "das", "weich", "die", "Wim\u00b7per", "be\u00b7de\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-+--", "measure": "trochaic.hexa.relaxed"}, "line.18": {"text": "Und zerschlagen die Brust, die jetzt atmend sich hebt.", "tokens": ["Und", "zer\u00b7schla\u00b7gen", "die", "Brust", ",", "die", "jetzt", "at\u00b7mend", "sich", "hebt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "PRELS", "ADV", "ADJD", "PRF", "VVFIN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.19": {"text": "Banges Mitleid erfa\u00dft die Seele des himmlischen Boten,", "tokens": ["Ban\u00b7ges", "Mit\u00b7leid", "er\u00b7fa\u00dft", "die", "See\u00b7le", "des", "himm\u00b7li\u00b7schen", "Bo\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.20": {"text": "Fragend schaut er empor, und der Allm\u00e4chtige nickt.", "tokens": ["Fra\u00b7gend", "schaut", "er", "em\u00b7por", ",", "und", "der", "All\u00b7m\u00e4ch\u00b7ti\u00b7ge", "nickt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.21": {"text": "Da umf\u00e4ngt er den Nacken und k\u00fc\u00dft die zuckenden Lippen,", "tokens": ["Da", "um\u00b7f\u00e4ngt", "er", "den", "Na\u00b7cken", "und", "k\u00fc\u00dft", "die", "zu\u00b7cken\u00b7den", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.22": {"text": "Spricht: Sei gl\u00fccklich, o Kind! \u2013 und die Kleine war tot.", "tokens": ["Spricht", ":", "Sei", "gl\u00fcck\u00b7lich", ",", "o", "Kind", "!", "\u2013", "und", "die", "Klei\u00b7ne", "war", "tot", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "VAIMP", "ADJD", "$,", "FM", "NN", "$.", "$(", "KON", "ART", "ADJA", "VAFIN", "ADJD", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}}}}}