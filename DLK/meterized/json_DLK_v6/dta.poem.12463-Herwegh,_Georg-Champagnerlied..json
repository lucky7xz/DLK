{"dta.poem.12463": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Champagnerlied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1843", "urn": "urn:nbn:de:kobv:b4-200905192440", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wir griffen j\u00fcngst, den Weltbrand anzufachen,", "tokens": ["Wir", "grif\u00b7fen", "j\u00fcngst", ",", "den", "Welt\u00b7brand", "an\u00b7zu\u00b7fa\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr Br\u00fcder, nach dem Schwert;", "tokens": ["Ihr", "Br\u00fc\u00b7der", ",", "nach", "dem", "Schwert", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch diese Welt, so la\u00dft uns dr\u00fcber lachen!", "tokens": ["Doch", "die\u00b7se", "Welt", ",", "so", "la\u00dft", "uns", "dr\u00fc\u00b7ber", "la\u00b7chen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "$,", "ADV", "VVIMP", "PPER", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ist unsers Ernsts nicht werth.", "tokens": ["Ist", "un\u00b7sers", "Ernsts", "nicht", "werth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Juchhe, die Narrenschelle!", "tokens": ["Juch\u00b7he", ",", "die", "Nar\u00b7ren\u00b7schel\u00b7le", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Die Jugend ist ein Glas Champagnerwein:", "tokens": ["Die", "Ju\u00b7gend", "ist", "ein", "Glas", "Cham\u00b7pag\u00b7ner\u00b7wein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Drum will sie schnelle, schnelle,", "tokens": ["Drum", "will", "sie", "schnel\u00b7le", ",", "schnel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Gleich frisch an ihrer Quelle,", "tokens": ["Gleich", "frisch", "an", "ih\u00b7rer", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Getrunken sein.", "tokens": ["Ge\u00b7trun\u00b7ken", "sein", "."], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Schenkt ein! Schenkt ein!", "tokens": ["Schenkt", "ein", "!", "Schenkt", "ein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$.", "VVFIN", "PTKVZ", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Was k\u00fcmmern uns die Kronen und die F\u00fcrsten?", "tokens": ["Was", "k\u00fcm\u00b7mern", "uns", "die", "Kro\u00b7nen", "und", "die", "F\u00fcrs\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gott segne unsre Herrn!", "tokens": ["Gott", "seg\u00b7ne", "uns\u00b7re", "Herrn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir wollen was zu trinken, wenn wir d\u00fcrsten,", "tokens": ["Wir", "wol\u00b7len", "was", "zu", "trin\u00b7ken", ",", "wenn", "wir", "d\u00fcrs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "PTKZU", "VVINF", "$,", "KOUS", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wir zechen All' so gern.", "tokens": ["Wir", "ze\u00b7chen", "All'", "so", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADV", "ADV", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.5": {"text": "La\u00dft uns die H\u00e4nde reichen", "tokens": ["La\u00dft", "uns", "die", "H\u00e4n\u00b7de", "rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zu trautem, frischem, fr\u00f6hlichem Verein!", "tokens": ["Zu", "trau\u00b7tem", ",", "fri\u00b7schem", ",", "fr\u00f6h\u00b7li\u00b7chem", "Ver\u00b7ein", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die Reben, nicht die Eichen,", "tokens": ["Die", "Re\u00b7ben", ",", "nicht", "die", "Ei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die sollen unser Zeichen,", "tokens": ["Die", "sol\u00b7len", "un\u00b7ser", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Ja, Zeichen sein.", "tokens": ["Ja", ",", "Zei\u00b7chen", "sein", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Schenkt ein! Schenkt ein!", "tokens": ["Schenkt", "ein", "!", "Schenkt", "ein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$.", "VVFIN", "PTKVZ", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Die S\u00fcndfluth drohte einstens zu verwaschen", "tokens": ["Die", "S\u00fcnd\u00b7fluth", "droh\u00b7te", "eins\u00b7tens", "zu", "ver\u00b7wa\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Herren liebsten Sohn:", "tokens": ["Des", "Her\u00b7ren", "liebs\u00b7ten", "Sohn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da barg er flugs den Witz in einer Flaschen,", "tokens": ["Da", "barg", "er", "flugs", "den", "Witz", "in", "ei\u00b7ner", "Fla\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der grausen Fluth zum Hohn.", "tokens": ["Der", "grau\u00b7sen", "Fluth", "zum", "Hohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wir haben sie gefangen!", "tokens": ["Wir", "ha\u00b7ben", "sie", "ge\u00b7fan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Heraus den Witz, die Weisheit heut hinein!", "tokens": ["He\u00b7raus", "den", "Witz", ",", "die", "Weis\u00b7heit", "heut", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der Witz soll heute prangen,", "tokens": ["Der", "Witz", "soll", "heu\u00b7te", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Weisheit soll gefangen,", "tokens": ["Die", "Weis\u00b7heit", "soll", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Gefangen sein.", "tokens": ["Ge\u00b7fan\u00b7gen", "sein", "."], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Schenkt ein! Schenkt ein!", "tokens": ["Schenkt", "ein", "!", "Schenkt", "ein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$.", "VVFIN", "PTKVZ", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "La\u00dft den Philister mit dem Leben sparen \u2014", "tokens": ["La\u00dft", "den", "Phi\u00b7lis\u00b7ter", "mit", "dem", "Le\u00b7ben", "spa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er ist ein armer Mann.", "tokens": ["Er", "ist", "ein", "ar\u00b7mer", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Soll ich zu Wasser in den Himmel fahren,", "tokens": ["Soll", "ich", "zu", "Was\u00b7ser", "in", "den", "Him\u00b7mel", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn ich's im Feuer kann?", "tokens": ["Wenn", "ich's", "im", "Feu\u00b7er", "kann", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPRART", "NN", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Juchhe, die Narrenschelle!", "tokens": ["Juch\u00b7he", ",", "die", "Nar\u00b7ren\u00b7schel\u00b7le", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Die Jugend ist ein Glas Champagnerwein:", "tokens": ["Die", "Ju\u00b7gend", "ist", "ein", "Glas", "Cham\u00b7pag\u00b7ner\u00b7wein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Drum will sie schnelle, schnelle,", "tokens": ["Drum", "will", "sie", "schnel\u00b7le", ",", "schnel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Gleich frisch an ihrer Quelle,", "tokens": ["Gleich", "frisch", "an", "ih\u00b7rer", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Getrunken sein.", "tokens": ["Ge\u00b7trun\u00b7ken", "sein", "."], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Schenkt ein! Schenkt ein!", "tokens": ["Schenkt", "ein", "!", "Schenkt", "ein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$.", "VVFIN", "PTKVZ", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}