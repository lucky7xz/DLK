{"textgrid.poem.44839": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die guten deutschen L\u00e4mmer,", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die guten deutschen L\u00e4mmer,", "tokens": ["Die", "gu\u00b7ten", "deut\u00b7schen", "L\u00e4m\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nicht gern nennt ich sie Schafe,", "tokens": ["Nicht", "gern", "nennt", "ich", "sie", "Scha\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVFIN", "PPER", "PPER", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Sie brauchen einen Widder,", "tokens": ["Sie", "brau\u00b7chen", "ei\u00b7nen", "Wid\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der vorgeht und sie leitet", "tokens": ["Der", "vor\u00b7geht", "und", "sie", "lei\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zur etwas d\u00fcrren Grasung", "tokens": ["Zur", "et\u00b7was", "d\u00fcr\u00b7ren", "Gra\u00b7sung"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Am deutschen Helikon.", "tokens": ["Am", "deut\u00b7schen", "He\u00b7li\u00b7kon", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der vorige hie\u00df Schlegel,", "tokens": ["Der", "vo\u00b7ri\u00b7ge", "hie\u00df", "Schle\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Halb Trotz und halb Geschw\u00e4nzel;", "tokens": ["Halb", "Trotz", "und", "halb", "Ge\u00b7schw\u00e4n\u00b7zel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Der jetzige hei\u00dft Menzel,", "tokens": ["Der", "jet\u00b7zi\u00b7ge", "hei\u00dft", "Men\u00b7zel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "NE", "$,"], "meter": "-++-+-+", "measure": "unknown.measure.tetra"}, "line.10": {"text": "Nicht Widder, eher Sch\u00f6ps.", "tokens": ["Nicht", "Wid\u00b7der", ",", "e\u00b7her", "Sch\u00f6ps", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Dem folgen sie in Scharen", "tokens": ["Dem", "fol\u00b7gen", "sie", "in", "Scha\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Den Weg zum heimisch Wahren,", "tokens": ["Den", "Weg", "zum", "hei\u00b7misch", "Wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Und ruft er: Ehrlos Goethe,", "tokens": ["Und", "ruft", "er", ":", "Ehr\u00b7los", "Goe\u00b7the", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$.", "NN", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Ein Schurk Johannes M\u00fcller,", "tokens": ["Ein", "Schurk", "Jo\u00b7han\u00b7nes", "M\u00fcl\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Die guten deutschen L\u00e4mmer,", "tokens": ["Die", "gu\u00b7ten", "deut\u00b7schen", "L\u00e4m\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Nicht gern nennt ich sie Schafe,", "tokens": ["Nicht", "gern", "nennt", "ich", "sie", "Scha\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVFIN", "PPER", "PPER", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.17": {"text": "Sie bl\u00f6ken wie im Schlafe", "tokens": ["Sie", "bl\u00f6\u00b7ken", "wie", "im", "Schla\u00b7fe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Ein beifallgebend: B\u00e4h!", "tokens": ["Ein", "bei\u00b7fall\u00b7ge\u00b7bend", ":", "B\u00e4h", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "VVPP", "$.", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}