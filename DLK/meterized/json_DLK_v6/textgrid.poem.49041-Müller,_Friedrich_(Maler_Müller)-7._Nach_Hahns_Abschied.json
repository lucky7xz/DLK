{"textgrid.poem.49041": {"metadata": {"author": {"name": "M\u00fcller, Friedrich (Maler M\u00fcller)", "birth": "N.A.", "death": "N.A."}, "title": "7. Nach Hahns Abschied", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach sie singet, die br\u00fcnstige Finke,", "tokens": ["Ach", "sie", "sin\u00b7get", ",", "die", "br\u00fcns\u00b7ti\u00b7ge", "Fin\u00b7ke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Breitet den zarten Fl\u00fcgel \u00fcbers vollendete Nestchen,", "tokens": ["Brei\u00b7tet", "den", "zar\u00b7ten", "Fl\u00fc\u00b7gel", "\u00fc\u00b7bers", "voll\u00b7en\u00b7de\u00b7te", "Nest\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+-+-", "measure": "iambic.septa.invert"}, "line.3": {"text": "Zwitschert, und schlummert zum erstenmal wonniglich ein.", "tokens": ["Zwit\u00b7schert", ",", "und", "schlum\u00b7mert", "zum", "ers\u00b7ten\u00b7mal", "won\u00b7nig\u00b7lich", "ein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KON", "VVFIN", "APPRART", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Und du, mein Freund, ferne! ferne!", "tokens": ["Und", "du", ",", "mein", "Freund", ",", "fer\u00b7ne", "!", "fer\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPOSAT", "NN", "$,", "ADV", "$.", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00fcttle den Thau, wehender Nachthauch! ich schaure!", "tokens": ["Sch\u00fctt\u00b7le", "den", "Thau", ",", "we\u00b7hen\u00b7der", "Nacht\u00b7hauch", "!", "ich", "schau\u00b7re", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJA", "NN", "$.", "PPER", "VVFIN", "$."], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Sch\u00fcttl', ach sch\u00fcttl' ihn mir,", "tokens": ["Sch\u00fcttl'", ",", "ach", "sch\u00fcttl'", "ihn", "mir", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ITJ", "VVFIN", "PPER", "PPER", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Das ich senke diese reisende", "tokens": ["Das", "ich", "sen\u00b7ke", "die\u00b7se", "rei\u00b7sen\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "PPER", "VVFIN", "PDAT", "ADJA"], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Dem Herzen entquellende Z\u00e4hre", "tokens": ["Dem", "Her\u00b7zen", "ent\u00b7quel\u00b7len\u00b7de", "Z\u00e4h\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Auf der Viole ... Hat ers geh\u00f6rt?", "tokens": ["Auf", "der", "Vi\u00b7o\u00b7le", "...", "Hat", "ers", "ge\u00b7h\u00f6rt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$(", "VAFIN", "PIS", "VVFIN", "$."], "meter": "+-+---+-+", "measure": "unknown.measure.tetra"}, "line.7": {"text": "O des Z\u00e4rtlichen! er hats geh\u00f6rt!", "tokens": ["O", "des", "Z\u00e4rt\u00b7li\u00b7chen", "!", "er", "hats", "ge\u00b7h\u00f6rt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$.", "PPER", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Murmelt und sch\u00fcttelt \u2013 \u2013 Meine Thr\u00e4ne", "tokens": ["Mur\u00b7melt", "und", "sch\u00fct\u00b7telt", "\u2013", "\u2013", "Mei\u00b7ne", "Thr\u00e4\u00b7ne"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word"], "pos": ["NE", "KON", "VVFIN", "$(", "$(", "PPOSAT", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Gleitet sachte die Wang' herab!", "tokens": ["Glei\u00b7tet", "sach\u00b7te", "die", "Wang'", "her\u00b7ab", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Ach kein M\u00e4dchen! kein Freund!", "tokens": ["Ach", "kein", "M\u00e4d\u00b7chen", "!", "kein", "Freund", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "PIAT", "NN", "$.", "PIAT", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Kein Z\u00e4rtliches, Z\u00e4rtlicher ach!", "tokens": ["Kein", "Z\u00e4rt\u00b7li\u00b7ches", ",", "Z\u00e4rt\u00b7li\u00b7cher", "ach", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "NN", "ITJ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Der ich sie breche,", "tokens": ["Der", "ich", "sie", "bre\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Dem ich sie gebe,", "tokens": ["Dem", "ich", "sie", "ge\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Diese dir thr\u00e4nenbethaute Viole!", "tokens": ["Die\u00b7se", "dir", "thr\u00e4\u00b7nen\u00b7be\u00b7thau\u00b7te", "Vi\u00b7o\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "PPER", "VVFIN", "NN", "$."], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Und so mu\u00df sie einsam welken,", "tokens": ["Und", "so", "mu\u00df", "sie", "ein\u00b7sam", "wel\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So geschm\u00fcckt mit meiner Wehmut", "tokens": ["So", "ge\u00b7schm\u00fcckt", "mit", "mei\u00b7ner", "Weh\u00b7mut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sterben, unbetrauret, ungeliebet, ach!", "tokens": ["Ster\u00b7ben", ",", "un\u00b7be\u00b7trau\u00b7ret", ",", "un\u00b7ge\u00b7lie\u00b7bet", ",", "ach", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "ADJD", "$,", "ITJ", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.5": {"line.1": {"text": "Mag sie doch \u2013 sinken, liegen, im Winde verstieben!", "tokens": ["Mag", "sie", "doch", "\u2013", "sin\u00b7ken", ",", "lie\u00b7gen", ",", "im", "Win\u00b7de", "ver\u00b7stie\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "$(", "VVINF", "$,", "VVFIN", "$,", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Meine Wehmut mit ihr!", "tokens": ["Mei\u00b7ne", "Weh\u00b7mut", "mit", "ihr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Bist du doch gl\u00fccklich, Geliebter!", "tokens": ["Bist", "du", "doch", "gl\u00fcck\u00b7lich", ",", "Ge\u00b7lieb\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$,", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Geliebt am Herzen derer, die meine Seele liebt!", "tokens": ["Ge\u00b7liebt", "am", "Her\u00b7zen", "de\u00b7rer", ",", "die", "mei\u00b7ne", "See\u00b7le", "liebt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "PDS", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Ha! die\u00df wilde, pochende,", "tokens": ["Ha", "!", "die\u00df", "wil\u00b7de", ",", "po\u00b7chen\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ITJ", "$.", "PDS", "ADJA", "$,", "ADJA", "$,"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Die\u00df unaufhaltsamfliegende,", "tokens": ["Die\u00df", "un\u00b7auf\u00b7hal\u00b7tsam\u00b7flie\u00b7gen\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["PDS", "ADJA", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Die\u00df \u00e4ngstlichtragende, mitf\u00fchlende Herz!", "tokens": ["Die\u00df", "\u00e4ngst\u00b7lich\u00b7tra\u00b7gen\u00b7de", ",", "mit\u00b7f\u00fch\u00b7len\u00b7de", "Herz", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+---+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Das, ungl\u00fcckselig ewig,", "tokens": ["Das", ",", "un\u00b7gl\u00fcck\u00b7se\u00b7lig", "e\u00b7wig", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PDS", "$,", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Barbarisch immer aufnimmt und tr\u00e4gt!", "tokens": ["Bar\u00b7ba\u00b7risch", "im\u00b7mer", "auf\u00b7nimmt", "und", "tr\u00e4gt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "KON", "VVFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "Wie's dr\u00e4ngt! wie's tobt! dir vorw\u00e4rts nacheilt,", "tokens": ["Wie's", "dr\u00e4ngt", "!", "wie's", "tobt", "!", "dir", "vor\u00b7w\u00e4rts", "na\u00b7ch\u00b7eilt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "NN", "VVFIN", "$.", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und mich peinigt und qu\u00e4lt,", "tokens": ["Und", "mich", "pei\u00b7nigt", "und", "qu\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Und meine Sinnen zerr\u00fcttet,", "tokens": ["Und", "mei\u00b7ne", "Sin\u00b7nen", "zer\u00b7r\u00fct\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Und mir die Nerven zerrei\u00dft!", "tokens": ["Und", "mir", "die", "Ner\u00b7ven", "zer\u00b7rei\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "Wachst zu Einem Freund', ihr Freunde!", "tokens": ["Wachst", "zu", "Ei\u00b7nem", "Freund'", ",", "ihr", "Freun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach Seligkeit des Himmels", "tokens": ["Ach", "Se\u00b7lig\u00b7keit", "des", "Him\u00b7mels"], "token_info": ["word", "word", "word", "word"], "pos": ["ITJ", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Tr\u00e4ufelt nieder dem,", "tokens": ["Tr\u00e4u\u00b7felt", "nie\u00b7der", "dem", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "ART", "$,"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.4": {"text": "Der des Geliebten Busen umschlingt!", "tokens": ["Der", "des", "Ge\u00b7lieb\u00b7ten", "Bu\u00b7sen", "um\u00b7schlingt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "O ich weine, da du, Trunkner,", "tokens": ["O", "ich", "wei\u00b7ne", ",", "da", "du", ",", "Trunk\u00b7ner", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "PPER", "VVFIN", "$,", "KOUS", "PPER", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da du, Seliger,", "tokens": ["Da", "du", ",", "Se\u00b7li\u00b7ger", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "An Leopold Stolbergs Busen dich kn\u00fcpfst!", "tokens": ["An", "Leo\u00b7pold", "Stol\u00b7bergs", "Bu\u00b7sen", "dich", "kn\u00fcpfst", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.8": {"line.1": {"text": "Genie\u00dfe! und gedenke meiner!", "tokens": ["Ge\u00b7nie\u00b7\u00dfe", "!", "und", "ge\u00b7den\u00b7ke", "mei\u00b7ner", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KON", "VVFIN", "PPOSAT", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ha! gedenke meiner, wenn du thr\u00e4nenschauernd", "tokens": ["Ha", "!", "ge\u00b7den\u00b7ke", "mei\u00b7ner", ",", "wenn", "du", "thr\u00e4\u00b7nen\u00b7schau\u00b7ernd"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "VVFIN", "PPOSAT", "$,", "KOUS", "PPER", "VVPP"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Unter der Liebe F\u00fclle versinkst,", "tokens": ["Un\u00b7ter", "der", "Lie\u00b7be", "F\u00fcl\u00b7le", "ver\u00b7sinkst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Und du am Herzen liegest dem \u2013", "tokens": ["Und", "du", "am", "Her\u00b7zen", "lie\u00b7gest", "dem", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "VVFIN", "ART", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O wie soll ich ihn nennen!", "tokens": ["O", "wie", "soll", "ich", "ihn", "nen\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAV", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+---+-", "measure": "dactylic.init"}, "line.6": {"text": "Vater! Freund! Vater!", "tokens": ["Va\u00b7ter", "!", "Freund", "!", "Va\u00b7ter", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Klopstock! Klopstock! Ihm!", "tokens": ["Klops\u00b7tock", "!", "Klops\u00b7tock", "!", "Ihm", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$.", "NN", "$.", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Wenn du an ihm h\u00e4ngst,", "tokens": ["Wenn", "du", "an", "ihm", "h\u00e4ngst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Und herzerdr\u00fcckend und malmend", "tokens": ["Und", "her\u00b7zer\u00b7dr\u00fc\u00b7ckend", "und", "mal\u00b7mend"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVPP", "KON", "ADJD"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ueber dir die Wonne liegt,", "tokens": ["Ue\u00b7ber", "dir", "die", "Won\u00b7ne", "liegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "O dann reisse dich auf, athm' und schaure,", "tokens": ["O", "dann", "reis\u00b7se", "dich", "auf", ",", "athm'", "und", "schau\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VVFIN", "PPER", "PTKVZ", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und gedenke deines Einsamen hier;", "tokens": ["Und", "ge\u00b7den\u00b7ke", "dei\u00b7nes", "Ein\u00b7sa\u00b7men", "hier", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Und, indem du noch einmal", "tokens": ["Und", ",", "in\u00b7dem", "du", "noch", "ein\u00b7mal"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "PPER", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Die geliebte Stirn dr\u00fcckst,", "to