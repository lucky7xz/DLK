{"textgrid.poem.62083": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "Der stolze Heinrich", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zartes Bl\u00fcmlein wunderhold,", "tokens": ["Zar\u00b7tes", "Bl\u00fcm\u00b7lein", "wun\u00b7der\u00b7hold", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Zogest aus der Gartenwelt,", "tokens": ["Zo\u00b7gest", "aus", "der", "Gar\u00b7ten\u00b7welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In das freie, offne Feld,", "tokens": ["In", "das", "frei\u00b7e", ",", "off\u00b7ne", "Feld", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Helles Bl\u00fcmlein, duftend Gold!", "tokens": ["Hel\u00b7les", "Bl\u00fcm\u00b7lein", ",", "duf\u00b7tend", "Gold", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Liebtest's nicht, im engen Raum", "tokens": ["Lieb\u00b7test's", "nicht", ",", "im", "en\u00b7gen", "Raum"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PTKNEG", "$,", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Einzeln, m\u00fc\u00dfig dazustehen,", "tokens": ["Ein\u00b7zeln", ",", "m\u00fc\u00b7\u00dfig", "da\u00b7zu\u00b7ste\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "PAV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Steif und nutzlos auszusehen", "tokens": ["Steif", "und", "nutz\u00b7los", "aus\u00b7zu\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ein stilles Bild im Traum! \u2013", "tokens": ["Wie", "ein", "stil\u00b7les", "Bild", "im", "Traum", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPRART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}