{"textgrid.poem.46413": {"metadata": {"author": {"name": "Ewald, Johann Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Philindens Ku\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1744, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O welchen Ku\u00df empfing ich von Philinden", "tokens": ["O", "wel\u00b7chen", "Ku\u00df", "emp\u00b7fing", "ich", "von", "Phi\u00b7lin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PWAT", "NN", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O Ku\u00df, o Lust, in der ich mich verlohr!", "tokens": ["O", "Ku\u00df", ",", "o", "Lust", ",", "in", "der", "ich", "mich", "ver\u00b7lohr", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "NN", "$,", "APPR", "PRELS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Solt' ich so gro\u00dfen Schmertz o Himmel, einst empfinden!", "tokens": ["Solt'", "ich", "so", "gro\u00b7\u00dfen", "Schmertz", "o", "Him\u00b7mel", ",", "einst", "emp\u00b7fin\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJA", "NN", "FM", "NN", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So t\u00f6dte mich zuvor!", "tokens": ["So", "t\u00f6d\u00b7te", "mich", "zu\u00b7vor", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}