{"textgrid.poem.49836": {"metadata": {"author": {"name": "Sachs, Hans", "birth": "N.A.", "death": "N.A."}, "title": "Fabel: Der vogel Cassita mit sein jungen", "genre": "verse", "period": "N.A.", "pub_year": 1562, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Doctor Sebastianus Brant", "tokens": ["Doc\u00b7tor", "Se\u00b7bas\u00b7ti\u00b7a\u00b7nus", "Brant"], "token_info": ["word", "word", "word"], "pos": ["NN", "NE", "NE"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "der macht ein fabel uns bekant", "tokens": ["der", "macht", "ein", "fa\u00b7bel", "uns", "be\u00b7kant"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "VVFIN", "ART", "NN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "vom vogel Cassita mit namen;", "tokens": ["vom", "vo\u00b7gel", "Cas\u00b7si\u00b7ta", "mit", "na\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NE", "NE", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "der nistet in des treides samen,", "tokens": ["der", "nis\u00b7tet", "in", "des", "trei\u00b7des", "sa\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "APPR", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "darin junge aufziehen tet,", "tokens": ["da\u00b7rin", "jun\u00b7ge", "auf\u00b7zie\u00b7hen", "tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJA", "VVINF", "VVFIN", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.6": {"text": "sein narung von der fr\u00fcchte het.", "tokens": ["sein", "na\u00b7rung", "von", "der", "fr\u00fcch\u00b7te", "het", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "als zeit der ernte gieng herein,", "tokens": ["als", "zeit", "der", "ern\u00b7te", "gieng", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "ADJA", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "und sich ferbet das treit gemein,", "tokens": ["und", "sich", "fer\u00b7bet", "das", "treit", "ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.9": {"text": "wolt diser vogel obgemelt", "tokens": ["wolt", "di\u00b7ser", "vo\u00b7gel", "ob\u00b7ge\u00b7melt"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PDAT", "NN", "VVPP"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.10": {"text": "hinaus fliegen in weites felt", "tokens": ["hin\u00b7aus", "flie\u00b7gen", "in", "wei\u00b7tes", "felt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APZR", "VVFIN", "APPR", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.11": {"text": "und lie\u00df die jungen in dem nest", "tokens": ["und", "lie\u00df", "die", "jun\u00b7gen", "in", "dem", "nest"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "und befalch in aufs aller best,", "tokens": ["und", "be\u00b7falch", "in", "aufs", "al\u00b7ler", "best", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "APPRART", "ADJA", "VVFIN", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.13": {"text": "aufzumerken an diser stet,", "tokens": ["auf\u00b7zu\u00b7mer\u00b7ken", "an", "di\u00b7ser", "stet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PDS", "VVFIN", "$,"], "meter": "+----+-+", "measure": "dactylic.init"}, "line.14": {"text": "was da w\u00fcrt ghandelt und geret", "tokens": ["was", "da", "w\u00fcrt", "ghan\u00b7delt", "und", "ge\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "VAFIN", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "dieweil bi\u00df das er wider kem.", "tokens": ["die\u00b7weil", "bi\u00df", "das", "er", "wi\u00b7der", "kem", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PRELS", "PPER", "APPR", "PIS", "$."], "meter": "--+--+--", "measure": "anapaest.di.plus"}, "line.16": {"text": "also schiet er von in. nach dem", "tokens": ["al\u00b7so", "schiet", "er", "von", "in", ".", "nach", "dem"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "APPR", "$.", "APPR", "ART"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "kam der bauer mit seinem son", "tokens": ["kam", "der", "bau\u00b7er", "mit", "sei\u00b7nem", "son"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "XY"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.18": {"text": "und sprach: wir m\u00fc\u00dfen schneiden lon,", "tokens": ["und", "sprach", ":", "wir", "m\u00fc\u00b7\u00dfen", "schnei\u00b7den", "lon", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPER", "VMFIN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "das korn ist reif in beten allen;", "tokens": ["das", "korn", "ist", "reif", "in", "be\u00b7ten", "al\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "APPR", "VVFIN", "PIS", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "sonst w\u00fcrt es zu d\u00fcrr und ausfallen.", "tokens": ["sonst", "w\u00fcrt", "es", "zu", "d\u00fcrr", "und", "aus\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKA", "ADJD", "KON", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.21": {"text": "ge hin und unsern nachbaurn sag,", "tokens": ["ge", "hin", "und", "un\u00b7sern", "nach\u00b7baurn", "sag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "das sie kommen morgen vor tag", "tokens": ["das", "sie", "kom\u00b7men", "mor\u00b7gen", "vor", "tag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PPER", "VVFIN", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "und uns einschneiden unser treid.", "tokens": ["und", "uns", "ein\u00b7schnei\u00b7den", "un\u00b7ser", "treid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PIS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "nach dem abschiden sie all beid.", "tokens": ["nach", "dem", "ab\u00b7schi\u00b7den", "sie", "all", "beid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "PPER", "PIAT", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.25": {"text": "des warn die jungen v\u00f6gl forchtsam.", "tokens": ["des", "warn", "die", "jun\u00b7gen", "v\u00f6gl", "forcht\u00b7sam", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.26": {"text": "nach dem die alt geflogen kam,", "tokens": ["nach", "dem", "die", "alt", "ge\u00b7flo\u00b7gen", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJD", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "die jungen sagten b\u00f6se mer,", "tokens": ["die", "jun\u00b7gen", "sag\u00b7ten", "b\u00f6\u00b7se", "mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "wie beim acker gewesen wer", "tokens": ["wie", "beim", "ac\u00b7ker", "ge\u00b7we\u00b7sen", "wer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "VAPP", "PWS"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.29": {"text": "der baur, seim son befolhen het,", "tokens": ["der", "baur", ",", "seim", "son", "be\u00b7fol\u00b7hen", "het", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "XY", "XY", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "das er sein nachbaurn brufen tet", "tokens": ["das", "er", "sein", "nach\u00b7baurn", "bru\u00b7fen", "tet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRELS", "PPER", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.31": {"text": "auf morgen, abzuschneidn sein korn:", "tokens": ["auf", "mor\u00b7gen", ",", "ab\u00b7zu\u00b7schneidn", "sein", "korn", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "$,", "VVIZU", "PPOSAT", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.32": {"text": "des sein wir hart bek\u00fcmmert worn.", "tokens": ["des", "sein", "wir", "hart", "be\u00b7k\u00fcm\u00b7mert", "worn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "PPER", "ADJD", "VVPP", "VAPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "die alt sprach: lieben, f\u00f6rcht euch nicht,", "tokens": ["die", "alt", "sprach", ":", "lie\u00b7ben", ",", "f\u00f6rcht", "euch", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "$.", "VVINF", "$,", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "das schneidn auf morgen nit geschicht.", "tokens": ["das", "schneidn", "auf", "mor\u00b7gen", "nit", "ge\u00b7schicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "des andern tages gleicher weis", "tokens": ["des", "an\u00b7dern", "ta\u00b7ges", "glei\u00b7cher", "weis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "flog sie aus, zu samlen die speis.", "tokens": ["flog", "sie", "aus", ",", "zu", "sam\u00b7len", "die", "speis", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "PTKZU", "VVINF", "ART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.37": {"text": "der baur mit seim son wider kam", "tokens": ["der", "baur", "mit", "seim", "son", "wi\u00b7der", "kam"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "XY", "XY", "APPR", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "und sprach: wie gar mit schand und scham", "tokens": ["und", "sprach", ":", "wie", "gar", "mit", "schand", "und", "scham"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "PWAV", "ADV", "APPR", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "haben mich mein nachbaurn verla\u00dfen,", "tokens": ["ha\u00b7ben", "mich", "mein", "nach\u00b7baurn", "ver\u00b7la\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.40": {"text": "den ich vil guts tet \u00fcberma\u00dfen!", "tokens": ["den", "ich", "vil", "guts", "tet", "\u00fc\u00b7berm\u00b7a\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PIAT", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "drumb ge zu den blutfreunden mein,", "tokens": ["drumb", "ge", "zu", "den", "blut\u00b7freun\u00b7den", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NE", "APPR", "ART", "ADJA", "PPOSAT", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.42": {"text": "die in dem nechsten dorfe sein,", "tokens": ["die", "in", "dem", "nechs\u00b7ten", "dor\u00b7fe", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "sag zu in: komt auf morgen fru,", "tokens": ["sag", "zu", "in", ":", "komt", "auf", "mor\u00b7gen", "fru", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPR", "$.", "VVFIN", "APPR", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "das man das korn einernten tu,", "tokens": ["das", "man", "das", "korn", "ei\u00b7nern\u00b7ten", "tu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ART", "NN", "VVFIN", "NE", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.45": {"text": "wan es ist zeitig \u00fcberaus.", "tokens": ["wan", "es", "ist", "zei\u00b7tig", "\u00fc\u00b7be\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "ADJD", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.46": {"text": "nach dem giengen sie beid zu haus.", "tokens": ["nach", "dem", "gien\u00b7gen", "sie", "beid", "zu", "haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVFIN", "PPER", "PIS", "PTKZU", "PTKVZ", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.47": {"text": "die alt die kam geflogen wider", "tokens": ["die", "alt", "die", "kam", "ge\u00b7flo\u00b7gen", "wi\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ART", "VVFIN", "VVPP", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.48": {"text": "und lie\u00df sich zu den jungen nider;", "tokens": ["und", "lie\u00df", "sich", "zu", "den", "jun\u00b7gen", "ni\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.49": {"text": "die sagten, was befolhen war", "tokens": ["die", "sag\u00b7ten", ",", "was", "be\u00b7fol\u00b7hen", "war"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "VVFIN", "$,", "PRELS", "VVPP", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "vom bauren seiner blutfreunt schar.", "tokens": ["vom", "bau\u00b7ren", "sei\u00b7ner", "blut\u00b7freunt", "schar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "VVFIN", "PPOSAT", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "die mutter sprach: seit auch on sorgen,", "tokens": ["die", "mut\u00b7ter", "sprach", ":", "seit", "auch", "on", "sor\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "APPR", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.52": {"text": "die freunt kommen auch nit auf morgen,", "tokens": ["die", "freunt", "kom\u00b7men", "auch", "nit", "auf", "mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "ADV", "PTKNEG", "APPR", "ADV", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.53": {"text": "das korn ein zu schneiden umbsunst,", "tokens": ["das", "korn", "ein", "zu", "schnei\u00b7den", "umbs\u00b7unst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "--+-+---", "measure": "anapaest.init"}, "line.54": {"text": "wan schmal und ring ist lieb und gunst", "tokens": ["wan", "schmal", "und", "ring", "ist", "lieb", "und", "gunst"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "KON", "VVFIN", "VAFIN", "ADJD", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.55": {"text": "bei blutfreunden, drumb seit zu ru.", "tokens": ["bei", "blut\u00b7freun\u00b7den", ",", "drumb", "seit", "zu", "ru", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "PAV", "ADV", "APPR", "NE", "$."], "meter": "+-+--+-