er": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der geschehe allezeit.", "tokens": ["Der", "ge\u00b7sche\u00b7he", "al\u00b7le\u00b7zeit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Mit Gebeth nahmst du das Gute", "tokens": ["Mit", "Ge\u00b7beth", "nahmst", "du", "das", "Gu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Von der Hand des Herren an;", "tokens": ["Von", "der", "Hand", "des", "Her\u00b7ren", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.9": {"text": "Und gleich also seine Ruthe,", "tokens": ["Und", "gleich", "al\u00b7so", "sei\u00b7ne", "Ru\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Ob sie dir schon weh gethan.", "tokens": ["Ob", "sie", "dir", "schon", "weh", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Dein so langes Todten-Lager,", "tokens": ["Dein", "so", "lan\u00b7ges", "Tod\u00b7ten\u00b7La\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Machte deinen Leib zwar mager;", "tokens": ["Mach\u00b7te", "dei\u00b7nen", "Leib", "zwar", "ma\u00b7ger", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber deine Seele stark.", "tokens": ["A\u00b7ber", "dei\u00b7ne", "See\u00b7le", "stark", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Stille hast du die\u00df erlitten,", "tokens": ["Stil\u00b7le", "hast", "du", "die\u00df", "er\u00b7lit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PDS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Tapfer, scharf und gut gestritten,", "tokens": ["Tap\u00b7fer", ",", "scharf", "und", "gut", "ge\u00b7strit\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ob sich Gott wohl oft verbarg.", "tokens": ["Ob", "sich", "Gott", "wohl", "oft", "ver\u00b7barg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "NN", "ADV", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Hier hast du mit Gott gek\u00e4mpfet,", "tokens": ["Hier", "hast", "du", "mit", "Gott", "ge\u00b7k\u00e4mp\u00b7fet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Und ihn gl\u00fccklich obgesiegt:", "tokens": ["Und", "ihn", "gl\u00fcck\u00b7lich", "ob\u00b7ge\u00b7siegt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Auch dadurch den Feind ged\u00e4mpfet,", "tokens": ["Auch", "da\u00b7durch", "den", "Feind", "ge\u00b7d\u00e4mp\u00b7fet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Der die ganze Welt bekriegt.", "tokens": ["Der", "die", "gan\u00b7ze", "Welt", "be\u00b7kriegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Nun auf dein so sehnlich Bitten,", "tokens": ["Nun", "auf", "dein", "so", "sehn\u00b7lich", "Bit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADV", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hohlt dich Gott aus Kedars-H\u00fctten,", "tokens": ["Hohlt", "dich", "Gott", "aus", "Ke\u00b7dar\u00b7sH\u00fct\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und aus Mesechs M\u00f6rder-Nest.", "tokens": ["Und", "aus", "Me\u00b7sechs", "M\u00f6r\u00b7der\u00b7Nest", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Er bringt dich nach seinen Worten,", "tokens": ["Er", "bringt", "dich", "nach", "sei\u00b7nen", "Wor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Zu den g\u00fcldnen Salems-Pforten,", "tokens": ["Zu", "den", "g\u00fcld\u00b7nen", "Sa\u00b7lems\u00b7P\u00b7for\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zu den grossen Freuden-Fest.", "tokens": ["Zu", "den", "gros\u00b7sen", "Freu\u00b7den\u00b7Fest", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sch\u00f6ner Tausch, den du getroffen!", "tokens": ["Sch\u00f6\u00b7ner", "Tausch", ",", "den", "du", "ge\u00b7trof\u00b7fen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Gottes grosse Herrlichkeit,", "tokens": ["Got\u00b7tes", "gros\u00b7se", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Und der Himmel steht dir offen,", "tokens": ["Und", "der", "Him\u00b7mel", "steht", "dir", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Der dir deine Krone beut.", "tokens": ["Der", "dir", "dei\u00b7ne", "Kro\u00b7ne", "beut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Wenn wir dir mit vielen Klagen,", "tokens": ["Wenn", "wir", "dir", "mit", "vie\u00b7len", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gute Nacht jetzt m\u00fcssen sagen,", "tokens": ["Gu\u00b7te", "