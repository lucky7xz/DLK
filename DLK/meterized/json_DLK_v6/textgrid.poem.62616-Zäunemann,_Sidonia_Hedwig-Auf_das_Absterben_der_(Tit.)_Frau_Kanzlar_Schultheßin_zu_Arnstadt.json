{"textgrid.poem.62616": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Auf das Absterben der (Tit.) Frau Kanzlar Schulthe\u00dfin zu Arnstadt", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach! wie ist mir jetzt zu muthe?", "tokens": ["Ach", "!", "wie", "ist", "mir", "jetzt", "zu", "mu\u00b7the", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "VAFIN", "PPER", "ADV", "PTKZU", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine Seele schwimmt im Blute!", "tokens": ["Mei\u00b7ne", "See\u00b7le", "schwimmt", "im", "Blu\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Meiner Adern Purpur kocht;", "tokens": ["Mei\u00b7ner", "A\u00b7dern", "Pur\u00b7pur", "kocht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine matten F\u00fcsse zittern;", "tokens": ["Mei\u00b7ne", "mat\u00b7ten", "F\u00fcs\u00b7se", "zit\u00b7tern", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich f\u00fchl in mir ein Ersch\u00fcttern,", "tokens": ["Ich", "f\u00fchl", "in", "mir", "ein", "Er\u00b7sch\u00fct\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und mein Herz ganz \u00e4ngstlich pocht.", "tokens": ["Und", "mein", "Herz", "ganz", "\u00e4ngst\u00b7lich", "pocht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Aus den schwachen Augen quillet", "tokens": ["Aus", "den", "schwa\u00b7chen", "Au\u00b7gen", "quil\u00b7let"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Eine hei\u00dfe Thr\u00e4nen-Fluth.", "tokens": ["Ei\u00b7ne", "hei\u00b7\u00dfe", "Thr\u00e4\u00b7nen\u00b7Fluth", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Was hat mich in Boy verh\u00fcllet?", "tokens": ["Was", "hat", "mich", "in", "Boy", "ver\u00b7h\u00fcl\u00b7let", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "NE", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Was betr\u00fcbet meinen Muth?", "tokens": ["Was", "be\u00b7tr\u00fc\u00b7bet", "mei\u00b7nen", "Muth", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Mich versetzt in dieses Leiden", "tokens": ["Mich", "ver\u00b7setzt", "in", "die\u00b7ses", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meiner liebsten Schwester Scheiden,", "tokens": ["Mei\u00b7ner", "liebs\u00b7ten", "Schwes\u00b7ter", "Schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ihr Grab und Leichenstein.", "tokens": ["Und", "ihr", "Grab", "und", "Lei\u00b7chen\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Meiner Schwester Abschied nehmen,", "tokens": ["Mei\u00b7ner", "Schwes\u00b7ter", "Ab\u00b7schied", "neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Setzet mich in dieses Gr\u00e4men,", "tokens": ["Set\u00b7zet", "mich", "in", "die\u00b7ses", "Gr\u00e4\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und so bittre Seelen-Pein.", "tokens": ["Und", "so", "bitt\u00b7re", "See\u00b7len\u00b7Pein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Das Verh\u00e4ngni\u00df schneidt die Kette", "tokens": ["Das", "Ver\u00b7h\u00e4ng\u00b7ni\u00df", "schneidt", "die", "Ket\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Unsrer Eintracht ganz entwey.", "tokens": ["Uns\u00b7rer", "Ein\u00b7tracht", "ganz", "ent\u00b7wey", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Schwester! wenn ich dich noch h\u00e4tte,", "tokens": ["Schwes\u00b7ter", "!", "wenn", "ich", "dich", "noch", "h\u00e4t\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KOUS", "PPER", "PRF", "ADV", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Gienge dieser Schmerz vorbey.", "tokens": ["Gien\u00b7ge", "die\u00b7ser", "Schmerz", "vor\u00b7bey", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Meine Schwester, die mich liebte,", "tokens": ["Mei\u00b7ne", "Schwes\u00b7ter", ",", "die", "mich", "lieb\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die mich niemahls nicht betr\u00fcbte;", "tokens": ["Die", "mich", "nie\u00b7mahls", "nicht", "be\u00b7tr\u00fcb\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Als nur jetzt, durch ihren Tod;", "tokens": ["Als", "nur", "jetzt", ",", "durch", "ih\u00b7ren", "Tod", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die mir ihr ergebnes Herze", "tokens": ["Die", "mir", "ihr", "er\u00b7geb\u00b7nes", "Her\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In der Freude, in dem Schmerze", "tokens": ["In", "der", "Freu\u00b7de", ",", "in", "dem", "Schmer\u00b7ze"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Allzeit willigst anerboth:", "tokens": ["All\u00b7zeit", "wil\u00b7ligst", "a\u00b7ner\u00b7both", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Der mu\u00df ich jetzt Thr\u00e4nen weyhen,", "tokens": ["Der", "mu\u00df", "ich", "jetzt", "Thr\u00e4\u00b7nen", "wey\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und auf ihre Ruhestatt", "tokens": ["Und", "auf", "ih\u00b7re", "Ru\u00b7he\u00b7statt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Traurige Cypressen streuen,", "tokens": ["Trau\u00b7ri\u00b7ge", "Cyp\u00b7res\u00b7sen", "streu\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.10": {"text": "Die die Hand gar reichlich hat.", "tokens": ["Die", "die", "Hand", "gar", "reich\u00b7lich", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Meine Freude war ihr Lachen,", "tokens": ["Mei\u00b7ne", "Freu\u00b7de", "war", "ihr", "La\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schlief ich nicht, so blieb sie wachen,", "tokens": ["Schlief", "ich", "nicht", ",", "so", "blieb", "sie", "wa\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "ADV", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mein Betr\u00fcbni\u00df war ihr Leid.", "tokens": ["Mein", "Be\u00b7tr\u00fcb\u00b7ni\u00df", "war", "ihr", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Niemahls haben wir gestritten", "tokens": ["Nie\u00b7mahls", "ha\u00b7ben", "wir", "ge\u00b7strit\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Unsern Werken, Thun und Schritten", "tokens": ["Un\u00b7sern", "Wer\u00b7ken", ",", "Thun", "und", "Schrit\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Folgte Fried und Einigkeit.", "tokens": ["Folg\u00b7te", "Fried", "und", "Ei\u00b7nig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Keine Schwester, eine Seele", "tokens": ["Kei\u00b7ne", "Schwes\u00b7ter", ",", "ei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die der fr\u00f6mmsten Mutter gleich,", "tokens": ["Die", "der", "fr\u00f6mms\u00b7ten", "Mut\u00b7ter", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Bring ich jetzt zur Grabes-H\u00f6hle,", "tokens": ["Bring", "ich", "jetzt", "zur", "Gra\u00b7bes\u00b7H\u00f6h\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "In das finstre Todten-Reich.", "tokens": ["In", "das", "finst\u00b7re", "Tod\u00b7ten\u00b7Reich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Solte dieses mich nicht kr\u00e4nken?", "tokens": ["Sol\u00b7te", "die\u00b7ses", "mich", "nicht", "kr\u00e4n\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDAT", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wer wird mich darum verdenken,", "tokens": ["Wer", "wird", "mich", "da\u00b7rum", "ver\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PRF", "PAV", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wenn mein Auge heftig weint?", "tokens": ["Wenn", "mein", "Au\u00b7ge", "hef\u00b7tig", "weint", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00dft mich seufzen! la\u00dft mich sehnen!", "tokens": ["La\u00dft", "mich", "seuf\u00b7zen", "!", "la\u00dft", "mich", "seh\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$.", "VVIMP", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich vergiesse wahre Thr\u00e4nen,", "tokens": ["Ich", "ver\u00b7gies\u00b7se", "wah\u00b7re", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Um den allerliebsten Freund.", "tokens": ["Um", "den", "al\u00b7ler\u00b7liebs\u00b7ten", "Freund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Deine Tugend, dein Gem\u00fcthe", "tokens": ["Dei\u00b7ne", "Tu\u00b7gend", ",", "dein", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Hat dergleichen Zoll verdient.", "tokens": ["Hat", "derg\u00b7lei\u00b7chen", "Zoll", "ver\u00b7dient", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Das Ged\u00e4chtni\u00df deiner G\u00fcte,", "tokens": ["Das", "Ge\u00b7d\u00e4cht\u00b7ni\u00df", "dei\u00b7ner", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Auch durch heise Thr\u00e4nen gr\u00fcnt.", "tokens": ["Auch", "durch", "hei\u00b7se", "Thr\u00e4\u00b7nen", "gr\u00fcnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Jedermann wird deinem Leben", "tokens": ["Je\u00b7der\u00b7mann", "wird", "dei\u00b7nem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das warhaftge Zeugni\u00df geben,", "tokens": ["Das", "war\u00b7haft\u00b7ge", "Zeug\u00b7ni\u00df", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da\u00df es voller Tugend war.", "tokens": ["Da\u00df", "es", "vol\u00b7ler", "Tu\u00b7gend", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Was wir von der Ruth gelesen,", "tokens": ["Was", "wir", "von", "der", "Ruth", "ge\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Das bezeugte stets dein Wesen", "tokens": ["Das", "be\u00b7zeug\u00b7te", "stets", "dein", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ohne Ruhm, und dennoch klar.", "tokens": ["Oh\u00b7ne", "Ruhm", ",", "und", "den\u00b7noch", "klar", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KON", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sanftmuth, Demuth, Huld und Liebe,", "tokens": ["Sanft\u00b7muth", ",", "De\u00b7muth", ",", "Huld", "und", "Lie\u00b7be", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.8": {"text": "Stiller Flei\u00df und Redlichkeit,", "tokens": ["Stil\u00b7ler", "Flei\u00df", "und", "Red\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Gottesfur