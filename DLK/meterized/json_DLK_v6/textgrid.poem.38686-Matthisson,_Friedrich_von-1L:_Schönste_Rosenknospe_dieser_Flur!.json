{"textgrid.poem.38686": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sch\u00f6nste Rosenknospe dieser Flur!", "genre": "verse", "period": "N.A.", "pub_year": 1792, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6nste Rosenknospe dieser Flur!", "tokens": ["Sch\u00f6ns\u00b7te", "Ro\u00b7sen\u00b7knos\u00b7pe", "die\u00b7ser", "Flur", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PDAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Unter Nachtigallgesang entbl\u00fche,", "tokens": ["Un\u00b7ter", "Nach\u00b7ti\u00b7gall\u00b7ge\u00b7sang", "ent\u00b7bl\u00fc\u00b7he", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Bei des goldnen Maitags leiser Fr\u00fche,", "tokens": ["Bei", "des", "gold\u00b7nen", "Mai\u00b7tags", "lei\u00b7ser", "Fr\u00fc\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Still im Schoo\u00dfe l\u00e4ndlicher Natur.", "tokens": ["Still", "im", "Schoo\u00b7\u00dfe", "l\u00e4nd\u00b7li\u00b7cher", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Sicher, gleich dem Hesperidenhain,", "tokens": ["Si\u00b7cher", ",", "gleich", "dem", "Hes\u00b7pe\u00b7ri\u00b7den\u00b7hain", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Ruhvoll, wie Arkadiens Gefilde,", "tokens": ["Ruh\u00b7voll", ",", "wie", "Ar\u00b7ka\u00b7di\u00b7ens", "Ge\u00b7fil\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "NE", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ein Elysium an Fr\u00fchlingsmilde,", "tokens": ["Ein", "E\u00b7ly\u00b7si\u00b7um", "an", "Fr\u00fch\u00b7lings\u00b7mil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "M\u00fcsse dein umschirmtes G\u00e4rtchen seyn.", "tokens": ["M\u00fcs\u00b7se", "dein", "um\u00b7schirm\u00b7tes", "G\u00e4rt\u00b7chen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "VAINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Nur von zarter Nymfen Hand ber\u00fchrt", "tokens": ["Nur", "von", "zar\u00b7ter", "Nym\u00b7fen", "Hand", "be\u00b7r\u00fchrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "NN", "VVPP"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Prang' empor in keuscher Jugendr\u00f6the,", "tokens": ["Prang'", "em\u00b7por", "in", "keu\u00b7scher", "Ju\u00b7gend\u00b7r\u00f6\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Bis auch dich dem heimatlichen Beete", "tokens": ["Bis", "auch", "dich", "dem", "hei\u00b7mat\u00b7li\u00b7chen", "Bee\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Edler Blumen Loos, o Hold', entf\u00fchrt.", "tokens": ["Ed\u00b7ler", "Blu\u00b7men", "Loos", ",", "o", "Hold'", ",", "ent\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "NE", "$,", "FM", "NN", "$,", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Wonne dann, im reinen G\u00f6tterlicht", "tokens": ["Won\u00b7ne", "dann", ",", "im", "rei\u00b7nen", "G\u00f6t\u00b7ter\u00b7licht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADV", "$,", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Sch\u00f6ner M\u00e4nnlichkeit, dem Erdensohne,", "tokens": ["Sch\u00f6\u00b7ner", "M\u00e4nn\u00b7lich\u00b7keit", ",", "dem", "Er\u00b7den\u00b7soh\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Der, umgr\u00fcnt von Amors Mirtenkrone,", "tokens": ["Der", ",", "um\u00b7gr\u00fcnt", "von", "A\u00b7mors", "Mir\u00b7ten\u00b7kro\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "VVPP", "APPR", "NE", "NE", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Dich den Grazien zum Opfer bricht!", "tokens": ["Dich", "den", "Gra\u00b7zi\u00b7en", "zum", "Op\u00b7fer", "bricht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}