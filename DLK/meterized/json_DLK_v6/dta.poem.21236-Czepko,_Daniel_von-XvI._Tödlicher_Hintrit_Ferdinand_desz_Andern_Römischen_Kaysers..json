{"dta.poem.21236": {"metadata": {"author": {"name": "Czepko, Daniel von", "birth": "N.A.", "death": "N.A."}, "title": "XvI.  \n  T\u00f6dlicher Hintrit  \n Ferdinand desz Andern  \n R\u00f6mischen Kaysers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20871-8", "language": ["de:0.99"], "booktitle": "Czepko, Daniel von: Triumph Bogen. Breslau, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Der Vater dieser Welt/ hat Sich zur Ruh gewand/", "tokens": ["Der", "Va\u00b7ter", "die\u00b7ser", "Welt", "/", "hat", "Sich", "zur", "Ruh", "ge\u00b7wand", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "$(", "VAFIN", "PRF", "APPRART", "NN", "VAPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die in dem Himmel ist/ der vor ", "tokens": ["Die", "in", "dem", "Him\u00b7mel", "ist", "/", "der", "vor"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "VAFIN", "$(", "ART", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jedoch eh ", "tokens": ["Je\u00b7doch", "eh"], "token_info": ["word", "word"], "pos": ["ADV", "KOUS"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Rufft ", "tokens": ["Rufft"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}}, "stanza.2": {"line.1": {"text": "Er wolte Deutschland ja nicht sonder Kayser sehn/", "tokens": ["Er", "wol\u00b7te", "Deutschland", "ja", "nicht", "son\u00b7der", "Kay\u00b7ser", "sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NE", "ADV", "PTKNEG", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da\u00df ", "tokens": ["Da\u00df"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "O Gottes-voller Sinn; Dann schawt nach diesen Gaben/", "tokens": ["O", "Got\u00b7tes\u00b7vol\u00b7ler", "Sinn", ";", "Dann", "schawt", "nach", "die\u00b7sen", "Ga\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "$.", "ADV", "VVFIN", "APPR", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hie\u00df ", "tokens": ["Hie\u00df"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}}, "stanza.3": {"line.1": {"text": "So starb ", "tokens": ["So", "starb"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Es war wie abgeredt mit Gott vnd mit den Erben;", "tokens": ["Es", "war", "wie", "ab\u00b7ge\u00b7redt", "mit", "Gott", "vnd", "mit", "den", "Er\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "VVPP", "APPR", "NN", "KON", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jedoch ", "tokens": ["Je\u00b7doch"], "token_info": ["word"], "pos": ["ADV"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}}}}}