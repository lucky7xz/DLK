{"textgrid.poem.31521": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du / aller Engel Kron / der Himmel Zier und Pracht /", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du / aller Engel Kron / der Himmel Zier und Pracht /", "tokens": ["Du", "/", "al\u00b7ler", "En\u00b7gel", "Kron", "/", "der", "Him\u00b7mel", "Zier", "und", "Pracht", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "PIAT", "NN", "NE", "$(", "ART", "NN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "von dessen wehrtem Haupt die Gottheit-Strahlen blitzen /", "tokens": ["von", "des\u00b7sen", "wehr\u00b7tem", "Haupt", "die", "Gott\u00b7heit\u00b7Strah\u00b7len", "blit\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "ADJA", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "l\u00e4sst mit der Dornen-Kron den Thron der Hoheit ritzen;", "tokens": ["l\u00e4sst", "mit", "der", "Dor\u00b7nen\u00b7Kron", "den", "Thron", "der", "Ho\u00b7heit", "rit\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.4": {"text": "vom Tyger-drucken dir ein jede Ader kracht.", "tokens": ["vom", "Ty\u00b7ger\u00b7dru\u00b7cken", "dir", "ein", "je\u00b7de", "A\u00b7der", "kracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wol! uns ein jeder Dorn ein Lebens-Quell aufmacht /", "tokens": ["Wol", "!", "uns", "ein", "je\u00b7der", "Dorn", "ein", "Le\u00b7bens\u00b7Quell", "auf\u00b7macht", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "ART", "PIAT", "NN", "ART", "NN", "VVPP", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.6": {"text": "aus der Erl\u00f6sungs Safft / die Blut-Rubinen / spritzen.", "tokens": ["aus", "der", "Er\u00b7l\u00f6\u00b7sungs", "Safft", "/", "die", "Blut\u00b7Ru\u00b7bi\u00b7nen", "/", "sprit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$(", "ART", "NN", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es h\u00e4ngt mein' Himmels-Kron an diesen Stachelspitzen /", "tokens": ["Es", "h\u00e4ngt", "mein'", "Him\u00b7mels\u00b7Kron", "an", "die\u00b7sen", "Sta\u00b7chel\u00b7spit\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "die du erworben hast in dieser Leidens-Schlacht.", "tokens": ["die", "du", "er\u00b7wor\u00b7ben", "hast", "in", "die\u00b7ser", "Lei\u00b7dens\u00b7Schlacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "VAFIN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sie solten in mein Herz / nit in sein Haupt / seyn gangen!", "tokens": ["Sie", "sol\u00b7ten", "in", "mein", "Herz", "/", "nit", "in", "sein", "Haupt", "/", "seyn", "gan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "$(", "PTKNEG", "APPR", "PPOSAT", "NN", "$(", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "doch nein: mein-Heil geschick / und deine Liebe / must", "tokens": ["doch", "nein", ":", "mein\u00b7Heil", "ge\u00b7schick", "/", "und", "dei\u00b7ne", "Lie\u00b7be", "/", "must"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADV", "PTKANT", "$.", "PPOSAT", "NN", "$(", "KON", "PPOSAT", "NN", "$(", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "durch diesen D\u00f6rner-Weg zu ihrem Ziel gelangen.", "tokens": ["durch", "die\u00b7sen", "D\u00f6r\u00b7ner\u00b7Weg", "zu", "ih\u00b7rem", "Ziel", "ge\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Du dultest Stachel-Ri\u00df / erwirbst mir Rosen-Lust.", "tokens": ["Du", "dul\u00b7test", "Sta\u00b7chel\u00b7Ri\u00df", "/", "er\u00b7wirbst", "mir", "Ro\u00b7sen\u00b7Lust", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$(", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Lieb / der ich mich zu Lieb will willig ritzen lassen:", "tokens": ["Lieb", "/", "der", "ich", "mich", "zu", "Lieb", "will", "wil\u00b7lig", "rit\u00b7zen", "las\u00b7sen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PRELS", "PPER", "PRF", "APPR", "NN", "VMFIN", "ADJD", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "gib / da\u00df ich dich ausbreit' in Kron-und Sternen-Strassen!", "tokens": ["gib", "/", "da\u00df", "ich", "dich", "aus\u00b7breit'", "in", "Kron\u00b7\u00b7und", "Ster\u00b7nen\u00b7Stras\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$(", "KOUS", "PPER", "PRF", "VVFIN", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}