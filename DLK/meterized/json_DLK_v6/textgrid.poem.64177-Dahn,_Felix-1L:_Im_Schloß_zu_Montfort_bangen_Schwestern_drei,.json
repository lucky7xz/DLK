{"textgrid.poem.64177": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im Schlo\u00df zu Montfort bangen Schwestern drei,", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Schlo\u00df zu Montfort bangen Schwestern drei,", "tokens": ["Im", "Schlo\u00df", "zu", "Mont\u00b7fort", "ban\u00b7gen", "Schwes\u00b7tern", "drei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "ADJA", "NN", "CARD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ob K\u00f6nig Richard noch am Leben sei.", "tokens": ["Ob", "K\u00f6\u00b7nig", "Ric\u00b7hard", "noch", "am", "Le\u00b7ben", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NE", "ADV", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Oft sprach er zu: \u2013 gleich sch\u00f6n die Fr\u00e4ulein waren", "tokens": ["Oft", "sprach", "er", "zu", ":", "\u2013", "gleich", "sch\u00f6n", "die", "Fr\u00e4u\u00b7lein", "wa\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$.", "$(", "ADV", "ADJD", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In schwarzen, braunen und in goldnen Haaren.", "tokens": ["In", "schwar\u00b7zen", ",", "brau\u00b7nen", "und", "in", "gold\u00b7nen", "Haa\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVINF", "$,", "VVFIN", "KON", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Man wu\u00dfte nicht, f\u00fcr welche schlug sein Herz:", "tokens": ["Man", "wu\u00df\u00b7te", "nicht", ",", "f\u00fcr", "wel\u00b7che", "schlug", "sein", "Herz", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKNEG", "$,", "APPR", "PRELS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "\u00bber wei\u00df es selbst nicht!\u00ab neckte Blondels Scherz.", "tokens": ["\u00bb", "er", "wei\u00df", "es", "selbst", "nicht", "!", "\u00ab", "neck\u00b7te", "Blon\u00b7dels", "Scherz", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "$.", "$(", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch jede liebet ihn, den Wundervollen;", "tokens": ["Doch", "je\u00b7de", "lie\u00b7bet", "ihn", ",", "den", "Wun\u00b7der\u00b7vol\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er nahm das Kreuz: \u2013 seitdem ist er verschollen.", "tokens": ["Er", "nahm", "das", "Kreuz", ":", "\u2013", "seit\u00b7dem", "ist", "er", "ver\u00b7schol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "$(", "PAV", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Die Schwestern harr'n. \u2013 Da tritt nach Tag und Jahr", "tokens": ["Die", "Schwes\u00b7tern", "harr'", "n.", "\u2013", "Da", "tritt", "nach", "Tag", "und", "Jahr"], "token_info": ["word", "word", "word", "abbreviation", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "NE", "$(", "ADV", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In ihre Kemenat ein Pilgerpaar:", "tokens": ["In", "ih\u00b7re", "Ke\u00b7me\u00b7nat", "ein", "Pil\u00b7ger\u00b7paar", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der lange Bart, der Muschelhut beweisen,", "tokens": ["Der", "lan\u00b7ge", "Bart", ",", "der", "Mu\u00b7schel\u00b7hut", "be\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Jordanstab der Pilger fromme Reisen.", "tokens": ["Der", "Jor\u00b7dan\u00b7stab", "der", "Pil\u00b7ger", "from\u00b7me", "Rei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "\u00bbeuch edeln Fr\u00e4ulein k\u00fcnden wir nun Leid:", "tokens": ["\u00bb", "euch", "e\u00b7deln", "Fr\u00e4u\u00b7lein", "k\u00fcn\u00b7den", "wir", "nun", "Leid", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADJA", "NN", "VVFIN", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gebunden liegt der Stolz der Christenheit:", "tokens": ["Ge\u00b7bun\u00b7den", "liegt", "der", "Stolz", "der", "Chris\u00b7ten\u00b7heit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In Trifels Burg, in schweren Eisenspangen,", "tokens": ["In", "Tri\u00b7fels", "Burg", ",", "in", "schwe\u00b7ren", "Ei\u00b7sen\u00b7span\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fcrs Leben liegt der L\u00f6wenherz gefangen!\u00ab", "tokens": ["F\u00fcrs", "Le\u00b7ben", "liegt", "der", "L\u00f6\u00b7wen\u00b7herz", "ge\u00b7fan\u00b7gen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Da strich die erste, Gr\u00e4fin Eleanor,", "tokens": ["Da", "strich", "die", "ers\u00b7te", ",", "Gr\u00e4\u00b7fin", "E\u00b7lea\u00b7nor", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "$,", "NN", "NE", "$,"], "meter": "-+-+-+----", "measure": "unknown.measure.tri"}, "line.2": {"text": "Die stolzen schwarzen Brau'n gemach empor:", "tokens": ["Die", "stol\u00b7zen", "schwar\u00b7zen", "Brau'n", "ge\u00b7mach", "em\u00b7por", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbich schwankte lang, wen der Rivalen w\u00e4hlen: \u2013", "tokens": ["\u00bb", "ich", "schwank\u00b7te", "lang", ",", "wen", "der", "Ri\u00b7va\u00b7len", "w\u00e4h\u00b7len", ":", "\u2013"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADJD", "$,", "PWS", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Nun werd' ich Frankreichs K\u00f6nig mich verm\u00e4hlen.\u00ab", "tokens": ["Nun", "werd'", "ich", "Fran\u00b7kreichs", "K\u00f6\u00b7nig", "mich", "ver\u00b7m\u00e4h\u00b7len", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NE", "NN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "In Tr\u00e4nen sprach die zweite, Gr\u00e4fin Maud:", "tokens": ["In", "Tr\u00e4\u00b7nen", "sprach", "die", "zwei\u00b7te", ",", "Gr\u00e4\u00b7fin", "Maud", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "ADJA", "$,", "NN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbund ist der edle Mann lebendig tot,", "tokens": ["\u00bb", "und", "ist", "der", "ed\u00b7le", "Mann", "le\u00b7ben\u00b7dig", "tot", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VAFIN", "ART", "ADJA", "NN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Will ich mein langes braunes Haar verschneiden", "tokens": ["Will", "ich", "mein", "lan\u00b7ges", "brau\u00b7nes", "Haar", "ver\u00b7schnei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPOSAT", "ADJA", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und bis ich sterbe mich als Nonne kleiden.\u00ab", "tokens": ["Und", "bis", "ich", "ster\u00b7be", "mich", "als", "Non\u00b7ne", "klei\u00b7den", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "PPER", "KOUS", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Die j\u00fcngste Schwester aber sprach kein Wort: \u2013", "tokens": ["Die", "j\u00fcngs\u00b7te", "Schwes\u00b7ter", "a\u00b7ber", "sprach", "kein", "Wort", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Stumm stand sie auf: zur T\u00fcr schritt sie so fort:", "tokens": ["Stumm", "stand", "sie", "auf", ":", "zur", "T\u00fcr", "schritt", "sie", "so", "fort", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PTKVZ", "$.", "APPRART", "NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da sank sie fast: der Herzschlag blieb ihr stocken:", "tokens": ["Da", "sank", "sie", "fast", ":", "der", "Herz\u00b7schlag", "blieb", "ihr", "sto\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$.", "ART", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gen Himmel sch\u00fcttelt sie die gelben Locken.", "tokens": ["Gen", "Him\u00b7mel", "sch\u00fct\u00b7telt", "sie", "die", "gel\u00b7ben", "Lo\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Der gr\u00f6\u00dfre Pilger sprach: \u00bbWo wollt Ihr hin?\u00ab", "tokens": ["Der", "gr\u00f6\u00df\u00b7re", "Pil\u00b7ger", "sprach", ":", "\u00bb", "Wo", "wollt", "Ihr", "hin", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "$(", "PWAV", "VMFIN", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbzu ihm! Zu ihm!\u00ab \u2013 \u00bbWie, was k\u00f6mmt Euch zu Sinn?\u00ab", "tokens": ["\u00bb", "zu", "ihm", "!", "Zu", "ihm", "!", "\u00ab", "\u2013", "\u00bb", "Wie", ",", "was", "k\u00f6mmt", "Euch", "zu", "Sinn", "?", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "punct", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "PPER", "$.", "APPR", "PPER", "$.", "$(", "$(", "$(", "PWAV", "$,", "PWS", "VVFIN", "PPER", "APPR", "NN", "$.", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "\u00bbich lieb' ihn und ich will so lange flehen,", "tokens": ["\u00bb", "ich", "lieb'", "ihn", "und", "ich", "will", "so", "lan\u00b7ge", "fle\u00b7hen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "KON", "PPER", "VMFIN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bis Eines von zwei Dingen ist geschehen:", "tokens": ["Bis", "Ei\u00b7nes", "von", "zwei", "Din\u00b7gen", "ist", "ge\u00b7sche\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "APPR", "CARD", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Die Freiheit ihm: \u2013 wenn nicht \u2013: mir selbst der Tod!\u00ab", "tokens": ["Die", "Frei\u00b7heit", "ihm", ":", "\u2013", "wenn", "nicht", "\u2013", ":", "mir", "selbst", "der", "Tod", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "PPER", "$.", "$(", "KOUS", "PTKNEG", "$(", "$.", "PPER", "ADV", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da k\u00fc\u00dft der Pilger ihr die Lippen rot:", "tokens": ["Da", "k\u00fc\u00dft", "der", "Pil\u00b7ger", "ihr", "die", "Lip\u00b7pen", "rot", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbgut war dein Rat, Freund Blondel, kluger S\u00e4nger!", "tokens": ["\u00bb", "gut", "war", "dein", "Rat", ",", "Freund", "Blon\u00b7del", ",", "klu\u00b7ger", "S\u00e4n\u00b7ger", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADJD", "VAFIN", "PPOSAT", "NN", "$,", "NN", "NE", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du herrlich Kind, nein, zweifle mir nicht l\u00e4nger.", "tokens": ["Du", "herr\u00b7lich", "Kind", ",", "nein", ",", "zweif\u00b7le", "mir", "nicht", "l\u00e4n\u00b7ger", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "$,", "PTKANT", "$,", "VVFIN", "PPER", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Gefangen war ich: \u2013 doch nun bin ich frei,", "tokens": ["Ge\u00b7fan\u00b7gen", "war", "ich", ":", "\u2013", "doch", "nun", "bin", "ich", "frei", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$.", "$(", "ADV", "ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf da\u00df ich ewig dir zu eigen sei.", "tokens": ["Auf", "da\u00df", "ich", "e\u00b7wig", "dir", "zu", "ei\u00b7gen", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "ADJD", "PPER", "PTKA", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dein Herz ist, wie dein Haar, von lautrem Golde:", "tokens": ["Dein", "Herz", "ist", ",", "wie", "dein", "Haar", ",", "von", "laut\u00b7rem", "Gol\u00b7de", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "$,", "PWAV", "PPOSAT", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich liebe dich, du s\u00fc\u00df' Gesch\u00f6pf, Isolde!\u00ab", "tokens": ["Ich", "lie\u00b7be", "dich", ",", "du", "s\u00fc\u00df'", "Ge\u00b7sch\u00f6pf", ",", "I\u00b7sol\u00b7de", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "ADJA", "NN", "$,", "NE", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}