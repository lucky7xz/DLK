{"textgrid.poem.60584": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "An Herrn Canonicus Gleim", "genre": "verse", "period": "N.A.", "pub_year": 1758, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Kriege Friederichs und wie mit g\u00fcldnen Schwingen", "tokens": ["Die", "Krie\u00b7ge", "Frie\u00b7de\u00b7richs", "und", "wie", "mit", "g\u00fcld\u00b7nen", "Schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "KON", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Sieg an seiner Seite gl\u00e4nzt,", "tokens": ["Der", "Sieg", "an", "sei\u00b7ner", "Sei\u00b7te", "gl\u00e4nzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wird Kleist, mit Lorbeern selbst bekr\u00e4nzt,", "tokens": ["Wird", "Kleist", ",", "mit", "Lor\u00b7beern", "selbst", "be\u00b7kr\u00e4nzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In seine k\u00fchnre Leyer singen.", "tokens": ["In", "sei\u00b7ne", "k\u00fchn\u00b7re", "Le\u00b7yer", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mein sch\u00fcchtern Saitenspiel str\u00e4ubt in verw\u00f6hnten H\u00e4nden,", "tokens": ["Mein", "sch\u00fcch\u00b7tern", "Sai\u00b7ten\u00b7spiel", "str\u00e4ubt", "in", "ver\u00b7w\u00f6hn\u00b7ten", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O Gleim, sich wider kriegrisch Lob,", "tokens": ["O", "Gleim", ",", "sich", "wi\u00b7der", "krie\u00b7grisch", "Lob", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PRF", "APPR", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und traurt, seit Zwietracht sich erhob,", "tokens": ["Und", "traurt", ",", "seit", "Zwiet\u00b7racht", "sich", "er\u00b7hob", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Helden edles Blut verschwenden.", "tokens": ["Und", "Hel\u00b7den", "ed\u00b7les", "Blut", "ver\u00b7schwen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die deutsche Muse soll nicht jauchzen, sondern klagen:", "tokens": ["Die", "deut\u00b7sche", "Mu\u00b7se", "soll", "nicht", "jauch\u00b7zen", ",", "son\u00b7dern", "kla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PTKNEG", "VVINF", "$,", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn Deutschland f\u00fchlt der Waffen Wuth.", "tokens": ["Denn", "Deutschland", "f\u00fchlt", "der", "Waf\u00b7fen", "Wuth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mars donnert wild einher, und Blut", "tokens": ["Mars", "don\u00b7nert", "wild", "ein\u00b7her", ",", "und", "Blut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "ADJD", "PTKVZ", "$,", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Umflie\u00dfet seinen ehrnen Wagen.", "tokens": ["Um\u00b7flie\u00b7\u00dfet", "sei\u00b7nen", "ehr\u00b7nen", "Wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Gewaltige der Welt, ihr f\u00fchret mit Entz\u00fccken", "tokens": ["Ge\u00b7wal\u00b7ti\u00b7ge", "der", "Welt", ",", "ihr", "f\u00fch\u00b7ret", "mit", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das rauschende Verderben an?", "tokens": ["Das", "rau\u00b7schen\u00b7de", "Ver\u00b7der\u00b7ben", "an", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und euer l\u00e4chelnd Auge kann", "tokens": ["Und", "eu\u00b7er", "l\u00e4\u00b7chelnd", "Au\u00b7ge", "kann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJD", "NN", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Furien des Kriegs erblicken?", "tokens": ["Die", "Fu\u00b7ri\u00b7en", "des", "Kriegs", "er\u00b7bli\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Seht! Eures Volkes Blut raucht str\u00f6mend von der Erden!", "tokens": ["Seht", "!", "Eu\u00b7res", "Vol\u00b7kes", "Blut", "raucht", "str\u00f6\u00b7mend", "von", "der", "Er\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PPOSAT", "NN", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ach! Die\u00df betrogne Volk ergab", "tokens": ["Ach", "!", "Die\u00df", "be\u00b7trog\u00b7ne", "Volk", "er\u00b7gab"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PDS", "VVFIN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sich unter euern Hirtenstab,", "tokens": ["Sich", "un\u00b7ter", "eu\u00b7ern", "Hir\u00b7ten\u00b7stab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geweidet, nicht gew\u00fcrgt zu werden.", "tokens": ["Ge\u00b7wei\u00b7det", ",", "nicht", "ge\u00b7w\u00fcrgt", "zu", "wer\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PTKNEG", "VVPP", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der Vater seines Lands und blieb' er auch verborgen,", "tokens": ["Der", "Va\u00b7ter", "sei\u00b7nes", "Lands", "und", "blieb'", "er", "auch", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "KON", "VVFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist nicht geringer, als der Held.", "tokens": ["Ist", "nicht", "ge\u00b7rin\u00b7ger", ",", "als", "der", "Held", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADJD", "$,", "KOUS", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Sorgen um das Gl\u00fcck der Welt", "tokens": ["Die", "Sor\u00b7gen", "um", "das", "Gl\u00fcck", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sind wahre k\u00f6nigliche Sorgen.", "tokens": ["Sind", "wah\u00b7re", "k\u00f6\u00b7nig\u00b7li\u00b7che", "Sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Macht euer Land begl\u00fcckt, an statt es zu vergr\u00f6\u00dfern,", "tokens": ["Macht", "eu\u00b7er", "Land", "be\u00b7gl\u00fcckt", ",", "an", "statt", "es", "zu", "ver\u00b7gr\u00f6\u00b7\u00dfern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVPP", "$,", "APPR", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ermuntert mit verdientem Preis", "tokens": ["Er\u00b7mun\u00b7tert", "mit", "ver\u00b7dien\u00b7tem", "Preis"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die scheue Wissenschaft, den Flei\u00df,", "tokens": ["Die", "scheu\u00b7e", "Wis\u00b7sen\u00b7schaft", ",", "den", "Flei\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sucht, die Sitten zu verbessern.", "tokens": ["Und", "sucht", ",", "die", "Sit\u00b7ten", "zu", "ver\u00b7bes\u00b7sern", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Sucht, ungebautes Land in Auen umzuschaffen,", "tokens": ["Sucht", ",", "un\u00b7ge\u00b7bau\u00b7tes", "Land", "in", "Au\u00b7en", "um\u00b7zu\u00b7schaf\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "APPR", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit r\u00e4chender Gerechtigkeit", "tokens": ["Mit", "r\u00e4\u00b7chen\u00b7der", "Ge\u00b7rech\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wacht f\u00fcr der Unschuld Sicherheit,", "tokens": ["Wacht", "f\u00fcr", "der", "Un\u00b7schuld", "Si\u00b7cher\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sch\u00fctzt sie mit gerechten Waffen:", "tokens": ["Und", "sch\u00fctzt", "sie", "mit", "ge\u00b7rech\u00b7ten", "Waf\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "So wartet einst auf euch der Name guter F\u00fcrsten,", "tokens": ["So", "war\u00b7tet", "einst", "auf", "euch", "der", "Na\u00b7me", "gu\u00b7ter", "F\u00fcrs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So strahlt mit eurem sch\u00f6nern Ruhm", "tokens": ["So", "strahlt", "mit", "eu\u00b7rem", "sch\u00f6\u00b7nern", "Ruhm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Ehre lichtes Heiligthum,", "tokens": ["Der", "Eh\u00b7re", "lich\u00b7tes", "Hei\u00b7lig\u00b7thum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor denen, die nach L\u00e4ndern d\u00fcrsten.", "tokens": ["Vor", "de\u00b7nen", ",", "die", "nach", "L\u00e4n\u00b7dern", "d\u00fcrs\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "$,", "PRELS", "APPR", "NN", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Umsonst! Sie h\u00f6ren nicht der frommen Muse Klagen;", "tokens": ["Um\u00b7sonst", "!", "Sie", "h\u00f6\u00b7ren", "nicht", "der", "from\u00b7men", "Mu\u00b7se", "Kla\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie wollen Krieg, und nun bereits", "tokens": ["Sie", "wol\u00b7len", "Krieg", ",", "und", "nun", "be\u00b7reits"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "NN", "$,", "KON", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Br\u00fcllt weit umher die Wuth des Streits,", "tokens": ["Br\u00fcllt", "weit", "um\u00b7her", "die", "Wuth", "des", "Streits", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKVZ", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und alle Nationen zagen!", "tokens": ["Und", "al\u00b7le", "Na\u00b7ti\u00b7o\u00b7nen", "za\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}