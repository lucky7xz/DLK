{"textgrid.poem.46458": {"metadata": {"author": {"name": "Engelke, Gerrit", "birth": "N.A.", "death": "N.A."}, "title": "1L: ... denke so \u2013 da\u00df du mit vielen Leuten", "genre": "verse", "period": "N.A.", "pub_year": 1904, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "... denke so \u2013 da\u00df du mit vielen Leuten", "tokens": ["...", "den\u00b7ke", "so", "\u2013", "da\u00df", "du", "mit", "vie\u00b7len", "Leu\u00b7ten"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "ADV", "$(", "KOUS", "PPER", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Durch die Stra\u00dfen gehst \u2013", "tokens": ["Durch", "die", "Stra\u00b7\u00dfen", "gehst", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Vor diesem Laden da stehst \u2013", "tokens": ["Vor", "die\u00b7sem", "La\u00b7den", "da", "stehst", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Unter vielen, fremden Leuten \u2013", "tokens": ["Un\u00b7ter", "vie\u00b7len", ",", "frem\u00b7den", "Leu\u00b7ten", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Da\u00df du im Alltagseinerlei", "tokens": ["Da\u00df", "du", "im", "All\u00b7ta\u00b7gsei\u00b7ner\u00b7lei"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Menschenrudeln", "tokens": ["In", "Men\u00b7schen\u00b7ru\u00b7deln"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Durch die Stra\u00dfenstrudel", "tokens": ["Durch", "die", "Stra\u00b7\u00dfen\u00b7stru\u00b7del"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Dahingetrieben \u2013 \u2013", "tokens": ["Da\u00b7hin\u00b7ge\u00b7trie\u00b7ben", "\u2013", "\u2013"], "token_info": ["word", "punct", "punct"], "pos": ["PAV", "$(", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Immer an mir vorbei \u2013 \u2013", "tokens": ["Im\u00b7mer", "an", "mir", "vor\u00b7bei", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPR", "PPER", "PTKVZ", "$(", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "... denke so \u2013 da\u00df zu viele Menschen", "tokens": ["...", "den\u00b7ke", "so", "\u2013", "da\u00df", "zu", "vie\u00b7le", "Men\u00b7schen"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "ADV", "$(", "KOUS", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch die Stra\u00dfen gehn \u2013", "tokens": ["Durch", "die", "Stra\u00b7\u00dfen", "gehn", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df die Stra\u00dfen alle auseinander zweigen \u2013", "tokens": ["Da\u00df", "die", "Stra\u00b7\u00dfen", "al\u00b7le", "aus\u00b7ein\u00b7an\u00b7der", "zwei\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PIAT", "NN", "VVINF", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Da\u00df", "tokens": ["Da\u00df"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Wir uns nie im Abendschweigen", "tokens": ["Wir", "uns", "nie", "im", "A\u00b7bend\u00b7schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "PPER", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In Einer Gasse wiedersehn.", "tokens": ["In", "Ei\u00b7ner", "Gas\u00b7se", "wie\u00b7der\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}