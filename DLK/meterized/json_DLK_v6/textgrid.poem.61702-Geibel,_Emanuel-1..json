{"textgrid.poem.61702": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Winde kommt ein scharfer Ton,", "tokens": ["Im", "Win\u00b7de", "kommt", "ein", "schar\u00b7fer", "Ton", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die wilden Schw\u00e4ne wandern schon,", "tokens": ["Die", "wil\u00b7den", "Schw\u00e4\u00b7ne", "wan\u00b7dern", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die sch\u00f6ne Zeit geht scheiden;", "tokens": ["Die", "sch\u00f6\u00b7ne", "Zeit", "geht", "schei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du hast mich sommerlang gek\u00fc\u00dft,", "tokens": ["Du", "hast", "mich", "som\u00b7mer\u00b7lang", "ge\u00b7k\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nun steht nach anderm dein Gel\u00fcst,", "tokens": ["Nun", "steht", "nach", "an\u00b7derm", "dein", "Ge\u00b7l\u00fcst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PIS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie sollt' ich's dir verleiden!", "tokens": ["Wie", "sollt'", "ich's", "dir", "ver\u00b7lei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PIS", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Am Berge liegt ein wei\u00dfer Streif,", "tokens": ["Am", "Ber\u00b7ge", "liegt", "ein", "wei\u00b7\u00dfer", "Streif", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So fiel auf deine Lieb' ein Reif,", "tokens": ["So", "fiel", "auf", "dei\u00b7ne", "Lieb'", "ein", "Reif", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hei\u00dft: \u00dcberdru\u00df und Reue;", "tokens": ["Hei\u00dft", ":", "\u00dc\u00b7berd\u00b7ru\u00df", "und", "Reu\u00b7e", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In Windeswirbeln fliegt der Staub,", "tokens": ["In", "Win\u00b7des\u00b7wir\u00b7beln", "fliegt", "der", "Staub", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es bricht der Ast, es stiebt das Laub,", "tokens": ["Es", "bricht", "der", "Ast", ",", "es", "stiebt", "das", "Laub", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Warum nicht deine Treue?", "tokens": ["Wa\u00b7rum", "nicht", "dei\u00b7ne", "Treu\u00b7e", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Fahr hin, ich wei\u00df nun, wie du liebst;", "tokens": ["Fahr", "hin", ",", "ich", "wei\u00df", "nun", ",", "wie", "du", "liebst", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPER", "VVFIN", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Herz, das du nur halb vergibst,", "tokens": ["Ein", "Herz", ",", "das", "du", "nur", "halb", "ver\u00b7gibst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das g\u00f6nn' ich jedem andern.", "tokens": ["Das", "g\u00f6nn'", "ich", "je\u00b7dem", "an\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PIAT", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Fahr hin! Dein Weinen d\u00fcnkt mich Hohn.", "tokens": ["Fahr", "hin", "!", "Dein", "Wei\u00b7nen", "d\u00fcnkt", "mich", "Hohn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "PPOSAT", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die wilden Schw\u00e4ne wandern schon,", "tokens": ["Die", "wil\u00b7den", "Schw\u00e4\u00b7ne", "wan\u00b7dern", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und ich, auch ich will wandern.", "tokens": ["Und", "ich", ",", "auch", "ich", "will", "wan\u00b7dern", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ADV", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}