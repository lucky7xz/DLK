{"textgrid.poem.56151": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "1L: Alles Erworbne bedroht die Maschine, solange", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alles Erworbne bedroht die Maschine, solange", "tokens": ["Al\u00b7les", "Er\u00b7worb\u00b7ne", "be\u00b7droht", "die", "Ma\u00b7schi\u00b7ne", ",", "so\u00b7lan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "$,", "KOUS"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.2": {"text": "sie sich erdreistet, im Geist, statt im Gehorchen, zu sein.", "tokens": ["sie", "sich", "er\u00b7dreis\u00b7tet", ",", "im", "Geist", ",", "statt", "im", "Ge\u00b7hor\u00b7chen", ",", "zu", "sein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PRF", "VVPP", "$,", "APPRART", "NN", "$,", "KOUI", "APPRART", "NN", "$,", "PTKZU", "VAINF", "$."], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da\u00df nicht der herrlichen Hand sch\u00f6neres Z\u00f6gern mehr prange,", "tokens": ["Da\u00df", "nicht", "der", "herr\u00b7li\u00b7chen", "Hand", "sch\u00f6\u00b7ne\u00b7res", "Z\u00f6\u00b7gern", "mehr", "pran\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "ADJA", "NN", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+--++--+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "zu dem entschlossenern Bau schneidet sie steifer den Stein.", "tokens": ["zu", "dem", "ent\u00b7schlos\u00b7se\u00b7nern", "Bau", "schnei\u00b7det", "sie", "stei\u00b7fer", "den", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "ART", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Nirgends bleibt sie zur\u00fcck, da\u00df wir ihr ", "tokens": ["Nir\u00b7gends", "bleibt", "sie", "zu\u00b7r\u00fcck", ",", "da\u00df", "wir", "ihr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "PPER", "PPOSAT"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "und sie in stiller Fabrik \u00f6lend sich selber geh\u00f6rt.", "tokens": ["und", "sie", "in", "stil\u00b7ler", "Fab\u00b7rik", "\u00f6\u00b7lend", "sich", "sel\u00b7ber", "ge\u00b7h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN", "ADJD", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Sie ist das Leben, \u2013 sie meint es am besten zu k\u00f6nnen,", "tokens": ["Sie", "ist", "das", "Le\u00b7ben", ",", "\u2013", "sie", "meint", "es", "am", "bes\u00b7ten", "zu", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "$(", "PPER", "VVFIN", "PPER", "PTKA", "ADJD", "PTKZU", "VMINF", "$,"], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "die mit dem gleichen Entschlu\u00df ordnet und schafft und zerst\u00f6rt.", "tokens": ["die", "mit", "dem", "glei\u00b7chen", "Ent\u00b7schlu\u00df", "ord\u00b7net", "und", "schafft", "und", "zer\u00b7st\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "KON", "VVPP", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Aber noch ist uns das Dasein verzaubert; an hundert", "tokens": ["A\u00b7ber", "noch", "ist", "uns", "das", "Da\u00b7sein", "ver\u00b7zau\u00b7bert", ";", "an", "hun\u00b7dert"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ART", "NN", "VVFIN", "$.", "APPR", "CARD"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.2": {"text": "Stellen ist es noch Ursprung. Ein Spielen von reinen", "tokens": ["Stel\u00b7len", "ist", "es", "noch", "Ur\u00b7sprung", ".", "Ein", "Spie\u00b7len", "von", "rei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "ADV", "NN", "$.", "ART", "NN", "APPR", "ADJA"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Kr\u00e4ften, die keiner ber\u00fchrt, der nicht kniet und bewundert.", "tokens": ["Kr\u00e4f\u00b7ten", ",", "die", "kei\u00b7ner", "be\u00b7r\u00fchrt", ",", "der", "nicht", "kniet", "und", "be\u00b7wun\u00b7dert", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PIS", "VVPP", "$,", "PRELS", "PTKNEG", "VVFIN", "KON", "VVPP", "$."], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}}, "stanza.4": {"line.1": {"text": "Worte gehen noch zart am Uns\u00e4glichen aus...", "tokens": ["Wor\u00b7te", "ge\u00b7hen", "noch", "zart", "am", "Un\u00b7s\u00e4g\u00b7li\u00b7chen", "aus", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+--+-++--+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Und die Musik, immer neu, aus den bebendsten Steinen,", "tokens": ["Und", "die", "Mu\u00b7sik", ",", "im\u00b7mer", "neu", ",", "aus", "den", "be\u00b7bends\u00b7ten", "Stei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ADV", "ADJD", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.3": {"text": "baut im unbrauchbaren Raum ihr verg\u00f6ttlichtes Haus.", "tokens": ["baut", "im", "un\u00b7brauch\u00b7ba\u00b7ren", "Raum", "ihr", "ver\u00b7g\u00f6tt\u00b7lich\u00b7tes", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}}}}}