{"textgrid.poem.49864": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Versuchung", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn sie in silberner Schale mit Wein uns w\u00fcrzet die Erdbeern,", "tokens": ["Wenn", "sie", "in", "sil\u00b7ber\u00b7ner", "Scha\u00b7le", "mit", "Wein", "uns", "w\u00fcr\u00b7zet", "die", "Erd\u00b7beern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "APPR", "NN", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Dicht mit Zucker noch erst streuet die Kinder des Walds:", "tokens": ["Dicht", "mit", "Zu\u00b7cker", "noch", "erst", "streu\u00b7et", "die", "Kin\u00b7der", "des", "Walds", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ADV", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "O wie schmacht ich hinauf zu den duftigern Lippen, wie d\u00fcrstet", "tokens": ["O", "wie", "schmacht", "ich", "hin\u00b7auf", "zu", "den", "duf\u00b7ti\u00b7gern", "Lip\u00b7pen", ",", "wie", "d\u00fcrs\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "KOKOM", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$,", "PWAV", "VMFIN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Nach des gebogenen Arms schimmernder Wei\u00dfe mein Mund!", "tokens": ["Nach", "des", "ge\u00b7bo\u00b7ge\u00b7nen", "Arms", "schim\u00b7mern\u00b7der", "Wei\u00b7\u00dfe", "mein", "Mund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}}}}}