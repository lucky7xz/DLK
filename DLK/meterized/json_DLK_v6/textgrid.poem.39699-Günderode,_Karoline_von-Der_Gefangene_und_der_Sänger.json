{"textgrid.poem.39699": {"metadata": {"author": {"name": "G\u00fcnderode, Karoline von", "birth": "N.A.", "death": "N.A."}, "title": "Der Gefangene und der S\u00e4nger", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wallte mit leichtem und lustigem Sinn", "tokens": ["Ich", "wall\u00b7te", "mit", "leich\u00b7tem", "und", "lus\u00b7ti\u00b7gem", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "KON", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und singend am Kerker vor\u00fcber;", "tokens": ["Und", "sin\u00b7gend", "am", "Ker\u00b7ker", "vor\u00b7\u00fc\u00b7ber", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Da schallt aus der Tiefe, da schallt aus dem Thurm", "tokens": ["Da", "schallt", "aus", "der", "Tie\u00b7fe", ",", "da", "schallt", "aus", "dem", "Thurm"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$,", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Mir Stimme des Freundes her\u00fcber. \u2013", "tokens": ["Mir", "Stim\u00b7me", "des", "Freun\u00b7des", "her\u00b7\u00fc\u00b7ber", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NN", "ART", "NN", "ADV", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbach S\u00e4nger! verweile, mich tr\u00f6stet dein Lied,\u00ab", "tokens": ["\u00bb", "ach", "S\u00e4n\u00b7ger", "!", "ver\u00b7wei\u00b7le", ",", "mich", "tr\u00f6s\u00b7tet", "dein", "Lied", ",", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "XY", "NN", "$.", "VVFIN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "\u00bbes steigt zum Gefangnen herunter,\u00ab", "tokens": ["\u00bb", "es", "steigt", "zum", "Ge\u00b7fang\u00b7nen", "her\u00b7un\u00b7ter", ",", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "$,", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "\u00bbihm macht es gesellig die einsame Zeit,\u00ab", "tokens": ["\u00bb", "ihm", "macht", "es", "ge\u00b7sel\u00b7lig", "die", "ein\u00b7sa\u00b7me", "Zeit", ",", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ADJD", "ART", "ADJA", "NN", "$,", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "\u00bbdas krankende Herz ihm gesunder.\u00ab", "tokens": ["\u00bb", "das", "kran\u00b7ken\u00b7de", "Herz", "ihm", "ge\u00b7sun\u00b7der", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "PPER", "ADJD", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Ich horchte der Stimme, gehorchte ihr bald,", "tokens": ["Ich", "horch\u00b7te", "der", "Stim\u00b7me", ",", "ge\u00b7horch\u00b7te", "ihr", "bald", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Zum Kerker hin wandt' ich die Schritte,", "tokens": ["Zum", "Ker\u00b7ker", "hin", "wandt'", "ich", "die", "Schrit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gern sprach ich die freundlichsten Worte hinab,", "tokens": ["Gern", "sprach", "ich", "die", "freund\u00b7lichs\u00b7ten", "Wor\u00b7te", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Begegnete jeglicher Bitte.", "tokens": ["Be\u00b7ge\u00b7gne\u00b7te", "jeg\u00b7li\u00b7cher", "Bit\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Da war dem Gefangenen freier der Sinn,", "tokens": ["Da", "war", "dem", "Ge\u00b7fan\u00b7ge\u00b7nen", "frei\u00b7er", "der", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Gesellig die einsamen Stunden. \u2013", "tokens": ["Ge\u00b7sel\u00b7lig", "die", "ein\u00b7sa\u00b7men", "Stun\u00b7den", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbgern g\u00e4b ich dir Lieber! so rief er: die Hand,\u00ab", "tokens": ["\u00bb", "gern", "g\u00e4b", "ich", "dir", "Lie\u00b7ber", "!", "so", "rief", "er", ":", "die", "Hand", ",", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "PPER", "ADJD", "$.", "ADV", "VVFIN", "PPER", "$.", "ART", "NN", "$,", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "\u00bbdoch ist sie von Banden umwunden.\u00ab", "tokens": ["\u00bb", "doch", "ist", "sie", "von", "Ban\u00b7den", "um\u00b7wun\u00b7den", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "APPR", "NN", "VVINF", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "\u00bbgern k\u00e4m' ich Geliebter! gern k\u00e4m' ich herauf\u00ab", "tokens": ["\u00bb", "gern", "k\u00e4m'", "ich", "Ge\u00b7lieb\u00b7ter", "!", "gern", "k\u00e4m'", "ich", "her\u00b7auf", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "NN", "$.", "ADV", "VVFIN", "PPER", "PTKVZ", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "\u00bbam Herzen dich treulich zu herzen;\u00ab", "tokens": ["\u00bb", "am", "Her\u00b7zen", "dich", "treu\u00b7lich", "zu", "her\u00b7zen", ";", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPRART", "NN", "PPER", "ADJD", "PTKZU", "VVINF", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "\u00bbdoch trennen mich Mauern und Riegel von dir,\u00ab", "tokens": ["\u00bb", "doch", "tren\u00b7nen", "mich", "Mau\u00b7ern", "und", "Rie\u00b7gel", "von", "dir", ",", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "NN", "KON", "NN", "APPR", "PPER", "$,", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "\u00bbo f\u00fchl' des Gefangenen Schmerzen.\u00ab", "tokens": ["\u00bb", "o", "f\u00fchl'", "des", "Ge\u00b7fan\u00b7ge\u00b7nen", "Schmer\u00b7zen", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "VVFIN", "ART", "NN", "NN", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.6": {"line.1": {"text": "\u00bbes ziehet mich mancherlei Sehnsucht zu dir;\u00ab", "tokens": ["\u00bb", "es", "zie\u00b7het", "mich", "man\u00b7cher\u00b7lei", "Sehn\u00b7sucht", "zu", "dir", ";", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "PIAT", "NN", "APPR", "PPER", "$.", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "\u00bbdoch Ketten umfangen mein Leben,\u00ab", "tokens": ["\u00bb", "doch", "Ket\u00b7ten", "um\u00b7fan\u00b7gen", "mein", "Le\u00b7ben", ",", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "NN", "VVFIN", "PPOSAT", "NN", "$,", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "\u00bbdrum gehe mein Lieber und la\u00df mich allein,\u00ab", "tokens": ["\u00bb", "drum", "ge\u00b7he", "mein", "Lie\u00b7ber", "und", "la\u00df", "mich", "al\u00b7lein", ",", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PAV", "VVFIN", "PPOSAT", "NN", "KON", "VVIMP", "PPER", "ADV", "$,", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "\u00bbich Armer ich kann dir nichts geben.\u00ab \u2013", "tokens": ["\u00bb", "ich", "Ar\u00b7mer", "ich", "kann", "dir", "nichts", "ge\u00b7ben", ".", "\u00ab", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "PPER", "NN", "PPER", "VMFIN", "PPER", "PIS", "VVINF", "$.", "$(", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.7": {"line.1": {"text": "Da ward mir so weich und so wehe ums Herz,", "tokens": ["Da", "ward", "mir", "so", "weich", "und", "so", "we\u00b7he", "ums", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "KON", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ich konnte den Lieben nicht lassen.", "tokens": ["Ich", "konn\u00b7te", "den", "Lie\u00b7ben", "nicht", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "PTKNEG", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Am Kerker nun lausch' ich von Fr\u00fchrothes Schein", "tokens": ["Am", "Ker\u00b7ker", "nun", "lausch'", "ich", "von", "Fr\u00fch\u00b7ro\u00b7thes", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Bis Abends die Farben erblassen.", "tokens": ["Bis", "A\u00b7bends", "die", "Far\u00b7ben", "er\u00b7blas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.8": {"line.1": {"text": "Und harren dort werd' ich die Jahre hindurch,", "tokens": ["Und", "har\u00b7ren", "dort", "werd'", "ich", "die", "Jah\u00b7re", "hin\u00b7durch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "VAFIN", "PPER", "ART", "NN", "PAV", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Und sollt' ich drob selber erblassen.", "tokens": ["Und", "sollt'", "ich", "drob", "sel\u00b7ber", "er\u00b7blas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Es ist mir so weich und so sehnend ums Herz", "tokens": ["Es", "ist", "mir", "so", "weich", "und", "so", "seh\u00b7nend", "ums", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADJD", "KON", "ADV", "ADJD", "APPRART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ich kann den Geliebten nicht lassen.", "tokens": ["Ich", "kann", "den", "Ge\u00b7lieb\u00b7ten", "nicht", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}