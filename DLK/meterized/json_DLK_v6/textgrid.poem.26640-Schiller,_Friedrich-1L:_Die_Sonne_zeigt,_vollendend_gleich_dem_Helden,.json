{"textgrid.poem.26640": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Sonne zeigt, vollendend gleich dem Helden,", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sonne zeigt, vollendend gleich dem Helden,", "tokens": ["Die", "Son\u00b7ne", "zeigt", ",", "voll\u00b7en\u00b7dend", "gleich", "dem", "Hel\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADJD", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem tiefen Tal ihr Abendangesicht,", "tokens": ["Dem", "tie\u00b7fen", "Tal", "ihr", "A\u00b7ben\u00b7dan\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "(f\u00fcr andre, ach! gl\u00fcckselgre Welten", "tokens": ["(", "f\u00fcr", "and\u00b7re", ",", "ach", "!", "gl\u00fcck\u00b7selg\u00b7re", "Wel\u00b7ten"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["$(", "APPR", "PIS", "$,", "ITJ", "$.", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist das ein Morgenangesicht),", "tokens": ["Ist", "das", "ein", "Mor\u00b7ge\u00b7nan\u00b7ge\u00b7sicht", ")", ","], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PDS", "ART", "NN", "$(", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie sinkt herab vom blauen Himmel,", "tokens": ["Sie", "sinkt", "her\u00b7ab", "vom", "blau\u00b7en", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ruft die Gesch\u00e4ftigkeit zur Ruh,", "tokens": ["Ruft", "die", "Ge\u00b7sch\u00e4f\u00b7tig\u00b7keit", "zur", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Ihr Abschied stillt das Weltget\u00fcmmel", "tokens": ["Ihr", "Ab\u00b7schied", "stillt", "das", "Welt\u00b7ge\u00b7t\u00fcm\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und winkt dem Tag sein Ende zu.", "tokens": ["Und", "winkt", "dem", "Tag", "sein", "En\u00b7de", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Jetzt schwillt des Dichters Geist zu g\u00f6ttlichen Ges\u00e4ngen,", "tokens": ["Jetzt", "schwillt", "des", "Dich\u00b7ters", "Geist", "zu", "g\u00f6tt\u00b7li\u00b7chen", "Ge\u00b7s\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "La\u00df str\u00f6men sie, o Herr, aus h\u00f6herem Gef\u00fchl,", "tokens": ["La\u00df", "str\u00f6\u00b7men", "sie", ",", "o", "Herr", ",", "aus", "h\u00f6\u00b7he\u00b7rem", "Ge\u00b7f\u00fchl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "PPER", "$,", "FM", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "La\u00df die Begeisterung die k\u00fchnen Fl\u00fcgel schwingen,", "tokens": ["La\u00df", "die", "Be\u00b7geis\u00b7te\u00b7rung", "die", "k\u00fch\u00b7nen", "Fl\u00fc\u00b7gel", "schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Zu dir, zu dir, des hohen Fluges Ziel,", "tokens": ["Zu", "dir", ",", "zu", "dir", ",", "des", "ho\u00b7hen", "Flu\u00b7ges", "Ziel", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "PPER", "$,", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mich \u00fcber Sph\u00e4ren himmelan gehoben,", "tokens": ["Mich", "\u00fc\u00b7ber", "Sph\u00e4\u00b7ren", "him\u00b7me\u00b7lan", "ge\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Getragen sein vom herrlichen Gef\u00fchl,", "tokens": ["Ge\u00b7tra\u00b7gen", "sein", "vom", "herr\u00b7li\u00b7chen", "Ge\u00b7f\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAINF", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Den Abend und des Abends Sch\u00f6pfer loben,", "tokens": ["Den", "A\u00b7bend", "und", "des", "A\u00b7bends", "Sch\u00f6p\u00b7fer", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Durchstr\u00f6mt vom paradiesischen Gef\u00fchl.", "tokens": ["Durch\u00b7str\u00f6mt", "vom", "pa\u00b7ra\u00b7die\u00b7si\u00b7schen", "Ge\u00b7f\u00fchl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "F\u00fcr K\u00f6nige, f\u00fcr Gro\u00dfe ists geringe,", "tokens": ["F\u00fcr", "K\u00f6\u00b7ni\u00b7ge", ",", "f\u00fcr", "Gro\u00b7\u00dfe", "ists", "ge\u00b7rin\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "ADJA", "VAFIN", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Die Niederen besucht es nur \u2013", "tokens": ["Die", "Nie\u00b7de\u00b7ren", "be\u00b7sucht", "es", "nur", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "O Gott, du gabest mir Natur,", "tokens": ["O", "Gott", ",", "du", "ga\u00b7best", "mir", "Na\u00b7tur", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Teil Welten unter sie \u2013 nur, Vater, mir Ges\u00e4nge.", "tokens": ["Teil", "Wel\u00b7ten", "un\u00b7ter", "sie", "\u2013", "nur", ",", "Va\u00b7ter", ",", "mir", "Ge\u00b7s\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "PPER", "$(", "ADV", "$,", "NN", "$,", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ha! wie die m\u00fcden Abschiedsstrahlen", "tokens": ["Ha", "!", "wie", "die", "m\u00fc\u00b7den", "Ab\u00b7schieds\u00b7strah\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das wallende Gew\u00f6lk bemalen,", "tokens": ["Das", "wal\u00b7len\u00b7de", "Ge\u00b7w\u00f6lk", "be\u00b7ma\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie dort die Abendwolken sich", "tokens": ["Wie", "dort", "die", "Ab\u00b7end\u00b7wol\u00b7ken", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Scho\u00df der Silberwellen baden;", "tokens": ["Im", "Scho\u00df", "der", "Sil\u00b7ber\u00b7wel\u00b7len", "ba\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "O Anblick, wie entz\u00fcckst du mich!", "tokens": ["O", "An\u00b7blick", ",", "wie", "ent\u00b7z\u00fcckst", "du", "mich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gold, wie das Gelb gereifter Saaten,", "tokens": ["Gold", ",", "wie", "das", "Gelb", "ge\u00b7reif\u00b7ter", "Saa\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Gold liegt um alle H\u00fcgel her,", "tokens": ["Gold", "liegt", "um", "al\u00b7le", "H\u00fc\u00b7gel", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Verg\u00f6ldet sind der Eichen Wipfel,", "tokens": ["Ver\u00b7g\u00f6l\u00b7det", "sind", "der", "Ei\u00b7chen", "Wip\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Verg\u00f6ldet sind der Berge Gipfel,", "tokens": ["Ver\u00b7g\u00f6l\u00b7det", "sind", "der", "Ber\u00b7ge", "Gip\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Das Tal beschwimmt ein Feuermeer;", "tokens": ["Das", "Tal", "be\u00b7schwimmt", "ein", "Feu\u00b7er\u00b7meer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Der hohe Stern des Abends strahlet", "tokens": ["Der", "ho\u00b7he", "Stern", "des", "A\u00b7bends", "strah\u00b7let"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Aus Wolken, welche um ihn gl\u00fchn,", "tokens": ["Aus", "Wol\u00b7ken", ",", "wel\u00b7che", "um", "ihn", "gl\u00fchn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Wie der Rubin am falben Haar, das wallet", "tokens": ["Wie", "der", "Ru\u00b7bin", "am", "fal\u00b7ben", "Haar", ",", "das", "wal\u00b7let"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ART", "NE", "APPRART", "ADJA", "NN", "$,", "PDS", "VVFIN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.14": {"text": "Ums Angesicht der K\u00f6nigin.", "tokens": ["Ums", "An\u00b7ge\u00b7sicht", "der", "K\u00f6\u00b7ni\u00b7gin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Schau, wie der Sonnenglanz die K\u00f6nigsstadt beschimmert", "tokens": ["Schau", ",", "wie", "der", "Son\u00b7nen\u00b7glanz", "die", "K\u00f6\u00b7nigs\u00b7stadt", "be\u00b7schim\u00b7mert"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "ART", "NN", "VVPP"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Und fern die gr\u00fcne Heide lacht;", "tokens": ["Und", "fern", "die", "gr\u00fc\u00b7ne", "Hei\u00b7de", "lacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie hier in jugendlicher Pracht", "tokens": ["Wie", "hier", "in", "ju\u00b7gend\u00b7li\u00b7cher", "Pracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der ganze Himmel niederd\u00e4mmert;", "tokens": ["Der", "gan\u00b7ze", "Him\u00b7mel", "nie\u00b7der\u00b7d\u00e4m\u00b7mert", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie jetzt des Abends Purpurstrom,", "tokens": ["Wie", "jetzt", "des", "A\u00b7bends", "Pur\u00b7pur\u00b7strom", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gleich einem Beet von Fr\u00fchlingsrosen,", "tokens": ["Gleich", "ei\u00b7nem", "Beet", "von", "Fr\u00fch\u00b7lings\u00b7ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Gepfl\u00fccket im Elysium,", "tokens": ["Ge\u00b7pfl\u00fc\u00b7cket", "im", "E\u00b7ly\u00b7si\u00b7um", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Auf goldne Wolken hingegossen,", "tokens": ["Auf", "gold\u00b7ne", "Wol\u00b7ken", "hin\u00b7ge\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Ihn \u00fcberschwemmet um und um.", "tokens": ["Ihn", "\u00fc\u00b7bersc\u00b7hwem\u00b7met", "um", "und", "um", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "KOUI", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Vom Felsen rieselt spiegelhelle", "tokens": ["Vom", "Fel\u00b7sen", "rie\u00b7selt", "spie\u00b7gel\u00b7hel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ins Gras die reinste Silberquelle", "tokens": ["Ins", "Gras", "die", "reins\u00b7te", "Sil\u00b7ber\u00b7quel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und tr\u00e4nkt die Herd' und tr\u00e4nkt den Hirt;", "tokens": ["Und", "tr\u00e4nkt", "die", "Herd'", "und", "tr\u00e4nkt", "den", "Hirt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Am Weidenbusche liegt der Sch\u00e4fer,", "tokens": ["Am", "Wei\u00b7den\u00b7bu\u00b7sche", "liegt", "der", "Sch\u00e4\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Des Lied das ganze Tal durchirrt", "tokens": ["Des", "Lied", "das", "gan\u00b7ze", "Tal", "durch\u00b7irrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und wiederholt im Tale wird.", "tokens": ["Und", "wie\u00b7der\u00b7holt", "im", "Ta\u00b7le", "wird", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die stille Luft durchsumst der K\u00e4fer;", "tokens": ["Die", "stil\u00b7le", "Luft", "durch\u00b7sumst", "der", "K\u00e4\u00b7fer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Vom Zweige schl\u00e4gt die Nachtigall,", "tokens": ["Vom", "Zwei\u00b7ge", "schl\u00e4gt", "die", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ihr Meisterlied macht alle Ohren lauschen,", "tokens": ["Ihr", "Meis\u00b7ter\u00b7lied", "macht", "al\u00b7le", "Oh\u00b7ren", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.pen