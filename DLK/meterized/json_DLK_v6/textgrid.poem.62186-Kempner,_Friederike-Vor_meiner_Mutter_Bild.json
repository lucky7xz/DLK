{"textgrid.poem.62186": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "Vor meiner Mutter Bild", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sah Dich heut im Traume", "tokens": ["Ich", "sah", "Dich", "heut", "im", "Trau\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An eines Waldes Saume,", "tokens": ["An", "ei\u00b7nes", "Wal\u00b7des", "Sau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du sprachst ein gro\u00dfes Wort:", "tokens": ["Du", "sprachst", "ein", "gro\u00b7\u00dfes", "Wort", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "\u00bbmein Kind, geh' eilig fort.", "tokens": ["\u00bb", "mein", "Kind", ",", "geh'", "ei\u00b7lig", "fort", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Auf, z\u00f6gere nicht mit S\u00e4umen,", "tokens": ["Auf", ",", "z\u00f6\u00b7ge\u00b7re", "nicht", "mit", "S\u00e4u\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "VVFIN", "PTKNEG", "APPR", "NN", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "In lieblos engen R\u00e4umen", "tokens": ["In", "lieb\u00b7los", "en\u00b7gen", "R\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Versteht man kein Gedicht", "tokens": ["Ver\u00b7steht", "man", "kein", "Ge\u00b7dicht"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und auch \u2013 Dich selber nicht!\u00ab", "tokens": ["Und", "auch", "\u2013", "Dich", "sel\u00b7ber", "nicht", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "$(", "PPER", "ADV", "PTKNEG", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}