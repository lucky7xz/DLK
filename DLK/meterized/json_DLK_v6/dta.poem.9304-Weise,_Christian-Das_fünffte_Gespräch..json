{"dta.poem.9304": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Das f\u00fcnffte Gespr\u00e4ch.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Kommt ihr jungfern an der Saale/", "tokens": ["Kommt", "ihr", "jung\u00b7fern", "an", "der", "Saa\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seht das neue weibgen an/", "tokens": ["Seht", "das", "neu\u00b7e", "weib\u00b7gen", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "VVFIN", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie sie sich zum ersten mahle", "tokens": ["Wie", "sie", "sich", "zum", "ers\u00b7ten", "mah\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In den handel schicken kan/", "tokens": ["In", "den", "han\u00b7del", "schi\u00b7cken", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "VMFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weil die nechst verflo\u00dfne nacht", "tokens": ["Weil", "die", "nechst", "ver\u00b7flo\u00df\u00b7ne", "nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADV", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sie der haube w\u00fcrdig macht.", "tokens": ["Sie", "der", "hau\u00b7be", "w\u00fcr\u00b7dig", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "2. Kan sie auch das lachen lassen", "tokens": ["Kan", "sie", "auch", "das", "la\u00b7chen", "las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "PDS", "VVINF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn man ihr daran gedenckt/", "tokens": ["Wenn", "man", "ihr", "da\u00b7ran", "ge\u00b7denckt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "PAV", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und numehr verdienter massen", "tokens": ["Und", "nu\u00b7mehr", "ver\u00b7dien\u00b7ter", "mas\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Jhr ein dutzent hauben schenckt?", "tokens": ["Ihr", "ein", "dut\u00b7zent", "hau\u00b7ben", "schenckt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Seht doch seht wie stellt sie sich", "tokens": ["Seht", "doch", "seht", "wie", "stellt", "sie", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "VVFIN", "KOKOM", "VVFIN", "PPER", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nur von aussen wunderlich.", "tokens": ["Nur", "von", "aus\u00b7sen", "wun\u00b7der\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "3. Ach die jungferlichen minen", "tokens": ["Ach", "die", "jung\u00b7fer\u00b7li\u00b7chen", "mi\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ITJ", "ART", "ADJA", "ADJA"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Sind nur auff den blossen schein/", "tokens": ["Sind", "nur", "auff", "den", "blos\u00b7sen", "schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Welche zwar zu etwas dienen/", "tokens": ["Wel\u00b7che", "zwar", "zu", "et\u00b7was", "die\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PTKA", "PIS", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn die leute bl\u00f6de seyn.", "tokens": ["Wenn", "die", "leu\u00b7te", "bl\u00f6\u00b7de", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJA", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dennoch wers errathen kan/", "tokens": ["Den\u00b7noch", "wers", "er\u00b7ra\u00b7then", "kan", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "VMFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Greifft den krantz mit willen an.", "tokens": ["Greifft", "den", "krantz", "mit", "wil\u00b7len", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "4. Fort du neu-gebacknes weibgen/", "tokens": ["Fort", "du", "neu\u00b7ge\u00b7back\u00b7nes", "weib\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gib den krantz mit willen hin/", "tokens": ["Gib", "den", "krantz", "mit", "wil\u00b7len", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "APPR", "NN", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und bedancke dich vors h\u00e4ubgen/", "tokens": ["Und", "be\u00b7dan\u00b7cke", "dich", "vors", "h\u00e4ub\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "ADJA", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als in welchem der gewinn", "tokens": ["Als", "in", "wel\u00b7chem", "der", "ge\u00b7winn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PRELS", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der verkaufften jungferschafft", "tokens": ["Der", "ver\u00b7kauff\u00b7ten", "jung\u00b7fer\u00b7schafft"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit gewiessem wucher hafft.", "tokens": ["Mit", "ge\u00b7wies\u00b7sem", "wu\u00b7cher", "hafft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "5. Sch\u00e4me dich nur vor den leuten/", "tokens": ["Sch\u00e4\u00b7me", "dich", "nur", "vor", "den", "leu\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und vor deinen g\u00e4sten nicht/", "tokens": ["Und", "vor", "dei\u00b7nen", "g\u00e4s\u00b7ten", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "PTKNEG", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn sie haben auch vor zeiten", "tokens": ["Denn", "sie", "ha\u00b7ben", "auch", "vor", "zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Solche possen angericht/", "tokens": ["Sol\u00b7che", "pos\u00b7sen", "an\u00b7ge\u00b7richt", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Oder w\u00fcnschen da\u00df der tag", "tokens": ["O\u00b7der", "w\u00fcn\u00b7schen", "da\u00df", "der", "tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KOUS", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Auch vor sie bald kommen mag.", "tokens": ["Auch", "vor", "sie", "bald", "kom\u00b7men", "mag."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "APPR", "PPER", "ADV", "VVFIN", "NE"], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "6. Alle welt besteht im lieben/", "tokens": ["Al\u00b7le", "welt", "be\u00b7steht", "im", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPRART", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Drum so hastu keine schuld:", "tokens": ["Drum", "so", "has\u00b7tu", "kei\u00b7ne", "schuld", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VAFIN", "PIAT", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hastu gleich die nacht vertrieben", "tokens": ["Has\u00b7tu", "gleich", "die", "nacht", "ver\u00b7trie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In dergleichen ungedult/", "tokens": ["In", "derg\u00b7lei\u00b7chen", "un\u00b7ge\u00b7dult", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Welche man verliebt und still/", "tokens": ["Wel\u00b7che", "man", "ver\u00b7liebt", "und", "still", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVPP", "KON", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Lieber thun als sagen will.", "tokens": ["Lie\u00b7ber", "thun", "als", "sa\u00b7gen", "will", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "KOKOM", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "7. Seht/ sie nicket mit dem kopffe/", "tokens": ["Seht", "/", "sie", "ni\u00b7cket", "mit", "dem", "kopf\u00b7fe", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "PPER", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gelt/ die rechnung trifft uns ein/", "tokens": ["Gelt", "/", "die", "rech\u00b7nung", "trifft", "uns", "ein", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ART", "NN", "VVFIN", "PPER", "ART", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der krantz wird auff dem zopffe", "tokens": ["Und", "der", "krantz", "wird", "auff", "dem", "zopf\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "APPR", "ART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Nun gewi\u00df verfallen seyn/", "tokens": ["Nun", "ge\u00b7wi\u00df", "ver\u00b7fal\u00b7len", "seyn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "VAINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Drum ihr jungfern kommt und sprecht", "tokens": ["Drum", "ihr", "jung\u00b7fern", "kommt", "und", "sprecht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "PPOSAT", "ADJA", "VVFIN", "KON", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Jhr das neue weiber-recht.", "tokens": ["Ihr", "das", "neu\u00b7e", "wei\u00b7ber\u00b7recht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}