{"dta.poem.5261": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung der in den Knospen ent-  \n haltenen Wunder.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Willkommen, liebste Fr\u00fchlings-Sprossen,", "tokens": ["Will\u00b7kom\u00b7men", ",", "liebs\u00b7te", "Fr\u00fch\u00b7lings\u00b7Spros\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die ihr theils gr\u00fcnet, theils schon bl\u00fcht;", "tokens": ["Die", "ihr", "theils", "gr\u00fc\u00b7net", ",", "theils", "schon", "bl\u00fcht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "$,", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jhr Knospen, die man ofters sieht,", "tokens": ["Ihr", "Knos\u00b7pen", ",", "die", "man", "of\u00b7ters", "sieht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Eh\u2019 man es meint, schon aufgeschlossen;", "tokens": ["Eh'", "man", "es", "meint", ",", "schon", "auf\u00b7ge\u00b7schlos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVFIN", "$,", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die ihr recht von einander springt,", "tokens": ["Die", "ihr", "recht", "von", "ein\u00b7an\u00b7der", "springt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPR", "PRF", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Und, sonderlich bey sch\u00f6nen Wetter,", "tokens": ["Und", ",", "son\u00b7der\u00b7lich", "bey", "sch\u00f6\u00b7nen", "Wet\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Uns eine Menge sch\u00f6ner Bl\u00e4tter,", "tokens": ["Uns", "ei\u00b7ne", "Men\u00b7ge", "sch\u00f6\u00b7ner", "Bl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Als lauter Wunder-Kinder, bringt!", "tokens": ["Als", "lau\u00b7ter", "Wun\u00b7der\u00b7Kin\u00b7der", ",", "bringt", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wie w\u00fcrd\u2019 uns euer Glantz und Schein", "tokens": ["Wie", "w\u00fcrd'", "uns", "eu\u00b7er", "Glantz", "und", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "So n\u00fctzlich und ersprie\u00dflich seyn,", "tokens": ["So", "n\u00fctz\u00b7lich", "und", "er\u00b7sprie\u00df\u00b7lich", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Er\u00f6fnetet ihr uns zugleich", "tokens": ["Er\u00b7\u00f6f\u00b7ne\u00b7tet", "ihr", "uns", "zu\u00b7gleich"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Welch eine wunderbare Kraft", "tokens": ["Welch", "ei\u00b7ne", "wun\u00b7der\u00b7ba\u00b7re", "Kraft"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "In euren Wesen wirckt! was euch", "tokens": ["In", "eu\u00b7ren", "We\u00b7sen", "wirckt", "!", "was", "euch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$.", "PWS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Und euren regen Wunder-Saft", "tokens": ["Und", "eu\u00b7ren", "re\u00b7gen", "Wun\u00b7der\u00b7Saft"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "So kr\u00e4ftig in Bewegung bringet!", "tokens": ["So", "kr\u00e4f\u00b7tig", "in", "Be\u00b7we\u00b7gung", "brin\u00b7get", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Ob ihr nur Sto\u00df-weis\u2019, oder nicht", "tokens": ["Ob", "ihr", "nur", "Sto\u00df\u00b7weis'", ",", "o\u00b7der", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "NN", "$,", "KON", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Vielmehr best\u00e4ndig, vorwerts dringet!", "tokens": ["Viel\u00b7mehr", "be\u00b7st\u00e4n\u00b7dig", ",", "vor\u00b7werts", "drin\u00b7get", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Ach, g\u00e4bet ihr uns doch Bericht,", "tokens": ["Ach", ",", "g\u00e4\u00b7bet", "ihr", "uns", "doch", "Be\u00b7richt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "PRF", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Woher der Farben Schmuck, der Bildung Zierlichkeit,", "tokens": ["Wo\u00b7her", "der", "Far\u00b7ben", "Schmuck", ",", "der", "Bil\u00b7dung", "Zier\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die Anmuth des Geschmacks in eurer Frucht entstehet!", "tokens": ["Die", "An\u00b7muth", "des", "Ge\u00b7schmacks", "in", "eu\u00b7rer", "Frucht", "ent\u00b7ste\u00b7het", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie kann sich in so rauhen Rinden", "tokens": ["Wie", "kann", "sich", "in", "so", "rau\u00b7hen", "Rin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PRF", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein solcher strenger Trieb, solch\u2019 eine Kunst, befinden,", "tokens": ["Ein", "sol\u00b7cher", "stren\u00b7ger", "Trieb", ",", "solch'", "ei\u00b7ne", "Kunst", ",", "be\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "$,", "PIAT", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die, besser als der Menschen Hand,", "tokens": ["Die", ",", "bes\u00b7ser", "als", "der", "Men\u00b7schen", "Hand", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADJD", "KOKOM", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ja aller menschlicher Verstand,", "tokens": ["Ja", "al\u00b7ler", "menschli\u00b7cher", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Geschickt, so liebliche Figuren zu formiren,", "tokens": ["Ge\u00b7schickt", ",", "so", "lieb\u00b7li\u00b7che", "Fi\u00b7gu\u00b7ren", "zu", "for\u00b7mi\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADV", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sie so zu f\u00e4rben und zu zieren!", "tokens": ["Sie", "so", "zu", "f\u00e4r\u00b7ben", "und", "zu", "zie\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wahrhaftig, wenn ich bey euch steh", "tokens": ["Wahr\u00b7haf\u00b7tig", ",", "wenn", "ich", "bey", "euch", "steh"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "KOUS", "PPER", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und eure Form und Farben-Pracht beseh,", "tokens": ["Und", "eu\u00b7re", "Form", "und", "Fa\u00b7rben\u00b7Pracht", "be\u00b7seh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Erstaunt mein Geist mit Recht und dencket:", "tokens": ["Er\u00b7staunt", "mein", "Geist", "mit", "Recht", "und", "den\u00b7cket", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "APPR", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kommt die\u00df von ungefehr? Ach nein:", "tokens": ["Kommt", "die\u00df", "von", "un\u00b7ge\u00b7fehr", "?", "Ach", "nein", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "APPR", "ADJD", "$.", "NN", "PTKANT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der diese Kraft in euch gesencket,", "tokens": ["Der", "die\u00b7se", "Kraft", "in", "euch", "ge\u00b7sen\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mu\u00df gro\u00df, mu\u00df eine GOttheit seyn!", "tokens": ["Mu\u00df", "gro\u00df", ",", "mu\u00df", "ei\u00b7ne", "Got\u00b7theit", "seyn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "$,", "VMFIN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}