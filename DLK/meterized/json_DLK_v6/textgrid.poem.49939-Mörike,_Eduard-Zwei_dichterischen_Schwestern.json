{"textgrid.poem.49939": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Zwei dichterischen Schwestern", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heut lehr ich euch die Regel der Son\u2013 \u2013.", "tokens": ["Heut", "lehr", "ich", "euch", "die", "Re\u00b7gel", "der", "Son", "\u2013", "\u2013", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ART", "NN", "ART", "NN", "$(", "$(", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Versucht gleich eins! Gewi\u00df, es wird ge\u2013 \u2013,", "tokens": ["Ver\u00b7sucht", "gleich", "eins", "!", "Ge\u00b7wi\u00df", ",", "es", "wird", "ge", "\u2013", "\u2013", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "ADV", "PIS", "$.", "PTKANT", "$,", "PPER", "VAFIN", "NE", "$(", "$(", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vier Reime h\u00fcbsch mit vieren zu versch\u2013 \u2013,", "tokens": ["Vier", "Rei\u00b7me", "h\u00fcbsch", "mit", "vie\u00b7ren", "zu", "ver\u00b7sch", "\u2013", "\u2013", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["CARD", "NN", "ADJD", "APPR", "PIS", "PTKA", "ADJD", "$(", "$(", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dann noch drei Paare, da\u00df man vierzehn h\u2013 \u2013.", "tokens": ["Dann", "noch", "drei", "Paa\u00b7re", ",", "da\u00df", "man", "vier\u00b7zehn", "h", "\u2013", "\u2013", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "ADV", "CARD", "NN", "$,", "KOUS", "PIS", "VVINF", "VVFIN", "$(", "$(", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "La\u00dft demnach an der vielgeteilten K\u2013 \u2013", "tokens": ["La\u00dft", "dem\u00b7nach", "an", "der", "viel\u00b7ge\u00b7teil\u00b7ten", "K", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PAV", "APPR", "ART", "ADJA", "NN", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Als Glied in Glied so einen Schlu\u00dfring sp\u2013 \u2013:", "tokens": ["Als", "Glied", "in", "Glied", "so", "ei\u00b7nen", "Schlu\u00df\u00b7ring", "sp", "\u2013", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KOUS", "NN", "APPR", "NN", "ADV", "ART", "NN", "VVFIN", "$(", "$(", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das mu\u00df alsdann wie pures Gold erk\u2013 \u2013;", "tokens": ["Das", "mu\u00df", "als\u00b7dann", "wie", "pu\u00b7res", "Gold", "erk", "\u2013", "\u2013", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PDS", "VMFIN", "ADV", "KOKOM", "ADJA", "NN", "VVFIN", "$(", "$(", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gewisse Herrn zwar h\u00e4ngen Klett an K\u2013 \u2013.", "tokens": ["Ge\u00b7wis\u00b7se", "Herrn", "zwar", "h\u00e4n\u00b7gen", "Klett", "an", "K", "\u2013", "\u2013", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADJA", "NN", "ADV", "ADJA", "NN", "APPR", "NE", "$(", "$(", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein solcher findet meine sch\u00f6nen N\u2013 \u2013", "tokens": ["Ein", "sol\u00b7cher", "fin\u00b7det", "mei\u00b7ne", "sch\u00f6\u00b7nen", "N", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PIAT", "VVFIN", "PPOSAT", "ADJA", "XY", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bei diesem Muster. \u00bbAh, Fr\u00e4ulein, Sie st\u2013 \u2013!\u00ab", "tokens": ["Bei", "die\u00b7sem", "Mus\u00b7ter", ".", "\u00bb", "Ah", ",", "Fr\u00e4u\u00b7lein", ",", "Sie", "st", "\u2013", "\u2013", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["APPR", "PDAT", "NN", "$.", "$(", "NN", "$,", "NN", "$,", "PPER", "VVFIN", "$(", "$(", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbo nein, Herr Graf, hier gilt es Silben z\u2013 \u2013.\u00bb", "tokens": ["\u00bb", "o", "nein", ",", "Herr", "Graf", ",", "hier", "gilt", "es", "Sil\u00b7ben", "z", "\u2013", "\u2013", ".", "\u00bb"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["$(", "FM", "PTKANT", "$,", "NN", "NE", "$,", "ADV", "VVFIN", "PPER", "NN", "NE", "$(", "$(", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "\u00bbwirklich! Doch wenn die Lauren selber d\u2013 \u2013,", "tokens": ["\u00bb", "wirk\u00b7lich", "!", "Doch", "wenn", "die", "Lau\u00b7ren", "sel\u00b7ber", "d", "\u2013", "\u2013", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "ADJD", "$.", "KON", "KOUS", "ART", "NN", "ADV", "XY", "$(", "$(", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Was soll Petrarca?\u00ab Der mag Str\u00fcmpfe str\u2013 \u2013.", "tokens": ["Was", "soll", "Pe\u00b7trar\u00b7ca", "?", "\u00ab", "Der", "mag", "Str\u00fcmp\u00b7fe", "str", "\u2013", "\u2013", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PWS", "VMFIN", "NE", "$.", "$(", "PDS", "VMFIN", "NN", "VVFIN", "$(", "$(", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Eins wie das andre ist f\u00fcr sch\u00f6ne S\u2013 \u2013.", "tokens": ["Eins", "wie", "das", "and\u00b7re", "ist", "f\u00fcr", "sch\u00f6\u00b7ne", "S", "\u2013", "\u2013", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PIS", "KOKOM", "ART", "ADJA", "VAFIN", "APPR", "ADJA", "NE", "$(", "$(", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}