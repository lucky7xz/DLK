{"textgrid.poem.23968": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Stiller Gang", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stille geh ich meinen Gang", "tokens": ["Stil\u00b7le", "geh", "ich", "mei\u00b7nen", "Gang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wiesen, W\u00e4lder, Felder lang.", "tokens": ["Wie\u00b7sen", ",", "W\u00e4l\u00b7der", ",", "Fel\u00b7der", "lang", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was ich h\u00f6re, was ich sehe,", "tokens": ["Was", "ich", "h\u00f6\u00b7re", ",", "was", "ich", "se\u00b7he", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df mir nichts vor\u00fcber wehe,", "tokens": ["Da\u00df", "mir", "nichts", "vor\u00b7\u00fc\u00b7ber", "we\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Fasse ichs in Verse ein,", "tokens": ["Fas\u00b7se", "ichs", "in", "Ver\u00b7se", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "APPR", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und die ganze Welt wird mein.", "tokens": ["Und", "die", "gan\u00b7ze", "Welt", "wird", "mein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sind wohl unscheinbare Dinge;", "tokens": ["Sind", "wohl", "un\u00b7schein\u00b7ba\u00b7re", "Din\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Mancher achtet sie geringe,", "tokens": ["Man\u00b7cher", "ach\u00b7tet", "sie", "ge\u00b7rin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein Nabob wird man nicht,", "tokens": ["Und", "ein", "Na\u00b7bob", "wird", "man", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PIS", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00e4ngt man solche Schmetterlinge.", "tokens": ["F\u00e4ngt", "man", "sol\u00b7che", "Schmet\u00b7ter\u00b7lin\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber manches wird Gedicht.", "tokens": ["A\u00b7ber", "man\u00b7ches", "wird", "Ge\u00b7dicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ist nicht mehr wie Blumen pfl\u00fccken,", "tokens": ["Ist", "nicht", "mehr", "wie", "Blu\u00b7men", "pfl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "KOKOM", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Linde sich ins Gr\u00fcne b\u00fccken,", "tokens": ["Lin\u00b7de", "sich", "ins", "Gr\u00fc\u00b7ne", "b\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ist nicht mehr als wie ein Lauschen,", "tokens": ["Ist", "nicht", "mehr", "als", "wie", "ein", "Lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "KOUS", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gr\u00fc\u00dfe mit den V\u00f6geln tauschen,", "tokens": ["Gr\u00fc\u00b7\u00dfe", "mit", "den", "V\u00f6\u00b7geln", "tau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist nichts, als bescheiden sein", "tokens": ["Ist", "nichts", ",", "als", "be\u00b7schei\u00b7den", "sein"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PIS", "$,", "KOUS", "ADJD", "VAINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit der Sch\u00f6nheit, mit dem Schein.", "tokens": ["Mit", "der", "Sch\u00f6n\u00b7heit", ",", "mit", "dem", "Schein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und ist dennoch tiefe Labe,", "tokens": ["Und", "ist", "den\u00b7noch", "tie\u00b7fe", "La\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Dauernde und reiche Habe:", "tokens": ["Dau\u00b7ern\u00b7de", "und", "rei\u00b7che", "Ha\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}}, "stanza.6": {"line.1": {"text": "Wer die Sch\u00f6nheit sich erfa\u00dft,", "tokens": ["Wer", "die", "Sch\u00f6n\u00b7heit", "sich", "er\u00b7fa\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schenkt der Welt den Rest mit Lachen,", "tokens": ["Schenkt", "der", "Welt", "den", "Rest", "mit", "La\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "All die plumpen Siebensachen,", "tokens": ["All", "die", "plum\u00b7pen", "Sie\u00b7ben\u00b7sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat die G\u00f6tter selbst zu Gast.", "tokens": ["Hat", "die", "G\u00f6t\u00b7ter", "selbst", "zu", "Gast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}