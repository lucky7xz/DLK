{"dta.poem.18245": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "24.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Verklingen h\u00f6ren \u00fcber Raum und Zeit", "tokens": ["Ver\u00b7klin\u00b7gen", "h\u00f6\u00b7ren", "\u00fc\u00b7ber", "Raum", "und", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVINF", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und schimmernd eingesargt von wei\u00dfen Flocken", "tokens": ["Und", "schim\u00b7mernd", "ein\u00b7ge\u00b7sargt", "von", "wei\u00b7\u00dfen", "Flo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVPP", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Versinkt es in die Ewigkeit.", "tokens": ["Ver\u00b7sinkt", "es", "in", "die", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch leuchtend aus dem Schoo\u00df der Winternacht", "tokens": ["Doch", "leuch\u00b7tend", "aus", "dem", "Schoo\u00df", "der", "Win\u00b7ter\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ringt schon das neue seine jungen Glieder", "tokens": ["Ringt", "schon", "das", "neu\u00b7e", "sei\u00b7ne", "jun\u00b7gen", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und tr\u00e4umt, die Erde sei mit ihm erwacht,", "tokens": ["Und", "tr\u00e4umt", ",", "die", "Er\u00b7de", "sei", "mit", "ihm", "er\u00b7wacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ART", "NN", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Geweckt vom s\u00fc\u00dfen Klang der Fr\u00fchlingslieder.", "tokens": ["Ge\u00b7weckt", "vom", "s\u00fc\u00b7\u00dfen", "Klang", "der", "Fr\u00fch\u00b7lings\u00b7lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch schau, wie fr\u00f6stelnd es die wei\u00dfe Decke", "tokens": ["Doch", "schau", ",", "wie", "fr\u00f6s\u00b7telnd", "es", "die", "wei\u00b7\u00dfe", "De\u00b7cke"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "PWAV", "ADJD", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schon wieder \u00fcber seine Glieder zieht,", "tokens": ["Schon", "wie\u00b7der", "\u00fc\u00b7ber", "sei\u00b7ne", "Glie\u00b7der", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Weil es von Eis umglitzert Hag und Hecke", "tokens": ["Weil", "es", "von", "Eis", "um\u00b7glit\u00b7zert", "Hag", "und", "He\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVPP", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und ach, kein einzig Veilchen sieht!", "tokens": ["Und", "ach", ",", "kein", "ein\u00b7zig", "Veil\u00b7chen", "sieht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "PIAT", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch fasse neue Hoffnung, neues Jahr,", "tokens": ["Doch", "fas\u00b7se", "neu\u00b7e", "Hoff\u00b7nung", ",", "neu\u00b7es", "Jahr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Denn so wie dir ist's jedem noch ergangen", "tokens": ["Denn", "so", "wie", "dir", "ist's", "je\u00b7dem", "noch", "er\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "KOKOM", "PPER", "VAFIN", "PIS", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Aus deiner ewigen Geschwisterschaar;", "tokens": ["Aus", "dei\u00b7ner", "e\u00b7wi\u00b7gen", "Ge\u00b7schwis\u00b7ter\u00b7schaar", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und doch, der Lenz kam immer noch gegangen!", "tokens": ["Und", "doch", ",", "der", "Lenz", "kam", "im\u00b7mer", "noch", "ge\u00b7gan\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ART", "NN", "VVFIN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Noch herrscht der Tod; doch wenig Wochen sp\u00e4ter", "tokens": ["Noch", "herrscht", "der", "Tod", ";", "doch", "we\u00b7nig", "Wo\u00b7chen", "sp\u00e4\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "ADV", "PIAT", "NN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und hoch im Winde schwankt das junge Ried,", "tokens": ["Und", "hoch", "im", "Win\u00b7de", "schwankt", "das", "jun\u00b7ge", "Ried", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dann singt ein Lerchenchor im blauen Aether", "tokens": ["Dann", "singt", "ein", "Ler\u00b7chen\u00b7chor", "im", "blau\u00b7en", "A\u00b7e\u00b7ther"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.4": {"text": "Des Fr\u00fchlings Auferstehungslied.", "tokens": ["Des", "Fr\u00fch\u00b7lings", "Auf\u00b7er\u00b7ste\u00b7hungs\u00b7lied", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wonniger, als du dir je ertr\u00e4umt,", "tokens": ["Und", "won\u00b7ni\u00b7ger", ",", "als", "du", "dir", "je", "er\u00b7tr\u00e4umt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "KOUS", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wird die Natur dir noch ihr Herz erschlie\u00dfen,", "tokens": ["Wird", "die", "Na\u00b7tur", "dir", "noch", "ihr", "Herz", "er\u00b7schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wenn von des Sommers Aehrengold ums\u00e4umt,", "tokens": ["Wenn", "von", "des", "Som\u00b7mers", "A\u00b7eh\u00b7ren\u00b7gold", "um\u00b7s\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.8": {"text": "Des Lebens Quellen rauschend dich umflie\u00dfen.", "tokens": ["Des", "Le\u00b7bens", "Quel\u00b7len", "rau\u00b7schend", "dich", "um\u00b7flie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch was in dieser Welt dich auch entz\u00fcckte,", "tokens": ["Doch", "was", "in", "die\u00b7ser", "Welt", "dich", "auch", "ent\u00b7z\u00fcck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "PDAT", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vergilt es ", "tokens": ["Ver\u00b7gilt", "es"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Und jede Bl\u00fcthe, die im Lenz dich schm\u00fcckte,", "tokens": ["Und", "je\u00b7de", "Bl\u00fc\u00b7the", ",", "die", "im", "Lenz", "dich", "schm\u00fcck\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gieb uns im Herbst als reife Frucht!", "tokens": ["Gieb", "uns", "im", "Herbst", "als", "rei\u00b7fe", "Frucht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "KOUS", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und schl\u00e4gt dereinst die Stunde deines Seins,", "tokens": ["Und", "schl\u00e4gt", "de\u00b7reinst", "die", "Stun\u00b7de", "dei\u00b7nes", "Seins", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Dann sei dein Segen f\u00fcr das Wunschgedeihen,", "tokens": ["Dann", "sei", "dein", "Se\u00b7gen", "f\u00fcr", "das", "Wunschge\u00b7dei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Wenn wir statt eines todten Marmorsteins,", "tokens": ["Wenn", "wir", "statt", "ei\u00b7nes", "tod\u00b7ten", "Mar\u00b7mor\u00b7steins", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Dir ein lebendiges Ged\u00e4chtni\u00df weihen!", "tokens": ["Dir", "ein", "le\u00b7ben\u00b7di\u00b7ges", "Ge\u00b7d\u00e4cht\u00b7ni\u00df", "wei\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}