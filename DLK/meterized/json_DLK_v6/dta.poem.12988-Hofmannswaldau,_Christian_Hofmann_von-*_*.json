{"dta.poem.12988": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "* *", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es schmeckt nach eigensinn, nicht aber nach verstande,", "tokens": ["Es", "schmeckt", "nach", "ei\u00b7gen\u00b7sinn", ",", "nicht", "a\u00b7ber", "nach", "ver\u00b7stan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "PTKNEG", "ADV", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn man die thoren nicht vor augen leiden kan.", "tokens": ["Wenn", "man", "die", "tho\u00b7ren", "nicht", "vor", "au\u00b7gen", "lei\u00b7den", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "PTKNEG", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein Pharis\u00e4er nimmt zwar keinen s\u00fcnder an:", "tokens": ["Ein", "Pha\u00b7ri\u00b7s\u00e4\u00b7er", "nimmt", "zwar", "kei\u00b7nen", "s\u00fcn\u00b7der", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PIAT", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wer nicht so denckt, wie er, den w\u00fcnscht er aus dem lande;", "tokens": ["Wer", "nicht", "so", "denckt", ",", "wie", "er", ",", "den", "w\u00fcnscht", "er", "aus", "dem", "lan\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "ADV", "VVFIN", "$,", "PWAV", "PPER", "$,", "ART", "VVFIN", "PPER", "APPR", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch wer verst\u00e4ndig ist, flieht ihren umgang nicht,", "tokens": ["Doch", "wer", "ver\u00b7st\u00e4n\u00b7dig", "ist", ",", "flieht", "ih\u00b7ren", "um\u00b7gang", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADJD", "VAFIN", "$,", "VVFIN", "PPOSAT", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "So k\u00f6nnen sie von ihm, er auch von ihnen lernen.", "tokens": ["So", "k\u00f6n\u00b7nen", "sie", "von", "ihm", ",", "er", "auch", "von", "ih\u00b7nen", "ler\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "PPER", "$,", "PPER", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wer von den thoren sich best\u00e4ndig will entfernen,", "tokens": ["Wer", "von", "den", "tho\u00b7ren", "sich", "be\u00b7st\u00e4n\u00b7dig", "will", "ent\u00b7fer\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "PRF", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der seh\u2019 erst, da\u00df er sich vor seiner selbst entbricht.", "tokens": ["Der", "seh'", "erst", ",", "da\u00df", "er", "sich", "vor", "sei\u00b7ner", "selbst", "ent\u00b7bricht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "$,", "KOUS", "PPER", "PRF", "APPR", "PPOSAT", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}