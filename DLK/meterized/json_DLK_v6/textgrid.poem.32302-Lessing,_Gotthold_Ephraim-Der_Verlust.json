{"textgrid.poem.32302": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "Der Verlust", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alles ging f\u00fcr mich verloren,", "tokens": ["Al\u00b7les", "ging", "f\u00fcr", "mich", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als ich Sylvien verlor.", "tokens": ["Als", "ich", "Syl\u00b7vi\u00b7en", "ver\u00b7lor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Du nur gingst nicht mit verloren,", "tokens": ["Du", "nur", "gingst", "nicht", "mit", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PTKNEG", "APPR", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Liebe, da ich sie verlor!", "tokens": ["Lie\u00b7be", ",", "da", "ich", "sie", "ver\u00b7lor", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}