{"textgrid.poem.47465": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[ob feindselige Winde]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ob feindselige Winde", "tokens": ["Ob", "feind\u00b7se\u00b7li\u00b7ge", "Win\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schreckhaft tosen, o gr\u00e4me dich nicht!", "tokens": ["Schreck\u00b7haft", "to\u00b7sen", ",", "o", "gr\u00e4\u00b7me", "dich", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "FM", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Denn hold werden im Lenze", "tokens": ["Denn", "hold", "wer\u00b7den", "im", "Len\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAINF", "APPRART", "NN"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.4": {"text": "L\u00fcftlein kosen, o gr\u00e4me dich nicht!", "tokens": ["L\u00fcft\u00b7lein", "ko\u00b7sen", ",", "o", "gr\u00e4\u00b7me", "dich", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "FM", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ob erstorbne Geb\u00fcsche", "tokens": ["Ob", "ers\u00b7torb\u00b7ne", "Ge\u00b7b\u00fc\u00b7sche"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADJA", "NN"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.2": {"text": "Rings dein Auge beleidigen,", "tokens": ["Rings", "dein", "Au\u00b7ge", "be\u00b7lei\u00b7di\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.3": {"text": "Aus dem Tode lebendig", "tokens": ["Aus", "dem", "To\u00b7de", "le\u00b7ben\u00b7dig"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD"], "meter": "--+-+--", "measure": "anapaest.init"}, "line.4": {"text": "Bl\u00fch'n einst Rosen, o gr\u00e4me dich nicht!", "tokens": ["Bl\u00fch'n", "einst", "Ro\u00b7sen", ",", "o", "gr\u00e4\u00b7me", "dich", "nicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "NN", "$,", "FM", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Ob durch stachlige W\u00fcste", "tokens": ["Ob", "durch", "stach\u00b7li\u00b7ge", "W\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Hin zur Kaba die Reise geht,", "tokens": ["Hin", "zur", "Ka\u00b7ba", "die", "Rei\u00b7se", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "La\u00df dich Dornen und Disteln", "tokens": ["La\u00df", "dich", "Dor\u00b7nen", "und", "Dis\u00b7teln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "NN", "KON", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Nicht erbosen, o gr\u00e4me dich nicht!", "tokens": ["Nicht", "er\u00b7bo\u00b7sen", ",", "o", "gr\u00e4\u00b7me", "dich", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$,", "FM", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Ob gl\u00fcckseliger Heimath", "tokens": ["Ob", "gl\u00fcck\u00b7se\u00b7li\u00b7ger", "Hei\u00b7math"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Jussuf grausam entrissen weint,", "tokens": ["Jus\u00b7suf", "grau\u00b7sam", "ent\u00b7ris\u00b7sen", "weint", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVPP", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Hoch in Glorie prangt einst,", "tokens": ["Hoch", "in", "Glo\u00b7rie", "prangt", "einst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Der versto\u00dfen, o gr\u00e4me dich nicht!", "tokens": ["Der", "ver\u00b7sto\u00b7\u00dfen", ",", "o", "gr\u00e4\u00b7me", "dich", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVINF", "$,", "FM", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Alles kreiset und wechselt,", "tokens": ["Al\u00b7les", "krei\u00b7set", "und", "wech\u00b7selt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Auch dein Leiden, es wandelt sich;", "tokens": ["Auch", "dein", "Lei\u00b7den", ",", "es", "wan\u00b7delt", "sich", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PRF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Nicht erliege den herben", "tokens": ["Nicht", "er\u00b7lie\u00b7ge", "den", "her\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Schicksalslosen, o gr\u00e4me dich nicht!", "tokens": ["Schick\u00b7sals\u00b7lo\u00b7sen", ",", "o", "gr\u00e4\u00b7me", "dich", "nicht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}}}}