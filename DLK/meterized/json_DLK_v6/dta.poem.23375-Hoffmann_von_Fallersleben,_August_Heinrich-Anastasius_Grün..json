{"dta.poem.23375": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Anastasius Gr\u00fcn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es stand ein Baum in Oesterreich,", "tokens": ["Es", "stand", "ein", "Baum", "in", "O\u00b7es\u00b7ter\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Der gr\u00fcnt' und bl\u00fchte manches Jahr", "tokens": ["Der", "gr\u00fcnt'", "und", "bl\u00fch\u00b7te", "man\u00b7ches", "Jahr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcrwahr so sch\u00f6n und wunderbar.", "tokens": ["F\u00fcr\u00b7wahr", "so", "sch\u00f6n", "und", "wun\u00b7der\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Manch Vogel sa\u00df darin und sang,", "tokens": ["Manch", "Vo\u00b7gel", "sa\u00df", "da\u00b7rin", "und", "sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PAV", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df weithin Berg und Thal erklang.", "tokens": ["Da\u00df", "weit\u00b7hin", "Berg", "und", "Thal", "er\u00b7klang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es hatte jeder deutsche Mann", "tokens": ["Es", "hat\u00b7te", "je\u00b7der", "deut\u00b7sche", "Mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Recht seine Lust und Freude dran.", "tokens": ["Recht", "sei\u00b7ne", "Lust", "und", "Freu\u00b7de", "dran", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein Doppeladler flog einmal", "tokens": ["Ein", "Dop\u00b7pe\u00b7lad\u00b7ler", "flog", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu diesem Wunderbaum' hinan,", "tokens": ["Zu", "die\u00b7sem", "Wun\u00b7der\u00b7baum'", "hi\u00b7nan", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Band einen g\u00fcldnen Schl\u00fcssel dran:", "tokens": ["Band", "ei\u00b7nen", "g\u00fcld\u00b7nen", "Schl\u00fcs\u00b7sel", "dran", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da wurden seine Bl\u00fcthen taub", "tokens": ["Da", "wur\u00b7den", "sei\u00b7ne", "Bl\u00fc\u00b7then", "taub"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "CARD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und falbe ward sein gr\u00fcnes Laub;", "tokens": ["Und", "fal\u00b7be", "ward", "sein", "gr\u00fc\u00b7nes", "Laub", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Schaar der lieben V\u00f6glein schied,", "tokens": ["Die", "Schaar", "der", "lie\u00b7ben", "V\u00f6\u00b7glein", "schied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "F\u00fcr immer schwieg ihr Sang und Lied.", "tokens": ["F\u00fcr", "im\u00b7mer", "schwieg", "ihr", "Sang", "und", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}