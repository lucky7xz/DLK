{"textgrid.poem.42727": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "[es waren einmal zwei Gummischuh]", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es waren einmal zwei Gummischuh,", "tokens": ["Es", "wa\u00b7ren", "ein\u00b7mal", "zwei", "Gum\u00b7mi\u00b7schuh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "CARD", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die waren stehen gelassen.", "tokens": ["Die", "wa\u00b7ren", "ste\u00b7hen", "ge\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ihr Herr, der suchte sie immerzu", "tokens": ["Ihr", "Herr", ",", "der", "such\u00b7te", "sie", "im\u00b7mer\u00b7zu"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "VVFIN", "PPER", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und konnte sie nirgend fassen.", "tokens": ["Und", "konn\u00b7te", "sie", "nir\u00b7gend", "fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Er suchte sie nah und suchte sie fern,", "tokens": ["Er", "such\u00b7te", "sie", "nah", "und", "such\u00b7te", "sie", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "KON", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er suchte sie vorn und hinten,", "tokens": ["Er", "such\u00b7te", "sie", "vorn", "und", "hin\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "KON", "ADV", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und die Gummischuhe suchten den Herrn", "tokens": ["Und", "die", "Gum\u00b7mi\u00b7schu\u00b7he", "such\u00b7ten", "den", "Herrn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und konnten ihn nirgend finden.", "tokens": ["Und", "konn\u00b7ten", "ihn", "nir\u00b7gend", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Der Herr durchsuchte die ganze Welt;", "tokens": ["Der", "Herr", "durch\u00b7such\u00b7te", "die", "gan\u00b7ze", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Gummischuhe desgleichen,", "tokens": ["Die", "Gum\u00b7mi\u00b7schu\u00b7he", "des\u00b7glei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und wenn die Sache so weiter geht,", "tokens": ["Und", "wenn", "die", "Sa\u00b7che", "so", "wei\u00b7ter", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "So werden sie nie sich erreichen.", "tokens": ["So", "wer\u00b7den", "sie", "nie", "sich", "er\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PRF", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}