{"textgrid.poem.47648": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[sch\u00f6n, wie Thirza, bist du]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6n, wie Thirza, bist du,", "tokens": ["Sch\u00f6n", ",", "wie", "Thir\u00b7za", ",", "bist", "du", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "NE", "$,", "VAFIN", "PPER", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Wonnevollen Anblicks,", "tokens": ["Won\u00b7ne\u00b7vol\u00b7len", "An\u00b7blicks", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wie die Pracht Jerusalems,", "tokens": ["Wie", "die", "Pracht", "Je\u00b7ru\u00b7sa\u00b7lems", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und wie kriegerische", "tokens": ["Und", "wie", "krie\u00b7ge\u00b7ri\u00b7sche"], "token_info": ["word", "word", "word"], "pos": ["KON", "PWAV", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Heldenheere furchtbar.", "tokens": ["Hel\u00b7den\u00b7hee\u00b7re", "furcht\u00b7bar", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Wende, weh, sie t\u00f6dten,", "tokens": ["Wen\u00b7de", ",", "weh", ",", "sie", "t\u00f6d\u00b7ten", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PTKVZ", "$,", "PPER", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wende deine Blicke,", "tokens": ["Wen\u00b7de", "dei\u00b7ne", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ende meinem Herzen", "tokens": ["En\u00b7de", "mei\u00b7nem", "Her\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Diesen Zauberbann!", "tokens": ["Die\u00b7sen", "Zau\u00b7ber\u00b7bann", "!"], "token_info": ["word", "word", "punct"], "pos": ["PDAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Aufgel\u00f6st in Sehnsucht,", "tokens": ["Auf\u00b7ge\u00b7l\u00f6st", "in", "Sehn\u00b7sucht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Nimmermehr begn\u00fcgter,", "tokens": ["Nim\u00b7mer\u00b7mehr", "be\u00b7gn\u00fcg\u00b7ter", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Schmacht' ich hin und sterbe,", "tokens": ["Schmacht'", "ich", "hin", "und", "ster\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Sterb' an einem Gl\u00fccke,", "tokens": ["Sterb'", "an", "ei\u00b7nem", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Das die Brust, die schwache,", "tokens": ["Das", "die", "Brust", ",", "die", "schwa\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Menschliche, nicht fassen,", "tokens": ["Menschli\u00b7che", ",", "nicht", "fas\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PTKNEG", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Nicht ertragen kann.", "tokens": ["Nicht", "er\u00b7tra\u00b7gen", "kann", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}