{"textgrid.poem.50677": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Darfst du darum auf Chlorus schm\u00e4hn,", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Darfst du darum auf Chlorus schm\u00e4hn,", "tokens": ["Darfst", "du", "da\u00b7rum", "auf", "Chlo\u00b7rus", "schm\u00e4hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PAV", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df er vergebens hofft auf, was nicht kann geschehn?", "tokens": ["Da\u00df", "er", "ver\u00b7ge\u00b7bens", "hofft", "auf", ",", "was", "nicht", "kann", "ge\u00b7schehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "PTKVZ", "$,", "PRELS", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Stell' ein dein nichtiges Geschw\u00e4tze,", "tokens": ["Stell'", "ein", "dein", "nich\u00b7ti\u00b7ges", "Ge\u00b7schw\u00e4t\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Jedermann zum Zorn bewegt,", "tokens": ["Das", "Je\u00b7der\u00b7mann", "zum", "Zorn", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und denke, da\u00df die Welt auf ", "tokens": ["Und", "den\u00b7ke", ",", "da\u00df", "die", "Welt", "auf"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ART", "NN", "APPR"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}}}}}