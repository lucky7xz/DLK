{"textgrid.poem.62195": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[unschuldig verurteilt sein]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unschuldig verurteilt sein,", "tokens": ["Un\u00b7schul\u00b7dig", "ver\u00b7ur\u00b7teilt", "sein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VAINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ist ein Ungl\u00fcck, das nicht klein,", "tokens": ["Ist", "ein", "Un\u00b7gl\u00fcck", ",", "das", "nicht", "klein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "PRELS", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch nat\u00fcrlich ist es fast", "tokens": ["Doch", "na\u00b7t\u00fcr\u00b7lich", "ist", "es", "fast"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Trifft den Richter keine Last \u2013", "tokens": ["Trifft", "den", "Rich\u00b7ter", "kei\u00b7ne", "Last", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PIAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Keine Schuld \u2013 ach unfehlbar!", "tokens": ["Kei\u00b7ne", "Schuld", "\u2013", "ach", "un\u00b7fehl\u00b7bar", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$(", "XY", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ob ein Richter stets es war?", "tokens": ["Ob", "ein", "Rich\u00b7ter", "stets", "es", "war", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "PPER", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Straflos mu\u00df ein Richter sein?", "tokens": ["Straf\u00b7los", "mu\u00df", "ein", "Rich\u00b7ter", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ART", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Darauf sagt ein Jeder nein!", "tokens": ["Da\u00b7rauf", "sagt", "ein", "Je\u00b7der", "nein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "PIS", "PTKANT", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}}}}