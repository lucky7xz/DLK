{"textgrid.poem.30178": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "92.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K\u00fcmt vom Weinen, k\u00fcmt vom Weihen, k\u00fcmt vom Wein Weinachten her?", "tokens": ["K\u00fcmt", "vom", "Wei\u00b7nen", ",", "k\u00fcmt", "vom", "Wei\u00b7hen", ",", "k\u00fcmt", "vom", "Wein", "Wei\u00b7nach\u00b7ten", "her", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,", "VVFIN", "APPRART", "NN", "$,", "VVFIN", "APPRART", "NN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "So wie ieder ihm sie brauchte, kamen sie ihm ohn Gefehr.", "tokens": ["So", "wie", "ie\u00b7der", "ihm", "sie", "brauch\u00b7te", ",", "ka\u00b7men", "sie", "ihm", "ohn", "Ge\u00b7fehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PIAT", "PPER", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PPER", "APPR", "NN", "$."], "meter": "--+-+-+-+-+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Weil der Welt-Erl\u00f6ser drinnen in die Welt ist kummen ein,", "tokens": ["Weil", "der", "Welt\u00b7Er\u00b7l\u00f6\u00b7ser", "drin\u00b7nen", "in", "die", "Welt", "ist", "kum\u00b7men", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "APPR", "ART", "NN", "VAFIN", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Solten sie Frei-nachten heissen, solten sie Freu-nachten seyn.", "tokens": ["Sol\u00b7ten", "sie", "Frei\u00b7nach\u00b7ten", "heis\u00b7sen", ",", "sol\u00b7ten", "sie", "Freu\u00b7nach\u00b7ten", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "VVINF", "$,", "VMFIN", "PPER", "NN", "VAINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}