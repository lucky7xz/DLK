{"textgrid.poem.43320": {"metadata": {"author": {"name": "Gerstenberg, Heinrich Wilhelm von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du Amme mu\u00dft das M\u00e4dchen strafen;", "genre": "verse", "period": "N.A.", "pub_year": 1780, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Amme mu\u00dft das M\u00e4dchen strafen;", "tokens": ["Du", "Am\u00b7me", "mu\u00dft", "das", "M\u00e4d\u00b7chen", "stra\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VMFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich leide sie, das sag ich dir,", "tokens": ["Ich", "lei\u00b7de", "sie", ",", "das", "sag", "ich", "dir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PDS", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht in dem Bette mehr bey mir.", "tokens": ["Nicht", "in", "dem", "Bet\u00b7te", "mehr", "bey", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Nur immer seh ich hin nach ihr,", "tokens": ["Nur", "im\u00b7mer", "seh", "ich", "hin", "nach", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und kann daf\u00fcr nicht schlafen!", "tokens": ["Und", "kann", "da\u00b7f\u00fcr", "nicht", "schla\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PAV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}