{"textgrid.poem.30582": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "96.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00c4rtzte bauen ihre M\u00fchlen an die Menschen-Fl\u00fcsse;", "tokens": ["\u00c4rtz\u00b7te", "bau\u00b7en", "ih\u00b7re", "M\u00fch\u00b7len", "an", "die", "Men\u00b7schen\u00b7F\u00b7l\u00fcs\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Selten sind sonst Wasserm\u00fchlen, die man so geniesse.", "tokens": ["Sel\u00b7ten", "sind", "sonst", "Was\u00b7ser\u00b7m\u00fch\u00b7len", ",", "die", "man", "so", "ge\u00b7nies\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "NN", "$,", "PRELS", "PIS", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}}}}