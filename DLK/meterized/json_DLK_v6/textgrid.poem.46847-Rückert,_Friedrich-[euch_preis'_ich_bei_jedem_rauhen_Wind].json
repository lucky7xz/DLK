{"textgrid.poem.46847": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[euch preis' ich bei jedem rauhen Wind]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Euch preis' ich bei jedem rauhen Wind,", "tokens": ["Euch", "preis'", "ich", "bei", "je\u00b7dem", "rau\u00b7hen", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Da\u00df er euch nicht ber\u00fchret,", "tokens": ["Da\u00df", "er", "euch", "nicht", "be\u00b7r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und klag' euch bei jedem L\u00fcftchen lind,", "tokens": ["Und", "klag'", "euch", "bei", "je\u00b7dem", "L\u00fcft\u00b7chen", "lind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PIAT", "NN", "ADJD", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Da\u00df ihrs im Grab nicht sp\u00fcret.", "tokens": ["Da\u00df", "ihrs", "im", "Grab", "nicht", "sp\u00fc\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPRART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Euch preis' ich bei jedem Schmerz in der Brust,", "tokens": ["Euch", "preis'", "ich", "bei", "je\u00b7dem", "Schmerz", "in", "der", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Da\u00df ihr ihm seid entr\u00fccket,", "tokens": ["Da\u00df", "ihr", "ihm", "seid", "ent\u00b7r\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und klag' euch bei jeder Fr\u00fchlingslust,", "tokens": ["Und", "klag'", "euch", "bei", "je\u00b7der", "Fr\u00fch\u00b7lings\u00b7lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da\u00df euch kein Kranz mehr schm\u00fccket.", "tokens": ["Da\u00df", "euch", "kein", "Kranz", "mehr", "schm\u00fc\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und rechn' ich Leid und Freude voll", "tokens": ["Und", "rechn'", "ich", "Leid", "und", "Freu\u00b7de", "voll"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "NN", "KON", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zusammen in diesen Tagen,", "tokens": ["Zu\u00b7sam\u00b7men", "in", "die\u00b7sen", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "So wei\u00df ich nicht, ob ich euch soll", "tokens": ["So", "wei\u00df", "ich", "nicht", ",", "ob", "ich", "euch", "soll"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "PPER", "PPER", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gl\u00fcckpreisen oder beklagen.", "tokens": ["Gl\u00fcck\u00b7prei\u00b7sen", "o\u00b7der", "be\u00b7kla\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}