{"dta.poem.16830": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "189.  Der Anfang findt das Ende.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Wann GOtt sich mit mir Mensch vereinigt und ver-", "tokens": ["Wann", "Gott", "sich", "mit", "mir", "Mensch", "ver\u00b7ei\u00b7nigt", "und", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "PRF", "APPR", "PPER", "NN", "VVPP", "KON", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "bindt/", "tokens": ["bindt", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "-", "measure": "single.down"}, "line.3": {"text": "So siht der Anbegin da\u00df er sein Ende findt.", "tokens": ["So", "siht", "der", "An\u00b7be\u00b7gin", "da\u00df", "er", "sein", "En\u00b7de", "findt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}