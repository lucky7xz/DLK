{"dta.poem.7160": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung des Schlafs,  \n als  \n eine G\u00f6ttliche Wohlthat,  \n bey dem 1728sten Jahres-Wechsel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich hab\u2019 aufs neu Gelegenheit", "tokens": ["Ich", "hab'", "aufs", "neu", "Ge\u00b7le\u00b7gen\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bey diesem Zufall \u00fcberkommen,", "tokens": ["Bey", "die\u00b7sem", "Zu\u00b7fall", "\u00fc\u00b7ber\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Des Sch\u00f6pffers Weisheit, Macht und G\u00fctigkeit,", "tokens": ["Des", "Sch\u00f6pf\u00b7fers", "Weis\u00b7heit", ",", "Macht", "und", "G\u00fc\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Er in Bildungen der C\u00f6rper zeigt, zu preisen.", "tokens": ["Die", "Er", "in", "Bil\u00b7dun\u00b7gen", "der", "C\u00f6r\u00b7per", "zeigt", ",", "zu", "prei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "ART", "NN", "VVFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Wie k\u00f6nnt Er g\u00fctiger sich gegen uns erweisen,", "tokens": ["Wie", "k\u00f6nnt", "Er", "g\u00fc\u00b7ti\u00b7ger", "sich", "ge\u00b7gen", "uns", "er\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADJA", "PRF", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Als da Er wunderbar den Knochen", "tokens": ["Als", "da", "Er", "wun\u00b7der\u00b7bar", "den", "Kno\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wollt\u2019 eine Eigenschafft ertheilen,", "tokens": ["Wollt'", "ei\u00b7ne", "Ei\u00b7gen\u00b7schafft", "er\u00b7thei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df, wenn sie wo, von ungefehr gebrochen;", "tokens": ["Da\u00df", ",", "wenn", "sie", "wo", ",", "von", "un\u00b7ge\u00b7fehr", "ge\u00b7bro\u00b7chen", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PPER", "PWAV", "$,", "APPR", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Sie selbst geschickt, sich wiederum zu heilen,", "tokens": ["Sie", "selbst", "ge\u00b7schickt", ",", "sich", "wie\u00b7de\u00b7rum", "zu", "hei\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "$,", "PRF", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Ohn da\u00df mit wilder Pein und schmertzlichen Beschwerden,", "tokens": ["Ohn", "da\u00df", "mit", "wil\u00b7der", "Pein", "und", "schmertz\u00b7li\u00b7chen", "Be\u00b7schwer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So Haut als Fleisch erst d\u00fcrfft zerschnitten werden.", "tokens": ["So", "Haut", "als", "Fleisch", "erst", "d\u00fcrfft", "zer\u00b7schnit\u00b7ten", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KOUS", "NN", "ADV", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Was dieses f\u00fcr ein Gl\u00fcck, wei\u00df der allein zu sch\u00e4tzen,", "tokens": ["Was", "die\u00b7ses", "f\u00fcr", "ein", "Gl\u00fcck", ",", "wei\u00df", "der", "al\u00b7lein", "zu", "sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "APPR", "ART", "NN", "$,", "VVFIN", "ART", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Den Zuf\u00e4ll\u2019 ohngefehr in solchen Stand versetzen.", "tokens": ["Den", "Zu\u00b7f\u00e4ll'", "ohn\u00b7ge\u00b7fehr", "in", "sol\u00b7chen", "Stand", "ver\u00b7set\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}