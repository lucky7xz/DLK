{"textgrid.poem.61351": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein gut Gedicht ist wie ein sch\u00f6ner Traum,", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein gut Gedicht ist wie ein sch\u00f6ner Traum,", "tokens": ["Ein", "gut", "Ge\u00b7dicht", "ist", "wie", "ein", "sch\u00f6\u00b7ner", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VAFIN", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es zieht dich in sich, und du merkst es kaum;", "tokens": ["Es", "zieht", "dich", "in", "sich", ",", "und", "du", "merkst", "es", "kaum", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PRF", "$,", "KON", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es tr\u00e4gt dich m\u00fchlos fort durch Raum und Zeit,", "tokens": ["Es", "tr\u00e4gt", "dich", "m\u00fch\u00b7los", "fort", "durch", "Raum", "und", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Du schaust und trinkst im Schaun Vergessenheit,", "tokens": ["Du", "schaust", "und", "trinkst", "im", "Schaun", "Ver\u00b7ges\u00b7sen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPRART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und, gleich als h\u00e4ttest du im Schlaf geruht,", "tokens": ["Und", ",", "gleich", "als", "h\u00e4t\u00b7test", "du", "im", "Schlaf", "ge\u00b7ruht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "KOKOM", "VAFIN", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Steigst du erfrischt aus seiner klaren Flut.", "tokens": ["Steigst", "du", "er\u00b7frischt", "aus", "sei\u00b7ner", "kla\u00b7ren", "Flut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}