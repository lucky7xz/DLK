{"textgrid.poem.25393": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Letzten, die am Weg die Lust verschm\u00e4ht;", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Letzten, die am Weg die Lust verschm\u00e4ht;", "tokens": ["Die", "Letz\u00b7ten", ",", "die", "am", "Weg", "die", "Lust", "ver\u00b7schm\u00e4ht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "entleert aus allen", "tokens": ["ent\u00b7leert", "aus", "al\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "PIAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Gassen der Stadt. In Not und Frost gepaart.", "tokens": ["Gas\u00b7sen", "der", "Stadt", ".", "In", "Not", "und", "Frost", "ge\u00b7paart", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Da die Laternen schon in schmutzigem Licht", "tokens": ["Da", "die", "La\u00b7ter\u00b7nen", "schon", "in", "schmut\u00b7zi\u00b7gem", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "verd\u00e4mmern,", "tokens": ["ver\u00b7d\u00e4m\u00b7mern", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Geht stumm ihr Zug zum Norden,", "tokens": ["Geht", "stumm", "ihr", "Zug", "zum", "Nor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "wo aus lichtdurchsungnen Hallen", "tokens": ["wo", "aus", "licht\u00b7durch\u00b7sung\u00b7nen", "Hal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Die Schienenstr\u00e4nge Welt und Schicksal", "tokens": ["Die", "Schie\u00b7nen\u00b7str\u00e4n\u00b7ge", "Welt", "und", "Schick\u00b7sal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "\u00fcber Winkelqueren h\u00e4mmern.", "tokens": ["\u00fc\u00b7ber", "Win\u00b7kel\u00b7que\u00b7ren", "h\u00e4m\u00b7mern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Tag l\u00e4\u00dft die scharfen Morgenwinde los.", "tokens": ["Tag", "l\u00e4\u00dft", "die", "schar\u00b7fen", "Mor\u00b7gen\u00b7win\u00b7de", "los", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Auffr\u00f6stelnd raffen", "tokens": ["Auf\u00b7fr\u00f6s\u00b7telnd", "raf\u00b7fen"], "token_info": ["word", "word"], "pos": ["VVPP", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "Sie ihre R\u00f6cke enger. Regen f\u00e4llt in F\u00e4den.", "tokens": ["Sie", "ih\u00b7re", "R\u00f6\u00b7cke", "en\u00b7ger", ".", "Re\u00b7gen", "f\u00e4llt", "in", "F\u00e4\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "ADJD", "$.", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Kaltes graues Licht", "tokens": ["Kal\u00b7tes", "grau\u00b7es", "Licht"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.14": {"text": "Entbl\u00f6\u00dft den Trug der Nacht.", "tokens": ["Ent\u00b7bl\u00f6\u00dft", "den", "Trug", "der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Geschminkte Wangen klaffen", "tokens": ["Ge\u00b7schmink\u00b7te", "Wan\u00b7gen", "klaf\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Wie giftige Wunden", "tokens": ["Wie", "gif\u00b7ti\u00b7ge", "Wun\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.17": {"text": "\u00fcber eingesunkenem Gesicht.", "tokens": ["\u00fc\u00b7ber", "ein\u00b7ge\u00b7sun\u00b7ke\u00b7nem", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.18": {"text": "Kein Wort. Die Masken brechen.", "tokens": ["Kein", "Wort", ".", "Die", "Mas\u00b7ken", "bre\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$.", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Lust und Gier sind tot. Nun schleppen", "tokens": ["Lust", "und", "Gier", "sind", "tot", ".", "Nun", "schlep\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "KON", "NN", "VAFIN", "ADJD", "$.", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Sie ihren Leib wie eine ekle Last", "tokens": ["Sie", "ih\u00b7ren", "Leib", "wie", "ei\u00b7ne", "ek\u00b7le", "Last"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "NN", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "in arme Schenken", "tokens": ["in", "ar\u00b7me", "Schen\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.22": {"text": "Und kauern regungslos im Kaffeedunst,", "tokens": ["Und", "kau\u00b7ern", "re\u00b7gungs\u00b7los", "im", "Kaf\u00b7fee\u00b7dunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "der \u00fcber Kellertreppen", "tokens": ["der", "\u00fc\u00b7ber", "Kel\u00b7ler\u00b7trep\u00b7pen"], "token_info": ["word", "word", "word"], "pos": ["ART", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "Aufsteigt \u2013 wie Geister, die das Taglicht angefallen \u2013", "tokens": ["Auf\u00b7steigt", "\u2013", "wie", "Geis\u00b7ter", ",", "die", "das", "Tag\u00b7licht", "an\u00b7ge\u00b7fal\u00b7len", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$(", "KOKOM", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "auf den B\u00e4nken.", "tokens": ["auf", "den", "B\u00e4n\u00b7ken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}