{"dta.poem.20760": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An Quitungsstatt geschrieben .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Seine Majest\u00e4t befahlen,               ", "tokens": ["Sei\u00b7ne", "Ma\u00b7jes\u00b7t\u00e4t", "be\u00b7fah\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir, anstatt ein Haus zu baun,", "tokens": ["Mir", ",", "an\u00b7statt", "ein", "Haus", "zu", "baun", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUI", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Doch drei Thaler auszuzahlen \u2014", "tokens": ["Doch", "drei", "Tha\u00b7ler", "aus\u00b7zu\u00b7zah\u00b7len"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "VVIZU", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der Monarchbefehl ward traun", "tokens": ["Der", "Mon\u00b7arch\u00b7be\u00b7fehl", "ward", "traun"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVINF"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Prompt und freundlich ausgerichtet,", "tokens": ["Prompt", "und", "freund\u00b7lich", "aus\u00b7ge\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und zum Dank bin ich verpflichtet.", "tokens": ["Und", "zum", "Dank", "bin", "ich", "ver\u00b7pflich\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Aber f\u00fcr drei Thaler kann", "tokens": ["A\u00b7ber", "f\u00fcr", "drei", "Tha\u00b7ler", "kann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "CARD", "NN", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Zu Berlin kein Hobelmann", "tokens": ["Zu", "Ber\u00b7lin", "kein", "Ho\u00b7bel\u00b7mann"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "PIAT", "NN"], "meter": "-++-+-+", "measure": "unknown.measure.tetra"}, "line.9": {"text": "Mir mein letztes Haus erbauen,", "tokens": ["Mir", "mein", "letz\u00b7tes", "Haus", "er\u00b7bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Sonst bestellt\u2019 ich ohne Grauen", "tokens": ["Sonst", "be\u00b7stellt'", "ich", "oh\u00b7ne", "Grau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Heute mir ein solches Haus,", "tokens": ["Heu\u00b7te", "mir", "ein", "sol\u00b7ches", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Wo einst W\u00fcrmer Tafel halten", "tokens": ["Wo", "einst", "W\u00fcr\u00b7mer", "Ta\u00b7fel", "hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Und sich \u00e4rgern \u00fcbern Schmau\u00df", "tokens": ["Und", "sich", "\u00e4r\u00b7gern", "\u00fc\u00b7bern", "Schmau\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PRF", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Bei des abgegr\u00e4mten, alten,", "tokens": ["Bei", "des", "ab\u00b7ge\u00b7gr\u00e4m\u00b7ten", ",", "al\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Magern Weibes Ueberrest,", "tokens": ["Ma\u00b7gern", "Wei\u00b7bes", "Ue\u00b7ber\u00b7rest", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Die der K\u00f6nig darben l\u00e4\u00dft.", "tokens": ["Die", "der", "K\u00f6\u00b7nig", "dar\u00b7ben", "l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PAV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}