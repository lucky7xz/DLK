{"textgrid.poem.67745": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "An den abwesenden Freund", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo bist Du, Z\u00e4rtlicher, den mein Gedanke k\u00fcsset,", "tokens": ["Wo", "bist", "Du", ",", "Z\u00e4rt\u00b7li\u00b7cher", ",", "den", "mein", "Ge\u00b7dan\u00b7ke", "k\u00fcs\u00b7set", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nach dem mein Seufzer seufzt und stille Sehnsucht brennt,", "tokens": ["Nach", "dem", "mein", "Seuf\u00b7zer", "seufzt", "und", "stil\u00b7le", "Sehn\u00b7sucht", "brennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "NN", "VVFIN", "KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo bist Du? S\u00e4hest Du, wie Dich die Thr\u00e4ne misset,", "tokens": ["Wo", "bist", "Du", "?", "S\u00e4\u00b7hest", "Du", ",", "wie", "Dich", "die", "Thr\u00e4\u00b7ne", "mis\u00b7set", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$.", "VVFIN", "PPER", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+---", "measure": "unknown.measure.penta"}, "line.4": {"text": "Die aus dem Auge stirbt und sterbend Dich noch nennt!", "tokens": ["Die", "aus", "dem", "Au\u00b7ge", "stirbt", "und", "ster\u00b7bend", "Dich", "noch", "nennt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "KON", "ADJD", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie in der Einsamkeit, wenn, m\u00fcde von Gesch\u00e4ften,", "tokens": ["Wie", "in", "der", "Ein\u00b7sam\u00b7keit", ",", "wenn", ",", "m\u00fc\u00b7de", "von", "Ge\u00b7sch\u00e4f\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "$,", "KOUS", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Geist kaum Athem sch\u00f6pft, er nichts sieht als Dein Bild,", "tokens": ["Der", "Geist", "kaum", "A\u00b7them", "sch\u00f6pft", ",", "er", "nichts", "sieht", "als", "Dein", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "NN", "VVFIN", "$,", "PPER", "PIS", "VVFIN", "KOKOM", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wie uns der Himmel einst, als J\u00fcngling', gleich an Kr\u00e4ften", "tokens": ["Wie", "uns", "der", "Him\u00b7mel", "einst", ",", "als", "J\u00fcng\u00b7ling'", ",", "gleich", "an", "Kr\u00e4f\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN", "ADV", "$,", "KOUS", "NN", "$,", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Gen\u00e4hrt, da flo\u00df er klar, jetzt ist er eingeh\u00fcllt;", "tokens": ["Ge\u00b7n\u00e4hrt", ",", "da", "flo\u00df", "er", "klar", ",", "jetzt", "ist", "er", "ein\u00b7ge\u00b7h\u00fcllt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADV", "VVFIN", "PPER", "ADJD", "$,", "ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Da flo\u00df Dein Arm um mich, und Deine Lippen sprachen", "tokens": ["Da", "flo\u00df", "Dein", "Arm", "um", "mich", ",", "und", "Dei\u00b7ne", "Lip\u00b7pen", "spra\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "PPER", "$,", "KON", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der Freundschaft sanften Ton, wie ihn das Herz gebar;", "tokens": ["Der", "Freund\u00b7schaft", "sanf\u00b7ten", "Ton", ",", "wie", "ihn", "das", "Herz", "ge\u00b7bar", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "PWAV", "PPER", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Empfindung seufzete, und Thr\u00e4nen unterbrachen", "tokens": ["Emp\u00b7fin\u00b7dung", "seuf\u00b7ze\u00b7te", ",", "und", "Thr\u00e4\u00b7nen", "un\u00b7ter\u00b7bra\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Des Herzens s\u00fc\u00dfen Ton, der uns ein Himmel war.", "tokens": ["Des", "Her\u00b7zens", "s\u00fc\u00b7\u00dfen", "Ton", ",", "der", "uns", "ein", "Him\u00b7mel", "war", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der Himmel ist nicht mehr! Hier wecket meine Z\u00e4hre", "tokens": ["Der", "Him\u00b7mel", "ist", "nicht", "mehr", "!", "Hier", "we\u00b7cket", "mei\u00b7ne", "Z\u00e4h\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADV", "$.", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die Deine; Thr\u00e4ne, flieh und r\u00fchre ihn, den Freund!", "tokens": ["Die", "Dei\u00b7ne", ";", "Thr\u00e4\u00b7ne", ",", "flieh", "und", "r\u00fch\u00b7re", "ihn", ",", "den", "Freund", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "$.", "NN", "$,", "VVFIN", "KON", "VVFIN", "PPER", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Unm\u00e4nnlich ist sie nicht; sie ist der Freundschaft Ehre,", "tokens": ["Un\u00b7m\u00e4nn\u00b7lich", "ist", "sie", "nicht", ";", "sie", "ist", "der", "Freund\u00b7schaft", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "PTKNEG", "$.", "PPER", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Weil sie die Z\u00e4rtlichkeit, die Menschheit selbst geweint.", "tokens": ["Weil", "sie", "die", "Z\u00e4rt\u00b7lich\u00b7keit", ",", "die", "Menschheit", "selbst", "ge\u00b7weint", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.17": {"text": "Wir sind nicht mehr vereint, uns trennen gr\u00f6\u00dfte Weiten!", "tokens": ["Wir", "sind", "nicht", "mehr", "ver\u00b7eint", ",", "uns", "tren\u00b7nen", "gr\u00f6\u00df\u00b7te", "Wei\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "VVPP", "$,", "PPER", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Du, Schicksal, trenntest uns mit gar zu harter Hand.", "tokens": ["Du", ",", "Schick\u00b7sal", ",", "trenn\u00b7test", "uns", "mit", "gar", "zu", "har\u00b7ter", "Hand", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VVFIN", "PPER", "APPR", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So rei\u00dft ein fr\u00fcher Tod die Gattin von der Seiten", "tokens": ["So", "rei\u00dft", "ein", "fr\u00fc\u00b7her", "Tod", "die", "Gat\u00b7tin", "von", "der", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Des Gatten, der erst Lust, jetzt doppelt Schmerz empfand.", "tokens": ["Des", "Gat\u00b7ten", ",", "der", "erst", "Lust", ",", "jetzt", "dop\u00b7pelt", "Schmerz", "emp\u00b7fand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "NN", "$,", "ADV", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Hier seufze ich nach Ruh und seufze nur vergebens;", "tokens": ["Hier", "seuf\u00b7ze", "ich", "nach", "Ruh", "und", "seuf\u00b7ze", "nur", "ver\u00b7ge\u00b7bens", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "KON", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Er flieht, der traur'ge Gram, und kommt versteckt zur\u00fcck.", "tokens": ["Er", "flieht", ",", "der", "traur'\u00b7ge", "Gram", ",", "und", "kommt", "ver\u00b7steckt", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "$,", "KON", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Dort weinst Du vaterlos und weinest nur vergebens;", "tokens": ["Dort", "weinst", "Du", "va\u00b7ter\u00b7los", "und", "wei\u00b7nest", "nur", "ver\u00b7ge\u00b7bens", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Bringt eine Thr\u00e4ne wol den Todten noch zur\u00fcck?", "tokens": ["Bringt", "ei\u00b7ne", "Thr\u00e4\u00b7ne", "wol", "den", "Tod\u00b7ten", "noch", "zu\u00b7r\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Nein, auch den Vater nicht; er siehet von den Sternen", "tokens": ["Nein", ",", "auch", "den", "Va\u00b7ter", "nicht", ";", "er", "sie\u00b7het", "von", "den", "Ster\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADV", "ART", "NN", "PTKNEG", "$.", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Dein, seines Kindes, Gram, der doch umsonst ihn klagt,", "tokens": ["Dein", ",", "sei\u00b7nes", "Kin\u00b7des", ",", "Gram", ",", "der", "doch", "um\u00b7sonst", "ihn", "klagt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "$,", "PPOSAT", "NN", "$,", "NN", "$,", "PRELS", "ADV", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Hat Mitleid mit Dir, Freund, und w\u00fcnscht in sel'gen Fernen", "tokens": ["Hat", "Mit\u00b7leid", "mit", "Dir", ",", "Freund", ",", "und", "w\u00fcnscht", "in", "sel'\u00b7gen", "Fer\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "APPR", "PPER", "$,", "NN", "$,", "KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Dir Gottes Weisheit zu, die leeren Schmerz verjagt.", "tokens": ["Dir", "Got\u00b7tes", "Weis\u00b7heit", "zu", ",", "die", "lee\u00b7ren", "Schmerz", "ver\u00b7jagt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "NN", "PTKVZ", "$,", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Auch ich, ich fasse mich; Geschick kann Leiber trennen,", "tokens": ["Auch", "ich", ",", "ich", "fas\u00b7se", "mich", ";", "Ge\u00b7schick", "kann", "Lei\u00b7ber", "tren\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "PPER", "VVFIN", "PPER", "$.", "NN", "VMFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Doch Herzen kann es nicht; die lieben ihm zum Trutz.", "tokens": ["Doch", "Her\u00b7zen", "kann", "es", "nicht", ";", "die", "lie\u00b7ben", "ihm", "zum", "Trutz", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VMFIN", "PPER", "PTKNEG", "$.", "ART", "ADJA", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Wenn Dich mein Aug' nicht nennt, soll mein Gedank Dich nennen,", "tokens": ["Wenn", "Dich", "mein", "Aug'", "nicht", "nennt", ",", "soll", "mein", "Ge\u00b7dank", "Dich", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$,", "VMFIN", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und k\u00fc\u00dft Dich nicht mein Mund, k\u00fc\u00dft Dich die Phantasei.", "tokens": ["Und", "k\u00fc\u00dft", "Dich", "nicht", "mein", "Mund", ",", "k\u00fc\u00dft", "Dich", "die", "Phan\u00b7ta\u00b7sei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Oft in der Einsamkeit will ich Dir Seufzer schicken:", "tokens": ["Oft", "in", "der", "Ein\u00b7sam\u00b7keit", "will", "ich", "Dir", "Seuf\u00b7zer", "schi\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VMFIN", "PPER", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "\u00bbnehmt, Zephyrs, leitet sie und dr\u00fcckt sie an sein Herz!\u00ab", "tokens": ["\u00bb", "nehmt", ",", "Ze\u00b7phyrs", ",", "lei\u00b7tet", "sie", "und", "dr\u00fcckt", "sie", "an", "sein", "Herz", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "$,", "NN", "$,", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Und Dein Gedank an mich, wie wird er mich erquicken!", "tokens": ["Und", "Dein", "Ge\u00b7dank", "an", "mich", ",", "wie", "wird", "er", "mich", "er\u00b7qui\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "PPER", "$,", "PWAV", "VAFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Wenn Ahndung ihn mir sagt, dann lindert sich mein Schmerz.", "tokens": ["Wenn", "Ahn\u00b7dung", "ihn", "mir", "sagt", ",", "dann", "lin\u00b7dert", "sich", "mein", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Wenn schon die Zunge lallt, soll sie noch halb Dich sprechen;", "tokens": ["Wenn", "schon", "die", "Zun\u00b7ge", "lallt", ",", "soll", "sie", "noch", "halb", "Dich", "spre\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$,", "VMFIN", "PPER", "ADV", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Wenn einst das Auge bricht, sieht es Dich, Freund, und stirbt.", "tokens": ["Wenn", "einst", "das", "Au\u00b7ge", "bricht", ",", "sieht", "es", "Dich", ",", "Freund", ",", "und", "stirbt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$,", "VVFIN", "PPER", "PPER", "$,", "NN", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.39": {"text": "Noch flammt die Seele auf, denkt Dich und \u2013 und mu\u00df brechen,", "tokens": ["Noch", "flammt", "die", "See\u00b7le", "auf", ",", "denkt", "Dich", "und", "\u2013", "und", "mu\u00df", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,", "VVFIN", "PPER", "KON", "$(", "KON", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Wie eine Flamme glimmt, auflodert und dann stirbt.", "tokens": ["Wie", "ei\u00b7ne", "Flam\u00b7me", "glimmt", ",", "auf\u00b7lo\u00b7dert", "und", "dann", "stirbt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,", "VVPP", "KON", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Ruht unser Aschenkrug dereinst getrennt im Grabe,", "tokens": ["Ruht", "un\u00b7ser", "A\u00b7schen\u00b7krug", "de\u00b7reinst", "ge\u00b7trennt", "im", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "An dem mit stiller Pracht Cypressen einsam bl\u00fchn,", "tokens": ["An", "dem", "mit", "stil\u00b7ler", "Pracht", "Cyp\u00b7res\u00b7sen", "ein\u00b7sam", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPR", "ADJA", "NN", "NE", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "So rausche sanfter Schau'r noch oft von Grab zu Grabe,", "tokens": ["So", "rau\u00b7sche", "sanf\u00b7ter", "Schau'r", "noch", "oft", "von", "Grab", "zu", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "ADJA", "NN", "ADV", "ADV", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "So sprech' ein frommer Greis: \u00bbRuht! ich will zu Euch fliehn!\u00ab", "tokens": ["So", "sprech'", "ein", "from\u00b7mer", "Greis", ":", "\u00bb", "Ruht", "!", "ich", "will", "zu", "Euch", "fliehn", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$.", "$(", "VVFIN", "$.", "PPER", "VMFIN", "APPR", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}}}}}