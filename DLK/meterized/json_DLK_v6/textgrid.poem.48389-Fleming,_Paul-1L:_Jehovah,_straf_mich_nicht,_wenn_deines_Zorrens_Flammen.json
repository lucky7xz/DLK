{"textgrid.poem.48389": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Jehovah, straf mich nicht, wenn deines Zorrens Flammen", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jehovah, straf mich nicht, wenn deines Zorrens Flammen", "tokens": ["Je\u00b7ho\u00b7vah", ",", "straf", "mich", "nicht", ",", "wenn", "dei\u00b7nes", "Zor\u00b7rens", "Flam\u00b7men"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "verzehren alle Gunst, gehn \u00fcber mir zusammen!", "tokens": ["ver\u00b7zeh\u00b7ren", "al\u00b7le", "Gunst", ",", "gehn", "\u00fc\u00b7ber", "mir", "zu\u00b7sam\u00b7men", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$,", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn deines Grimmes Loh in vollem Sturme f\u00e4hrt,", "tokens": ["Wenn", "dei\u00b7nes", "Grim\u00b7mes", "Loh", "in", "vol\u00b7lem", "Stur\u00b7me", "f\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "die dieses alles auch in einem Nu verheert,", "tokens": ["die", "die\u00b7ses", "al\u00b7les", "auch", "in", "ei\u00b7nem", "Nu", "ver\u00b7heert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "PIS", "ADV", "APPR", "ART", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "dann z\u00fcchtige mich nicht! Du siehst ohn di\u00df die Schmerzen,", "tokens": ["dann", "z\u00fcch\u00b7ti\u00b7ge", "mich", "nicht", "!", "Du", "siehst", "ohn", "di\u00df", "die", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "$.", "PPER", "VVFIN", "APPR", "PDS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "so deine grimme Pfeil' erregen meinem Herzen.", "tokens": ["so", "dei\u00b7ne", "grim\u00b7me", "Pfeil'", "er\u00b7re\u00b7gen", "mei\u00b7nem", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "F\u00fcr deinem Dr\u00e4uen, Herr, ist nichts an mir gesund,", "tokens": ["F\u00fcr", "dei\u00b7nem", "Dr\u00e4u\u00b7en", ",", "Herr", ",", "ist", "nichts", "an", "mir", "ge\u00b7sund", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "NN", "$,", "VAFIN", "PIS", "APPR", "PPER", "ADJD", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "dein' Hand ist mir zu schwer, sie schl\u00e4gt mich krank und wund;", "tokens": ["dein'", "Hand", "ist", "mir", "zu", "schwer", ",", "sie", "schl\u00e4gt", "mich", "krank", "und", "wund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "PTKA", "ADJD", "$,", "PPER", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "mein Leib ist strimenvoll, ich habe keinen Friede,", "tokens": ["mein", "Leib", "ist", "stri\u00b7men\u00b7voll", ",", "ich", "ha\u00b7be", "kei\u00b7nen", "Frie\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPER", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "ich, wolgeplagter Mensch, in irgends einem Gliede.", "tokens": ["ich", ",", "wol\u00b7ge\u00b7plag\u00b7ter", "Mensch", ",", "in", "ir\u00b7gends", "ei\u00b7nem", "Glie\u00b7de", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$,", "APPR", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der schwere S\u00fcndenschmerz greift auch die Knochen an,", "tokens": ["Der", "schwe\u00b7re", "S\u00fcn\u00b7den\u00b7schmerz", "greift", "auch", "die", "Kno\u00b7chen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "der \u00fcbermachte Schmerz, und w\u00fctet was er kan.", "tokens": ["der", "\u00fc\u00b7ber\u00b7mach\u00b7te", "Schmerz", ",", "und", "w\u00fc\u00b7tet", "was", "er", "kan", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "VVFIN", "PWS", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die Gr\u00f6\u00dfe meiner Schuld ist \u00fcber mich gestiegen,", "tokens": ["Die", "Gr\u00f6\u00b7\u00dfe", "mei\u00b7ner", "Schuld", "ist", "\u00fc\u00b7ber", "mich", "ge\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "hoch \u00fcber dieses Haupt. Ich mu\u00df, ich mu\u00df erliegen.", "tokens": ["hoch", "\u00fc\u00b7ber", "die\u00b7ses", "Haupt", ".", "Ich", "mu\u00df", ",", "ich", "mu\u00df", "er\u00b7lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PDAT", "NN", "$.", "PPER", "VMFIN", "$,", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Sie dr\u00fcckt mich unter sich, wie von der schweren B\u00fcrd'", "tokens": ["Sie", "dr\u00fcckt", "mich", "un\u00b7ter", "sich", ",", "wie", "von", "der", "schwe\u00b7ren", "B\u00fcrd'"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PRF", "$,", "PWAV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "ein schwacher R\u00fccken gar in sich gequetschet wird.", "tokens": ["ein", "schwa\u00b7cher", "R\u00fc\u00b7cken", "gar", "in", "sich", "ge\u00b7quet\u00b7schet", "wird", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "APPR", "PRF", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die Haut ist voller Wust, die Torheitwunden stinken,", "tokens": ["Die", "Haut", "ist", "vol\u00b7ler", "Wust", ",", "die", "Tor\u00b7heit\u00b7wun\u00b7den", "stin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJA", "NN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "die Schwere gehen auf. Ich mu\u00df f\u00fcr Schmerzen sinken.", "tokens": ["die", "Schwe\u00b7re", "ge\u00b7hen", "auf", ".", "Ich", "mu\u00df", "f\u00fcr", "Schmer\u00b7zen", "sin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$.", "PPER", "VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ich gehe manchen Tag ganz traurig, krumb, geb\u00fcckt,", "tokens": ["Ich", "ge\u00b7he", "man\u00b7chen", "Tag", "ganz", "trau\u00b7rig", ",", "krumb", ",", "ge\u00b7b\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ADV", "ADJD", "$,", "VVFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "die Lenden dorren aus. Da ist nichts, das erquickt", "tokens": ["die", "Len\u00b7den", "dor\u00b7ren", "aus", ".", "Da", "ist", "nichts", ",", "das", "er\u00b7quickt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$.", "ADV", "VAFIN", "PIS", "$,", "PRELS", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "den ungesunden Leib und lindert meine Beulen.", "tokens": ["den", "un\u00b7ge\u00b7sun\u00b7den", "Leib", "und", "lin\u00b7dert", "mei\u00b7ne", "Beu\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Ich bin nicht itzo ich. Ich mu\u00df f\u00fcr Unruh heulen,", "tokens": ["Ich", "bin", "nicht", "it\u00b7zo", "ich", ".", "Ich", "mu\u00df", "f\u00fcr", "Un\u00b7ruh", "heu\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "PPER", "$.", "PPER", "VMFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "die mir mein Leben fri\u00dft. Herr, du weists besser noch", "tokens": ["die", "mir", "mein", "Le\u00b7ben", "fri\u00dft", ".", "Herr", ",", "du", "weists", "bes\u00b7ser", "noch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VVFIN", "$.", "NN", "$,", "PPER", "VVFIN", "ADJD", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "als ich dirs klagen kan, was mich dr\u00fcckt f\u00fcr ein Joch.", "tokens": ["als", "ich", "dirs", "kla\u00b7gen", "kan", ",", "was", "mich", "dr\u00fcckt", "f\u00fcr", "ein", "Joch", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VVINF", "VMFIN", "$,", "PWS", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-++--+", "measure": "iambic.hexa.chol"}, "line.25": {"text": "Di\u00df Seufzen kennest du. Mein mattes Herze zittert,", "tokens": ["Di\u00df", "Seuf\u00b7zen", "ken\u00b7nest", "du", ".", "Mein", "mat\u00b7tes", "Her\u00b7ze", "zit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VVFIN", "PPER", "$.", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "die erste Kraft ist hin, der ganze Leib ersch\u00fcttert.", "tokens": ["die", "ers\u00b7te", "Kraft", "ist", "hin", ",", "der", "gan\u00b7ze", "Leib", "er\u00b7sch\u00fct\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "$,", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Die Glieder werden welk, der bl\u00f6den Augen Liecht", "tokens": ["Die", "Glie\u00b7der", "wer\u00b7den", "welk", ",", "der", "bl\u00f6\u00b7den", "Au\u00b7gen", "Liecht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "ist wie ein dicker Dampf. Da ist kein Kl\u00e4ger nicht,", "tokens": ["ist", "wie", "ein", "di\u00b7cker", "Dampf", ".", "Da", "ist", "kein", "Kl\u00e4\u00b7ger", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "ADJA", "NN", "$.", "ADV", "VAFIN", "PIAT", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "der Beileid mit mir tr\u00e4gt. Ein Greuel ists zu sagen!", "tokens": ["der", "Bei\u00b7leid", "mit", "mir", "tr\u00e4gt", ".", "Ein", "Greu\u00b7el", "ists", "zu", "sa\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVFIN", "$.", "ART", "NN", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Ja, auch die Freunde selbst, die scheuen meine Plagen", "tokens": ["Ja", ",", "auch", "die", "Freun\u00b7de", "selbst", ",", "die", "scheu\u00b7en", "mei\u00b7ne", "Pla\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADV", "ART", "NN", "ADV", "$,", "PRELS", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "und stehen weit von mir. Der vor mein N\u00e4chster war,", "tokens": ["und", "ste\u00b7hen", "weit", "von", "mir", ".", "Der", "vor", "mein", "N\u00e4chs\u00b7ter", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "PPER", "$.", "ART", "APPR", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "ist jetzt der Ferneste. Versto\u00dfen bin ich gar.", "tokens": ["ist", "jetzt", "der", "Fer\u00b7nes\u00b7te", ".", "Ver\u00b7sto\u00b7\u00dfen", "bin", "ich", "gar", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$.", "NN", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Was noch das Gr\u00f6\u00dfest ist, die Feinde seh' ich stellen", "tokens": ["Was", "noch", "das", "Gr\u00f6\u00b7\u00dfest", "ist", ",", "die", "Fein\u00b7de", "seh'", "ich", "stel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ART", "NN", "VAFIN", "$,", "ART", "NN", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "auf allen Seiten auf, wie sie nur m\u00f6gen f\u00e4llen", "tokens": ["auf", "al\u00b7len", "Sei\u00b7ten", "auf", ",", "wie", "sie", "nur", "m\u00f6\u00b7gen", "f\u00e4l\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "PTKVZ", "$,", "PWAV", "PPER", "ADV", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "mein abgeseelte Seel'. Hier lauren sie und dort", "tokens": ["mein", "ab\u00b7ge\u00b7seel\u00b7te", "Seel'", ".", "Hier", "lau\u00b7ren", "sie", "und", "dort"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "ADV", "VVFIN", "PPER", "KON", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "und reden wider mich nur lauter Schadenswort", "tokens": ["und", "re\u00b7den", "wi\u00b7der", "mich", "nur", "lau\u00b7ter", "Scha\u00b7dens\u00b7wort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "und bergens listiglich. Ich aber mu\u00df nicht h\u00f6ren,", "tokens": ["und", "ber\u00b7gens", "lis\u00b7tig\u00b7lich", ".", "Ich", "a\u00b7ber", "mu\u00df", "nicht", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "$.", "PPER", "ADV", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "mu\u00df wie ein Tauber sein, darf ihren Rat nicht st\u00f6ren,", "tokens": ["mu\u00df", "wie", "ein", "Tau\u00b7ber", "sein", ",", "darf", "ih\u00b7ren", "Rat", "nicht", "st\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "KOKOM", "ART", "NN", "VAINF", "$,", "VMFIN", "PPOSAT", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "ganz einem Stummen gleich, der sich nicht sch\u00fctzen kan,", "tokens": ["ganz", "ei\u00b7nem", "Stum\u00b7men", "gleich", ",", "der", "sich", "nicht", "sch\u00fct\u00b7zen", "kan", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "$,", "PRELS", "PRF", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "wenn ihm zur Ungeb\u00fchr ein Schimpf wird angetan.", "tokens": ["wenn", "ihm", "zur", "Un\u00b7ge\u00b7b\u00fchr", "ein", "Schimpf", "wird", "an\u00b7ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Auf dich, Herr, Herr, auf dich harr ich in diesen N\u00f6ten!", "tokens": ["Auf", "dich", ",", "Herr", ",", "Herr", ",", "auf", "dich", "harr", "ich", "in", "die\u00b7sen", "N\u00f6\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "NN", "$,", "NN", "$,", "APPR", "PPER", "VVFIN", "PPER", "APPR", "PDAT", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.42": {"text": "du, mein Gott, wirst ja nicht mich gar so lassen t\u00f6ten.", "tokens": ["du", ",", "mein", "Gott", ",", "wirst", "ja", "nicht", "mich", "gar", "so", "las\u00b7sen", "t\u00f6\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$,", "VAFIN", "ADV", "PTKNEG", "PPER", "ADV", "ADV", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Erh\u00f6r', erh\u00f6re mich, auf da\u00df ich ihrer Rott',", "tokens": ["Er\u00b7h\u00f6r'", ",", "er\u00b7h\u00f6\u00b7re", "mich", ",", "auf", "da\u00df", "ich", "ih\u00b7rer", "Rott'", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "$,", "APPR", "KOUS", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "im Fall mirs \u00fcbel geht, nicht gar mu\u00df sein ein Spott!", "tokens": ["im", "Fall", "mirs", "\u00fc\u00b7bel", "geht", ",", "nicht", "gar", "mu\u00df", "sein", "ein", "Spott", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "ADJD", "VVFIN", "$,", "PTKNEG", "ADV", "VMFIN", "PPOSAT", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Wenn dein Verh\u00e4ngn\u00fc\u00df mich lie\u00df auf dem Glatten wanken,", "tokens": ["Wenn", "dein", "Ver\u00b7h\u00e4ng\u00b7n\u00fc\u00df", "mich", "lie\u00df", "auf", "dem", "Glat\u00b7ten", "wan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+++-+-+-", "measure": "unknown.measure.septa"}, "line.46": {"text": "hilf Gott, was w\u00fcrden sie nicht haben f\u00fcr Gedanken,", "tokens": ["hilf", "Gott", ",", "was", "w\u00fcr\u00b7den", "sie", "nicht", "ha\u00b7ben", "f\u00fcr", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "$,", "PWS", "VAFIN", "PPER", "PTKNEG", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "wie w\u00fcrden sie sich doch hoch r\u00fchmen wider mich!", "tokens": ["wie", "w\u00fcr\u00b7den", "sie", "sich", "doch", "hoch", "r\u00fch\u00b7men", "wi\u00b7der", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PRF", "ADV", "ADJD", "VVINF", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Es ist kein m\u00fchsamer, kein \u00e4rmer Mensch, als ich.", "tokens": ["Es", "ist", "kein", "m\u00fch\u00b7sa\u00b7mer", ",", "kein", "\u00e4r\u00b7mer", "Mensch", ",", "als", "ich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "ADJD", "$,", "PIAT", "ADJA", "NN", "$,", "KOUS", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Ich bin zu steter Angst und Leiden nur geboren.", "tokens": ["Ich", "bin", "zu", "ste\u00b7ter", "Angst", "und", "Lei\u00b7den", "nur", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "KON", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Mein Schmerz ist immer neu. Herr, nimb doch du zu Ohren", "tokens": ["Mein", "Schmerz", "ist", "im\u00b7mer", "neu", ".", "Herr", ",", "nimb", "doch", "du", "zu", "Oh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$.", "NN", "$,", "VVIMP", "ADV", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "mein heiser Notgeschrei! Dir beicht' ich meine Schuld,", "tokens": ["mein", "hei\u00b7ser", "Not\u00b7ge\u00b7schrei", "!", "Dir", "beicht'", "ich", "mei\u00b7ne", "Schuld", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "ich sorge stets f\u00fcr sie. Herr, habe doch Geduld,", "tokens": ["ich", "sor\u00b7ge", "stets", "f\u00fcr", "sie", ".", "Herr", ",", "ha\u00b7be", "doch", "Ge\u00b7duld", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "$.", "NN", "$,", "VAFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "und t\u00f6te mich nicht gar! Sie, meine Feinde, leben", "tokens": ["und", "t\u00f6\u00b7te", "mich", "nicht", "gar", "!", "Sie", ",", "mei\u00b7ne", "Fein\u00b7de", ",", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ADV", "$.", "PPER", "$,", "PPOSAT", "NN", "$,", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "und trutzen auf die Macht. Sie seh' ich oben schweben", "tokens": ["und", "trut\u00b7zen", "auf", "die", "Macht", ".", "Sie", "seh'", "ich", "o\u00b7ben", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$.", "PPER", "VVFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "und gr\u00f6\u00dfer sein als ich, die mich, wei\u00df nicht, warumb", "tokens": ["und", "gr\u00f6\u00b7\u00dfer", "sein", "als", "ich", ",", "die", "mich", ",", "wei\u00df", "nicht", ",", "wa\u00b7rumb"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "ADJD", "VAINF", "KOKOM", "PPER", "$,", "PRELS", "PPER", "$,", "VVFIN", "PTKNEG", "$,", "PWAV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "aus selbstgefastem Ha\u00df und Gramsein rennen umb,", "tokens": ["aus", "selbst\u00b7ge\u00b7fas\u00b7tem", "Ha\u00df", "und", "Gram\u00b7sein", "ren\u00b7nen", "umb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "die mir f\u00fcr Segen Fluch, f\u00fcr Gutes B\u00f6ses g\u00f6nnen.", "tokens": ["die", "mir", "f\u00fcr", "Se\u00b7gen", "Fluch", ",", "f\u00fcr", "Gu\u00b7tes", "B\u00f6\u00b7ses", "g\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "NN", "$,", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Herr, wirstu l\u00e4nger auch dem \u00dcbel zusehn k\u00f6nnen?", "tokens": ["Herr", ",", "wirs\u00b7tu", "l\u00e4n\u00b7ger", "auch", "dem", "\u00dc\u00b7bel", "zu\u00b7sehn", "k\u00f6n\u00b7nen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "ADJD", "ADV", "ART", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Ach eile, weil die Not ietzt in dem H\u00f6chsten ist,", "tokens": ["Ach", "ei\u00b7le", ",", "weil", "die", "Not", "ietzt", "in", "dem", "H\u00f6chs\u00b7ten", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "$,", "KOUS", "ART", "NN", "ADV", "APPR", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "weil du mein' ein'ge H\u00fclf' und starker Beistand bist!", "tokens": ["weil", "du", "mein'", "ein'\u00b7ge", "H\u00fcl\u00b7f'", "und", "star\u00b7ker", "Bei\u00b7stand", "bist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "