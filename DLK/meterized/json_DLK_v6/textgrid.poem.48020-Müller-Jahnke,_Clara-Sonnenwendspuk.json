{"textgrid.poem.48020": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Sonnenwendspuk", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da blitzt aus mittern\u00e4chtgem Dunkel", "tokens": ["Da", "blitzt", "aus", "mit\u00b7ter\u00b7n\u00e4cht\u00b7gem", "Dun\u00b7kel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "ein ferner fahler Schein herauf;", "tokens": ["ein", "fer\u00b7ner", "fah\u00b7ler", "Schein", "her\u00b7auf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "mit Augen, licht wie Sterngefunkel", "tokens": ["mit", "Au\u00b7gen", ",", "licht", "wie", "Stern\u00b7ge\u00b7fun\u00b7kel"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "ADJD", "KOKOM", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "steht meine Kindheit vor mir auf;", "tokens": ["steht", "mei\u00b7ne", "Kind\u00b7heit", "vor", "mir", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "sie gr\u00fc\u00dft so s\u00fc\u00df, und l\u00e4chelnd lauschen", "tokens": ["sie", "gr\u00fc\u00dft", "so", "s\u00fc\u00df", ",", "und", "l\u00e4\u00b7chelnd", "lau\u00b7schen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "KON", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "m\u00f6cht ich den Worten, die sie spricht, \u2013", "tokens": ["m\u00f6cht", "ich", "den", "Wor\u00b7ten", ",", "die", "sie", "spricht", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "ich h\u00f6r ihr Raunen, Fl\u00fcstern, Rauschen,", "tokens": ["ich", "h\u00f6r", "ihr", "Rau\u00b7nen", ",", "Fl\u00fcs\u00b7tern", ",", "Rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "doch ihren Sinn erfa\u00df ich nicht.", "tokens": ["doch", "ih\u00b7ren", "Sinn", "er\u00b7fa\u00df", "ich", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da hallt in n\u00e4chtlich-tiefem Schweigen", "tokens": ["Da", "hallt", "in", "n\u00e4cht\u00b7lich\u00b7tie\u00b7fem", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "ein leiser Laut wie Harfenton,", "tokens": ["ein", "lei\u00b7ser", "Laut", "wie", "Har\u00b7fen\u00b7ton", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KOKOM", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "da gaukelt um mein Bett ein Reigen", "tokens": ["da", "gau\u00b7kelt", "um", "mein", "Bett", "ein", "Rei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "von Freuden, die mir l\u00e4ngst entflohn \u2013", "tokens": ["von", "Freu\u00b7den", ",", "die", "mir", "l\u00e4ngst", "ent\u00b7flohn", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mir ist, als sollt noch einmal wenden", "tokens": ["Mir", "ist", ",", "als", "sollt", "noch", "ein\u00b7mal", "wen\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VMFIN", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "die dunkle Bahn sich sonnenw\u00e4rts:", "tokens": ["die", "dunk\u00b7le", "Bahn", "sich", "son\u00b7nen\u00b7w\u00e4rts", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "mit leisen, k\u00fchlen Geisterh\u00e4nden", "tokens": ["mit", "lei\u00b7sen", ",", "k\u00fch\u00b7len", "Geis\u00b7ter\u00b7h\u00e4n\u00b7den"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "VVINF", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "pocht meine Jugend an mein Herz.", "tokens": ["pocht", "mei\u00b7ne", "Ju\u00b7gend", "an", "mein", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O Kinderlust, verklungne Weise", "tokens": ["O", "Kin\u00b7der\u00b7lust", ",", "ver\u00b7klung\u00b7ne", "Wei\u00b7se"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "von Heimatflur und Vaterhaus,", "tokens": ["von", "Hei\u00b7mat\u00b7flur", "und", "Va\u00b7ter\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "du nahst wie Gottes Engel leise", "tokens": ["du", "nahst", "wie", "Got\u00b7tes", "En\u00b7gel", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "NN", "NE", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "und teilst des Lichtes Botschaft aus \u2013", "tokens": ["und", "teilst", "des", "Lich\u00b7tes", "Bot\u00b7schaft", "aus", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O Liebe, die mit Rosenketten", "tokens": ["O", "Lie\u00b7be", ",", "die", "mit", "Ro\u00b7sen\u00b7ket\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "mein liederspr\u00fchend Haupt umwand,", "tokens": ["mein", "lie\u00b7der\u00b7spr\u00fc\u00b7hend", "Haupt", "um\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "kommst du noch einmal, mich zu retten,", "tokens": ["kommst", "du", "noch", "ein\u00b7mal", ",", "mich", "zu", "ret\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "zu retten von des Abgrunds Rand?! \u2013", "tokens": ["zu", "ret\u00b7ten", "von", "des", "Ab\u00b7grunds", "Rand", "?!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Von der Adventszeit geht die Sage,", "tokens": ["Von", "der", "Ad\u00b7vents\u00b7zeit", "geht", "die", "Sa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "--+-++-+-", "measure": "anapaest.init"}, "line.2": {"text": "sie locke manch verlornes Kind", "tokens": ["sie", "lo\u00b7cke", "manch", "ver\u00b7lor\u00b7nes", "Kind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "zur Heimkehr \u2013 ach, durch leere Hage", "tokens": ["zur", "Heim\u00b7kehr", "\u2013", "ach", ",", "durch", "lee\u00b7re", "Ha\u00b7ge"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$(", "XY", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "streicht seufzend der Dezemberwind,", "tokens": ["streicht", "seuf\u00b7zend", "der", "De\u00b7zem\u00b7ber\u00b7wind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "das ist die Nacht der Sonnenwende, \u2013", "tokens": ["das", "ist", "die", "Nacht", "der", "Son\u00b7nen\u00b7wen\u00b7de", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "doch gl\u00e4nzt f\u00fcr mich kein Weihnachtslicht \u2013", "tokens": ["doch", "gl\u00e4nzt", "f\u00fcr", "mich", "kein", "Weih\u00b7nachts\u00b7licht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "PIAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "O Herr im Himmel, mach ein Ende,", "tokens": ["O", "Herr", "im", "Him\u00b7mel", ",", "mach", "ein", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPRART", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "denn meine Kraft zerbricht \u2013 zerbricht! \u2013", "tokens": ["denn", "mei\u00b7ne", "Kraft", "zer\u00b7bricht", "\u2013", "zer\u00b7bricht", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$(", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "O Herr im Himmel, mach ein Ende! \u2013", "tokens": ["O", "Herr", "im", "Him\u00b7mel", ",", "mach", "ein", "En\u00b7de", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NN", "APPRART", "NN", "$,", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "doch schon erbla\u00dft der matte Schein;", "tokens": ["doch", "schon", "er\u00b7bla\u00dft", "der", "mat\u00b7te", "Schein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "dumpf schl\u00e4gt die Uhr: \u2013 der Sonnenwende", "tokens": ["dumpf", "schl\u00e4gt", "die", "Uhr", ":", "\u2013", "der", "Son\u00b7nen\u00b7wen\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$.", "$(", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gespensterstunde bricht herein.", "tokens": ["Ge\u00b7spens\u00b7ter\u00b7stun\u00b7de", "bricht", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wieder t\u00f6nt ein Raunen, Locken", "tokens": ["Und", "wie\u00b7der", "t\u00f6nt", "ein", "Rau\u00b7nen", ",", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "wie Nixensang, wie Geisterchor. \u2013", "tokens": ["wie", "Ni\u00b7xen\u00b7sang", ",", "wie", "Geis\u00b7ter\u00b7chor", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PWAV", "NN", "$,", "PWAV", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mit fliegenden Pulsen, tief erschrocken", "tokens": ["Mit", "flie\u00b7gen\u00b7den", "Pul\u00b7sen", ",", "tief", "er\u00b7schro\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "VVINF"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "richt ich vom Lager mich empor.", "tokens": ["richt", "ich", "vom", "La\u00b7ger", "mich", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ein Gaukeln ist's, ein irres Schweifen \u2013", "tokens": ["Ein", "Gau\u00b7keln", "ist's", ",", "ein", "ir\u00b7res", "Schwei\u00b7fen", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "die alten G\u00f6tter sind erwacht,", "tokens": ["die", "al\u00b7ten", "G\u00f6t\u00b7ter", "sind", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "die fieberhei\u00dfe Stirne streichen", "tokens": ["die", "fie\u00b7ber\u00b7hei\u00b7\u00dfe", "Stir\u00b7ne", "strei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "mir Schemen der Mittwinternacht, \u2013", "tokens": ["mir", "Sche\u00b7men", "der", "Mitt\u00b7win\u00b7ter\u00b7nacht", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "aus l\u00e4ngst verschollner Vorzeit Feiern", "tokens": ["aus", "l\u00e4ngst", "ver\u00b7scholl\u00b7ner", "Vor\u00b7zeit", "Fei\u00b7ern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "klingt Zauberkunde dumpf herauf:", "tokens": ["klingt", "Zau\u00b7ber\u00b7kun\u00b7de", "dumpf", "her\u00b7auf", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "das Haupt umh\u00fcllt von Nebelschleiern", "tokens": ["das", "Haupt", "um\u00b7h\u00fcllt", "von", "Ne\u00b7bel\u00b7schlei\u00b7ern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "steht meine Zukunft vor mir auf.", "tokens": ["steht", "mei\u00b7ne", "Zu\u00b7kunft", "vor", "mir", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Sie hebt beschw\u00f6rend ihre H\u00e4nde,", "tokens": ["Sie", "hebt", "be\u00b7schw\u00f6\u00b7rend", "ih\u00b7re", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "wie Drudenweisheit klingt ihr Spruch:", "tokens": ["wie", "Dru\u00b7den\u00b7weis\u00b7heit", "klingt", "ihr", "Spruch", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbdu stehst an deines Lebens Wende \u2013", "tokens": ["\u00bb", "du", "stehst", "an", "dei\u00b7nes", "Le\u00b7bens", "Wen\u00b7de", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "nun gilt es Segen oder Fluch!", "tokens": ["nun", "gilt", "es", "Se\u00b7gen", "o\u00b7der", "Fluch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zwei Wege hat auch dir beschieden", "tokens": ["Zwei", "We\u00b7ge", "hat", "auch", "dir", "be\u00b7schie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VAFIN", "ADV", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "geheimnisvolle Schicksalsmacht \u2013", "tokens": ["ge\u00b7heim\u00b7nis\u00b7vol\u