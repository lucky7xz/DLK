{"textgrid.poem.49418": {"metadata": {"author": {"name": "Verlaine, Paul-Marie", "birth": "N.A.", "death": "N.A."}, "title": "[der Schenken L\u00e4rm, der Schmutz der n\u00e4cht'gen Stadt]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Schenken L\u00e4rm, der Schmutz der n\u00e4cht'gen Stadt,", "tokens": ["Der", "Schen\u00b7ken", "L\u00e4rm", ",", "der", "Schmutz", "der", "n\u00e4cht'\u00b7gen", "Stadt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Welk sinkt von den Platanen Blatt um Blatt,", "tokens": ["Welk", "sinkt", "von", "den", "Pla\u00b7ta\u00b7nen", "Blatt", "um", "Blatt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein alter Omnibus auf schlechten Federn", "tokens": ["Ein", "al\u00b7ter", "Om\u00b7ni\u00b7bus", "auf", "schlech\u00b7ten", "Fe\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Quietscht schief und schwankend zwischen seinen R\u00e4dern", "tokens": ["Qui\u00b7etscht", "schief", "und", "schwan\u00b7kend", "zwi\u00b7schen", "sei\u00b7nen", "R\u00e4\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "KON", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Mit gr\u00fcn' und roten Augen, die sich dreh'n,", "tokens": ["Mit", "gr\u00fcn'", "und", "ro\u00b7ten", "Au\u00b7gen", ",", "die", "sich", "dreh'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "KON", "ADJA", "NN", "$,", "PRELS", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Arbeiter, die zur Kneipe rauchend gehn,", "tokens": ["Ar\u00b7bei\u00b7ter", ",", "die", "zur", "Knei\u00b7pe", "rau\u00b7chend", "gehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPRART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dem Schutzmann qualmend ins Gesicht den Knaster,", "tokens": ["Dem", "Schutz\u00b7mann", "qual\u00b7mend", "ins", "Ge\u00b7sicht", "den", "Knas\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die D\u00e4cher nass, Asphalt und glitschig Pflaster", "tokens": ["Die", "D\u00e4\u00b7cher", "nass", ",", "As\u00b7phalt", "und", "glit\u00b7schig", "Pflas\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "NN", "KON", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und Gossen, die der Regen schwellen liess,", "tokens": ["Und", "Gos\u00b7sen", ",", "die", "der", "Re\u00b7gen", "schwel\u00b7len", "liess", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "PRELS", "ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Das ist mein Weg \u2013 mein Ziel das Paradies.", "tokens": ["Das", "ist", "mein", "Weg", "\u2013", "mein", "Ziel", "das", "Pa\u00b7ra\u00b7dies", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}