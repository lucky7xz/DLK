{"textgrid.poem.24759": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich trat aus meinem Haus und freute mich", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich trat aus meinem Haus und freute mich", "tokens": ["Ich", "trat", "aus", "mei\u00b7nem", "Haus", "und", "freu\u00b7te", "mich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "KON", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "an einer S\u00e4ule, die in meinem Garten", "tokens": ["an", "ei\u00b7ner", "S\u00e4u\u00b7le", ",", "die", "in", "mei\u00b7nem", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "zwischen den beiden \u00e4ltesten der B\u00e4ume", "tokens": ["zwi\u00b7schen", "den", "bei\u00b7den", "\u00e4l\u00b7tes\u00b7ten", "der", "B\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "PIAT", "ADJA", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "beredten Schatten wirft auf einen Rasen.", "tokens": ["be\u00b7red\u00b7ten", "Schat\u00b7ten", "wirft", "auf", "ei\u00b7nen", "Ra\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dem Schatten den sie wirft nickte ich zu", "tokens": ["Dem", "Schat\u00b7ten", "den", "sie", "wirft", "nick\u00b7te", "ich", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "PPER", "VVFIN", "VVFIN", "PPER", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "wie einem Knaben der im Grase l\u00e4ge,", "tokens": ["wie", "ei\u00b7nem", "Kna\u00b7ben", "der", "im", "Gra\u00b7se", "l\u00e4\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "und trat dann durch die h\u00f6lzerne kleine T\u00fcr", "tokens": ["und", "trat", "dann", "durch", "die", "h\u00f6l\u00b7zer\u00b7ne", "klei\u00b7ne", "T\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "heraus aus meinem Garten.", "tokens": ["he\u00b7raus", "aus", "mei\u00b7nem", "Gar\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}