{"textgrid.poem.66744": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mit dem \u00bbDu\u00ab im Herzen darf man schweigen,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit dem \u00bbDu\u00ab im Herzen darf man schweigen,", "tokens": ["Mit", "dem", "\u00bb", "Du", "\u00ab", "im", "Her\u00b7zen", "darf", "man", "schwei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$(", "PPER", "$(", "APPRART", "NN", "VMFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Um so tiefer dann sein Innres zeigen,", "tokens": ["Um", "so", "tie\u00b7fer", "dann", "sein", "Inn\u00b7res", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ADJD", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wenn die Stunde kommt, da ganz allein", "tokens": ["Wenn", "die", "Stun\u00b7de", "kommt", ",", "da", "ganz", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,", "KOUS", "ADV", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Leben sich dem Leben dr\u00e4ngt zu weihn ...", "tokens": ["Le\u00b7ben", "sich", "dem", "Le\u00b7ben", "dr\u00e4ngt", "zu", "weihn", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Und es ist ein still best\u00e4ndig Wissen,", "tokens": ["Und", "es", "ist", "ein", "still", "be\u00b7st\u00e4n\u00b7dig", "Wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "ADJD", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und es ist ein ruhiges Vertrauen:", "tokens": ["Und", "es", "ist", "ein", "ru\u00b7hi\u00b7ges", "Ver\u00b7trau\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.7": {"text": "Unser Freundeskranz wird unzerrissen", "tokens": ["Un\u00b7ser", "Freun\u00b7des\u00b7kranz", "wird", "un\u00b7zer\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Schweben in Maienl\u00fcften wie in rauhen", "tokens": ["Schwe\u00b7ben", "in", "Mai\u00b7en\u00b7l\u00fcf\u00b7ten", "wie", "in", "rau\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "KOKOM", "APPR", "ADJA"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.9": {"text": "Sturmesn\u00e4chten schlimmeren Geschicks ...", "tokens": ["Stur\u00b7mes\u00b7n\u00e4ch\u00b7ten", "schlim\u00b7me\u00b7ren", "Ge\u00b7schicks", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "Nein, es ist kein Rausch des Augenblicks,", "tokens": ["Nein", ",", "es", "ist", "kein", "Rausch", "des", "Au\u00b7gen\u00b7blicks", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "PIAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Wie ihn rasches Jugendblut verdampft,", "tokens": ["Wie", "ihn", "ra\u00b7sches", "Ju\u00b7gend\u00b7blut", "ver\u00b7dampft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.12": {"text": "Keine Traumsaat, die der Tag zerstampft \u2013", "tokens": ["Kei\u00b7ne", "Traum\u00b7saat", ",", "die", "der", "Tag", "zer\u00b7stampft", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.13": {"text": "Wir belauschen unser altes Spiel", "tokens": ["Wir", "be\u00b7lau\u00b7schen", "un\u00b7ser", "al\u00b7tes", "Spiel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.14": {"text": "Und gedenken und besinnen viel ...", "tokens": ["Und", "ge\u00b7den\u00b7ken", "und", "be\u00b7sin\u00b7nen", "viel", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "KON", "VVFIN", "ADV", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}