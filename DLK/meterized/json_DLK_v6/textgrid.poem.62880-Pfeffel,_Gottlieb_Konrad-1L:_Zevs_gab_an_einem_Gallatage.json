{"textgrid.poem.62880": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zevs gab an einem Gallatage", "genre": "verse", "period": "N.A.", "pub_year": 1778, "urn": "N.A.", "language": ["de:0.85", "et:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zevs gab an einem Gallatage", "tokens": ["Zevs", "gab", "an", "ei\u00b7nem", "Gal\u00b7la\u00b7ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In seinem blauen Sternensaal", "tokens": ["In", "sei\u00b7nem", "blau\u00b7en", "Ster\u00b7nen\u00b7saal"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem G\u00f6tterchor ein Mittagsmahl,", "tokens": ["Dem", "G\u00f6t\u00b7ter\u00b7chor", "ein", "Mit\u00b7tags\u00b7mahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So recht nach altem deutschen Schlage.", "tokens": ["So", "recht", "nach", "al\u00b7tem", "deut\u00b7schen", "Schla\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Erbaulich war Prinz Ganymed", "tokens": ["Er\u00b7bau\u00b7lich", "war", "Prinz", "Ga\u00b7ny\u00b7med"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit seinem Becher anzusehen;", "tokens": ["Mit", "sei\u00b7nem", "Be\u00b7cher", "an\u00b7zu\u00b7se\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Er mu\u00dfte stets, wie ein Planet,", "tokens": ["Er", "mu\u00df\u00b7te", "stets", ",", "wie", "ein", "Pla\u00b7net", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Sich um die runde Tafel drehen:", "tokens": ["Sich", "um", "die", "run\u00b7de", "Ta\u00b7fel", "dre\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Auch wies der Nektar seine Kraft;", "tokens": ["Auch", "wies", "der", "Nek\u00b7tar", "sei\u00b7ne", "Kraft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Die Damen l\u00fcfteten das Mieder", "tokens": ["Die", "Da\u00b7men", "l\u00fcf\u00b7te\u00b7ten", "das", "Mie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und Zevs, beym Klange froher Lieder,", "tokens": ["Und", "Zevs", ",", "beym", "Klan\u00b7ge", "fro\u00b7her", "Lie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Trank mit den Herren Br\u00fcderschaft", "tokens": ["Trank", "mit", "den", "Her\u00b7ren", "Br\u00fc\u00b7der\u00b7schaft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.13": {"text": "Und k\u00fc\u00dfte sie mit heissem Munde.", "tokens": ["Und", "k\u00fc\u00df\u00b7te", "sie", "mit", "heis\u00b7sem", "Mun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Herr Momus, dem, wie jeder wei\u00df,", "tokens": ["Herr", "Mo\u00b7mus", ",", "dem", ",", "wie", "je\u00b7der", "wei\u00df", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PRELS", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Nichts recht ist, machte seine Runde", "tokens": ["Nichts", "recht", "ist", ",", "mach\u00b7te", "sei\u00b7ne", "Run\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "ADJD", "VAFIN", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Und nahte h\u00f6hnisch sich dem Kreis:", "tokens": ["Und", "nah\u00b7te", "h\u00f6h\u00b7nisch", "sich", "dem", "Kreis", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Darf ich, o Jupiter, es wagen", "tokens": ["Darf", "ich", ",", "o", "Ju\u00b7pi\u00b7ter", ",", "es", "wa\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "PPER", "$,", "FM", "NN", "$,", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Ein kleines W\u00f6rtchen dich zu fragen? ...", "tokens": ["Ein", "klei\u00b7nes", "W\u00f6rt\u00b7chen", "dich", "zu", "fra\u00b7gen", "?", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "\u00bbla\u00df h\u00f6ren, Herr geheimer Rath!\u00ab", "tokens": ["\u00bb", "la\u00df", "h\u00f6\u00b7ren", ",", "Herr", "ge\u00b7hei\u00b7mer", "Rath", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVIMP", "VVINF", "$,", "NN", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Da doch der Krieg, der Sturm, die Liebe,", "tokens": ["Da", "doch", "der", "Krieg", ",", "der", "Sturm", ",", "die", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Die Reimer, Aerzte, Kuppler, Diebe,", "tokens": ["Die", "Rei\u00b7mer", ",", "A\u00b7erz\u00b7te", ",", "Kupp\u00b7ler", ",", "Die\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.22": {"text": "Kurz alles seine Gottheit hat;", "tokens": ["Kurz", "al\u00b7les", "sei\u00b7ne", "Got\u00b7theit", "hat", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PIS", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "So m\u00f6cht ich unma\u00dfgeblich wissen,", "tokens": ["So", "m\u00f6cht", "ich", "un\u00b7ma\u00df\u00b7geb\u00b7lich", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Warum wir wohl an deinem Hof", "tokens": ["Wa\u00b7rum", "wir", "wohl", "an", "dei\u00b7nem", "Hof"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Der Freundschaft G\u00f6ttin blos vermissen?", "tokens": ["Der", "Freund\u00b7schaft", "G\u00f6t\u00b7tin", "blos", "ver\u00b7mis\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Zum Gl\u00fcck bin ich kein Philosoph,", "tokens": ["Zum", "Gl\u00fcck", "bin", "ich", "kein", "Phi\u00b7lo\u00b7soph", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Sonst w\u00fcrd ich aus dem Umstand schlie\u00dfen,", "tokens": ["Sonst", "w\u00fcrd", "ich", "aus", "dem", "Um\u00b7stand", "schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Da\u00df Wesen aus der Oberwelt", "tokens": ["Da\u00df", "We\u00b7sen", "aus", "der", "O\u00b7ber\u00b7welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Zur Freundschaft nicht gestimmt seyn m\u00fc\u00dfen;", "tokens": ["Zur", "Freund\u00b7schaft", "nicht", "ge\u00b7stimmt", "seyn", "m\u00fc\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKNEG", "VVPP", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Allein das Gegentheil erhellt", "tokens": ["Al\u00b7lein", "das", "Ge\u00b7gen\u00b7theil", "er\u00b7hellt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Aus euren br\u00fcderlichen K\u00fcssen.", "tokens": ["Aus", "eu\u00b7ren", "br\u00fc\u00b7der\u00b7li\u00b7chen", "K\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Beym Styx! rief Zevs, da\u00df Tisch und Saal", "tokens": ["Beym", "Styx", "!", "rief", "Zevs", ",", "da\u00df", "Tisch", "und", "Saal"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$.", "VVFIN", "NE", "$,", "KOUS", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Vom Schwur erbebten, diesesmal", "tokens": ["Vom", "Schwur", "er\u00b7beb\u00b7ten", ",", "die\u00b7ses\u00b7mal"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPRART", "NN", "VVFIN", "$,", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Hat Momus Recht! Ihr Herrn, ich d\u00e4chte,", "tokens": ["Hat", "Mo\u00b7mus", "Recht", "!", "Ihr", "Herrn", ",", "ich", "d\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "NE", "NN", "$.", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Wir h\u00e4lfen stracks dem Mangel ab.", "tokens": ["Wir", "h\u00e4l\u00b7fen", "stracks", "dem", "Man\u00b7gel", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Er sprach es und erhob die Rechte,", "tokens": ["Er", "sprach", "es", "und", "er\u00b7hob", "die", "Rech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Die der Natur ihr Daseyn gab.", "tokens": ["Die", "der", "Na\u00b7tur", "ihr", "Da\u00b7seyn", "gab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.38": {"text": "Flugs stand ein Bild, die holdste Dirne,", "tokens": ["Flugs", "stand", "ein", "Bild", ",", "die", "holds\u00b7te", "Dir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "Die der Olymp noch werden sah,", "tokens": ["Die", "der", "O\u00b7lymp", "noch", "wer\u00b7den", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "VAINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Mit Venus Reitz, mit Lunens Stirne", "tokens": ["Mit", "Ve\u00b7nus", "Reitz", ",", "mit", "Lu\u00b7nens", "Stir\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NE", "NE", "$,", "APPR", "NE", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Und Pallas stiller W\u00fcrde da.", "tokens": ["Und", "Pal\u00b7las", "stil\u00b7ler", "W\u00fcr\u00b7de", "da", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Ha bravo! rief der helle Haufen.", "tokens": ["Ha", "bra\u00b7vo", "!", "rief", "der", "hel\u00b7le", "Hau\u00b7fen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "$.", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Sie kommen in gedr\u00e4ngten Reihn", "tokens": ["Sie", "kom\u00b7men", "in", "ge\u00b7dr\u00e4ng\u00b7ten", "Reihn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Der neuen Schwester zugelaufen", "tokens": ["Der", "neu\u00b7en", "Schwes\u00b7ter", "zu\u00b7ge\u00b7lau\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "Und weihen sie mit K\u00fcssen ein.", "tokens": ["Und", "wei\u00b7hen", "sie", "mit", "K\u00fcs\u00b7sen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Zevs macht sie sich zur Seite sitzen,", "tokens": ["Zevs", "macht", "sie", "sich", "zur", "Sei\u00b7te", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PRF", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.47": {"text": "Dr\u00fcckt zehnmal ihr die wei\u00dfe Hand,", "tokens": ["Dr\u00fcckt", "zehn\u00b7mal", "ihr", "die", "wei\u00b7\u00dfe", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Und t\u00e4ndelt mit des Halstuchs Spitzen.", "tokens": ["Und", "t\u00e4n\u00b7delt", "mit", "des", "Hal\u00b7stuchs", "Spit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.49": {"text": "Frau Juno roch die Lunte gleich;", "tokens": ["Frau", "Ju\u00b7no", "roch", "die", "Lun\u00b7te", "gleich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Sie zittert, wird bald roth, bald bleich,", "tokens": ["Sie", "zit\u00b7tert", ",", "wird", "bald", "roth", ",", "bald", "bleich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VAFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Und raunt den Schwestern in die Ohren.", "tokens": ["Und", "raunt", "den", "Schwes\u00b7tern", "in", "die", "Oh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.52": {"text": "Im Huy entspinnt sich ein Complot;", "tokens": ["Im", "Huy", "ent\u00b7spinnt", "sich", "ein", "Com\u00b7plot", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NE", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.53": {"text": "Der Sch\u00f6nen wird der Sturz geschworen;", "tokens": ["Der", "Sch\u00f6\u00b7nen", "wird", "der", "Sturz", "ge\u00b7schwo\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.54": {"text": "Und kurz, man zwang den armen Gott,", "tokens": ["Und", "kurz", ",", "man", "zwang", "den", "ar\u00b7men", "Gott", ","], "token_info": ["word", "word",