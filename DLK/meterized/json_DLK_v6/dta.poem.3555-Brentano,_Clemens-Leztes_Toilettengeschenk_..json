{"dta.poem.3555": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Leztes Toilettengeschenk .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zart A\u00fcglein zu winken,               ", "tokens": ["Zart", "A\u00b7\u00fc\u00b7glein", "zu", "win\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "PTKZU", "VVINF", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Die M\u00e4gdlein jetzund han;", "tokens": ["Die", "M\u00e4gd\u00b7lein", "je\u00b7tzund", "han", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr Angesicht zu schminken,", "tokens": ["Ihr", "An\u00b7ge\u00b7sicht", "zu", "schmin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gro\u00df Flei\u00df sie legen an.", "tokens": ["Gro\u00df", "Flei\u00df", "sie", "le\u00b7gen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ihr Haupt thun sie beladen,", "tokens": ["Ihr", "Haupt", "thun", "sie", "be\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit Gold und Perlen schon;", "tokens": ["Mit", "Gold", "und", "Per\u00b7len", "schon", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sollten sie's bezahlen,", "tokens": ["Und", "soll\u00b7ten", "sie's", "be\u00b7zah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie br\u00e4chten nichts davon.", "tokens": ["Sie", "br\u00e4ch\u00b7ten", "nichts", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie m\u00fcssen seyn geschm\u00fccket,", "tokens": ["Sie", "m\u00fcs\u00b7sen", "seyn", "ge\u00b7schm\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df es nur hab gro\u00df Schein;", "tokens": ["Da\u00df", "es", "nur", "hab", "gro\u00df", "Schein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ob sie schon Armuth dr\u00fccket,", "tokens": ["Ob", "sie", "schon", "Ar\u00b7muth", "dr\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Geborget mu\u00df es seyn.", "tokens": ["Ge\u00b7bor\u00b7get", "mu\u00df", "es", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da\u00df man sie doch lieb habe,", "tokens": ["Da\u00df", "man", "sie", "doch", "lieb", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.2": {"text": "(wenn ja solchs h\u00fclfe nicht,)", "tokens": ["(", "wenn", "ja", "solchs", "h\u00fcl\u00b7fe", "nicht", ",", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KOUS", "ADV", "PIS", "VVFIN", "PTKNEG", "$,", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So gebens selbst aus Gaben,", "tokens": ["So", "ge\u00b7bens", "selbst", "aus", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie man erf\u00e4hrt und sicht.", "tokens": ["Wie", "man", "er\u00b7f\u00e4hrt", "und", "sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wenn sie den Knaben haben,", "tokens": ["Wenn", "sie", "den", "Kna\u00b7ben", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und jeder bezahlt will seyn;", "tokens": ["Und", "je\u00b7der", "be\u00b7zahlt", "will", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "VMFIN", "VAINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mu\u00df viel zum Juden traben,", "tokens": ["Mu\u00df", "viel", "zum", "Ju\u00b7den", "tra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was vor gab gro\u00dfen Schein.", "tokens": ["Was", "vor", "gab", "gro\u00b7\u00dfen", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}