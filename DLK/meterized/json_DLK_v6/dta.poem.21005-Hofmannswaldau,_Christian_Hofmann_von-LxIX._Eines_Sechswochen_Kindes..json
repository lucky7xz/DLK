{"dta.poem.21005": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "LxIX.  \n Eines Sechswochen Kindes.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Ich gr\u00fcste kaum die Welt und derer weite Pracht/", "tokens": ["Ich", "gr\u00fcs\u00b7te", "kaum", "die", "Welt", "und", "de\u00b7rer", "wei\u00b7te", "Pracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "KON", "PDS", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So zwang mich meine Schuld zu geben gute Nacht.", "tokens": ["So", "zwang", "mich", "mei\u00b7ne", "Schuld", "zu", "ge\u00b7ben", "gu\u00b7te", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "PTKZU", "VVINF", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das Fr\u00fchst\u00fcck hatt\u2019 ich kaum in meinen Mund ge-", "tokens": ["Das", "Fr\u00fch\u00b7st\u00fcck", "hatt'", "ich", "kaum", "in", "mei\u00b7nen", "Mund", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "nommen/", "tokens": ["nom\u00b7men", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "So war der Pa\u00dfport mir auch in die H\u00e4nde kom-", "tokens": ["So", "war", "der", "Pa\u00df\u00b7port", "mir", "auch", "in", "die", "H\u00e4n\u00b7de", "kom"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "ADV", "APPR", "ART", "NN", "TRUNC"], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.6": {"text": "men.", "tokens": ["men", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-", "measure": "single.down"}}}}}