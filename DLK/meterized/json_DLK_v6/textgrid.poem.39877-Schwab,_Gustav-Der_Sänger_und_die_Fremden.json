{"textgrid.poem.39877": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Der S\u00e4nger und die Fremden", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Harfner sitzt auf moos'gen Steinen,", "tokens": ["Ein", "Harf\u00b7ner", "sitzt", "auf", "moos'\u00b7gen", "Stei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er l\u00e4\u00dft das Volk des Weges ziehn,", "tokens": ["Er", "l\u00e4\u00dft", "das", "Volk", "des", "We\u00b7ges", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er spielt und k\u00fcmmert sich um Keinen,", "tokens": ["Er", "spielt", "und", "k\u00fcm\u00b7mert", "sich", "um", "Kei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Keiner k\u00fcmmert sich um ihn.", "tokens": ["Und", "Kei\u00b7ner", "k\u00fcm\u00b7mert", "sich", "um", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PRF", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Zuweilen schielet wohl den S\u00e4nger", "tokens": ["Zu\u00b7wei\u00b7len", "schie\u00b7let", "wohl", "den", "S\u00e4n\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Waidmann oder Pfl\u00fcger an,", "tokens": ["Ein", "Waid\u00b7mann", "o\u00b7der", "Pfl\u00fc\u00b7ger", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und denkt: Wer ist der M\u00fc\u00dfigg\u00e4nger,", "tokens": ["Und", "denkt", ":", "Wer", "ist", "der", "M\u00fc\u00b7\u00dfig\u00b7g\u00e4n\u00b7ger", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PWS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der nur zum Liede klimpern kann?", "tokens": ["Der", "nur", "zum", "Lie\u00b7de", "klim\u00b7pern", "kann", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Man sieht, es mag ihn Niemand h\u00f6ren,", "tokens": ["Man", "sieht", ",", "es", "mag", "ihn", "Nie\u00b7mand", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PPER", "VMFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er f\u00e4hrt, in sich versunken, fort,", "tokens": ["Er", "f\u00e4hrt", ",", "in", "sich", "ver\u00b7sun\u00b7ken", ",", "fort", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "PRF", "VVINF", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als spielt' und s\u00e4ng' er Geisterch\u00f6ren,", "tokens": ["Als", "spielt'", "und", "s\u00e4ng'", "er", "Geis\u00b7ter\u00b7ch\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "KON", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So in der Wolke lauschen dort.", "tokens": ["So", "in", "der", "Wol\u00b7ke", "lau\u00b7schen", "dort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Jetzt nimmt der Wind auf seinen Fl\u00fcgel", "tokens": ["Jetzt", "nimmt", "der", "Wind", "auf", "sei\u00b7nen", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Ton, der in den L\u00fcften schwamm;", "tokens": ["Den", "Ton", ",", "der", "in", "den", "L\u00fcf\u00b7ten", "schwamm", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und tr\u00e4gt ihn \u00fcber gr\u00fcne H\u00fcgel", "tokens": ["Und", "tr\u00e4gt", "ihn", "\u00fc\u00b7ber", "gr\u00fc\u00b7ne", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In's Thal, zu einem frohen Stamm.", "tokens": ["In's", "Thal", ",", "zu", "ei\u00b7nem", "fro\u00b7hen", "Stamm", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Da spielt ums Ohr der Hirtens\u00f6hne", "tokens": ["Da", "spielt", "ums", "Ohr", "der", "Hir\u00b7ten\u00b7s\u00f6h\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der ferne, wunderbare Klang,", "tokens": ["Der", "fer\u00b7ne", ",", "wun\u00b7der\u00b7ba\u00b7re", "Klang", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Frauen horchen auf die T\u00f6ne,", "tokens": ["Die", "Frau\u00b7en", "hor\u00b7chen", "auf", "die", "T\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und mancher pilgert nach dem Sang.", "tokens": ["Und", "man\u00b7cher", "pil\u00b7gert", "nach", "dem", "Sang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Sie steigen von dem Berge nieder,", "tokens": ["Sie", "stei\u00b7gen", "von", "dem", "Ber\u00b7ge", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie reihn sich um den Mann im Kreis", "tokens": ["Sie", "reihn", "sich", "um", "den", "Mann", "im", "Kreis"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und trinken seine s\u00fc\u00dfen Lieder,", "tokens": ["Und", "trin\u00b7ken", "sei\u00b7ne", "s\u00fc\u00b7\u00dfen", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Inde\u00df er nichts von ihnen wei\u00df.", "tokens": ["In\u00b7de\u00df", "er", "nichts", "von", "ih\u00b7nen", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Die M\u00fctter mit den T\u00f6chtern lauschen,", "tokens": ["Die", "M\u00fct\u00b7ter", "mit", "den", "T\u00f6ch\u00b7tern", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie senken hold ihr Lockenhaupt,", "tokens": ["Sie", "sen\u00b7ken", "hold", "ihr", "Lo\u00b7cken\u00b7haupt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Harfners T\u00f6ne m\u00e4chtig rauschen,", "tokens": ["Des", "Harf\u00b7ners", "T\u00f6\u00b7ne", "m\u00e4ch\u00b7tig", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der immer noch sich einsam glaubt.", "tokens": ["Der", "im\u00b7mer", "noch", "sich", "ein\u00b7sam", "glaubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Doch wie er nun sein Lied geendet,", "tokens": ["Doch", "wie", "er", "nun", "sein", "Lied", "ge\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schl\u00e4gt er die Augen auf, erschrickt \u2013", "tokens": ["Schl\u00e4gt", "er", "die", "Au\u00b7gen", "auf", ",", "er\u00b7schrickt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er spricht: \u00bbWer hat mir euch gesendet,", "tokens": ["Er", "spricht", ":", "\u00bb", "Wer", "hat", "mir", "euch", "ge\u00b7sen\u00b7det", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "PWS", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Euch, so in Wolken ich erblickt?\u00ab", "tokens": ["Euch", ",", "so", "in", "Wol\u00b7ken", "ich", "er\u00b7blickt", "?", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "$,", "ADV", "APPR", "NN", "PPER", "VVPP", "$.", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.9": {"line.1": {"text": "Und voller schl\u00e4gt er in die Saiten:", "tokens": ["Und", "vol\u00b7ler", "schl\u00e4gt", "er", "in", "die", "Sai\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbnimm an, o Muse, mein Gebet!", "tokens": ["\u00bb", "nimm", "an", ",", "o", "Mu\u00b7se", ",", "mein", "Ge\u00b7bet", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PTKVZ", "$,", "FM", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Du tr\u00e4gst mein Lied in alle Weiten,", "tokens": ["Du", "tr\u00e4gst", "mein", "Lied", "in", "al\u00b7le", "Wei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn es die N\u00e4he nicht versteht!", "tokens": ["Wenn", "es", "die", "N\u00e4\u00b7he", "nicht", "ver\u00b7steht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Du h\u00fctest deines S\u00e4ngers Ehre,", "tokens": ["Du", "h\u00fc\u00b7test", "dei\u00b7nes", "S\u00e4n\u00b7gers", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nie bleibt um ihn die St\u00e4tte leer;", "tokens": ["Nie", "bleibt", "um", "ihn", "die", "St\u00e4t\u00b7te", "leer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du br\u00e4chtest ihm selbst \u00fcber Meere", "tokens": ["Du", "br\u00e4ch\u00b7test", "ihm", "selbst", "\u00fc\u00b7ber", "Mee\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Ohr, das ihn vernommen, her.\u00ab", "tokens": ["Das", "Ohr", ",", "das", "ihn", "ver\u00b7nom\u00b7men", ",", "her", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}