{"textgrid.poem.38494": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Konterfait und Aussteuer", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Schatz ist kreidewei\u00df,", "tokens": ["Mein", "Schatz", "ist", "krei\u00b7de\u00b7wei\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Hat krumme Glieder,", "tokens": ["Hat", "krum\u00b7me", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Geht schief zum Thor hinaus,", "tokens": ["Geht", "schief", "zum", "Thor", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "APZR", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "K\u00f6mmt bucklicht wieder.", "tokens": ["K\u00f6mmt", "buck\u00b7licht", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Ein ungleich Paar Ochsen,", "tokens": ["Ein", "un\u00b7gleich", "Paar", "Och\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Eine bucklichte Kuh,", "tokens": ["Ei\u00b7ne", "buck\u00b7lich\u00b7te", "Kuh", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Die giebt mir meine Mutter,", "tokens": ["Die", "giebt", "mir", "mei\u00b7ne", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn ich heurathen thu.", "tokens": ["Wenn", "ich", "heu\u00b7ra\u00b7then", "thu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}