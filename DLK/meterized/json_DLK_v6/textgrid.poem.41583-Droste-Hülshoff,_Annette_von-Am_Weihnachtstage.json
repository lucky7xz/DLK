{"textgrid.poem.41583": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am Weihnachtstage", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch alle Stra\u00dfen w\u00e4lzt sich das Get\u00fcmmel,", "tokens": ["Durch", "al\u00b7le", "Stra\u00b7\u00dfen", "w\u00e4lzt", "sich", "das", "Ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Maultier', Kamele, Treiber; welch Gebimmel!", "tokens": ["Maul\u00b7tier'", ",", "Ka\u00b7me\u00b7le", ",", "Trei\u00b7ber", ";", "welch", "Ge\u00b7bim\u00b7mel", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "NN", "$.", "PWAT", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Als wolle wieder in die Steppe ziehn", "tokens": ["Als", "wol\u00b7le", "wie\u00b7der", "in", "die", "Step\u00b7pe", "ziehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VMFIN", "ADV", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Same Jakobs, und Jud\u00e4as Himmel,", "tokens": ["Der", "Sa\u00b7me", "Ja\u00b7kobs", ",", "und", "Ju\u00b7d\u00e4\u00b7as", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "KON", "NE", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ein Saphirscheinen \u00fcber dem Gewimmel,", "tokens": ["Ein", "Sa\u00b7phir\u00b7schei\u00b7nen", "\u00fc\u00b7ber", "dem", "Ge\u00b7wim\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "L\u00e4\u00dft blendend seine Funkenstr\u00f6me spr\u00fchn.", "tokens": ["L\u00e4\u00dft", "blen\u00b7dend", "sei\u00b7ne", "Fun\u00b7ken\u00b7str\u00f6\u00b7me", "spr\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Verschleiert Frauen durch die Gassen schreiten,", "tokens": ["Ver\u00b7schlei\u00b7ert", "Frau\u00b7en", "durch", "die", "Gas\u00b7sen", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "M\u00fchselig vom beladnen Tiere gleiten", "tokens": ["M\u00fch\u00b7se\u00b7lig", "vom", "be\u00b7lad\u00b7nen", "Tie\u00b7re", "glei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "VVINF"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Bejahrte M\u00fctterchen; all\u00fcberall", "tokens": ["Be\u00b7jahr\u00b7te", "M\u00fct\u00b7ter\u00b7chen", ";", "al\u00b7l\u00fc\u00b7be\u00b7rall"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADJA", "NN", "$.", "PIAT"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Geschrei und Treiben, wie vor Jehus Wagen.", "tokens": ["Ge\u00b7schrei", "und", "Trei\u00b7ben", ",", "wie", "vor", "Je\u00b7hus", "Wa\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "PWAV", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "L\u00e4\u00dft wieder Jezabel ihr Antlitz ragen", "tokens": ["L\u00e4\u00dft", "wie\u00b7der", "Je\u00b7za\u00b7bel", "ihr", "Ant\u00b7litz", "ra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "NE", "PPOSAT", "NN", "VVINF"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Aus jener S\u00e4ulen luftigem Portal?", "tokens": ["Aus", "je\u00b7ner", "S\u00e4u\u00b7len", "luf\u00b7ti\u00b7gem", "Por\u00b7tal", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "'s ist Rom, die \u00fcpp'ge Priesterin der G\u00f6tzen,", "tokens": ["'s", "ist", "Rom", ",", "die", "\u00fcpp'\u00b7ge", "Pries\u00b7te\u00b7rin", "der", "G\u00f6t\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "$,", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Die gl\u00e4nzendste und grausamste der Metzen,", "tokens": ["Die", "gl\u00e4n\u00b7zends\u00b7te", "und", "grau\u00b7sams\u00b7te", "der", "Met\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ADJA", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die ihre Sklaven z\u00e4hlt zu dieser Zeit.", "tokens": ["Die", "ih\u00b7re", "Skla\u00b7ven", "z\u00e4hlt", "zu", "die\u00b7ser", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit einem Griffel, noch vom Blute tr\u00e4ufend,", "tokens": ["Mit", "ei\u00b7nem", "Grif\u00b7fel", ",", "noch", "vom", "Blu\u00b7te", "tr\u00e4u\u00b7fend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Gr\u00e4bt sie in Tafeln, Zahl auf Zahlen h\u00e4ufend,", "tokens": ["Gr\u00e4bt", "sie", "in", "Ta\u00b7feln", ",", "Zahl", "auf", "Zah\u00b7len", "h\u00e4u\u00b7fend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Buhlen Namen, so ihr Schwert gefreit.", "tokens": ["Der", "Buh\u00b7len", "Na\u00b7men", ",", "so", "ihr", "Schwert", "ge\u00b7freit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "O Israel, wo ist dein Stolz geblieben,", "tokens": ["O", "Is\u00b7rael", ",", "wo", "ist", "dein", "Stolz", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PWAV", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Hast du die H\u00e4nde blutig nicht gerieben,", "tokens": ["Hast", "du", "die", "H\u00e4n\u00b7de", "blu\u00b7tig", "nicht", "ge\u00b7rie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADJD", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und deine Tr\u00e4ne war sie siedend Blut?", "tokens": ["Und", "dei\u00b7ne", "Tr\u00e4\u00b7ne", "war", "sie", "sie\u00b7dend", "Blut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPER", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nein, als zum Marktplatz deine Scharen wallen", "tokens": ["Nein", ",", "als", "zum", "Markt\u00b7platz", "dei\u00b7ne", "Scha\u00b7ren", "wal\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "KOUS", "APPRART", "NN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Verkaufend, feilschend unter Tempels Hallen,", "tokens": ["Ver\u00b7kau\u00b7fend", ",", "feil\u00b7schend", "un\u00b7ter", "Tem\u00b7pels", "Hal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mit ihrem Gott zerronnen ist ihr Mut!", "tokens": ["Mit", "ih\u00b7rem", "Gott", "zer\u00b7ron\u00b7nen", "ist", "ihr", "Mut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Zum tr\u00fcben Irrwisch ward die Feuers\u00e4ule,", "tokens": ["Zum", "tr\u00fc\u00b7ben", "Irr\u00b7wisch", "ward", "die", "Feu\u00b7er\u00b7s\u00e4u\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der gr\u00fcne Aaronsstab zum Henkerbeile;", "tokens": ["Der", "gr\u00fc\u00b7ne", "Aa\u00b7rons\u00b7stab", "zum", "Hen\u00b7ker\u00b7bei\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und grausig \u00fcbersteint das tote Wort", "tokens": ["Und", "grau\u00b7sig", "\u00fc\u00b7bers\u00b7teint", "das", "to\u00b7te", "Wort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Liegt, eine Mumie, im heil'gen Buche,", "tokens": ["Liegt", ",", "ei\u00b7ne", "Mu\u00b7mie", ",", "im", "heil'\u00b7gen", "Bu\u00b7che", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Drin sucht der Pharis\u00e4er nach dem Fluche,", "tokens": ["Drin", "sucht", "der", "Pha\u00b7ri\u00b7s\u00e4\u00b7er", "nach", "dem", "Flu\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ihn donnernd \u00fcber Freund und Fremdling fort.", "tokens": ["Ihn", "don\u00b7nernd", "\u00fc\u00b7ber", "Freund", "und", "Fremd\u00b7ling", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "So Israel bist du gereift zum Schnitte,", "tokens": ["So", "Is\u00b7rael", "bist", "du", "ge\u00b7reift", "zum", "Schnit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VAFIN", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-++-+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Wie reift die Distel in der Saaten Mitte,", "tokens": ["Wie", "reift", "die", "Dis\u00b7tel", "in", "der", "Saa\u00b7ten", "Mit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wie du stehst in deinem grimmen Ha\u00df", "tokens": ["Und", "wie", "du", "stehst", "in", "dei\u00b7nem", "grim\u00b7men", "Ha\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gen\u00fcber der geschminkt und hohlen Buhle,", "tokens": ["Ge\u00b7n\u00fc\u00b7ber", "der", "ge\u00b7schminkt", "und", "hoh\u00b7len", "Buh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVPP", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Seid gleich ihr vor gerechtem Richterstuhle", "tokens": ["Seid", "gleich", "ihr", "vor", "ge\u00b7rech\u00b7tem", "Rich\u00b7ter\u00b7stuh\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAIMP", "ADJD", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Von Blute sie und du von Geifer na\u00df.", "tokens": ["Von", "Blu\u00b7te", "sie", "und", "du", "von", "Gei\u00b7fer", "na\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "KON", "PPER", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "O tauet Himmel, tauet den Gerechten,", "tokens": ["O", "tau\u00b7et", "Him\u00b7mel", ",", "tau\u00b7et", "den", "Ge\u00b7rech\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr Wolken regnet ihn den wahr und echten", "tokens": ["Ihr", "Wol\u00b7ken", "reg\u00b7net", "ihn", "den", "wahr", "und", "ech\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ART", "ADJD", "KON", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Messias, den Jud\u00e4a nicht erharrt,", "tokens": ["Mes\u00b7si\u00b7as", ",", "den", "Ju\u00b7d\u00e4a", "nicht", "er\u00b7harrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Den Heiligen und Milden und Gerechten,", "tokens": ["Den", "Hei\u00b7li\u00b7gen", "und", "Mil\u00b7den", "und", "Ge\u00b7rech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Den Friedensk\u00f6nig unter Hassesknechten,", "tokens": ["Den", "Frie\u00b7dens\u00b7k\u00f6\u00b7nig", "un\u00b7ter", "Has\u00b7ses\u00b7knech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Gekommen zu erw\u00e4rmen was erstarrt!", "tokens": ["Ge\u00b7kom\u00b7men", "zu", "er\u00b7w\u00e4r\u00b7men", "was", "er\u00b7starrt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PTKZU", "VVINF", "PWS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Still ist die Nacht; in seinem Zelt geborgen", "tokens": ["Still", "ist", "die", "Nacht", ";", "in", "sei\u00b7nem", "Zelt", "ge\u00b7bor\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$.", "APPR", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Schriftgelehrte sp\u00e4ht mit finstren Sorgen,", "tokens": ["Der", "Schrift\u00b7ge\u00b7lehr\u00b7te", "sp\u00e4ht", "mit", "finst\u00b7ren", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wann Judas m\u00e4chtiger Tyrann erscheint;", "tokens": ["Wann", "Ju\u00b7das", "m\u00e4ch\u00b7ti\u00b7ger", "Ty\u00b7rann", "er\u00b7scheint", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dann l\u00fcftet er den Vorhang starrend lange", "tokens": ["Dann", "l\u00fcf\u00b7tet", "er", "den", "Vor\u00b7hang", "star\u00b7rend", "lan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADJD", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dem Sterne nach, der streicht des \u00c4thers Wange", "tokens": ["Dem", "Ster\u00b7ne", "nach", ",", "der", "streicht", "des", "\u00c4\u00b7thers", "Wan\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$,", "PRELS", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wie Freudenz\u00e4hre, die der Himmel weint.", "tokens": ["Wie", "Freu\u00b7den\u00b7z\u00e4h\u00b7re", ",", "die", "der", "Him\u00b7mel", "weint", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.pent