{"textgrid.poem.61217": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchmorgens", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df nicht, s\u00e4uselt' in den B\u00e4umen", "tokens": ["Ich", "wei\u00df", "nicht", ",", "s\u00e4u\u00b7selt'", "in", "den", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Fr\u00fchlings Zauberlied zu Nacht?", "tokens": ["Des", "Fr\u00fch\u00b7lings", "Zau\u00b7berl\u00b7ied", "zu", "Nacht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aus unerkl\u00e4rlich holden Tr\u00e4umen", "tokens": ["Aus", "un\u00b7er\u00b7kl\u00e4r\u00b7lich", "hol\u00b7den", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bin fr\u00fch und frisch ich heut erwacht.", "tokens": ["Bin", "fr\u00fch", "und", "frisch", "ich", "heut", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ADJD", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Morgen weht mit goldner Schwinge", "tokens": ["Der", "Mor\u00b7gen", "weht", "mit", "gold\u00b7ner", "Schwin\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mir um die Stirn den k\u00fchlen Schein;", "tokens": ["Mir", "um", "die", "Stirn", "den", "k\u00fch\u00b7len", "Schein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Noch m\u00f6cht' ich rasten, doch ich singe,", "tokens": ["Noch", "m\u00f6cht'", "ich", "ras\u00b7ten", ",", "doch", "ich", "sin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "$,", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mein Herz ist wie der Himmel rein.", "tokens": ["Mein", "Herz", "ist", "wie", "der", "Him\u00b7mel", "rein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "KOKOM", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "In s\u00fc\u00dfen Schauern r\u00fchrt sich wieder,", "tokens": ["In", "s\u00fc\u00b7\u00dfen", "Schau\u00b7ern", "r\u00fchrt", "sich", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PRF", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was je gebl\u00fcht in meiner Brust,", "tokens": ["Was", "je", "ge\u00b7bl\u00fcht", "in", "mei\u00b7ner", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und alte Liebe, junge Lieder", "tokens": ["Und", "al\u00b7te", "Lie\u00b7be", ",", "jun\u00b7ge", "Lie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJA", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Empfind' ich in vereinter Lust,", "tokens": ["Emp\u00b7find'", "ich", "in", "ver\u00b7ein\u00b7ter", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So wie der Schwan, der seine Bogen", "tokens": ["So", "wie", "der", "Schwan", ",", "der", "sei\u00b7ne", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Auf blauem Wasser kreisend zieht,", "tokens": ["Auf", "blau\u00b7em", "Was\u00b7ser", "krei\u00b7send", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zugleich im Spiegelglanz der Wogen", "tokens": ["Zu\u00b7gleich", "im", "Spie\u00b7gel\u00b7glanz", "der", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Den Himmel mit den Sternen sieht.", "tokens": ["Den", "Him\u00b7mel", "mit", "den", "Ster\u00b7nen", "sieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}