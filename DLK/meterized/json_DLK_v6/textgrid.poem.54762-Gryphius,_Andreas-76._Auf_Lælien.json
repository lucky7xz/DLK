{"textgrid.poem.54762": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "76. Auf L\u00e6lien", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Marin hi\u00df ", "tokens": ["Ma\u00b7rin", "hi\u00df"], "token_info": ["word", "word"], "pos": ["NE", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Der Lelien die ihn so steiff verlacht/", "tokens": ["Der", "Le\u00b7li\u00b7en", "die", "ihn", "so", "steiff", "ver\u00b7lacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPER", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Marin hat sich mehr als zu wohl bedacht/", "tokens": ["Ma\u00b7rin", "hat", "sich", "mehr", "als", "zu", "wohl", "be\u00b7dacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PRF", "PIAT", "KOKOM", "APPR", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Was Demand-fest wie sie kan nichts als Demand zwingen.", "tokens": ["Was", "De\u00b7man\u00b7d\u00b7fest", "wie", "sie", "kan", "nichts", "als", "De\u00b7mand", "zwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KOKOM", "PPER", "VMFIN", "PIS", "KOKOM", "NN", "VVINF", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}