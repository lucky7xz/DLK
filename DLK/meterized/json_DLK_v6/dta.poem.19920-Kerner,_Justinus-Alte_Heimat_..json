{"dta.poem.19920": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Alte Heimat .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In einem dunklen Thal,", "tokens": ["In", "ei\u00b7nem", "dunk\u00b7len", "Thal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Lag ich j\u00fcngst tr\u00e4umend nieder,", "tokens": ["Lag", "ich", "j\u00fcngst", "tr\u00e4u\u00b7mend", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da sah ich einen Stral", "tokens": ["Da", "sah", "ich", "ei\u00b7nen", "Stral"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Von meiner Heimat wieder.", "tokens": ["Von", "mei\u00b7ner", "Hei\u00b7mat", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Auf morgenrother Au", "tokens": ["Auf", "mor\u00b7gen\u00b7ro\u00b7ther", "Au"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "War Vaters Haus gelegen;", "tokens": ["War", "Va\u00b7ters", "Haus", "ge\u00b7le\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie war der Himmel blau!", "tokens": ["Wie", "war", "der", "Him\u00b7mel", "blau", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Flur, wie reich an Segen!", "tokens": ["Die", "Flur", ",", "wie", "reich", "an", "Se\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wie war mein Heimatland", "tokens": ["Wie", "war", "mein", "Hei\u00b7mat\u00b7land"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Voll Gold und Rosenhelle!", "tokens": ["Voll", "Gold", "und", "Ro\u00b7se\u00b7nhel\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch bald der Traum verschwand,", "tokens": ["Doch", "bald", "der", "Traum", "ver\u00b7schwand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Schmerz trat an seine Stelle.", "tokens": ["Schmerz", "trat", "an", "sei\u00b7ne", "Stel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Da irr't ich weit hinaus", "tokens": ["Da", "irr't", "ich", "weit", "hin\u00b7aus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "In's \u00f6de Land voll Sehnen;", "tokens": ["In's", "\u00f6\u00b7de", "Land", "voll", "Seh\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Noch irr' ich, such das Haus,", "tokens": ["Noch", "irr'", "ich", ",", "such", "das", "Haus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und find es nicht vor Thr\u00e4nen.", "tokens": ["Und", "find", "es", "nicht", "vor", "Thr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}