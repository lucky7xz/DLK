{"dta.poem.7773": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Die Geschichte des Davids,  \n K\u00f6nigs in Juda.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198429", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der alte Jsai wie er di\u00df kaum vernommen,", "tokens": ["Der", "al\u00b7te", "Jsai", "wie", "er", "di\u00df", "kaum", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "PWAV", "PPER", "PDS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wollt nicht, da\u00df David sollt mit in das Treffen kommen,", "tokens": ["Wollt", "nicht", ",", "da\u00df", "Da\u00b7vid", "sollt", "mit", "in", "das", "Tref\u00b7fen", "kom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "$,", "KOUS", "NE", "VMFIN", "APPR", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil seine Jugend ihn noch sch\u00fctzte, da\u00df er nicht", "tokens": ["Weil", "sei\u00b7ne", "Ju\u00b7gend", "ihn", "noch", "sch\u00fctz\u00b7te", ",", "da\u00df", "er", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "ADV", "VVFIN", "$,", "KOUS", "PPER", "PTKNEG"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dem K\u00f6nig dienen dorfft, nach der Soldaten Pflicht;", "tokens": ["Dem", "K\u00f6\u00b7nig", "die\u00b7nen", "dorfft", ",", "nach", "der", "Sol\u00b7da\u00b7ten", "Pflicht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$,", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Drum seine \u00e4ltste S\u00f6hn er nach dem Heere sandte,", "tokens": ["Drum", "sei\u00b7ne", "\u00e4lts\u00b7te", "S\u00f6hn", "er", "nach", "dem", "Hee\u00b7re", "sand\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Daf\u00fcr er David nun bey ihm zu seyn ernannte,", "tokens": ["Da\u00b7f\u00fcr", "er", "Da\u00b7vid", "nun", "bey", "ihm", "zu", "seyn", "er\u00b7nann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "NE", "ADV", "APPR", "PPER", "PTKZU", "VAINF", "VVFIN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Bis dieser Krieg f\u00fcrbey, und lie\u00df es Saul geschehn,", "tokens": ["Bis", "die\u00b7ser", "Krieg", "f\u00fcr\u00b7bey", ",", "und", "lie\u00df", "es", "Saul", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df David wiederum nach Bethlehem m\u00f6cht gehn;", "tokens": ["Da\u00df", "Da\u00b7vid", "wie\u00b7de\u00b7rum", "nach", "Beth\u00b7le\u00b7hem", "m\u00f6cht", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADV", "APPR", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der dann in allem sich so willig wust zu geben,", "tokens": ["Der", "dann", "in", "al\u00b7lem", "sich", "so", "wil\u00b7lig", "wust", "zu", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PIS", "PRF", "ADV", "ADJD", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da\u00df was man von ihm wollt, auch war sein Will und Leben.", "tokens": ["Da\u00df", "was", "man", "von", "ihm", "wollt", ",", "auch", "war", "sein", "Will", "und", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWS", "PIS", "APPR", "PPER", "VMFIN", "$,", "ADV", "VAFIN", "VAINF", "VMFIN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Sein hoher Heldenmuth sehnt sich zwar mit ins Heer,", "tokens": ["Sein", "ho\u00b7her", "Hel\u00b7den\u00b7muth", "sehnt", "sich", "zwar", "mit", "ins", "Heer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PRF", "ADV", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Doch galt des Vaters Will bey ihme noch vielmehr,", "tokens": ["Doch", "galt", "des", "Va\u00b7ters", "Will", "bey", "ih\u00b7me", "noch", "viel\u00b7mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VMFIN", "APPR", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Da\u00df, als nur Saul hinweg, er gleich den Hof verliesse,", "tokens": ["Da\u00df", ",", "als", "nur", "Saul", "hin\u00b7weg", ",", "er", "gleich", "den", "Hof", "ver\u00b7lies\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "ADV", "NN", "PTKVZ", "$,", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und wiederum der Art des Feldes sich beflisse,", "tokens": ["Und", "wie\u00b7de\u00b7rum", "der", "Art", "des", "Fel\u00b7des", "sich", "be\u00b7flis\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Aus einem Hofmann ward hinwiederum ein Hirt,", "tokens": ["Aus", "ei\u00b7nem", "Hof\u00b7mann", "ward", "hin\u00b7wie\u00b7de\u00b7rum", "ein", "Hirt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Der beydes Stadt und Feld mit seinem Wesen ziert.", "tokens": ["Der", "bey\u00b7des", "Stadt", "und", "Feld", "mit", "sei\u00b7nem", "We\u00b7sen", "ziert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}