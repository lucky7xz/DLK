{"dta.poem.14988": {"metadata": {"author": {"name": "K\u00f6rner, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Am Hedwigsbrunnen bei Jauer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1814", "urn": "urn:nbn:de:kobv:b4-200905193258", "language": ["de:0.99"], "booktitle": "K\u00f6rner, Theodor: Leyer und Schwerdt. Berlin, 1814."}, "poem": {"stanza.1": {"line.1": {"text": "Der Freude wie der Wehmuth Schwingen tragen", "tokens": ["Der", "Freu\u00b7de", "wie", "der", "Weh\u00b7muth", "Schwin\u00b7gen", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KOKOM", "ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das milde Herz zu liebefrohen Tagen,", "tokens": ["Das", "mil\u00b7de", "Herz", "zu", "lie\u00b7be\u00b7fr\u00b7o\u00b7hen", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Von keinem Thr\u00e4nengifte mehr verbittert. \u2014", "tokens": ["Von", "kei\u00b7nem", "Thr\u00e4\u00b7nen\u00b7gif\u00b7te", "mehr", "ver\u00b7bit\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wer hat mein freies Paradies umgittert? \u2014", "tokens": ["Wer", "hat", "mein", "frei\u00b7es", "Pa\u00b7ra\u00b7dies", "um\u00b7git\u00b7tert", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VAFIN", "PPOSAT", "ADJA", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wer durfte mich in diese Fesseln schlagen,", "tokens": ["Wer", "durf\u00b7te", "mich", "in", "die\u00b7se", "Fes\u00b7seln", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PRF", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Den Lieder Sohn ins Kriegsget\u00fcmmel jagen?", "tokens": ["Den", "Lie\u00b7der", "Sohn", "ins", "Kriegs\u00b7ge\u00b7t\u00fcm\u00b7mel", "ja\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wer hat mir meinen Freudenbaum zersplittert?", "tokens": ["Wer", "hat", "mir", "mei\u00b7nen", "Freu\u00b7den\u00b7baum", "zer\u00b7split\u00b7tert", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wie? griff' ich nicht mit freier Hand zum Schwerdte,", "tokens": ["Wie", "?", "griff'", "ich", "nicht", "mit", "frei\u00b7er", "Hand", "zum", "Schwerd\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VVFIN", "PPER", "PTKNEG", "APPR", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Da\u00df blutvers\u00f6hnend aus der deutschen Erde", "tokens": ["Da\u00df", "blut\u00b7ver\u00b7s\u00f6h\u00b7nend", "aus", "der", "deut\u00b7schen", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Ein heilig Wort jung und lebendig werde? \u2014", "tokens": ["Ein", "hei\u00b7lig", "Wort", "jung", "und", "le\u00b7ben\u00b7dig", "wer\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJD", "NN", "ADJD", "KON", "ADJD", "VAFIN", "$.", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Es sprichts ein Gott im Rauschen dieser Wellen:", "tokens": ["Es", "sprichts", "ein", "Gott", "im", "Rau\u00b7schen", "die\u00b7ser", "Wel\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "\u201eam Klippenherzen mu\u00df die Kraft zerschellen,", "tokens": ["\u201e", "am", "Klip\u00b7pen\u00b7her\u00b7zen", "mu\u00df", "die", "Kraft", "zer\u00b7schel\u00b7len", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPRART", "NN", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "\u201eund aus dem Tode soll das Leben quellen.\u201c", "tokens": ["\u201e", "und", "aus", "dem", "To\u00b7de", "soll", "das", "Le\u00b7ben", "quel\u00b7len", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "APPR", "ART", "NN", "VMFIN", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}