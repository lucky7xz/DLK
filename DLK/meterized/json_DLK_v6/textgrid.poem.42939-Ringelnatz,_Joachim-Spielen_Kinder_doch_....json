{"textgrid.poem.42939": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Spielen Kinder doch ...", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sahst du in der Bahn auf Reisen:", "tokens": ["Sahst", "du", "in", "der", "Bahn", "auf", "Rei\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00e4hrt dein Spiegelbild daneben", "tokens": ["F\u00e4hrt", "dein", "Spie\u00b7gel\u00b7bild", "da\u00b7ne\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "PAV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Drau\u00dfen heil durch Fels und Eisen?", "tokens": ["Drau\u00b7\u00dfen", "heil", "durch", "Fels", "und", "Ei\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was ist Schein und was ist Leben?", "tokens": ["Was", "ist", "Schein", "und", "was", "ist", "Le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "NN", "KON", "PWS", "VAFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wirrgespr\u00e4ch von Schizophrenen \u2013?", "tokens": ["Wirr\u00b7ge\u00b7spr\u00e4ch", "von", "Schi\u00b7zop\u00b7hre\u00b7nen", "\u2013", "?"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "NN", "$(", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der Wirrsinn deiner Tr\u00e4ume \u2013?", "tokens": ["Und", "der", "Wirr\u00b7sinn", "dei\u00b7ner", "Tr\u00e4u\u00b7me", "\u2013", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN", "$(", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Warum suchen wir, ersehnen", "tokens": ["Wa\u00b7rum", "su\u00b7chen", "wir", ",", "er\u00b7seh\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unterschiede, Zwischenr\u00e4ume?", "tokens": ["Un\u00b7ter\u00b7schie\u00b7de", ",", "Zwi\u00b7schen\u00b7r\u00e4u\u00b7me", "?"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nach dem Nichts, dem Garnichts schielen", "tokens": ["Nach", "dem", "Nichts", ",", "dem", "Gar\u00b7nichts", "schie\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alle, Freude, Gleichmut, Trauer.", "tokens": ["Al\u00b7le", ",", "Freu\u00b7de", ",", "Gleich\u00b7mut", ",", "Trau\u00b7er", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PIS", "$,", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus dem Garnichts lockt ein Schauer", "tokens": ["Aus", "dem", "Gar\u00b7nichts", "lockt", "ein", "Schau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So und so mit fremden Spielen.", "tokens": ["So", "und", "so", "mit", "frem\u00b7den", "Spie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Manchmal, zwischen trocknen Zeilen:", "tokens": ["Manch\u00b7mal", ",", "zwi\u00b7schen", "trock\u00b7nen", "Zei\u00b7len", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Barmt es, winkt es oder lacht es. \u2013", "tokens": ["Barmt", "es", ",", "winkt", "es", "o\u00b7der", "lacht", "es", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Spielen Kinder doch zuweilen", "tokens": ["Spie\u00b7len", "Kin\u00b7der", "doch", "zu\u00b7wei\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "NN", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wundersames Selbsterdachtes.", "tokens": ["Wun\u00b7der\u00b7sa\u00b7mes", "Selbs\u00b7ter\u00b7dach\u00b7tes", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}