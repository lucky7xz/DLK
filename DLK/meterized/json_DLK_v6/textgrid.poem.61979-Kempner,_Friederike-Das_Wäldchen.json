{"textgrid.poem.61979": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "Das W\u00e4ldchen", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein W\u00e4ldchen sich erhebt,", "tokens": ["Ein", "W\u00e4ld\u00b7chen", "sich", "er\u00b7hebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Spro\u00dft fr\u00f6hlich himmelan,", "tokens": ["Spro\u00dft", "fr\u00f6h\u00b7lich", "him\u00b7me\u00b7lan", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ob unser eins noch lebt,", "tokens": ["Ob", "un\u00b7ser", "eins", "noch", "lebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wenn einst die Axt daran?", "tokens": ["Wenn", "einst", "die", "Axt", "da\u00b7ran", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Man pflanzt den Berg mit Wein,", "tokens": ["Man", "pflanzt", "den", "Berg", "mit", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der Muskateller bringt;", "tokens": ["Der", "Mus\u00b7ka\u00b7tel\u00b7ler", "bringt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ob wir noch lebend sein,", "tokens": ["Ob", "wir", "noch", "le\u00b7bend", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wenn er im Becher blinkt?", "tokens": ["Wenn", "er", "im", "Be\u00b7cher", "blinkt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ein Rosenkn\u00f6spchen bl\u00fcht,", "tokens": ["Ein", "Ro\u00b7sen\u00b7kn\u00f6spc\u00b7hen", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und morgen auf es bricht,", "tokens": ["Und", "mor\u00b7gen", "auf", "es", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ob es mein Aug' noch sieht,", "tokens": ["Ob", "es", "mein", "Aug'", "noch", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wei\u00df Gott, ich wei\u00df es nicht!", "tokens": ["Wei\u00df", "Gott", ",", "ich", "wei\u00df", "es", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}