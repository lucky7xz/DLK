{"textgrid.poem.35714": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "38. Der Freier", "genre": "verse", "period": "N.A.", "pub_year": 1789, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das M\u00e4gdlein, braun von Aug' und Haar,", "tokens": ["Das", "M\u00e4gd\u00b7lein", ",", "braun", "von", "Aug'", "und", "Haar", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kam \u00fcber Feld gegangen;", "tokens": ["Kam", "\u00fc\u00b7ber", "Feld", "ge\u00b7gan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Abendr\u00f6te schien so klar,", "tokens": ["Die", "A\u00b7bend\u00b7r\u00f6\u00b7te", "schien", "so", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Nachtigallen sangen.", "tokens": ["Und", "Nach\u00b7ti\u00b7gal\u00b7len", "san\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich sah und h\u00f6rte sie allein.", "tokens": ["Ich", "sah", "und", "h\u00f6r\u00b7te", "sie", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dalderi daldera, das M\u00e4gdelein", "tokens": ["Dal\u00b7de\u00b7ri", "dal\u00b7de\u00b7ra", ",", "das", "M\u00e4g\u00b7del\u00b7ein"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "$,", "ART", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "Soll mein Herzliebchen sein!", "tokens": ["Soll", "mein", "Herz\u00b7lieb\u00b7chen", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}}, "stanza.2": {"line.1": {"text": "Ein R\u00f6ckchen trug sie, d\u00fcnn und kurz,", "tokens": ["Ein", "R\u00f6ck\u00b7chen", "trug", "sie", ",", "d\u00fcnn", "und", "kurz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und leichtgeschn\u00fcrt ihr Mieder;", "tokens": ["Und", "leicht\u00b7ge\u00b7schn\u00fcrt", "ihr", "Mie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es weht' ihr Haar, es weht' ihr Schurz", "tokens": ["Es", "weht'", "ihr", "Haar", ",", "es", "weht'", "ihr", "Schurz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Weste hin und wieder;", "tokens": ["Im", "Wes\u00b7te", "hin", "und", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "KON", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Str\u00fcmpfe schienen wei\u00df und fein.", "tokens": ["Die", "Str\u00fcmp\u00b7fe", "schie\u00b7nen", "wei\u00df", "und", "fein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dalderi daldera, das M\u00e4gdelein", "tokens": ["Dal\u00b7de\u00b7ri", "dal\u00b7de\u00b7ra", ",", "das", "M\u00e4g\u00b7del\u00b7ein"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "$,", "ART", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "Soll mein Herzliebchen sein!", "tokens": ["Soll", "mein", "Herz\u00b7lieb\u00b7chen", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}}, "stanza.3": {"line.1": {"text": "Die bunte Kuh, gelockt mit Gras,", "tokens": ["Die", "bun\u00b7te", "Kuh", ",", "ge\u00b7lockt", "mit", "Gras", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kam her vom Anger trabend;", "tokens": ["Kam", "her", "vom", "An\u00b7ger", "tra\u00b7bend", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APZR", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und als das M\u00e4gdlein melkend sa\u00df,", "tokens": ["Und", "als", "das", "M\u00e4gd\u00b7lein", "mel\u00b7kend", "sa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da bot ich guten Abend,", "tokens": ["Da", "bot", "ich", "gu\u00b7ten", "A\u00b7bend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und sah durchs Busentuch hinein.", "tokens": ["Und", "sah", "durchs", "Bu\u00b7sen\u00b7tuch", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dalderi daldera, das M\u00e4gdelein", "tokens": ["Dal\u00b7de\u00b7ri", "dal\u00b7de\u00b7ra", ",", "das", "M\u00e4g\u00b7del\u00b7ein"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "$,", "ART", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "Soll mein Herzliebchen sein!", "tokens": ["Soll", "mein", "Herz\u00b7lieb\u00b7chen", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}}, "stanza.4": {"line.1": {"text": "Sie nickte mir mit holdem Gru\u00df;", "tokens": ["Sie", "nick\u00b7te", "mir", "mit", "hol\u00b7dem", "Gru\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da ward mir wohl und bange,", "tokens": ["Da", "ward", "mir", "wohl", "und", "ban\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und herzhaft dr\u00fcckt' ich einen Ku\u00df", "tokens": ["Und", "herz\u00b7haft", "dr\u00fcckt'", "ich", "ei\u00b7nen", "Ku\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf ihre rote Wange,", "tokens": ["Auf", "ih\u00b7re", "ro\u00b7te", "Wan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So rot, so rot wie Abendschein.", "tokens": ["So", "rot", ",", "so", "rot", "wie", "A\u00b7bend\u00b7schein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "KOKOM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dalderi daldera, das M\u00e4gdelein", "tokens": ["Dal\u00b7de\u00b7ri", "dal\u00b7de\u00b7ra", ",", "das", "M\u00e4g\u00b7del\u00b7ein"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "$,", "ART", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "Soll mein Herzliebchen sein!", "tokens": ["Soll", "mein", "Herz\u00b7lieb\u00b7chen", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}}, "stanza.5": {"line.1": {"text": "Ich half ihr \u00fcber Steg und Zaun", "tokens": ["Ich", "half", "ihr", "\u00fc\u00b7ber", "Steg", "und", "Zaun"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Milch zu Hause bringen,", "tokens": ["Die", "Milch", "zu", "Hau\u00b7se", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und gegen Unget\u00fcm und Graun", "tokens": ["Und", "ge\u00b7gen", "Un\u00b7ge\u00b7t\u00fcm", "und", "Graun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Sch\u00e4ferliedchen singen;", "tokens": ["Ein", "Sch\u00e4\u00b7fer\u00b7lied\u00b7chen", "sin\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Denn dunkel war's im Buchenhain.", "tokens": ["Denn", "dun\u00b7kel", "wa\u00b7r's", "im", "Bu\u00b7chen\u00b7hain", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "APPRART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Dalderi daldera, das M\u00e4gdelein", "tokens": ["Dal\u00b7de\u00b7ri", "dal\u00b7de\u00b7ra", ",", "das", "M\u00e4g\u00b7del\u00b7ein"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "$,", "ART", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "Soll mein Herzliebchen sein!", "tokens": ["Soll", "mein", "Herz\u00b7lieb\u00b7chen", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}}, "stanza.6": {"line.1": {"text": "Die Mutter schalt: So sp\u00e4t bei Nacht?", "tokens": ["Die", "Mut\u00b7ter", "schalt", ":", "So", "sp\u00e4t", "bei", "Nacht", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ADV", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da stand sie ach! so sch\u00e4mig.", "tokens": ["Da", "stand", "sie", "ach", "!", "so", "sch\u00e4\u00b7mig", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$.", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sacht, sprach ich, gute Mutter, sacht!", "tokens": ["Sacht", ",", "sprach", "ich", ",", "gu\u00b7te", "Mut\u00b7ter", ",", "sacht", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das T\u00f6chterlein, das nehm' ich!", "tokens": ["Das", "T\u00f6ch\u00b7ter\u00b7lein", ",", "das", "nehm'", "ich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "VVFIN", "PPER", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nur freundlich, Mutter, willigt ein!", "tokens": ["Nur", "freund\u00b7lich", ",", "Mut\u00b7ter", ",", "wil\u00b7ligt", "ein", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dalderi daldera, das M\u00e4gdelein", "tokens": ["Dal\u00b7de\u00b7ri", "dal\u00b7de\u00b7ra", ",", "das", "M\u00e4g\u00b7del\u00b7ein"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "$,", "ART", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "Soll mein Herzliebchen sein!", "tokens": ["Soll", "mein", "Herz\u00b7lieb\u00b7chen", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}}}}}