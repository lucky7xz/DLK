{"dta.poem.2821": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "22.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie manchen priesest du, was er nicht war, begl\u00fcckt,", "tokens": ["Wie", "man\u00b7chen", "prie\u00b7sest", "du", ",", "was", "er", "nicht", "war", ",", "be\u00b7gl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "PPER", "$,", "PWS", "PPER", "PTKNEG", "VAFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil er mit falschem Schein den innern Fehl geschm\u00fcckt.", "tokens": ["Weil", "er", "mit", "fal\u00b7schem", "Schein", "den", "in\u00b7nern", "Fehl", "ge\u00b7schm\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ob einer wirklich sei zu preisen, zu beklagen,", "tokens": ["Ob", "ei\u00b7ner", "wirk\u00b7lich", "sei", "zu", "prei\u00b7sen", ",", "zu", "be\u00b7kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJD", "VAFIN", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sagt er sich selber nur, dir braucht ers nicht zu sagen.", "tokens": ["Sagt", "er", "sich", "sel\u00b7ber", "nur", ",", "dir", "braucht", "ers", "nicht", "zu", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "ADV", "$,", "PPER", "VVFIN", "PIS", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich aber sag' es dir, wie du mich immerhin", "tokens": ["Ich", "a\u00b7ber", "sag'", "es", "dir", ",", "wie", "du", "mich", "im\u00b7mer\u00b7hin"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "PPER", "$,", "PWAV", "PPER", "PRF", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bedauerst, wi\u00df da\u00df ich beneidenswerth noch bin.", "tokens": ["Be\u00b7dau\u00b7erst", ",", "wi\u00df", "da\u00df", "ich", "be\u00b7nei\u00b7dens\u00b7werth", "noch", "bin", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "KOUS", "PPER", "ADJD", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}