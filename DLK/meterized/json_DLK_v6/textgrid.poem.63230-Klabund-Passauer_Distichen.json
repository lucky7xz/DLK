{"textgrid.poem.63230": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Passauer Distichen", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ins fallende Laub vor zwanzig s\u00e4uselnden Jahren", "tokens": ["Als", "ins", "fal\u00b7len\u00b7de", "Laub", "vor", "zwan\u00b7zig", "s\u00e4u\u00b7seln\u00b7den", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "ADJA", "NN", "APPR", "CARD", "ADJA", "NN"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Herbst dich bettete bunt, rief er die G\u00f6ttinnen all:", "tokens": ["Herbst", "dich", "bet\u00b7te\u00b7te", "bunt", ",", "rief", "er", "die", "G\u00f6t\u00b7tin\u00b7nen", "all", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "ADJD", "$,", "VVFIN", "PPER", "ART", "NN", "PIAT", "$."], "meter": "+-+--++-+---+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Seht von der letzten Libelle umschw\u00e4rmt das schm\u00e4chtige Menschlein!", "tokens": ["Seht", "von", "der", "letz\u00b7ten", "Li\u00b7bel\u00b7le", "um\u00b7schw\u00e4rmt", "das", "schm\u00e4ch\u00b7ti\u00b7ge", "Mensc\u00b7hlein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "---+--+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Eine G\u00f6ttin wie ihr \u2013 nur noch schleierverh\u00fcllt.", "tokens": ["Ei\u00b7ne", "G\u00f6t\u00b7tin", "wie", "ihr", "\u2013", "nur", "noch", "schlei\u00b7er\u00b7ver\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "PPOSAT", "$(", "ADV", "ADV", "VVFIN", "$."], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Und sie traten herzu und sahn die blonde Beseelung", "tokens": ["Und", "sie", "tra\u00b7ten", "her\u00b7zu", "und", "sahn", "die", "blon\u00b7de", "Be\u00b7see\u00b7lung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Unter den Schleiern, die herbstlich die Spinne gewebt.", "tokens": ["Un\u00b7ter", "den", "Schlei\u00b7ern", ",", "die", "herbst\u00b7lich", "die", "Spin\u00b7ne", "ge\u00b7webt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.3": {"text": "Eine nur senkte den Blick und hob die Hand und zerriss das", "tokens": ["Ei\u00b7ne", "nur", "senk\u00b7te", "den", "Blick", "und", "hob", "die", "Hand", "und", "zer\u00b7riss", "das"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN", "KON", "VVFIN", "ART"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Leichte Gewebe: es war Venus. Sie segnete dich.", "tokens": ["Leich\u00b7te", "Ge\u00b7we\u00b7be", ":", "es", "war", "Ve\u00b7nus", ".", "Sie", "seg\u00b7ne\u00b7te", "dich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPER", "VAFIN", "NN", "$.", "PPER", "VVFIN", "PPER", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Dass wir einander in seliger Ruhe geniessen durften,", "tokens": ["Dass", "wir", "ein\u00b7an\u00b7der", "in", "se\u00b7li\u00b7ger", "Ru\u00b7he", "ge\u00b7nies\u00b7sen", "durf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ADJA", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+--+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Dankten wir himmlisch erfreut nur dem christlichen Gott.", "tokens": ["Dank\u00b7ten", "wir", "himm\u00b7lisch", "er\u00b7freut", "nur", "dem", "christ\u00b7li\u00b7chen", "Gott", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.3": {"text": "Fromm und feierlich wir schritten von Kirche zu Kirche,", "tokens": ["Fromm", "und", "fei\u00b7er\u00b7lich", "wir", "schrit\u00b7ten", "von", "Kir\u00b7che", "zu", "Kir\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "PPER", "VVFIN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Und im d\u00e4mmrigen Gang fand sich Lippe zu Mund.", "tokens": ["Und", "im", "d\u00e4mm\u00b7ri\u00b7gen", "Gang", "fand", "sich", "Lip\u00b7pe", "zu", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "VVFIN", "PRF", "NE", "APPR", "NN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.5": {"text": "Und im Beichtstuhle fand sich Brust zu bebenden Br\u00fcsten,", "tokens": ["Und", "im", "Beicht\u00b7stuh\u00b7le", "fand", "sich", "Brust", "zu", "be\u00b7ben\u00b7den", "Br\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PRF", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Und im Herzschlag schlug dr\u00f6hnend die Glocke vom Turm.", "tokens": ["Und", "im", "Herz\u00b7schlag", "schlug", "dr\u00f6h\u00b7nend", "die", "Glo\u00b7cke", "vom", "Turm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "ADJD", "ART", "NN", "APPRART", "NN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}}, "stanza.4": {"line.1": {"text": "Unter bl\u00fchenden Kirschen im m\u00e4chtig sprossenden Grase", "tokens": ["Un\u00b7ter", "bl\u00fc\u00b7hen\u00b7den", "Kir\u00b7schen", "im", "m\u00e4ch\u00b7tig", "spros\u00b7sen\u00b7den", "Gra\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPRART", "ADJD", "ADJA", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Liegen die Liebste und ich. Schatten breitet der Baum", "tokens": ["Lie\u00b7gen", "die", "Liebs\u00b7te", "und", "ich", ".", "Schat\u00b7ten", "brei\u00b7tet", "der", "Baum"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "KON", "PPER", "$.", "NN", "VVFIN", "ART", "NN"], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "\u00dcber das gr\u00fcne Bett mit weissen Bl\u00fcten durchmustert.", "tokens": ["\u00dc\u00b7ber", "das", "gr\u00fc\u00b7ne", "Bett", "mit", "weis\u00b7sen", "Bl\u00fc\u00b7ten", "durch\u00b7mus\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Bl\u00fcten mit leichter Hand sch\u00fcttelt der Fr\u00fchling herab.", "tokens": ["Bl\u00fc\u00b7ten", "mit", "leich\u00b7ter", "Hand", "sch\u00fct\u00b7telt", "der", "Fr\u00fch\u00b7ling", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "+--+-+---+--+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Doch von des M\u00e4dchens Lippe pfl\u00fcck ich die s\u00fcssesten Fr\u00fcchte,", "tokens": ["Doch", "von", "des", "M\u00e4d\u00b7chens", "Lip\u00b7pe", "pfl\u00fcck", "ich", "die", "s\u00fcs\u00b7ses\u00b7ten", "Fr\u00fcch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NE", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "F\u00e4llt ihr ein Blatt auf den Mund, k\u00fcss ich es z\u00e4rtlich hinweg.", "tokens": ["F\u00e4llt", "ihr", "ein", "Blatt", "auf", "den", "Mund", ",", "k\u00fcss", "ich", "es", "z\u00e4rt\u00b7lich", "hin\u00b7weg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$,", "VVFIN", "PPER", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Also ein g\u00fctig Geschick uns Herbst und Fr\u00fchling vereinte:", "tokens": ["Al\u00b7so", "ein", "g\u00fc\u00b7tig", "Ge\u00b7schick", "uns", "Herbst", "und", "Fr\u00fch\u00b7ling", "ver\u00b7ein\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJD", "NN", "PPER", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Schwebt die Bl\u00fcte vom Baum, reift auf dem Mund sie zur Frucht.", "tokens": ["Schwebt", "die", "Bl\u00fc\u00b7te", "vom", "Baum", ",", "reift", "auf", "dem", "Mund", "sie", "zur", "Frucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "PPER", "APPRART", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Wo der Fl\u00fcsse drei sich ineinander ergiessen,", "tokens": ["Wo", "der", "Fl\u00fcs\u00b7se", "drei", "sich", "in\u00b7ein\u00b7an\u00b7der", "er\u00b7gies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "CARD", "PRF", "ADV", "VVPP", "$,"], "meter": "--+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Standen wir liebend gelehnt, sahn in die jagende Flut.", "tokens": ["Stan\u00b7den", "wir", "lie\u00b7bend", "ge\u00b7lehnt", ",", "sahn", "in", "die", "ja\u00b7gen\u00b7de", "Flut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "VVPP", "$,", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Drei ward eins. Ich fasste fester die Hand dir und dachte:", "tokens": ["Drei", "ward", "eins", ".", "Ich", "fass\u00b7te", "fes\u00b7ter", "die", "Hand", "dir", "und", "dach\u00b7te", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "VAFIN", "PIS", "$.", "PPER", "VVFIN", "ADJD", "ART", "NN", "PPER", "KON", "VVFIN", "$."], "meter": "-+--+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Du und ich \u2013 und das Kind. Also dreieinig auch wir.", "tokens": ["Du", "und", "ich", "\u2013", "und", "das", "Kind", ".", "Al\u00b7so", "drei\u00b7ei\u00b7nig", "auch", "wir", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "PPER", "$(", "KON", "ART", "NN", "$.", "ADV", "CARD", "ADV", "PPER", "$."], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}}}}}