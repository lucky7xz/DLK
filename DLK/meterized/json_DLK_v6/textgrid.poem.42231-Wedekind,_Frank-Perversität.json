{"textgrid.poem.42231": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Perversit\u00e4t", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Waisenkind mit nassen, blassen Wangen,", "tokens": ["Ein", "Wai\u00b7sen\u00b7kind", "mit", "nas\u00b7sen", ",", "blas\u00b7sen", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit hohlen Augen und mit d\u00fcnnen Armen", "tokens": ["Mit", "hoh\u00b7len", "Au\u00b7gen", "und", "mit", "d\u00fcn\u00b7nen", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Huscht scheu hervor, inst\u00e4ndig mein Erbarmen", "tokens": ["Huscht", "scheu", "her\u00b7vor", ",", "in\u00b7st\u00e4n\u00b7dig", "mein", "Er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADJD", "PTKVZ", "$,", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Anflehend, stotternd, schlotternd, furchtbefangen.", "tokens": ["An\u00b7fle\u00b7hend", ",", "stot\u00b7ternd", ",", "schlot\u00b7ternd", ",", "furcht\u00b7be\u00b7fan\u00b7gen", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "$,", "ADJD", "$,", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Eisig sein K\u00f6rper, gl\u00fchend sein Verlangen,", "tokens": ["Ei\u00b7sig", "sein", "K\u00f6r\u00b7per", ",", "gl\u00fc\u00b7hend", "sein", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$,", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "M\u00fcht sich's frostbebend, menschlich zu erwarmen.", "tokens": ["M\u00fcht", "sich's", "frost\u00b7be\u00b7bend", ",", "menschlich", "zu", "er\u00b7war\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJD", "$,", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Vergebne Qual; erschlafft in meinen Armen", "tokens": ["Ver\u00b7geb\u00b7ne", "Qual", ";", "er\u00b7schlafft", "in", "mei\u00b7nen", "Ar\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bewimmert es sein Hoffen und sein Bangen.", "tokens": ["Be\u00b7wim\u00b7mert", "es", "sein", "Hof\u00b7fen", "und", "sein", "Ban\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Besch\u00e4mt schleicht sich's von hinnen, \u00e4chzend, siechend,", "tokens": ["Be\u00b7sch\u00e4mt", "schleicht", "sich's", "von", "hin\u00b7nen", ",", "\u00e4ch\u00b7zend", ",", "sie\u00b7chend", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "VVFIN", "PIS", "APPR", "ADV", "$,", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nachts bettelnd und bei Tage sich verkriechend,", "tokens": ["Nachts", "bet\u00b7telnd", "und", "bei", "Ta\u00b7ge", "sich", "ver\u00b7krie\u00b7chend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "KON", "APPR", "NN", "PRF", "VVPP", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Heut in Verzweiflung, morgen in Verz\u00fccktheit;", "tokens": ["Heut", "in", "Ver\u00b7zwei\u00b7flung", ",", "mor\u00b7gen", "in", "Ver\u00b7z\u00fcckt\u00b7heit", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Verf\u00e4llt gemach schmerzstillender Verr\u00fccktheit,", "tokens": ["Ver\u00b7f\u00e4llt", "ge\u00b7mach", "schmerz\u00b7stil\u00b7len\u00b7der", "Ver\u00b7r\u00fcckt\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Stutzt, lacht, jauchzt todesfroh, und, der Gewandung", "tokens": ["Stutzt", ",", "lacht", ",", "jauchzt", "to\u00b7des\u00b7froh", ",", "und", ",", "der", "Ge\u00b7wan\u00b7dung"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "ADJD", "$,", "KON", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vom Gischt beraubt, zerschellt es in der Brandung.", "tokens": ["Vom", "Gischt", "be\u00b7raubt", ",", "zer\u00b7schellt", "es", "in", "der", "Bran\u00b7dung", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}