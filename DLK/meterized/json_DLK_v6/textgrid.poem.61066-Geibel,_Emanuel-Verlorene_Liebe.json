{"textgrid.poem.61066": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Verlorene Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und fragst du mich mit vorwurfsvollem Blick:", "tokens": ["Und", "fragst", "du", "mich", "mit", "vor\u00b7wurfs\u00b7vol\u00b7lem", "Blick", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Warum so tr\u00fcbe? Welch ein Mi\u00dfgeschick", "tokens": ["Wa\u00b7rum", "so", "tr\u00fc\u00b7be", "?", "Welch", "ein", "Mi\u00df\u00b7ge\u00b7schick"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADJA", "$.", "PIAT", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vermag der Seele Frieden dir zu st\u00f6ren? -", "tokens": ["Ver\u00b7mag", "der", "See\u00b7le", "Frie\u00b7den", "dir", "zu", "st\u00f6\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "ART", "NN", "NN", "PPER", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wohlan! Es sei! Die n\u00e4cht'ge Stund' ist gut,", "tokens": ["Wo\u00b7hlan", "!", "Es", "sei", "!", "Die", "n\u00e4cht'\u00b7ge", "Stund'", "ist", "gut", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VAFIN", "$.", "ART", "ADJA", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Im Becher gl\u00fcht der Traube dunkles Blut -", "tokens": ["Im", "Be\u00b7cher", "gl\u00fcht", "der", "Trau\u00b7be", "dunk\u00b7les", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Von meiner Jugendliebe sollst du h\u00f6ren.", "tokens": ["Von", "mei\u00b7ner", "Ju\u00b7gend\u00b7lie\u00b7be", "sollst", "du", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich war ein Knab', wie andre Knaben sind,", "tokens": ["Ich", "war", "ein", "Knab'", ",", "wie", "and\u00b7re", "Kna\u00b7ben", "sind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PWAV", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Halb trotzig-hei\u00dfer J\u00fcngling, halb noch Kind,", "tokens": ["Halb", "trot\u00b7zig\u00b7hei\u00b7\u00dfer", "J\u00fcng\u00b7ling", ",", "halb", "noch", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,", "ADJD", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zu scheu, des Lebens R\u00e4tsel zu entsiegeln;", "tokens": ["Zu", "scheu", ",", "des", "Le\u00b7bens", "R\u00e4t\u00b7sel", "zu", "ent\u00b7sie\u00b7geln", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "$,", "ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mein junges Herz war voll und sehnsuchtsschwer,", "tokens": ["Mein", "jun\u00b7ges", "Herz", "war", "voll", "und", "sehn\u00b7suchts\u00b7schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es wu\u00dfte kaum, weshalb - es glich dem Meer,", "tokens": ["Es", "wu\u00df\u00b7te", "kaum", ",", "we\u00b7shalb", "es", "glich", "dem", "Meer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWAV", "$(", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Das still des Mondes harrt, ihn abzuspiegeln.", "tokens": ["Das", "still", "des", "Mon\u00b7des", "harrt", ",", "ihn", "ab\u00b7zu\u00b7spie\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "VVFIN", "$,", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da fand ich sie, das blonde Kind der Flur,", "tokens": ["Da", "fand", "ich", "sie", ",", "das", "blon\u00b7de", "Kind", "der", "Flur", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und zwiegeschaffen f\u00fchlten wir uns nur,", "tokens": ["Und", "zwie\u00b7ge\u00b7schaf\u00b7fen", "f\u00fchl\u00b7ten", "wir", "uns", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VVFIN", "PPER", "PRF", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Uns neu zu einen wie in Edens R\u00e4umen;", "tokens": ["Uns", "neu", "zu", "ei\u00b7nen", "wie", "in", "E\u00b7dens", "R\u00e4u\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "PIS", "KOKOM", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Blau war ihr Auge wie die Sommernacht;", "tokens": ["Blau", "war", "ihr", "Au\u00b7ge", "wie", "die", "Som\u00b7mer\u00b7nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "KOKOM", "ART", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Und diese Lippen! - Wem sie nur gelacht,", "tokens": ["Und", "die\u00b7se", "Lip\u00b7pen", "!", "Wem", "sie", "nur", "ge\u00b7lacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "$.", "$(", "PWS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der mu\u00dft' hinfort von hei\u00dfen K\u00fcssen tr\u00e4umen.", "tokens": ["Der", "mu\u00dft'", "hin\u00b7fort", "von", "hei\u00b7\u00dfen", "K\u00fcs\u00b7sen", "tr\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wohl bl\u00fcht' uns damals eine sch\u00f6ne Zeit,", "tokens": ["Wohl", "bl\u00fcht'", "uns", "da\u00b7mals", "ei\u00b7ne", "sch\u00f6\u00b7ne", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Als wir in dunkler Waldeseinsamkeit", "tokens": ["Als", "wir", "in", "dunk\u00b7ler", "Wal\u00b7de\u00b7sein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das Reh belauschten und der Knospen Schwellen,", "tokens": ["Das", "Reh", "be\u00b7lauschten", "und", "der", "Knos\u00b7pen", "Schwel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Als wir im Kahne - D\u00e4mmrung rings umher -", "tokens": ["Als", "wir", "im", "Kah\u00b7ne", "D\u00e4mm\u00b7rung", "rings", "um\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "$(", "NN", "ADV", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Uns wiegten auf dem abendstillen Meer,", "tokens": ["Uns", "wieg\u00b7ten", "auf", "dem", "a\u00b7bend\u00b7stil\u00b7len", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Vom Sp\u00e4trot nur gesehn und von den Wellen;", "tokens": ["Vom", "Sp\u00e4t\u00b7rot", "nur", "ge\u00b7sehn", "und", "von", "den", "Wel\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVPP", "KON", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Als wir auf mondbeleuchtetem Balkon", "tokens": ["Als", "wir", "auf", "mond\u00b7be\u00b7leuch\u00b7te\u00b7tem", "Bal\u00b7kon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Zweistimmig sangen zu der Laute Ton,", "tokens": ["Zweis\u00b7tim\u00b7mig", "san\u00b7gen", "zu", "der", "Lau\u00b7te", "Ton", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als wir uns heimlich fl\u00fcsternd dann umfingen,", "tokens": ["Als", "wir", "uns", "heim\u00b7lich", "fl\u00fcs\u00b7ternd", "dann", "um\u00b7fin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVPP", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Aug' in Auge seligen Ergu\u00df", "tokens": ["Und", "Aug'", "in", "Au\u00b7ge", "se\u00b7li\u00b7gen", "Er\u00b7gu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Herniedertaute, und im ersten Ku\u00df", "tokens": ["Her\u00b7nie\u00b7der\u00b7tau\u00b7te", ",", "und", "im", "ers\u00b7ten", "Ku\u00df"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KON", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die Seelen brennend aneinanderhingen.", "tokens": ["Die", "See\u00b7len", "bren\u00b7nend", "an\u00b7ein\u00b7an\u00b7der\u00b7hin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "O w\u00e4r' ich bei des ersten Kusses Tausch", "tokens": ["O", "w\u00e4r'", "ich", "bei", "des", "ers\u00b7ten", "Kus\u00b7ses", "Tausch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Damals gestorben in begl\u00fccktem Rausch,", "tokens": ["Da\u00b7mals", "ge\u00b7stor\u00b7ben", "in", "be\u00b7gl\u00fcck\u00b7tem", "Rausch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Aus weichen Armen in die Gruft getrieben!", "tokens": ["Aus", "wei\u00b7chen", "Ar\u00b7men", "in", "die", "Gruft", "ge\u00b7trie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich w\u00e4re jetzt kein Greis mit braunem Haar,", "tokens": ["Ich", "w\u00e4\u00b7re", "jetzt", "kein", "Greis", "mit", "brau\u00b7nem", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Frisch au\u00dfen, innen Leiche. - O f\u00fcrwahr,", "tokens": ["Frisch", "au\u00b7\u00dfen", ",", "in\u00b7nen", "Lei\u00b7che", ".", "O", "f\u00fcr\u00b7wahr", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "ADJA", "NN", "$.", "$(", "NE", "ADV", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Es stirbt als Knabe, wen die G\u00f6tter lieben.", "tokens": ["Es", "stirbt", "als", "Kna\u00b7be", ",", "wen", "die", "G\u00f6t\u00b7ter", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "NN", "$,", "PWS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Nun mu\u00dft' ich sie verlieren. An den Mann", "tokens": ["Nun", "mu\u00dft'", "ich", "sie", "ver\u00b7lie\u00b7ren", ".", "An", "den", "Mann"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "VVINF", "$.", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ist sie gebannt, den sie nicht lieben kann,", "tokens": ["Ist", "sie", "ge\u00b7bannt", ",", "den", "sie", "nicht", "lie\u00b7ben", "kann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "PRELS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dem ihre ersten K\u00fcsse nicht zu eigen.", "tokens": ["Dem", "ih\u00b7re", "ers\u00b7ten", "K\u00fcs\u00b7se", "nicht", "zu", "ei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "ADJA", "NN", "PTKNEG", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er f\u00fchrte l\u00e4chelnd zum Altar sie fort;", "tokens": ["Er", "f\u00fchr\u00b7te", "l\u00e4\u00b7chelnd", "zum", "Al\u00b7tar", "sie", "fort", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sie wurde bleich, der Priester sprach das Wort,", "tokens": ["Sie", "wur\u00b7de", "bleich", ",", "der", "Pries\u00b7ter", "sprach", "das", "Wort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ich aber stand dabei und mu\u00dfte schweigen.", "tokens": ["Ich", "a\u00b7ber", "stand", "da\u00b7bei", "und", "mu\u00df\u00b7te", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PAV", "KON", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Und denk' ich dran, so kocht im Grimm mein Herz,", "tokens": ["Und", "denk'", "ich", "dran", ",", "so", "kocht", "im", "Grimm", "mein", "Herz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PAV", "$,", "ADV", "VVFIN", "APPRART", "NE", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und wie ein kaltes Eisen f\u00e4hrt der Schmerz", "tokens": ["Und", "wie", "ein", "kal\u00b7tes", "Ei\u00b7sen", "f\u00e4hrt", "der", "Schmerz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mir durch die Brust, und jeder Trost versaget.", "tokens": ["Mir", "durch", "die", "Brust", ",", "und", "je\u00b7der", "Trost", "ver\u00b7sa\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$,", "KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Darum bin ich so tr\u00fcb, darum so wild.", "tokens": ["Da\u00b7rum", "bin", "ich", "so", "tr\u00fcb", ",", "da\u00b7rum", "so", "wild", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "ADJD", "$,", "PAV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch nun hinweg damit! - Das Glas gef\u00fcllt!", "tokens": ["Doch", "nun", "hin\u00b7weg", "da\u00b7mit", "!", "Das", "Glas", "ge\u00b7f\u00fcllt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "p