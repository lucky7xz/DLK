{"dta.poem.6144": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Subtilen Materie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es zeigt die Lufft, so man zusammen dr\u00e4ngt und presst,", "tokens": ["Es", "zeigt", "die", "Lufft", ",", "so", "man", "zu\u00b7sam\u00b7men", "dr\u00e4ngt", "und", "presst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ADV", "PIS", "ADV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In einen kleinen Raum, durch enge R\u00f6hren,", "tokens": ["In", "ei\u00b7nen", "klei\u00b7nen", "Raum", ",", "durch", "en\u00b7ge", "R\u00f6h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df die Struetur von ihren C\u00f6rperlein,", "tokens": ["Da\u00df", "die", "Stru\u00b7e\u00b7tur", "von", "ih\u00b7ren", "C\u00f6r\u00b7per\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "So nicht einmal zu sehen seyn,", "tokens": ["So", "nicht", "ein\u00b7mal", "zu", "se\u00b7hen", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADV", "PTKZU", "VVINF", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch andern noch den Durchgang l\u00e4sst.", "tokens": ["Doch", "an\u00b7dern", "noch", "den", "Durch\u00b7gang", "l\u00e4sst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Man sieht durch diesen Druck die Oeffnungen der Lufft,", "tokens": ["Man", "sieht", "durch", "die\u00b7sen", "Druck", "die", "Oeff\u00b7nun\u00b7gen", "der", "Lufft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PDAT", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Erf\u00fcllt mit solchem d\u00fcnnen Dufft;", "tokens": ["Er\u00b7f\u00fcllt", "mit", "sol\u00b7chem", "d\u00fcn\u00b7nen", "Dufft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wogegen sie selbst grob. Ein Stoff, der allezeit", "tokens": ["Wo\u00b7ge\u00b7gen", "sie", "selbst", "grob", ".", "Ein", "Stoff", ",", "der", "al\u00b7le\u00b7zeit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "$.", "ART", "NN", "$,", "PRELS", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "In unver\u00e4nderlich-stets reger Fl\u00fcchtigkeit.", "tokens": ["In", "un\u00b7ver\u00b7\u00e4n\u00b7der\u00b7lich\u00b7stets", "re\u00b7ger", "Fl\u00fcch\u00b7tig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Der, da er die Figur zu \u00e4ndern stets geschickt,", "tokens": ["Der", ",", "da", "er", "die", "Fi\u00b7gur", "zu", "\u00e4n\u00b7dern", "stets", "ge\u00b7schickt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "ART", "NN", "PTKZU", "VVINF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Sich in die Oeffnungen der andern C\u00f6rper dr\u00fcckt,", "tokens": ["Sich", "in", "die", "Oeff\u00b7nun\u00b7gen", "der", "an\u00b7dern", "C\u00f6r\u00b7per", "dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die so verschiedlich sind. Der das Bewegen lehrt,", "tokens": ["Die", "so", "ver\u00b7schied\u00b7lich", "sind", ".", "Der", "das", "Be\u00b7we\u00b7gen", "lehrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VAFIN", "$.", "ART", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Indem er seine Krafft beym kleinsten Druck vermehrt.", "tokens": ["In\u00b7dem", "er", "sei\u00b7ne", "Krafft", "beym", "kleins\u00b7ten", "Druck", "ver\u00b7mehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}