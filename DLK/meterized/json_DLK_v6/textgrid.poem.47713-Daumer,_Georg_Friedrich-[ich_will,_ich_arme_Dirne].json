{"textgrid.poem.47713": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[ich will, ich arme Dirne]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich will, ich arme Dirne,", "tokens": ["Ich", "will", ",", "ich", "ar\u00b7me", "Dir\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im Lenze, wenn im Garten", "tokens": ["Im", "Len\u00b7ze", ",", "wenn", "im", "Gar\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "KOUS", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Rauten um die Beete,", "tokens": ["Die", "Rau\u00b7ten", "um", "die", "Bee\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Lilien in der Mitten", "tokens": ["Die", "Li\u00b7li\u00b7en", "in", "der", "Mit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "In ihrer Bl\u00fcthe steh'n,", "tokens": ["In", "ih\u00b7rer", "Bl\u00fc\u00b7the", "steh'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ich will sodann ein Str\u00e4u\u00dfchen,", "tokens": ["Ich", "will", "so\u00b7dann", "ein", "Str\u00e4u\u00df\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ein Pfand der Liebe, binden,", "tokens": ["Ein", "Pfand", "der", "Lie\u00b7be", ",", "bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und will es in die Ferne,", "tokens": ["Und", "will", "es", "in", "die", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Ihm, den ich liebe, senden;", "tokens": ["Ihm", ",", "den", "ich", "lie\u00b7be", ",", "sen\u00b7den", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "VVFIN", "$,", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Nicht kann ich es selber bringen", "tokens": ["Nicht", "kann", "ich", "es", "sel\u00b7ber", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VMFIN", "PPER", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Und nicht durch Andre spenden;", "tokens": ["Und", "nicht", "durch", "And\u00b7re", "spen\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Ich will die St\u00fcrme bitten,", "tokens": ["Ich", "will", "die", "St\u00fcr\u00b7me", "bit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Da\u00df sie's hin\u00fcberweh'n.", "tokens": ["Da\u00df", "sie's", "hin\u00b7\u00fcber\u00b7weh'", "n."], "token_info": ["word", "word", "word", "abbreviation"], "pos": ["KOUS", "PPER", "VVFIN", "NE"], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}