{"dta.poem.3789": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1892", "urn": "urn:nbn:de:kobv:b4-200905191524", "language": ["de:0.99"], "booktitle": "George, Stefan: Algabal. Paris u. a., 1892."}, "poem": {"stanza.1": {"line.1": {"text": "Und ich mit br\u00e4nden und durch adlerfl\u00fcge?", "tokens": ["Und", "ich", "mit", "br\u00e4n\u00b7den", "und", "durch", "ad\u00b7ler\u00b7fl\u00fc\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ADJA", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dass niemals dieser knospe keusche lippe", "tokens": ["Dass", "nie\u00b7mals", "die\u00b7ser", "knos\u00b7pe", "keu\u00b7sche", "lip\u00b7pe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PDS", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom windgef\u00fchrten seim der freundin nippe", "tokens": ["Vom", "wind\u00b7ge\u00b7f\u00fchr\u00b7ten", "seim", "der", "freun\u00b7din", "nip\u00b7pe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dass sie im schwall der salben und gew\u00fcrze", "tokens": ["Dass", "sie", "im", "schwall", "der", "sal\u00b7ben", "und", "ge\u00b7w\u00fcr\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ART", "ADJA", "KON", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des schw\u00fclen kerkers weile sich verk\u00fcrze", "tokens": ["Des", "schw\u00fc\u00b7len", "ker\u00b7kers", "wei\u00b7le", "sich", "ver\u00b7k\u00fcr\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Besprengt vom saft des hanfes und der rebe", "tokens": ["Be\u00b7sprengt", "vom", "saft", "des", "han\u00b7fes", "und", "der", "re\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "KON", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die tr\u00e4gen adern zu beleben strebe", "tokens": ["Die", "tr\u00e4\u00b7gen", "a\u00b7dern", "zu", "be\u00b7le\u00b7ben", "stre\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VVINF", "PTKZU", "VVINF", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und flehend bis sie welke stehen bleibe", "tokens": ["Und", "fle\u00b7hend", "bis", "sie", "wel\u00b7ke", "ste\u00b7hen", "blei\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVPP", "APPR", "PPER", "PWAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vor einer s\u00e4ule spr\u00f6dem marmorleibe", "tokens": ["Vor", "ei\u00b7ner", "s\u00e4u\u00b7le", "spr\u00f6\u00b7dem", "mar\u00b7mor\u00b7lei\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}