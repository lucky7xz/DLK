{"dta.poem.14320": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Am Strom .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Der Flu\u00df glitt einsam hin und rauschte", "tokens": ["Der", "Flu\u00df", "glitt", "ein\u00b7sam", "hin", "und", "rauschte"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie sonst, noch immer, immerfort,", "tokens": ["Wie", "sonst", ",", "noch", "im\u00b7mer", ",", "im\u00b7mer\u00b7fort", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "ADV", "ADV", "$,", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich stand am Strand gelehnt und lauschte,", "tokens": ["Ich", "stand", "am", "Strand", "ge\u00b7lehnt", "und", "lauschte", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ach, was ich liebt', war lange fort!", "tokens": ["Ach", ",", "was", "ich", "liebt'", ",", "war", "lan\u00b7ge", "fort", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWS", "PPER", "VVFIN", "$,", "VAFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Kein Laut, kein Windeshauch, kein Singen", "tokens": ["Kein", "Laut", ",", "kein", "Win\u00b7des\u00b7hauch", ",", "kein", "Sin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ging durch den weiten Mittag schw\u00fcl,", "tokens": ["Ging", "durch", "den", "wei\u00b7ten", "Mit\u00b7tag", "schw\u00fcl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Vertr\u00e4umt die stillen Weiden hingen", "tokens": ["Ver\u00b7tr\u00e4umt", "die", "stil\u00b7len", "Wei\u00b7den", "hin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Hinab bis in die Wellen k\u00fchl.", "tokens": ["Hin\u00b7ab", "bis", "in", "die", "Wel\u00b7len", "k\u00fchl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}