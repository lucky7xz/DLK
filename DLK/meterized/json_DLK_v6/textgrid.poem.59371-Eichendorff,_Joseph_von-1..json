{"textgrid.poem.59371": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was klingt mir so heiter", "tokens": ["Was", "klingt", "mir", "so", "hei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADJD"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Durch Busen und Sinn?", "tokens": ["Durch", "Bu\u00b7sen", "und", "Sinn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Zu Wolken und weiter,", "tokens": ["Zu", "Wol\u00b7ken", "und", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADV", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Wo tr\u00e4gt es mich hin?", "tokens": ["Wo", "tr\u00e4gt", "es", "mich", "hin", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Wie auf Bergen hoch bin ich", "tokens": ["Wie", "auf", "Ber\u00b7gen", "hoch", "bin", "ich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "ADJD", "VAFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "So einsam gestellt", "tokens": ["So", "ein\u00b7sam", "ge\u00b7stellt"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJD", "VVPP"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und gr\u00fc\u00dfe herzinnig,", "tokens": ["Und", "gr\u00fc\u00b7\u00dfe", "her\u00b7zin\u00b7nig", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Was sch\u00f6n auf der Welt.", "tokens": ["Was", "sch\u00f6n", "auf", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Ja, Bacchus, dich seh ich,", "tokens": ["Ja", ",", "Bac\u00b7chus", ",", "dich", "seh", "ich", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NE", "$,", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Wie g\u00f6ttlich bist du!", "tokens": ["Wie", "g\u00f6tt\u00b7lich", "bist", "du", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Dein Gl\u00fchen versteh ich,", "tokens": ["Dein", "Gl\u00fc\u00b7hen", "ver\u00b7steh", "ich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die tr\u00e4umende Ruh.", "tokens": ["Die", "tr\u00e4u\u00b7men\u00b7de", "Ruh", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "O rosenbekr\u00e4nztes", "tokens": ["O", "ro\u00b7sen\u00b7be\u00b7kr\u00e4nz\u00b7tes"], "token_info": ["word", "word"], "pos": ["NE", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "J\u00fcnglingsbild,", "tokens": ["J\u00fcng\u00b7lings\u00b7bild", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Dein Auge, wie gl\u00e4nzt es,", "tokens": ["Dein", "Au\u00b7ge", ",", "wie", "gl\u00e4nzt", "es", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "VVFIN", "PPER", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die Flammen so mild!", "tokens": ["Die", "Flam\u00b7men", "so", "mild", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Ist's Liebe, ist's Andacht,", "tokens": ["Ist's", "Lie\u00b7be", ",", "ist's", "An\u00b7dacht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VAFIN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Was so dich begl\u00fcckt?", "tokens": ["Was", "so", "dich", "be\u00b7gl\u00fcckt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPER", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Rings Fr\u00fchling dich anlacht,", "tokens": ["Rings", "Fr\u00fch\u00b7ling", "dich", "an\u00b7lacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Du sinnest entz\u00fcckt. \u2013", "tokens": ["Du", "sin\u00b7nest", "ent\u00b7z\u00fcckt", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$.", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Frau Venus, du Frohe,", "tokens": ["Frau", "Ve\u00b7nus", ",", "du", "Fro\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PPER", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "So klingend und weich,", "tokens": ["So", "klin\u00b7gend", "und", "weich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "In Morgenrots Lohe", "tokens": ["In", "Mor\u00b7gen\u00b7rots", "Lo\u00b7he"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NE", "NE"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Erblick ich dein Reich", "tokens": ["Er\u00b7blick", "ich", "dein", "Reich"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.7": {"line.1": {"text": "Auf sonnigen H\u00fcgeln", "tokens": ["Auf", "son\u00b7ni\u00b7gen", "H\u00fc\u00b7geln"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Wie ein Zauberring. \u2013", "tokens": ["Wie", "ein", "Zau\u00b7ber\u00b7ring", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ART", "NN", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Zart' B\u00fcbchen mit Fl\u00fcgeln", "tokens": ["Zart'", "B\u00fcb\u00b7chen", "mit", "Fl\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "NN", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Bedienen dich flink,", "tokens": ["Be\u00b7die\u00b7nen", "dich", "flink", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.8": {"line.1": {"text": "Durchs\u00e4useln die R\u00e4ume", "tokens": ["Durch\u00b7s\u00e4u\u00b7seln", "die", "R\u00e4u\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Und laden, was fein,", "tokens": ["Und", "la\u00b7den", ",", "was", "fein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "PRELS", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Als goldene Tr\u00e4ume", "tokens": ["Als", "gol\u00b7de\u00b7ne", "Tr\u00e4u\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Zur K\u00f6nigin ein.", "tokens": ["Zur", "K\u00f6\u00b7ni\u00b7gin", "ein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.9": {"line.1": {"text": "Und Ritter und Frauen", "tokens": ["Und", "Rit\u00b7ter", "und", "Frau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Im gr\u00fcnen Revier", "tokens": ["Im", "gr\u00fc\u00b7nen", "Re\u00b7vier"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Durchschw\u00e4rmen die Auen", "tokens": ["Durch\u00b7schw\u00e4r\u00b7men", "die", "Au\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Wie Blumen zur Zier.", "tokens": ["Wie", "Blu\u00b7men", "zur", "Zier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.10": {"line.1": {"text": "Und jeglicher hegt sich", "tokens": ["Und", "jeg\u00b7li\u00b7cher", "hegt", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "VVFIN", "PRF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Sein Liebchen im Arm,", "tokens": ["Sein", "Lieb\u00b7chen", "im", "Arm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "So wirrt und bewegt sich", "tokens": ["So", "wirrt", "und", "be\u00b7wegt", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PRF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Der selige Schwarm. \u2013", "tokens": ["Der", "se\u00b7li\u00b7ge", "Schwarm", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.11": {"line.1": {"text": "Die Kl\u00e4nge verrinnen,", "tokens": ["Die", "Kl\u00e4n\u00b7ge", "ver\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Es bleichet das Gr\u00fcn,", "tokens": ["Es", "blei\u00b7chet", "das", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Die Frauen stehn sinnend,", "tokens": ["Die", "Frau\u00b7en", "stehn", "sin\u00b7nend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die Ritter schaun k\u00fchn.", "tokens": ["Die", "Rit\u00b7ter", "schaun", "k\u00fchn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.12": {"line.1": {"text": "Und himmlisches Sehnen", "tokens": ["Und", "himm\u00b7li\u00b7sches", "Seh\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Geht singend durchs Blau,", "tokens": ["Geht", "sin\u00b7gend", "durchs", "Blau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Da schimmert von Tr\u00e4nen", "tokens": ["Da", "schim\u00b7mert", "von", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Rings Garten und Au. \u2013", "tokens": ["Rings", "Gar\u00b7ten", "und", "Au", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "NN", "KON", "NN", "$.", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.13": {"line.1": {"text": "Und mitten im Feste", "tokens": ["Und", "mit\u00b7ten", "im", "Fes\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Erblick ich, wie mild!", "tokens": ["Er\u00b7blick", "ich", ",", "wie", "mild", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "PWAV", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Den stillsten der G\u00e4ste. \u2013", "tokens": ["Den", "stills\u00b7ten", "der", "G\u00e4s\u00b7te", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "ART", "NN", "$.", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Woher, einsam Bild?", "tokens": ["Wo\u00b7her", ",", "ein\u00b7sam", "Bild", "?"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PWAV", "$,", "ADJD", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.14": {"line.1": {"text": "Mit bl\u00fchendem Mohne,", "tokens": ["Mit", "bl\u00fc\u00b7hen\u00b7dem", "Moh\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Der tr\u00e4umerisch gl\u00e4nzt,", "tokens": ["Der", "tr\u00e4u\u00b7me\u00b7risch", "gl\u00e4nzt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und mit Lilienkrone", "tokens": ["Und", "mit", "Li\u00b7li\u00b7en\u00b7kro\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Erscheint er bekr\u00e4nzt.", "tokens": ["Er\u00b7scheint", "er", "be\u00b7kr\u00e4nzt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.15": {"line.1": {"text": "Sein Mund schwillt zum K\u00fcssen", "tokens": ["Sein", "Mund", "schwillt", "zum", "K\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "So lieblich und bleich,", "tokens": ["So", "lieb\u00b7lich", "und", "bleich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Als br\u00e4cht er ein Gr\u00fc\u00dfen", "tokens": ["Als", "br\u00e4cht", "er", "ein", "Gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "PPER", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Aus himmlischem Reich.", "tokens": ["Aus", "himm\u00b7li\u00b7schem", "Reich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.16": {"line.1": {"text": "Eine Fackel wohl tr\u00e4gt er,", "tokens": ["Ei\u00b7ne", "Fa\u00b7ckel", "wohl", "tr\u00e4gt", "er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPER", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Die wunderbar prangt.", "tokens": ["Die", "wun\u00b7der\u00b7bar", "prangt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "\u00bbwo ist einer\u00ab, fr\u00e4gt er,", "tokens": ["\u00bb", "wo", "ist", "ei\u00b7ner", "\u00ab", ",", "fr\u00e4gt", "er", ","], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "PWAV", "VAFIN", "ART", "$(", "$,", "VVFIN", "PPER", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "\u00bbdem heimw\u00e4rts verlangt?\u00ab", "tokens": ["\u00bb", "dem", "heim\u00b7w\u00e4rts", "ver\u00b7langt", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "ADV", "VVPP", "$.", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.17": {"line.1": {"text": "Und manchmal da drehet", "tokens": ["Und", "manch\u00b7mal", "da", "dre\u00b7het"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Die Fackel er um \u2013", "tokens": ["Die", "Fa\u00b7ckel", "er", "um", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Tiefschauernd vergehet", "tokens": ["Tief\u00b7schau\u00b7ernd", "ver\u00b7ge\u00b7het"], "token_info": ["word", "word"], "pos": ["ADJD", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die Welt und wird stumm.", "tokens": ["Die", "Welt", "und", "wird", "stumm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VAFIN", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.18": {"line.1": {"text": "Und was hier versunken", "tokens": ["Und", "was", "hier", "ver\u00b7sun\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWS", "ADV", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Als Blumen zum Spiel,", "tokens": ["Als", "Blu\u00b7men", "zum", "Spiel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPRART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Siehst oben du funkeln", "tokens": ["Siehst", "o\u00b7ben", "du", "fun\u00b7keln"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Als Sterne nun k\u00fchl. \u2013", "tokens": ["Als", "Ster\u00b7ne", "nun", "k\u00fchl", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "NN", "ADV", "ADJD", "$.", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.19": {"line.1": {"text": "O J\u00fcngling vom Himmel,", "tokens": ["O", "J\u00fcng\u00b7ling", "vom", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Wie bist du so sch\u00f6n!", "tokens": ["Wie", "bist", "du", "so", "sch\u00f6n", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ich la\u00df das Gewimmel,", "tokens": ["Ich", "la\u00df", "das", "Ge\u00b7wim\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Mit dir will ich gehn!", "tokens": ["Mit", "dir", "will", "ich", "gehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.20": {"line.1": {"text": "Was will ich noch hoffen?", "tokens": ["Was", "will", "ich", "noch", "hof\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+---+-", "measure": "dactylic.init"}, "line.2": {"text": "Hinauf, ach hinauf!", "tokens": ["Hin\u00b7auf", ",", "ach", "hin\u00b7auf", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Der Himmel ist offen,", "tokens": ["Der", "Him\u00b7mel", "ist", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Nimm, Vater, mich auf!", "tokens": ["Nimm", ",", "Va\u00b7ter", ",", "mich", "auf", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "PPER", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}