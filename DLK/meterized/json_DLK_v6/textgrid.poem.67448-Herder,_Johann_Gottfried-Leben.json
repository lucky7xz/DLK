{"textgrid.poem.67448": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Leben", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nur ", "tokens": ["Nur"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Unser Schauspiel, es ruft Scene nach Scenen hervor.", "tokens": ["Un\u00b7ser", "Schau\u00b7spiel", ",", "es", "ruft", "Sce\u00b7ne", "nach", "Sce\u00b7nen", "her\u00b7vor", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VVFIN", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und doch binden so selten in uns sich Alter und Scenen;", "tokens": ["Und", "doch", "bin\u00b7den", "so", "sel\u00b7ten", "in", "uns", "sich", "Al\u00b7ter", "und", "Sce\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ADV", "ADJD", "APPR", "PPER", "PRF", "NN", "KON", "NN", "$."], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Neulinge sind wir als Kind, Neulinge gehn wir ins Grab.", "tokens": ["Neu\u00b7lin\u00b7ge", "sind", "wir", "als", "Kind", ",", "Neu\u00b7lin\u00b7ge", "gehn", "wir", "ins", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "KOUS", "NN", "$,", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+--+-+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Auch die uns h\u00f6ren und sehn, Neulinge gehn sie vor\u00fcber;", "tokens": ["Auch", "die", "uns", "h\u00f6\u00b7ren", "und", "sehn", ",", "Neu\u00b7lin\u00b7ge", "gehn", "sie", "vor\u00b7\u00fc\u00b7ber", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "PPER", "VVINF", "KON", "VVINF", "$,", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Also spiele Dein Spiel, nicht f\u00fcr die Menge, f\u00fcr Dich!", "tokens": ["Al\u00b7so", "spie\u00b7le", "Dein", "Spiel", ",", "nicht", "f\u00fcr", "die", "Men\u00b7ge", ",", "f\u00fcr", "Dich", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "PTKNEG", "APPR", "ART", "NN", "$,", "APPR", "PPER", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}}}}}