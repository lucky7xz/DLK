{"dta.poem.5463": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zu sehen f\u00e4hig ist. Nichts kann ihn mehr erheben", "tokens": ["Zu", "se\u00b7hen", "f\u00e4\u00b7hig", "ist", ".", "Nichts", "kann", "ihn", "mehr", "er\u00b7he\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "ADJD", "VAFIN", "$.", "PIS", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nichts kann auch mehr Trost, mehr Zuversicht,", "tokens": ["Und", "nichts", "kann", "auch", "mehr", "Trost", ",", "mehr", "Zu\u00b7ver\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "ADV", "PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df er uns helffen kann, wenn er nur will, uns geben.", "tokens": ["Da\u00df", "er", "uns", "helf\u00b7fen", "kann", ",", "wenn", "er", "nur", "will", ",", "uns", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "VMFIN", "$,", "KOUS", "PPER", "ADV", "VMFIN", "$,", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Beweiset es nun gleich, da\u00df er auch wolle, nicht;", "tokens": ["Be\u00b7wei\u00b7set", "es", "nun", "gleich", ",", "da\u00df", "er", "auch", "wol\u00b7le", ",", "nicht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$,", "KOUS", "PPER", "ADV", "VMFIN", "$,", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So giebt uns ja davon den besten Unterricht,", "tokens": ["So", "giebt", "uns", "ja", "da\u00b7von", "den", "bes\u00b7ten", "Un\u00b7ter\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df er in seinem Wort und unsre Seelen schriebe:", "tokens": ["Da\u00df", "er", "in", "sei\u00b7nem", "Wort", "und", "uns\u00b7re", "See\u00b7len", "schrie\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der GOTT, der alles kann, ist auch die ew'ge Liebe!", "tokens": ["Der", "GoTT", ",", "der", "al\u00b7les", "kann", ",", "ist", "auch", "die", "ew'\u00b7ge", "Lie\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "VMFIN", "$,", "VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}