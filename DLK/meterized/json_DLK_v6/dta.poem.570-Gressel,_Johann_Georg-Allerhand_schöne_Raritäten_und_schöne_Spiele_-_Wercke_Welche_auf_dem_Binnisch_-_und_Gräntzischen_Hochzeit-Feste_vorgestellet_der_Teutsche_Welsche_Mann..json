{"dta.poem.570": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Allerhand sch\u00f6ne Rarit\u00e4ten und sch\u00f6ne  \n Spiele - Wercke/  \n Welche auf dem  \n Binnisch - und Gr\u00e4ntzischen  \n Hochzeit-Feste vorgestellet  \n der Teutsche Welsche Mann.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Ey sehet doch/ dort sitzt die sch\u00f6ne Katze Bricks", "tokens": ["Ey", "se\u00b7het", "doch", "/", "dort", "sitzt", "die", "sch\u00f6\u00b7ne", "Kat\u00b7ze", "Bricks"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "$(", "ADV", "VVFIN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Am Tage ", "tokens": ["Am", "Ta\u00b7ge"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Wie lecket sie das Maul/ und w\u00fcnscht darzu viel Gl\u00fccks", "tokens": ["Wie", "le\u00b7cket", "sie", "das", "Maul", "/", "und", "w\u00fcnscht", "dar\u00b7zu", "viel", "Gl\u00fccks"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "$(", "KON", "VVFIN", "PAV", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der neuen Jungfer Brant/ die ihr von Fisch und Haasen", "tokens": ["Der", "neu\u00b7en", "Jung\u00b7fer", "Brant", "/", "die", "ihr", "von", "Fisch", "und", "Haa\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NE", "$(", "PRELS", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Viel gute Bi\u00dfgen vorgelegt/", "tokens": ["Viel", "gu\u00b7te", "Bi\u00df\u00b7gen", "vor\u00b7ge\u00b7legt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auch sie gestr\u00f6pelt und gehegt.", "tokens": ["Auch", "sie", "ge\u00b7str\u00f6\u00b7pelt", "und", "ge\u00b7hegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sch\u00f6ne Rarit\u00e4t/ sch\u00f6ne Spiele-Werckerc.", "tokens": ["Sch\u00f6\u00b7ne", "Ra\u00b7ri\u00b7t\u00e4t", "/", "sch\u00f6\u00b7ne", "Spie\u00b7le\u00b7\u00b7Wer\u00b7ckerc", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "ADJA", "NN", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}}}}}