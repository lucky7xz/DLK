{"dta.poem.7365": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Das Fegefeuer des westph\u00e4lischen Adels.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Wo der selige Himmel, das wissen wir nicht,", "tokens": ["Wo", "der", "se\u00b7li\u00b7ge", "Him\u00b7mel", ",", "das", "wis\u00b7sen", "wir", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,", "PDS", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Und nicht, wo der gr\u00e4uliche H\u00f6llenschlund,", "tokens": ["Und", "nicht", ",", "wo", "der", "gr\u00e4u\u00b7li\u00b7che", "H\u00f6l\u00b7len\u00b7schlund", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Ob auch die Wolke zittert im Licht,", "tokens": ["Ob", "auch", "die", "Wol\u00b7ke", "zit\u00b7tert", "im", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Ob siedet und qualmet Vulkanes Mund;", "tokens": ["Ob", "sie\u00b7det", "und", "qual\u00b7met", "Vul\u00b7ka\u00b7nes", "Mund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "KON", "VVFIN", "NE", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Doch wo die westph\u00e4lischen Edeln m\u00fcssen", "tokens": ["Doch", "wo", "die", "west\u00b7ph\u00e4\u00b7li\u00b7schen", "E\u00b7deln", "m\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "VMFIN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Sich sauber brennen ihr rostig Gewissen,", "tokens": ["Sich", "sau\u00b7ber", "bren\u00b7nen", "ihr", "ros\u00b7tig", "Ge\u00b7wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVINF", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Das wissen wir alle, das ward uns kund.", "tokens": ["Das", "wis\u00b7sen", "wir", "al\u00b7le", ",", "das", "ward", "uns", "kund", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PIS", "$,", "PDS", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Grau war die Nacht, nicht \u00f6de und schwer,", "tokens": ["Grau", "war", "die", "Nacht", ",", "nicht", "\u00f6\u00b7de", "und", "schwer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "PTKNEG", "ADJD", "KON", "ADJD", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Ein Aschenschleier hing in der Luft;", "tokens": ["Ein", "A\u00b7schen\u00b7schlei\u00b7er", "hing", "in", "der", "Luft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Der Wanderbursche schritt flink einher,", "tokens": ["Der", "Wan\u00b7der\u00b7bur\u00b7sche", "schritt", "flink", "ein\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Mit Wollust saugend den Heimatduft;", "tokens": ["Mit", "Wol\u00b7lust", "sau\u00b7gend", "den", "Hei\u00b7mat\u00b7duft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "O bald, bald wird er schauen sein Eigen,", "tokens": ["O", "bald", ",", "bald", "wird", "er", "schau\u00b7en", "sein", "Ei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "ADV", "VAFIN", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Schon sieht am Lutterberge er steigen", "tokens": ["Schon", "sieht", "am", "Lut\u00b7ter\u00b7ber\u00b7ge", "er", "stei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "PPER", "VVINF"], "meter": "-+-+----+-", "measure": "unknown.measure.tri"}, "line.7": {"text": "Sich leise schattend die schwarze Kluft.", "tokens": ["Sich", "lei\u00b7se", "schat\u00b7tend", "die", "schwar\u00b7ze", "Kluft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Er richtet sich, wie Trompetensto\u00df", "tokens": ["Er", "rich\u00b7tet", "sich", ",", "wie", "Trom\u00b7pe\u00b7ten\u00b7sto\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "$,", "PWAV", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ein Hollah ho! seiner Brust entsteigt \u2014", "tokens": ["Ein", "Hol\u00b7lah", "ho", "!", "sei\u00b7ner", "Brust", "ent\u00b7steigt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$.", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Was ihm im Nacken? ein schnaubend Ro\u00df,", "tokens": ["Was", "ihm", "im", "Na\u00b7cken", "?", "ein", "schnau\u00b7bend", "Ro\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "$.", "ART", "ADJD", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "An seiner Schulter es rasselt, keucht,", "tokens": ["An", "sei\u00b7ner", "Schul\u00b7ter", "es", "ras\u00b7selt", ",", "keucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ein Rappe \u2014 gr\u00fcnliche Funken irren", "tokens": ["Ein", "Rap\u00b7pe", "gr\u00fcn\u00b7li\u00b7che", "Fun\u00b7ken", "ir\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$(", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ueber die Flanken, die knistern und knirren,", "tokens": ["Ue\u00b7ber", "die", "Flan\u00b7ken", ",", "die", "knis\u00b7tern", "und", "knir\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "NN", "KON", "VVFIN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.7": {"text": "Wie wenn man den murrenden Kater streicht.", "tokens": ["Wie", "wenn", "man", "den", "mur\u00b7ren\u00b7den", "Ka\u00b7ter", "streicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "PIS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "\u201ejesus Maria!\u201c \u2014 er setzt seitab,", "tokens": ["\u201e", "je\u00b7sus", "Ma\u00b7ria", "!", "\u201c", "er", "setzt", "seit\u00b7ab", ","], "token_info": ["punct", "word", "word", "punct", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "NE", "NE", "$.", "$(", "$(", "PPER", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da langt vom Sattel es \u00fcberzwerg \u2014", "tokens": ["Da", "langt", "vom", "Sat\u00b7tel", "es", "\u00fc\u00b7berz\u00b7werg"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ein eherner Griff, und in w\u00fcstem Trab", "tokens": ["Ein", "e\u00b7her\u00b7ner", "Griff", ",", "und", "in", "w\u00fcs\u00b7tem", "Trab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "APPR", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Wie Wind und Wirbel zum Lutterberg!", "tokens": ["Wie", "Wind", "und", "Wir\u00b7bel", "zum", "Lut\u00b7ter\u00b7berg", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "An seinem Ohre h\u00f6rt er es raunen", "tokens": ["An", "sei\u00b7nem", "Oh\u00b7re", "h\u00f6rt", "er", "es", "rau\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PPER", "VVINF"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Dumpf und hohl, wie ged\u00e4mpfte Posaunen,", "tokens": ["Dumpf", "und", "hohl", ",", "wie", "ge\u00b7d\u00e4mpf\u00b7te", "Po\u00b7sau\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$,", "PWAV", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "So an ihm raunt der gespenstige Scherg':", "tokens": ["So", "an", "ihm", "raunt", "der", "ge\u00b7spens\u00b7ti\u00b7ge", "Scher\u00b7g'", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "\u201ejohannes Deweth! ich kenne dich!", "tokens": ["\u201e", "jo\u00b7han\u00b7nes", "De\u00b7weth", "!", "ich", "ken\u00b7ne", "dich", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$.", "PPER", "VVFIN", "PPER", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Johann! du bist uns verfallen heut!", "tokens": ["Jo\u00b7hann", "!", "du", "bist", "uns", "ver\u00b7fal\u00b7len", "heut", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VAFIN", "PPER", "VVFIN", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Bei deinem Heile, nicht lach' noch sprich,", "tokens": ["Bei", "dei\u00b7nem", "Hei\u00b7le", ",", "nicht", "lach'", "noch", "sprich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PTKNEG", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und r\u00fchre nicht an was man dir beut;", "tokens": ["Und", "r\u00fch\u00b7re", "nicht", "an", "was", "man", "dir", "beut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "APPR", "PRELS", "PIS", "PPER", "VVFIN", "$."], "meter": "-+---+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Vom Brode nur magst du brechen in Frieden,", "tokens": ["Vom", "Bro\u00b7de", "nur", "magst", "du", "bre\u00b7chen", "in", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VMFIN", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ewiges Heil ward dem Brode beschieden,", "tokens": ["E\u00b7wi\u00b7ges", "Heil", "ward", "dem", "Bro\u00b7de", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.7": {"text": "Als Christus in froner Nacht es geweiht!\u201c \u2014", "tokens": ["Als", "Chris\u00b7tus", "in", "fro\u00b7ner", "Nacht", "es", "ge\u00b7weiht", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KOUS", "NE", "APPR", "ADJA", "NN", "PPER", "VVPP", "$.", "$(", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Ob mehr gesprochen, man wei\u00df es nicht,", "tokens": ["Ob", "mehr", "ge\u00b7spro\u00b7chen", ",", "man", "wei\u00df", "es", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VVPP", "$,", "PIS", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da seine Sinne der Bursche verlor,", "tokens": ["Da", "sei\u00b7ne", "Sin\u00b7ne", "der", "Bur\u00b7sche", "ver\u00b7lor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und sp\u00e4t erst hebt er sein bleiches Gesicht", "tokens": ["Und", "sp\u00e4t", "erst", "hebt", "er", "sein", "blei\u00b7ches", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Vom Estrich einer Halle empor;", "tokens": ["Vom", "Est\u00b7rich", "ei\u00b7ner", "Hal\u00b7le", "em\u00b7por", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Um ihn Gesumme, Geschwirr, Gemunkel,", "tokens": ["Um", "ihn", "Ge\u00b7sum\u00b7me", ",", "Ge\u00b7schwirr", ",", "Ge\u00b7mun\u00b7kel", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUI", "PPER", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.6": {"text": "Von tausend Fl\u00e4mmchen ein mattes Gefunkel,", "tokens": ["Von", "tau\u00b7send", "Fl\u00e4mm\u00b7chen", "ein", "mat\u00b7tes", "Ge\u00b7fun\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und dr\u00fcber schwimmend ein Nebelflor.", "tokens": ["Und", "dr\u00fc\u00b7ber", "schwim\u00b7mend", "ein", "Ne\u00b7bel\u00b7flor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ADJD", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Er reibt die Augen, er schwankt voran,", "tokens": ["Er", "reibt", "die", "Au\u00b7gen", ",", "er", "schwankt", "vo\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "An hundert Tischen, die Halle entlang,", "tokens": ["An", "hun\u00b7dert", "Ti\u00b7schen", ",", "die", "Hal\u00b7le", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$,", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "All edle Geschlechter, so Mann an Mann;", "tokens": ["All", "ed\u00b7le", "Ge\u00b7schlech\u00b7ter", ",", "so", "Mann", "an", "Mann", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "ADV", "NN", "APPR", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Es r\u00fchren die Gl\u00e4ser sich sonder Klang,", "tokens": ["Es", "r\u00fch\u00b7ren", "die", "Gl\u00e4\u00b7ser", "sich", "son\u00b7der", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PRF", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Es regen die Messer sich sonder Klirren,", "tokens": ["Es", "re\u00b7gen", "die", "Mes\u00b7ser", "sich", "son\u00b7der", "Klir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PRF", "APPR", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Wechselnde Reden summen und schwirren,", "tokens": ["Wech\u00b7seln\u00b7de", "Re\u00b7den", "sum\u00b7men", "und", "schwir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Wie Glockengel\u00e4ut, ein wirrer Gesang.", "tokens": ["Wie", "Glo\u00b7cken\u00b7ge\u00b7l\u00e4ut", ",", "ein", "wir\u00b7rer", "Ge\u00b7sang", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Ob jedem Haupte des Wappens Glast,", "tokens": ["Ob", "je\u00b7dem", "Haup\u00b7te", "des", "Wap\u00b7pens", "Glast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das langsam schwellende Tropfen speit,", "tokens": ["Das", "lang\u00b7sam", "schwel\u00b7len\u00b7de", "Trop\u00b7fen", "speit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und wenn sie fallen, dann zuckt der Gast,", "tokens": ["Und", "wenn", "sie", "fal\u00b7len", ",", "dann", "zuckt", "der", "Gast", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVINF", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und dr\u00e4ngt sich einen Moment zur Seit';", "tokens": ["Und", "dr\u00e4ngt", "sich", "ei\u00b7nen", "Mo\u00b7ment", "zur", "Seit'", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und lauter, lauter dann wird das Rauschen,", "tokens": ["Und", "lau\u00b7ter", ",", "lau\u00b7ter", "dann", "wird", "das", "Rau\u00b7schen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADJD", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wie St\u00fcrme die zornigen Seufzer tauschen,", "tokens": ["Wie", "St\u00fcr\u00b7me", "die", "zor\u00b7ni\u00b7gen", "Seuf\u00b7zer", "tau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Und wirrer summet das Glockengel\u00e4ut.", "tokens": ["Und", "wir\u00b7rer", "sum\u00b7met", "das", "Glo\u00b7cken\u00b7ge\u00b7l\u00e4ut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Strack steht Johann wie ein Lanzenknecht,", "tokens": ["Strack", "steht", "Jo\u00b7hann", "wie", "ein", "Lan\u00b7zen\u00b7knecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NE", "KOKOM", "ART", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Nicht m\u00f6chte der glei\u00dfenden Wand er trau'n,", "tokens": ["Nicht", "m\u00f6ch\u00b7te", "der", "glei\u00b7\u00dfen\u00b7den", "Wand", "er", "trau'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VMFIN", "ART", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Noch w\u00e4re der glimmernde Sitz ihm recht,", "tokens": ["Noch", "w\u00e4\u00b7re", "der", "glim\u00b7mern\u00b7de", "Sitz", "ihm", "recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "PPER", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Wo rutschen die Knappen mit zuckenden Brau'n.", "tokens": ["Wo", "rut\u00b7schen", "die", "Knap\u00b7pen", "mit", "zu\u00b7cken\u00b7den", "Brau'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["PWAV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "NE"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Da mu\u00df, o Himmel, wer sollt' es denken!", "tokens": ["Da", "mu\u00df", ",", "o", "Him\u00b7mel", ",", "wer", "sollt'", "es", "den\u00b7ken", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "$,", "FM", "NN", "$,", "PWS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Den frommen Herrn, den Friedrich von Brenken,", "tokens": ["Den", "from\u00b7men", "Herrn", ",", "den", "Fried\u00b7rich", "von", "Bren\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NE", "APPR", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Den alten stattlichen Ritter er schaun.", "tokens": ["Den", "al\u00b7ten", "statt\u00b7li\u00b7chen", "Rit\u00b7ter", "er", "schaun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "\u201emein Heiland, mach' ihn der S\u00fcnden baar!\u201c", "tokens": ["\u201e", "mein", "Hei\u00b7land", ",", "mach'", "ihn", "der", "S\u00fcn\u00b7den", "baar", "!", "\u201c"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "ADJD", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der J\u00fcngling seufzet in schwerem Leid;", "tokens": ["Der", "J\u00fcng\u00b7ling", "seuf\u00b7zet", "in", "schwe\u00b7rem", "Leid", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Er hat ihm gedienet ein ganzes Jahr;", "tokens": ["Er", "hat", "ihm", "ge\u00b7die\u00b7net", "ein", "gan\u00b7zes", "Jahr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch ungern kredenzt er den Becher ihm heut!", "tokens": ["Doch", "un\u00b7gern", "kre\u00b7denzt", "er", "den", "Be\u00b7cher", "ihm", "heut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "PPER", "ADV", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Bei jedem Schlucke sieht er ihn sch\u00fcttern,", "tokens": ["Bei", "je\u00b7dem", "Schlu\u00b7cke", "sieht", "er", "ihn", "sch\u00fct\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ein blaues W\u00f6lkchen dem Schlund entzittern,", "tokens": ["Ein", "blau\u00b7es", "W\u00f6lk\u00b7chen", "dem", "Schlund", "ent\u00b7zit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Wie wenn auf Kohlen man Weihrauch streut.", "tokens": ["Wie", "wenn", "auf", "Koh\u00b7len", "man", "Weih\u00b7rauch", "streut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "APPR", "NN", "PIS", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.11": {"line.1": {"text": "O manche Gestalt noch d\u00e4mmert ihm auf,", "tokens": ["O", "man\u00b7che", "Ge\u00b7stalt", "noch", "d\u00e4m\u00b7mert", "ihm", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dort sitzt sein Pathe, der Metternich,", "tokens": ["Dort", "sitzt", "sein", "Pa\u00b7the", ",", "der", "Met\u00b7ter\u00b7nich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "ART", "NE", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und eben durch den wimmelnden Hauf", "tokens": ["Und", "e\u00b7ben", "durch", "den", "wim\u00b7meln\u00b7den", "Hauf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Johann von Spiegel, der Schenke, strich;", "tokens": ["Jo\u00b7hann", "von", "Spie\u00b7gel", ",", "der", "Schen\u00b7ke", ",", "strich", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "APPR", "NN", "$,", "ART", "NN", "$,", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Pr\u00e4laten auch, je viere und viere,", "tokens": ["Pr\u00e4\u00b7la\u00b7ten", "auch", ",", "je", "vie\u00b7re", "und", "vie\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "ADV", "ADJA", "KON", "PIS", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Sie bl\u00e4ttern und rispeln im grauen Breviere,", "tokens": ["Sie", "bl\u00e4t\u00b7tern", "und", "ris\u00b7peln", "im", "grau\u00b7en", "Bre\u00b7vie\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KON", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.7": {"text": "Und zuckend kr\u00fcmmen die Finger sich.", "tokens": ["Und", "zu\u00b7ckend", "kr\u00fcm\u00b7men", "die", "Fin\u00b7ger", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "PRF", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.12": {"line.1": {"text": "Und unten im Saale, da kn\u00f6cheln frisch", "tokens": ["Und", "un\u00b7ten", "im", "Saa\u00b7le", ",", "da", "kn\u00f6\u00b7cheln", "frisch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN", "$,", "ADV", "VVFIN", "ADJD"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Schaumburger Grafen um Leut' und Land,", "tokens": ["Schaum\u00b7bur\u00b7ger", "Gra\u00b7fen", "um", "Leut'", "und", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Graf Simon sch\u00fcttelt den Becher risch,", "tokens": ["Graf", "Si\u00b7mon", "sch\u00fct\u00b7telt", "den", "Be\u00b7cher", "risch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Und reibt mitunter die knisternde Hand;", "tokens": ["Und", "reibt", "mi\u00b7tun\u00b7ter", "die", "knis\u00b7tern\u00b7de", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ein Knappe nahet, er surret leise \u2014", "tokens": ["Ein", "Knap\u00b7pe", "na\u00b7het", ",", "er", "sur\u00b7ret", "lei\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ADJD", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ha, welches Gesummse im weiten Kreise,", "tokens": ["Ha", ",", "wel\u00b7ches", "Ge\u00b7summ\u00b7se", "im", "wei\u00b7ten", "Krei\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAT", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Wie hundert Schw\u00e4rme an Klippenrand!", "tokens": ["Wie", "hun\u00b7dert", "Schw\u00e4r\u00b7me", "an", "Klip\u00b7pen\u00b7rand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "CARD", "NN", "APPR", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.13": {"line.1": {"text": "\u201egeschwind den Sessel, den Humpen werth,", "tokens": ["\u201e", "ge\u00b7schwind", "den", "Ses\u00b7sel", ",", "den", "Hum\u00b7pen", "werth", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "ART", "NN", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Den schleichenden Wolf* geschwinde herbei!\u201c", "tokens": ["Den", "schlei\u00b7chen\u00b7den", "Wolf", "*", "ge\u00b7schwin\u00b7de", "her\u00b7bei", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NE", "XY", "ADJA", "PTKVZ", "$.", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Horch, wie es drau\u00dfen rasselt und f\u00e4hrt!", "tokens": ["Horch", ",", "wie", "es", "drau\u00b7\u00dfen", "ras\u00b7selt", "und", "f\u00e4hrt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPER", "ADV", "VVFIN", "KON", "VVFIN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Baarhaupt stehet die Massoney,", "tokens": ["Baar\u00b7haupt", "ste\u00b7het", "die", "Mas\u00b7so\u00b7ney", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Hundert Lanzen dr\u00e4ngen nach binnen,", "tokens": ["Hun\u00b7dert", "Lan\u00b7zen", "dr\u00e4n\u00b7gen", "nach", "bin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "APPR", "VVINF", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Hundert Lanzen und mitten darinnen", "tokens": ["Hun\u00b7dert", "Lan\u00b7zen", "und", "mit\u00b7ten", "da\u00b7rin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "KON", "ADV", "ADV"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Der Asseburger, der blutige Weih!", "tokens": ["Der", "As\u00b7se\u00b7bur\u00b7ger", ",", "der", "blu\u00b7ti\u00b7ge", "Weih", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.14": {"line.1": {"text": "Und als ihm alles entgegen zieht,", "tokens": ["Und", "als", "ihm", "al\u00b7les", "ent\u00b7ge\u00b7gen", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PIS", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da spricht Johannes ein Sto\u00dfgebet:", "tokens": ["Da", "spricht", "Jo\u00b7han\u00b7nes", "ein", "Sto\u00df\u00b7ge\u00b7bet", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dann risch hinein! sein Ermel spr\u00fcht,", "tokens": ["Dann", "risch", "hin\u00b7ein", "!", "sein", "Er\u00b7mel", "spr\u00fcht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "$.", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Funken \u00fcber die Finger ihm geht.", "tokens": ["Ein", "Fun\u00b7ken", "\u00fc\u00b7ber", "die", "Fin\u00b7ger", "ihm", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Voran \u2014 da \u201esieben\u201c schwirren die L\u00fcfte", "tokens": ["Vo\u00b7ran", "da", "\u201e", "sie\u00b7ben", "\u201c", "schwir\u00b7ren", "die", "L\u00fcf\u00b7te"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$(", "KOUS", "$(", "CARD", "$(", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "\u201esieben, sieben, sieben,\u201c die Kl\u00fcfte,", "tokens": ["\u201e", "sie\u00b7ben", ",", "sie\u00b7ben", ",", "sie\u00b7ben", ",", "\u201c", "die", "Kl\u00fcf\u00b7te", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "CARD", "$,", "VVFIN", "$,", "VVFIN", "$,", "$(", "ART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "\u201ein sieben Wochen, Johann Deweth!\u201c", "tokens": ["\u201e", "in", "sie\u00b7ben", "Wo\u00b7chen", ",", "Jo\u00b7hann", "De\u00b7weth", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "CARD", "NN", "$,", "NE", "NE", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.15": {"line.1": {"text": "Der sinkt auf schwellenden Rasen hin,", "tokens": ["Der", "sinkt", "auf", "schwel\u00b7len\u00b7den", "Ra\u00b7sen", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und sch\u00fcttelt gegen den Mond die Hand,", "tokens": ["Und", "sch\u00fct\u00b7telt", "ge\u00b7gen", "den", "Mond", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Drei Finger die br\u00f6ckeln und st\u00e4uben hin,", "tokens": ["Drei", "Fin\u00b7ger", "die", "br\u00f6\u00b7ckeln", "und", "st\u00e4u\u00b7ben", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ART", "NN", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Zu Asch' und Kn\u00f6chelchen abgebrannt.", "tokens": ["Zu", "Asch'", "und", "Kn\u00f6\u00b7chel\u00b7chen", "ab\u00b7ge\u00b7brannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Er rafft sich auf, er rennt, er schie\u0