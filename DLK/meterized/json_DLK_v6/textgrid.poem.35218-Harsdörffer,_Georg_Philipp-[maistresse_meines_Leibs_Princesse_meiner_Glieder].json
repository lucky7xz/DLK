{"textgrid.poem.35218": {"metadata": {"author": {"name": "Harsd\u00f6rffer, Georg Philipp", "birth": "N.A.", "death": "N.A."}, "title": "[maistresse meines Leibs/ Princesse meiner Glieder]", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die meinem ", "tokens": ["Die", "mei\u00b7nem"], "token_info": ["word", "word"], "pos": ["ART", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Ein ", "tokens": ["Ein"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Des Denkens/ das ich nehr/ ein st\u00e4tes ", "tokens": ["Des", "Den\u00b7kens", "/", "das", "ich", "nehr", "/", "ein", "st\u00e4\u00b7tes"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "ADV", "$(", "ART", "ADJA"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "H\u00f6rt an mein ", "tokens": ["H\u00f6rt", "an", "mein"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "APPR", "PPOSAT"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Ihr seyds/ ", "tokens": ["Ihr", "seyds", "/"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VAFIN", "$("], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Die mich ", "tokens": ["Die", "mich"], "token_info": ["word", "word"], "pos": ["ART", "PPER"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Mein Hertz ", "tokens": ["Mein", "Hertz"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Die Kehle ", "tokens": ["Die", "Keh\u00b7le"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Mich wolt ", "tokens": ["Mich", "wolt"], "token_info": ["word", "word"], "pos": ["PPER", "VMFIN"], "meter": "-+", "measure": "iambic.single"}, "line.10": {"text": "Vnd meine ", "tokens": ["Vnd", "mei\u00b7ne"], "token_info": ["word", "word"], "pos": ["KON", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Daher ich ", "tokens": ["Da\u00b7her", "ich"], "token_info": ["word", "word"], "pos": ["PAV", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.12": {"text": "Das macht das ", "tokens": ["Das", "macht", "das"], "token_info": ["word", "word", "word"], "pos": ["PDS", "VVFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "Der Augen ", "tokens": ["Der", "Au\u00b7gen"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "Vnd meine starke Brust in ", "tokens": ["Vnd", "mei\u00b7ne", "star\u00b7ke", "Brust", "in"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Schaut hier/ mein Juppenbeltz spielt m\u00e4hlig", "tokens": ["Schaut", "hier", "/", "mein", "Jup\u00b7pen\u00b7beltz", "spielt", "m\u00e4h\u00b7lig"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$(", "PPOSAT", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Weil da\u00df ihn ", "tokens": ["Weil", "da\u00df", "ihn"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.17": {"text": "Der euch vvn Augen blitzt/ gleich einer ", "tokens": ["Der", "euch", "vvn", "Au\u00b7gen", "blitzt", "/", "gleich", "ei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PPER", "NE", "NN", "VVFIN", "$(", "ADV", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.19": {"text": "Den harten Vordantz macht. Nehmt an die", "tokens": ["Den", "har\u00b7ten", "Vor\u00b7dantz", "macht", ".", "Nehmt", "an", "die"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "VVFIN", "APPR", "ART"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.20": {"text": "Die euch ", "tokens": ["Die", "euch"], "token_info": ["word", "word"], "pos": ["ART", "PPER"], "meter": "++", "measure": "spondeus"}, "line.21": {"text": "Zwar ihr ", "tokens": ["Zwar", "ihr"], "token_info": ["word", "word"], "pos": ["ADV", "PPOSAT"], "meter": "+-", "measure": "trochaic.single"}, "line.22": {"text": "Ihr nennt mich einen Schourk und eures Dieners ", "tokens": ["Ihr", "nennt", "mich", "ei\u00b7nen", "Schourk", "und", "eu\u00b7res", "Die\u00b7ners"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Doch ", "tokens": ["Doch"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.24": {"text": "Der Schmertzen ", "tokens": ["Der", "Schmert\u00b7zen"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.25": {"text": "Bin ich kein ", "tokens": ["Bin", "ich", "kein"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PPER", "PIAT"], "meter": "+-+", "measure": "trochaic.di"}, "line.26": {"text": "De\u00df/ was den Buhler macht bey ", "tokens": ["De\u00df", "/", "was", "den", "Buh\u00b7ler", "macht", "bey"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "$(", "PWS", "ART", "NN", "VVFIN", "APPR"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.27": {"text": "Es ist/ wisst/ mein ", "tokens": ["Es", "ist", "/", "wisst", "/", "mein"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "$(", "VVFIN", "$(", "PPOSAT"], "meter": "-+-+", "measure": "iambic.di"}, "line.28": {"text": "Der Reden/ die ich brauch/ mit der ", "tokens": ["Der", "Re\u00b7den", "/", "die", "ich", "brauch", "/", "mit", "der"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "VVFIN", "$(", "APPR", "ART"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.29": {"text": "Die man in St\u00e4dten treibt: Der W\u00f6rter ", "tokens": ["Die", "man", "in", "St\u00e4d\u00b7ten", "treibt", ":", "Der", "W\u00f6r\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PIS", "APPR", "NN", "VVFIN", "$.", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Die fremde ", "tokens": ["Die", "frem\u00b7de"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.31": {"text": "De\u00df/ der da r\u00fcmt/ er hab ein Welsches ", "tokens": ["De\u00df", "/", "der", "da", "r\u00fcmt", "/", "er", "hab", "ein", "Wel\u00b7sches"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "$(", "ART", "ADV", "VVFIN", "$(", "PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mein ", "tokens": ["Mein"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Euch ", "tokens": ["Euch"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Wie hart es ", "tokens": ["Wie", "hart", "es"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADJD", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Wer ", "tokens": ["Wer"], "token_info": ["word"], "pos": ["PWS"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Vnd ", "tokens": ["Vnd"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}}, "stanza.3": {"line.1": {"text": "Was hab ich dann ", "tokens": ["Was", "hab", "ich", "dann"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Zu eurer ", "tokens": ["Zu", "eu\u00b7rer"], "token_info": ["word", "word"], "pos": ["APPR", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Die gegen eurer Zier sind schlechte ", "tokens": ["Die", "ge\u00b7gen", "eu\u00b7rer", "Zier", "sind", "schlech\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VAFIN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Soll ich \u00fcm diese ", "tokens": ["Soll", "ich", "\u00fcm", "die\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "VVFIN", "PDAT"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Vnd ", "tokens": ["Vnd"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Steht bey ", "tokens": ["Steht", "bey"], "token_info": ["word", "word"], "pos": ["VVFIN", "APPR"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Bald wird man sprengen aus/ gebt ihr mir nicht", "tokens": ["Bald", "wird", "man", "spren\u00b7gen", "aus", "/", "gebt", "ihr", "mir", "nicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIS", "VVFIN", "PTKVZ", "$(", "VVFIN", "PPER", "PPER", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wie ist es dann/ ", "tokens": ["Wie", "ist", "es", "dann", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Da\u00df euer Hertze sey von einem Felsengibel", "tokens": ["Da\u00df", "eu\u00b7er", "Hert\u00b7ze", "sey", "von", "ei\u00b7nem", "Fel\u00b7sen\u00b7gi\u00b7bel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.10": {"text": "Vnd Eisendichtem Zeug? das Augenpar ", "tokens": ["Vnd", "Ei\u00b7sen\u00b7dich\u00b7tem", "Zeug", "?", "das", "Au\u00b7gen\u00b7par"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJA", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Wie sonst ", "tokens": ["Wie", "sonst"], "token_info": ["word", "word"], "pos": ["PWAV", "ADV"], "meter": "-+", "measure": "iambic.single"}, "line.12": {"text": "In ihnen h\u00e4mmert st\u00e4ts ein krummer Schmid der Liebe/", "tokens": ["In", "ih\u00b7nen", "h\u00e4m\u00b7mert", "st\u00e4ts", "ein", "krum\u00b7mer", "Schmid", "der", "Lie\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der Schorstein d\u00fcnket mich noch gegen ihnen tr\u00fcbe/", "tokens": ["Der", "Schor\u00b7stein", "d\u00fcn\u00b7ket", "mich", "noch", "ge\u00b7gen", "ih\u00b7nen", "tr\u00fc\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Vnd br\u00e4n er liechter Loh: doch/ ob sie noch so fein/", "tokens": ["Vnd", "br\u00e4n", "er", "liech\u00b7ter", "Loh", ":", "doch", "/", "ob", "sie", "noch", "so", "fein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "NN", "$.", "ADV", "$(", "KOUS", "PPER", "ADV", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Noch mu\u00df ich/ ", "tokens": ["Noch", "mu\u00df", "ich", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.16": {"text": "Zum ", "tokens": ["Zum"], "token_info": ["word"], "pos": ["APPRART"], "meter": "+", "measure": "single.up"}, "line.17": {"text": "Vnd w\u00fcnscht nicht seinen Mund sobald auf ihnen", "tokens": ["Vnd", "w\u00fcnscht", "nicht", "sei\u00b7nen", "Mund", "so\u00b7bald", "auf", "ih\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "PPOSAT", "NN", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Der Lippen Trempeldantz/ (auf denen ", "tokens": ["Der", "Lip\u00b7pen", "Trem\u00b7pel\u00b7dantz", "/", "(", "auf", "de\u00b7nen"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word"], "pos": ["ART", "NN", "NN", "$(", "$(", "APPR", "PRELS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Ein speichelweisser Tau) den Himmel selbst", "tokens": ["Ein", "spei\u00b7chel\u00b7weis\u00b7ser", "Tau", ")", "den", "Him\u00b7mel", "selbst"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Vnd w\u00e4r er \u00fcm und \u00fcm ", "tokens": ["Vnd", "w\u00e4r", "er", "\u00fcm", "und", "\u00fcm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "KON", "XY"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "So fein seit ihr/ ", "tokens": ["So", "fein", "seit", "ihr", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.22": {"text": "Noch droht das Himmelmaul mir lauter b\u00f6\u00df ", "tokens": ["Noch", "droht", "das", "Him\u00b7mel\u00b7maul", "mir", "lau\u00b7ter", "b\u00f6\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Es f\u00e4llet die ", "tokens": ["Es", "f\u00e4l\u00b7let", "die"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ART"], "meter": "-+-+", "measure": "iambic.di"}, "line.24": {"text": "Von hinnen ", "tokens": ["Von", "hin\u00b7nen"], "token_info": ["word", "word"], "pos": ["APPR", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.25": {"text": "Der Haare blanker Flachs/ ", "tokens": ["Der", "Haa\u00b7re", "blan\u00b7ker", "Flachs", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.26": {"text": "Wie ", "tokens": ["Wie"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}, "line.27": {"text": "Geschicht mir nicht ", "tokens": ["Ge\u00b7schicht", "mir", "nicht"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPER", "PTKNEG"], "meter": "-+-+", "measure": "iambic.di"}, "line.28": {"text": "Der Atstein-weisse Hals/ der H\u00e4nde Gold-", "tokens": ["Der", "At\u00b7stein\u00b7weis\u00b7se", "Hals", "/", "der", "H\u00e4n\u00b7de", "Gold"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$(", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Der Br\u00fcste gelbes Wachs die ich f\u00fcr Quitten r\u00fcme/", "tokens": ["Der", "Br\u00fcs\u00b7te", "gel\u00b7bes", "Wachs", "die", "ich", "f\u00fcr", "Quit\u00b7ten", "r\u00fc\u00b7me", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "ART", "PPER", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Bedenkt ihr meine Brunst/ ", "tokens": ["Be\u00b7denkt", "ihr", "mei\u00b7ne", "Brunst", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.31": {"text": "So acht ich mich ", "tokens": ["So", "acht", "ich", "mich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "CARD", "PPER", "PRF"], "meter": "-+-+", "measure": "iambic.di"}, "line.32": {"text": "So wird euch die ", "tokens": ["So", "wird", "euch", "die"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART"], "meter": "-+-+", "measure": "iambic.di"}}}}}