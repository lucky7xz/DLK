{"dta.poem.6485": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Ton.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn eines Bauern Hals sehr laut und widrig schreyt,", "tokens": ["Wenn", "ei\u00b7nes", "Bau\u00b7ern", "Hals", "sehr", "laut", "und", "wid\u00b7rig", "schreyt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "ADV", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wird ein Bau\u2019r dadurch gelocket und erfreut:", "tokens": ["So", "wird", "ein", "Bau'r", "da\u00b7durch", "ge\u00b7lo\u00b7cket", "und", "er\u00b7freut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PAV", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Indem ein z\u00e4rtlich Ohr dadurch, fast wie zerrissen,", "tokens": ["In\u00b7dem", "ein", "z\u00e4rt\u00b7lich", "Ohr", "da\u00b7durch", ",", "fast", "wie", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJD", "NN", "PAV", "$,", "ADV", "KOKOM", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sich wird davor entfernen m\u00fcssen.", "tokens": ["Sich", "wird", "da\u00b7vor", "ent\u00b7fer\u00b7nen", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VAFIN", "PAV", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}