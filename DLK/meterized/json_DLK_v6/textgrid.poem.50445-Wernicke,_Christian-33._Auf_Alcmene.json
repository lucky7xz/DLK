{"textgrid.poem.50445": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "33. Auf Alcmene", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wahr ist's, es machte gern' ", "tokens": ["Wahr", "ist's", ",", "es", "mach\u00b7te", "gern'"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Offt durch ein ", "tokens": ["Offt", "durch", "ein"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPR", "ART"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Doch weil der frechen ", "tokens": ["Doch", "weil", "der", "fre\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "So machet sie sich offt durch ", "tokens": ["So", "ma\u00b7chet", "sie", "sich", "offt", "durch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "APPR"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}}}}}