{"textgrid.poem.50969": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Unter Sternen", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wende dich, du kleiner Stern,", "tokens": ["Wen\u00b7de", "dich", ",", "du", "klei\u00b7ner", "Stern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Erde! wo ich lebe,", "tokens": ["Er\u00b7de", "!", "wo", "ich", "le\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df mein Aug, der Sonne fern,", "tokens": ["Da\u00df", "mein", "Aug", ",", "der", "Son\u00b7ne", "fern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sternenw\u00e4rts sich hebe!", "tokens": ["Ster\u00b7nen\u00b7w\u00e4rts", "sich", "he\u00b7be", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PRF", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Heilig ist die Sternenzeit,", "tokens": ["Hei\u00b7lig", "ist", "die", "Ster\u00b7nen\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00d6ffnet alle Gr\u00fcfte;", "tokens": ["\u00d6ff\u00b7net", "al\u00b7le", "Gr\u00fcf\u00b7te", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Strahlende Unsterblichkeit", "tokens": ["Strah\u00b7len\u00b7de", "U\u00b7nsterb\u00b7lich\u00b7keit"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Wandelt durch die L\u00fcfte.", "tokens": ["Wan\u00b7delt", "durch", "die", "L\u00fcf\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Mag die Sonne nun bislang", "tokens": ["Mag", "die", "Son\u00b7ne", "nun", "bis\u00b7lang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "ADV", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Andern Zonen scheinen,", "tokens": ["An\u00b7dern", "Zo\u00b7nen", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Hier f\u00fchl ich Zusammenhang", "tokens": ["Hier", "f\u00fchl", "ich", "Zu\u00b7sam\u00b7men\u00b7hang"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mit dem All und Einen!", "tokens": ["Mit", "dem", "All", "und", "Ei\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "ART", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Hohe Lust, im dunklen Tal,", "tokens": ["Ho\u00b7he", "Lust", ",", "im", "dunk\u00b7len", "Tal", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Selber ungesehen,", "tokens": ["Sel\u00b7ber", "un\u00b7ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Durch den majest\u00e4t'schen Saal", "tokens": ["Durch", "den", "ma\u00b7je\u00b7st\u00e4t'\u00b7schen", "Saal"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Atmend mitzugehen!", "tokens": ["At\u00b7mend", "mit\u00b7zu\u00b7ge\u00b7hen", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Schwinge dich, o gr\u00fcnes Rund,", "tokens": ["Schwin\u00b7ge", "dich", ",", "o", "gr\u00fc\u00b7nes", "Rund", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "FM", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In die Morgenr\u00f6te!", "tokens": ["In", "die", "Mor\u00b7gen\u00b7r\u00f6\u00b7te", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Scheidend r\u00fcckw\u00e4rts singt mein Mund", "tokens": ["Schei\u00b7dend", "r\u00fcck\u00b7w\u00e4rts", "singt", "mein", "Mund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jubelnde Gebete!", "tokens": ["Ju\u00b7beln\u00b7de", "Ge\u00b7be\u00b7te", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}}}}}