{"textgrid.poem.43822": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schlafe, liebes Elselein,", "tokens": ["Schla\u00b7fe", ",", "lie\u00b7bes", "El\u00b7sel\u00b7ein", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schlafe nun ein!", "tokens": ["Schla\u00b7fe", "nun", "ein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Schmetterling sucht am Blatt", "tokens": ["Schmet\u00b7ter\u00b7ling", "sucht", "am", "Blatt"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPRART", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Sich eine Lagerstatt.", "tokens": ["Sich", "ei\u00b7ne", "La\u00b7ger\u00b7statt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "K\u00e4ferchen eilt zur Ruh,", "tokens": ["K\u00e4\u00b7fer\u00b7chen", "eilt", "zur", "Ruh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Thut seine Aeuglein zu.", "tokens": ["Thut", "sei\u00b7ne", "A\u00b7e\u00b7u\u00b7glein", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Fliegen und M\u00fcckelein", "tokens": ["Flie\u00b7gen", "und", "M\u00fc\u00b7ckel\u00b7ein"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Summen in Schlaf sich ein.", "tokens": ["Sum\u00b7men", "in", "Schlaf", "sich", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PRF", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Sperling fliegt unter's Dach,", "tokens": ["Sper\u00b7ling", "fliegt", "un\u00b7ter's", "Dach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Flink in sein Schlafgemach.", "tokens": ["Flink", "in", "sein", "Schlaf\u00b7ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Kickriki l\u00e4\u00dft sein Schrei'n,", "tokens": ["Kic\u00b7kri\u00b7ki", "l\u00e4\u00dft", "sein", "Schrei'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Geht in den Stall hinein.", "tokens": ["Geht", "in", "den", "Stall", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Kuh und Kalb, Pferd und Schaf,", "tokens": ["Kuh", "und", "Kalb", ",", "Pferd", "und", "Schaf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-++-+", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Alles will Ruh und Schlaf.", "tokens": ["Al\u00b7les", "will", "Ruh", "und", "Schlaf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "NN", "KON", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.8": {"line.1": {"text": "Schlafe, liebes Elselein,", "tokens": ["Schla\u00b7fe", ",", "lie\u00b7bes", "El\u00b7sel\u00b7ein", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schlafe nun ein!", "tokens": ["Schla\u00b7fe", "nun", "ein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}