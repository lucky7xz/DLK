{"textgrid.poem.67441": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Die Luft", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbtr\u00fcber Schleier der Luft, der uns den goldenen Tag raubt,", "tokens": ["\u00bb", "tr\u00fc\u00b7ber", "Schlei\u00b7er", "der", "Luft", ",", "der", "uns", "den", "gol\u00b7de\u00b7nen", "Tag", "raubt", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "ART", "NN", "$,", "PRELS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Uns mit Seuchen und Frost, uns auch mit Launen betr\u00fcbt!\u00ab", "tokens": ["Uns", "mit", "Seu\u00b7chen", "und", "Frost", ",", "uns", "auch", "mit", "Lau\u00b7nen", "be\u00b7tr\u00fcbt", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "APPR", "NN", "KON", "NN", "$,", "PPER", "ADV", "APPR", "NN", "VVPP", "$.", "$("], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Also z\u00fcrnete ich. Da klangen liebliche T\u00f6ne,", "tokens": ["Al\u00b7so", "z\u00fcr\u00b7ne\u00b7te", "ich", ".", "Da", "klan\u00b7gen", "lieb\u00b7li\u00b7che", "T\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "ADV", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Und in entnebelter Luft fangen mir Genien zu:", "tokens": ["Und", "in", "ent\u00b7ne\u00b7bel\u00b7ter", "Luft", "fan\u00b7gen", "mir", "Ge\u00b7ni\u00b7en", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "PPER", "NN", "PTKVZ", "$."], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "\u00bbsterblicher, hast Du die Morgen-, die Abendr\u00f6the gesehen?", "tokens": ["\u00bb", "sterb\u00b7li\u00b7cher", ",", "hast", "Du", "die", "Mor\u00b7gen", ",", "die", "A\u00b7bend\u00b7r\u00f6\u00b7the", "ge\u00b7se\u00b7hen", "?"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "VAFIN", "PPER", "ART", "TRUNC", "$,", "ART", "NN", "VVPP", "$."], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Hast Du den lieblichen Ton Deiner Geliebten geh\u00f6rt?", "tokens": ["Hast", "Du", "den", "lieb\u00b7li\u00b7chen", "Ton", "Dei\u00b7ner", "Ge\u00b7lieb\u00b7ten", "ge\u00b7h\u00f6rt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Sahst Du den Regenbogen und trankst mit der Blume den Thau auf,", "tokens": ["Sahst", "Du", "den", "Re\u00b7gen\u00b7bo\u00b7gen", "und", "trankst", "mit", "der", "Blu\u00b7me", "den", "Thau", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "KON", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Der in der Rose Dir lacht, der in der Traube Dir gl\u00fcht?", "tokens": ["Der", "in", "der", "Ro\u00b7se", "Dir", "lacht", ",", "der", "in", "der", "Trau\u00b7be", "Dir", "gl\u00fcht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PPER", "VVFIN", "$,", "PRELS", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Unzufriedner, k\u00fcsse den Saum des wallenden Schleiers,", "tokens": ["Un\u00b7zu\u00b7fried\u00b7ner", ",", "k\u00fcs\u00b7se", "den", "Saum", "des", "wal\u00b7len\u00b7den", "Schlei\u00b7ers", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Durch den Aether und Licht, Athem und Speise Dir ward!\u00ab", "tokens": ["Durch", "den", "A\u00b7e\u00b7ther", "und", "Licht", ",", "A\u00b7them", "und", "Spei\u00b7se", "Dir", "ward", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "$,", "NN", "KON", "NN", "PPER", "VAFIN", "$.", "$("], "meter": "+--+--+-+-+-+-", "measure": "elegiambus"}}}}}