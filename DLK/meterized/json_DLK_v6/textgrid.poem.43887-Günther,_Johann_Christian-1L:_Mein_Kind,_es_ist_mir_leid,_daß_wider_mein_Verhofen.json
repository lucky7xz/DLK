{"textgrid.poem.43887": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Kind, es ist mir leid, da\u00df wider mein Verhofen", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Kind, es ist mir leid, da\u00df wider mein Verhofen", "tokens": ["Mein", "Kind", ",", "es", "ist", "mir", "leid", ",", "da\u00df", "wi\u00b7der", "mein", "Ver\u00b7ho\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VAFIN", "PPER", "ADJD", "$,", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein erst erzehlter Traum so pl\u00f6zlich eingetrofen,", "tokens": ["Dein", "erst", "er\u00b7zehl\u00b7ter", "Traum", "so", "pl\u00f6z\u00b7lich", "ein\u00b7ge\u00b7tro\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "ADJA", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da der Gevatterbrief, den deine Schwester kriegt,", "tokens": ["Da", "der", "Ge\u00b7vat\u00b7ter\u00b7brief", ",", "den", "dei\u00b7ne", "Schwes\u00b7ter", "kriegt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So dein- als meinen Wuntsch auf diesen Tag betriegt.", "tokens": ["So", "dein", "als", "mei\u00b7nen", "Wunt\u00b7sch", "auf", "die\u00b7sen", "Tag", "be\u00b7triegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "TRUNC", "KOKOM", "PPOSAT", "NN", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Allein bescheide dich und las das Misvergn\u00fcgen", "tokens": ["Al\u00b7lein", "be\u00b7schei\u00b7de", "dich", "und", "las", "das", "Mis\u00b7ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht die Gela\u00dfenheit in deiner Brust besiegen;", "tokens": ["Nicht", "die", "Ge\u00b7la\u00b7\u00dfen\u00b7heit", "in", "dei\u00b7ner", "Brust", "be\u00b7sie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du weist ja ohnedem, da\u00df deiner Freundin List", "tokens": ["Du", "weist", "ja", "oh\u00b7ne\u00b7dem", ",", "da\u00df", "dei\u00b7ner", "Freun\u00b7din", "List"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PAV", "$,", "KOUS", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dich, wo sie weis und kan, zu kr\u00e4ncken f\u00e4hig ist.", "tokens": ["Dich", ",", "wo", "sie", "weis", "und", "kan", ",", "zu", "kr\u00e4n\u00b7cken", "f\u00e4\u00b7hig", "ist", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PWAV", "PPER", "PTKVZ", "KON", "VMFIN", "$,", "PTKZU", "VVINF", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Hat diese gleich vorjezt dich auch wie sonst betrogen", "tokens": ["Hat", "die\u00b7se", "gleich", "vor\u00b7jezt", "dich", "auch", "wie", "sonst", "be\u00b7tro\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PDS", "ADV", "VVFIN", "PPER", "ADV", "KOKOM", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und deiner Sittsamkeit den Hochmuth vorgezogen,", "tokens": ["Und", "dei\u00b7ner", "Sitt\u00b7sam\u00b7keit", "den", "Hoch\u00b7muth", "vor\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So bleibt es dennoch wahr: Die stolze Werckmarie", "tokens": ["So", "bleibt", "es", "den\u00b7noch", "wahr", ":", "Die", "stol\u00b7ze", "Werck\u00b7ma\u00b7rie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kriegt die Gevatterschaft, und du verdienest sie.", "tokens": ["Kriegt", "die", "Ge\u00b7vat\u00b7ter\u00b7schaft", ",", "und", "du", "ver\u00b7die\u00b7nest", "sie", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "KON", "PPER", "VVFIN", "PPER", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}}}}