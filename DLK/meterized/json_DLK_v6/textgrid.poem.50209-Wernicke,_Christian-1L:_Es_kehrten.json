{"textgrid.poem.50209": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es kehrten ", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es kehrten ", "tokens": ["Es", "kehr\u00b7ten"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Und legten beyd' erm\u00fcdt die K\u00f6cher und die Pfeile", "tokens": ["Und", "leg\u00b7ten", "beyd'", "er\u00b7m\u00fcdt", "die", "K\u00f6\u00b7cher", "und", "die", "Pfei\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIS", "VVFIN", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Von ihrer Seiten ab; sie ", "tokens": ["Von", "ih\u00b7rer", "Sei\u00b7ten", "ab", ";", "sie"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$.", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Sonn' im S\u00fcden war, so dass ", "tokens": ["Der", "Sonn'", "im", "S\u00fc\u00b7den", "war", ",", "so", "dass"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "VAFIN", "$,", "ADV", "KOUS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie nun bei ", "tokens": ["Wie", "nun", "bei"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADV", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Und jeder ", "tokens": ["Und", "je\u00b7der"], "token_info": ["word", "word"], "pos": ["KON", "PIS"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "So ", "tokens": ["So"], "token_info": ["word"], "pos": ["ADV"], "meter": "-", "measure": "single.down"}}}}}