{"textgrid.poem.38970": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Alle W\u00fcnsche, alle Tr\u00e4ume", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alle W\u00fcnsche, alle Tr\u00e4ume", "tokens": ["Al\u00b7le", "W\u00fcn\u00b7sche", ",", "al\u00b7le", "Tr\u00e4u\u00b7me"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Waren herrlich nun gestillt,", "tokens": ["Wa\u00b7ren", "herr\u00b7lich", "nun", "ge\u00b7stillt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das Verlangen war erf\u00fcllt,", "tokens": ["Das", "Ver\u00b7lan\u00b7gen", "war", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Fr\u00f6hlich rauschten gr\u00fcne B\u00e4ume.", "tokens": ["Fr\u00f6h\u00b7lich", "rauschten", "gr\u00fc\u00b7ne", "B\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Aus geh ich die Spur zu finden", "tokens": ["Aus", "geh", "ich", "die", "Spur", "zu", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "VVFIN", "PPER", "ART", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Alles sagt mir von dem Gl\u00fccke,", "tokens": ["Al\u00b7les", "sagt", "mir", "von", "dem", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Jene Zeit k\u00f6mmt mir zur\u00fccke;", "tokens": ["Je\u00b7ne", "Zeit", "k\u00f6mmt", "mir", "zu\u00b7r\u00fc\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mu\u00dfte sie so schnell entschwinden?", "tokens": ["Mu\u00df\u00b7te", "sie", "so", "schnell", "ent\u00b7schwin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Ach wie war die Stunde s\u00fc\u00dfe,", "tokens": ["Ach", "wie", "war", "die", "Stun\u00b7de", "s\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Als sich unsre Blick' erkannten,", "tokens": ["Als", "sich", "uns\u00b7re", "Blick'", "er\u00b7kann\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Unsre Herzen schnell entbrannten,", "tokens": ["Uns\u00b7re", "Her\u00b7zen", "schnell", "ent\u00b7brann\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Sich begegneten die K\u00fcsse.", "tokens": ["Sich", "be\u00b7ge\u00b7gne\u00b7ten", "die", "K\u00fcs\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.13": {"text": "Jeder Fr\u00fchling sagt mir wieder,", "tokens": ["Je\u00b7der", "Fr\u00fch\u00b7ling", "sagt", "mir", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Wie ich seelig einst gewesen,", "tokens": ["Wie", "ich", "see\u00b7lig", "einst", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "ADV", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Darum kann ich nicht genesen,", "tokens": ["Da\u00b7rum", "kann", "ich", "nicht", "ge\u00b7ne\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Und die Sorge wirft mich nieder.", "tokens": ["Und", "die", "Sor\u00b7ge", "wirft", "mich", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Kommt der Herbst, bin ich vermessen,", "tokens": ["Kommt", "der", "Herbst", ",", "bin", "ich", "ver\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Kommt der Winter seh ich gl\u00e4nzen", "tokens": ["Kommt", "der", "Win\u00b7ter", "seh", "ich", "gl\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Manche Sch\u00f6nheit bei den T\u00e4nzen,", "tokens": ["Man\u00b7che", "Sch\u00f6n\u00b7heit", "bei", "den", "T\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Und die Einz'ge wird vergessen.", "tokens": ["Und", "die", "Einz'\u00b7ge", "wird", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Aber wann die Blumen sprie\u00dfen,", "tokens": ["A\u00b7ber", "wann", "die", "Blu\u00b7men", "sprie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Wann die Nachtigallen singen,", "tokens": ["Wann", "die", "Nach\u00b7ti\u00b7gal\u00b7len", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Mu\u00df sie wieder mich bezwingen,", "tokens": ["Mu\u00df", "sie", "wie\u00b7der", "mich", "be\u00b7zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Ich den schn\u00f6den Frevel b\u00fc\u00dfen.", "tokens": ["Ich", "den", "schn\u00f6\u00b7den", "Fre\u00b7vel", "b\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Flie\u00dfet, flie\u00dfet treue Thr\u00e4nen,", "tokens": ["Flie\u00b7\u00dfet", ",", "flie\u00b7\u00dfet", "treu\u00b7e", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Herz vergeh im tiefen Schmachten,", "tokens": ["Herz", "ver\u00b7geh", "im", "tie\u00b7fen", "Schmach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "M\u00f6gt ihr Augen euch umnachten,", "tokens": ["M\u00f6gt", "ihr", "Au\u00b7gen", "euch", "um\u00b7nach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Leben, l\u00f6se dich in Sehnen.", "tokens": ["Le\u00b7ben", ",", "l\u00f6\u00b7se", "dich", "in", "Seh\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}