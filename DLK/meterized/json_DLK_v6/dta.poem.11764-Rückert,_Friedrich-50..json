{"dta.poem.11764": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "50.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sie haben ihr Vertraun auf dich gesetzt, und baun", "tokens": ["Sie", "ha\u00b7ben", "ihr", "Ver\u00b7traun", "auf", "dich", "ge\u00b7setzt", ",", "und", "baun"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "APPR", "PPER", "VVPP", "$,", "KON", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf dich; so setze du auf Gott auch dein Vertraun.", "tokens": ["Auf", "dich", ";", "so", "set\u00b7ze", "du", "auf", "Gott", "auch", "dein", "Ver\u00b7traun", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$.", "ADV", "VVFIN", "PPER", "APPR", "NN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie sie vertrauenvoll auf dich schaun als Berather,", "tokens": ["Wie", "sie", "ver\u00b7trau\u00b7en\u00b7voll", "auf", "dich", "schaun", "als", "Be\u00b7ra\u00b7ther", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "APPR", "PPER", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "So schau mit doppeltem Vertraun auf deinen Vater.", "tokens": ["So", "schau", "mit", "dop\u00b7pel\u00b7tem", "Ver\u00b7traun", "auf", "dei\u00b7nen", "Va\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-