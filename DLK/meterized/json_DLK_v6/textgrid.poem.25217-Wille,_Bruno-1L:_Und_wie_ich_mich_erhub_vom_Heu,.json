{"textgrid.poem.25217": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und wie ich mich erhub vom Heu,", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und wie ich mich erhub vom Heu,", "tokens": ["Und", "wie", "ich", "mich", "er\u00b7hub", "vom", "Heu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PRF", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wie mein Blick ging staunend um,", "tokens": ["Und", "wie", "mein", "Blick", "ging", "stau\u00b7nend", "um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPOSAT", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da schlug aufs Herze mir die Reu:", "tokens": ["Da", "schlug", "aufs", "Her\u00b7ze", "mir", "die", "Reu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "VVFIN", "PPER", "ART", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O weh, du hast verschlafen", "tokens": ["O", "weh", ",", "du", "hast", "ver\u00b7schla\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PTKVZ", "$,", "PPER", "VAFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Den ganzen Sonntag schier/ wie dumm!", "tokens": ["Den", "gan\u00b7zen", "Sonn\u00b7tag", "schier", "/", "wie", "dumm", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$(", "PWAV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und wie mein Blick ging staunend um,", "tokens": ["Und", "wie", "mein", "Blick", "ging", "stau\u00b7nend", "um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPOSAT", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Stund dort mein Schatz und sah zur\u00fcck/", "tokens": ["Stund", "dort", "mein", "Schatz", "und", "sah", "zu\u00b7r\u00fcck", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "KON", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An eines Fremden Arm, wie dumm/", "tokens": ["An", "ei\u00b7nes", "Frem\u00b7den", "Arm", ",", "wie", "dumm", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,", "PWAV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Seelenschatz vom Himmel/", "tokens": ["Mein", "See\u00b7len\u00b7schatz", "vom", "Him\u00b7mel", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sein d\u00fcrstend Auge leer von Gl\u00fcck!", "tokens": ["Sein", "d\u00fcrs\u00b7tend", "Au\u00b7ge", "leer", "von", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Verd\u00fcrstend sah mein Schatz zur\u00fcck:", "tokens": ["Ver\u00b7d\u00fcrs\u00b7tend", "sah", "mein", "Schatz", "zu\u00b7r\u00fcck", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbwas schliefest, N\u00e4rrchen, auch so lang!", "tokens": ["\u00bb", "was", "schlie\u00b7fest", ",", "N\u00e4rr\u00b7chen", ",", "auch", "so", "lang", "!"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "$,", "NN", "$,", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vertr\u00e4umt ist unser Liebesgl\u00fcck,", "tokens": ["Ver\u00b7tr\u00e4umt", "ist", "un\u00b7ser", "Lie\u00b7bes\u00b7gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Sinken schon die Sonne ...", "tokens": ["Im", "Sin\u00b7ken", "schon", "die", "Son\u00b7ne", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ade! Mir ist wie dir so bang.\u00ab/", "tokens": ["A\u00b7de", "!", "Mir", "ist", "wie", "dir", "so", "bang", ".", "\u00ab", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "$.", "PPER", "VAFIN", "KOKOM", "PPER", "ADV", "ADJD", "$.", "$(", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Was schliefest, N\u00e4rrchen, auch so lang!", "tokens": ["Was", "schlie\u00b7fest", ",", "N\u00e4rr\u00b7chen", ",", "auch", "so", "lang", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "NN", "$,", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und was nun weiter? Bleib im Traum!", "tokens": ["Und", "was", "nun", "wei\u00b7ter", "?", "Bleib", "im", "Traum", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "PTKVZ", "$.", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Beliebt vielleicht ein Schlendergang,", "tokens": ["Be\u00b7liebt", "viel\u00b7leicht", "ein", "Schlen\u00b7der\u00b7gang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Recht einsam, ohne Hoffen?", "tokens": ["Recht", "ein\u00b7sam", ",", "oh\u00b7ne", "Hof\u00b7fen", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "KOUI", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Vielleicht zu Totenackers Saum?", "tokens": ["Viel\u00b7leicht", "zu", "To\u00b7te\u00b7na\u00b7ckers", "Saum", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ja, was nun weiter? Bleib im Traum!", "tokens": ["Ja", ",", "was", "nun", "wei\u00b7ter", "?", "Bleib", "im", "Traum", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PRELS", "ADV", "PTKVZ", "$.", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Welt geht ihren starren Gang,", "tokens": ["Die", "Welt", "geht", "ih\u00b7ren", "star\u00b7ren", "Gang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Z\u00e4hrenfluten lindern kaum,", "tokens": ["Und", "Z\u00e4h\u00b7ren\u00b7flu\u00b7ten", "lin\u00b7dern", "kaum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo m\u00e4dchenschwach ein Sch\u00e4tzchen", "tokens": ["Wo", "m\u00e4d\u00b7chen\u00b7schwach", "ein", "Sch\u00e4tz\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mit seinem harten Schicksal rang.", "tokens": ["Mit", "sei\u00b7nem", "har\u00b7ten", "Schick\u00b7sal", "rang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die Welt geht ihren starren Gang.", "tokens": ["Die", "Welt", "geht", "ih\u00b7ren", "star\u00b7ren", "Gang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wohin? Mein armer Kopf ist irr.", "tokens": ["Wo\u00b7hin", "?", "Mein", "ar\u00b7mer", "Kopf", "ist", "irr", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "PPOSAT", "ADJA", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mag sein, mir w\u00e4re minder bang,", "tokens": ["Mag", "sein", ",", "mir", "w\u00e4\u00b7re", "min\u00b7der", "bang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VAINF", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So ich noch k\u00f6nnte beten.", "tokens": ["So", "ich", "noch", "k\u00f6nn\u00b7te", "be\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich hab's verlernt, vom Heuduft wirr.", "tokens": ["Ich", "hab's", "ver\u00b7lernt", ",", "vom", "Heu\u00b7duft", "wirr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wohin? Mein armer Kopf ist irr.", "tokens": ["Wo\u00b7hin", "?", "Mein", "ar\u00b7mer", "Kopf", "ist", "irr", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "PPOSAT", "ADJA", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Denk' wohl, ich bette mich aufs neu", "tokens": ["Denk'", "wohl", ",", "ich", "bet\u00b7te", "mich", "aufs", "neu"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "$,", "PPER", "VVFIN", "PPER", "APPRART", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zum s\u00fc\u00dfen Duft ins Halmgewirr,", "tokens": ["Zum", "s\u00fc\u00b7\u00dfen", "Duft", "ins", "Halm\u00b7ge\u00b7wirr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und von verblichnen Blumen", "tokens": ["Und", "von", "ver\u00b7blich\u00b7nen", "Blu\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Tr\u00e4um ich zu Tode mich im Heu.", "tokens": ["Tr\u00e4um", "ich", "zu", "To\u00b7de", "mich", "im", "Heu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}