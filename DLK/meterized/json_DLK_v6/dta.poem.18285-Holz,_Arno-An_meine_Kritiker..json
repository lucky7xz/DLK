{"dta.poem.18285": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "An meine Kritiker.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "So oft ihr auch gegen mich aufgemuckt;", "tokens": ["So", "oft", "ihr", "auch", "ge\u00b7gen", "mich", "auf\u00b7ge\u00b7muckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+++-+-+", "measure": "zehnsilber"}, "line.2": {"text": "Das macht, ihr seid total entnervt:", "tokens": ["Das", "macht", ",", "ihr", "seid", "to\u00b7tal", "ent\u00b7nervt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PPER", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr donnert, eh ihr Blitze werft!", "tokens": ["Ihr", "don\u00b7nert", ",", "eh", "ihr", "Blit\u00b7ze", "werft", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}