{"textgrid.poem.54254": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Engel, lebe wohl, was kan ich anders schreiben?", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Engel, lebe wohl, was kan ich anders schreiben?", "tokens": ["Mein", "En\u00b7gel", ",", "le\u00b7be", "wohl", ",", "was", "kan", "ich", "an\u00b7ders", "schrei\u00b7ben", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "ADV", "$,", "PWS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich mu\u00df noch heute fort, eh da\u00df sich Luna zeigt.", "tokens": ["Ich", "mu\u00df", "noch", "heu\u00b7te", "fort", ",", "eh", "da\u00df", "sich", "Lu\u00b7na", "zeigt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "PTKVZ", "$,", "KOUS", "KOUS", "PRF", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein widriges Geschick sucht mich von dir zu treiben,", "tokens": ["Ein", "wid\u00b7ri\u00b7ges", "Ge\u00b7schick", "sucht", "mich", "von", "dir", "zu", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das mich, ich schw\u00f6r es dir, mehr als empfindlich beugt.", "tokens": ["Das", "mich", ",", "ich", "schw\u00f6r", "es", "dir", ",", "mehr", "als", "emp\u00b7find\u00b7lich", "beugt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "$,", "PPER", "VVFIN", "PPER", "PPER", "$,", "PIAT", "KOKOM", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Erwege, wie mir mu\u00df das Blut in Adern wallen,", "tokens": ["Er\u00b7we\u00b7ge", ",", "wie", "mir", "mu\u00df", "das", "Blut", "in", "A\u00b7dern", "wal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "VMFIN", "ART", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das Wehmuth Gram und Schmertz aus seinen Circul bringt.", "tokens": ["Das", "Weh\u00b7muth", "Gram", "und", "Schmertz", "aus", "sei\u00b7nen", "Cir\u00b7cul", "bringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mir ist in einen Huy, Muth, Hertz und Sinn entfallen,", "tokens": ["Mir", "ist", "in", "ei\u00b7nen", "Huy", ",", "Muth", ",", "Hertz", "und", "Sinn", "ent\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NE", "$,", "NN", "$,", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Weil dieser Donnerschlag durch Marck und Beine dringt.", "tokens": ["Weil", "die\u00b7ser", "Don\u00b7ner\u00b7schlag", "durch", "Marck", "und", "Bei\u00b7ne", "dringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich dencke, doch mit Schmertz, an die vergn\u00fcgten Stunden,", "tokens": ["Ich", "den\u00b7cke", ",", "doch", "mit", "Schmertz", ",", "an", "die", "ver\u00b7gn\u00fcg\u00b7ten", "Stun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "APPR", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da dein begl\u00fcckter Knecht dir noch zur Seiten sa\u00df", "tokens": ["Da", "dein", "be\u00b7gl\u00fcck\u00b7ter", "Knecht", "dir", "noch", "zur", "Sei\u00b7ten", "sa\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "PPER", "ADV", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "O! was vor S\u00fc\u00dfigkeit hab ich nicht offt empfunden,", "tokens": ["O", "!", "was", "vor", "S\u00fc\u00b7\u00dfig\u00b7keit", "hab", "ich", "nicht", "offt", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWS", "APPR", "NN", "VAFIN", "PPER", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So da\u00df ich vielmahl mich dar\u00fcber selbst verga\u00df.", "tokens": ["So", "da\u00df", "ich", "viel\u00b7mahl", "mich", "da\u00b7r\u00fc\u00b7ber", "selbst", "ver\u00b7ga\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADV", "PRF", "PAV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Durfft ich dir deine Hand von Elffenbeine dr\u00fccken,", "tokens": ["Durfft", "ich", "dir", "dei\u00b7ne", "Hand", "von", "Elf\u00b7fen\u00b7bei\u00b7ne", "dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "PPOSAT", "NN", "APPR", "NE", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "K\u00fc\u00dft ich den Purpur-Mund, wo tausend Rosen bl\u00fchn,", "tokens": ["K\u00fc\u00dft", "ich", "den", "Pur\u00b7pur\u00b7Mund", ",", "wo", "tau\u00b7send", "Ro\u00b7sen", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PWAV", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "So kont ich mich nicht satt an solcher Kost erquicken,", "tokens": ["So", "kont", "ich", "mich", "nicht", "satt", "an", "sol\u00b7cher", "Kost", "er\u00b7qui\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "PTKNEG", "ADJD", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die leider! mir nunmehr das Schicksal will entziehn.", "tokens": ["Die", "lei\u00b7der", "!", "mir", "nun\u00b7mehr", "das", "Schick\u00b7sal", "will", "ent\u00b7ziehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$.", "PPER", "ADV", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Dein englischer Verstand, dein unvergleichlich Wesen,", "tokens": ["Dein", "eng\u00b7li\u00b7scher", "Ver\u00b7stand", ",", "dein", "un\u00b7ver\u00b7gleich\u00b7lich", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Der Sitten Artigkeit, der Stellung Zierd und Pracht,", "tokens": ["Der", "Sit\u00b7ten", "Ar\u00b7tig\u00b7keit", ",", "der", "Stel\u00b7lung", "Zierd", "und", "Pracht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Die mich dein Umgang lie\u00df aus Min und Worten lesen,", "tokens": ["Die", "mich", "dein", "Um\u00b7gang", "lie\u00df", "aus", "Min", "und", "Wor\u00b7ten", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Hat mich viel tausendmahl mehr als entz\u00fcckt gemacht.", "tokens": ["Hat", "mich", "viel", "tau\u00b7send\u00b7mahl", "mehr", "als", "ent\u00b7z\u00fcckt", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "PIAT", "KOKOM", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Di\u00df alles soll ich nun, Armseeliger, vermissen,", "tokens": ["Di\u00df", "al\u00b7les", "soll", "ich", "nun", ",", "Arm\u00b7see\u00b7li\u00b7ger", ",", "ver\u00b7mis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PDS", "PIS", "VMFIN", "PPER", "ADV", "$,", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+++-+-+-", "measure": "unknown.measure.septa"}, "line.22": {"text": "Unsch\u00e4tzbarer Verlust! durch den die Helffte mir,", "tokens": ["Un\u00b7sch\u00e4tz\u00b7ba\u00b7rer", "Ver\u00b7lust", "!", "durch", "den", "die", "Helff\u00b7te", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "APPR", "ART", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Von meinen Hertzen wird, Annehmlichste, gerissen.", "tokens": ["Von", "mei\u00b7nen", "Hert\u00b7zen", "wird", ",", "An\u00b7nehm\u00b7lichs\u00b7te", ",", "ge\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "$,", "NE", "$,", "VVPP", "$."], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "Ach! stelle dir nur selbst Philanders Elend f\u00fcr.", "tokens": ["Ach", "!", "stel\u00b7le", "dir", "nur", "selbst", "Phi\u00b7lan\u00b7ders", "E\u00b7lend", "f\u00fcr", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "ADV", "ADV", "NE", "NN", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Der Abschied f\u00e4llt mir schwer, wie bitter ist das Scheiden!", "tokens": ["Der", "Ab\u00b7schied", "f\u00e4llt", "mir", "schwer", ",", "wie", "bit\u00b7ter", "ist", "das", "Schei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$,", "PWAV", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ein jeder Glocken-Schlag schl\u00e4gt mir ins Hertz hinein.", "tokens": ["Ein", "je\u00b7der", "Glo\u00b7cken\u00b7Schlag", "schl\u00e4gt", "mir", "ins", "Hertz", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Du glaubst nicht, was dabey mu\u00df meine Seele leiden;", "tokens": ["Du", "glaubst", "nicht", ",", "was", "da\u00b7bey", "mu\u00df", "mei\u00b7ne", "See\u00b7le", "lei\u00b7den", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PRELS", "PAV", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Ists m\u00f6glich kan in mir noch Geist und Athem seyn?", "tokens": ["Ists", "m\u00f6g\u00b7lich", "kan", "in", "mir", "noch", "Geist", "und", "A\u00b7them", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VMFIN", "APPR", "PPER", "ADV", "NN", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Kan das Verh\u00e4ngni\u00df wohl an mir sich grimmger r\u00e4chen?", "tokens": ["Kan", "das", "Ver\u00b7h\u00e4ng\u00b7ni\u00df", "wohl", "an", "mir", "sich", "grimm\u00b7ger", "r\u00e4\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADV", "APPR", "PPER", "PRF", "ADJD", "VVINF", "$."], "meter": "+--+-++--+-+-", "measure": "iambic.hexa.invert"}, "line.30": {"text": "Als da\u00df es mir durch dich der Seelen Nahrung nimmt.", "tokens": ["Als", "da\u00df", "es", "mir", "durch", "dich", "der", "See\u00b7len", "Nah\u00b7rung", "nimmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "PRF", "APPR", "PPER", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Mein Hertze m\u00f6chte mir in tausend St\u00fccken brechen,", "tokens": ["Mein", "Hert\u00b7ze", "m\u00f6ch\u00b7te", "mir", "in", "tau\u00b7send", "St\u00fc\u00b7cken", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "VMFIN", "PPER", "APPR", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Zu was hat leider! mich der Sternen-Muth bestimmt.", "tokens": ["Zu", "was", "hat", "lei\u00b7der", "!", "mich", "der", "Ster\u00b7nen\u00b7Muth", "be\u00b7stimmt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWS", "VAFIN", "ADV", "$.", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Doch was vergr\u00f6\u00dfr ich mir die Pein durch h\u00e4uffges Klagen?", "tokens": ["Doch", "was", "ver\u00b7gr\u00f6\u00dfr", "ich", "mir", "die", "Pein", "durch", "h\u00e4uff\u00b7ges", "Kla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PPER", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Genug da\u00df das Schicksal spricht: es mu\u00df geschieden seyn.", "tokens": ["Ge\u00b7nug", "da\u00df", "das", "Schick\u00b7sal", "spricht", ":", "es", "mu\u00df", "ge\u00b7schie\u00b7den", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "VVFIN", "$.", "PPER", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.35": {"text": "Was nicht zu \u00e4ndern steht, mu\u00df man gedultig tragen", "tokens": ["Was", "nicht", "zu", "\u00e4n\u00b7dern", "steht", ",", "mu\u00df", "man", "ge\u00b7dul\u00b7tig", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PTKNEG", "PTKZU", "VVINF", "VVFIN", "$,", "VMFIN", "PIS", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Vielleicht zeigt mir das Gl\u00fcck bald wieder Sonnen-Schein.", "tokens": ["Viel\u00b7leicht", "zeigt", "mir", "das", "Gl\u00fcck", "bald", "wie\u00b7der", "Son\u00b7nen\u00b7Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADV", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Mein Engel, gute Nacht! mu\u00df ich dich gleich verlassen,", "tokens": ["Mein", "En\u00b7gel", ",", "gu\u00b7te", "Nacht", "!", "mu\u00df", "ich", "dich", "gleich", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJA", "NN", "$.", "VMFIN", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "So reist Philander doch nur halb hinweg von dir.", "tokens": ["So", "reist", "Phi\u00b7lan\u00b7der", "doch", "nur", "halb", "hin\u00b7weg", "von", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "ADV", "ADV", "ADJD", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "F\u00fchrt das Verh\u00e4ngni\u00df mich gleich auf entfernte Strassen,", "tokens": ["F\u00fchrt", "das", "Ver\u00b7h\u00e4ng\u00b7ni\u00df", "mich", "gleich", "auf", "ent\u00b7fern\u00b7te", "Stras\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "So nehm ich doch mein Hertz nicht, wie du denckst, mit mir.", "tokens": ["So", "nehm", "ich", "doch", "mein", "Hertz", "nicht", ",", "wie", "du", "denckst", ",", "mit", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "PTKNEG", "$,", "PWAV", "PPER", "VVFIN", "$,", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Du hast es mir geraubt, du solst es auch behalten,", "tokens": ["Du", "hast", "es", "mir", "ge\u00b7raubt", ",", "du", "solst", "es", "auch", "be\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PPER", "VVPP", "$,", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Verbanne Harm und Gram, gib selbgen kein Geh\u00f6r,", "tokens": ["Ver\u00b7ban\u00b7ne", "Harm", "und", "Gram", ",", "gib", "selb\u00b7gen", "kein", "Ge\u00b7h\u00f6r", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "$,", "VVIMP", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Denn meine Liebe wird so leichte nicht erkalten,", "tokens": ["Denn", "mei\u00b7ne", "Lie\u00b7be", "wird", "so", "leich\u00b7te", "nicht", "er\u00b7kal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "ADJA", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Und wenn ich auch von dir viel tausend Meilen w\u00e4r.", "tokens": ["Und", "wenn", "ich", "auch", "von", "dir", "viel", "tau\u00b7send", "Mei\u00b7len", "w\u00e4r", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "PPER", "ADV", "CARD", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Dein reitzend Wesen wird mir stets vor Augen bleiben,", "tokens": ["Dein", "reit\u00b7zend", "We\u00b7sen", "wird", "mir", "stets", "vor", "Au\u00b7gen", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VAFIN", "PPER", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Dein Bild bleibt meiner Brust best\u00e4ndig eingepr\u00e4gt,", "tokens": ["Dein", "Bild", "bleibt", "mei\u00b7ner", "Brust", "be\u00b7st\u00e4n\u00b7dig", "ein\u00b7ge\u00b7pr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Der Zeiten Schwamm vermag es nicht daraus zu reiben,", "tokens": ["Der", "Zei\u00b7ten", "Schwamm", "ver\u00b7mag", "es", "nicht", "da\u00b7raus", "zu", "rei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "PTKNEG", "PAV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Nicht eher bis der Tod in k\u00fchlen Sand mich legt.", "tokens": ["Nicht", "e\u00b7her", "bis", "der", "Tod", "in", "k\u00fch\u00b7len", "Sand", "mich", "legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Ich bleibe dir getreu und will dich stets verehren,", "tokens": ["Ich", "blei\u00b7be", "dir", "ge\u00b7treu", "und", "will", "dich", "stets", "ver\u00b7eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "KON", "VMFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Die weil kein frembder Strahl das Deinge rauben soll.", "tokens": ["Die", "weil", "kein", "fremb\u00b7der", "Strahl", "das", "Dein\u00b7ge", "rau\u00b7ben", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOUS", "PIAT", "ADJA", "NN", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Philandes Hertz wird sich, ich schw\u00f6r es, nicht verkehren,", "tokens": ["Phi\u00b7lan\u00b7des", "Hertz", "wird", "sich", ",", "ich", "schw\u00f6r", "es", ",", "nicht", "ver\u00b7keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PRF", "$,", "PPER", "VVFIN", "PPER", "$,", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Das Post-Horn klingt bereits, mein Engel, lebe wohl!", "tokens": ["Das", "Post\u00b7Horn", "klingt", "be\u00b7reits", ",", "mein", "En\u00b7gel", ",", "le\u00b7be", "wohl", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "PPOSAT", "NN", "$,", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}