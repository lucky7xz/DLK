{"textgrid.poem.57052": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Z\u00e4zilie soll die Fenster putzen,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Z\u00e4zilie soll die Fenster putzen,", "tokens": ["Z\u00e4\u00b7zi\u00b7lie", "soll", "die", "Fens\u00b7ter", "put\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "sich selbst zum Gram, jedoch dem Haus zum Nutzen.", "tokens": ["sich", "selbst", "zum", "Gram", ",", "je\u00b7doch", "dem", "Haus", "zum", "Nut\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPRART", "NN", "$,", "ADV", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Durch meine Fenster mu\u00df man, spricht die Frau,", "tokens": ["Durch", "mei\u00b7ne", "Fens\u00b7ter", "mu\u00df", "man", ",", "spricht", "die", "Frau", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PIS", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "so durchsehn k\u00f6nnen, da\u00df man nicht genau", "tokens": ["so", "durch\u00b7sehn", "k\u00f6n\u00b7nen", ",", "da\u00df", "man", "nicht", "ge\u00b7nau"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVINF", "VMINF", "$,", "KOUS", "PIS", "PTKNEG", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "erkennen kann, ob dieser Fenster Glas", "tokens": ["er\u00b7ken\u00b7nen", "kann", ",", "ob", "die\u00b7ser", "Fens\u00b7ter", "Glas"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVINF", "VMFIN", "$,", "KOUS", "PDAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Glas oder blo\u00dfe Luft ist. Merk dir das.", "tokens": ["Glas", "o\u00b7der", "blo\u00b7\u00dfe", "Luft", "ist", ".", "Merk", "dir", "das", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "VAFIN", "$.", "VVIMP", "PPER", "PDS", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Z\u00e4zilie ringt mit allen Menschen-Waffen ...", "tokens": ["Z\u00e4\u00b7zi\u00b7lie", "ringt", "mit", "al\u00b7len", "Men\u00b7schen\u00b7Waf\u00b7fen", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PIAT", "NN", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Doch \u00c4hnlichkeit mit Luft ist nicht zu schaffen.", "tokens": ["Doch", "\u00c4hn\u00b7lich\u00b7keit", "mit", "Luft", "ist", "nicht", "zu", "schaf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "VAFIN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zuletzt ermannt sie sich mit einem Schrei \u2013", "tokens": ["Zu\u00b7letzt", "er\u00b7mannt", "sie", "sich", "mit", "ei\u00b7nem", "Schrei", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "und schl\u00e4gt die Fenster allesamt entzwei!", "tokens": ["und", "schl\u00e4gt", "die", "Fens\u00b7ter", "al\u00b7le\u00b7samt", "ent\u00b7zwei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dann s\u00e4ubert sie die Rahmen von den Resten,", "tokens": ["Dann", "s\u00e4u\u00b7bert", "sie", "die", "Rah\u00b7men", "von", "den", "Res\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "und ohne Zweifel ist es so am besten.", "tokens": ["und", "oh\u00b7ne", "Zwei\u00b7fel", "ist", "es", "so", "am", "bes\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VAFIN", "PPER", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Sogar die Dame spricht, zun\u00e4chst verdutzt:", "tokens": ["So\u00b7gar", "die", "Da\u00b7me", "spricht", ",", "zu\u00b7n\u00e4chst", "ver\u00b7dutzt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "So hat Z\u00e4zilie ja noch nie geputzt.", "tokens": ["So", "hat", "Z\u00e4\u00b7zi\u00b7lie", "ja", "noch", "nie", "ge\u00b7putzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "ADV", "ADV", "ADV", "VVPP", "$."], "meter": "-+---+-+-+", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Doch alsobald ersieht man, was geschehn,", "tokens": ["Doch", "al\u00b7so\u00b7bald", "er\u00b7sieht", "man", ",", "was", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "$,", "PRELS", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und spricht einstimmig: Diese Magd mu\u00df gehn.", "tokens": ["und", "spricht", "ein\u00b7stim\u00b7mig", ":", "Die\u00b7se", "Magd", "mu\u00df", "gehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$.", "PDAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}