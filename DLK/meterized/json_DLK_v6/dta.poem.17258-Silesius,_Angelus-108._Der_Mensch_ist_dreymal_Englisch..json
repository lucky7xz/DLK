{"dta.poem.17258": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "108.  Der Mensch ist dreymal Englisch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Der Thronf\u00fcrst ruht in GOtt; Jhn schaut der Cheru-", "tokens": ["Der", "Thron\u00b7f\u00fcrst", "ruht", "in", "Gott", ";", "Jhn", "schaut", "der", "Che\u00b7ru"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+---", "measure": "unknown.measure.tetra"}, "line.2": {"text": "bin;", "tokens": ["bin", ";"], "token_info": ["word", "punct"], "pos": ["VAFIN", "$."], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Der Seraphin zerschmeltzt f\u00fcr lauter Lieb\u2019 in Jhn.", "tokens": ["Der", "Se\u00b7ra\u00b7phin", "zer\u00b7schmeltzt", "f\u00fcr", "lau\u00b7ter", "Lieb'", "in", "Jhn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "PIAT", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich finde diese Drey in einer Seel allein:", "tokens": ["Ich", "fin\u00b7de", "die\u00b7se", "Drey", "in", "ei\u00b7ner", "Seel", "al\u00b7lein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "CARD", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So mu\u00df ein heilger Mensch ja dreyfach Englisch seyn:", "tokens": ["So", "mu\u00df", "ein", "heil\u00b7ger", "Mensch", "ja", "drey\u00b7fach", "Eng\u00b7lisch", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "ADV", "ADJD", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}