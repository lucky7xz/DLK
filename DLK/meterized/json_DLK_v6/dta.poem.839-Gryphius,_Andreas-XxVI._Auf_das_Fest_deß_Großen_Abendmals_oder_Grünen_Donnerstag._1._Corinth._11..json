{"dta.poem.839": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XxVI.  Auf das Fest de\u00df Gro\u00dfen Abendmals/  \n oder Gr\u00fcnen Donnerstag. 1. Corinth. 11.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Oh\u00f6chster Liebe pfand! O Brunquell guter gaben! ", "tokens": ["O\u00b7h\u00f6chs\u00b7ter", "Lie\u00b7be", "pfand", "!", "O", "Brun\u00b7quell", "gu\u00b7ter", "ga\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$.", "NE", "NE", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O beste S\u00fcssigkeit! O wahres Engelbrodt!", "tokens": ["O", "bes\u00b7te", "S\u00fcs\u00b7sig\u00b7keit", "!", "O", "wah\u00b7res", "En\u00b7gel\u00b7brodt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "O Edle Seelen Kost/ die in der h\u00f6chsten noth/", "tokens": ["O", "Ed\u00b7le", "See\u00b7len", "Kost", "/", "die", "in", "der", "h\u00f6chs\u00b7ten", "noth", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "NN", "$(", "ART", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Will mein verwundtes Hertz vnd siech Gewissen laben!", "tokens": ["Will", "mein", "ver\u00b7wund\u00b7tes", "Hertz", "vnd", "siech", "Ge\u00b7wis\u00b7sen", "la\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN", "KON", "VVIMP", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "O Schatz in dem ich mag recht reiche Sch\u00e4tze haben/", "tokens": ["O", "Schatz", "in", "dem", "ich", "mag", "recht", "rei\u00b7che", "Sch\u00e4t\u00b7ze", "ha\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "PRELS", "PPER", "VMFIN", "ADJD", "ADJA", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O ewig lebend Fleisch/ das meinen Leib vom Todt", "tokens": ["O", "e\u00b7wig", "le\u00b7bend", "Fleisch", "/", "das", "mei\u00b7nen", "Leib", "vom", "Todt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "ADJD", "NN", "$(", "ART", "PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "O Blutt! das mich vom Fluch/ von Blutschuld/ ach vnd", "tokens": ["O", "Blutt", "!", "das", "mich", "vom", "Fluch", "/", "von", "Blut\u00b7schuld", "/", "ach", "vnd"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "ADJD", "$.", "PRELS", "PPER", "APPRART", "NN", "$(", "APPR", "NN", "$(", "XY", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kott ", "tokens": ["Kott"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Der S\u00fcnden ledig mach\u2019t: fliht/ fliht jhr hellen-Raben!", "tokens": ["Der", "S\u00fcn\u00b7den", "le\u00b7dig", "mach't", ":", "fliht", "/", "fliht", "jhr", "hel\u00b7len\u00b7Ra\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$.", "VVFIN", "$(", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Di\u00df wehrte Himmels Aa\u00df geht nur die Adler an!", "tokens": ["Di\u00df", "wehr\u00b7te", "Him\u00b7mels", "Aa\u00df", "geht", "nur", "die", "Ad\u00b7ler", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "NN", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es nehrt den der sich selbst mit Eyver pr\u00fcfen kan.", "tokens": ["Es", "nehrt", "den", "der", "sich", "selbst", "mit", "Ey\u00b7ver", "pr\u00fc\u00b7fen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "PRELS", "PRF", "ADV", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es ist der B\u00f6sen Gifft/ der Frommen st\u00e4rck vnd wonne.", "tokens": ["Es", "ist", "der", "B\u00f6\u00b7sen", "Gifft", "/", "der", "From\u00b7men", "st\u00e4rck", "vnd", "won\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$(", "ART", "NN", "ADJD", "KON", "PWAV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Kom\u2019t die jhr jrre geht in dieser w\u00fcsten welt/", "tokens": ["Kom't", "die", "jhr", "jr\u00b7re", "geht", "in", "die\u00b7ser", "w\u00fcs\u00b7ten", "welt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "PPOSAT", "NN", "VVFIN", "APPR", "PDAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Zehrung die sich selbst f\u00fcr ewre noth auffstelt", "tokens": ["Die", "Zeh\u00b7rung", "die", "sich", "selbst", "f\u00fcr", "ew\u00b7re", "noth", "auffs\u00b7telt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "PRF", "ADV", "APPR", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Verdecket Brod vnd Wein/ wie Wolcken eine Sonne!", "tokens": ["Ver\u00b7de\u00b7cket", "Brod", "vnd", "Wein", "/", "wie", "Wol\u00b7cken", "ei\u00b7ne", "Son\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$(", "KOKOM", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}