{"textgrid.poem.51074": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da hab ich gar die Rose aufgegessen,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da hab ich gar die Rose aufgegessen,", "tokens": ["Da", "hab", "ich", "gar", "die", "Ro\u00b7se", "auf\u00b7ge\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die sie mir in die starre Hand gegeben!", "tokens": ["Die", "sie", "mir", "in", "die", "star\u00b7re", "Hand", "ge\u00b7ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df ich noch einmal w\u00fcrde Rosen essen,", "tokens": ["Da\u00df", "ich", "noch", "ein\u00b7mal", "w\u00fcr\u00b7de", "Ro\u00b7sen", "es\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VAFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "H\u00e4tt nimmer ich geglaubt in meinem Leben!", "tokens": ["H\u00e4tt", "nim\u00b7mer", "ich", "ge\u00b7glaubt", "in", "mei\u00b7nem", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich m\u00f6cht nur wissen, ob es eine rote,", "tokens": ["Ich", "m\u00f6cht", "nur", "wis\u00b7sen", ",", "ob", "es", "ei\u00b7ne", "ro\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "$,", "KOUS", "PPER", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ob eine wei\u00dfe Rose das gewesen?", "tokens": ["Ob", "ei\u00b7ne", "wei\u00b7\u00dfe", "Ro\u00b7se", "das", "ge\u00b7we\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "VAPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gib t\u00e4glich uns, o Herr! von deinem Brote", "tokens": ["Gib", "t\u00e4g\u00b7lich", "uns", ",", "o", "Herr", "!", "von", "dei\u00b7nem", "Bro\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "ADJD", "PPER", "$,", "FM", "NN", "$.", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und, wenn du willst, erl\u00f6s uns von dem B\u00f6sen!", "tokens": ["Und", ",", "wenn", "du", "willst", ",", "er\u00b7l\u00f6s", "uns", "von", "dem", "B\u00f6\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "VMFIN", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}