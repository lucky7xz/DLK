{"textgrid.poem.37675": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Die Staatskunst", "genre": "verse", "period": "N.A.", "pub_year": 1807, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hingebeuget nach Einer Seite fast alle die B\u00e4ume!", "tokens": ["Hin\u00b7ge\u00b7beu\u00b7get", "nach", "Ei\u00b7ner", "Sei\u00b7te", "fast", "al\u00b7le", "die", "B\u00e4u\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADV", "PIS", "ART", "NN", "$."], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Sage, was deutet das an? Kommt das vom Sturme der Zeit?", "tokens": ["Sa\u00b7ge", ",", "was", "deu\u00b7tet", "das", "an", "?", "Kommt", "das", "vom", "Stur\u00b7me", "der", "Zeit", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VVFIN", "PDS", "PTKVZ", "$.", "VVFIN", "ART", "APPRART", "NN", "ART", "NN", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Festere \u00e4ltere St\u00e4mme brachen im Zuge der V\u00f6lker,", "tokens": ["Fes\u00b7te\u00b7re", "\u00e4l\u00b7te\u00b7re", "St\u00e4m\u00b7me", "bra\u00b7chen", "im", "Zu\u00b7ge", "der", "V\u00f6l\u00b7ker", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Nur in dem j\u00fcngeren Stamm w\u00e4chst die Gewohnheit der Noth.", "tokens": ["Nur", "in", "dem", "j\u00fcn\u00b7ge\u00b7ren", "Stamm", "w\u00e4chst", "die", "Ge\u00b7wohn\u00b7heit", "der", "Noth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Lasse Dein Urtheil nimmermehr richten nach vielen von keinem,", "tokens": ["Las\u00b7se", "Dein", "Ur\u00b7theil", "nim\u00b7mer\u00b7mehr", "rich\u00b7ten", "nach", "vie\u00b7len", "von", "kei\u00b7nem", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADV", "VVFIN", "APPR", "PIAT", "APPR", "PIS", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Schliff sich der Spiegel doch schief, weil doch alles so schief,", "tokens": ["Schliff", "sich", "der", "Spie\u00b7gel", "doch", "schief", ",", "weil", "doch", "al\u00b7les", "so", "schief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "ADV", "VVFIN", "$,", "KOUS", "ADV", "PIS", "ADV", "ADJD", "$,"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.7": {"text": "\u00bbfeuer!\u00ab schreit er am Fenster. \u00bbFeuer?\u00ab lachen die Knaben,", "tokens": ["\u00bb", "feu\u00b7er", "!", "\u00ab", "schreit", "er", "am", "Fens\u00b7ter", ".", "\u00bb", "Feu\u00b7er", "?", "\u00ab", "la\u00b7chen", "die", "Kna\u00b7ben", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$.", "$(", "VVFIN", "PPER", "APPRART", "NN", "$.", "$(", "NN", "$.", "$(", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.8": {"text": "\u00bbm\u00e4nnchen, ei siehst du denn nicht, da\u00df dir die M\u00fctze hell brennt.\u00ab", "tokens": ["\u00bb", "m\u00e4nn\u00b7chen", ",", "ei", "siehst", "du", "denn", "nicht", ",", "da\u00df", "dir", "die", "M\u00fct\u00b7ze", "hell", "brennt", ".", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVINF", "$,", "NE", "VVFIN", "PPER", "ADV", "PTKNEG", "$,", "KOUS", "PPER", "ART", "NN", "ADJD", "VVFIN", "$.", "$("], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.9": {"text": "Freilich, die Lampe ergriff die \u00fcberhangende Kappe,", "tokens": ["Frei\u00b7lich", ",", "die", "Lam\u00b7pe", "er\u00b7griff", "die", "\u00fc\u00b7berh\u00b7an\u00b7gen\u00b7de", "Kap\u00b7pe", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Wo die Schelle versteckt, zeigt uns das Licht und die Angst,", "tokens": ["Wo", "die", "Schel\u00b7le", "ver\u00b7steckt", ",", "zeigt", "uns", "das", "Licht", "und", "die", "Angst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "$,", "VVFIN", "PPER", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.11": {"text": "Freilich so leuchtet n\u00e4chtlich die Lampe den Erdkreis erhellend,", "tokens": ["Frei\u00b7lich", "so", "leuch\u00b7tet", "n\u00e4cht\u00b7lich", "die", "Lam\u00b7pe", "den", "Erd\u00b7kreis", "er\u00b7hel\u00b7lend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.12": {"text": "Wenn sie lange genug nur die Kammer geschw\u00e4rzt,", "tokens": ["Wenn", "sie", "lan\u00b7ge", "ge\u00b7nug", "nur", "die", "Kam\u00b7mer", "ge\u00b7schw\u00e4rzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.13": {"text": "Zweimal h\u00f6rtet vergebens ihr V\u00f6lker Philippische Rede,", "tokens": ["Zwei\u00b7mal", "h\u00f6r\u00b7tet", "ver\u00b7ge\u00b7bens", "ihr", "V\u00f6l\u00b7ker", "Phil\u00b7ip\u00b7pi\u00b7sche", "Re\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "Schweige zum drittenmal still, dreifach thue was recht.", "tokens": ["Schwei\u00b7ge", "zum", "drit\u00b7ten\u00b7mal", "still", ",", "drei\u00b7fach", "thue", "was", "recht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADV", "PTKVZ", "$,", "PAV", "VVFIN", "PWS", "ADJD", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.15": {"text": "Manches Pulver verknallt zur \u00dcbung und fehlet im Kriege,", "tokens": ["Man\u00b7ches", "Pul\u00b7ver", "ver\u00b7knallt", "zur", "\u00dc\u00b7bung", "und", "feh\u00b7let", "im", "Krie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPRART", "NN", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.16": {"text": "Mancher Vorsatz verfliegt, sage, wo blieb denn das Wort,", "tokens": ["Man\u00b7cher", "Vor\u00b7satz", "ver\u00b7fliegt", ",", "sa\u00b7ge", ",", "wo", "blieb", "denn", "das", "Wort", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$,", "VVFIN", "$,", "PWAV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.17": {"text": "Sage, wo blieb denn die That, wo blieben die herrlichen Menschen,", "tokens": ["Sa\u00b7ge", ",", "wo", "blieb", "denn", "die", "That", ",", "wo", "blie\u00b7ben", "die", "herr\u00b7li\u00b7chen", "Men\u00b7schen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VVFIN", "ADV", "ART", "NN", "$,", "PWAV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.18": {"text": "Lerne des Augenblicks Werth, nur will Worte und That.", "tokens": ["Ler\u00b7ne", "des", "Au\u00b7gen\u00b7blicks", "Werth", ",", "nur", "will", "Wor\u00b7te", "und", "That", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$,", "ADV", "VMFIN", "NN", "KON", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.19": {"text": "Wahnsinnig nennt ihr den Mann, der meinte, er sei schon gek\u00f6pfet,", "tokens": ["Wahn\u00b7sin\u00b7nig", "nennt", "ihr", "den", "Mann", ",", "der", "mein\u00b7te", ",", "er", "sei", "schon", "ge\u00b7k\u00f6p\u00b7fet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "VVFIN", "$,", "PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.20": {"text": "Denn ein anderer Kopf sei ihm gegeben zur\u00fcck,", "tokens": ["Denn", "ein", "an\u00b7de\u00b7rer", "Kopf", "sei", "ihm", "ge\u00b7ge\u00b7ben", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "PPER", "VVPP", "PTKVZ", "$,"], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.21": {"text": "Werdet wie er und gebet nur auf vergangene Klugheit,", "tokens": ["Wer\u00b7det", "wie", "er", "und", "ge\u00b7bet", "nur", "auf", "ver\u00b7gan\u00b7ge\u00b7ne", "Klug\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "PPER", "KON", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "Setzet den Kopf nicht darauf, setzet den Kopf nur daran.", "tokens": ["Set\u00b7zet", "den", "Kopf", "nicht", "da\u00b7rauf", ",", "set\u00b7zet", "den", "Kopf", "nur", "da\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "PAV", "$,", "VVFIN", "ART", "NN", "ADV", "PAV", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}}}}}