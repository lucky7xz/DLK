{"dta.poem.8776": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "An die Diana des Niesen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Des J\u00e4gers Gang durch Feld und Wiesen,", "tokens": ["Des", "J\u00e4\u00b7gers", "Gang", "durch", "Feld", "und", "Wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und gern das Hochgebirg beschreitest,", "tokens": ["Und", "gern", "das", "Hoch\u00b7ge\u00b7birg", "be\u00b7schrei\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Bl\u00fcmlisalp und unsern Niesen,", "tokens": ["Die", "Bl\u00fcm\u00b7li\u00b7salp", "und", "un\u00b7sern", "Nie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Allen stets dich hold erwiesen,", "tokens": ["Und", "Al\u00b7len", "stets", "dich", "hold", "er\u00b7wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die dir, des St\u00e4dtelebens satt,", "tokens": ["Die", "dir", ",", "des", "St\u00e4d\u00b7te\u00b7le\u00b7bens", "satt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auf wald'ger Berge R\u00fccken huldigen:", "tokens": ["Auf", "wald'\u00b7ger", "Ber\u00b7ge", "R\u00fc\u00b7cken", "hul\u00b7di\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.7": {"text": "Was z\u00fcrnst du deinen ungeduldigen", "tokens": ["Was", "z\u00fcrnst", "du", "dei\u00b7nen", "un\u00b7ge\u00b7dul\u00b7di\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Verehrern auf der M\u00fcllimatt?", "tokens": ["Ver\u00b7eh\u00b7rern", "auf", "der", "M\u00fcl\u00b7li\u00b7matt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Auf da\u00df uns froh dein Auge nicke,", "tokens": ["Auf", "da\u00df", "uns", "froh", "dein", "Au\u00b7ge", "ni\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dein heil'ger Grimm uns endlich schone,", "tokens": ["Dein", "heil'\u00b7ger", "Grimm", "uns", "end\u00b7lich", "scho\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NE", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie gerne lenkten wir die Blicke", "tokens": ["Wie", "ger\u00b7ne", "lenk\u00b7ten", "wir", "die", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hinauf zu deinem h\u00f6chsten Throne,", "tokens": ["Hin\u00b7auf", "zu", "dei\u00b7nem", "h\u00f6chs\u00b7ten", "Thro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Zu jener keuschen Gl\u00e4tscherzone,", "tokens": ["Zu", "je\u00b7ner", "keu\u00b7schen", "Gl\u00e4t\u00b7scher\u00b7zo\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die dir den Namen hat geraubt;", "tokens": ["Die", "dir", "den", "Na\u00b7men", "hat", "ge\u00b7raubt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch Nebel ach! sich ewig h\u00e4ufende,", "tokens": ["Doch", "Ne\u00b7bel", "ach", "!", "sich", "e\u00b7wig", "h\u00e4u\u00b7fen\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ITJ", "$.", "PRF", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.8": {"text": "Von allen Seiten niedertr\u00e4ufende,", "tokens": ["Von", "al\u00b7len", "Sei\u00b7ten", "nie\u00b7der\u00b7tr\u00e4u\u00b7fen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJA", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.9": {"text": "Umwehn der Jungfrau Strahlenhaupt.", "tokens": ["Um\u00b7wehn", "der", "Jung\u00b7frau", "Strah\u00b7len\u00b7haupt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wir ziehn dem Regengu\u00df entgegen,", "tokens": ["Wir", "ziehn", "dem", "Re\u00b7gen\u00b7gu\u00df", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und weih'n dir manchen Tag und Morgen;", "tokens": ["Und", "weih'n", "dir", "man\u00b7chen", "Tag", "und", "Mor\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch keine Schnepfe will sich regen,", "tokens": ["Doch", "kei\u00b7ne", "Schnep\u00b7fe", "will", "sich", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VMFIN", "PRF", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und alle Hasen sind verborgen:", "tokens": ["Und", "al\u00b7le", "Ha\u00b7sen", "sind", "ver\u00b7bor\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So kehren wir denn stets in Sorgen", "tokens": ["So", "keh\u00b7ren", "wir", "denn", "stets", "in", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Von mancher eitlen Fahrt zur\u00fcck,", "tokens": ["Von", "man\u00b7cher", "eit\u00b7len", "Fahrt", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die M\u00fch' und Schwei\u00df genug uns kostete,", "tokens": ["Die", "M\u00fch'", "und", "Schwei\u00df", "ge\u00b7nug", "uns", "kos\u00b7te\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und uns're Flinte, die verrostete,", "tokens": ["Und", "un\u00b7s'\u00b7re", "Flin\u00b7te", ",", "die", "ver\u00b7ros\u00b7te\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Ersehnt umsonst ihr altes Gl\u00fcck.", "tokens": ["Er\u00b7sehnt", "um\u00b7sonst", "ihr", "al\u00b7tes", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Zwar l\u00e4\u00dft sich Manches in den Lauben", "tokens": ["Zwar", "l\u00e4\u00dft", "sich", "Man\u00b7ches", "in", "den", "Lau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "PIS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der sch\u00f6nen M\u00fcllimatt erwerben:", "tokens": ["Der", "sch\u00f6\u00b7nen", "M\u00fcl\u00b7li\u00b7matt", "er\u00b7wer\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bey holden Frau'n, beym Saft der Trauben,", "tokens": ["Bey", "hol\u00b7den", "Frau'n", ",", "beym", "Saft", "der", "Trau\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Beym Duft so vieler Blumenscherben,", "tokens": ["Beym", "Duft", "so", "vie\u00b7ler", "Blu\u00b7men\u00b7scher\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Hier lie\u00dfe leben sich's und sterben;", "tokens": ["Hier", "lie\u00b7\u00dfe", "le\u00b7ben", "sich's", "und", "ster\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VVFIN", "PIS", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Doch, G\u00f6ttin, sieh, zu dir nur schau'n", "tokens": ["Doch", ",", "G\u00f6t\u00b7tin", ",", "sieh", ",", "zu", "dir", "nur", "schau'n"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "NN", "$,", "VVFIN", "$,", "APPR", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wir hoffend auf, zu deinen luftigen", "tokens": ["Wir", "hof\u00b7fend", "auf", ",", "zu", "dei\u00b7nen", "luf\u00b7ti\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADJD", "PTKVZ", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und wilden H\u00f6hn von diesen duftigen", "tokens": ["Und", "wil\u00b7den", "H\u00f6hn", "von", "die\u00b7sen", "duf\u00b7ti\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "APPR", "PDAT", "ADJA"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Gew\u00e4chsen, diesen sch\u00f6nen Frau'n!", "tokens": ["Ge\u00b7w\u00e4ch\u00b7sen", ",", "die\u00b7sen", "sch\u00f6\u00b7nen", "Frau'n", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "La\u00df dich von unserm Flehn erweichen,", "tokens": ["La\u00df", "dich", "von", "un\u00b7serm", "Flehn", "er\u00b7wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und sey mit uns in diesen Tagen:", "tokens": ["Und", "sey", "mit", "uns", "in", "die\u00b7sen", "Ta\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PPER", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das H\u00f6chste wollen wir erreichen,", "tokens": ["Das", "H\u00f6chs\u00b7te", "wol\u00b7len", "wir", "er\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die pfeilgeschwinde Gemse jagen;", "tokens": ["Die", "pfeil\u00b7ge\u00b7schwin\u00b7de", "Gem\u00b7se", "ja\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Es wird uns kein Gewehr versagen,", "tokens": ["Es", "wird", "uns", "kein", "Ge\u00b7wehr", "ver\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn du uns sch\u00fctzen willst, o du!", "tokens": ["Wenn", "du", "uns", "sch\u00fct\u00b7zen", "willst", ",", "o", "du", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "VMFIN", "$,", "FM", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sey gn\u00e4dig unserer Verwegenheit,", "tokens": ["Sey", "gn\u00e4\u00b7dig", "un\u00b7se\u00b7rer", "Ver\u00b7we\u00b7gen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ersp\u00e4he selbst uns die Gelegenheit,", "tokens": ["Er\u00b7sp\u00e4\u00b7he", "selbst", "uns", "die", "Ge\u00b7le\u00b7gen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und jag' uns alle Gemsen zu!", "tokens": ["Und", "jag'", "uns", "al\u00b7le", "Gem\u00b7sen", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und wenn du uns von Schmach mit diesen", "tokens": ["Und", "wenn", "du", "uns", "von", "Schmach", "mit", "die\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PRF", "APPR", "NN", "APPR", "PDAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Geschenken deiner Gunst gerettet,", "tokens": ["Ge\u00b7schen\u00b7ken", "dei\u00b7ner", "Gunst", "ge\u00b7ret\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So m\u00f6ge dir am Rand des Niesen,", "tokens": ["So", "m\u00f6\u00b7ge", "dir", "am", "Rand", "des", "Nie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf Alpenrosen hingebettet,", "tokens": ["Auf", "Al\u00b7pen\u00b7ro\u00b7sen", "hin\u00b7ge\u00b7bet\u00b7tet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Erscheinen was dich ewig kettet:", "tokens": ["Er\u00b7schei\u00b7nen", "was", "dich", "e\u00b7wig", "ket\u00b7tet", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Auf da\u00df du senkst den Wagenthron,", "tokens": ["Auf", "da\u00df", "du", "senkst", "den", "Wa\u00b7gen\u00b7thron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Erscheine dir ein hingesunkener,", "tokens": ["Er\u00b7schei\u00b7ne", "dir", "ein", "hin\u00b7ge\u00b7sun\u00b7ke\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.8": {"text": "Von Lieb' und Wein und Schlummer trunkener,", "tokens": ["Von", "Lieb'", "und", "Wein", "und", "Schlum\u00b7mer", "trun\u00b7ke\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.9": {"text": "Ein schnarchender Endymion!", "tokens": ["Ein", "schnar\u00b7chen\u00b7der", "En\u00b7dy\u00b7mi\u00b7on", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}