{"textgrid.poem.62036": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[hoch auf der Berge Gipfel]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hoch auf der Berge Gipfel", "tokens": ["Hoch", "auf", "der", "Ber\u00b7ge", "Gip\u00b7fel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vergess' ich die ganze Welt,", "tokens": ["Ver\u00b7ge\u00b7ss'", "ich", "die", "gan\u00b7ze", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Selbstsucht breite Wipfel,", "tokens": ["Der", "Selbst\u00b7sucht", "brei\u00b7te", "Wip\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Bosheit und das Geld.", "tokens": ["Die", "Bos\u00b7heit", "und", "das", "Geld", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "In kleiner mosiger H\u00fctte,", "tokens": ["In", "klei\u00b7ner", "mo\u00b7si\u00b7ger", "H\u00fct\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da leb' ich so wohlgemut", "tokens": ["Da", "leb'", "ich", "so", "wohl\u00b7ge\u00b7mut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Voll Gottesfurcht im Herzen,", "tokens": ["Voll", "Got\u00b7tes\u00b7furcht", "im", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Herzen froh und gut.", "tokens": ["Im", "Her\u00b7zen", "froh", "und", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Morgen h\u00f6rt mich beten,", "tokens": ["Der", "Mor\u00b7gen", "h\u00f6rt", "mich", "be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Abend den Gottesdank,", "tokens": ["Der", "A\u00b7bend", "den", "Got\u00b7tes\u00b7dank", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es ernten diejenigen, die s\u00e4eten,", "tokens": ["Es", "ern\u00b7ten", "die\u00b7je\u00b7ni\u00b7gen", ",", "die", "s\u00e4e\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ihr ganzes Leben lang.", "tokens": ["Ihr", "gan\u00b7zes", "Le\u00b7ben", "lang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}