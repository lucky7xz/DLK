{"textgrid.poem.39050": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hier war mein Herz schon l\u00e4ngst,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier war mein Herz schon l\u00e4ngst,", "tokens": ["Hier", "war", "mein", "Herz", "schon", "l\u00e4ngst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "In deiner Heimath, o lieblichster Correggio,", "tokens": ["In", "dei\u00b7ner", "Hei\u00b7math", ",", "o", "lieb\u00b7lichs\u00b7ter", "Cor\u00b7reg\u00b7gio", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "FM", "FM", "FM", "$,"], "meter": "-+-+--+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Jetzt betret' ich die B\u00fchne deiner Kunst, Holdseliger,", "tokens": ["Jetzt", "be\u00b7tret'", "ich", "die", "B\u00fch\u00b7ne", "dei\u00b7ner", "Kunst", ",", "Hold\u00b7se\u00b7li\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PPOSAT", "NN", "$,", "NN", "$,"], "meter": "--+--+-+--+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Du, von allen Musen und Grazien,", "tokens": ["Du", ",", "von", "al\u00b7len", "Mu\u00b7sen", "und", "Gra\u00b7zi\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "PIAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Von den G\u00f6ttern geliebter froher, herrlicher Allegri!", "tokens": ["Von", "den", "G\u00f6t\u00b7tern", "ge\u00b7lieb\u00b7ter", "fro\u00b7her", ",", "herr\u00b7li\u00b7cher", "Al\u00b7le\u00b7gri", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "--+--+-+-+--+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Schon seit Jahren n\u00e4hrt mich deiner Begeistrung Wein,", "tokens": ["Schon", "seit", "Jah\u00b7ren", "n\u00e4hrt", "mich", "dei\u00b7ner", "Be\u00b7geis\u00b7trung", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Wie oft schaut' ich in Tr\u00e4umen dein edles Schaffen,", "tokens": ["Wie", "oft", "schaut'", "ich", "in", "Tr\u00e4u\u00b7men", "dein", "ed\u00b7les", "Schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "APPR", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+---+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sprach mit dir, vernahm den Ton deiner Stimme,", "tokens": ["Sprach", "mit", "dir", ",", "ver\u00b7nahm", "den", "Ton", "dei\u00b7ner", "Stim\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$,", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Ger\u00fchrt von deiner Freundlichkeit erwacht' ich.", "tokens": ["Ge\u00b7r\u00fchrt", "von", "dei\u00b7ner", "Freund\u00b7lich\u00b7keit", "er\u00b7wacht'", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Hier nun, wo dein hoher Geist", "tokens": ["Hier", "nun", ",", "wo", "dein", "ho\u00b7her", "Geist"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "PWAV", "PPOSAT", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Zwei Tempel mit Pracht und tiefen Sinn geschm\u00fcckt,", "tokens": ["Zwei", "Tem\u00b7pel", "mit", "Pracht", "und", "tie\u00b7fen", "Sinn", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "NN", "KON", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Fehlt mir fast in br\u00fcnstiger Liebe", "tokens": ["Fehlt", "mir", "fast", "in", "br\u00fcns\u00b7ti\u00b7ger", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Die Kraft und Ruhe, deine Rede ganz zu vernehmen.", "tokens": ["Die", "Kraft", "und", "Ru\u00b7he", ",", "dei\u00b7ne", "Re\u00b7de", "ganz", "zu", "ver\u00b7neh\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,", "PPOSAT", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Welcher Genius hat dir alle Sch\u00e4tze entriegelt?", "tokens": ["Wel\u00b7cher", "Ge\u00b7nius", "hat", "dir", "al\u00b7le", "Sch\u00e4t\u00b7ze", "ent\u00b7rie\u00b7gelt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "VAFIN", "PPER", "PIAT", "NN", "VVPP", "$."], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Alle Gebilde der Welt traten dir entgegen,", "tokens": ["Al\u00b7le", "Ge\u00b7bil\u00b7de", "der", "Welt", "tra\u00b7ten", "dir", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+--+--++-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Und gaben sich dir zu eigen, und freundlich", "tokens": ["Und", "ga\u00b7ben", "sich", "dir", "zu", "ei\u00b7gen", ",", "und", "freund\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "PPER", "PTKA", "ADJD", "$,", "KON", "ADJD"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Hast du mit ihnen die frohe Haushaltung getrieben.", "tokens": ["Hast", "du", "mit", "ih\u00b7nen", "die", "fro\u00b7he", "Haus\u00b7hal\u00b7tung", "ge\u00b7trie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+-++--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Das war ein muntres Get\u00fcmmel,", "tokens": ["Das", "war", "ein", "mun\u00b7tres", "Ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Als die lachenden Engelchen dir die Farben reichten,", "tokens": ["Als", "die", "la\u00b7chen\u00b7den", "En\u00b7gel\u00b7chen", "dir", "die", "Far\u00b7ben", "reich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Hohe Geister dir als Modell in ihrem Adel standen,", "tokens": ["Ho\u00b7he", "Geis\u00b7ter", "dir", "als", "Mo\u00b7dell", "in", "ih\u00b7rem", "A\u00b7del", "stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "KOUS", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Und Musik des Himmels dazu mit Macht erklang.", "tokens": ["Und", "Mu\u00b7sik", "des", "Him\u00b7mels", "da\u00b7zu", "mit", "Macht", "er\u00b7klang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "PAV", "APPR", "NN", "VVFIN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.6": {"line.1": {"text": "Sage doch Keiner, er habe Italien gesehn,", "tokens": ["Sa\u00b7ge", "doch", "Kei\u00b7ner", ",", "er", "ha\u00b7be", "I\u00b7ta\u00b7li\u00b7en", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PIS", "$,", "PPER", "VAFIN", "NE", "VVPP", "$,"], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}, "line.2": {"text": "R\u00fchme sich Keiner, ihm sei das H\u00f6chste vertraut,", "tokens": ["R\u00fch\u00b7me", "sich", "Kei\u00b7ner", ",", "ihm", "sei", "das", "H\u00f6chs\u00b7te", "ver\u00b7traut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PIS", "$,", "PPER", "VAFIN", "ART", "ADJA", "ADJD", "$,"], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "W\u00e4hne doch Niemand, das Geheimni\u00df der Kunst geahndet zu haben,", "tokens": ["W\u00e4h\u00b7ne", "doch", "Nie\u00b7mand", ",", "das", "Ge\u00b7heim\u00b7ni\u00df", "der", "Kunst", "ge\u00b7ahn\u00b7det", "zu", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIS", "$,", "ART", "NN", "ART", "NN", "VVPP", "PTKZU", "VAINF", "$,"], "meter": "+--+-+-+--+-+--+-", "measure": "iambic.septa.invert"}, "line.4": {"text": "Der dich nicht, Parma, und deinen Dom besucht.", "tokens": ["Der", "dich", "nicht", ",", "Par\u00b7ma", ",", "und", "dei\u00b7nen", "Dom", "be\u00b7sucht", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "$,", "NE", "$,", "KON", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}