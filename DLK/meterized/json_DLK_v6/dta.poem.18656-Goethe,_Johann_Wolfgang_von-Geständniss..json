{"dta.poem.18656": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Gest\u00e4ndniss.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Was ist schwer zu verbergen? Das Feuer!", "tokens": ["Was", "ist", "schwer", "zu", "ver\u00b7ber\u00b7gen", "?", "Das", "Feu\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADJD", "PTKZU", "VVINF", "$.", "ART", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Denn bey Tage verr\u00e4th\u2019s der Rauch,", "tokens": ["Denn", "bey", "Ta\u00b7ge", "ver\u00b7r\u00e4th's", "der", "Rauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Bey Nacht die Flamme, das Ungeheuer.", "tokens": ["Bey", "Nacht", "die", "Flam\u00b7me", ",", "das", "Un\u00b7ge\u00b7heu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ferner ist schwer zu verbergen auch", "tokens": ["Fer\u00b7ner", "ist", "schwer", "zu", "ver\u00b7ber\u00b7gen", "auch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADJD", "PTKZU", "VVINF", "ADV"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Die Liebe, noch so stille gehegt,", "tokens": ["Die", "Lie\u00b7be", ",", "noch", "so", "stil\u00b7le", "ge\u00b7hegt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADV", "ADJA", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Sie doch gar leicht aus den Augen schl\u00e4gt.", "tokens": ["Sie", "doch", "gar", "leicht", "aus", "den", "Au\u00b7gen", "schl\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Am schwersten zu bergen ist ein Gedicht,", "tokens": ["Am", "schwers\u00b7ten", "zu", "ber\u00b7gen", "ist", "ein", "Ge\u00b7dicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "PTKZU", "VVINF", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.8": {"text": "Man stellt es untern Scheffel nicht.", "tokens": ["Man", "stellt", "es", "un\u00b7tern", "Schef\u00b7fel", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Hat es der Dichter frisch gesungen,", "tokens": ["Hat", "es", "der", "Dich\u00b7ter", "frisch", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "So ist er ganz davon durchdrungen,", "tokens": ["So", "ist", "er", "ganz", "da\u00b7von", "durch\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Hat er es zierlich nett geschrieben,", "tokens": ["Hat", "er", "es", "zier\u00b7lich", "nett", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADJD", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Will er die ganze Welt soll\u2019s lieben.", "tokens": ["Will", "er", "die", "gan\u00b7ze", "Welt", "soll's", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Er liest es jedem froh und laut,", "tokens": ["Er", "liest", "es", "je\u00b7dem", "froh", "und", "laut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Ob es uns qu\u00e4lt, ob es erbaut.", "tokens": ["Ob", "es", "uns", "qu\u00e4lt", ",", "ob", "es", "er\u00b7baut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "KOUS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}