{"textgrid.poem.67367": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Der Krieger und die Pelzh\u00e4ndler", "genre": "verse", "period": "N.A.", "pub_year": 1789, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Um die Besitzungen der Briten an", "tokens": ["Um", "die", "Be\u00b7sit\u00b7zun\u00b7gen", "der", "Bri\u00b7ten", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "ART", "NN", "APPR"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.2": {"text": "Der Hudsonsbai zu st\u00f6ren, kam Peluse", "tokens": ["Der", "Hud\u00b7sons\u00b7bai", "zu", "st\u00f6\u00b7ren", ",", "kam", "Pe\u00b7lu\u00b7se"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,", "VVFIN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und st\u00f6rte sie. Die Briten flohn ins Land", "tokens": ["Und", "st\u00f6r\u00b7te", "sie", ".", "Die", "Bri\u00b7ten", "flohn", "ins", "Land"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$.", "ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und litten Mangel am Nothwendigsten.", "tokens": ["Und", "lit\u00b7ten", "Man\u00b7gel", "am", "Noth\u00b7wen\u00b7digs\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "\u00bbdamit sie,\u00ab sprach Peluse, \u00bbnicht verhungern,", "tokens": ["\u00bb", "da\u00b7mit", "sie", ",", "\u00ab", "sprach", "Pe\u00b7lu\u00b7se", ",", "\u00bb", "nicht", "ver\u00b7hun\u00b7gern", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "$,", "$(", "VVFIN", "NN", "$,", "$(", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die frierenden Pelzh\u00e4ndler, la\u00dft uns noch", "tokens": ["Die", "frie\u00b7ren\u00b7den", "Pelz\u00b7h\u00e4nd\u00b7ler", ",", "la\u00dft", "uns", "noch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "VVIMP", "PPER", "ADV"], "meter": "-+--++-+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Mit Lebensmitteln ihre H\u00e4user f\u00fcllen!\u00ab", "tokens": ["Mit", "Le\u00b7bens\u00b7mit\u00b7teln", "ih\u00b7re", "H\u00e4u\u00b7ser", "f\u00fcl\u00b7len", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Er that's und segelte davon.", "tokens": ["Er", "that's", "und", "se\u00b7gel\u00b7te", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}