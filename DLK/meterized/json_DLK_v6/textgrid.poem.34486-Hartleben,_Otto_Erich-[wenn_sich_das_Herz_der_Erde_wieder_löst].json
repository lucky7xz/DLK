{"textgrid.poem.34486": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "[wenn sich das Herz der Erde wieder l\u00f6st]", "genre": "verse", "period": "N.A.", "pub_year": 1888, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn sich das Herz der Erde wieder l\u00f6st,", "tokens": ["Wenn", "sich", "das", "Herz", "der", "Er\u00b7de", "wie\u00b7der", "l\u00f6st", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und wieder weich die Fluth der Winde gleitet \u2013", "tokens": ["und", "wie\u00b7der", "weich", "die", "Fluth", "der", "Win\u00b7de", "glei\u00b7tet", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "aufthaut in mir ein klagendes Erinnern.", "tokens": ["auft\u00b7haut", "in", "mir", "ein", "kla\u00b7gen\u00b7des", "E\u00b7rin\u00b7nern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Auch mir hat einmal sich das Herz gel\u00f6st,", "tokens": ["Auch", "mir", "hat", "ein\u00b7mal", "sich", "das", "Herz", "ge\u00b7l\u00f6st", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "ADV", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "einmal auch mir die Seele sich geweitet \u2013", "tokens": ["ein\u00b7mal", "auch", "mir", "die", "See\u00b7le", "sich", "ge\u00b7wei\u00b7tet", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ART", "NN", "PRF", "VVPP", "$("], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "nun ward auch das ein Schmerz, geheim im Innern.", "tokens": ["nun", "ward", "auch", "das", "ein", "Schmerz", ",", "ge\u00b7heim", "im", "In\u00b7nern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "ART", "NN", "$,", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}