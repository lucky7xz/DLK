{"dta.poem.3182": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "33.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Sterne m\u00f6gen dir aus Wintern\u00e4chten blinken,", "tokens": ["Die", "Ster\u00b7ne", "m\u00f6\u00b7gen", "dir", "aus", "Win\u00b7ter\u00b7n\u00e4ch\u00b7ten", "blin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Blumen einen Gru\u00df von Sommerh\u00fcgeln winken.", "tokens": ["Und", "Blu\u00b7men", "ei\u00b7nen", "Gru\u00df", "von", "Som\u00b7mer\u00b7h\u00fc\u00b7geln", "win\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So bleibt dir liebend nah von unten und von oben,", "tokens": ["So", "bleibt", "dir", "lie\u00b7bend", "nah", "von", "un\u00b7ten", "und", "von", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADJD", "APPR", "ADV", "KON", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was dir der Tod in Erd' und Himmel aufgehoben.", "tokens": ["Was", "dir", "der", "Tod", "in", "Erd'", "und", "Him\u00b7mel", "auf\u00b7ge\u00b7ho\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch wenn ein Lebender den Gru\u00df mir schuldig bleibt;", "tokens": ["Doch", "wenn", "ein", "Le\u00b7ben\u00b7der", "den", "Gru\u00df", "mir", "schul\u00b7dig", "bleibt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ART", "NN", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sch\u00e4mt er sich nicht vor dem, was Blum' und Strom mir schreibt?", "tokens": ["Sch\u00e4mt", "er", "sich", "nicht", "vor", "dem", ",", "was", "Blum'", "und", "Strom", "mir", "schreibt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKNEG", "APPR", "ART", "$,", "PWS", "NN", "KON", "NN", "PPER", "VVFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}}}}