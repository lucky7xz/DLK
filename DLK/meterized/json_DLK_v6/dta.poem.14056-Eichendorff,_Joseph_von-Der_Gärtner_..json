{"dta.poem.14056": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der G\u00e4rtner .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wohin ich geh' und schaue,", "tokens": ["Wo\u00b7hin", "ich", "geh'", "und", "schau\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Feld und Wald und Thal", "tokens": ["In", "Feld", "und", "Wald", "und", "Thal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vom Berg' hinab in die Aue:", "tokens": ["Vom", "Ber\u00b7g'", "hin\u00b7ab", "in", "die", "Au\u00b7e", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Viel sch\u00f6ne, hohe Fraue,", "tokens": ["Viel", "sch\u00f6\u00b7ne", ",", "ho\u00b7he", "Frau\u00b7e", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Gr\u00fc\u00df' ich Dich tausendmal.", "tokens": ["Gr\u00fc\u00df'", "ich", "Dich", "tau\u00b7send\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PRF", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}