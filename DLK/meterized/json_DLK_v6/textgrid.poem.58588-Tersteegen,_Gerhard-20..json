{"textgrid.poem.58588": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "20.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer seine Augen schlie\u00dft, nichts Eiteles zu sehen,", "tokens": ["Wer", "sei\u00b7ne", "Au\u00b7gen", "schlie\u00dft", ",", "nichts", "Ei\u00b7te\u00b7les", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$,", "PIS", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von allem los und blo\u00df in Gott sich kehret ein,", "tokens": ["Von", "al\u00b7lem", "los", "und", "blo\u00df", "in", "Gott", "sich", "keh\u00b7ret", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PTKVZ", "KON", "ADV", "APPR", "NN", "PRF", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kann wie ein Adler stets im Geist erhaben stehen", "tokens": ["Kann", "wie", "ein", "Ad\u00b7ler", "stets", "im", "Geist", "er\u00b7ha\u00b7ben", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "KOKOM", "ART", "NN", "ADV", "APPRART", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und bleiben unber\u00fchrt von Unruh, Furcht und Pein;", "tokens": ["Und", "blei\u00b7ben", "un\u00b7be\u00b7r\u00fchrt", "von", "Un\u00b7ruh", ",", "Furcht", "und", "Pein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "In dieser hohen Burg, weit \u00fcber alle Sinnen,", "tokens": ["In", "die\u00b7ser", "ho\u00b7hen", "Burg", ",", "weit", "\u00fc\u00b7ber", "al\u00b7le", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Im Felsen Jesus er nur seine Wohnung baut,", "tokens": ["Im", "Fel\u00b7sen", "Je\u00b7sus", "er", "nur", "sei\u00b7ne", "Woh\u00b7nung", "baut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Er findet Lebensbrot und Wasser g'nug darinnen,", "tokens": ["Er", "fin\u00b7det", "Le\u00b7bens\u00b7brot", "und", "Was\u00b7ser", "g'\u00b7nug", "da\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Sein Aug' den K\u00f6nig da in seiner Sch\u00f6ne schaut.", "tokens": ["Sein", "Aug'", "den", "K\u00f6\u00b7nig", "da", "in", "sei\u00b7ner", "Sch\u00f6\u00b7ne", "schaut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}