{"textgrid.poem.66143": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "An dieselbe", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn Schwermuth schrecklich \u00fcber mir", "tokens": ["Wenn", "Schwer\u00b7muth", "schreck\u00b7lich", "\u00fc\u00b7ber", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "ADJD", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Zackenfl\u00fcgeln h\u00e4ngt.", "tokens": ["Mit", "Za\u00b7cken\u00b7fl\u00fc\u00b7geln", "h\u00e4ngt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und fast das Bild von dir", "tokens": ["Und", "fast", "das", "Bild", "von", "dir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Aus meinem Herzen dr\u00e4ngt;", "tokens": ["Aus", "mei\u00b7nem", "Her\u00b7zen", "dr\u00e4ngt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So schlie\u00df' ich mich in meine Kerkergruft", "tokens": ["So", "schlie\u00df'", "ich", "mich", "in", "mei\u00b7ne", "Ker\u00b7ker\u00b7gruft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und mache mir durch Thr\u00e4nen Luft.", "tokens": ["Und", "ma\u00b7che", "mir", "durch", "Thr\u00e4\u00b7nen", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}