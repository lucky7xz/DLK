{"textgrid.poem.60818": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Theater und Dichtkunst", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ehmals wollt ich in Hast ausmisten den Stall des Augeias;", "tokens": ["Eh\u00b7mals", "wollt", "ich", "in", "Hast", "aus\u00b7mis\u00b7ten", "den", "Stall", "des", "Au\u00b7gei\u00b7as", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "NE", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Aber es trat Hermes, w\u00e4hrend ich keuchte, zu mir:", "tokens": ["A\u00b7ber", "es", "trat", "Her\u00b7mes", ",", "w\u00e4h\u00b7rend", "ich", "keuch\u00b7te", ",", "zu", "mir", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "NE", "$,", "KOUS", "PPER", "VVFIN", "$,", "APPR", "PPER", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Nimm hier, sagte der Gott, die unsterblichen Saiten des Orpheus;", "tokens": ["Nimm", "hier", ",", "sag\u00b7te", "der", "Gott", ",", "die", "uns\u00b7terb\u00b7li\u00b7chen", "Sai\u00b7ten", "des", "Or\u00b7pheus", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "ART", "NE", "$."], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Jedes Bem\u00fchns unwert ist der verpestete Stall.", "tokens": ["Je\u00b7des", "Be\u00b7m\u00fchns", "un\u00b7wert", "ist", "der", "ver\u00b7pes\u00b7te\u00b7te", "Stall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}}}}}