{"textgrid.poem.40167": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Eine Weihnachtsstunde", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df, Liebster, die Lampe noch ", "tokens": ["La\u00df", ",", "Liebs\u00b7ter", ",", "die", "Lam\u00b7pe", "noch"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "$,", "NN", "$,", "ART", "NN", "ADV"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "und r\u00fccke mit mir zum Kamin,", "tokens": ["und", "r\u00fc\u00b7cke", "mit", "mir", "zum", "Ka\u00b7min", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "APPRART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "und la\u00df in die Flammen uns sehen", "tokens": ["und", "la\u00df", "in", "die", "Flam\u00b7men", "uns", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "APPR", "ART", "NN", "PPER", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "und lauschen dem Zauber darin!", "tokens": ["und", "lau\u00b7schen", "dem", "Zau\u00b7ber", "da\u00b7rin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PAV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Und lege dein Haupt ans Herz mir", "tokens": ["Und", "le\u00b7ge", "dein", "Haupt", "ans", "Herz", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "PPER"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "und blicke nicht traurig drein,", "tokens": ["und", "bli\u00b7cke", "nicht", "trau\u00b7rig", "drein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADJD", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "da\u00df wir am Heiligen Abend", "tokens": ["da\u00df", "wir", "am", "Hei\u00b7li\u00b7gen", "A\u00b7bend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "im Dunkeln sitzen! allein!", "tokens": ["im", "Dun\u00b7keln", "sit\u00b7zen", "!", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "ADJA", "VVINF", "$.", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Horch, wie im Ofen wispert", "tokens": ["Horch", ",", "wie", "im", "O\u00b7fen", "wis\u00b7pert"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "APPRART", "NN", "VVFIN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "die Glut ihr heimlich Lied!", "tokens": ["die", "Glut", "ihr", "heim\u00b7lich", "Lied", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "schau, wie ein Lichterreigen", "tokens": ["schau", ",", "wie", "ein", "Lich\u00b7ter\u00b7rei\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKVZ", "$,", "PWAV", "ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "\u00fcber die Diele zieht!", "tokens": ["\u00fc\u00b7ber", "die", "Die\u00b7le", "zieht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Draus schwillt's wie ein Singen und Weben", "tokens": ["Draus", "schwillt's", "wie", "ein", "Sin\u00b7gen", "und", "We\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "KOKOM", "ART", "NN", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "von M\u00e4rchenherrlichkeit,", "tokens": ["von", "M\u00e4r\u00b7chen\u00b7herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "drin spielt's wie ein Schwingen und Schweben", "tokens": ["drin", "spielt's", "wie", "ein", "Schwin\u00b7gen", "und", "Schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KOKOM", "ART", "NN", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "von Tr\u00e4umen der Kinderzeit:", "tokens": ["von", "Tr\u00e4u\u00b7men", "der", "Kin\u00b7der\u00b7zeit", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "als wir noch fromm gebetet", "tokens": ["als", "wir", "noch", "fromm", "ge\u00b7be\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "zum lieben Jesuchrist,", "tokens": ["zum", "lie\u00b7ben", "Je\u00b7suc\u00b7hrist", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "der f\u00fcr uns arme S\u00fcnder", "tokens": ["der", "f\u00fcr", "uns", "ar\u00b7me", "S\u00fcn\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPER", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "vom Himmel kommen ist, \u2013", "tokens": ["vom", "Him\u00b7mel", "kom\u00b7men", "ist", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "VVINF", "VAFIN", "$,", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "als wir noch nicht verstanden,", "tokens": ["als", "wir", "noch", "nicht", "ver\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "warum auf Golgatha", "tokens": ["wa\u00b7rum", "auf", "Gol\u00b7ga\u00b7tha"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "APPR", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "ein brechend ", "tokens": ["ein", "bre\u00b7chend"], "token_info": ["word", "word"], "pos": ["ART", "ADJD"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "einst mild zur Erde sah.", "tokens": ["einst", "mild", "zur", "Er\u00b7de", "sah", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und denke der gro\u00dfen Liebe,", "tokens": ["Und", "den\u00b7ke", "der", "gro\u00b7\u00dfen", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "die treu bis in den Tod", "tokens": ["die", "treu", "bis", "in", "den", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPR", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "gerungen und gelitten", "tokens": ["ge\u00b7run\u00b7gen", "und", "ge\u00b7lit\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "KON", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "f\u00fcr all der Br\u00fcder Not!", "tokens": ["f\u00fcr", "all", "der", "Br\u00fc\u00b7der", "Not", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und denke des gro\u00dfen Glaubens,", "tokens": ["Und", "den\u00b7ke", "des", "gro\u00b7\u00dfen", "Glau\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.2": {"text": "den Er zur Menschheit trug", "tokens": ["den", "Er", "zur", "Menschheit", "trug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "noch in der letzten Stunde,", "tokens": ["noch", "in", "der", "letz\u00b7ten", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "da man ans Kreuz ihn schlug!", "tokens": ["da", "man", "ans", "Kreuz", "ihn", "schlug", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Und blicke nicht tr\u00fcb, mein Liebster,", "tokens": ["Und", "bli\u00b7cke", "nicht", "tr\u00fcb", ",", "mein", "Liebs\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADJD", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "da\u00df ", "tokens": ["da\u00df"], "token_info": ["word"], "pos": ["KOUS"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "und hoffe wie Er, da\u00df Einstens", "tokens": ["und", "hof\u00b7fe", "wie", "Er", ",", "da\u00df", "Eins\u00b7tens"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "KOKOM", "PPER", "$,", "KOUS", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "die Goldne Zeit wird sein! \u2013 \u2013", "tokens": ["die", "Gold\u00b7ne", "Zeit", "wird", "sein", "!", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VAINF", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Nun sehe dein Auge ich leuchten", "tokens": ["Nun", "se\u00b7he", "dein", "Au\u00b7ge", "ich", "leuch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PPER", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "und strahlen Eigne Glut,", "tokens": ["und", "strah\u00b7len", "Eig\u00b7ne", "Glut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "nun richtet das Haupt dir wieder", "tokens": ["nun", "rich\u00b7tet", "das", "Haupt", "dir", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "ADV"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "empor der alte Mut.", "tokens": ["em\u00b7por", "der", "al\u00b7te", "Mut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Du bist mein Stolzer, mein Starker!", "tokens": ["Du", "bist", "mein", "Stol\u00b7zer", ",", "mein", "Star\u00b7ker", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "du f\u00fchrst es Alles aus!", "tokens": ["du", "f\u00fchrst", "es", "Al\u00b7les", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Oh gr\u00fcnde und baue nur weiter", "tokens": ["Oh", "gr\u00fcn\u00b7de", "und", "bau\u00b7e", "nur", "wei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "KON", "VVFIN", "ADV", "ADV"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "an deinem stolzen Haus! \u2013", "tokens": ["an", "dei\u00b7nem", "stol\u00b7zen", "Haus", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Und \u00fcbers Jahr ist's anders \u2013", "tokens": ["Und", "\u00fc\u00b7bers", "Jahr", "ist's", "an\u00b7ders", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VAFIN", "ADV", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "neig' her dein Ohr geschwind:", "tokens": ["neig'", "her", "dein", "Ohr", "ge\u00b7schwind", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "da schm\u00fccken wir ein B\u00e4umchen", "tokens": ["da", "schm\u00fc\u00b7cken", "wir", "ein", "B\u00e4um\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "f\u00fcr ein lieb Menschenkind.", "tokens": ["f\u00fcr", "ein", "lieb", "Men\u00b7schen\u00b7kind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}