{"dta.poem.21640": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Die Rosen von Florenz.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr Lieblichen, ihr Rosen,", "tokens": ["Ihr", "Lieb\u00b7li\u00b7chen", ",", "ihr", "Ro\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nun mu\u00df ich still euch preisen", "tokens": ["Nun", "mu\u00df", "ich", "still", "euch", "prei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VMFIN", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit zarten Liedern, leisen,", "tokens": ["Mit", "zar\u00b7ten", "Lie\u00b7dern", ",", "lei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihr Rosen von Florenz.", "tokens": ["Ihr", "Ro\u00b7sen", "von", "Flo\u00b7renz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Gar hoch ist diese Mauer,", "tokens": ["Gar", "hoch", "ist", "die\u00b7se", "Mau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da nickt ihr mir her\u00fcber,", "tokens": ["Da", "nickt", "ihr", "mir", "her\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dein Himmel stumm dar\u00fcber,", "tokens": ["Dein", "Him\u00b7mel", "stumm", "da\u00b7r\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "PAV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Tiefblau, mein sch\u00f6n Florenz!", "tokens": ["Tief\u00b7blau", ",", "mein", "sch\u00f6n", "Flo\u00b7renz", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Gar hoch ist diese Mauer,", "tokens": ["Gar", "hoch", "ist", "die\u00b7se", "Mau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich kann euch nicht erreichen,", "tokens": ["Ich", "kann", "euch", "nicht", "er\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich kann euch nicht beschleichen,", "tokens": ["Ich", "kann", "euch", "nicht", "be\u00b7schlei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihr Rosen von Florenz.", "tokens": ["Ihr", "Ro\u00b7sen", "von", "Flo\u00b7renz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Weit liegt das Thal gebreitet,", "tokens": ["Weit", "liegt", "das", "Thal", "ge\u00b7brei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da schimmern mild Oliven,", "tokens": ["Da", "schim\u00b7mern", "mild", "O\u00b7li\u00b7ven", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "NN", "$,"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.3": {"text": "Cypressen stehn, als schliefen", "tokens": ["Cyp\u00b7res\u00b7sen", "stehn", ",", "als", "schlie\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVINF", "$,", "KOUS", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Die Todten von Florenz.", "tokens": ["Die", "Tod\u00b7ten", "von", "Flo\u00b7renz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Doch euch allein, ihr Rosen,", "tokens": ["Doch", "euch", "al\u00b7lein", ",", "ihr", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Euch will ich l\u00e4chelnd preisen", "tokens": ["Euch", "will", "ich", "l\u00e4\u00b7chelnd", "prei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit leichten Liedern, leisen,", "tokens": ["Mit", "leich\u00b7ten", "Lie\u00b7dern", ",", "lei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihr Rosen von Florenz.", "tokens": ["Ihr", "Ro\u00b7sen", "von", "Flo\u00b7renz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Jungfr\u00e4uliches Erinnern", "tokens": ["Jung\u00b7fr\u00e4u\u00b7li\u00b7ches", "E\u00b7rin\u00b7nern"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Beschleichet meine Tr\u00e4ume", "tokens": ["Be\u00b7schlei\u00b7chet", "mei\u00b7ne", "Tr\u00e4u\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Durch \u00e4therferne R\u00e4ume", "tokens": ["Durch", "\u00e4t\u00b7her\u00b7fer\u00b7ne", "R\u00e4u\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Nach meiner Liebe Lenz.", "tokens": ["Nach", "mei\u00b7ner", "Lie\u00b7be", "Lenz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "So weit weilt meine Liebe!", "tokens": ["So", "weit", "weilt", "mei\u00b7ne", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So hoch ist diese Mauer!", "tokens": ["So", "hoch", "ist", "die\u00b7se", "Mau\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ausweinen meine Trauer", "tokens": ["Aus\u00b7wei\u00b7nen", "mei\u00b7ne", "Trau\u00b7er"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mu\u00df ich nun dir, Florenz.", "tokens": ["Mu\u00df", "ich", "nun", "dir", ",", "Flo\u00b7renz", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPER", "$,", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Jungfr\u00e4uliches Erinnern,", "tokens": ["Jung\u00b7fr\u00e4u\u00b7li\u00b7ches", "E\u00b7rin\u00b7nern", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein s\u00fc\u00dfer Liebesschauer", "tokens": ["Ein", "s\u00fc\u00b7\u00dfer", "Lie\u00b7bes\u00b7schau\u00b7er"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Singt durch Cypressentrauer \u2014", "tokens": ["Singt", "durch", "Cyp\u00b7res\u00b7sen\u00b7trau\u00b7er"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "O seliges Florenz!", "tokens": ["O", "se\u00b7li\u00b7ges", "Flo\u00b7renz", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}