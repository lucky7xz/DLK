{"dta.poem.19524": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Der Triumphbogen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Ein leuchtend blauer Tag. Ein wogend Aehrenfeld,", "tokens": ["Ein", "leuch\u00b7tend", "blau\u00b7er", "Tag", ".", "Ein", "wo\u00b7gend", "A\u00b7eh\u00b7ren\u00b7feld", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$.", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Daraus ein wetterschwarzer Mauerbogen steigt.", "tokens": ["Da\u00b7raus", "ein", "wet\u00b7ter\u00b7schwar\u00b7zer", "Mau\u00b7er\u00b7bo\u00b7gen", "steigt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In seinem kurzen Schatten schl\u00e4ft das Schnittervolk.", "tokens": ["In", "sei\u00b7nem", "kur\u00b7zen", "Schat\u00b7ten", "schl\u00e4ft", "das", "Schnit\u00b7ter\u00b7volk", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Allein emporgerichtet sitzt die sch\u00f6nste Maid,", "tokens": ["Al\u00b7lein", "em\u00b7por\u00b7ge\u00b7rich\u00b7tet", "sitzt", "die", "sch\u00f6ns\u00b7te", "Maid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Des Landes Kind, doch welchen Lands? Italiens!", "tokens": ["Des", "Lan\u00b7des", "Kind", ",", "doch", "wel\u00b7chen", "Lands", "?", "I\u00b7ta\u00b7li\u00b7ens", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "KON", "PWAT", "NN", "$.", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein strenggeschnittnes, musenhaftes Angesicht,", "tokens": ["Ein", "streng\u00b7ge\u00b7schnitt\u00b7nes", ",", "mu\u00b7sen\u00b7haf\u00b7tes", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Am halbzerst\u00f6rten Sims des Bogens hangt der Blick,", "tokens": ["Am", "halb\u00b7zer\u00b7st\u00f6r\u00b7ten", "Sims", "des", "Bo\u00b7gens", "hangt", "der", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Als m\u00fcht' er zu entr\u00e4thseln dort die Inschrift sich.", "tokens": ["Als", "m\u00fcht'", "er", "zu", "ent\u00b7r\u00e4th\u00b7seln", "dort", "die", "In\u00b7schrift", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "PTKZU", "VVINF", "ADV", "ART", "NN", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "(wenn nicht des Auges Dunkel von dem Liebsten tr\u00e4umt.)", "tokens": ["(", "wenn", "nicht", "des", "Au\u00b7ges", "Dun\u00b7kel", "von", "dem", "Liebs\u00b7ten", "tr\u00e4umt", ".", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KOUS", "PTKNEG", "ART", "NN", "NN", "APPR", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Sie hebt die erste sich, erweckt die Schnitterschaar,", "tokens": ["Sie", "hebt", "die", "ers\u00b7te", "sich", ",", "er\u00b7weckt", "die", "Schnit\u00b7ter\u00b7schaar", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "PRF", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ergreift die blanke Sichel, die im Schatten lag,", "tokens": ["Er\u00b7greift", "die", "blan\u00b7ke", "Si\u00b7chel", ",", "die", "im", "Schat\u00b7ten", "lag", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und schreitet herrlich durch das golden wogende Korn,", "tokens": ["Und", "schrei\u00b7tet", "herr\u00b7lich", "durch", "das", "gol\u00b7den", "wo\u00b7gen\u00b7de", "Korn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.13": {"text": "Umblaut vom Himmel als ein g\u00f6ttliches Gebild.", "tokens": ["Um\u00b7blaut", "vom", "Him\u00b7mel", "als", "ein", "g\u00f6tt\u00b7li\u00b7ches", "Ge\u00b7bild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "S'ist Klio, die das Alterthum entr\u00e4thselnde,", "tokens": ["S'\u00b7ist", "Klio", ",", "die", "das", "Al\u00b7ter\u00b7thum", "ent\u00b7r\u00e4th\u00b7seln\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+--", "measure": "anapaest.di.plus"}, "line.15": {"text": "Vergilbten Pergaments und der Archive m\u00fcd,", "tokens": ["Ver\u00b7gilb\u00b7ten", "Per\u00b7ga\u00b7ments", "und", "der", "Ar\u00b7chi\u00b7ve", "m\u00fcd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Gelockt vom Rauschen einer \u00fcberreifen Saat,", "tokens": ["Ge\u00b7lockt", "vom", "Rau\u00b7schen", "ei\u00b7ner", "\u00fc\u00b7berr\u00b7ei\u00b7fen", "Saat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wird sie zur starken Schnitterin. Die Sichel klingt.", "tokens": ["Wird", "sie", "zur", "star\u00b7ken", "Schnit\u00b7te\u00b7rin", ".", "Die", "Si\u00b7chel", "klingt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "ADJA", "NN", "$.", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}