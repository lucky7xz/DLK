{"textgrid.poem.65914": {"metadata": {"author": {"name": "D\u00e4ubler, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Erg\u00f6ttlichung", "genre": "verse", "period": "N.A.", "pub_year": 1905, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Athenerinnen sehn des Friedens Freude,", "tokens": ["A\u00b7the\u00b7ne\u00b7rin\u00b7nen", "sehn", "des", "Frie\u00b7dens", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Als G\u00f6ttin ihrer Stadt, nach altem Streit:", "tokens": ["Als", "G\u00f6t\u00b7tin", "ih\u00b7rer", "Stadt", ",", "nach", "al\u00b7tem", "Streit", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPOSAT", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie bringen Opfer, kommen dienstbereit,", "tokens": ["Sie", "brin\u00b7gen", "Op\u00b7fer", ",", "kom\u00b7men", "dienst\u00b7be\u00b7reit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df lang der Krieg kein schweres Gut vergeude!", "tokens": ["Da\u00df", "lang", "der", "Krieg", "kein", "schwe\u00b7res", "Gut", "ver\u00b7geu\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Herden bleiben fett, verschont von R\u00e4ude:", "tokens": ["Die", "Her\u00b7den", "blei\u00b7ben", "fett", ",", "ver\u00b7schont", "von", "R\u00e4u\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "ADJD", "$,", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Friedensfrau, mit freundlichem Geleit,", "tokens": ["Die", "Frie\u00b7dens\u00b7frau", ",", "mit", "freund\u00b7li\u00b7chem", "Ge\u00b7leit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Umsorgt das Plutos-Kind, voll Z\u00e4rtlichkeit,", "tokens": ["Um\u00b7sorgt", "das", "Plu\u00b7to\u00b7sKind", ",", "voll", "Z\u00e4rt\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und strahlt am Markt vor marmornem Geb\u00e4ude.", "tokens": ["Und", "strahlt", "am", "Markt", "vor", "mar\u00b7mor\u00b7nem", "Ge\u00b7b\u00e4u\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Das Weib zur Wohlheit kam aus Westen angezogen,", "tokens": ["Das", "Weib", "zur", "Wohl\u00b7heit", "kam", "aus", "Wes\u00b7ten", "an\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Eleusis' Tor gibt Reicht\u00fcmer der Welt:", "tokens": ["E\u00b7leu\u00b7sis'", "Tor", "gibt", "Reich\u00b7t\u00fc\u00b7mer", "der", "Welt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Athen, dem kostbaren, ist Gl\u00fcck gewogen.", "tokens": ["A\u00b7then", ",", "dem", "kost\u00b7ba\u00b7ren", ",", "ist", "Gl\u00fcck", "ge\u00b7wo\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "VVINF", "$,", "VAFIN", "NN", "VVPP", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "Ein Dreifu\u00df ward vor die Gestalt gestellt,", "tokens": ["Ein", "Drei\u00b7fu\u00df", "ward", "vor", "die", "Ge\u00b7stalt", "ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Von T\u00e4ubchen bleib der Weihaltar umflogen!", "tokens": ["Von", "T\u00e4ub\u00b7chen", "bleib", "der", "Wei\u00b7hal\u00b7tar", "um\u00b7flo\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mein Purpurteppich gl\u00fcht dahingewellt.", "tokens": ["Mein", "Pur\u00b7pur\u00b7tep\u00b7pich", "gl\u00fcht", "da\u00b7hin\u00b7ge\u00b7wellt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}