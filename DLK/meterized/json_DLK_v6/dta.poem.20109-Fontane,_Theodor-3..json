{"dta.poem.20109": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Gefolgt von seinen Meutehunden,", "tokens": ["Ge\u00b7folgt", "von", "sei\u00b7nen", "Meu\u00b7te\u00b7hun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hat aus dem nahgelegnen Schlo\u00df", "tokens": ["Hat", "aus", "dem", "nah\u00b7ge\u00b7leg\u00b7nen", "Schlo\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Graf, mit seinem Dienertro\u00df,", "tokens": ["Der", "Graf", ",", "mit", "sei\u00b7nem", "Die\u00b7ner\u00b7tro\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Kind, besinnungslos, gefunden.", "tokens": ["Das", "Kind", ",", "be\u00b7sin\u00b7nungs\u00b7los", ",", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "$,", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Doch wenig Wein auf Brust und Stirn,", "tokens": ["Doch", "we\u00b7nig", "Wein", "auf", "Brust", "und", "Stirn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "L\u00e4\u00dft bald die Pulse wieder schlagen,", "tokens": ["L\u00e4\u00dft", "bald", "die", "Pul\u00b7se", "wie\u00b7der", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und heim wird die Zigeuner-Dirn", "tokens": ["Und", "heim", "wird", "die", "Zi\u00b7geu\u00b7ner\u00b7Dirn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PTKVZ", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zu neuem Lebenslauf getragen.", "tokens": ["Zu", "neu\u00b7em", "Le\u00b7bens\u00b7lauf", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}