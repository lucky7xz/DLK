{"textgrid.poem.63568": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "[in dem wei\u00dfen Seidenhut]", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In dem wei\u00dfen Seidenhut", "tokens": ["In", "dem", "wei\u00b7\u00dfen", "Sei\u00b7den\u00b7hut"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00f6nnt' ich heut noch dich betrachten,", "tokens": ["K\u00f6nnt'", "ich", "heut", "noch", "dich", "be\u00b7trach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie wir damals frischverlobt", "tokens": ["Wie", "wir", "da\u00b7mals", "frischver\u00b7lobt"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "VVFIN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Unsre Brautvisiten machten!", "tokens": ["Uns\u00b7re", "Braut\u00b7vi\u00b7si\u00b7ten", "mach\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Reizend war der Hut und fest", "tokens": ["Rei\u00b7zend", "war", "der", "Hut", "und", "fest"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ART", "NN", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Unterm Kinne zugebunden,", "tokens": ["Un\u00b7term", "Kin\u00b7ne", "zu\u00b7ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht dem grauen H\u00fctchen gleich,", "tokens": ["Nicht", "dem", "grau\u00b7en", "H\u00fct\u00b7chen", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jenem \u00fcberm\u00fct'gen runden.", "tokens": ["Je\u00b7nem", "\u00fc\u00b7ber\u00b7m\u00fct'\u00b7gen", "run\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und so ehrbar winkten mir", "tokens": ["Und", "so", "ehr\u00b7bar", "wink\u00b7ten", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Deine sechzehnj\u00e4hr'gen Augen,", "tokens": ["Dei\u00b7ne", "sech\u00b7zehn\u00b7j\u00e4hr'\u00b7gen", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ganz wie fragend: Sollten wir", "tokens": ["Ganz", "wie", "fra\u00b7gend", ":", "Soll\u00b7ten", "wir"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "KOKOM", "VVPP", "$.", "VMFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht zur Hausfraunw\u00fcrde taugen?", "tokens": ["Nicht", "zur", "Haus\u00b7fraun\u00b7w\u00fcr\u00b7de", "tau\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und wie dann dein Kindermund", "tokens": ["Und", "wie", "dann", "dein", "Kin\u00b7der\u00b7mund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADV", "PPOSAT", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ernsthaft mich zur Rede setzte,", "tokens": ["Ernst\u00b7haft", "mich", "zur", "Re\u00b7de", "setz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil ich bei den Tanten oft", "tokens": ["Weil", "ich", "bei", "den", "Tan\u00b7ten", "oft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gar zu tolle Sachen schw\u00e4tzte!", "tokens": ["Gar", "zu", "tol\u00b7le", "Sa\u00b7chen", "schw\u00e4tz\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch ich \u00fcberf\u00fchrte dich,", "tokens": ["Doch", "ich", "\u00fc\u00b7berf\u00b7\u00fchr\u00b7te", "dich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Als nach Hause fuhr der Wagen,", "tokens": ["Als", "nach", "Hau\u00b7se", "fuhr", "der", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df wir beide musterhaft", "tokens": ["Da\u00df", "wir", "bei\u00b7de", "mus\u00b7ter\u00b7haft"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Angemessen uns betragen.", "tokens": ["An\u00b7ge\u00b7mes\u00b7sen", "uns", "be\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "W\u00e4hrend deine Reden, Kind,", "tokens": ["W\u00e4h\u00b7rend", "dei\u00b7ne", "Re\u00b7den", ",", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6chst gesetzt und weise waren,", "tokens": ["H\u00f6chst", "ge\u00b7setzt", "und", "wei\u00b7se", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "KON", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schien ich selbst ein Sausewind,", "tokens": ["Schien", "ich", "selbst", "ein", "Sau\u00b7se\u00b7wind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kaum von hochzeitlichen Jahren.", "tokens": ["Kaum", "von", "hoch\u00b7zeit\u00b7li\u00b7chen", "Jah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+---+-", "measure": "unknown.measure.tri"}}, "stanza.7": {"line.1": {"text": "Mu\u00df nicht unsern Herzensbund", "tokens": ["Mu\u00df", "nicht", "un\u00b7sern", "Her\u00b7zens\u00b7bund"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PTKNEG", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch der \u00e4rgste Zweifler segnen,", "tokens": ["Auch", "der", "\u00e4rgs\u00b7te", "Zweif\u00b7ler", "seg\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn wir so der Jahre Kluft", "tokens": ["Wenn", "wir", "so", "der", "Jah\u00b7re", "Kluft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "\u00dcberbr\u00fcckend uns begegnen?", "tokens": ["\u00dc\u00b7berb\u00b7r\u00fc\u00b7ckend", "uns", "be\u00b7geg\u00b7nen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Gar zu gerne wollt ich wissen,", "tokens": ["Gar", "zu", "ger\u00b7ne", "wollt", "ich", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKA", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was aus diesen Z\u00fcgen spricht,", "tokens": ["Was", "aus", "die\u00b7sen", "Z\u00fc\u00b7gen", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie so schnell mich hingerissen", "tokens": ["Wie", "so", "schnell", "mich", "hin\u00b7ge\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADJD", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieses reizende Gesicht.", "tokens": ["Die\u00b7ses", "rei\u00b7zen\u00b7de", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Manche sah ich, Blond' und Braune,", "tokens": ["Man\u00b7che", "sah", "ich", ",", "Blond'", "und", "Brau\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir in Jugendbl\u00fcte nahn;", "tokens": ["Mir", "in", "Ju\u00b7gend\u00b7bl\u00fc\u00b7te", "nahn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Warum wandelte die Laune,", "tokens": ["Wa\u00b7rum", "wan\u00b7del\u00b7te", "die", "Lau\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie zu lieben, nie mich an?", "tokens": ["Sie", "zu", "lie\u00b7ben", ",", "nie", "mich", "an", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "ADV", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Konnt' ich nicht in F\u00fclle schauen", "tokens": ["Konnt'", "ich", "nicht", "in", "F\u00fcl\u00b7le", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PTKNEG", "APPR", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "lles, was das Herz begehrt:", "tokens": ["lles", ",", "was", "das", "Herz", "be\u00b7gehrt", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sanfte Lippen, stolze Brauen,", "tokens": ["Sanf\u00b7te", "Lip\u00b7pen", ",", "stol\u00b7ze", "Brau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wei\u00dfen Hals, umhalsenswert?", "tokens": ["Wei\u00b7\u00dfen", "Hals", ",", "um\u00b7hal\u00b7sens\u00b7wert", "?"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Dennoch wie am Zauberf\u00e4dchen", "tokens": ["Den\u00b7noch", "wie", "am", "Zau\u00b7ber\u00b7f\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lockte mich in raschem Gang", "tokens": ["Lock\u00b7te", "mich", "in", "ra\u00b7schem", "Gang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Stets sich nach dies schlanke M\u00e4dchen,", "tokens": ["Stets", "sich", "nach", "dies", "schlan\u00b7ke", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "APPR", "PDS", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eh' noch ihre Stimme klang;", "tokens": ["Eh'", "noch", "ih\u00b7re", "Stim\u00b7me", "klang", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Eh' ein Hauch aus ihrer Seele", "tokens": ["Eh'", "ein", "Hauch", "aus", "ih\u00b7rer", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00fcchtern sich zu meiner stahl,", "tokens": ["Sch\u00fcch\u00b7tern", "sich", "zu", "mei\u00b7ner", "stahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich wu\u00dfte: Die erw\u00e4hle!", "tokens": ["Und", "ich", "wu\u00df\u00b7te", ":", "Die", "er\u00b7w\u00e4h\u00b7le", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$.", "ART", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach, dir bleibt ja keine Wahl.", "tokens": ["Ach", ",", "dir", "bleibt", "ja", "kei\u00b7ne", "Wahl", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Jetzt, da ich bei Nacht und Tage", "tokens": ["Jetzt", ",", "da", "ich", "bei", "Nacht", "und", "Ta\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihr Gesicht studieren mag,", "tokens": ["Ihr", "Ge\u00b7sicht", "stu\u00b7die\u00b7ren", "mag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bleibt die gro\u00dfe R\u00e4tselfrage", "tokens": ["Bleibt", "die", "gro\u00b7\u00dfe", "R\u00e4t\u00b7sel\u00b7fra\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dunkel wie am ersten Tag.", "tokens": ["Dun\u00b7kel", "wie", "am", "ers\u00b7ten", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Doch entsag' ich gern dem Wissen;", "tokens": ["Doch", "ent\u00b7sag'", "ich", "gern", "dem", "Wis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schauen ist die h\u00f6h're Pflicht.", "tokens": ["Schau\u00b7en", "ist", "die", "h\u00f6h'\u00b7re", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fort das Gr\u00fcbeln! La\u00df dich k\u00fcssen,", "tokens": ["Fort", "das", "Gr\u00fc\u00b7beln", "!", "La\u00df", "dich", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "VVIMP", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unerforschlich s\u00fc\u00df Gesicht!", "tokens": ["Un\u00b7er\u00b7for\u00b7schlich", "s\u00fc\u00df", "Ge\u00b7sicht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}