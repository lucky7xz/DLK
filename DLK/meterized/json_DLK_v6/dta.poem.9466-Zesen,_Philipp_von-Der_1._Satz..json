{"dta.poem.9466": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "Der 1. Satz.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "War\u00fcmb ist der Himmel offen?", "tokens": ["Wa\u00b7r\u00fcmb", "ist", "der", "Him\u00b7mel", "of\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df uns hoffen.", "tokens": ["La\u00df", "uns", "hof\u00b7fen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Wird die Nacht nicht hell und klar?", "tokens": ["Wird", "die", "Nacht", "nicht", "hell", "und", "klar", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PTKNEG", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Es ist wahr.", "tokens": ["Es", "ist", "wahr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Siht mann nicht den Mond verbleichen?", "tokens": ["Siht", "mann", "nicht", "den", "Mond", "ver\u00b7blei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Er mu\u00df weichen.", "tokens": ["Er", "mu\u00df", "wei\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Das gestirnte Wolcken-heer", "tokens": ["Das", "ge\u00b7stirn\u00b7te", "Wol\u00b7cken\u00b7heer"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Fliht je mehr und mehr;", "tokens": ["Fliht", "je", "mehr", "und", "mehr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "KON", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Es entspringt ein neues Licht/", "tokens": ["Es", "ent\u00b7springt", "ein", "neu\u00b7es", "Licht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Das mit solchen g\u00fcldnen strahlen", "tokens": ["Das", "mit", "sol\u00b7chen", "g\u00fcld\u00b7nen", "strah\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Durch die f\u00fcnstern N\u00e4chte bricht", "tokens": ["Durch", "die", "f\u00fcns\u00b7tern", "N\u00e4ch\u00b7te", "bricht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "und der W\u00f6lcken-zelt kann mahlen", "tokens": ["und", "der", "W\u00f6l\u00b7cken\u00b7zelt", "kann", "mah\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Mit Rubien und Hyacinth/", "tokens": ["Mit", "Ru\u00b7bi\u00b7en", "und", "Hya\u00b7cin\u00b7th", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Das den N\u00e4chten abgewinnt.", "tokens": ["Das", "den", "N\u00e4ch\u00b7ten", "ab\u00b7ge\u00b7winnt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Was doch h\u00f6rt mann an dem Himmel?", "tokens": ["Was", "doch", "h\u00f6rt", "mann", "an", "dem", "Him\u00b7mel", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ein get\u00fcmmel.", "tokens": ["Ein", "ge\u00b7t\u00fcm\u00b7mel", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "und was ist das vor ein klang?", "tokens": ["und", "was", "ist", "das", "vor", "ein", "klang", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "ART", "APPR", "ART", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ein Gesang.", "tokens": ["Ein", "Ge\u00b7sang", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Der dort aus den L\u00fcfften wallet?", "tokens": ["Der", "dort", "aus", "den", "L\u00fcff\u00b7ten", "wal\u00b7let", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Ja es schallet.", "tokens": ["Ja", "es", "schal\u00b7let", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Eine s\u00fc\u00dfe Melodey", "tokens": ["Ei\u00b7ne", "s\u00fc\u00b7\u00dfe", "Me\u00b7lo\u00b7dey"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Macht uns Kummers-frey.", "tokens": ["Macht", "uns", "Kum\u00b7mer\u00b7sfrey", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "H\u00f6r ich/ oder deucht mich so/", "tokens": ["H\u00f6r", "ich", "/", "o\u00b7der", "deucht", "mich", "so", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$(", "KON", "VVFIN", "PPER", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dort die g\u00fcldnen Cherubienen/", "tokens": ["Dort", "die", "g\u00fcld\u00b7nen", "Che\u00b7ru\u00b7bie\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach wie seyn sie doch so froh", "tokens": ["Ach", "wie", "seyn", "sie", "doch", "so", "froh"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "PPOSAT", "PPER", "ADV", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit den klahren Seraphinen/", "tokens": ["Mit", "den", "klah\u00b7ren", "Se\u00b7ra\u00b7phi\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "H\u00f6re das liebliche geth\u00f6n/", "tokens": ["H\u00f6\u00b7re", "das", "lieb\u00b7li\u00b7che", "ge\u00b7th\u00f6n", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "ADJD", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Ach wie klingt es nur so sch\u00f6n!", "tokens": ["Ach", "wie", "klingt", "es", "nur", "so", "sch\u00f6n", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Freuet Euch alle seyd fr\u00f6lich im HErrn/", "tokens": ["Freu\u00b7et", "Euch", "al\u00b7le", "seyd", "fr\u00f6\u00b7lich", "im", "Herrn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "VAFIN", "ADJD", "APPRART", "NN", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Das trauren sey ferrn/", "tokens": ["Das", "trau\u00b7ren", "sey", "ferrn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVINF", "VAFIN", "ADV", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Kommet und schauet und f\u00fcrchtet Euch nicht/", "tokens": ["Kom\u00b7met", "und", "schau\u00b7et", "und", "f\u00fcrch\u00b7tet", "Euch", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "KON", "VVFIN", "PPER", "PTKNEG", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Ein edeles Licht", "tokens": ["Ein", "e\u00b7de\u00b7les", "Licht"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Ist jtzund auffgangen", "tokens": ["Ist", "jt\u00b7zund", "auff\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ADV", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Mit g\u00fcldenen Wangen/", "tokens": ["Mit", "g\u00fcl\u00b7de\u00b7nen", "Wan\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "O Fr\u00f6liche zen/", "tokens": ["O", "Fr\u00f6\u00b7li\u00b7che", "zen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVINF", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Jesus der Heyland ist heute gebohren/", "tokens": ["Je\u00b7sus", "der", "Hey\u00b7land", "ist", "heu\u00b7te", "ge\u00b7boh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VAFIN", "ADV", "VVPP", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.9": {"text": "Welcher zum Spiegel der Gottheit erkohren/", "tokens": ["Wel\u00b7cher", "zum", "Spie\u00b7gel", "der", "Got\u00b7theit", "er\u00b7koh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "APPRART", "NN", "ART", "NN", "VVPP", "$("], "meter": "+--+--++-+-", "measure": "dactylic.di.plus"}, "line.10": {"text": "Wird jtzund auch fleischlich bekleidt/", "tokens": ["Wird", "jt\u00b7zund", "auch", "fleischlich", "be\u00b7kleidt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADJD", "ADJD", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "O fr\u00f6liche Zeit!", "tokens": ["O", "fr\u00f6\u00b7li\u00b7che", "Zeit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Wer ists/ der sich hier einstellt?", "tokens": ["Wer", "ists", "/", "der", "sich", "hier", "ein\u00b7stellt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$(", "PRELS", "PRF", "ADV", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "unser Held.", "tokens": ["un\u00b7ser", "Held", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Der dort ligt auf Stroh und Heu/", "tokens": ["Der", "dort", "ligt", "auf", "Stroh", "und", "Heu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "APPR", "NN", "KON", "NN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "S\u00fcnden-frey?", "tokens": ["S\u00fcn\u00b7den\u00b7frey", "?"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Ach wie zieht Er ein die Lippen", "tokens": ["Ach", "wie", "zieht", "Er", "ein", "die", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "VVFIN", "PPER", "ART", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In der Krippen/", "tokens": ["In", "der", "Krip\u00b7pen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Dessen H\u00e4nde-werck wir seyn/", "tokens": ["Des\u00b7sen", "H\u00e4n\u00b7de\u00b7\u00b7werck", "wir", "seyn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PPER", "VAINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Der uns nehrt allein/", "tokens": ["Der", "uns", "nehrt", "al\u00b7lein", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ADV", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Der des Gro\u00dfen Gottes Sohn/", "tokens": ["Der", "des", "Gro\u00b7\u00dfen", "Got\u00b7tes", "Sohn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Mischt sich in der Menschen-Orden/", "tokens": ["Mischt", "sich", "in", "der", "Men\u00b7schen\u00b7Or\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", 