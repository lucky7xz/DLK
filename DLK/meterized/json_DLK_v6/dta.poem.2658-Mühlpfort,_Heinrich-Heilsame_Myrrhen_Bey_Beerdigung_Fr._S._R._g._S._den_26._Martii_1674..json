{"dta.poem.2658": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Heilsame Myrrhen/  \n  Bey Beerdigung Fr. S. R. g. S. den 26.  \n Martii 1674.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So hast du/ Selige/ dein Vaterland gefunden/", "tokens": ["So", "hast", "du", "/", "Se\u00b7li\u00b7ge", "/", "dein", "Va\u00b7ter\u00b7land", "ge\u00b7fun\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$(", "NN", "$(", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Die Windeln haben dich in ", "tokens": ["Die", "Win\u00b7deln", "ha\u00b7ben", "dich", "in"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPR"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Gantz ", "tokens": ["Gantz"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Wiewol des Todes Grimm sie allzufr\u00fch verlescht.", "tokens": ["Wie\u00b7wol", "des", "To\u00b7des", "Grimm", "sie", "all\u00b7zu\u00b7fr\u00fch", "ver\u00b7lescht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NE", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ja des Verh\u00e4ngn\u00fc\u00df Schlu\u00df hie\u00df dich nach ", "tokens": ["Ja", "des", "Ver\u00b7h\u00e4ng\u00b7n\u00fc\u00df", "Schlu\u00df", "hie\u00df", "dich", "nach"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "ART", "NN", "NN", "VVFIN", "PRF", "APPR"], "meter": "+--+-++-+", "measure": "iambic.penta.invert"}, "line.8": {"text": "Das unser Oderstrom mit seinen Wellen w\u00e4scht.", "tokens": ["Das", "un\u00b7ser", "O\u00b7der\u00b7strom", "mit", "sei\u00b7nen", "Wel\u00b7len", "w\u00e4scht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Auch diese Pilgramschafft nahm ein geschwindes Ende.", "tokens": ["Auch", "die\u00b7se", "Pil\u00b7gramsc\u00b7hafft", "nahm", "ein", "ge\u00b7schwin\u00b7des", "En\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wer leugnet/ da\u00df der Mensch ein Gauckelspiel der Zeit?", "tokens": ["Wer", "leug\u00b7net", "/", "da\u00df", "der", "Mensch", "ein", "Gau\u00b7ckel\u00b7spiel", "der", "Zeit", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$(", "KOUS", "ART", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Als Polen ward verheert durch Kriege/ Mord/ und Br\u00e4nde/", "tokens": ["Als", "Po\u00b7len", "ward", "ver\u00b7heert", "durch", "Krie\u00b7ge", "/", "Mord", "/", "und", "Br\u00e4n\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NE", "VAFIN", "VVPP", "APPR", "NN", "$(", "NN", "$(", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Hat ", "tokens": ["Hat"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "-", "measure": "single.down"}, "line.13": {"text": "Ach stete Pilgramin auf diesem Rund der Erden!", "tokens": ["Ach", "ste\u00b7te", "Pilg\u00b7ra\u00b7min", "auf", "die\u00b7sem", "Rund", "der", "Er\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "NE", "APPR", "PDAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Dein Wandel hat gelehrt/ da\u00df hier kein bleiben sey:", "tokens": ["Dein", "Wan\u00b7del", "hat", "ge\u00b7lehrt", "/", "da\u00df", "hier", "kein", "blei\u00b7ben", "sey", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$(", "KOUS", "ADV", "PIAT", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Da\u00df wir als Frembdlinge bald auffgejaget werden/", "tokens": ["Da\u00df", "wir", "als", "Frembd\u00b7lin\u00b7ge", "bald", "auff\u00b7ge\u00b7ja\u00b7get", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOUS", "NN", "ADV", "VVPP", "VAINF", "$("], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Pall\u00e4st und St\u00e4dte nur ein \u00f6de W\u00fcsteney:", "tokens": ["Pal\u00b7l\u00e4st", "und", "St\u00e4d\u00b7te", "nur", "ein", "\u00f6\u00b7de", "W\u00fcs\u00b7te\u00b7ney", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wie unsers Lebens-Lauff gar eine schwere Reise/", "tokens": ["Wie", "un\u00b7sers", "Le\u00b7bens\u00b7Lauff", "gar", "ei\u00b7ne", "schwe\u00b7re", "Rei\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Da Tr\u00fcbsal und Gefahr jedwede Stunde bl\u00fcth:", "tokens": ["Da", "Tr\u00fcb\u00b7sal", "und", "Ge\u00b7fahr", "jed\u00b7we\u00b7de", "Stun\u00b7de", "bl\u00fcth", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Da Thr\u00e4nen seyn der Tranck/ und Hertzens-Angst die Speise/", "tokens": ["Da", "Thr\u00e4\u00b7nen", "seyn", "der", "Tranck", "/", "und", "Hert\u00b7zens\u00b7Angst", "die", "Spei\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPOSAT", "ART", "NN", "$(", "KON", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Bi\u00df uns mit seinem Garn der blasse Tod bezieht.", "tokens": ["Bi\u00df", "uns", "mit", "sei\u00b7nem", "Garn", "der", "blas\u00b7se", "Tod", "be\u00b7zieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Standhaffte Wanderin! die sich lie\u00df nichts verwirren/", "tokens": ["Stand\u00b7haff\u00b7te", "Wan\u00b7de\u00b7rin", "!", "die", "sich", "lie\u00df", "nichts", "ver\u00b7wir\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PRELS", "PRF", "VVFIN", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Ob manch betr\u00fcbter Tag das Leben dir verg\u00e4llt:", "tokens": ["Ob", "manch", "be\u00b7tr\u00fcb\u00b7ter", "Tag", "das", "Le\u00b7ben", "dir", "ver\u00b7g\u00e4llt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ADJA", "NN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Du siehst nun unter dir die Berge voller Myrrhen/", "tokens": ["Du", "siehst", "nun", "un\u00b7ter", "dir", "die", "Ber\u00b7ge", "vol\u00b7ler", "Myr\u00b7rhen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und hast auff die zugleich dein gantzes Thun gestellt.", "tokens": ["Und", "hast", "auff", "die", "zu\u00b7gleich", "dein", "gant\u00b7zes", "Thun", "ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "ART", "ADV", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ich meine nicht der Welt geh\u00e4uffte Bitterkeiten/", "tokens": ["Ich", "mei\u00b7ne", "nicht", "der", "Welt", "ge\u00b7h\u00e4uff\u00b7te", "Bit\u00b7ter\u00b7kei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Die ihren Siegern auch/ wie Rom/ nur Wermuth schenckt:", "tokens": ["Die", "ih\u00b7ren", "Sie\u00b7gern", "auch", "/", "wie", "Rom", "/", "nur", "Wer\u00b7muth", "schenckt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "$(", "KOKOM", "NE", "$(", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Nein/ ach Erblaste/ nein! Du ruhst an JEsus Seiten/", "tokens": ["Nein", "/", "ach", "Er\u00b7blas\u00b7te", "/", "nein", "!", "Du", "ruhst", "an", "Je\u00b7sus", "Sei\u00b7ten", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "XY", "NN", "$(", "PTKANT", "$.", "PPER", "VVFIN", "APPR", "NE", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Sein P\u00fcschel Myrrhen ist umb deine Brust gehenckt.", "tokens": ["Sein", "P\u00fc\u00b7schel", "Myr\u00b7rhen", "ist", "umb", "dei\u00b7ne", "Brust", "ge\u00b7henckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Du Sulamithin ruffst: Mein Liebster ist wie Myrrhen/", "tokens": ["Du", "Su\u00b7la\u00b7mi\u00b7thin", "ruffst", ":", "Mein", "Liebs\u00b7ter", "ist", "wie", "Myr\u00b7rhen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVFIN", "$.", "PPOSAT", "NN", "VAFIN", "KOKOM", "NN", "$("], "meter": "-+-+-+-+-+---", "measure": "unknown.measure.penta"}, "line.30": {"text": "Ich mu\u00df zu diesem Berg und Weyrauch-H\u00fcgeln gehn:", "tokens": ["Ich", "mu\u00df", "zu", "die\u00b7sem", "Berg", "und", "Wey\u00b7rauch\u00b7H\u00fc\u00b7geln", "gehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PDAT", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Er klopfft mir an das Hertz: in meinen Angst-Geschirren", "tokens": ["Er", "klopfft", "mir", "an", "das", "Hertz", ":", "in", "mei\u00b7nen", "Angst\u00b7Ge\u00b7schir\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Seh\u2019 ich den Perlen-Thau von seinen Fingern stehn.", "tokens": ["Seh'", "ich", "den", "Per\u00b7len\u00b7\u00b7T\u00b7hau", "von", "sei\u00b7nen", "Fin\u00b7gern", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.33": {"text": "Wie fr\u00f6lich brechen mir/ gleich in der Marter-Wochen/", "tokens": ["Wie", "fr\u00f6\u00b7lich", "bre\u00b7chen", "mir", "/", "gleich", "in", "der", "Mar\u00b7ter\u00b7Wo\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "$(", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "Die Augen/ da mein GOtt stirbt f\u00fcr der Erden Heil.", "tokens": ["Die", "Au\u00b7gen", "/", "da", "mein", "Gott", "stirbt", "f\u00fcr", "der", "Er\u00b7den", "Heil", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "KOUS", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Ich hab auf ewig ihm/ Geliebte/ mich versprochen/", "tokens": ["Ich", "hab", "auf", "e\u00b7wig", "ihm", "/", "Ge\u00b7lieb\u00b7te", "/", "mich", "ver\u00b7spro\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJD", "PPER", "$(", "NN", "$(", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Hier ernd\u2019 und saml\u2019 ich mir mein au\u00dferwehltes Theil.", "tokens": ["Hier", "ernd'", "und", "saml'", "ich", "mir", "mein", "au\u00b7\u00dfer\u00b7wehl\u00b7tes", "Theil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "VVFIN", "PPER", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Solt ich jedweden Zweig nicht in mein Str\u00e4u\u00dflein binden?", "tokens": ["Solt", "ich", "jed\u00b7we\u00b7den", "Zweig", "nicht", "in", "mein", "Str\u00e4u\u00df\u00b7lein", "bin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIAT", "NN", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Da diese Statte mir des Lebens Freyheit giebt/", "tokens": ["Da", "die\u00b7se", "Stat\u00b7te", "mir", "des", "Le\u00b7bens", "Frey\u00b7heit", "giebt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "PPER", "ART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Da ich die Hefftigkeit der Liebe kan empfinden/", "tokens": ["Da", "ich", "die", "Heff\u00b7tig\u00b7keit", "der", "Lie\u00b7be", "kan", "emp\u00b7fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Gehorsam bi\u00df in Tod/ den GOtt hat selbst geliebt.", "tokens": ["Ge\u00b7hor\u00b7sam", "bi\u00df", "in", "Tod", "/", "den", "Gott", "hat", "selbst", "ge\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPR", "NN", "$(", "ART", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Die wahre Myrrhen kont\u2019 auch nicht ein Werth erreichen/", "tokens": ["Die", "wah\u00b7re", "Myr\u00b7rhen", "kont'", "auch", "nicht", "ein", "Werth", "er\u00b7rei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ADV", "PTKNEG", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Denn Heyden haben sie den G\u00f6ttern fast versagt:", "tokens": ["Denn", "Hey\u00b7den", "ha\u00b7ben", "sie", "den", "G\u00f6t\u00b7tern", "fast", "ver\u00b7sagt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPER", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "De\u00df HErren Leiden ist mit nichts nicht zu vergleichen/", "tokens": ["De\u00df", "Her\u00b7ren", "Lei\u00b7den", "ist", "mit", "nichts", "nicht", "zu", "ver\u00b7glei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "APPR", "PIS", "PTKNEG", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Als der sein eigen Blut f\u00fcr mich im Creutz gewagt.", "tokens": ["Als", "der", "sein", "ei\u00b7gen", "Blut", "f\u00fcr", 