{"textgrid.poem.31714": {"metadata": {"author": {"name": "Weerth, Georg", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Friederich! O Friederich!", "tokens": ["O", "Frie\u00b7de\u00b7rich", "!", "O", "Frie\u00b7de\u00b7rich", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich war erstaunlich liederlich.", "tokens": ["Ich", "war", "er\u00b7staun\u00b7lich", "lie\u00b7der\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im r\u00f6tlichen Wein ist alles verschlemmt,", "tokens": ["Im", "r\u00f6t\u00b7li\u00b7chen", "Wein", "ist", "al\u00b7les", "ver\u00b7schlemmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "PIS", "VVFIN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Rock, die Hose, der Hut und das Hemd.", "tokens": ["Der", "Rock", ",", "die", "Ho\u00b7se", ",", "der", "Hut", "und", "das", "Hemd", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Doch fr\u00f6hlich bin ich und wunderk\u00fchn,", "tokens": ["Doch", "fr\u00f6h\u00b7lich", "bin", "ich", "und", "wun\u00b7der\u00b7k\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "KON", "VVINF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da nun am Strande die Rosen bl\u00fchn.", "tokens": ["Da", "nun", "am", "Stran\u00b7de", "die", "Ro\u00b7sen", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich springe hinab in den gr\u00fcnen Strom", "tokens": ["Ich", "sprin\u00b7ge", "hin\u00b7ab", "in", "den", "gr\u00fc\u00b7nen", "Strom"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und schwimme vor\u00fcber an Burg und Dom.", "tokens": ["Und", "schwim\u00b7me", "vor\u00b7\u00fc\u00b7ber", "an", "Burg", "und", "Dom", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Ein schmucker Delphin kommt eben daher,", "tokens": ["Ein", "schmu\u00b7cker", "Del\u00b7phin", "kommt", "e\u00b7ben", "da\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "PAV", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er tr\u00e4gt mich hinunter ins stille Meer.", "tokens": ["Er", "tr\u00e4gt", "mich", "hin\u00b7un\u00b7ter", "ins", "stil\u00b7le", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Gen Westen ist unser Zug gewandt:", "tokens": ["Gen", "Wes\u00b7ten", "ist", "un\u00b7ser", "Zug", "ge\u00b7wandt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Gott gr\u00fc\u00dfe dich, sch\u00f6nes Engelland!", "tokens": ["Gott", "gr\u00fc\u00b7\u00dfe", "dich", ",", "sch\u00f6\u00b7nes", "En\u00b7gel\u00b7land", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Gott gr\u00fc\u00dfe dich, Spanien und Portugal!", "tokens": ["Gott", "gr\u00fc\u00b7\u00dfe", "dich", ",", "Spa\u00b7ni\u00b7en", "und", "Por\u00b7tu\u00b7gal", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "NE", "KON", "NE", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ich fliege dahin auf der Wogen Schwall.", "tokens": ["Ich", "flie\u00b7ge", "da\u00b7hin", "auf", "der", "Wo\u00b7gen", "Schwall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Nixe singt und der Haifisch springt,", "tokens": ["Die", "Ni\u00b7xe", "singt", "und", "der", "Hai\u00b7fisch", "springt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ein M\u00f6wenlied in den L\u00fcften erklingt.", "tokens": ["Ein", "M\u00f6\u00b7wen\u00b7lied", "in", "den", "L\u00fcf\u00b7ten", "er\u00b7klingt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Dort steigen die gr\u00fcnen Inseln herauf,", "tokens": ["Dort", "stei\u00b7gen", "die", "gr\u00fc\u00b7nen", "In\u00b7seln", "her\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dort nehmen mich freundliche V\u00f6lker auf.", "tokens": ["Dort", "neh\u00b7men", "mich", "freund\u00b7li\u00b7che", "V\u00f6l\u00b7ker", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und K\u00f6nig werd ich zur selbigen Stund,", "tokens": ["Und", "K\u00f6\u00b7nig", "werd", "ich", "zur", "sel\u00b7bi\u00b7gen", "Stund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dieweil ich am meisten vertrinken kunnt.", "tokens": ["Die\u00b7weil", "ich", "am", "meis\u00b7ten", "ver\u00b7trin\u00b7ken", "kunnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKA", "PIS", "VVFIN", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.6": {"line.1": {"text": "Nun seufz ich nicht l\u00e4nger \u2013 ich s\u00e4ufe nur,", "tokens": ["Nun", "seufz", "ich", "nicht", "l\u00e4n\u00b7ger", "\u2013", "ich", "s\u00e4u\u00b7fe", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PTKNEG", "ADJD", "$(", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Mein Minister ist ein Mundschenke nur,", "tokens": ["Mein", "Mi\u00b7nis\u00b7ter", "ist", "ein", "Mund\u00b7schen\u00b7ke", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Mein Geheimrat singt wie die Nachtigall \u2013", "tokens": ["Mein", "Ge\u00b7heim\u00b7rat", "singt", "wie", "die", "Nach\u00b7ti\u00b7gall", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "KOKOM", "ART", "NN", "$("], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und wild w\u00e4chst der Wein im Gebirg und im Tal.", "tokens": ["Und", "wild", "w\u00e4chst", "der", "Wein", "im", "Ge\u00b7birg", "und", "im", "Tal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.7": {"line.1": {"text": "Wie mag es da dr\u00fcben in Deutschland sein?", "tokens": ["Wie", "mag", "es", "da", "dr\u00fc\u00b7ben", "in", "Deutschland", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "ADV", "APPR", "NE", "VAINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ach Bruder, gr\u00fc\u00dfe die Deutschen fein.", "tokens": ["Ach", "Bru\u00b7der", ",", "gr\u00fc\u00b7\u00dfe", "die", "Deut\u00b7schen", "fein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ach gr\u00fc\u00dfe mir jeden, der mich kennt,", "tokens": ["Ach", "gr\u00fc\u00b7\u00dfe", "mir", "je\u00b7den", ",", "der", "mich", "kennt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "PIAT", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und jeden sch\u00f6nen deutschen Student.", "tokens": ["Und", "je\u00b7den", "sch\u00f6\u00b7nen", "deut\u00b7schen", "Stu\u00b7dent", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}