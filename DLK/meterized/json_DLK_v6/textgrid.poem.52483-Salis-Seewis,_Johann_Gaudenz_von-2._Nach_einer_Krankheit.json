{"textgrid.poem.52483": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "2. Nach einer Krankheit", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin so froh, da\u00df ich dich wieder sehe,", "tokens": ["Ich", "bin", "so", "froh", ",", "da\u00df", "ich", "dich", "wie\u00b7der", "se\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "KOUS", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dich, meines Gottes sch\u00f6ne Welt!", "tokens": ["Dich", ",", "mei\u00b7nes", "Got\u00b7tes", "sch\u00f6\u00b7ne", "Welt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df wieder ich auf diesem Pl\u00e4tzchen stehe,", "tokens": ["Da\u00df", "wie\u00b7der", "ich", "auf", "die\u00b7sem", "Pl\u00e4tz\u00b7chen", "ste\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das mehr als alles mir gef\u00e4llt.", "tokens": ["Das", "mehr", "als", "al\u00b7les", "mir", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "KOKOM", "PIS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Vor mir die Flur, im Gold der Sonnenstrahlen,", "tokens": ["Vor", "mir", "die", "Flur", ",", "im", "Gold", "der", "Son\u00b7nen\u00b7strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hier gelb und gr\u00fcn, dort rot und blau;", "tokens": ["Hier", "gelb", "und", "gr\u00fcn", ",", "dort", "rot", "und", "blau", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Regenbogens hohe Farben malen", "tokens": ["Des", "Re\u00b7gen\u00b7bo\u00b7gens", "ho\u00b7he", "Far\u00b7ben", "ma\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Bach, die Wiese, Busch und Au'.", "tokens": ["Den", "Bach", ",", "die", "Wie\u00b7se", ",", "Busch", "und", "Au'", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "All\u00fcberall ist nichts als reges Leben", "tokens": ["Al\u00b7l\u00fc\u00b7be\u00b7rall", "ist", "nichts", "als", "re\u00b7ges", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIS", "KOKOM", "ADJA", "NN"], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Im weiten Reiche der Natur!", "tokens": ["Im", "wei\u00b7ten", "Rei\u00b7che", "der", "Na\u00b7tur", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NE", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die M\u00fccken, die im lichten Strahle schweben,", "tokens": ["Die", "M\u00fc\u00b7cken", ",", "die", "im", "lich\u00b7ten", "Strah\u00b7le", "schwe\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und jedes Gr\u00e4schen auf der Flur!", "tokens": ["Und", "je\u00b7des", "Gr\u00e4sc\u00b7hen", "auf", "der", "Flur", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Lerche, die sich k\u00fchn zur Sonne schwinget", "tokens": ["Die", "Ler\u00b7che", ",", "die", "sich", "k\u00fchn", "zur", "Son\u00b7ne", "schwin\u00b7get"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "ADJD", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und hoch in Wolken sich verirrt;", "tokens": ["Und", "hoch", "in", "Wol\u00b7ken", "sich", "ver\u00b7irrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Nachtigall, die laute Lieder singet;", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", ",", "die", "lau\u00b7te", "Lie\u00b7der", "sin\u00b7get", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Grille, die im Grase schwirrt \u2013", "tokens": ["Die", "Gril\u00b7le", ",", "die", "im", "Gra\u00b7se", "schwirrt", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wohl alles zeugt, im fr\u00f6hlichen Gewimmel:", "tokens": ["Wohl", "al\u00b7les", "zeugt", ",", "im", "fr\u00f6h\u00b7li\u00b7chen", "Ge\u00b7wim\u00b7mel", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es ist ein Gott, der uns die Freuden giebt;", "tokens": ["Es", "ist", "ein", "Gott", ",", "der", "uns", "die", "Freu\u00b7den", "giebt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein guter Gott \u2013 ein Vater ist im Himmel,", "tokens": ["Ein", "gu\u00b7ter", "Gott", "\u2013", "ein", "Va\u00b7ter", "ist", "im", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der alle seine Wesen liebt.", "tokens": ["Der", "al\u00b7le", "sei\u00b7ne", "We\u00b7sen", "liebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Dank, Vater, dir! Es ist auch deine Gabe,", "tokens": ["Dank", ",", "Va\u00b7ter", ",", "dir", "!", "Es", "ist", "auch", "dei\u00b7ne", "Ga\u00b7be", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PPER", "$.", "PPER", "VAFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was heut mein frohes Herz genie\u00dft;", "tokens": ["Was", "heut", "mein", "fro\u00b7hes", "Herz", "ge\u00b7nie\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Thr\u00e4nen Dank! da\u00df nicht im dunkeln Grabe", "tokens": ["Mit", "Thr\u00e4\u00b7nen", "Dank", "!", "da\u00df", "nicht", "im", "dun\u00b7keln", "Gra\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "NN", "$.", "KOUS", "PTKNEG", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mich itzt der enge Sarg verschlie\u00dft:", "tokens": ["Mich", "itzt", "der", "en\u00b7ge", "Sarg", "ver\u00b7schlie\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Da\u00df diese Augen, statt itzt zu verwesen,", "tokens": ["Da\u00df", "die\u00b7se", "Au\u00b7gen", ",", "statt", "itzt", "zu", "ver\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "$,", "KOUI", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ringsum in der Natur, entz\u00fcckt", "tokens": ["Ring\u00b7sum", "in", "der", "Na\u00b7tur", ",", "ent\u00b7z\u00fcckt"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "VVFIN"], "meter": "+-++-+-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Die gro\u00dfen Spuren deiner G\u00fcte lesen,", "tokens": ["Die", "gro\u00b7\u00dfen", "Spu\u00b7ren", "dei\u00b7ner", "G\u00fc\u00b7te", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die du so sch\u00f6n ihr aufgedr\u00fcckt;", "tokens": ["Die", "du", "so", "sch\u00f6n", "ihr", "auf\u00b7ge\u00b7dr\u00fcckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Da\u00df ich, gest\u00e4rkt, noch wandle auf der sch\u00f6nen,", "tokens": ["Da\u00df", "ich", ",", "ge\u00b7st\u00e4rkt", ",", "noch", "wand\u00b7le", "auf", "der", "sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "VVPP", "$,", "ADV", "VVFIN", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit Lust bes\u00e4'ten Pilgerbahn.", "tokens": ["Mit", "Lust", "be\u00b7s\u00e4'\u00b7ten", "Pil\u00b7ger\u00b7bahn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des bin ich froh, und danke dir mit Thr\u00e4nen,", "tokens": ["Des", "bin", "ich", "froh", ",", "und", "dan\u00b7ke", "dir", "mit", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VAFIN", "PPER", "ADJD", "$,", "KON", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So viel, so viel ich danken kann!", "tokens": ["So", "viel", ",", "so", "viel", "ich", "dan\u00b7ken", "kann", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADV", "ADV", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "La\u00df, Vater, mich! Noch weil' ich gern hienieden.", "tokens": ["La\u00df", ",", "Va\u00b7ter", ",", "mich", "!", "Noch", "weil'", "ich", "gern", "hien\u00b7ie\u00b7den", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "NN", "$,", "PPER", "$.", "ADV", "VVFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch giebt mir einst der Tod die Hand \u2013", "tokens": ["Doch", "giebt", "mir", "einst", "der", "Tod", "die", "Hand", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich zittre nicht; froh geh' ich und zufrieden", "tokens": ["Ich", "zitt\u00b7re", "nicht", ";", "froh", "geh'", "ich", "und", "zu\u00b7frie\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$.", "ADJD", "VVFIN", "PPER", "KON", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu dir, ins bessre Vaterland!", "tokens": ["Zu", "dir", ",", "ins", "bess\u00b7re", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}