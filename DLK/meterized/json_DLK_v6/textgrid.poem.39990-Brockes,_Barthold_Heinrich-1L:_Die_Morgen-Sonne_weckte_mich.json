 "measure": "iambic.penta"}, "line.7": {"text": "Ach! wie viel weniger war er dazu geschickt,", "tokens": ["Ach", "!", "wie", "viel", "we\u00b7ni\u00b7ger", "war", "er", "da\u00b7zu", "ge\u00b7schickt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "ADV", "ADV", "VAFIN", "PPER", "PAV", "VVPP", "$,"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.8": {"text": "Als sie selbst von der Sonnen Strahlen", "tokens": ["Als", "sie", "selbst", "von", "der", "Son\u00b7nen", "Strah\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "NN", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.9": {"text": "Beflossen wurden und geschm\u00fcckt!", "tokens": ["Be\u00b7flos\u00b7sen", "wur\u00b7den", "und", "ge\u00b7schm\u00fcckt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "KON", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Hingegen fiel des Rosen-Busches Schatte,", "tokens": ["Hin\u00b7ge\u00b7gen", "fiel", "des", "Ro\u00b7sen\u00b7Bu\u00b7sches", "Schat\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.11": {"text": "Der sich so zierlich auf der Erden,", "tokens": ["Der", "sich", "so", "zier\u00b7lich", "auf", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Erzeuget durch der Sonnen Licht,", "tokens": ["Er\u00b7zeu\u00b7get", "durch", "der", "Son\u00b7nen", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART"