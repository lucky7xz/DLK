.12": {"text": "Nein, nur der Schatten-Rose, gleich.\u00ab", "tokens": ["Nein", ",", "nur", "der", "Schat\u00b7ten\u00b7Ro\u00b7se", ",", "gleich", ".", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["PTKANT", "$,", "ADV", "ART", "NN", "$,", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}