{"textgrid.poem.39990": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Morgen-Sonne weckte mich", "genre": "verse", "period": "N.A.", "pub_year": 1713, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Morgen-Sonne weckte mich", "tokens": ["Die", "Mor\u00b7gen\u00b7Son\u00b7ne", "weck\u00b7te", "mich"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit ihren Rosen-rothen Strahlen,", "tokens": ["Mit", "ih\u00b7ren", "Ro\u00b7sen\u00b7ro\u00b7then", "Strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und hie\u00df mich, Dem mein Fr\u00fch-Gel\u00fcbde zahlen,", "tokens": ["Und", "hie\u00df", "mich", ",", "Dem", "mein", "Fr\u00fch\u00b7Ge\u00b7l\u00fcb\u00b7de", "zah\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "ART", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Selbst der Sonnen ew'ge Sonne,", "tokens": ["Der", "Selbst", "der", "Son\u00b7nen", "ew'\u00b7ge", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der aller Anmuth, aller Wonne", "tokens": ["Der", "al\u00b7ler", "An\u00b7muth", ",", "al\u00b7ler", "Won\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PIAT", "NN", "$,", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Quell, Ursprung, ", "tokens": ["Quell", ",", "Ur\u00b7sprung", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+--", "measure": "dactylic.init"}, "line.7": {"text": "De\u00df Gr\u00f6sse man dennoch nur nach dem Schatten miss't,", "tokens": ["De\u00df", "Gr\u00f6s\u00b7se", "man", "den\u00b7noch", "nur", "nach", "dem", "Schat\u00b7ten", "miss't", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "ADV", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Derhalben gieng ich in den Garten,", "tokens": ["Der\u00b7hal\u00b7ben", "gieng", "ich", "in", "den", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und sahe gleich,", "tokens": ["Und", "sa\u00b7he", "gleich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Im bunt-gef\u00e4rbten Bluhmen-Reich,", "tokens": ["Im", "bunt\u00b7ge\u00b7f\u00e4rb\u00b7ten", "Bluh\u00b7men\u00b7Reich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Viel tausend Vorw\u00fcrf' auf mich warten.", "tokens": ["Viel", "tau\u00b7send", "Vor\u00b7w\u00fcr\u00b7f'", "auf", "mich", "war\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Es lie\u00df, als ob jedwede Bluhme,", "tokens": ["Es", "lie\u00df", ",", "als", "ob", "jed\u00b7we\u00b7de", "Bluh\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOKOM", "KOUS", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu ihrem nicht, zu ihres Sch\u00f6pfers, Ruhme,", "tokens": ["Zu", "ih\u00b7rem", "nicht", ",", "zu", "ih\u00b7res", "Sch\u00f6p\u00b7fers", ",", "Ruh\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "PTKNEG", "$,", "APPR", "PPOSAT", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Am ersten w\u00fcnscht' ger\u00fchmt zu seyn.", "tokens": ["Am", "ers\u00b7ten", "w\u00fcnscht'", "ge\u00b7r\u00fchmt", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VVFIN", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch hie\u00df der weisse Schein,", "tokens": ["Doch", "hie\u00df", "der", "weis\u00b7se", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Worin ein Rosen-Strauch, mit hellem Schimmer, brannt,", "tokens": ["Wo\u00b7rin", "ein", "Ro\u00b7sen\u00b7Strauch", ",", "mit", "hel\u00b7lem", "Schim\u00b7mer", ",", "brannt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "APPR", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mich, eine weisse Rose w\u00e4hlen,", "tokens": ["Mich", ",", "ei\u00b7ne", "weis\u00b7se", "Ro\u00b7se", "w\u00e4h\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die, ob ihr hoher Busch gleich noch im Schatten stand,", "tokens": ["Die", ",", "ob", "ihr", "ho\u00b7her", "Busch", "gleich", "noch", "im", "Schat\u00b7ten", "stand", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPOSAT", "ADJA", "NN", "ADV", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dennoch die Augen meiner Seelen", "tokens": ["Den\u00b7noch", "die", "Au\u00b7gen", "mei\u00b7ner", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "So starck auf sich zu ziehen wuste;", "tokens": ["So", "starck", "auf", "sich", "zu", "zie\u00b7hen", "wus\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PRF", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Da\u00df ich vor andern sie zuerst betrachten muste.", "tokens": ["Da\u00df", "ich", "vor", "an\u00b7dern", "sie", "zu\u00b7erst", "be\u00b7trach\u00b7ten", "mus\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PIS", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die\u00df war ein hoher Busch, de\u00df gr\u00fcne Pracht so dicht,", "tokens": ["Die\u00df", "war", "ein", "ho\u00b7her", "Busch", ",", "de\u00df", "gr\u00fc\u00b7ne", "Pracht", "so", "dicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df kaum der Wind", "tokens": ["Da\u00df", "kaum", "der", "Wind"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Sein ungez\u00e4hltes Laub durchbricht,", "tokens": ["Sein", "un\u00b7ge\u00b7z\u00e4hl\u00b7tes", "Laub", "durch\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df weder Licht, noch Luft, durch ihn den Durchgang find't,", "tokens": ["Da\u00df", "we\u00b7der", "Licht", ",", "noch", "Luft", ",", "durch", "ihn", "den", "Durch\u00b7gang", "find't", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KON", "NN", "$,", "ADV", "NN", "$,", "APPR", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Indem jedweder Zweig f\u00fcnf Neben-Zweige heget,", "tokens": ["In\u00b7dem", "jed\u00b7we\u00b7der", "Zweig", "f\u00fcnf", "Ne\u00b7ben\u00b7Zwei\u00b7ge", "he\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und jeder Neben-Zweig f\u00fcnf sch\u00f6ne Bl\u00e4tter tr\u00e4get,", "tokens": ["Und", "je\u00b7der", "Ne\u00b7ben\u00b7Zweig", "f\u00fcnf", "sch\u00f6\u00b7ne", "Bl\u00e4t\u00b7ter", "tr\u00e4\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "CARD", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die so verwunderlich verworren und verschrenckt,", "tokens": ["Die", "so", "ver\u00b7wun\u00b7der\u00b7lich", "ver\u00b7wor\u00b7ren", "und", "ver\u00b7schrenckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VVINF", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df sich das Aug' umsonst in ihre Tiefen senckt,", "tokens": ["Da\u00df", "sich", "das", "Aug'", "um\u00b7sonst", "in", "ih\u00b7re", "Tie\u00b7fen", "senckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und sich der Blick, der keinen Durchgang sp\u00fcret,", "tokens": ["Und", "sich", "der", "Blick", ",", "der", "kei\u00b7nen", "Durch\u00b7gang", "sp\u00fc\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ART", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Im gr\u00fcnen Labyrinth, doch h\u00f6chst-vergn\u00fcgt, verlieret.", "tokens": ["Im", "gr\u00fc\u00b7nen", "La\u00b7by\u00b7rinth", ",", "doch", "h\u00f6chst\u00b7ver\u00b7gn\u00fcgt", ",", "ver\u00b7lie\u00b7ret", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "ADV", "ADJD", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Allein", "tokens": ["Al\u00b7lein"], "token_info": ["word"], "pos": ["ADV"], "meter": "-+", "measure": "iambic.single"}, "line.12": {"text": "Der weissen Bluhmen weisser Schein.", "tokens": ["Der", "weis\u00b7sen", "Bluh\u00b7men", "weis\u00b7ser", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Der mir, in gr\u00fcner Bl\u00e4tter Gr\u00fcnden", "tokens": ["Der", "mir", ",", "in", "gr\u00fc\u00b7ner", "Bl\u00e4t\u00b7ter", "Gr\u00fcn\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPER", "$,", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Der Anmuth lange Dau'r zu finden,", "tokens": ["Der", "An\u00b7muth", "lan\u00b7ge", "Dau'r", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Durch gr\u00f6sser' Anmuth, nicht erlaubte,", "tokens": ["Durch", "gr\u00f6s\u00b7ser'", "An\u00b7muth", ",", "nicht", "er\u00b7laub\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Ri\u00df recht, als wie ein Licht, den Blick auf sich allein.", "tokens": ["Ri\u00df", "recht", ",", "als", "wie", "ein", "Licht", ",", "den", "Blick", "auf", "sich", "al\u00b7lein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "KOUS", "KOKOM", "ART", "NN", "$,", "ART", "NN", "APPR", "PRF", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Mein Gott, wie ward mein Hertz ger\u00fchret,", "tokens": ["Mein", "Gott", ",", "wie", "ward", "mein", "Hertz", "ge\u00b7r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Da ich, im Schimmer, der ihn zieret,", "tokens": ["Da", "ich", ",", "im", "Schim\u00b7mer", ",", "der", "ihn", "zie\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPRART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Als wie am gr\u00fcnen Firmament,", "tokens": ["Als", "wie", "am", "gr\u00fc\u00b7nen", "Fir\u00b7ma\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "(woran der Rosen Heer, wie weisse Lichter, brennt)", "tokens": ["(", "wo\u00b7ran", "der", "Ro\u00b7sen", "Heer", ",", "wie", "weis\u00b7se", "Lich\u00b7ter", ",", "brennt", ")"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PWAV", "ART", "NN", "NN", "$,", "PWAV", "ADJA", "NN", "$,", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ein schimmernd Sternen-Heer zu sehen, glaubte.", "tokens": ["Ein", "schim\u00b7mernd", "Ster\u00b7nen\u00b7Heer", "zu", "se\u00b7hen", ",", "glaub\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJD", "NN", "PTKZU", "VVINF", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wie kleine Monden, gl\u00e4ntzt die Schaar", "tokens": ["Wie", "klei\u00b7ne", "Mon\u00b7den", ",", "gl\u00e4ntzt", "die", "Schaar"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Rosen, die schon gantz ge\u00f6ffnet war,", "tokens": ["Der", "Ro\u00b7sen", ",", "die", "schon", "gantz", "ge\u00b7\u00f6ff\u00b7net", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Indem die, so noch halb geschlossen sitzen,", "tokens": ["In\u00b7dem", "die", ",", "so", "noch", "halb", "ge\u00b7schlos\u00b7sen", "sit\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "$,", "ADV", "ADV", "ADJD", "VVPP", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gleich Sternen erster Gr\u00f6sse, blitzen.", "tokens": ["Gleich", "Ster\u00b7nen", "ers\u00b7ter", "Gr\u00f6s\u00b7se", ",", "blit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "NN", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die andern, die in gr\u00f6ss'rer Zahl,", "tokens": ["Die", "an\u00b7dern", ",", "die", "in", "gr\u00f6ss'\u00b7rer", "Zahl", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit einem f\u00fcnf-getheilten Strahl,", "tokens": ["Mit", "ei\u00b7nem", "f\u00fcn\u00b7fget\u00b7heil\u00b7ten", "Strahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Durch noch geschloss'ne Knospen, funckeln;", "tokens": ["Durch", "noch", "ge\u00b7schloss'\u00b7ne", "Knos\u00b7pen", ",", "fun\u00b7ckeln", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,", 