": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "War, welches nicht,", "tokens": ["War", ",", "wel\u00b7ches", "nicht", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PRELS", "PTKNEG", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Durch das gehemmte Sonnen-Licht,", "tokens": ["Durch", "das", "ge\u00b7hemm\u00b7te", "Son\u00b7nen\u00b7Licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sich auf dem Boden selbst gezeichnet hatt'.", "tokens": ["Sich", "auf", "dem", "Bo\u00b7den", "selbst", "ge\u00b7zeich\u00b7net", "hatt'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die netten Bildungen so vieler Kleinigkeiten,", "tokens": ["Die", "net\u00b7ten", "Bil\u00b7dun\u00b7gen", "so", "vie\u00b7ler", "Klei\u00b7nig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Die sich im Augenblick erzeugen und bereiten,", "tokens": ["Die", "sich", "im", "Au\u00b7gen\u00b7blick", "er\u00b7zeu\u00b7gen", "und", "be\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPRART", "NN", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Vergn\u00fcg'ten mich", "tokens": ["Ver\u00b7gn\u00fcg'\u00b7ten", "mich"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Recht inniglich.", "tokens": ["Recht", "in\u00b7nig\u00b7lich", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADJD", "$."], "meter": "+---", "measure": "dactylic.init"}}, "stanza.8": {"line.1": {"text": "Doch, da ich noch besch\u00e4fftigt stand;", "tokens": ["Doch", ",", "da", "ich", "noch", "be\u00b7sch\u00e4ff\u00b7tigt", "stand", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sah' ich, wie Strahl und Licht im Augenblick verschwand,", "tokens": ["Sah'", "ich", ",", "wie", "Strahl", "und", "Licht", "im", "Au\u00b7gen\u00b7blick", "ver\u00b7schwand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "NN", "KON", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und, samt dem Schatten-Busch, der andern Zierlichkeiten", "tokens": ["Und", ",", "samt", "dem", "Schat\u00b7ten\u00b7Busch", ",", "der", "an\u00b7dern", "Zier\u00b7lich\u00b7kei\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "APPR", "ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Geschwinde Zeichnung ja so schnell.", "tokens": ["Ge\u00b7schwin\u00b7de", "Zeich\u00b7nung", "ja", "so", "schnell", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was pl\u00f6tzlich ward, ward pl\u00f6tzlich nichts,", "tokens": ["Was", "pl\u00f6tz\u00b7lich", "ward", ",", "ward", "pl\u00f6tz\u00b7lich", "nichts", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "VAFIN", "ADJD", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Indem der Strahl des hellen Sonnen-Lichts,", "tokens": ["In\u00b7dem", "der", "Strahl", "des", "hel\u00b7len", "Son\u00b7nen\u00b7Lichts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der Vater von dem schnell erzeugten dunckeln Kinde,", "tokens": ["Der", "Va\u00b7ter", "von", "dem", "schnell", "er\u00b7zeug\u00b7ten", "dun\u00b7ckeln", "Kin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJD", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da ein schnell laufendes Gew\u00f6lck ihn schnell verdeckte;", "tokens": ["Da", "ein", "schnell", "lau\u00b7fen\u00b7des", "Ge\u00b7w\u00f6lck", "ihn", "schnell", "ver\u00b7deck\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJD", "ADJA", "NN", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die\u00df Schatten-Bild zugleich im Augenblick versteckte.", "tokens": ["Die\u00df", "Schat\u00b7ten\u00b7Bild", "zu\u00b7gleich", "im", "Au\u00b7gen\u00b7blick", "ver\u00b7steck\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Wo nun von unserer Verg\u00e4nglichkeit", "tokens": ["Wo", "nun", "von", "un\u00b7se\u00b7rer", "Ver\u00b7g\u00e4ng\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Man auf der Welt ein gleiches Conterfait", "tokens": ["Man", "auf", "der", "Welt", "ein", "glei\u00b7ches", "Con\u00b7ter\u00b7fait"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In einem Dinge finden kann:", "tokens": ["In", "ei\u00b7nem", "Din\u00b7ge", "fin\u00b7den", "kann", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So traf ich es in dieser Bildung an.", "tokens": ["So", "traf", "ich", "es", "in", "die\u00b7ser", "Bil\u00b7dung", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich sprach, so bald ich \u00fcberleget hatte:", "tokens": ["Ich", "sprach", ",", "so", "bald", "ich", "\u00fc\u00b7ber\u00b7le\u00b7get", "hat\u00b7te", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "ADV", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "\u00bbdie Rose, welche selbst so sehr ver\u00e4nderlich,", "tokens": ["\u00bb", "die", "Ro\u00b7se", ",", "wel\u00b7che", "selbst", "so", "sehr", "ver\u00b7\u00e4n\u00b7der\u00b7lich", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "PRELS", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ist doch so fl\u00fcchtig nicht, als ihr so fl\u00fccht'ger Schatte.", "tokens