{"dta.poem.23322": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der K\u00f6nig wei\u00df es nicht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie ist des Elends in der Welt", "tokens": ["Wie", "ist", "des", "E\u00b7lends", "in", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So viel und mancherlei!", "tokens": ["So", "viel", "und", "man\u00b7cher\u00b7lei", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "PIS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und dennoch giebt man soviel Geld,", "tokens": ["Und", "den\u00b7noch", "giebt", "man", "so\u00b7viel", "Geld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das; jeder gl\u00fccklich sei.", "tokens": ["Das", ";", "je\u00b7der", "gl\u00fcck\u00b7lich", "sei", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$.", "PIS", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ach! wer das Elend einmal sah,", "tokens": ["Ach", "!", "wer", "das", "E\u00b7lend", "ein\u00b7mal", "sah", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich wei\u00df, das Herz ihm bricht.", "tokens": ["Ich", "wei\u00df", ",", "das", "Herz", "ihm", "bricht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Was hilft's? ihr saget immer: ja,", "tokens": ["Was", "hilft's", "?", "ihr", "sa\u00b7get", "im\u00b7mer", ":", "ja", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "$.", "PPER", "VVFIN", "ADV", "$.", "PTKANT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der K\u00f6nig wei\u00df es nicht.", "tokens": ["Der", "K\u00f6\u00b7nig", "wei\u00df", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie geht das Unrecht allezeit", "tokens": ["Wie", "geht", "das", "Un\u00b7recht", "al\u00b7le\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So sicher doch umher!", "tokens": ["So", "si\u00b7cher", "doch", "um\u00b7her", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie ist doch die Gerechtigkeit", "tokens": ["Wie", "ist", "doch", "die", "Ge\u00b7rech\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So theuer und so schwer!", "tokens": ["So", "theu\u00b7er", "und", "so", "schwer", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Warum giebt's soviel Unrecht noch?", "tokens": ["Wa\u00b7rum", "giebt's", "so\u00b7viel", "Un\u00b7recht", "noch", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PIAT", "NN", "ADV", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "So manchen B\u00f6sewicht?", "tokens": ["So", "man\u00b7chen", "B\u00f6\u00b7se\u00b7wicht", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich wei\u00df, ihr wi\u00dft es alle, doch", "tokens": ["Ich", "wei\u00df", ",", "ihr", "wi\u00dft", "es", "al\u00b7le", ",", "doch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "PIS", "$,", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der K\u00f6nig wei\u00df es nicht.", "tokens": ["Der", "K\u00f6\u00b7nig", "wei\u00df", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wie das Verdienst so wenig gilt", "tokens": ["Wie", "das", "Ver\u00b7dienst", "so", "we\u00b7nig", "gilt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADV", "PIS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und doch Geburt so viel!", "tokens": ["Und", "doch", "Ge\u00b7burt", "so", "viel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ist das nach Christi Ebenbild?", "tokens": ["Ist", "das", "nach", "Chris\u00b7ti", "E\u00b7ben\u00b7bild", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das unsrer Liebe Ziel?", "tokens": ["Das", "uns\u00b7rer", "Lie\u00b7be", "Ziel", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ist Adel denn ein Vorzug noch?", "tokens": ["Ist", "A\u00b7del", "denn", "ein", "Vor\u00b7zug", "noch", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So gebt mir doch Bericht!", "tokens": ["So", "gebt", "mir", "doch", "Be\u00b7richt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich wei\u00df, ihr wi\u00dft es alle, doch", "tokens": ["Ich", "wei\u00df", ",", "ihr", "wi\u00dft", "es", "al\u00b7le", ",", "doch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "PIS", "$,", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der K\u00f6nig wei\u00df es nicht.", "tokens": ["Der", "K\u00f6\u00b7nig", "wei\u00df", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}