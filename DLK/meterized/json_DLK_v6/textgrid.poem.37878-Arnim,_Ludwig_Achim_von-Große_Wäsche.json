{"textgrid.poem.37878": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Gro\u00dfe W\u00e4sche", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Mai will sich mit Gunsten,", "tokens": ["Der", "Mai", "will", "sich", "mit", "Guns\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit Gunsten beweisen,", "tokens": ["Mit", "Guns\u00b7ten", "be\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Pr\u00fcf' ich an aller V\u00f6gelein Gesang,", "tokens": ["Pr\u00fcf'", "ich", "an", "al\u00b7ler", "V\u00f6\u00b7ge\u00b7lein", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Sommer k\u00f6mmt, vor nicht gar lang", "tokens": ["Der", "Som\u00b7mer", "k\u00f6mmt", ",", "vor", "nicht", "gar", "lang"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "APPR", "PTKNEG", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "H\u00f6rt ich Frau Nachtigal singen,", "tokens": ["H\u00f6rt", "ich", "Frau", "Nach\u00b7ti\u00b7gal", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "NN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Sie sang recht wie ein Saitenspiel:", "tokens": ["Sie", "sang", "recht", "wie", "ein", "Sai\u00b7ten\u00b7spiel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "\u00bbder Mai bald will", "tokens": ["\u00bb", "der", "Mai", "bald", "will"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "ART", "NN", "ADV", "VMFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Den lichten Sommer bringen, und zwingen", "tokens": ["Den", "lich\u00b7ten", "Som\u00b7mer", "brin\u00b7gen", ",", "und", "zwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,", "KON", "VVINF"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Die Jungfr\u00e4ulein zu Springen und Singen.", "tokens": ["Die", "Jung\u00b7fr\u00e4u\u00b7lein", "zu", "Sprin\u00b7gen", "und", "Sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Jedoch so sind die Kleider", "tokens": ["Je\u00b7doch", "so", "sind", "die", "Klei\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mir leider zerrissen,", "tokens": ["Mir", "lei\u00b7der", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Ich sch\u00e4me mich vor andrer M\u00e4gdlein Schaar,", "tokens": ["Ich", "sch\u00e4\u00b7me", "mich", "vor", "an\u00b7drer", "M\u00e4gd\u00b7lein", "Schaar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit meinen Schenklein geh ich bar,", "tokens": ["Mit", "mei\u00b7nen", "Schen\u00b7klein", "geh", "ich", "bar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Weil ich grad waschen wollte,", "tokens": ["Weil", "ich", "grad", "wa\u00b7schen", "woll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Reif und auch der kalte Schnee", "tokens": ["Der", "Reif", "und", "auch", "der", "kal\u00b7te", "Schnee"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "That mir wohl weh,", "tokens": ["That", "mir", "wohl", "weh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Ich will als Waschgesellen bestellen,", "tokens": ["Ich", "will", "als", "Waschge\u00b7sel\u00b7len", "be\u00b7stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KOUS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Die Jungfraun an den hellen Waldquellen.", "tokens": ["Die", "Jung\u00b7fraun", "an", "den", "hel\u00b7len", "Wald\u00b7quel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}}, "stanza.3": {"line.1": {"text": "Komm, komm, lieb, lieb, Agnette,", "tokens": ["Komm", ",", "komm", ",", "lieb", ",", "lieb", ",", "Ag\u00b7net\u00b7te", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "ADJD", "$,", "ADJD", "$,", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Margretha, Sophia,", "tokens": ["Mar\u00b7gre\u00b7tha", ",", "So\u00b7phia", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Elisabetha, Amaleya traut,", "tokens": ["E\u00b7lis\u00b7a\u00b7be\u00b7tha", ",", "A\u00b7ma\u00b7le\u00b7ya", "traut", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sibilla, Lilla, Frau Gertraut,", "tokens": ["Si\u00b7bil\u00b7la", ",", "Lil\u00b7la", ",", "Frau", "Ger\u00b7traut", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Kommt bald ihr M\u00e4gdlein sch\u00f6ne,", "tokens": ["Kommt", "bald", "ihr", "M\u00e4gd\u00b7lein", "sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Kommt bald und wascht euch s\u00e4uberlich,", "tokens": ["Kommt", "bald", "und", "wascht", "euch", "s\u00e4u\u00b7ber\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KON", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und schm\u00fccket mich.\u00ab", "tokens": ["Und", "schm\u00fc\u00b7cket", "mich", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Da kamen die Jungfrauen im Thaue", "tokens": ["Da", "ka\u00b7men", "die", "Jung\u00b7frau\u00b7en", "im", "Thau\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+---+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Sich waschen und beschauen, ja schauen.", "tokens": ["Sich", "wa\u00b7schen", "und", "be\u00b7schau\u00b7en", ",", "ja", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "VVINF", "KON", "VVINF", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Ich dank Frau Nachtigallen,", "tokens": ["Ich", "dank", "Frau", "Nach\u00b7ti\u00b7gal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vor Allen mein Gl\u00fccke,", "tokens": ["Vor", "Al\u00b7len", "mein", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Da\u00df sie zum Waschen rief die holde Schaar,", "tokens": ["Da\u00df", "sie", "zum", "Wa\u00b7schen", "rief", "die", "hol\u00b7de", "Schaar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit ihren Schenklein giengens bar,", "tokens": ["Mit", "ih\u00b7ren", "Schen\u00b7klein", "gien\u00b7gens", "bar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das Wasser ward nicht tr\u00fcbe,", "tokens": ["Das", "Was\u00b7ser", "ward", "nicht", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Jugendglanz, der Maienschnee", "tokens": ["Der", "Ju\u00b7gend\u00b7glanz", ",", "der", "Mai\u00b7en\u00b7schnee"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "That ihm nicht weh;", "tokens": ["That", "ihm", "nicht", "weh", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Doch mich wirds nicht mehr k\u00fchlen im Schw\u00fclen,", "tokens": ["Doch", "mich", "wirds", "nicht", "mehr", "k\u00fch\u00b7len", "im", "Schw\u00fc\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PTKNEG", "ADV", "VVINF", "APPRART", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.9": {"text": "Im Sommer werd ichs f\u00fchlen, ja f\u00fchlen.", "tokens": ["Im", "Som\u00b7mer", "werd", "ichs", "f\u00fch\u00b7len", ",", "ja", "f\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PIS", "VVINF", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}