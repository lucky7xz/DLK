{"textgrid.poem.43120": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: In Deinem d\u00fcstern Zimmer steh' ich wieder,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Deinem d\u00fcstern Zimmer steh' ich wieder,", "tokens": ["In", "Dei\u00b7nem", "d\u00fcs\u00b7tern", "Zim\u00b7mer", "steh'", "ich", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom Fenster hauchen schw\u00fcl die Blumend\u00fcfte,", "tokens": ["Vom", "Fens\u00b7ter", "hau\u00b7chen", "schw\u00fcl", "die", "Blu\u00b7men\u00b7d\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Sterne schauen gro\u00df und ruhig nieder", "tokens": ["Die", "Ster\u00b7ne", "schau\u00b7en", "gro\u00df", "und", "ru\u00b7hig", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und m\u00fcde T\u00f6ne zittern durch die L\u00fcfte.", "tokens": ["Und", "m\u00fc\u00b7de", "T\u00f6\u00b7ne", "zit\u00b7tern", "durch", "die", "L\u00fcf\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ach! tiefe Wehmuth folgt dem Lied dem langen,", "tokens": ["Ach", "!", "tie\u00b7fe", "Weh\u00b7muth", "folgt", "dem", "Lied", "dem", "lan\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADJA", "NN", "VVFIN", "ART", "NN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich f\u00fchle klar, was einstens ich nicht kannte,", "tokens": ["Ich", "f\u00fch\u00b7le", "klar", ",", "was", "eins\u00b7tens", "ich", "nicht", "kann\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "PRELS", "ADV", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als schweigend hier mit demuthsvollem Bangen", "tokens": ["Als", "schwei\u00b7gend", "hier", "mit", "de\u00b7muths\u00b7vol\u00b7lem", "Ban\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "ADV", "APPR", "ART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ich harrte Dein \u2013 der mich in's Leben sandte.", "tokens": ["Ich", "harr\u00b7te", "Dein", "\u2013", "der", "mich", "in's", "Le\u00b7ben", "sand\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "$(", "PRELS", "PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Mich d\u00fcnket, wieder m\u00fc\u00dft' ich Dich gewahren", "tokens": ["Mich", "d\u00fcn\u00b7ket", ",", "wie\u00b7der", "m\u00fc\u00dft'", "ich", "Dich", "ge\u00b7wah\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ADV", "VMFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An diesem Ort, wo Du so viel verschuldet,", "tokens": ["An", "die\u00b7sem", "Ort", ",", "wo", "Du", "so", "viel", "ver\u00b7schul\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PWAV", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als br\u00e4che hei\u00df hervor nach d\u00fcrren Jahren", "tokens": ["Als", "br\u00e4\u00b7che", "hei\u00df", "her\u00b7vor", "nach", "d\u00fcr\u00b7ren", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "ADJD", "PTKVZ", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In Thr\u00e4nen hier \u2013 was drau\u00dfen ich erduldet.", "tokens": ["In", "Thr\u00e4\u00b7nen", "hier", "\u2013", "was", "drau\u00b7\u00dfen", "ich", "er\u00b7dul\u00b7det", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "$(", "PWS", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}