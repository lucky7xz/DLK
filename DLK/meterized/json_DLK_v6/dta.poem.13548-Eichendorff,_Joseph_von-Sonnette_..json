{"dta.poem.13548": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Sonnette .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Viel goldne Br\u00fccken sind dort k\u00fchn geschlagen,", "tokens": ["Viel", "gold\u00b7ne", "Br\u00fc\u00b7cken", "sind", "dort", "k\u00fchn", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+++-+-", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Dar\u00fcber alte Br\u00fcder sinnend wallen \u2014", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "al\u00b7te", "Br\u00fc\u00b7der", "sin\u00b7nend", "wal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJA", "NN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn T\u00f6ne wie im Fr\u00fchlingsregen fallen,", "tokens": ["Wenn", "T\u00f6\u00b7ne", "wie", "im", "Fr\u00fch\u00b7lings\u00b7re\u00b7gen", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KOKOM", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Befreite Sehnsucht will dorthin dich tragen.", "tokens": ["Be\u00b7frei\u00b7te", "Sehn\u00b7sucht", "will", "dor\u00b7thin", "dich", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}