{"textgrid.poem.57070": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Die Stationen", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcberall, auf allen Stationen", "tokens": ["\u00dc\u00b7be\u00b7rall", ",", "auf", "al\u00b7len", "Sta\u00b7ti\u00b7o\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "ruft der Mensch den Namen der Station,", "tokens": ["ruft", "der", "Mensch", "den", "Na\u00b7men", "der", "Sta\u00b7ti\u00b7on", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "\u00fcberall, wo Bahnbeamte wohnen,", "tokens": ["\u00fc\u00b7be\u00b7rall", ",", "wo", "Bahn\u00b7be\u00b7am\u00b7te", "woh\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "schallt es K\u00f6pnick oder Iserlohn.", "tokens": ["schallt", "es", "K\u00f6p\u00b7nick", "o\u00b7der", "I\u00b7ser\u00b7lohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Wohl der Stadt, die Gott tut so belohnen:", "tokens": ["Wohl", "der", "Stadt", ",", "die", "Gott", "tut", "so", "be\u00b7loh\u00b7nen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ART", "NN", "VVFIN", "ADV", "VVINF", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Nicht im Stein nur lebt sie, auch im Ton!", "tokens": ["Nicht", "im", "Stein", "nur", "lebt", "sie", ",", "auch", "im", "Ton", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "ADV", "VVFIN", "PPER", "$,", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "T\u00e4glich vielmals wird sie laut verk\u00fcndet", "tokens": ["T\u00e4g\u00b7lich", "viel\u00b7mals", "wird", "sie", "laut", "ver\u00b7k\u00fcn\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "VAFIN", "PPER", "ADJD", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "und dem Hirn des Passagiers verb\u00fcndet.", "tokens": ["und", "dem", "Hirn", "des", "Pas\u00b7sa\u00b7giers", "ver\u00b7b\u00fcn\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Selbst des Nachts, wo sonst nur Diebe munkeln,", "tokens": ["Selbst", "des", "Nachts", ",", "wo", "sonst", "nur", "Die\u00b7be", "mun\u00b7keln", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADV", "$,", "PWAV", "ADV", "ADV", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "h\u00f6rt man: K\u00f6tzschenbroda, Schrimm, Kamenz,", "tokens": ["h\u00f6rt", "man", ":", "K\u00f6tz\u00b7schen\u00b7bro\u00b7da", ",", "Schrimm", ",", "Ka\u00b7menz", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PIS", "$.", "NE", "$,", "NE", "$,", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "sieht man Augen, Kn\u00f6pfe, Fenster funkeln;", "tokens": ["sieht", "man", "Au\u00b7gen", ",", "Kn\u00f6p\u00b7fe", ",", "Fens\u00b7ter", "fun\u00b7keln", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "NN", "$,", "NN", "$,", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "kein Stati\u00f6nchen ist so klein \u2013 man nennt's!", "tokens": ["kein", "Sta\u00b7ti\u00b7\u00f6n\u00b7chen", "ist", "so", "klein", "\u2013", "man", "nennt's", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "ADJD", "$(", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Prenzlau, Bunzlau kennt man selbst im Dunkeln", "tokens": ["Prenz\u00b7lau", ",", "Bunz\u00b7lau", "kennt", "man", "selbst", "im", "Dun\u00b7keln"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "NN", "VVFIN", "PIS", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "dank des Dampfs verbindender Tendenz.", "tokens": ["dank", "des", "Dampfs", "ver\u00b7bin\u00b7den\u00b7der", "Ten\u00b7denz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Nur die D\u00f6rfer seitw\u00e4rts liegen stille ...", "tokens": ["Nur", "die", "D\u00f6r\u00b7fer", "seit\u00b7w\u00e4rts", "lie\u00b7gen", "stil\u00b7le", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "VVFIN", "ADJA", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Doch getrost, auch dies ist Gottes Wille.", "tokens": ["Doch", "ge\u00b7trost", ",", "auch", "dies", "ist", "Got\u00b7tes", "Wil\u00b7le", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "$,", "ADV", "PDS", "VAFIN", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}