{"textgrid.poem.61203": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des eiteln Jammers trug ich immer Scham,", "tokens": ["Des", "ei\u00b7teln", "Jam\u00b7mers", "trug", "ich", "im\u00b7mer", "Scham", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Doch nicht err\u00f6t' ich \u00fcber diese Z\u00e4hre;", "tokens": ["Doch", "nicht", "er\u00b7r\u00f6t'", "ich", "\u00fc\u00b7ber", "die\u00b7se", "Z\u00e4h\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VVFIN", "PPER", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Achill, der G\u00f6tter Enkel, weint' am Meere,", "tokens": ["A\u00b7chill", ",", "der", "G\u00f6t\u00b7ter", "En\u00b7kel", ",", "weint'", "am", "Mee\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "NN", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Da seine Mutter ihn zu tr\u00f6sten kam.", "tokens": ["Da", "sei\u00b7ne", "Mut\u00b7ter", "ihn", "zu", "tr\u00f6s\u00b7ten", "kam", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch war das Leid, das ihn gefangennahm,", "tokens": ["Doch", "war", "das", "Leid", ",", "das", "ihn", "ge\u00b7fan\u00b7gen\u00b7nahm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nicht meinem gleich an Bitterkeit und Schwere;", "tokens": ["Nicht", "mei\u00b7nem", "gleich", "an", "Bit\u00b7ter\u00b7keit", "und", "Schwe\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er weint' im Zorn um seine Lieb' und Ehre,", "tokens": ["Er", "weint'", "im", "Zorn", "um", "sei\u00b7ne", "Lieb'", "und", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPR", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich weint' um meines Vaterlandes Gram.", "tokens": ["Ich", "weint'", "um", "mei\u00b7nes", "Va\u00b7ter\u00b7lan\u00b7des", "Gram", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch nun genug! Jetzt gilt es, sich zu fassen", "tokens": ["Doch", "nun", "ge\u00b7nug", "!", "Jetzt", "gilt", "es", ",", "sich", "zu", "fas\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "$.", "ADV", "VVFIN", "PPER", "$,", "PRF", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und nicht, ein h\u00e4nderingender Tribun,", "tokens": ["Und", "nicht", ",", "ein", "h\u00e4n\u00b7de\u00b7rin\u00b7gen\u00b7der", "Tri\u00b7bun", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den L\u00e4rm noch zu vergr\u00f6\u00dfern auf den Gassen.", "tokens": ["Den", "L\u00e4rm", "noch", "zu", "ver\u00b7gr\u00f6\u00b7\u00dfern", "auf", "den", "Gas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKZU", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Kannst du nicht handeln, la\u00df die Worte ruhn;", "tokens": ["Kannst", "du", "nicht", "han\u00b7deln", ",", "la\u00df", "die", "Wor\u00b7te", "ruhn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVINF", "$,", "VVIMP", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und lerne, wo nicht freudig, doch gelassen", "tokens": ["Und", "ler\u00b7ne", ",", "wo", "nicht", "freu\u00b7dig", ",", "doch", "ge\u00b7las\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PTKNEG", "ADJD", "$,", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und fest das Unab\u00e4nderliche tun.", "tokens": ["Und", "fest", "das", "Un\u00b7ab\u00b7\u00e4n\u00b7der\u00b7li\u00b7che", "tun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}