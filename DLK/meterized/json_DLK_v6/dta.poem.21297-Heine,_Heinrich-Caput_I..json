{"dta.poem.21297": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput I.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ja, Zuckererbsen f\u00fcr Jedermann,", "tokens": ["Ja", ",", "Zu\u00b7cker\u00b7erb\u00b7sen", "f\u00fcr", "Je\u00b7der\u00b7mann", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "APPR", "PIS", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sobald die Schooten platzen!", "tokens": ["So\u00b7bald", "die", "Schoo\u00b7ten", "plat\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den Himmel \u00fcberlassen wir", "tokens": ["Den", "Him\u00b7mel", "\u00fc\u00b7ber\u00b7las\u00b7sen", "wir"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Engeln und den Spatzen.", "tokens": ["Den", "En\u00b7geln", "und", "den", "Spat\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und wachsen uns Fl\u00fcgel nach dem Tod,", "tokens": ["Und", "wach\u00b7sen", "uns", "Fl\u00fc\u00b7gel", "nach", "dem", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So wollen wir Euch besuchen", "tokens": ["So", "wol\u00b7len", "wir", "Euch", "be\u00b7su\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dort oben, und wir wir essen mit Euch", "tokens": ["Dort", "o\u00b7ben", ",", "und", "wir", "wir", "es\u00b7sen", "mit", "Euch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "KON", "PPER", "PPER", "VVFIN", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die seligsten Torten und Kuchen.", "tokens": ["Die", "se\u00b7ligs\u00b7ten", "Tor\u00b7ten", "und", "Ku\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Ein neues Lied, ein besseres Lied,", "tokens": ["Ein", "neu\u00b7es", "Lied", ",", "ein", "bes\u00b7se\u00b7res", "Lied", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Es klingt wie Fl\u00f6ten und Geigen!", "tokens": ["Es", "klingt", "wie", "Fl\u00f6\u00b7ten", "und", "Gei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das Miserere ist vorbey,", "tokens": ["Das", "Mi\u00b7se\u00b7re\u00b7re", "ist", "vor\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Sterbeglocken schweigen.", "tokens": ["Die", "Ster\u00b7be\u00b7glo\u00b7cken", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Jungfer Europa ist verlobt", "tokens": ["Die", "Jung\u00b7fer", "Eu\u00b7ro\u00b7pa", "ist", "ver\u00b7lobt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "VAFIN", "VVPP"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit dem sch\u00f6nen Geniusse", "tokens": ["Mit", "dem", "sch\u00f6\u00b7nen", "Ge\u00b7ni\u00b7us\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der Freiheit, sie liegen einander im Arm,", "tokens": ["Der", "Frei\u00b7heit", ",", "sie", "lie\u00b7gen", "ein\u00b7an\u00b7der", "im", "Arm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Sie schwelgen im ersten Kusse.", "tokens": ["Sie", "schwel\u00b7gen", "im", "ers\u00b7ten", "Kus\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}