{"dta.poem.28": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die  \n  Stimme GOttes  \n im Hagel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198797", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Luft ist eine R\u00fcstungskammer, dar-\nin der K\u00f6nig aller Welt,", "tokens": ["Die", "Luft", "ist", "ei\u00b7ne", "R\u00fcs\u00b7tungs\u00b7kam\u00b7mer", ",", "dar", "in", "der", "K\u00f6\u00b7nig", "al\u00b7ler", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "TRUNC", "APPR", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+--+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Der starke Zebaoth die Waffen, wo-", "tokens": ["Der", "star\u00b7ke", "Ze\u00b7bao\u00b7th", "die", "Waf\u00b7fen", ",", "wo"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die seiner Allmacht wiederstehn; er\u00f6f-", "tokens": ["Die", "sei\u00b7ner", "All\u00b7macht", "wie\u00b7ders\u00b7tehn", ";", "er\u00b7\u00f6f"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "PPOSAT", "NN", "VVINF", "$.", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So h\u00f6rt man die Gerechtigkeit, mit ihrer starken", "tokens": ["So", "h\u00f6rt", "man", "die", "Ge\u00b7rech\u00b7tig\u00b7keit", ",", "mit", "ih\u00b7rer", "star\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ART", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das sind die br\u00fcllenden Carthaunen, die mit den", "tokens": ["Das", "sind", "die", "br\u00fcl\u00b7len\u00b7den", "Car\u00b7thau\u00b7nen", ",", "die", "mit", "den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Aus den von Schwefel schwangren L\u00fcften, als wie", "tokens": ["Aus", "den", "von", "Schwe\u00b7fel", "schwang\u00b7ren", "L\u00fcf\u00b7ten", ",", "als", "wie"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "APPR", "NN", "ADJA", "NN", "$,", "KOUS", "KOKOM"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "Das siedend Feur, der schnelle Bliz entflammt bey", "tokens": ["Das", "sie\u00b7dend", "Feur", ",", "der", "schnel\u00b7le", "Bliz", "ent\u00b7flammt", "bey"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "CARD", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "APPR"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und droht die b\u00f6se sichre Welt, in grausen Dam-", "tokens": ["Und", "droht", "die", "b\u00f6\u00b7se", "sich\u00b7re", "Welt", ",", "in", "grau\u00b7sen", "Dam"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$,", "APPR", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dies Zeughaus des erhabnen Sch\u00f6pfers, ist auch", "tokens": ["Dies", "Zeug\u00b7haus", "des", "er\u00b7hab\u00b7nen", "Sch\u00f6p\u00b7fers", ",", "ist", "auch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "NN", "ART", "ADJA", "NN", "$,", "VAFIN", "ADV"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.10": {"text": "Das sind die runden Hagelk\u00f6rner, die wenn der", "tokens": ["Das", "sind", "die", "run\u00b7den", "Ha\u00b7gel\u00b7k\u00f6r\u00b7ner", ",", "die", "wenn", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "KOUS", "ART"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Mit einem pfeiffenden Geheul, von Wind und Wir-", "tokens": ["Mit", "ei\u00b7nem", "pfeif\u00b7fen\u00b7den", "Ge\u00b7heul", ",", "von", "Wind", "und", "Wir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPR", "NN", "KON", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "In einen Augenblik bereit die Strafgerichte auszu-", "tokens": ["In", "ei\u00b7nen", "Au\u00b7gen\u00b7blik", "be\u00b7reit", "die", "Straf\u00b7ge\u00b7rich\u00b7te", "aus\u00b7zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.13": {"text": "Die die Gerechtigkeit befiehlet. Sie werden in der", "tokens": ["Die", "die", "Ge\u00b7rech\u00b7tig\u00b7keit", "be\u00b7fieh\u00b7let", ".", "Sie", "wer\u00b7den", "in", "der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "VVFIN", "$.", "PPER", "VAFIN", "APPR", "ART"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Aus denen flatterhaften Flokken des Schnees die", "tokens": ["Aus", "de\u00b7nen", "flat\u00b7ter\u00b7haf\u00b7ten", "Flok\u00b7ken", "des", "Schnees", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "ADJA", "NN", "ART", "NN", "ART"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Der sie in ihren Fall auff\u00e4ngt, verh\u00e4rtet und zu-", "tokens": ["Der", "sie", "in", "ih\u00b7ren", "Fall", "auf\u00b7f\u00e4ngt", ",", "ver\u00b7h\u00e4r\u00b7tet", "und", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$,", "VVPP", "KON", "TRUNC"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.16": {"text": "Wie durch Erfahrung woll gepr\u00fcft, der lehrt, der", "tokens": ["Wie", "durch", "Er\u00b7fah\u00b7rung", "woll", "ge\u00b7pr\u00fcft", ",", "der", "lehrt", ",", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PWAV", "APPR", "NN", "ADV", "VVPP", "$,", "PRELS", "VVFIN", "$,", "PRELS"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Die Kugeln des gefrornen Regens, die Klumpen", "tokens": ["Die", "Ku\u00b7geln", "des", "ge\u00b7fror\u00b7nen", "Re\u00b7gens", ",", "die", "Klum\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Die nach des Allerh\u00f6chsten Willen, auf seiner stren-", "tokens": ["Die", "nach", "des", "Al\u00b7ler\u00b7h\u00f6chs\u00b7ten", "Wil\u00b7len", ",", "auf", "sei\u00b7ner", "stren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "$,", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Mit h\u00e4uffig prasselnden Ger\u00e4usch, wenn wir vor", "tokens": ["Mit", "h\u00e4uf\u00b7fig", "pras\u00b7seln\u00b7den", "Ge\u00b7r\u00e4usch", ",", "wenn", "wir", "vor"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,", "KOUS", "PPER", "APPR"], "meter": "-+-+---+--+", "measure": "iambic.tetra.chol"}, "line.20": {"text": "Der gr\u00fcnen Hofnung f\u00fcsse Frucht, der Aekker Korn", "tokens": ["Der", "gr\u00fc\u00b7nen", "Hof\u00b7nung", "f\u00fcs\u00b7se", "Frucht", ",", "der", "A\u00b7ek\u00b7ker", "Korn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "Wenn solch ein strenges Hagel-Wetter, das Land", "tokens": ["Wenn", "solch", "ein", "stren\u00b7ges", "Ha\u00b7gel\u00b7Wet\u00b7ter", ",", "das", "Land"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PIAT", "ART", "ADJA", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.22": {"text": "Bey dik geschw\u00e4rzten Regenwolken, im Fr\u00fchling", "tokens": ["Bey", "dik", "ge\u00b7schw\u00e4rz\u00b7ten", "Re\u00b7gen\u00b7wol\u00b7ken", ",", "im", "Fr\u00fch\u00b7ling"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NE", "ADJA", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "So merkt der Landman da\u00df ein ", "tokens": ["So", "merkt", "der", "Land\u00b7man", "da\u00df", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "KOUS", "ART"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.24": {"text": "Da\u00df in dem Reiche der Natur, ein Herrscher der", "tokens": ["Da\u00df", "in", "dem", "Rei\u00b7che", "der", "Na\u00b7tur", ",", "ein", "Herr\u00b7scher", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NE", "ART", "NN", "$,", "ART", "NN", "ART"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.25": {"text": "Der alle Dinge stets regieret, und den ein jedes", "tokens": ["Der", "al\u00b7le", "Din\u00b7ge", "stets", "re\u00b7gie\u00b7ret", ",", "und", "den", "ein", "je\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "ADV", "VVFIN", "$,", "KON", "ART", "ART", "PIAT"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "Luft, Erde, Feuer und das Gew\u00e4sser, als seinen", "tokens": ["Luft", ",", "Er\u00b7de", ",", "Feu\u00b7er", "und", "das", "Ge\u00b7w\u00e4s\u00b7ser", ",", "als", "sei\u00b7nen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "ART", "NN", "$,", "KOUS", "PPOSAT"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.27": {"text": "Und bricht der Hagelregen lo\u00df; so sieht ein jeder sein", "tokens": ["Und", "bricht", "der", "Ha\u00b7gel\u00b7re\u00b7gen", "lo\u00df", ";", "so", "sieht", "ein", "je\u00b7der", "sein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$.", "ADV", "VVFIN", "ART", "PIAT", "PPOSAT"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.28": {"text": "Und wie die Allmacht was gebaut, zu unsrer Stra-", "tokens": ["Und", "wie", "die", "All\u00b7macht", "was", "ge\u00b7baut", ",", "zu", "uns\u00b7rer", "Stra"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "PWS", "VVPP", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Der Luftkreis h\u00fcllet sich ins Dunkle, verdekt der", "tokens": ["Der", "Luft\u00b7kreis", "h\u00fcl\u00b7let", "sich", "ins", "Dunk\u00b7le", ",", "ver\u00b7dekt", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPRART", "NN", "$,", "VVFIN", "ART"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "Das Sinnbild von des H\u00f6chsten Gnade; wenn solch", "tokens": ["Das", "Sinn\u00b7bild", "von", "des", "H\u00f6chs\u00b7ten", "Gna\u00b7de", ";", "wenn", "solch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$.", "KOUS", "PIAT"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.31": {"text": "Die Wolken \u00f6fnen ihren Schlauch, da h\u00f6rt man", "tokens": ["Die", "Wol\u00b7ken", "\u00f6f\u00b7nen", "ih\u00b7ren", "Schlauch", ",", "da", "h\u00f6rt", "man"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "PIS"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Auf dem besaamten Akkerfeld des Allerh\u00f6chsten", "tokens": ["Auf", "dem", "be\u00b7saam\u00b7ten", "Ak\u00b7ker\u00b7feld", "des", "Al\u00b7ler\u00b7h\u00f6chs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Die zu dem Landmann schreklich ruffet: ", "tokens": ["Die", "zu", "dem", "Land\u00b7mann", "schrek\u00b7lich", "ruf\u00b7fet", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Die dich als einen eitlen G\u00f6zzen, mit lee-\nren D\u00fcnsten aufgebl\u00e4ht;", "tokens": ["Die", "dich", "als", "ei\u00b7nen", "eit\u00b7len", "G\u00f6z\u00b7zen", ",", "mit", "lee", "ren", "D\u00fcns\u00b7ten", "auf\u00b7ge\u00b7bl\u00e4ht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "KOUS", "ART", "ADJA", "NN", "$,", "APPR", "TRUNC", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.35": {"text": "Du dachtest da der gr\u00fcne Schmuk sich auf\ndem Felde dir gewiesen,", "tokens": ["Du", "dach\u00b7test", "da", "der", "gr\u00fc\u00b7ne", "Schmuk", "sich", "auf", "dem", "Fel\u00b7de", "dir", "ge\u00b7wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "PRF", "APPR", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.36": {"text": "Da\u00df ist es was ich ausges\u00e4t; und nicht es\nsey der GOtt gepriesen,", "tokens": ["Da\u00df", "ist", "es", "was", "ich", "aus\u00b7ge\u00b7s\u00e4t", ";", "und", "nicht", "es", "sey", "der", "Gott", "ge\u00b7prie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "PWS", "PPER", "VVPP", "$.", "KON", "PTKNEG", "PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.37": {"text": "Der unsre Felder so bekr\u00f6net; und uns mit\ngn\u00e4digen Gedein,", "tokens": ["Der", "uns\u00b7re", "Fel\u00b7der", "so", "be\u00b7kr\u00f6\u00b7net", ";", "und", "uns", "mit", "gn\u00e4\u00b7di\u00b7gen", "Ge\u00b7dein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "VVFIN", "$.", "KON", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.38": {"text": "Bey einer fett und reichen Erndte, kan durch\nder K\u00f6rner Meng erfreun.", "tokens": ["Bey", "ei\u00b7ner", "fett", "und", "rei\u00b7chen", "Ernd\u00b7te", ",", "kan", "durch", "der", "K\u00f6r\u00b7ner", "Meng", "er\u00b7freun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "KON", "ADJA", "NN", "$,", "VMFIN", "APPR", "ART", "NN", "NE", "VVINF", "$."], "meter": "-+-+-+-+-+--+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.39": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.40": {"text": "Wie sie was sie erheben will, ganz kl\u00e4glich in dem", "tokens": ["Wie", "sie", "was", "sie", "er\u00b7he\u00b7ben", "will", ",", "ganz", "kl\u00e4g\u00b7lich", "in", "dem"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PWS", "PPER", "VVINF", "VMFIN", "$,", "ADV", "ADJD", "APPR", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Sie schl\u00e4gt durch die gefrorne Kugeln, die Bl\u00fcthen", "tokens": ["Sie", "schl\u00e4gt", "durch", "die", "ge\u00b7fror\u00b7ne", "Ku\u00b7geln", ",", "die", "Bl\u00fc\u00b7then"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.42": {"text": "Und drischt das Korn oft vor der Reiffe, auf denen", "tokens": ["Und", "drischt", "das", "Korn", "oft", "vor", "der", "Reif\u00b7fe", ",", "auf", "de\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "$,", "APPR", "PRELS"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.43": {"text": "Und die Gerechtigkeit die st\u00e4upt das Feld, da\u00df es", "tokens": ["Und", "die", "Ge\u00b7rech\u00b7tig\u00b7keit", "die", "st\u00e4upt", "das", "Feld", ",", "da\u00df", "es"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN", "ART", "NN", "$,", "KOUS", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Die von der Eitelkeit beherrscht, wie W\u00fcrmer in", "tokens": ["Die", "von", "der", "Ei\u00b7tel\u00b7keit", "be\u00b7herrscht", ",", "wie", "W\u00fcr\u00b7mer", "in"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$,", "PWAV", "NN", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Und l\u00e4st die an dem Staube kleben, an den zerknikten", "tokens": ["Und", "l\u00e4st", "die", "an", "dem", "Stau\u00b7be", "kle\u00b7ben", ",", "an", "den", "zer\u00b7knik\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "APPR", "ART", "NN", "VVINF", "$,", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.46": {"text": "Die Torheit derer Menschen-Kinder, die auf so", "tokens": ["Die", "Tor\u00b7heit", "de\u00b7rer", "Men\u00b7schen\u00b7Kin\u00b7der", ",", "die", "auf", "so"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PDS", "NN", "$,", "PRELS", "APPR", "ADV"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.47": {"text": "Alsdenn zerschmelzt der Bauren Stolz, bey ihrer", "tokens": ["Als\u00b7denn", "zer\u00b7schmelzt", "der", "Bau\u00b7ren", "Stolz", ",", "bey", "ih\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$,", "APPR", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.48": {"text": "Verkehrt sein wildes Jubel-Lied, in eine bange Jam-", "tokens": ["Ver\u00b7kehrt", "sein", "wil\u00b7des", "Ju\u00b7bel\u00b7Lied", ",", "in", "ei\u00b7ne", "ban\u00b7ge", "Jam"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,", "APPR", "ART", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.49": {"text": "Wenn diese Schrekkensstimme th\u00f6net, und von den", "tokens": ["Wenn", "die\u00b7se", "Schrek\u00b7kens\u00b7stim\u00b7me", "th\u00f6\u00b7net", ",", "und", "von", "den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PDAT", "NN", "VVFIN", "$,", "KON", "APPR", "ART"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.50": {"text": "Und von des H\u00f6chsten Wink getrieben, durch sei-", "tokens": ["Und", "von", "des", "H\u00f6chs\u00b7ten", "Wink", "ge\u00b7trie\u00b7ben", ",", "durch", "sei"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVPP", "$,", "APPR", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.51": {"text": "Die Halmen liegen eingeknikt, der Muth ist auch", "tokens": ["Die", "Hal\u00b7men", "lie\u00b7gen", "ein\u00b7ge\u00b7knikt", ",", "der", "Muth", "ist", "auch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "VVPP", "$,", "ART", "NN", "VAFIN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Der sonst den Landman taumelnd macht, der von", "tokens": ["Der", "sonst", "den", "Land\u00b7man", "tau\u00b7melnd", "macht", ",", "der", "von"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADV", "ART", "NN", "ADJD", "VVFIN", "$,", "PRELS", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.53": {"text": "Er h\u00e4tte schon das in den Scheuren, worauf die", "tokens": ["Er", "h\u00e4t\u00b7te", "schon", "das", "in", "den", "Scheu\u00b7ren", ",", "wo\u00b7rauf", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ART", "APPR", "ART", "NN", "$,", "PWAV", "ART"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.54": {"text": "Da\u00df noch bey ungewisser Bl\u00fcte, in mancherlei Ge-", "tokens": ["Da\u00df", "noch", "bey", "un\u00b7ge\u00b7wis\u00b7ser", "Bl\u00fc\u00b7te", ",", "in", "man\u00b7cher\u00b7lei", "Ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "ADJA", "NN", "$,", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.55": {"text": "Der Sch\u00f6pfer der im Zorn zwar straft; schleust", "tokens": ["Der", "Sch\u00f6p\u00b7fer", "der", "im", "Zorn", "zwar", "straft", ";", "schleust"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "ART", "APPRART", "NN", "ADV", "ADJD", "$.", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.56": {"text": "Die Menschen die da zu ihm schrein, in seine v\u00e4-", "tokens": ["Die", "Men\u00b7schen", "die", "da", "zu", "ihm", "schrein", ",", "in", "sei\u00b7ne", "v\u00e4"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADV", "APPR", "PPER", "PTKVZ", "$,", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Er giesset aus den strengen Hagel, der alles was der", "tokens": ["Er", "gies\u00b7set", "aus", "den", "stren\u00b7gen", "Ha\u00b7gel", ",", "der", "al\u00b7les", "was", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,", "PRELS", "PIS", "PWS", "ART"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.58": {"text": "Wenn sie in grosser Menge rasseln, in einem mahl", "tokens": ["Wenn", "sie", "in", "gros\u00b7ser", "Men\u00b7ge", "ras\u00b7seln", ",", "in", "ei\u00b7nem", "mahl"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "VVINF", "$,", "APPR", "ART", "ADV"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.59": {"text": "Doch aber zeigt sich seine G\u00fct, bey seines strengen", "tokens": ["Doch", "a\u00b7ber", "zeigt", "sich", "sei\u00b7ne", "G\u00fct", ",", "bey", "sei\u00b7nes", "stren\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PRF", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Wenn Hagel-Wetter wo entstehn in einer treuen", "tokens": ["Wenn", "Ha\u00b7gel\u00b7Wet\u00b7ter", "wo", "ent\u00b7stehn", "in", "ei\u00b7ner", "treu\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "PWAV", "VVFIN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "Er l\u00e4st zum Zeugnis seiner Gnade, die Hagel nicht", "tokens": ["Er", "l\u00e4st", "zum", "Zeug\u00b7nis", "sei\u00b7ner", "Gna\u00b7de", ",", "die", "Ha\u00b7gel", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PPOSAT", "NN", "$,", "ART", "NN", "PTKNEG"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.62": {"text": "Nein! nur in einen sanften Fallen, oft auf die", "tokens": ["Nein", "!", "nur", "in", "ei\u00b7nen", "sanf\u00b7ten", "Fal\u00b7len", ",", "oft", "auf", "die"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$.", "ADV", "APPR", "ART", "ADJA", "NN", "$,", "ADV", "APPR", "ART"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.63": {"text": "Da denn die K\u00f6rner nicht so stark; und nicht so", "tokens": ["Da", "denn", "die", "K\u00f6r\u00b7ner", "nicht", "so", "stark", ";", "und", "nicht", "so"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "PTKNEG", "ADV", "ADJD", "$.", "KON", "PTKNEG", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.64": {"text": "Als wenn sie in geschwinden Flug, das was sie fas-", "tokens": ["Als", "wenn", "sie", "in", "ge\u00b7schwin\u00b7den", "Flug", ",", "das", "was", "sie", "fas"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "APPR", "ADJA", "NN", "$,", "PDS", "PRELS", "PPER", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.65": {"text": "Er st\u00e4upet oft nur eine Seite und l\u00e4st die andre ohn-", "tokens": ["Er", "st\u00e4u\u00b7pet", "oft", "nur", "ei\u00b7ne", "Sei\u00b7te", "und", "l\u00e4st", "die", "and\u00b7re", "ohn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "NN", "KON", "VVFIN", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.66": {"text": "Zum Zeugnis, da\u00df er in dem Zorne, dennoch ein", "tokens": ["Zum", "Zeug\u00b7nis", ",", "da\u00df", "er", "in", "dem", "Zor\u00b7ne", ",", "den\u00b7noch", "ein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "$,", "ADV", "ART"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.67": {"text": "Vertilget er die Winterfrucht; so l\u00e4st er wol aus", "tokens": ["Ver\u00b7til\u00b7get", "er", "die", "Win\u00b7ter\u00b7frucht", ";", "so", "l\u00e4st", "er", "wol", "aus"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "$.", "ADV", "VVFIN", "PPER", "ADV", "APPR"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.68": {"text": "Das was in warmen Sommer keimmt, frei von", "tokens": ["Das", "was", "in", "war\u00b7men", "Som\u00b7mer", "keimmt", ",", "frei", "von"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "PRELS", "APPR", "ADJA", "NN", "VVFIN", "$,", "ADJD", "APPR"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.69": {"text": "Ja! seine G\u00fct die ohne Schranken, verriegelt oft", "tokens": ["Ja", "!", "sei\u00b7ne", "G\u00fct", "die", "oh\u00b7ne", "Schran\u00b7ken", ",", "ver\u00b7rie\u00b7gelt", "oft"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKANT", "$.", "PPOSAT", "NN", "ART", "APPR", "NN", "$,", "VVFIN", "ADV"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.70": {"text": "Wenn des erschroknen S\u00fcnders Herze in Thr\u00e4nen", "tokens": ["Wenn", "des", "er\u00b7schro\u00b7knen", "S\u00fcn\u00b7ders", "Her\u00b7ze", "in", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.71": {"t