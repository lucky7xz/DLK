{"textgrid.poem.25605": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "Auf drei Br\u00fcder in hohen Aemtern, die alle drei keinen Aufwand machten", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der darf, nur will er nicht; sein Blut ist schon zu lau.", "tokens": ["Der", "darf", ",", "nur", "will", "er", "nicht", ";", "sein", "Blut", "ist", "schon", "zu", "lau", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "$,", "ADV", "VMFIN", "PPER", "PTKNEG", "$.", "PPOSAT", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der will, nur kann er nicht; die Gl\u00e4ub'ger sind zu schlau.", "tokens": ["Der", "will", ",", "nur", "kann", "er", "nicht", ";", "die", "Gl\u00e4ub'\u00b7ger", "sind", "zu", "schlau", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "$,", "ADV", "VMFIN", "PPER", "PTKNEG", "$.", "ART", "NN", "VAFIN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der kann, nur darf er nicht; warum? Fragt seine Frau.", "tokens": ["Der", "kann", ",", "nur", "darf", "er", "nicht", ";", "wa\u00b7rum", "?", "Fragt", "sei\u00b7ne", "Frau", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "$,", "ADV", "VMFIN", "PPER", "PTKNEG", "$.", "PWAV", "$.", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}