{"textgrid.poem.34320": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "B.", "genre": "verse", "period": "N.A.", "pub_year": 1771, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein wohlgen\u00e4hrter Kandidat", "tokens": ["Ein", "wohl\u00b7ge\u00b7n\u00e4hr\u00b7ter", "Kan\u00b7di\u00b7dat"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der nie noch einen Fehltritt that,", "tokens": ["Der", "nie", "noch", "ei\u00b7nen", "Fehl\u00b7tritt", "that", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und den verbotnen Liebestrieb", "tokens": ["Und", "den", "ver\u00b7bot\u00b7nen", "Lie\u00b7be\u00b7strieb"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In lauter Predigten verschrieb,", "tokens": ["In", "lau\u00b7ter", "Pre\u00b7dig\u00b7ten", "ver\u00b7schrieb", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.5": {"text": "Kehrt einst bei einem Pfarrer ein,", "tokens": ["Kehrt", "einst", "bei", "ei\u00b7nem", "Pfar\u00b7rer", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Sontag sein Gehilf zu sein.", "tokens": ["Den", "Son\u00b7tag", "sein", "Ge\u00b7hilf", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der hatt' ein Kind, zwar still und bleich", "tokens": ["Der", "hatt'", "ein", "Kind", ",", "zwar", "still", "und", "bleich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Von Kummer krank, doch Engeln gleich", "tokens": ["Von", "Kum\u00b7mer", "krank", ",", "doch", "En\u00b7geln", "gleich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "ADJD", "$,", "KON", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Sie hilt im halberloschnen Blick", "tokens": ["Sie", "hilt", "im", "hal\u00b7ber\u00b7lo\u00b7schnen", "Blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Noch Flammen ohne Maa\u00df zur\u00fcck,", "tokens": ["Noch", "Flam\u00b7men", "oh\u00b7ne", "Maa\u00df", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "All itzt in Andacht eingeh\u00fcllt,", "tokens": ["All", "itzt", "in", "An\u00b7dacht", "ein\u00b7ge\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Sch\u00f6n wie ein marmorn Heiligenbild.", "tokens": ["Sch\u00f6n", "wie", "ein", "mar\u00b7morn", "Hei\u00b7li\u00b7gen\u00b7bild", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.13": {"text": "War nicht umsonst so still und schwach,", "tokens": ["War", "nicht", "um\u00b7sonst", "so", "still", "und", "schwach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Verla\u00dfne Liebe trug sie nach.", "tokens": ["Ver\u00b7la\u00df\u00b7ne", "Lie\u00b7be", "trug", "sie", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "In ihrer kleinen Kammer hoch", "tokens": ["In", "ih\u00b7rer", "klei\u00b7nen", "Kam\u00b7mer", "hoch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Sie stets an der Erinnrung sog,", "tokens": ["Sie", "stets", "an", "der", "E\u00b7rinn\u00b7rung", "sog", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.17": {"text": "An ihrem Brotschrank an der Wand", "tokens": ["An", "ih\u00b7rem", "Brot\u00b7schrank", "an", "der", "Wand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Er immer, immer vor ihr stand,", "tokens": ["Er", "im\u00b7mer", ",", "im\u00b7mer", "vor", "ihr", "stand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Und wenn ein Schlaf sie \u00fcbernahm", "tokens": ["Und", "wenn", "ein", "Schlaf", "sie", "\u00fc\u00b7bern\u00b7ahm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Im Traum er immer wieder kam.", "tokens": ["Im", "Traum", "er", "im\u00b7mer", "wie\u00b7der", "kam", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "F\u00fcr ihn sie noch ihr H\u00e4rlein stutzt,", "tokens": ["F\u00fcr", "ihn", "sie", "noch", "ihr", "H\u00e4r\u00b7lein", "stutzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Sich, wenn sie ganz allein ist, putzt,", "tokens": ["Sich", ",", "wenn", "sie", "ganz", "al\u00b7lein", "ist", ",", "putzt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PRF", "$,", "KOUS", "PPER", "ADV", "ADV", "VAFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "All ihre Sch\u00fcrzen anprobiert", "tokens": ["All", "ih\u00b7re", "Sch\u00fcr\u00b7zen", "an\u00b7pro\u00b7biert"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Und ihre sch\u00f6nen L\u00e4tzchen schn\u00fcrt,", "tokens": ["Und", "ih\u00b7re", "sch\u00f6\u00b7nen", "L\u00e4tz\u00b7chen", "schn\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Und vor dem Spiegel nur allein", "tokens": ["Und", "vor", "dem", "Spie\u00b7gel", "nur", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Verlangt er soll ein Schmeichler sein.", "tokens": ["Ver\u00b7langt", "er", "soll", "ein", "Schmeich\u00b7ler", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VMFIN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Kam aber etwas Fremds ins Haus", "tokens": ["Kam", "a\u00b7ber", "et\u00b7was", "Fremds", "ins", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "PIAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "So zog sie gleich den Schn\u00fcrleib aus,", "tokens": ["So", "zog", "sie", "gleich", "den", "Schn\u00fcr\u00b7leib", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "That sich so schlecht und h\u00e4uslich an,", "tokens": ["That", "sich", "so", "schlecht", "und", "h\u00e4us\u00b7lich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ADV", "ADJD", "KON", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Es \u00fcbersah sie jedermann.", "tokens": ["Es", "\u00fc\u00b7ber\u00b7sah", "sie", "je\u00b7der\u00b7mann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Zum Ungl\u00fcck unserm Pfaffen allein", "tokens": ["Zum", "Un\u00b7gl\u00fcck", "un\u00b7serm", "Pfaf\u00b7fen", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.32": {"text": "Der Lilie Nachtglanz leuchtet ein,", "tokens": ["Der", "Li\u00b7lie", "Nacht\u00b7glanz", "leuch\u00b7tet", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Obschon sie matt am Stengel hing.", "tokens": ["Ob\u00b7schon", "sie", "matt", "am", "Sten\u00b7gel", "hing", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Fr\u00fch eh er in die Kirche gieng", "tokens": ["Fr\u00fch", "eh", "er", "in", "die", "Kir\u00b7che", "gieng"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.35": {"text": "Er sehr er\u00e4schert zu ihr trat", "tokens": ["Er", "sehr", "e\u00b7r\u00e4sc\u00b7hert", "zu", "ihr", "trat"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVPP", "APPR", "PPER", "VVFIN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.36": {"text": "Und sie \u2013 um ein Glas Wasser bat \u2013", "tokens": ["Und", "sie", "\u2013", "um", "ein", "Glas", "Was\u00b7ser", "bat", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$(", "APPR", "ART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Dann laut er auf der Kanzel schreit", "tokens": ["Dann", "laut", "er", "auf", "der", "Kan\u00b7zel", "schreit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Man h\u00f6rt ihn auf dem Kirchhof weit", "tokens": ["Man", "h\u00f6rt", "ihn", "auf", "dem", "Kirch\u00b7hof", "weit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Und macht solch einen derben Schlu\u00df", "tokens": ["Und", "macht", "solch", "ei\u00b7nen", "der\u00b7ben", "Schlu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIAT", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Da\u00df alt und jung noch weinen mu\u00df,", "tokens": ["Da\u00df", "alt", "und", "jung", "noch", "wei\u00b7nen", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "KON", "ADJD", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Und der Gemeinde Sympathie", "tokens": ["Und", "der", "Ge\u00b7mein\u00b7de", "Sym\u00b7pa\u00b7thie"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Ergriff zu allerletzt auch sie \u2013", "tokens": ["Er\u00b7griff", "zu", "al\u00b7ler\u00b7letzt", "auch", "sie", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADV", "ADV", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "S' ging jeder wie gegei\u00dfelt fort \u2013", "tokens": ["S'", "ging", "je\u00b7der", "wie", "ge\u00b7gei\u00b7\u00dfelt", "fort", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "KOKOM", "VVPP", "PTKVZ", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.44": {"text": "Der Kandidat ward Pfarr am Ort.", "tokens": ["Der", "Kan\u00b7di\u00b7dat", "ward", "Pfarr", "am", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NE", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Obs nun die Dankbarkeit ihm that,", "tokens": ["Obs", "nun", "die", "Dank\u00b7bar\u00b7keit", "ihm", "that", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Ein's Tag's er in ihr Zimmer trat,", "tokens": ["Ein's", "Tag's", "er", "in", "ihr", "Zim\u00b7mer", "trat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Sehr holde Jungfrau, sagt er ihr,", "tokens": ["Sehr", "hol\u00b7de", "Jung\u00b7frau", ",", "sagt", "er", "ihr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Ihr schickt euch \u00fcbel nicht zu mir,", "tokens": ["Ihr", "schickt", "euch", "\u00fc\u00b7bel", "nicht", "zu", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "Ihr seid voll Tugend und Verstand,", "tokens": ["Ihr", "seid", "voll", "Tu\u00b7gend", "und", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Ihr habt mein Herz, da nehmt die Hand \u2013", "tokens": ["Ihr", "habt", "mein", "Herz", ",", "da", "nehmt", "die", "Hand", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Sie sehr erschrocken auf den Tod", "tokens": ["Sie", "sehr", "er\u00b7schro\u00b7cken", "auf", "den", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.52": {"text": "Ward endlich wieder einmal roth,", "tokens": ["Ward", "end\u00b7lich", "wie\u00b7der", "ein\u00b7mal", "roth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.53": {"text": "\u00bbach lieber Herr \u2013 \u2013 mein Vater \u2013 ich \u2013", "tokens": ["\u00bb", "ach", "lie\u00b7ber", "Herr", "\u2013", "\u2013", "mein", "Va\u00b7ter", "\u2013", "ich", "\u2013"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "punct"], "pos": ["$(", "ITJ", "ADV", "NN", "$(", "$(", "PPOSAT", "NN", "$(", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.54": {"text": "Ihr findet bessere als mich", "tokens": ["Ihr", "fin\u00b7det", "bes\u00b7se\u00b7re", "als", "mich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "KOKOM", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.55": {"text": "Ich bin zu jung \u2013 ich bin zu alt \u2013\u00ab", "tokens": ["Ich", "bin", "zu", "jung", "\u2013", "ich", "bin", "zu", "alt", "\u2013", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "$(", "PPER", "VAFIN", "PTKA", "ADJD", "