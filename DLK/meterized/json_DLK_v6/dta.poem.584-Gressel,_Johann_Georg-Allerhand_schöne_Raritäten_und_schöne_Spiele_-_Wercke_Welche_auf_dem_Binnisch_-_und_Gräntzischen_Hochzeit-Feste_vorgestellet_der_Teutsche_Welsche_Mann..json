{"dta.poem.584": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Allerhand sch\u00f6ne Rarit\u00e4ten und sch\u00f6ne  \n Spiele - Wercke/  \n Welche auf dem  \n Binnisch - und Gr\u00e4ntzischen  \n Hochzeit-Feste vorgestellet  \n der Teutsche Welsche Mann.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Zum Abschied zeig ich noch ein langes Blat Papier", "tokens": ["Zum", "Ab\u00b7schied", "zeig", "ich", "noch", "ein", "lan\u00b7ges", "Blat", "Pa\u00b7pier"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf welchem folgendes mit grosser Schrifft geschrieben/", "tokens": ["Auf", "wel\u00b7chem", "fol\u00b7gen\u00b7des", "mit", "gros\u00b7ser", "Schrifft", "ge\u00b7schrie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIS", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Lebt neu-verkn\u00fcpfftes Paar/ lebt gl\u00fccklich f\u00fcr und f\u00fcr/", "tokens": ["Lebt", "neu\u00b7ver\u00b7kn\u00fcpff\u00b7tes", "Paar", "/", "lebt", "gl\u00fcck\u00b7lich", "f\u00fcr", "und", "f\u00fcr", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$(", "VVFIN", "ADJD", "APPR", "KON", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Himmel sey euch hold und segne euer Lieben!", "tokens": ["Der", "Him\u00b7mel", "sey", "euch", "hold", "und", "seg\u00b7ne", "eu\u00b7er", "Lie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "KON", "VVFIN", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dis w\u00fcnschet euch der Welsche Mann/", "tokens": ["Dis", "w\u00fcn\u00b7schet", "euch", "der", "Wel\u00b7sche", "Mann", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und rufft zuletzt/ so laut er kan:", "tokens": ["Und", "rufft", "zu\u00b7letzt", "/", "so", "laut", "er", "kan", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$(", "ADV", "ADJD", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sch\u00f6ne Rarit\u00e4t/ sch\u00f6ne Spiele-Wercke/ ", "tokens": ["Sch\u00f6\u00b7ne", "Ra\u00b7ri\u00b7t\u00e4t", "/", "sch\u00f6\u00b7ne", "Spie\u00b7le\u00b7\u00b7Wer\u00b7cke", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "ADJA", "NN", "$("], "meter": "+----+-+-+-", "measure": "dactylic.init"}, "line.8": {"text": "sch\u00f6ne ", "tokens": ["sch\u00f6\u00b7ne"], "token_info": ["word"], "pos": ["ADJA"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "sch\u00f6ne Spiele-Wercke.", "tokens": ["sch\u00f6\u00b7ne", "Spie\u00b7le\u00b7\u00b7Wer\u00b7cke", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}