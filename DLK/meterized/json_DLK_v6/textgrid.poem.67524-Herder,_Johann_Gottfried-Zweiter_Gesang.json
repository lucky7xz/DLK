{"textgrid.poem.67524": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Zweiter Gesang", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gest\u00e4rkt vom Himmelstrost des Genius,", "tokens": ["Ge\u00b7st\u00e4rkt", "vom", "Him\u00b7mel\u00b7strost", "des", "Ge\u00b7nius", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ging ich auf Erden ruhig, still einher;", "tokens": ["Ging", "ich", "auf", "Er\u00b7den", "ru\u00b7hig", ",", "still", "ein\u00b7her", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ADJD", "$,", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mein Vaterland war in den Sternen. Einst", "tokens": ["Mein", "Va\u00b7ter\u00b7land", "war", "in", "den", "Ster\u00b7nen", ".", "Einst"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "ART", "NN", "$.", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Befiel mich mitten im Gedankenmeer", "tokens": ["Be\u00b7fiel", "mich", "mit\u00b7ten", "im", "Ge\u00b7dan\u00b7ken\u00b7meer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Von Gottes Schicksal mit der Menschenwelt", "tokens": ["Von", "Got\u00b7tes", "Schick\u00b7sal", "mit", "der", "Men\u00b7schen\u00b7welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ein himmlischer, ambrosisch-s\u00fc\u00dfer Schlaf.", "tokens": ["Ein", "himm\u00b7li\u00b7scher", ",", "am\u00b7bro\u00b7sischs\u00fc\u00b7\u00dfer", "Schlaf", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Ich war im Paradiese. Vor mir stand", "tokens": ["Ich", "war", "im", "Pa\u00b7ra\u00b7die\u00b7se", ".", "Vor", "mir", "stand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "$.", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der Vater und die Mutter alles Heers", "tokens": ["Der", "Va\u00b7ter", "und", "die", "Mut\u00b7ter", "al\u00b7les", "Heers"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Der Menschens\u00f6hne, hohe Traumgestalten!", "tokens": ["Der", "Men\u00b7schen\u00b7s\u00f6h\u00b7ne", ",", "ho\u00b7he", "Traum\u00b7ge\u00b7stal\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Der Vater, Gottes Sohn und Abbild; er", "tokens": ["Der", "Va\u00b7ter", ",", "Got\u00b7tes", "Sohn", "und", "Ab\u00b7bild", ";", "er"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "NN", "NN", "KON", "NN", "$.", "PPER"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.11": {"text": "Das Urbild aller Mannesw\u00fcrdigkeit;", "tokens": ["Das", "Ur\u00b7bild", "al\u00b7ler", "Man\u00b7nes\u00b7w\u00fcr\u00b7dig\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Sie Tochter Gottes, Paradieses Braut", "tokens": ["Sie", "Toch\u00b7ter", "Got\u00b7tes", ",", "Pa\u00b7ra\u00b7die\u00b7ses", "Braut"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "NN", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Und Jungfrau, Weib des ersten frohen Manns,", "tokens": ["Und", "Jung\u00b7frau", ",", "Weib", "des", "ers\u00b7ten", "fro\u00b7hen", "Manns", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "NN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Das Urbild aller Weibessch\u00f6ne! Fast", "tokens": ["Das", "Ur\u00b7bild", "al\u00b7ler", "Wei\u00b7bes\u00b7sch\u00f6\u00b7ne", "!", "Fast"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "PIAT", "NN", "$.", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Anbetend sah ich sie und f\u00fchlte mich", "tokens": ["An\u00b7be\u00b7tend", "sah", "ich", "sie", "und", "f\u00fchl\u00b7te", "mich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "PPER", "PPER", "KON", "VVFIN", "PPER"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.16": {"text": "So klein, so tief hinabgesunken, f\u00fchlte", "tokens": ["So", "klein", ",", "so", "tief", "hin\u00b7ab\u00b7ge\u00b7sun\u00b7ken", ",", "f\u00fchl\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "VVINF", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "So tief hinabgesunken mein Geschlecht", "tokens": ["So", "tief", "hin\u00b7ab\u00b7ge\u00b7sun\u00b7ken", "mein", "Ge\u00b7schlecht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Von jener W\u00fcrd' und Sch\u00f6ne, von der Kraft", "tokens": ["Von", "je\u00b7ner", "W\u00fcrd'", "und", "Sch\u00f6\u00b7ne", ",", "von", "der", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "KON", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Und Weisheit der beherrschenden Gestalt,", "tokens": ["Und", "Weis\u00b7heit", "der", "be\u00b7herr\u00b7schen\u00b7den", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Die Gottes Ebenbild hienieden war,", "tokens": ["Die", "Got\u00b7tes", "E\u00b7ben\u00b7bild", "hien\u00b7ie\u00b7den", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Und ihrer G\u00fct' und Unschuld. Wie der Bach", "tokens": ["Und", "ih\u00b7rer", "G\u00fct'", "und", "Un\u00b7schuld", ".", "Wie", "der", "Bach"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "KON", "NN", "$.", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Von seiner reinen Silberquelle fleu\u00dft", "tokens": ["Von", "sei\u00b7ner", "rei\u00b7nen", "Sil\u00b7ber\u00b7quel\u00b7le", "fleu\u00dft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Und tr\u00fcbt sich hie und da mit Schlamm und Koth", "tokens": ["Und", "tr\u00fcbt", "sich", "hie", "und", "da", "mit", "Schlamm", "und", "Koth"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "ADV", "KON", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Und schwillt von Gifte, f\u00e4rbet sich mit Blut", "tokens": ["Und", "schwillt", "von", "Gif\u00b7te", ",", "f\u00e4r\u00b7bet", "sich", "mit", "Blut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "$,", "VVFIN", "PRF", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Und Eiter, ist mit Leichnamen bedeckt", "tokens": ["Und", "Ei\u00b7ter", ",", "ist", "mit", "Leich\u00b7na\u00b7men", "be\u00b7deckt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "$,", "VAFIN", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Und stirbt zuletzt im Sande: so erschien", "tokens": ["Und", "stirbt", "zu\u00b7letzt", "im", "San\u00b7de", ":", "so", "er\u00b7schien"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "NN", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Dein Fortflu\u00df mir, Du armes Menschenvolk,", "tokens": ["Dein", "Fort\u00b7flu\u00df", "mir", ",", "Du", "ar\u00b7mes", "Men\u00b7schen\u00b7volk", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Von schw\u00e4cheren zu schw\u00e4cheren Geschlechtern.", "tokens": ["Von", "schw\u00e4\u00b7che\u00b7ren", "zu", "schw\u00e4\u00b7che\u00b7ren", "Ge\u00b7schlech\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "\u00bbwo ist Dein gottentsprungner Himmelsquell?", "tokens": ["\u00bb", "wo", "ist", "Dein", "got\u00b7tents\u00b7prung\u00b7ner", "Him\u00b7mels\u00b7quell", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Und kannst Du, armer, tr\u00fcber, blut'ger Bach,", "tokens": ["Und", "kannst", "Du", ",", "ar\u00b7mer", ",", "tr\u00fc\u00b7ber", ",", "blut'\u00b7ger", "Bach", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "$,", "ADJA", "$,", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Zur\u00fcck zur Quelle flie\u00dfen? Kannst Du je", "tokens": ["Zu\u00b7r\u00fcck", "zur", "Quel\u00b7le", "flie\u00b7\u00dfen", "?", "Kannst", "Du", "je"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VVINF", "$.", "VMFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Die erste, reine Himmelsquelle werden?", "tokens": ["Die", "ers\u00b7te", ",", "rei\u00b7ne", "Him\u00b7mels\u00b7quel\u00b7le", "wer\u00b7den", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Und bleiben?\u00ab Bittre Thr\u00e4nen flossen mir", "tokens": ["Und", "blei\u00b7ben", "?", "\u00ab", "Bitt\u00b7re", "Thr\u00e4\u00b7nen", "flos\u00b7sen", "mir"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "$.", "$(", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Da, wo ich stand, in meinen tr\u00fcben Bach", "tokens": ["Da", ",", "wo", "ich", "stand", ",", "in", "mei\u00b7nen", "tr\u00fc\u00b7ben", "Bach"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "PPER", "VVFIN", "$,", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Des Menschenlebens. Jene Traumgestalten", "tokens": ["Des", "Men\u00b7schen\u00b7le\u00b7bens", ".", "Je\u00b7ne", "Traum\u00b7ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$.", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Des Gottes und der G\u00f6ttin meines Stamms", "tokens": ["Des", "Got\u00b7tes", "und", "der", "G\u00f6t\u00b7tin", "mei\u00b7nes", "Stamms"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Verschwanden, und das Paradies verschwand.", "tokens": ["Ver\u00b7schwan\u00b7den", ",", "und", "das", "Pa\u00b7ra\u00b7dies", "ver\u00b7schwand", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Ich sah, im letzten Blick, des Lebens Baum", "tokens": ["Ich", "sah", ",", "im", "letz\u00b7ten", "Blick", ",", "des", "Le\u00b7bens", "Baum"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "APPRART", "ADJA", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "Verdorren, sah des Baums der Weisheit Frucht,", "tokens": ["Ver\u