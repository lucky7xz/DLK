{"textgrid.poem.44565": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wohlan! Werft um, rei\u00dft ein! macht euch nur laut!", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohlan! Werft um, rei\u00dft ein! macht euch nur laut!", "tokens": ["Wo\u00b7hlan", "!", "Werft", "um", ",", "rei\u00dft", "ein", "!", "macht", "euch", "nur", "laut", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "NN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$.", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Verkennt der Gottheit stillgesch\u00e4ftgen Finger,", "tokens": ["Ver\u00b7kennt", "der", "Got\u00b7theit", "still\u00b7ge\u00b7sch\u00e4ft\u00b7gen", "Fin\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und all, woran Jahrhunderte gebaut,", "tokens": ["Und", "all", ",", "wo\u00b7ran", "Jahr\u00b7hun\u00b7der\u00b7te", "ge\u00b7baut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "$,", "PWAV", "NN", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Erkl\u00e4rt es als der Willk\u00fcr Sklavenzwinger.", "tokens": ["Er\u00b7kl\u00e4rt", "es", "als", "der", "Will\u00b7k\u00fcr", "Skla\u00b7ven\u00b7zwin\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOUS", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das sch\u00f6nste Werk der Weisheit und der Kraft,", "tokens": ["Das", "sch\u00f6ns\u00b7te", "Werk", "der", "Weis\u00b7heit", "und", "der", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df sie die Roheit, schwer genug, geb\u00e4ndigt,", "tokens": ["Da\u00df", "sie", "die", "Ro\u00b7heit", ",", "schwer", "ge\u00b7nug", ",", "ge\u00b7b\u00e4n\u00b7digt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "ADJD", "ADV", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hebts auf! Entla\u00dft den P\u00f6bel seiner Haft,", "tokens": ["Hebts", "auf", "!", "Ent\u00b7la\u00dft", "den", "P\u00f6\u00b7bel", "sei\u00b7ner", "Haft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Erkl\u00e4rt der Bildung Werk als schon beendigt.", "tokens": ["Er\u00b7kl\u00e4rt", "der", "Bil\u00b7dung", "Werk", "als", "schon", "be\u00b7en\u00b7digt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "KOKOM", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Man meint das Volk. Hast du ein Volk dereinst,", "tokens": ["Man", "meint", "das", "Volk", ".", "Hast", "du", "ein", "Volk", "de\u00b7reinst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "$.", "VAFIN", "PPER", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Selbsthorchend auf der Ordnung leise Kl\u00e4nge,", "tokens": ["Selbst\u00b7hor\u00b7chend", "auf", "der", "Ord\u00b7nung", "lei\u00b7se", "Kl\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dann ist die Zeit, die du gekommen meinst,", "tokens": ["Dann", "ist", "die", "Zeit", ",", "die", "du", "ge\u00b7kom\u00b7men", "meinst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nicht jetzt, wo noch dein Volk die bl\u00f6de Menge;", "tokens": ["Nicht", "jetzt", ",", "wo", "noch", "dein", "Volk", "die", "bl\u00f6\u00b7de", "Men\u00b7ge", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$,", "PWAV", "ADV", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die hergebracht Gewohntes \u00fcberzeugt,", "tokens": ["Die", "her\u00b7ge\u00b7bracht", "Ge\u00b7wohn\u00b7tes", "\u00fc\u00b7berz\u00b7eugt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nicht eignes Sch\u00f6pfen aus des Denkens Quelle,", "tokens": ["Nicht", "eig\u00b7nes", "Sch\u00f6p\u00b7fen", "aus", "des", "Den\u00b7kens", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Nicht ", "tokens": ["Nicht"], "token_info": ["word"], "pos": ["PTKNEG"], "meter": "+", "measure": "single.up"}}, "stanza.5": {"line.1": {"text": "Macht alles gleich! h\u00fcllt in dasselbe Kleid", "tokens": ["Macht", "al\u00b7les", "gleich", "!", "h\u00fcllt", "in", "das\u00b7sel\u00b7be", "Kleid"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PIS", "ADV", "$.", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Menschheit urerschaffne nackte Bl\u00f6\u00dfe,", "tokens": ["Der", "Menschheit", "ur\u00b7er\u00b7schaff\u00b7ne", "nack\u00b7te", "Bl\u00f6\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "ADJA", "NN", "$,"], "meter": "-++-+-+-+-", "measure": "unknown.measure.penta"}, "line.3": {"text": "Bis alles \u00e4rmlich, wie ihr selber seid,", "tokens": ["Bis", "al\u00b7les", "\u00e4rm\u00b7lich", ",", "wie", "ihr", "sel\u00b7ber", "seid", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADJD", "$,", "PWAV", "PPER", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und euer Ma\u00df die vorbestimmte Gr\u00f6\u00dfe.", "tokens": ["Und", "eu\u00b7er", "Ma\u00df", "die", "vor\u00b7be\u00b7stimm\u00b7te", "Gr\u00f6\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Was soll der Adel? er ist unbequem,", "tokens": ["Was", "soll", "der", "A\u00b7del", "?", "er", "ist", "un\u00b7be\u00b7quem", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "$.", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Emporzuschaun ist ein verdrie\u00dflich Placken;", "tokens": ["Em\u00b7por\u00b7zu\u00b7schaun", "ist", "ein", "ver\u00b7drie\u00df\u00b7lich", "Pla\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Seit selbst zu Gott es uns nicht mehr genehm,", "tokens": ["Seit", "selbst", "zu", "Gott", "es", "uns", "nicht", "mehr", "ge\u00b7nehm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "NN", "PPER", "PRF", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Erm\u00fcdet es bedeutend unsre Nacken.", "tokens": ["Er\u00b7m\u00fc\u00b7det", "es", "be\u00b7deu\u00b7tend", "uns\u00b7re", "Na\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Allein die Sch\u00f6nheit ist ein Adel auch,", "tokens": ["Al\u00b7lein", "die", "Sch\u00f6n\u00b7heit", "ist", "ein", "A\u00b7del", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du w\u00e4hlst ein sch\u00f6nes M\u00e4dchen unter hundert,", "tokens": ["Du", "w\u00e4hlst", "ein", "sch\u00f6\u00b7nes", "M\u00e4d\u00b7chen", "un\u00b7ter", "hun\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPR", "CARD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Talent und Geist, der Kunstbegabung Hauch", "tokens": ["Ta\u00b7lent", "und", "Geist", ",", "der", "Kunst\u00b7be\u00b7ga\u00b7bung", "Hauch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sind Zufall, und doch auch als Wert bewundert.", "tokens": ["Sind", "Zu\u00b7fall", ",", "und", "doch", "auch", "als", "Wert", "be\u00b7wun\u00b7dert", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "KON", "ADV", "ADV", "KOUS", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Wenn in der Erblichkeit das Unrecht liegt,", "tokens": ["Wenn", "in", "der", "Er\u00b7blich\u00b7keit", "das", "Un\u00b7recht", "liegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nenn ich den Reichtum, dem ihr selbst gewogen,", "tokens": ["Nenn", "ich", "den", "Reich\u00b7tum", ",", "dem", "ihr", "selbst", "ge\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der auf den Sohn, der heut die Welt betriegt,", "tokens": ["Der", "auf", "den", "Sohn", ",", "der", "heut", "die", "Welt", "be\u00b7triegt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vom Vater erbt, der einst die Welt betrogen.", "tokens": ["Vom", "Va\u00b7ter", "erbt", ",", "der", "einst", "die", "Welt", "be\u00b7tro\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "PRELS", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "W\u00e4r das ein Adel, der euch l\u00e4\u00dflich scheint,", "tokens": ["W\u00e4r", "das", "ein", "A\u00b7del", ",", "der", "euch", "l\u00e4\u00df\u00b7lich", "scheint", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dem ihr verg\u00f6nnt, im Herrenhaus zu sitzen?", "tokens": ["Dem", "ihr", "ver\u00b7g\u00f6nnt", ",", "im", "Her\u00b7ren\u00b7haus", "zu", "sit\u00b7zen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "$,", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "La\u00dft ihr \u2013 was euch vom F\u00fcrsten schm\u00e4hlich scheint \u2013", "tokens": ["La\u00dft", "ihr", "\u2013", "was", "euch", "vom", "F\u00fcrs\u00b7ten", "schm\u00e4h\u00b7lich", "scheint", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$(", "PWS", "PPER", "APPRART", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vom Rad des M\u00e4klers euch mit Kot bespritzen?", "tokens": ["Vom", "Rad", "des", "M\u00e4k\u00b7lers", "euch", "mit", "Kot", "be\u00b7sprit\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Gebt euch zur Ruh! \u2013 Wer endlich seid denn ihr,", "tokens": ["Gebt", "euch", "zur", "Ruh", "!", "\u2013", "Wer", "end\u00b7lich", "seid", "denn", "ihr", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "$.", "$(", "PWS", "ADV", "VAFIN", "KON", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die ihr die Welt hinweist in neue Bahnen?", "tokens": ["Die", "ihr", "die", "Welt", "hin\u00b7weist", "in", "neu\u00b7e", "Bah\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Soll ich, was etwa gar unschicklich hier,", "tokens": ["Soll", "ich", ",", "was", "et\u00b7wa", "gar", "un\u00b7schick\u00b7lich", "hier", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "PRELS", "ADV", "ADV", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "An eure eigne Schw\u00e4chlichkeit euch mahnen?", "tokens": ["An", "eu\u00b7re", "eig\u00b7ne", "Schw\u00e4ch\u00b7lich\u00b7keit", "euch", "mah\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Nicht was ihr habt, nein, das nur, was euch fehlt,", "tokens": ["Nicht", "was", "ihr", "habt", ",", "nein", ",", "das", "nur", ",", "was", "euch", "fehlt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PWS", "PPER", "VAFIN", "$,", "PTKANT", "$,", "PRELS", "ADV", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Empfahl euch in des P\u00f6bels hohe Gnaden,", "tokens": ["Emp\u00b7fahl", "euch", "in", "des", "P\u00f6\u00b7bels", "ho\u00b7he", "Gna\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der trunken damals, als er euch gew\u00e4hlt,", "tokens": ["Der", "trun\u00b7ken", "da\u00b7mals", ",", "als", "er", "euch", "ge\u00b7w\u00e4hlt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "$,", "KOUS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und taumelnd noch von seinen Barrikaden.", "tokens": ["Und", "tau\u00b7melnd", "noch", "von", "sei\u00b7nen", "Bar\u00b7ri\u00b7ka\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Wer kennt euch? Wessen Name klingt f\u00fcr voll,", "tokens": ["Wer", "kennt", "euch", "?", "Wes\u00b7sen", "Na\u00b7me", "klingt", "f\u00fcr", "voll", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "NN", "NN", "VVFIN", "APPR", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nicht selbst den Nachbarn neu durch seine Fremdheit?", "tokens": ["Nicht", "selbst", "den", "Nach\u00b7barn", "neu", "durch", "sei\u00b7ne", "Fremd\u00b7heit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {