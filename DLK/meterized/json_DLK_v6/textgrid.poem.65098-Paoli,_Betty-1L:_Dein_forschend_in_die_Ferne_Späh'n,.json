{"textgrid.poem.65098": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dein forschend in die Ferne Sp\u00e4h'n,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein forschend in die Ferne Sp\u00e4h'n,", "tokens": ["Dein", "for\u00b7schend", "in", "die", "Fer\u00b7ne", "Sp\u00e4h'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVPP", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Glaub' mir, es wird dir wenig frommen!", "tokens": ["Glaub'", "mir", ",", "es", "wird", "dir", "we\u00b7nig", "from\u00b7men", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "VAFIN", "PPER", "PIAT", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du kannst kein Gotteswort versteh'n,", "tokens": ["Du", "kannst", "kein", "Got\u00b7tes\u00b7wort", "ver\u00b7steh'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn du's nicht schon in dir vernommen.", "tokens": ["Wenn", "du's", "nicht", "schon", "in", "dir", "ver\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PTKNEG", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Kein Geistesstrahl, der auf dich f\u00e4llt,", "tokens": ["Kein", "Geis\u00b7tes\u00b7strahl", ",", "der", "auf", "dich", "f\u00e4llt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Macht es vor deinen Blicken tagen,", "tokens": ["Macht", "es", "vor", "dei\u00b7nen", "Bli\u00b7cken", "ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Wenn nicht in deiner innern Welt", "tokens": ["Wenn", "nicht", "in", "dei\u00b7ner", "in\u00b7nern", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du ahnend ihn schon l\u00e4ngst getragen!", "tokens": ["Du", "ah\u00b7nend", "ihn", "schon", "l\u00e4ngst", "ge\u00b7tra\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Von einem Wahn zum andern irrt,", "tokens": ["Von", "ei\u00b7nem", "Wahn", "zum", "an\u00b7dern", "irrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer fremde Weisheit sich erkoren,", "tokens": ["Wer", "frem\u00b7de", "Weis\u00b7heit", "sich", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Denn eine neue Wahrheit wird", "tokens": ["Denn", "ei\u00b7ne", "neu\u00b7e", "Wahr\u00b7heit", "wird"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit jedem Menschen neu geboren.", "tokens": ["Mit", "je\u00b7dem", "Men\u00b7schen", "neu", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sie thront in deiner eig'nen Brust,", "tokens": ["Sie", "thront", "in", "dei\u00b7ner", "eig'\u00b7nen", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es thut nicht Noth nach ihr zu rennen;", "tokens": ["Es", "thut", "nicht", "Noth", "nach", "ihr", "zu", "ren\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "NN", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Allein der Sinne tr\u00fcber Wust", "tokens": ["Al\u00b7lein", "der", "Sin\u00b7ne", "tr\u00fc\u00b7ber", "Wust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verhindert dich, sie zu erkennen.", "tokens": ["Ver\u00b7hin\u00b7dert", "dich", ",", "sie", "zu", "er\u00b7ken\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Nur diesen Gegner, diesen nur,", "tokens": ["Nur", "die\u00b7sen", "Geg\u00b7ner", ",", "die\u00b7sen", "nur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "$,", "PDAT", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hast siegreich du zu \u00fcberwinden,", "tokens": ["Hast", "sieg\u00b7reich", "du", "zu", "\u00fc\u00b7berw\u00b7in\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dann wirst du ihre Strahlenspur", "tokens": ["Dann", "wirst", "du", "ih\u00b7re", "Strah\u00b7len\u00b7spur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In deiner tiefsten Seele finden.", "tokens": ["In", "dei\u00b7ner", "tiefs\u00b7ten", "See\u00b7le", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Doch was ein And'rer sich errang,", "tokens": ["Doch", "was", "ein", "An\u00b7d'\u00b7rer", "sich", "er\u00b7rang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "PRF", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das kommt dir nimmermehr zu gute;", "tokens": ["Das", "kommt", "dir", "nim\u00b7mer\u00b7mehr", "zu", "gu\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Echt ist nur das, was dir entsprang,", "tokens": ["Echt", "ist", "nur", "das", ",", "was", "dir", "ent\u00b7sprang", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "PDS", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was Blut von deinem eig'nen Blute.", "tokens": ["Was", "Blut", "von", "dei\u00b7nem", "eig'\u00b7nen", "Blu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Es kann dich keine fremde Hand", "tokens": ["Es", "kann", "dich", "kei\u00b7ne", "frem\u00b7de", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Dienst der ernsten G\u00f6ttin weihen:", "tokens": ["Zum", "Dienst", "der", "erns\u00b7ten", "G\u00f6t\u00b7tin", "wei\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nur Jener wei\u00df, der selbst erkannt,", "tokens": ["Nur", "Je\u00b7ner", "wei\u00df", ",", "der", "selbst", "er\u00b7kannt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "VVFIN", "$,", "PRELS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erkennen hei\u00dft: nur sich Befreien.", "tokens": ["Er\u00b7ken\u00b7nen", "hei\u00dft", ":", "nur", "sich", "Be\u00b7frei\u00b7en", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$.", "ADV", "PRF", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}