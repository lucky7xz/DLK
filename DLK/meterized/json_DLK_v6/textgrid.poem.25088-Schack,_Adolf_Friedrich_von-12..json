{"textgrid.poem.25088": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So find' ich wieder dich nach Jahren", "tokens": ["So", "find'", "ich", "wie\u00b7der", "dich", "nach", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und sehe wiederum die Zeit,", "tokens": ["Und", "se\u00b7he", "wie\u00b7de\u00b7rum", "die", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als schuldlos wir und gl\u00fccklich waren,", "tokens": ["Als", "schuld\u00b7los", "wir", "und", "gl\u00fcck\u00b7lich", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "KON", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erstehen, doch im Sterbekleid.", "tokens": ["Er\u00b7ste\u00b7hen", ",", "doch", "im", "Ster\u00b7be\u00b7kleid", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie matt dahin durch deine Rechte", "tokens": ["Wie", "matt", "da\u00b7hin", "durch", "dei\u00b7ne", "Rech\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PAV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Blau der welken Ader schleicht!", "tokens": ["Das", "Blau", "der", "wel\u00b7ken", "A\u00b7der", "schleicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie hat der Gram durchweinter N\u00e4chte", "tokens": ["Wie", "hat", "der", "Gram", "durch\u00b7wein\u00b7ter", "N\u00e4ch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das sch\u00f6ne Antlitz dir gebleicht!", "tokens": ["Das", "sch\u00f6\u00b7ne", "Ant\u00b7litz", "dir", "ge\u00b7bleicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wozu die alte Liebe wecken?", "tokens": ["Wo\u00b7zu", "die", "al\u00b7te", "Lie\u00b7be", "we\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Entsteigen w\u00fcrde, schattenbleich,", "tokens": ["Ent\u00b7stei\u00b7gen", "w\u00fcr\u00b7de", ",", "schat\u00b7ten\u00b7bleich", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur ihr Gespenst, um uns zu schrecken,", "tokens": ["Nur", "ihr", "Ge\u00b7spenst", ",", "um", "uns", "zu", "schre\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,", "KOUI", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie selber nicht, dem Totenreich.", "tokens": ["Sie", "sel\u00b7ber", "nicht", ",", "dem", "To\u00b7ten\u00b7reich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKNEG", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "F\u00fcr immer sei es denn geschieden,", "tokens": ["F\u00fcr", "im\u00b7mer", "sei", "es", "denn", "ge\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie wir f\u00fcr immer ausgeliebt!", "tokens": ["Wie", "wir", "f\u00fcr", "im\u00b7mer", "aus\u00b7ge\u00b7liebt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Tode such', wie ich, den Frieden,", "tokens": ["Im", "To\u00b7de", "such'", ",", "wie", "ich", ",", "den", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "PWAV", "PPER", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den uns das Leben nimmer giebt.", "tokens": ["Den", "uns", "das", "Le\u00b7ben", "nim\u00b7mer", "giebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}