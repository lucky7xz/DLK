{"dta.poem.541": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Dorinde  will nicht Mutter werden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Dorinde sehnst du dich nicht auch nach einen Mann/", "tokens": ["Do\u00b7rin\u00b7de", "sehnst", "du", "dich", "nicht", "auch", "nach", "ei\u00b7nen", "Mann", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PRF", "PTKNEG", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der dir die lange Zeit mit Lust vertreiben kan?", "tokens": ["Der", "dir", "die", "lan\u00b7ge", "Zeit", "mit", "Lust", "ver\u00b7trei\u00b7ben", "kan", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ha! sprichst du hier zu Ja/ wart nur/ ich will dich fangen/", "tokens": ["Ha", "!", "sprichst", "du", "hier", "zu", "Ja", "/", "wart", "nur", "/", "ich", "will", "dich", "fan\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "ADV", "APPR", "PTKANT", "$(", "VVFIN", "ADV", "$(", "PPER", "VMFIN", "PPER", "VVFIN", "$("], "meter": "-+-+-+---+-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Weil selten Mann und Weib ein ander Absehn hat/", "tokens": ["Weil", "sel\u00b7ten", "Mann", "und", "Weib", "ein", "an\u00b7der", "Ab\u00b7sehn", "hat", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "KON", "NN", "ART", "ADJA", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Als warum ", "tokens": ["Als", "wa\u00b7rum"], "token_info": ["word", "word"], "pos": ["KOUS", "PWAV"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "So sehnst du dich darnach/ was du nicht wilt verlangen.", "tokens": ["So", "sehnst", "du", "dich", "dar\u00b7nach", "/", "was", "du", "nicht", "wilt", "ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PAV", "$(", "PWS", "PPER", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}