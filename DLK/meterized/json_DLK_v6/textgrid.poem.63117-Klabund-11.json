{"textgrid.poem.63117": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "11", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin so weit von dir entfernt!", "tokens": ["Ich", "bin", "so", "weit", "von", "dir", "ent\u00b7fernt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O dieses Elend, das die Brust durchl\u00e4rmt.", "tokens": ["O", "die\u00b7ses", "E\u00b7lend", ",", "das", "die", "Brust", "durch\u00b7l\u00e4rmt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDAT", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bin ich es denn, der dunkel im Gesicht", "tokens": ["Bin", "ich", "es", "denn", ",", "der", "dun\u00b7kel", "im", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "$,", "PRELS", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So Stern auf Stern in blaue Zelte flicht?", "tokens": ["So", "Stern", "auf", "Stern", "in", "blau\u00b7e", "Zel\u00b7te", "flicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie habe ich den Tag so tr\u00fcb verbracht", "tokens": ["Wie", "ha\u00b7be", "ich", "den", "Tag", "so", "tr\u00fcb", "ver\u00b7bracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ART", "NN", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mit W\u00fcrfelspiel und k\u00fcnstlicherer Nacht.", "tokens": ["Mit", "W\u00fcr\u00b7fel\u00b7spiel", "und", "k\u00fcnst\u00b7li\u00b7che\u00b7rer", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Nichts will ich, als dich lieben; nimm mich hin,", "tokens": ["Nichts", "will", "ich", ",", "als", "dich", "lie\u00b7ben", ";", "nimm", "mich", "hin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "$,", "KOUS", "PPER", "VVINF", "$.", "VVIMP", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Weil ich in deinem Netz gefangen bin.", "tokens": ["Weil", "ich", "in", "dei\u00b7nem", "Netz", "ge\u00b7fan\u00b7gen", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "So schwer schon sinkt aufs Blatt mir Haupt und Kinn,", "tokens": ["So", "schwer", "schon", "sinkt", "aufs", "Blatt", "mir", "Haupt", "und", "Kinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "VVFIN", "APPRART", "NN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "O aller Strahlen sch\u00f6ne Spinnerin!", "tokens": ["O", "al\u00b7ler", "Strah\u00b7len", "sch\u00f6\u00b7ne", "Spin\u00b7ne\u00b7rin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}