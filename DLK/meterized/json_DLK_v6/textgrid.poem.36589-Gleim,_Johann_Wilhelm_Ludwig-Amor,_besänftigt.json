{"textgrid.poem.36589": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Amor, bes\u00e4nftigt", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Amor z\u00fcrnt, ich soll nicht trinken,", "tokens": ["A\u00b7mor", "z\u00fcrnt", ",", "ich", "soll", "nicht", "trin\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lieben soll ich nur!", "tokens": ["Lie\u00b7ben", "soll", "ich", "nur", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "VMFIN", "PPER", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Sich betrinken, sagt er, w\u00e4re", "tokens": ["Sich", "be\u00b7trin\u00b7ken", ",", "sagt", "er", ",", "w\u00e4\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PRF", "VVINF", "$,", "VVFIN", "PPER", "$,", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gegen die Natur.", "tokens": ["Ge\u00b7gen", "die", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Kluge M\u00e4nner, sagt er, lieben", "tokens": ["Klu\u00b7ge", "M\u00e4n\u00b7ner", ",", "sagt", "er", ",", "lie\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "$,", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fcr das Vaterland;", "tokens": ["F\u00fcr", "das", "Va\u00b7ter\u00b7land", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Dumme M\u00e4nner, sagt er, trinken", "tokens": ["Dum\u00b7me", "M\u00e4n\u00b7ner", ",", "sagt", "er", ",", "trin\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich um den Verstand.", "tokens": ["Sich", "um", "den", "Ver\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Amor, sag' ich, lieber Amor!", "tokens": ["A\u00b7mor", ",", "sag'", "ich", ",", "lie\u00b7ber", "A\u00b7mor", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "$,", "ADV", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Z\u00fcrne nicht so sehr!", "tokens": ["Z\u00fcr\u00b7ne", "nicht", "so", "sehr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ADV", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wie gerufen kommt Belinde", "tokens": ["Wie", "ge\u00b7ru\u00b7fen", "kommt", "Be\u00b7lin\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VVPP", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu der Kelter her!", "tokens": ["Zu", "der", "Kel\u00b7ter", "her", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "--+-+", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "Eine hohle Hand zu machen,", "tokens": ["Ei\u00b7ne", "hoh\u00b7le", "Hand", "zu", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lernet sie von mir;", "tokens": ["Ler\u00b7net", "sie", "von", "mir", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "S\u00fc\u00dfen Most damit zu sch\u00f6pfen,", "tokens": ["S\u00fc\u00b7\u00dfen", "Most", "da\u00b7mit", "zu", "sch\u00f6p\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PAV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lernet sie von mir!", "tokens": ["Ler\u00b7net", "sie", "von", "mir", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Lieben will ich, und auch trinken:", "tokens": ["Lie\u00b7ben", "will", "ich", ",", "und", "auch", "trin\u00b7ken", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "VMFIN", "PPER", "$,", "KON", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus der hohlen Hand", "tokens": ["Aus", "der", "hoh\u00b7len", "Hand"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "--+-+", "measure": "anapaest.init"}, "line.3": {"text": "Meiner z\u00e4rtlichen Belinde", "tokens": ["Mei\u00b7ner", "z\u00e4rt\u00b7li\u00b7chen", "Be\u00b7lin\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Trink' ich mir Verstand!", "tokens": ["Trink'", "ich", "mir", "Ver\u00b7stand", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}