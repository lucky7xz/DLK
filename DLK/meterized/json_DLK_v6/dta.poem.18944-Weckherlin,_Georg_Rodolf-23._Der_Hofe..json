{"dta.poem.18944": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "23.  \n  Der Hofe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.99"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Der lustig wolte mich gern lehren/", "tokens": ["Der", "lus\u00b7tig", "wol\u00b7te", "mich", "gern", "leh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VMFIN", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df der Hof gleich dem Paradi\u00df:", "tokens": ["Da\u00df", "der", "Hof", "gleich", "dem", "Pa\u00b7ra\u00b7di\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Dem Gre\u00fcner (den spruch zuverkehren)", "tokens": ["Dem", "Gre\u00fc\u00b7ner", "(", "den", "spruch", "zu\u00b7ver\u00b7keh\u00b7ren", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist der Hof wie die H\u00f6ll gewi\u00df/", "tokens": ["Ist", "der", "Hof", "wie", "die", "H\u00f6ll", "ge\u00b7wi\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "KOKOM", "ART", "NN", "ADV", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Weil da kein vnderschid (sprechen Sie) der personen.", "tokens": ["Weil", "da", "kein", "vn\u00b7der\u00b7schid", "(", "spre\u00b7chen", "Sie", ")", "der", "per\u00b7so\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIAT", "NN", "$(", "VVFIN", "PPER", "$(", "ART", "ADJA", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Ich sag/ der Hof ist Hof/ nicht H\u00f6ll noch Himmel-", "tokens": ["Ich", "sag", "/", "der", "Hof", "ist", "Hof", "/", "nicht", "H\u00f6ll", "noch", "Him\u00b7mel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "ART", "NN", "VAFIN", "NE", "$(", "PTKNEG", "NN", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "reich;", "tokens": ["reich", ";"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "-", "measure": "single.down"}, "line.8": {"text": "Da man/ weil s\u00fcnd der tugent gleich/", "tokens": ["Da", "man", "/", "weil", "s\u00fcnd", "der", "tu\u00b7gent", "gleich", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$(", "KOUS", "ADJD", "ART", "NN", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Was b\u00f6\u00df nicht strafen will/ Noch auch/ was gut/", "tokens": ["Was", "b\u00f6\u00df", "nicht", "stra\u00b7fen", "will", "/", "Noch", "auch", "/", "was", "gut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PTKNEG", "VVINF", "VMFIN", "$(", "ADV", "ADV", "$(", "PWS", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "belohnen.", "tokens": ["be\u00b7loh\u00b7nen", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}