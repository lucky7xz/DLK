{"textgrid.poem.57921": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "\u00e0 la Haby", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An der Bellawuppdichbr\u00fccke", "tokens": ["An", "der", "Bel\u00b7la\u00b7wupp\u00b7dich\u00b7br\u00fc\u00b7cke"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist Hannovers Wappen dran,", "tokens": ["Ist", "Han\u00b7no\u00b7vers", "Wap\u00b7pen", "dran", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wovon jeder Maschparkwandrer", "tokens": ["Wo\u00b7von", "je\u00b7der", "Maschpark\u00b7wand\u00b7rer"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PIAT", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Leicht sich \u00fcberziehen kann.", "tokens": ["Leicht", "sich", "\u00fc\u00b7ber\u00b7zie\u00b7hen", "kann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aber es ist fortgeschritten", "tokens": ["A\u00b7ber", "es", "ist", "fort\u00b7ge\u00b7schrit\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit der Zeit, die Enden sind", "tokens": ["Mit", "der", "Zeit", ",", "die", "En\u00b7den", "sind"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "ART", "NN", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Von dem Kleeblatt ausgezogen,", "tokens": ["Von", "dem", "Klee\u00b7blatt", "aus\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bbjugendstil!\u00ab ruft Greis und Kind.", "tokens": ["\u00bb", "ju\u00b7gend\u00b7stil", "!", "\u00ab", "ruft", "Greis", "und", "Kind", "."], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$.", "$(", "VVFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ja der Fortschritt, ja der bricht sich", "tokens": ["Ja", "der", "Fort\u00b7schritt", ",", "ja", "der", "bricht", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "ART", "NN", "$,", "ADV", "ART", "VVFIN", "PRF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bahn an unserm Leinestrand,", "tokens": ["Bahn", "an", "un\u00b7serm", "Lei\u00b7nes\u00b7trand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und man wird noch viel erleben,", "tokens": ["Und", "man", "wird", "noch", "viel", "er\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Man erlebt noch allerhand.", "tokens": ["Man", "er\u00b7lebt", "noch", "al\u00b7ler\u00b7hand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "PIS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Gar zu altmodsch und zu steif ist", "tokens": ["Gar", "zu", "alt\u00b7modsch", "und", "zu", "steif", "ist"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKA", "ADJD", "KON", "PTKA", "ADJD", "VAFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Unser alter Marktkirchturm,", "tokens": ["Un\u00b7ser", "al\u00b7ter", "Markt\u00b7kirch\u00b7turm", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf, und brennt ihn ", "tokens": ["Auf", ",", "und", "brennt", "ihn"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["APPR", "$,", "KON", "VVFIN", "PPER"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Kr\u00fcmmt ihn ", "tokens": ["Kr\u00fcmmt", "ihn"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "+-", "measure": "trochaic.single"}}}}}