{"textgrid.poem.57516": {"metadata": {"author": {"name": "Gottsched, Johann Christoph", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Jahr ist hin, o Sch\u00f6nste! da\u00df mein Bild", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Jahr ist hin, o Sch\u00f6nste! da\u00df mein Bild", "tokens": ["Ein", "Jahr", "ist", "hin", ",", "o", "Sch\u00f6ns\u00b7te", "!", "da\u00df", "mein", "Bild"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,", "FM", "NN", "$.", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sich schon bey dir zum Opfer eingefunden:", "tokens": ["Sich", "schon", "bey", "dir", "zum", "Op\u00b7fer", "ein\u00b7ge\u00b7fun\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch ist mein Wunsch nach deinem nicht erf\u00fcllt;", "tokens": ["Doch", "ist", "mein", "Wunsch", "nach", "dei\u00b7nem", "nicht", "er\u00b7f\u00fcllt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So sehr seit dem die Herzen sich verbunden.", "tokens": ["So", "sehr", "seit", "dem", "die", "Her\u00b7zen", "sich", "ver\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "ART", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Erfreue mich, dafern du lieben kannst,", "tokens": ["Er\u00b7freu\u00b7e", "mich", ",", "da\u00b7fern", "du", "lie\u00b7ben", "kannst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und la\u00df dein Herz durch keinen Einwurf st\u00f6ren.", "tokens": ["Und", "la\u00df", "dein", "Herz", "durch", "kei\u00b7nen", "Ein\u00b7wurf", "st\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPOSAT", "NN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ja, wo du mich nicht aus der Brust verbannst:", "tokens": ["Ja", ",", "wo", "du", "mich", "nicht", "aus", "der", "Brust", "ver\u00b7bannst", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PWAV", "PPER", "PRF", "PTKNEG", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So la\u00df dein Bild mich deine Neigung lehren.", "tokens": ["So", "la\u00df", "dein", "Bild", "mich", "dei\u00b7ne", "Nei\u00b7gung", "leh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPOSAT", "NN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der edle Geist, der deine Stirn erhebt,", "tokens": ["Der", "ed\u00b7le", "Geist", ",", "der", "dei\u00b7ne", "Stirn", "er\u00b7hebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der helle Blitz, aus deinen klugen Blicken,", "tokens": ["Der", "hel\u00b7le", "Blitz", ",", "aus", "dei\u00b7nen", "klu\u00b7gen", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wird deinen Freund, der nur durch dich noch lebt,", "tokens": ["Wird", "dei\u00b7nen", "Freund", ",", "der", "nur", "durch", "dich", "noch", "lebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "PRELS", "ADV", "APPR", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In manchem Gram, an deiner statt, erquicken.", "tokens": ["In", "man\u00b7chem", "Gram", ",", "an", "dei\u00b7ner", "statt", ",", "er\u00b7qui\u00b7cken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Victoria! mein Leben, Herz und Licht!", "tokens": ["Vic\u00b7to\u00b7ria", "!", "mein", "Le\u00b7ben", ",", "Herz", "und", "Licht", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPOSAT", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Fleh ich umsonst um deinen blo\u00dfen Schatten;", "tokens": ["Fleh", "ich", "um\u00b7sonst", "um", "dei\u00b7nen", "blo\u00b7\u00dfen", "Schat\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So schm\u00e4uchle mir mit deiner Liebe nicht:", "tokens": ["So", "schm\u00e4uch\u00b7le", "mir", "mit", "dei\u00b7ner", "Lie\u00b7be", "nicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie schwer wird sich das Wesen selber gatten?", "tokens": ["Wie", "schwer", "wird", "sich", "das", "We\u00b7sen", "sel\u00b7ber", "gat\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PRF", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}