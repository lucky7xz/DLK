{"textgrid.poem.52654": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Gl\u00fchtrunkverse", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O du gl\u00fchender Trank,", "tokens": ["O", "du", "gl\u00fc\u00b7hen\u00b7der", "Trank", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Den im traulichen Rund", "tokens": ["Den", "im", "trau\u00b7li\u00b7chen", "Rund"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPRART", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Den wir trinken bei k\u00f6stlicher Rede,", "tokens": ["Den", "wir", "trin\u00b7ken", "bei", "k\u00f6st\u00b7li\u00b7cher", "Re\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Wie du l\u00f6sest den Schwank", "tokens": ["Wie", "du", "l\u00f6\u00b7sest", "den", "Schwank"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Und belebest den Mund", "tokens": ["Und", "be\u00b7le\u00b7best", "den", "Mund"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Zu Gesang und zu geistiger Fehde!", "tokens": ["Zu", "Ge\u00b7sang", "und", "zu", "geis\u00b7ti\u00b7ger", "Feh\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "ADJA", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.2": {"line.1": {"text": "Wohl gedenkt uns die Zeit \u2013", "tokens": ["Wohl", "ge\u00b7denkt", "uns", "die", "Zeit", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "O wie lange das ist \u2013", "tokens": ["O", "wie", "lan\u00b7ge", "das", "ist", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADV", "PDS", "VAFIN", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Da wir fr\u00f6hlich beisammen so waren;", "tokens": ["Da", "wir", "fr\u00f6h\u00b7lich", "bei\u00b7sam\u00b7men", "so", "wa\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "ADV", "VAFIN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Und dar\u00fcber ging Leid", "tokens": ["Und", "da\u00b7r\u00fc\u00b7ber", "ging", "Leid"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PAV", "VVFIN", "NN"], "meter": "-+---+", "measure": "dactylic.init"}, "line.5": {"text": "Und so Manchen vermi\u00dft", "tokens": ["Und", "so", "Man\u00b7chen", "ver\u00b7mi\u00dft"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "PIS", "VVFIN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Man im sch\u00f6nen Verein mit den Jahren.", "tokens": ["Man", "im", "sch\u00f6\u00b7nen", "Ver\u00b7ein", "mit", "den", "Jah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPRART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Ja, die Besten dahin!", "tokens": ["Ja", ",", "die", "Bes\u00b7ten", "da\u00b7hin", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "PAV", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Unerbittlich Geschick!", "tokens": ["Un\u00b7er\u00b7bitt\u00b7lich", "Ge\u00b7schick", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wars Zufall, war es ein Walten?", "tokens": ["Wars", "Zu\u00b7fall", ",", "war", "es", "ein", "Wal\u00b7ten", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Doch der g\u00f6ttliche Sinn", "tokens": ["Doch", "der", "g\u00f6tt\u00b7li\u00b7che", "Sinn"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Darf bleiben zur\u00fcck,", "tokens": ["Darf", "blei\u00b7ben", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Die Erscheinung nur l\u00e4\u00dft sich nicht halten.", "tokens": ["Die", "Er\u00b7schei\u00b7nung", "nur", "l\u00e4\u00dft", "sich", "nicht", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PRF", "PTKNEG", "VVINF", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}}}}