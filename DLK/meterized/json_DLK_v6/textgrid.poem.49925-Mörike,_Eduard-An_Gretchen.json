{"textgrid.poem.49925": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "An Gretchen", "genre": "verse", "period": "N.A.", "pub_year": 1864, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "J\u00fcngst, als unsere M\u00e4dchen, zur Fastnacht beide verkleidet,", "tokens": ["J\u00fcngst", ",", "als", "un\u00b7se\u00b7re", "M\u00e4d\u00b7chen", ",", "zur", "Fast\u00b7nacht", "bei\u00b7de", "ver\u00b7klei\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPOSAT", "NN", "$,", "APPRART", "NN", "PIS", "VVPP", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Im Halbdunkel sich scheu erst an der T\u00fcre gezeigt,", "tokens": ["Im", "Halb\u00b7dun\u00b7kel", "sich", "scheu", "erst", "an", "der", "T\u00fc\u00b7re", "ge\u00b7zeigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "ADJD", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-++--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Dann sich die Blonde als Sch\u00e4ferin dir, mir aber die kleine", "tokens": ["Dann", "sich", "die", "Blon\u00b7de", "als", "Sch\u00e4\u00b7fe\u00b7rin", "dir", ",", "mir", "a\u00b7ber", "die", "klei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PRF", "ART", "NN", "KOUS", "NN", "PPER", "$,", "PPER", "ADV", "ART", "ADJA"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Mohrin mit Lachen zumal warf in den offenen Arm,", "tokens": ["Moh\u00b7rin", "mit", "La\u00b7chen", "zu\u00b7mal", "warf", "in", "den", "of\u00b7fe\u00b7nen", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Und du, Liebste, von fern mein Gef\u00fchl nicht ahnend, ins Ohr mir", "tokens": ["Und", "du", ",", "Liebs\u00b7te", ",", "von", "fern", "mein", "Ge\u00b7f\u00fchl", "nicht", "ah\u00b7nend", ",", "ins", "Ohr", "mir"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "NN", "$,", "APPR", "ADJD", "PPOSAT", "NN", "PTKNEG", "ADJD", "$,", "APPRART", "NN", "PPER"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "(der ich verbl\u00fcfft dasa\u00df) fl\u00fcstertest \u00bblobe sie doch\u00ab \u2013 \u2013:", "tokens": ["(", "der", "ich", "ver\u00b7bl\u00fcfft", "da\u00b7sa\u00df", ")", "fl\u00fcs\u00b7ter\u00b7test", "\u00bb", "lo\u00b7be", "sie", "doch", "\u00ab", "\u2013", "\u2013", ":"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["$(", "PRELS", "PPER", "VVFIN", "ART", "$(", "VVFIN", "$(", "VVFIN", "PPER", "ADV", "$(", "$(", "$(", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "O wie gedacht ich der Zeit, da ", "tokens": ["O", "wie", "ge\u00b7dacht", "ich", "der", "Zeit", ",", "da"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NE", "KOKOM", "VVPP", "PPER", "ART", "NN", "$,", "KOUS"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.8": {"text": "Beide noch fremd, ja du selber noch hie\u00dfest ein Kind.", "tokens": ["Bei\u00b7de", "noch", "fremd", ",", "ja", "du", "sel\u00b7ber", "noch", "hie\u00b7\u00dfest", "ein", "Kind", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "ADJD", "$,", "ADV", "PPER", "ADV", "ADV", "ADV", "ART", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.9": {"text": "Einst und jetzt im Wechsel \u2013 ein fliegender Blitz der Gedanken", "tokens": ["Einst", "und", "jetzt", "im", "Wech\u00b7sel", "\u2013", "ein", "flie\u00b7gen\u00b7der", "Blitz", "der", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "KON", "ADV", "APPRART", "NN", "$(", "ART", "ADJA", "NN", "ART", "NN"], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Machte mich stumm, und hoch wallte vor Freuden mein Herz.", "tokens": ["Mach\u00b7te", "mich", "stumm", ",", "und", "hoch", "wall\u00b7te", "vor", "Freu\u00b7den", "mein", "Herz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "KON", "ADJD", "VVFIN", "APPR", "NN", "PPOSAT", "NN", "$."], "meter": "+--+-+---+--+", "measure": "iambic.penta.invert"}}}}}