{"textgrid.poem.51053": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbnur Ordnung, Anmut!\u00ab t\u00f6nt es immerdar.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbnur Ordnung, Anmut!\u00ab t\u00f6nt es immerdar.", "tokens": ["\u00bb", "nur", "Ord\u00b7nung", ",", "An\u00b7mut", "!", "\u00ab", "t\u00f6nt", "es", "im\u00b7mer\u00b7dar", "."], "token_info": ["punct", "word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "NN", "$,", "NN", "$.", "$(", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wer spricht von Ordnung, wo die Berge wanken?", "tokens": ["Wer", "spricht", "von", "Ord\u00b7nung", ",", "wo", "die", "Ber\u00b7ge", "wan\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "NN", "$,", "PWAV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wer spricht von Anmut, w\u00e4hrend die Gedanken", "tokens": ["Wer", "spricht", "von", "An\u00b7mut", ",", "w\u00e4h\u00b7rend", "die", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "APPR", "NN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Noch schutzlos irren mit zerrauftem Haar?", "tokens": ["Noch", "schutz\u00b7los", "ir\u00b7ren", "mit", "zer\u00b7rauf\u00b7tem", "Haar", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Noch k\u00e4mpfen wir, durchringend Jahr um Jahr,", "tokens": ["Noch", "k\u00e4mp\u00b7fen", "wir", ",", "durch\u00b7rin\u00b7gend", "Jahr", "um", "Jahr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVPP", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Noch tut uns not ein scharf, ob unsch\u00f6n Zanken;", "tokens": ["Noch", "tut", "uns", "not", "ein", "scharf", ",", "ob", "un\u00b7sch\u00f6n", "Zan\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "ART", "ADJD", "$,", "KOUS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durch dieses Zeitenwaldes wirre Ranken", "tokens": ["Durch", "die\u00b7ses", "Zei\u00b7ten\u00b7wal\u00b7des", "wir\u00b7re", "Ran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Lacht eine Zukunftsau noch nicht uns klar.", "tokens": ["Lacht", "ei\u00b7ne", "Zu\u00b7kunft\u00b7sau", "noch", "nicht", "uns", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "PTKNEG", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und Goethe ist ein Kleinod, das im Kriege", "tokens": ["Und", "Goe\u00b7the", "ist", "ein", "Klei\u00b7nod", ",", "das", "im", "Krie\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NE", "VAFIN", "ART", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Man still vergr\u00e4bt im sichersten Gew\u00f6lbe,", "tokens": ["Man", "still", "ver\u00b7gr\u00e4bt", "im", "si\u00b7chers\u00b7ten", "Ge\u00b7w\u00f6l\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Es bergend vor des rauhen Feindes Hand;", "tokens": ["Es", "ber\u00b7gend", "vor", "des", "rau\u00b7hen", "Fein\u00b7des", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "---+-+-+-+", "measure": "zehnsilber"}}, "stanza.4": {"line.1": {"text": "Doch ist der Feind verjagt, nach hei\u00dfem Siege", "tokens": ["Doch", "ist", "der", "Feind", "ver\u00b7jagt", ",", "nach", "hei\u00b7\u00dfem", "Sie\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "NN", "VVPP", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Holt man erinnrungsfroh hervor dasselbe", "tokens": ["Holt", "man", "e\u00b7rinn\u00b7rungs\u00b7froh", "her\u00b7vor", "das\u00b7sel\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ADJD", "PTKVZ", "PDAT"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und l\u00e4\u00dft es friedlich leuchten durch das Land.", "tokens": ["Und", "l\u00e4\u00dft", "es", "fried\u00b7lich", "leuch\u00b7ten", "durch", "das", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}