{"textgrid.poem.33366": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Am Geburtsfeste der Gr\u00e4fin E.v.T*.", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir alle freuten uns des Tag's,", "tokens": ["Wir", "al\u00b7le", "freu\u00b7ten", "uns", "des", "Tag's", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der dich zur Welt gebracht,", "tokens": ["Der", "dich", "zur", "Welt", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und dachten an den Umstand nicht,", "tokens": ["Und", "dach\u00b7ten", "an", "den", "Um\u00b7stand", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der dir des Lebens s\u00fcsse Pflicht", "tokens": ["Der", "dir", "des", "Le\u00b7bens", "s\u00fcs\u00b7se", "Pflicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So schwer und bitter macht.", "tokens": ["So", "schwer", "und", "bit\u00b7ter", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ach! Mancher, der sein Pl\u00e4tzchen hier", "tokens": ["Ach", "!", "Man\u00b7cher", ",", "der", "sein", "Pl\u00e4tz\u00b7chen", "hier"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "NN", "$,", "PRELS", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Oft mehr entehrt, als ziert,", "tokens": ["Oft", "mehr", "ent\u00b7ehrt", ",", "als", "ziert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$,", "KOUS", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hat doch hienieden Luft genug,", "tokens": ["Hat", "doch", "hien\u00b7ie\u00b7den", "Luft", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Inde\u00df dir jeder Athemzug", "tokens": ["In\u00b7de\u00df", "dir", "je\u00b7der", "A\u00b7them\u00b7zug"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zum lauten Seufzer wird.", "tokens": ["Zum", "lau\u00b7ten", "Seuf\u00b7zer", "wird", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und trotz der vielen Seufzer scheint", "tokens": ["Und", "trotz", "der", "vie\u00b7len", "Seuf\u00b7zer", "scheint"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Leben dir nicht hart;", "tokens": ["Das", "Le\u00b7ben", "dir", "nicht", "hart", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denn niemand ist, der lebensfroh,", "tokens": ["Denn", "nie\u00b7mand", "ist", ",", "der", "le\u00b7bens\u00b7froh", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie du, mit jedem Seufzer so", "tokens": ["Wie", "du", ",", "mit", "je\u00b7dem", "Seuf\u00b7zer", "so"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "$,", "APPR", "PIAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Freudenl\u00e4cheln paart.", "tokens": ["Ein", "Freu\u00b7den\u00b7l\u00e4\u00b7cheln", "paart", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}