{"textgrid.poem.57761": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Das Geheimnis", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ich gestern einsam ging", "tokens": ["Als", "ich", "ge\u00b7stern", "ein\u00b7sam", "ging"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Auf der gr\u00fcnen, gr\u00fcnen Heid,", "tokens": ["Auf", "der", "gr\u00fc\u00b7nen", ",", "gr\u00fc\u00b7nen", "Heid", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kam ein junger J\u00e4ger an,", "tokens": ["Kam", "ein", "jun\u00b7ger", "J\u00e4\u00b7ger", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Trug ein gr\u00fcnes, gr\u00fcnes Kleid;", "tokens": ["Trug", "ein", "gr\u00fc\u00b7nes", ",", "gr\u00fc\u00b7nes", "Kleid", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja gr\u00fcn ist die Heide,", "tokens": ["Ja", "gr\u00fcn", "ist", "die", "Hei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Die Heide ist gr\u00fcn,", "tokens": ["Die", "Hei\u00b7de", "ist", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Aber rot sind die Rosen,", "tokens": ["A\u00b7ber", "rot", "sind", "die", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Wenn sie da bl\u00fchn.", "tokens": ["Wenn", "sie", "da", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Wo die gr\u00fcnen Tannen stehn,", "tokens": ["Wo", "die", "gr\u00fc\u00b7nen", "Tan\u00b7nen", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist so weich das gr\u00fcne Moos,", "tokens": ["Ist", "so", "weich", "das", "gr\u00fc\u00b7ne", "Moos", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und da hat er mich gek\u00fc\u00dft,", "tokens": ["Und", "da", "hat", "er", "mich", "ge\u00b7k\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ich sa\u00df auf seinem Scho\u00df;", "tokens": ["Und", "ich", "sa\u00df", "auf", "sei\u00b7nem", "Scho\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja gr\u00fcn ist die Heide,", "tokens": ["Ja", "gr\u00fcn", "ist", "die", "Hei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Die Heide ist gr\u00fcn,", "tokens": ["Die", "Hei\u00b7de", "ist", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Aber rot sind die Rosen,", "tokens": ["A\u00b7ber", "rot", "sind", "die", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Wenn sie da bl\u00fchn.", "tokens": ["Wenn", "sie", "da", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Als ich dann nach Hause kam,", "tokens": ["Als", "ich", "dann", "nach", "Hau\u00b7se", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat die Mutter mich gefragt,", "tokens": ["Hat", "die", "Mut\u00b7ter", "mich", "ge\u00b7fragt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo ich war die ganze Zeit,", "tokens": ["Wo", "ich", "war", "die", "gan\u00b7ze", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ich hab es nicht gesagt;", "tokens": ["Und", "ich", "hab", "es", "nicht", "ge\u00b7sagt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja gr\u00fcn ist die Heide,", "tokens": ["Ja", "gr\u00fcn", "ist", "die", "Hei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Die Heide ist gr\u00fcn,", "tokens": ["Die", "Hei\u00b7de", "ist", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Aber rot sind die Rosen,", "tokens": ["A\u00b7ber", "rot", "sind", "die", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Wenn sie da bl\u00fchn.", "tokens": ["Wenn", "sie", "da", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Was die gr\u00fcne Heide wei\u00df,", "tokens": ["Was", "die", "gr\u00fc\u00b7ne", "Hei\u00b7de", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Geht die Mutter gar nichts an,", "tokens": ["Geht", "die", "Mut\u00b7ter", "gar", "nichts", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "PIS", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Niemand wei\u00df es au\u00dfer mir", "tokens": ["Nie\u00b7mand", "wei\u00df", "es", "au\u00b7\u00dfer", "mir"], "token_info": ["wor