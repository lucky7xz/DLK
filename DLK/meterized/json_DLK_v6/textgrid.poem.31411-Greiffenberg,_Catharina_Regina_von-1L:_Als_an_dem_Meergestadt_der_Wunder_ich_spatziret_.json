{"textgrid.poem.31411": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als an dem Meergestadt der Wunder ich spatziret /", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als an dem Meergestadt der Wunder ich spatziret /", "tokens": ["Als", "an", "dem", "Meer\u00b7ge\u00b7stadt", "der", "Wun\u00b7der", "ich", "spat\u00b7zi\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und in Betrachtung mich der tieffen Tieff vertiefft /", "tokens": ["und", "in", "Be\u00b7trach\u00b7tung", "mich", "der", "tief\u00b7fen", "Tieff", "ver\u00b7tiefft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PPER", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "bedunkt mich / da\u00df ein Schall mir aus den Wellen r\u00fcfft:", "tokens": ["be\u00b7dunkt", "mich", "/", "da\u00df", "ein", "Schall", "mir", "aus", "den", "Wel\u00b7len", "r\u00fcfft", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "KOUS", "ART", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dich hat des Himmels schlu\u00df zu diesem Flu\u00df gef\u00fchret /", "tokens": ["Dich", "hat", "des", "Him\u00b7mels", "schlu\u00df", "zu", "die\u00b7sem", "Flu\u00df", "ge\u00b7f\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "APPR", "PDAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "da\u00df unergr\u00fcndlich werd sein Ertzabgrund gesp\u00fcret.", "tokens": ["da\u00df", "un\u00b7er\u00b7gr\u00fcnd\u00b7lich", "werd", "sein", "Ertz\u00b7ab\u00b7grund", "ge\u00b7sp\u00fc\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "kein Anker oder Bley den gnaden-sandgrund trifft:", "tokens": ["kein", "An\u00b7ker", "o\u00b7der", "Bley", "den", "gna\u00b7den\u00b7sand\u00b7grund", "trifft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "doch mit dem Glaubenskahn er gl\u00fccklich wird beschifft.", "tokens": ["doch", "mit", "dem", "Glau\u00b7bens\u00b7kahn", "er", "gl\u00fcck\u00b7lich", "wird", "be\u00b7schifft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PPER", "ADJD", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "ein ferne Rei\u00df / zum Prei\u00df des Himmels / dir geb\u00fchret.", "tokens": ["ein", "fer\u00b7ne", "Rei\u00df", "/", "zum", "Prei\u00df", "des", "Him\u00b7mels", "/", "dir", "ge\u00b7b\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "APPRART", "NN", "ART", "NN", "$(", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die Wallfisch seiner Krafft / ein grosse Wasserquell /", "tokens": ["Die", "Wall\u00b7fisch", "sei\u00b7ner", "Krafft", "/", "ein", "gros\u00b7se", "Was\u00b7ser\u00b7quell", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "da\u00df sich dein Schiff empor k\u00f6nn' heben / schnell ergiessen:", "tokens": ["da\u00df", "sich", "dein", "Schiff", "em\u00b7por", "k\u00f6nn'", "he\u00b7ben", "/", "schnell", "er\u00b7gies\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "PTKVZ", "VMFIN", "VVINF", "$(", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der wunder-wellen-schwall wird es so stark fort schiessen /", "tokens": ["Der", "wun\u00b7der\u00b7wel\u00b7len\u00b7schwall", "wird", "es", "so", "stark", "fort", "schies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "ADJD", "PTKVZ", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "das deinem Sinn-begrieff sein Gl\u00fcckes lauff zu schnell.", "tokens": ["das", "dei\u00b7nem", "Sinn\u00b7be\u00b7grieff", "sein", "Gl\u00fc\u00b7ckes", "lauff", "zu", "schnell", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "PPOSAT", "NN", "APPR", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Jetzt sey dir unsre Flut ein spiegel seiner zier:", "tokens": ["Jetzt", "sey", "dir", "uns\u00b7re", "Flut", "ein", "spie\u00b7gel", "sei\u00b7ner", "zier", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "dann werd' ein Flu\u00df / da\u00df sich die Nachwelt schau in dir.", "tokens": ["dann", "werd'", "ein", "Flu\u00df", "/", "da\u00df", "sich", "die", "Nach\u00b7welt", "schau", "in", "dir", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$(", "KOUS", "PRF", "ART", "NN", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}