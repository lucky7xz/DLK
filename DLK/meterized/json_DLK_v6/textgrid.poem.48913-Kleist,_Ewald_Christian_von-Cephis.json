{"textgrid.poem.48913": {"metadata": {"author": {"name": "Kleist, Ewald Christian von", "birth": "N.A.", "death": "N.A."}, "title": "Cephis", "genre": "verse", "period": "N.A.", "pub_year": 1757, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbsey mir gegr\u00fc\u00dft Philint! sey mir gegr\u00fc\u00dft!", "tokens": ["\u00bb", "sey", "mir", "ge\u00b7gr\u00fc\u00dft", "Phi\u00b7lint", "!", "sey", "mir", "ge\u00b7gr\u00fc\u00dft", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "VVPP", "NE", "$.", "VAFIN", "PPER", "VVPP", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Geseegnet sey der Tag, der dich mir schenkt.", "tokens": ["Ge\u00b7seeg\u00b7net", "sey", "der", "Tag", ",", "der", "dich", "mir", "schenkt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O tugendhafter Greis, wie lange schon", "tokens": ["O", "tu\u00b7gend\u00b7haf\u00b7ter", "Greis", ",", "wie", "lan\u00b7ge", "schon"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "PWAV", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hab ich dich nicht gesehn! Das Alter hat", "tokens": ["Hab", "ich", "dich", "nicht", "ge\u00b7sehn", "!", "Das", "Al\u00b7ter", "hat"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "PRF", "PTKNEG", "VVPP", "$.", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Seitdem dein Haupt noch mehr mit Schnee bestreut.", "tokens": ["Seit\u00b7dem", "dein", "Haupt", "noch", "mehr", "mit", "Schnee", "be\u00b7streut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "ADV", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Komm labe dich mit mir in Schatten! Komm!", "tokens": ["Komm", "la\u00b7be", "dich", "mit", "mir", "in", "Schat\u00b7ten", "!", "Komm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "VVFIN", "PRF", "APPR", "PPER", "APPR", "NN", "$.", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der Weinstock winkt uns dort, dort winkt uns auch,", "tokens": ["Der", "Wein\u00b7stock", "winkt", "uns", "dort", ",", "dort", "winkt", "uns", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der s\u00fc\u00dfe Feigenbaum. Erqvicke dich", "tokens": ["Der", "s\u00fc\u00b7\u00dfe", "Fei\u00b7gen\u00b7baum", ".", "E\u00b7rqvi\u00b7cke", "dich"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "An ihren Fr\u00fcchten, die die Jahrszeit reift!\u00ab", "tokens": ["An", "ih\u00b7ren", "Fr\u00fcch\u00b7ten", ",", "die", "die", "Jahrs\u00b7zeit", "reift", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "So sagte Cephis, als Philint einmal", "tokens": ["So", "sag\u00b7te", "Ce\u00b7phis", ",", "als", "Phi\u00b7lint", "ein\u00b7mal"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "$,", "KOUS", "NE", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "In seinen Garten kam. Sie gingen hin. \u2013", "tokens": ["In", "sei\u00b7nen", "Gar\u00b7ten", "kam", ".", "Sie", "gin\u00b7gen", "hin", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$.", "PPER", "VVFIN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Der arme kranke Greis erquickte sich,", "tokens": ["Der", "ar\u00b7me", "kran\u00b7ke", "Greis", "er\u00b7quick\u00b7te", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Und pries den Feigenbaum und seine Frucht.", "tokens": ["Und", "pries", "den", "Fei\u00b7gen\u00b7baum", "und", "sei\u00b7ne", "Frucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Der Baum sey dein, Philint, sprach Cephis; ihn", "tokens": ["Der", "Baum", "sey", "dein", ",", "Phi\u00b7lint", ",", "sprach", "Ce\u00b7phis", ";", "ihn"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "$,", "NE", "$,", "VVFIN", "NE", "$.", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Bedeck ich k\u00fcnftig nur f\u00fcr dich, wenn Frost", "tokens": ["Be\u00b7deck", "ich", "k\u00fcnf\u00b7tig", "nur", "f\u00fcr", "dich", ",", "wenn", "Frost"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PPER", "ADJD", "ADV", "APPR", "PPER", "$,", "KOUS", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Die Erde dr\u00fcckt, f\u00fcr dich soll er hier bl\u00fchn,", "tokens": ["Die", "Er\u00b7de", "dr\u00fcckt", ",", "f\u00fcr", "dich", "soll", "er", "hier", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "APPR", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Und tragen s\u00fc\u00dfe Frucht. Allein Philint", "tokens": ["Und", "tra\u00b7gen", "s\u00fc\u00b7\u00dfe", "Frucht", ".", "Al\u00b7lein", "Phi\u00b7lint"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$.", "ADV", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Starb bald, ihm trug der Baum nicht s\u00fc\u00dfe Frucht.", "tokens": ["Starb", "bald", ",", "ihm", "trug", "der", "Baum", "nicht", "s\u00fc\u00b7\u00dfe", "Frucht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPER", "VVFIN", "ART", "NN", "PTKNEG", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Und Cephis weint um ihn, und w\u00fcnscht sich arm", "tokens": ["Und", "Ce\u00b7phis", "weint", "um", "ihn", ",", "und", "w\u00fcnscht", "sich", "arm"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NE", "VVFIN", "APPR", "PPER", "$,", "KON", "VVFIN", "PRF", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Zu sterben, und so fromm als er; begrub", "tokens": ["Zu", "ster\u00b7ben", ",", "und", "so", "fromm", "als", "er", ";", "be\u00b7grub"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PTKZU", "VVINF", "$,", "KON", "ADV", "ADJD", "KOKOM", "PPER", "$.", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Ihn unter seinen Baum, baut ihm ein Grab", "tokens": ["Ihn", "un\u00b7ter", "sei\u00b7nen", "Baum", ",", "baut", "ihm", "ein", "Grab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Und pflanzte Rosen und Cypre\u00df umher.", "tokens": ["Und", "pflanz\u00b7te", "Ro\u00b7sen", "und", "Cyp\u00b7re\u00df", "um\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Er h\u00f6ret' oft seitdem beym Mondenschein,", "tokens": ["Er", "h\u00f6\u00b7ret'", "oft", "seit\u00b7dem", "beym", "Mon\u00b7den\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PAV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein heilig Rauschen in des Baumes Laub.", "tokens": ["Ein", "hei\u00b7lig", "Rau\u00b7schen", "in", "des", "Bau\u00b7mes", "Laub", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein s\u00fc\u00df Gelispel drang vom Grab herauf,", "tokens": ["Ein", "s\u00fc\u00df", "Ge\u00b7lis\u00b7pel", "drang", "vom", "Grab", "her\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das ihm zu danken schien. Und \u00dcberflu\u00df", "tokens": ["Das", "ihm", "zu", "dan\u00b7ken", "schien", ".", "Und", "\u00dc\u00b7berf\u00b7lu\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PPER", "PTKZU", "VVINF", "VVFIN", "$.", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Von Obst und Trauben wuchs ihm j\u00e4hrlich, denn", "tokens": ["Von", "Obst", "und", "Trau\u00b7ben", "wuchs", "ihm", "j\u00e4hr\u00b7lich", ",", "denn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PPER", "ADJD", "$,", "KON"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der Himmel seegnet stets die Fr\u00f6mmigkeit.", "tokens": ["Der", "Him\u00b7mel", "seeg\u00b7net", "stets", "die", "Fr\u00f6m\u00b7mig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}