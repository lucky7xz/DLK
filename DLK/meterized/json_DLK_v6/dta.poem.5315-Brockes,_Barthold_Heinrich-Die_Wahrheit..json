{"dta.poem.5315": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Wahrheit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Grosser Sch\u00f6pfer! ich erkenne, da\u00df ich nichts erkenn'\nund wei\u00df,", "tokens": ["Gros\u00b7ser", "Sch\u00f6p\u00b7fer", "!", "ich", "er\u00b7ken\u00b7ne", ",", "da\u00df", "ich", "nichts", "er\u00b7kenn'", "und", "wei\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPER", "VVFIN", "$,", "KOUS", "PPER", "PIS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Aber, selber die\u00df Erkennen mehrt in mir doch deinen Preis;", "tokens": ["A\u00b7ber", ",", "sel\u00b7ber", "die\u00df", "Er\u00b7ken\u00b7nen", "mehrt", "in", "mir", "doch", "dei\u00b7nen", "Preis", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "PDS", "NN", "VVFIN", "APPR", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Denn, indem ich die\u00df ", "tokens": ["Denn", ",", "in\u00b7dem", "ich", "die\u00df"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "PPER", "PDS"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Treff\u2019 ich was in meinen Wesen, welches was ", "tokens": ["Treff'", "ich", "was", "in", "mei\u00b7nen", "We\u00b7sen", ",", "wel\u00b7ches", "was"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "PPER", "PIS", "APPR", "PPOSAT", "NN", "$,", "PRELS", "PWS"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.5": {"text": "Die Betrachtungen sind f\u00e4hig, alle Zweiffels-Furcht zu stillen;", "tokens": ["Die", "Be\u00b7trach\u00b7tun\u00b7gen", "sind", "f\u00e4\u00b7hig", ",", "al\u00b7le", "Zweif\u00b7fels\u00b7Furcht", "zu", "stil\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Weil aus dieser Selbst-Erk\u00e4nntni\u00df ", "tokens": ["Weil", "aus", "die\u00b7ser", "Selbst\u00b7Er\u00b7k\u00e4nnt\u00b7ni\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "F\u00fchret meine leere Seele zu der ew\u2019gen Weisheit Licht.", "tokens": ["F\u00fch\u00b7ret", "mei\u00b7ne", "lee\u00b7re", "See\u00b7le", "zu", "der", "ew'\u00b7gen", "Weis\u00b7heit", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Die\u00df nun l\u00e4\u00dft mich Sonnen-klar in des Sch\u00f6pfers Wun-", "tokens": ["Die\u00df", "nun", "l\u00e4\u00dft", "mich", "Son\u00b7nen\u00b7klar", "in", "des", "Sch\u00f6p\u00b7fers", "Wun"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "VVFIN", "PPER", "NN", "APPR", "ART", "NN", "TRUNC"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Diese Strahlen-reiche Wahrheit allenthalben deutlich", "tokens": ["Die\u00b7se", "Strah\u00b7len\u00b7rei\u00b7che", "Wahr\u00b7heit", "al\u00b7len\u00b7thal\u00b7ben", "deut\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDAT", "ADJA", "NN", "ADV", "ADJD"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}, "stanza.2": {"line.1": {"text": "Der Sch\u00f6pfer will und kann allein", "tokens": ["Der", "Sch\u00f6p\u00b7fer", "will", "und", "kann", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "KON", "VMFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bewundert, nicht begriffen, seyn.", "tokens": ["Be\u00b7wun\u00b7dert", ",", "nicht", "be\u00b7grif\u00b7fen", ",", "seyn", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "PTKNEG", "VVPP", "$,", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}