{"dta.poem.3404": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Die Klage ohne Trost.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193292", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wo seyd ihr, meine Lieben?", "tokens": ["Wo", "seyd", "ihr", ",", "mei\u00b7ne", "Lie\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Euch ruft mein Wehgesang.", "tokens": ["Euch", "ruft", "mein", "Weh\u00b7ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Euch ruft des Herzens Sehnen", "tokens": ["Euch", "ruft", "des", "Her\u00b7zens", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit ungest\u00fcmen Drang.", "tokens": ["Mit", "un\u00b7ge\u00b7st\u00fc\u00b7men", "Drang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wo seyd ihr, meine Trauten?", "tokens": ["Wo", "seyd", "ihr", ",", "mei\u00b7ne", "Trau\u00b7ten", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie t\u00e4uscht ihr mich so sehr?", "tokens": ["Wie", "t\u00e4uscht", "ihr", "mich", "so", "sehr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die ihr mich feurig liebtet,", "tokens": ["Die", "ihr", "mich", "feu\u00b7rig", "lieb\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Liebt ihr mich itzt nicht mehr?", "tokens": ["Liebt", "ihr", "mich", "itzt", "nicht", "mehr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich steh' auf stickelm Felsen,", "tokens": ["Ich", "steh'", "auf", "sti\u00b7ckelm", "Fel\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und schaue rings umher.", "tokens": ["Und", "schau\u00b7e", "rings", "um\u00b7her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Sturmwind schwingt die Fl\u00fcgel,", "tokens": ["Der", "Sturm\u00b7wind", "schwingt", "die", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und geisselt Luft und Meer.", "tokens": ["Und", "geis\u00b7selt", "Luft", "und", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich schw\u00e4rme durch die Klippen,", "tokens": ["Ich", "schw\u00e4r\u00b7me", "durch", "die", "Klip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Durch struppiges Gestr\u00e4uch,", "tokens": ["Durch", "strup\u00b7pi\u00b7ges", "Ge\u00b7str\u00e4uch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Durch dichtverwachsne Schr\u00fcnde,", "tokens": ["Durch", "dicht\u00b7ver\u00b7wachs\u00b7ne", "Schr\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und suche rastlos euch!", "tokens": ["Und", "su\u00b7che", "rast\u00b7los", "euch", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Eichwald heult entbl\u00e4ttert;", "tokens": ["Der", "Eich\u00b7wald", "heult", "ent\u00b7bl\u00e4t\u00b7tert", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Erstorben starrt die Au.", "tokens": ["E\u00b7rstor\u00b7ben", "starrt", "die", "Au", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Um \u00f6de Stoppeln sauset", "tokens": ["Um", "\u00f6\u00b7de", "Stop\u00b7peln", "sau\u00b7set"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "ADJA", "NN", "VVFIN"], "meter": "-+-+-++", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Der Herbstwind, feucht und rauh.", "tokens": ["Der", "Herbst\u00b7wind", ",", "feucht", "und", "rauh", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Warum so traurend, Aue?", "tokens": ["Wa\u00b7rum", "so", "trau\u00b7rend", ",", "Au\u00b7e", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "$,", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Warum so jammernd, Hayn?", "tokens": ["Wa\u00b7rum", "so", "jam\u00b7mernd", ",", "Hayn", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "$,", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Seyd ihr, wie ich, verlassen?", "tokens": ["Seyd", "ihr", ",", "wie", "ich", ",", "ver\u00b7las\u00b7sen", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VAIMP", "PPER", "$,", "PWAV", "PPER", "$,", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Seyd ihr, wie ich, allein?", "tokens": ["Seyd", "ihr", ",", "wie", "ich", ",", "al\u00b7lein", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VAIMP", "PPER", "$,", "PWAV", "PPER", "$,", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Allein bin ich. In fremdem,", "tokens": ["Al\u00b7lein", "bin", "ich", ".", "In", "frem\u00b7dem", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$.", "APPR", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In \u00f6dem Land' allein.", "tokens": ["In", "\u00f6\u00b7dem", "Land'", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die lange D\u00e4mmrung lichtet", "tokens": ["Die", "lan\u00b7ge", "D\u00e4mm\u00b7rung", "lich\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kein Stern \u2014 kein Mondenschein.", "tokens": ["Kein", "Stern", "kein", "Mon\u00b7den\u00b7schein", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$(", "PIAT", "NN", "$."], "meter": "-++-+-", "measure": "unknown.measure.tri"}, "line.5": {"text": "Die heisse Wange f\u00e4chelt", "tokens": ["Die", "heis\u00b7se", "Wan\u00b7ge", "f\u00e4\u00b7chelt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Kein lispelnd Abendk\u00fchl,", "tokens": ["Kein", "lis\u00b7pelnd", "A\u00b7bend\u00b7k\u00fchl", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Noch schmelzt des Sp\u00e4troths Bl\u00e4sse", "tokens": ["Noch", "schmelzt", "des", "Sp\u00e4t\u00b7roths", "Bl\u00e4s\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das Herz in Ruhgef\u00fchl.", "tokens": ["Das", "Herz", "in", "Ruh\u00b7ge\u00b7f\u00fchl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wie starrt die \u00f6de Wildniss,", "tokens": ["Wie", "starrt", "die", "\u00f6\u00b7de", "Wild\u00b7niss", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Umbr\u00fcllt vom d\u00fcstern Belt.", "tokens": ["Um\u00b7br\u00fcllt", "vom", "d\u00fcs\u00b7tern", "Belt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "VVFIN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es kommt kein freundlicher Wandrer,", "tokens": ["Es", "kommt", "kein", "freund\u00b7li\u00b7cher", "Wand\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der um den Hals mir f\u00e4llt.", "tokens": ["Der", "um", "den", "Hals", "mir", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich rufe durch die Forste,", "tokens": ["Ich", "ru\u00b7fe", "durch", "die", "Fors\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Durch Berg' und Thal \u2014 und laut", "tokens": ["Durch", "Ber\u00b7g'", "und", "Thal", "und", "laut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$(", "KON", "ADJD"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Ruft mir das Echo Antwort,", "tokens": ["Ruft", "mir", "das", "E\u00b7cho", "Ant\u00b7wort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Doch keines Menschen Laut.", "tokens": ["Doch", "kei\u00b7nes", "Men\u00b7schen", "Laut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Es gaukeln Luftgebilde", "tokens": ["Es", "gau\u00b7keln", "Luft\u00b7ge\u00b7bil\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Um mich im Fackelschein", "tokens": ["Um", "mich", "im", "Fa\u00b7ckel\u00b7schein"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "PRF", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gluthrother Meteore,", "tokens": ["Gluth\u00b7ro\u00b7ther", "Me\u00b7te\u00b7o\u00b7re", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und spotten t\u00fcckisch mein.", "tokens": ["Und", "spot\u00b7ten", "t\u00fc\u00b7ckisch", "mein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ergrimmter tobt die Windsbraut,", "tokens": ["Er\u00b7grimm\u00b7ter", "tobt", "die", "Winds\u00b7braut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Erz\u00fcrnter th\u00fcrmt die Fluth.", "tokens": ["Er\u00b7z\u00fcrn\u00b7ter", "th\u00fcrmt", "die", "Fluth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Feste Pfeiler beben", "tokens": ["Der", "Fes\u00b7te", "Pfei\u00b7ler", "be\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Des Orkans wilder Wuth.", "tokens": ["Des", "Or\u00b7kans", "wil\u00b7der", "Wuth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Mag rasen Sturm; mag prasseln", "tokens": ["Mag", "ra\u00b7sen", "Sturm", ";", "mag", "pras\u00b7seln"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "VVINF", "NN", "$.", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Windsbraut donnerlaut!", "tokens": ["Die", "Winds\u00b7braut", "don\u00b7ner\u00b7laut", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mir klingt ihr wildes Kreischen", "tokens": ["Mir", "klingt", "ihr", "wil\u00b7des", "Krei\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie Morgengruss der Braut.", "tokens": ["Wie", "Mor\u00b7gen\u00b7gruss", "der", "Braut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mag mondhoch schwellend wogen", "tokens": ["Mag", "mond\u00b7hoch", "schwel\u00b7lend", "wo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der alte Ocean!", "tokens": ["Der", "al\u00b7te", "O\u00b7cean", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+---", "measure": "dactylic.init"}, "line.7": {"text": "Mich spricht sein dumpfes Grollen,", "tokens": ["Mich", "spricht", "sein", "dum\u00b7pfes", "Grol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wie Fl\u00f6tenwirbel an.", "tokens": ["Wie", "Fl\u00f6\u00b7ten\u00b7wir\u00b7bel", "an", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Was frommt mir Fr\u00fchlingsmilde", "tokens": ["Was", "frommt", "mir", "Fr\u00fch\u00b7lings\u00b7mil\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und laues Westgeweh?", "tokens": ["Und", "lau\u00b7es", "West\u00b7ge\u00b7weh", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie wiegen den Verlassnen", "tokens": ["Sie", "wie\u00b7gen", "den", "Ver\u00b7lass\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nur in noch heissres Weh!", "tokens": ["Nur", "in", "noch", "heiss\u00b7res", "Weh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was frommt mir Lerchenwirbel", "tokens": ["Was", "frommt", "mir", "Ler\u00b7chen\u00b7wir\u00b7bel"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Und Nachtigallenschlag?", "tokens": ["Und", "Nach\u00b7ti\u00b7gal\u00b7len\u00b7schlag", "?"], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der \u00f6den Brust ent\u00e4chzet", "tokens": ["Der", "\u00f6\u00b7den", "Brust", "en\u00b7t\u00e4ch\u00b7zet"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Nur desto lautres Ach.", "tokens": ["Nur", "des\u00b7to", "laut\u00b7res", "Ach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"