{"textgrid.poem.42581": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "5. An die Volksvertreter", "genre": "verse", "period": "N.A.", "pub_year": 1816, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schaffet fort am guten Werke", "tokens": ["Schaf\u00b7fet", "fort", "am", "gu\u00b7ten", "Wer\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit Besonnenheit und St\u00e4rke!", "tokens": ["Mit", "Be\u00b7son\u00b7nen\u00b7heit", "und", "St\u00e4r\u00b7ke", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00dft euch nicht das Lob bet\u00f6ren!", "tokens": ["La\u00dft", "euch", "nicht", "das", "Lob", "be\u00b7t\u00f6\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00dft euch nicht den Tadel st\u00f6ren!", "tokens": ["La\u00dft", "euch", "nicht", "den", "Ta\u00b7del", "st\u00f6\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Tadeln euch die \u00dcberweisen,", "tokens": ["Ta\u00b7deln", "euch", "die", "\u00dc\u00b7ber\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die um eigne Sonnen kreisen:", "tokens": ["Die", "um", "eig\u00b7ne", "Son\u00b7nen", "krei\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Haltet fester nur am echten,", "tokens": ["Hal\u00b7tet", "fes\u00b7ter", "nur", "am", "ech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "APPRART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alterprobten einfach Rechten!", "tokens": ["Al\u00b7ter\u00b7prob\u00b7ten", "ein\u00b7fach", "Rech\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "H\u00f6hnen euch die herzlos Kalten,", "tokens": ["H\u00f6h\u00b7nen", "euch", "die", "herz\u00b7los", "Kal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die Ergl\u00fchn f\u00fcr Torheit halten:", "tokens": ["Die", "Er\u00b7gl\u00fchn", "f\u00fcr", "Tor\u00b7heit", "hal\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Brennet hei\u00dfer nur und treuer", "tokens": ["Bren\u00b7net", "hei\u00b7\u00dfer", "nur", "und", "treu\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "ADV", "KON", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von des edlen Eifers Feuer!", "tokens": ["Von", "des", "ed\u00b7len", "Ei\u00b7fers", "Feu\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Schm\u00e4hn euch jene, die zum Guten", "tokens": ["Schm\u00e4hn", "euch", "je\u00b7ne", ",", "die", "zum", "Gu\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PDS", "$,", "PRELS", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lautern Antrieb nie vermuten:", "tokens": ["Lau\u00b7tern", "An\u00b7trieb", "nie", "ver\u00b7mu\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zeigt in desto sch\u00f6nrer Klarheit", "tokens": ["Zeigt", "in", "des\u00b7to", "sch\u00f6n\u00b7rer", "Klar\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Reinen Sinn f\u00fcr Recht und Wahrheit!", "tokens": ["Rei\u00b7nen", "Sinn", "f\u00fcr", "Recht", "und", "Wahr\u00b7heit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Was ihr Treues uns erwiesen,", "tokens": ["Was", "ihr", "Treu\u00b7es", "uns", "er\u00b7wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sei von uns mit Dank gepriesen!", "tokens": ["Sei", "von", "uns", "mit", "Dank", "ge\u00b7prie\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Was ihr ferner werdet bauen,", "tokens": ["Was", "ihr", "fer\u00b7ner", "wer\u00b7det", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sei erwartet mit Vertrauen!", "tokens": ["Sei", "er\u00b7war\u00b7tet", "mit", "Ver\u00b7trau\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}