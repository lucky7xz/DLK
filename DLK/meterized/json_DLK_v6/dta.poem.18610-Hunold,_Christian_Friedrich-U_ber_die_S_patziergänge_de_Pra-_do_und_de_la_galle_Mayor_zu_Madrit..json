{"dta.poem.18610": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "U ber die  S patzierg\u00e4nge   de Pra-  \n do und de la galle Mayor   zu Madrit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Gebrauch der edlen Zeit in diesen Lust-Revier", "tokens": ["Ge\u00b7brauch", "der", "ed\u00b7len", "Zeit", "in", "die\u00b7sen", "Lust\u00b7Re\u00b7vier"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und brich die Rosen ab/ weil sie noch sicher bl\u00fchen/", "tokens": ["Und", "brich", "die", "Ro\u00b7sen", "ab", "/", "weil", "sie", "noch", "si\u00b7cher", "bl\u00fc\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "PTKVZ", "$(", "KOUS", "PPER", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Leg' alle Gravit\u00e4t und Keuschheit vor die Th\u00fcr/", "tokens": ["Leg'", "al\u00b7le", "Gra\u00b7vi\u00b7t\u00e4t", "und", "Keuschheit", "vor", "die", "Th\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "KON", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Denn Schertz und Liebe ziert die sch\u00f6nen Gallerien.", "tokens": ["Denn", "Schertz", "und", "Lie\u00b7be", "ziert", "die", "sch\u00f6\u00b7nen", "Gal\u00b7le\u00b7ri\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "w