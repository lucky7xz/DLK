{"dta.poem.2893": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "94.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du stehst am Strand, und siehst noch ringen mit den Wogen", "tokens": ["Du", "stehst", "am", "Strand", ",", "und", "siehst", "noch", "rin\u00b7gen", "mit", "den", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "KON", "VVFIN", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie, die ein gleicher Trieb nach diesem Strand gezogen.", "tokens": ["Sie", ",", "die", "ein", "glei\u00b7cher", "Trieb", "nach", "die\u00b7sem", "Strand", "ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "ADJA", "NN", "APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Erinnre dich, wie du einst selber deine Hand", "tokens": ["E\u00b7rinn\u00b7re", "dich", ",", "wie", "du", "einst", "sel\u00b7ber", "dei\u00b7ne", "Hand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "PPER", "ADV", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Getreckt aus Wogenkampf nach denen hoch am Strand;", "tokens": ["Ge\u00b7treckt", "aus", "Wo\u00b7gen\u00b7kampf", "nach", "de\u00b7nen", "hoch", "am", "Strand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "APPR", "PRELS", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und wie es dich verdro\u00df, wenn jene dich verlie\u00dfen,", "tokens": ["Und", "wie", "es", "dich", "ver\u00b7dro\u00df", ",", "wenn", "je\u00b7ne", "dich", "ver\u00b7lie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PRF", "VVFIN", "$,", "KOUS", "PDS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und, um allein zu stehn, dich in die Fluten stie\u00dfen.", "tokens": ["Und", ",", "um", "al\u00b7lein", "zu", "stehn", ",", "dich", "in", "die", "Flu\u00b7ten", "stie\u00b7\u00dfen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUI", "ADV", "PTKZU", "VVINF", "$,", "PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Entgegen strecke der gestreckten deine Hand;", "tokens": ["Ent\u00b7ge\u00b7gen", "stre\u00b7cke", "der", "ge\u00b7streck\u00b7ten", "dei\u00b7ne", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "PPOSAT", "NN", "$."], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Am Strande neben dir ist noch f\u00fcr viele Stand.", "tokens": ["Am", "Stran\u00b7de", "ne\u00b7ben", "dir", "ist", "noch", "f\u00fcr", "vie\u00b7le", "Stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPER", "VAFIN", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Der ausgestreckten streck' entgegen deine; siehe", "tokens": ["Der", "aus\u00b7ge\u00b7streck\u00b7ten", "streck'", "ent\u00b7ge\u00b7gen", "dei\u00b7ne", ";", "sie\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "$.", "VVIMP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur zu, da\u00df keine selbst vom Strand dich niederziehe!", "tokens": ["Nur", "zu", ",", "da\u00df", "kei\u00b7ne", "selbst", "vom", "Strand", "dich", "nie\u00b7der\u00b7zie\u00b7he", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "KOUS", "PIAT", "ADV", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Nein, diese Vorsicht la\u00df der Vorsicht Hand ob dir!", "tokens": ["Nein", ",", "die\u00b7se", "Vor\u00b7sicht", "la\u00df", "der", "Vor\u00b7sicht", "Hand", "ob", "dir", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PDAT", "NN", "VVFIN", "ART", "NN", "NN", "KOUS", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du stehst durch sie und f\u00e4llst, und f\u00e4llst niemals aus ihr.", "tokens": ["Du", "stehst", "durch", "sie", "und", "f\u00e4llst", ",", "und", "f\u00e4llst", "nie\u00b7mals", "aus", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "KON", "VVFIN", "$,", "KON", "VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}}}}