{"textgrid.poem.61197": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das ist der Fluch von diesen tr\u00fcben Zeiten,", "tokens": ["Das", "ist", "der", "Fluch", "von", "die\u00b7sen", "tr\u00fc\u00b7ben", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo losgelassen die Parteien toben,", "tokens": ["Wo", "los\u00b7ge\u00b7las\u00b7sen", "die", "Par\u00b7tei\u00b7en", "to\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df kaum der Starke, welcher blickt nach oben,", "tokens": ["Da\u00df", "kaum", "der", "Star\u00b7ke", ",", "wel\u00b7cher", "blickt", "nach", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "$,", "PRELS", "VVFIN", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vermag in Reinheit mittendurch zu schreiten.", "tokens": ["Ver\u00b7mag", "in", "Rein\u00b7heit", "mit\u00b7ten\u00b7durch", "zu", "schrei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPR", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nur ", "tokens": ["Nur"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "So hat sein ganzes Wesen sich verschoben,", "tokens": ["So", "hat", "sein", "gan\u00b7zes", "We\u00b7sen", "sich", "ver\u00b7scho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nur ", "tokens": ["Nur"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Was er noch j\u00fcngst bedacht war zu bestreiten.", "tokens": ["Was", "er", "noch", "j\u00fcngst", "be\u00b7dacht", "war", "zu", "be\u00b7strei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "VVPP", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Drum gib, o Herr, da\u00df ich die Lebensamme,", "tokens": ["Drum", "gib", ",", "o", "Herr", ",", "da\u00df", "ich", "die", "Le\u00b7ben\u00b7sam\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "FM", "NN", "$,", "KOUS", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die heil'ge Freiheit, nie mit jenem Weibe", "tokens": ["Die", "heil'\u00b7ge", "Frei\u00b7heit", ",", "nie", "mit", "je\u00b7nem", "Wei\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im blut'gen, aufgesch\u00fcrzten Kleid verdamme!", "tokens": ["Im", "blut'\u00b7gen", ",", "auf\u00b7ge\u00b7sch\u00fcrz\u00b7ten", "Kleid", "ver\u00b7dam\u00b7me", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und ob die Wilde mich an meinem Leibe", "tokens": ["Und", "ob", "die", "Wil\u00b7de", "mich", "an", "mei\u00b7nem", "Lei\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schmerzlich versehren mag mit Erz und Flamme:", "tokens": ["Schmerz\u00b7lich", "ver\u00b7seh\u00b7ren", "mag", "mit", "Erz", "und", "Flam\u00b7me", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "VMFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Gib, da\u00df ich treu der Himmelstochter bleibe!", "tokens": ["Gib", ",", "da\u00df", "ich", "treu", "der", "Him\u00b7mel\u00b7stoch\u00b7ter", "blei\u00b7be", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "KOUS", "PPER", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}