{"dta.poem.3882": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "7) An eben dieselbe.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Freundin! da ich abgereist,", "tokens": ["Freun\u00b7din", "!", "da", "ich", "ab\u00b7ge\u00b7reist", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KOUS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Qu\u00e4let dein Verlust den Geist.", "tokens": ["Qu\u00e4\u00b7let", "dein", "Ver\u00b7lust", "den", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da die Nacht das Erdreich decket,", "tokens": ["Da", "die", "Nacht", "das", "Er\u00b7dreich", "de\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da mich Angst und Furcht erschrecket,", "tokens": ["Da", "mich", "Angst", "und", "Furcht", "er\u00b7schre\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da es ein\u00f6d, bang und kalt", "tokens": ["Da", "es", "ei\u00b7n\u00f6d", ",", "bang", "und", "kalt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "$,", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "In dem Schnee-bedeckten Wald.", "tokens": ["In", "dem", "Schnee\u00b7be\u00b7deck\u00b7ten", "Wald", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da dacht ich aus treuen Hertzen", "tokens": ["Da", "dacht", "ich", "aus", "treu\u00b7en", "Hert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.8": {"text": "An dich mit geh\u00e4ufften Schmertzen,", "tokens": ["An", "dich", "mit", "ge\u00b7h\u00e4uff\u00b7ten", "Schmert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und so fuhr ich zitternd fort,", "tokens": ["Und", "so", "fuhr", "ich", "zit\u00b7ternd", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Und der scheelen Mi\u00dfgunst Tort", "tokens": ["Und", "der", "schee\u00b7len", "Mi\u00df\u00b7gunst", "Tort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Such ich unter bangen Zagen", "tokens": ["Such", "ich", "un\u00b7ter", "ban\u00b7gen", "Za\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Stummer Creatur zu klagen;", "tokens": ["Stum\u00b7mer", "Crea\u00b7tur", "zu", "kla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.13": {"text": "Denn die fr\u00f6mmste Ehrlichkeit", "tokens": ["Denn", "die", "fr\u00f6mms\u00b7te", "Ehr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Hat dir Hertz und Sinn geweyht.", "tokens": ["Hat", "dir", "Hertz", "und", "Sinn", "ge\u00b7weyht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Denn ich kenne dein Gem\u00fcthe,", "tokens": ["Denn", "ich", "ken\u00b7ne", "dein", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Und die unverdiente G\u00fcte,", "tokens": ["Und", "die", "un\u00b7ver\u00b7dien\u00b7te", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Die da allzeit Tag und Nacht", "tokens": ["Die", "da", "all\u00b7zeit", "Tag", "und", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Auf des Vetters Wohl bedacht.", "tokens": ["Auf", "des", "Vet\u00b7ters", "Wohl", "be\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Nun so werd ich mich erk\u00fchnen,", "tokens": ["Nun", "so", "werd", "ich", "mich", "er\u00b7k\u00fch\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Dir in Ehrfurcht stets zu dienen,", "tokens": ["Dir", "in", "Ehr\u00b7furcht", "stets", "zu", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Treu, verschwiegen, Ehrsurchts-voll.", "tokens": ["Treu", ",", "ver\u00b7schwie\u00b7gen", ",", "Ehr\u00b7surchts\u00b7voll", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVPP", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Liebste Freundin, lebe wohl!", "tokens": ["Liebs\u00b7te", "Freun\u00b7din", ",", "le\u00b7be", "wohl", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}