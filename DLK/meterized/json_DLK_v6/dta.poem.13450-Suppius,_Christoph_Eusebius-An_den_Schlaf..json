{"dta.poem.13450": {"metadata": {"author": {"name": "Suppius, Christoph Eusebius", "birth": "N.A.", "death": "N.A."}, "title": "An den Schlaf.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1749", "urn": "urn:nbn:de:kobv:b4-20594-7", "language": ["de:0.99"], "booktitle": "Suppius, Christoph Eusebius: Oden und Lieder. Gotha, 1749."}, "poem": {"stanza.1": {"line.1": {"text": "Du Bruder von dem Blassen,", "tokens": ["Du", "Bru\u00b7der", "von", "dem", "Blas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dem Schrecken aller Leute!", "tokens": ["Dem", "Schre\u00b7cken", "al\u00b7ler", "Leu\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Geschwinde komm her\u00fcber,", "tokens": ["Ge\u00b7schwin\u00b7de", "komm", "her\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich warte mit Verlangen", "tokens": ["Ich", "war\u00b7te", "mit", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Auf deine Schlummerk\u00f6rner.", "tokens": ["Auf", "dei\u00b7ne", "Schlum\u00b7mer\u00b7k\u00f6r\u00b7ner", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hast du nicht h\u00f6ren rufen?", "tokens": ["Hast", "du", "nicht", "h\u00f6\u00b7ren", "ru\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "VVINF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Als du bey unserm Nachbar", "tokens": ["Als", "du", "bey", "un\u00b7serm", "Nach\u00b7bar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "So lange schon vergebens", "tokens": ["So", "lan\u00b7ge", "schon", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Jm Kalten klopfen m\u00fcssen;", "tokens": ["Jm", "Kal\u00b7ten", "klop\u00b7fen", "m\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Der Geizhals will ja wachen", "tokens": ["Der", "Geiz\u00b7hals", "will", "ja", "wa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Bey fest verschlossnen Th\u00fcren", "tokens": ["Bey", "fest", "ver\u00b7schloss\u00b7nen", "Th\u00fc\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Auf seinen vollen Kasten", "tokens": ["Auf", "sei\u00b7nen", "vol\u00b7len", "Kas\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Und vor dem Zwang-Gew\u00f6lbe,", "tokens": ["Und", "vor", "dem", "Zwang\u00b7Ge\u00b7w\u00f6l\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Worinn er seine M\u00fcndel,", "tokens": ["Wo\u00b7rinn", "er", "sei\u00b7ne", "M\u00fcn\u00b7del", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Das sch\u00f6ne blonde M\u00e4dchen,", "tokens": ["Das", "sch\u00f6\u00b7ne", "blon\u00b7de", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Die tugendhafte Phillis", "tokens": ["Die", "tu\u00b7gend\u00b7haf\u00b7te", "Phil\u00b7lis"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NE"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.17": {"text": "Aus blossem Neid verschlossen,", "tokens": ["Aus", "blos\u00b7sem", "Neid", "ver\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Dieweil sie dem Poeten", "tokens": ["Die\u00b7weil", "sie", "dem", "Po\u00b7et\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.19": {"text": "J\u00fcngst einen Blick gegeben,", "tokens": ["J\u00fcngst", "ei\u00b7nen", "Blick", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Wobey der Vormund dachte,", "tokens": ["Wo\u00b7bey", "der", "Vor\u00b7mund", "dach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Sein M\u00e4dchen sey nun \u00e4rmer,", "tokens": ["Sein", "M\u00e4d\u00b7chen", "sey", "nun", "\u00e4r\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Dieweil sie was verschenket.", "tokens": ["Die\u00b7weil", "sie", "was", "ver\u00b7schen\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Drum la\u00df den alten Narren", "tokens": ["Drum", "la\u00df", "den", "al\u00b7ten", "Nar\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "Doch bey den Thalern liegen,", "tokens": ["Doch", "bey", "den", "Tha\u00b7lern", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Er mag sein M\u00e4dchen h\u00fcthen;", "tokens": ["Er", "mag", "sein", "M\u00e4d\u00b7chen", "h\u00fct\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Komm du zu mir geschlichen", "tokens": ["Komm", "du", "zu", "mir", "ge\u00b7schli\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "Auf leisen leisen F\u00fcssen,", "tokens": ["Auf", "lei\u00b7sen", "lei\u00b7sen", "F\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "Es stehn die Th\u00fcren offen,", "tokens": ["Es", "stehn", "die", "Th\u00fc\u00b7ren", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.29": {"text": "Und hier sind keine Kasten;", "tokens": ["Und", "hier", "sind", "kei\u00b7ne", "Kas\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.30": {"text": "Bey B\u00fcchern will ich schlafen,", "tokens": ["Bey", "B\u00fc\u00b7chern", "will", "ich", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": "Wenn du mich eingewieget,", "tokens": ["Wenn", "du", "mich", "ein\u00b7ge\u00b7wie\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.32": {"text": "Auch von der Blonden tr\u00e4umen;", "tokens": ["Auch", "von", "der", "Blon\u00b7den", "tr\u00e4u\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.33": {"text": "Wenn das ihr Vormund w\u00fc\u00dfte!", "tokens": ["Wenn", "das", "ihr", "Vor\u00b7mund", "w\u00fc\u00df\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "Ich will sie gar besuchen,", "tokens": ["Ich", "will", "sie", "gar", "be\u00b7su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.35": {"text": "Mich aus dem Traume k\u00fcssen,", "tokens": ["Mich", "aus", "dem", "Trau\u00b7me", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.36": {"text": "Wie die Poeten pflegen,", "tokens": ["Wie", "die", "Po\u00b7et\u00b7en", "pfle\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.37": {"text": "Die gar zu gerne k\u00fcssen,", "tokens": ["Die", "gar", "zu", "ger\u00b7ne", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PTKA", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.38": {"text": "Wird das der Alte h\u00f6ren?", "tokens": ["Wird", "das", "der", "Al\u00b7te", "h\u00f6\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}