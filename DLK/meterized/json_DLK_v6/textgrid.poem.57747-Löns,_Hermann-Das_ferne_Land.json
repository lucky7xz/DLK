{"textgrid.poem.57747": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Das ferne Land", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und das ist offenbar:", "tokens": ["Und", "das", "ist", "of\u00b7fen\u00b7bar", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ich wei\u00df ein Land, in dem ich niemals war;", "tokens": ["Ich", "wei\u00df", "ein", "Land", ",", "in", "dem", "ich", "nie\u00b7mals", "war", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da flie\u00dft ein Wasser, das ist silberklar,", "tokens": ["Da", "flie\u00dft", "ein", "Was\u00b7ser", ",", "das", "ist", "sil\u00b7ber\u00b7klar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PDS", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da bl\u00fchen Blumen, deren Duft ist rein", "tokens": ["Da", "bl\u00fc\u00b7hen", "Blu\u00b7men", ",", "de\u00b7ren", "Duft", "ist", "rein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "$,", "PRELAT", "NN", "VAFIN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und ihre Farben sind so zart und fein,", "tokens": ["Und", "ih\u00b7re", "Far\u00b7ben", "sind", "so", "zart", "und", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "So zart und fein, wie sonst am Himmel nur", "tokens": ["So", "zart", "und", "fein", ",", "wie", "sonst", "am", "Him\u00b7mel", "nur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,", "PWAV", "ADV", "APPRART", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der Abendr\u00f6te allerletzte Spur", "tokens": ["Der", "A\u00b7bend\u00b7r\u00f6\u00b7te", "al\u00b7ler\u00b7letz\u00b7te", "Spur"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "An hellen Abenden im jungen Mai", "tokens": ["An", "hel\u00b7len", "A\u00b7ben\u00b7den", "im", "jun\u00b7gen", "Mai"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Beim allerersten fernen Eulenschrei.", "tokens": ["Beim", "al\u00b7le\u00b7rers\u00b7ten", "fer\u00b7nen", "Eu\u00b7len\u00b7schrei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Auch singt ein Vogel in dem fernen Land,", "tokens": ["Auch", "singt", "ein", "Vo\u00b7gel", "in", "dem", "fer\u00b7nen", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er singt ein Lied, das ist mir unbekannt;", "tokens": ["Er", "singt", "ein", "Lied", ",", "das", "ist", "mir", "un\u00b7be\u00b7kannt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PDS", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich h\u00f6rt' es nie und wei\u00df doch, wie es klingt,", "tokens": ["Ich", "h\u00f6rt'", "es", "nie", "und", "wei\u00df", "doch", ",", "wie", "es", "klingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "KON", "VVFIN", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und wei\u00df es auch, was mir der Vogel singt;", "tokens": ["Und", "wei\u00df", "es", "auch", ",", "was", "mir", "der", "Vo\u00b7gel", "singt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "PWS", "PPER", "ART", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das Leben singt er, und er singt den Tod,", "tokens": ["Das", "Le\u00b7ben", "singt", "er", ",", "und", "er", "singt", "den", "Tod", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "KON", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die h\u00f6chste Wonne und die tiefste Not,", "tokens": ["Die", "h\u00f6chs\u00b7te", "Won\u00b7ne", "und", "die", "tiefs\u00b7te", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Jedwede Lust und jeglich Herzeleid,", "tokens": ["Jed\u00b7we\u00b7de", "Lust", "und", "jeg\u00b7lich", "Her\u00b7ze\u00b7leid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.8": {"text": "Die Lust der Zeit, das Weh der Ewigkeit.", "tokens": ["Die", "Lust", "der", "Zeit", ",", "das", "Weh", "der", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich kenn' das Land und wei\u00df nicht, wo es liegt,", "tokens": ["Ich", "kenn'", "das", "Land", "und", "wei\u00df", "nicht", ",", "wo", "es", "liegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und wei\u00df es nicht, wohin der Vogel fliegt,", "tokens": ["Und", "wei\u00df", "es", "nicht", ",", "wo\u00b7hin", "der", "Vo\u00b7gel", "fliegt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$,", "PWAV", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und h\u00f6rte von dem Bach das Rauschen kaum,", "tokens": ["Und", "h\u00f6r\u00b7te", "von", "dem", "Bach", "das", "Rau\u00b7schen", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Blumen Duft empfand ich nur im Traum;", "tokens": ["Der", "Blu\u00b7men", "Duft", "emp\u00b7fand", "ich", "nur", "im", "Traum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Im Traume nur sind einst sie mir erbl\u00fcht,", "tokens": ["Im", "Trau\u00b7me", "nur", "sind", "einst", "sie", "mir", "er\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VAFIN", "ADV", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Im Traum nur h\u00f6rte ich des Vogels Lied,", "tokens": ["Im", "Traum", "nur", "h\u00f6r\u00b7te", "ich", "des", "Vo\u00b7gels", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Das Lied vom Leben und das Lied vom Tod,", "tokens": ["Das", "Lied", "vom", "Le\u00b7ben", "und", "das", "Lied", "vom", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "KON", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Das Lied der Wonne und das Lied der Not.", "tokens": ["Das", "Lied", "der", "Won\u00b7ne", "und", "das", "Lied", "der", "Not", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Erreiche ich das ferne, fremde Land,", "tokens": ["Er\u00b7rei\u00b7che", "ich", "das", "fer\u00b7ne", ",", "frem\u00b7de", "Land", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dann bl\u00fcht das Lebensmal in meiner Hand;", "tokens": ["Dann", "bl\u00fcht", "das", "Le\u00b7bens\u00b7mal", "in", "mei\u00b7ner", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn nicht, dann sang der Vogel nur von Tod,", "tokens": ["Wenn", "nicht", ",", "dann", "sang", "der", "Vo\u00b7gel", "nur", "von", "Tod", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "$,", "ADV", "VVFIN", "ART", "NE", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sang mir ein Leben, bitter und voll Not;", "tokens": ["Sang", "mir", "ein", "Le\u00b7ben", ",", "bit\u00b7ter", "und", "voll", "Not", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Du wei\u00dft den Weg nach jenem Land; sag' ja!", "tokens": ["Du", "wei\u00dft", "den", "Weg", "nach", "je\u00b7nem", "Land", ";", "sag'", "ja", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PDAT", "NN", "$.", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Dann ist das ferne, fremde Land so nah',", "tokens": ["Dann", "ist", "das", "fer\u00b7ne", ",", "frem\u00b7de", "Land", "so", "nah'", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "$,", "ADJA", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dann singt der Vogel nimmermehr von Tod", "tokens": ["Dann", "singt", "der", "Vo\u00b7gel", "nim\u00b7mer\u00b7mehr", "von", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und Not; dann bl\u00fchen alle Blumen rot, so rot,", "tokens": ["Und", "Not", ";", "dann", "bl\u00fc\u00b7hen", "al\u00b7le", "Blu\u00b7men", "rot", ",", "so", "rot", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "$.", "ADV", "VVFIN", "PIAT", "NN", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So rosenrot.", "tokens": ["So", "ro\u00b7sen\u00b7rot", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}