{"textgrid.poem.49533": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "Seladon", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf diesem Rasen, den die Liebe", "tokens": ["Auf", "die\u00b7sem", "Ra\u00b7sen", ",", "den", "die", "Lie\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So reizend sch\u00f6n f\u00fcr Liebende gemacht,", "tokens": ["So", "rei\u00b7zend", "sch\u00f6n", "f\u00fcr", "Lie\u00b7ben\u00b7de", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sa\u00df ", "tokens": ["Sa\u00df"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Voll z\u00e4rtlicher Empfindungen und Triebe:", "tokens": ["Voll", "z\u00e4rt\u00b7li\u00b7cher", "Emp\u00b7fin\u00b7dun\u00b7gen", "und", "Trie\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Und schnitt in einen Baum mit mattverliebter Hand", "tokens": ["Und", "schnitt", "in", "ei\u00b7nen", "Baum", "mit", "matt\u00b7ver\u00b7lieb\u00b7ter", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Verse, die sein Hertz erfand.", "tokens": ["Die", "Ver\u00b7se", ",", "die", "sein", "Hertz", "er\u00b7fand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "O seelig! w\u00fcrde mir gegeben", "tokens": ["O", "see\u00b7lig", "!", "w\u00fcr\u00b7de", "mir", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADJD", "$.", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "In diesem Thal, das Fried und Ruh umgibt,", "tokens": ["In", "die\u00b7sem", "Thal", ",", "das", "Fried", "und", "Ruh", "um\u00b7gibt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Mit Iris, stets in sie verliebt,", "tokens": ["Mit", "I\u00b7ris", ",", "stets", "in", "sie", "ver\u00b7liebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und stets von ihr geliebt, zu leben!", "tokens": ["Und", "stets", "von", "ihr", "ge\u00b7liebt", ",", "zu", "le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPER", "VVPP", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Wie gern wollt ich, mein Vaterland,", "tokens": ["Wie", "gern", "wollt", "ich", ",", "mein", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Von dir verbannt,", "tokens": ["Von", "dir", "ver\u00b7bannt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Unter zarten Linden, unter stillen Buchen", "tokens": ["Un\u00b7ter", "zar\u00b7ten", "Lin\u00b7den", ",", "un\u00b7ter", "stil\u00b7len", "Bu\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NE", "$,", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.14": {"text": "Meine Ruh, mein Gl\u00fcck,", "tokens": ["Mei\u00b7ne", "Ruh", ",", "mein", "Gl\u00fcck", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.15": {"text": "Nur in ihrem Blick,", "tokens": ["Nur", "in", "ih\u00b7rem", "Blick", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.16": {"text": "Und an ihrem Busen suchen.", "tokens": ["Und", "an", "ih\u00b7rem", "Bu\u00b7sen", "su\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Bis wir einstens alle zween,", "tokens": ["Bis", "wir", "eins\u00b7tens", "al\u00b7le", "zween", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIS", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Lebenssatt, nicht Liebensm\u00fcde,", "tokens": ["Le\u00b7bens\u00b7satt", ",", "nicht", "Lie\u00b7bens\u00b7m\u00fc\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PTKNEG", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Unsre Hirtenst\u00e4b in Friede", "tokens": ["Uns\u00b7re", "Hir\u00b7ten\u00b7st\u00e4b", "in", "Frie\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "In das Thal Elysien", "tokens": ["In", "das", "Thal", "E\u00b7ly\u00b7si\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Zu den frommen Schaaren drehn;", "tokens": ["Zu", "den", "from\u00b7men", "Schaa\u00b7ren", "drehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Da in amaranthnen Schatten", "tokens": ["Da", "in", "a\u00b7ma\u00b7ranth\u00b7nen", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.23": {"text": "Uns vollkommener zu gatten;", "tokens": ["Uns", "voll\u00b7kom\u00b7me\u00b7ner", "zu", "gat\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.24": {"text": "Uns nicht mehr getrennt zu sehn.", "tokens": ["Uns", "nicht", "mehr", "ge\u00b7trennt", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADV", "VVPP", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}