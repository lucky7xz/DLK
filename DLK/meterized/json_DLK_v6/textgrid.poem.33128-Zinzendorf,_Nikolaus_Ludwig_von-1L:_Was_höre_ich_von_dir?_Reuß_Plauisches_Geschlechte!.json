{"textgrid.poem.33128": {"metadata": {"author": {"name": "Zinzendorf, Nikolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was h\u00f6re ich von dir? Reu\u00df Plauisches Geschlechte!", "genre": "verse", "period": "N.A.", "pub_year": 1730, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was h\u00f6re ich von dir? Reu\u00df Plauisches Geschlechte!", "tokens": ["Was", "h\u00f6\u00b7re", "ich", "von", "dir", "?", "Reu\u00df", "Plau\u00b7i\u00b7sches", "Ge\u00b7schlech\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "PPER", "$.", "NE", "ADJA", "NN", "$."], "meter": "-+-+--+-++-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Es ist ein Ri\u00df geschehn durch Stamm, durch Stadt und Land:", "tokens": ["Es", "ist", "ein", "Ri\u00df", "ge\u00b7schehn", "durch", "Stamm", ",", "durch", "Stadt", "und", "Land", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "APPR", "NN", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Graf zu Ober-Greitz wird selig ausgespannt.", "tokens": ["Der", "Graf", "zu", "O\u00b7ber\u00b7G\u00b7reitz", "wird", "se\u00b7lig", "aus\u00b7ge\u00b7spannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dir ist vollkommen wohl, vollendeter Gerechte.", "tokens": ["Dir", "ist", "voll\u00b7kom\u00b7men", "wohl", ",", "voll\u00b7en\u00b7de\u00b7ter", "Ge\u00b7rech\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ADV", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Allein, was dringet nicht f\u00fcr ein gebrochner Ton", "tokens": ["Al\u00b7lein", ",", "was", "drin\u00b7get", "nicht", "f\u00fcr", "ein", "ge\u00b7broch\u00b7ner", "Ton"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWS", "VVFIN", "PTKNEG", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Klage \u00fcber dich, bis zu des Lammes Thron?", "tokens": ["Der", "Kla\u00b7ge", "\u00fc\u00b7ber", "dich", ",", "bis", "zu", "des", "Lam\u00b7mes", "Thron", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "$,", "KOUS", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ihr Seelen, die ihr j\u00fcngst den jungen Held empfangen,", "tokens": ["Ihr", "See\u00b7len", ",", "die", "ihr", "j\u00fcngst", "den", "jun\u00b7gen", "Held", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Indem er, von der Last des Irdischen befreyt,", "tokens": ["In\u00b7dem", "er", ",", "von", "der", "Last", "des", "Ir\u00b7di\u00b7schen", "be\u00b7freyt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zum seligen Genu\u00df der stillen Ewigkeit,", "tokens": ["Zum", "se\u00b7li\u00b7gen", "Ge\u00b7nu\u00df", "der", "stil\u00b7len", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Nach wohl vollbrachtem Lauf, im Segen eingegangen;", "tokens": ["Nach", "wohl", "voll\u00b7brach\u00b7tem", "Lauf", ",", "im", "Se\u00b7gen", "ein\u00b7ge\u00b7gan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Bewundert, neben mir, den unerforschten Rath,", "tokens": ["Be\u00b7wun\u00b7dert", ",", "ne\u00b7ben", "mir", ",", "den", "un\u00b7er\u00b7forschten", "Rath", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "APPR", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Der diesen Cederbaum so bald versetzet hat.", "tokens": ["Der", "die\u00b7sen", "Ce\u00b7der\u00b7baum", "so", "bald", "ver\u00b7set\u00b7zet", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "NN", "ADV", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Was, treue G\u00e4rtners Hand! was hat Dich wol bewogen,", "tokens": ["Was", ",", "treu\u00b7e", "G\u00e4rt\u00b7ners", "Hand", "!", "was", "hat", "Dich", "wol", "be\u00b7wo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "ADJA", "NN", "NN", "$.", "PWS", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df Du dem edelsten, dem Hoffnungs-vollen Reis,", "tokens": ["Da\u00df", "Du", "dem", "e\u00b7dels\u00b7ten", ",", "dem", "Hoff\u00b7nungs\u00b7vol\u00b7len", "Reis", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Gewurzelt und gepflanzt zu Deiner Liebe Preis,", "tokens": ["Ge\u00b7wur\u00b7zelt", "und", "ge\u00b7pflanzt", "zu", "Dei\u00b7ner", "Lie\u00b7be", "Preis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bald nach der ersten Frucht, den Saft der Erd entzogen?", "tokens": ["Bald", "nach", "der", "ers\u00b7ten", "Frucht", ",", "den", "Saft", "der", "Erd", "ent\u00b7zo\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Pflanze Libanons ist allzu hoch begl\u00fckt,", "tokens": ["Die", "Pflan\u00b7ze", "Li\u00b7ba\u00b7nons", "ist", "all\u00b7zu", "hoch", "be\u00b7gl\u00fckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VAFIN", "PTKA", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die itzt Dein Tempel-Haus gleich einem Pfeifer schm\u00fckt.", "tokens": ["Die", "itzt", "Dein", "Tem\u00b7pel\u00b7Haus", "gleich", "ei\u00b7nem", "Pfei\u00b7fer", "schm\u00fckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ach, aber Herr, die Zahl beginnet abzunehmen", "tokens": ["Ach", ",", "a\u00b7ber", "Herr", ",", "die", "Zahl", "be\u00b7gin\u00b7net", "ab\u00b7zu\u00b7neh\u00b7men"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KON", "NN", "$,", "ART", "NN", "VVFIN", "VVIZU"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Heiligen, die Du in dieser argen Welt,", "tokens": ["Der", "Hei\u00b7li\u00b7gen", ",", "die", "Du", "in", "die\u00b7ser", "ar\u00b7gen", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zum Zeichen jedermann, zum Preise Dir bestellt.", "tokens": ["Zum", "Zei\u00b7chen", "je\u00b7der\u00b7mann", ",", "zum", "Prei\u00b7se", "Dir", "be\u00b7stellt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIS", "$,", "APPRART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wann wirds sichs dann einmal zur bessern Zeit bequemen?", "tokens": ["Wann", "wirds", "sichs", "dann", "ein\u00b7mal", "zur", "bes\u00b7sern", "Zeit", "be\u00b7que\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PIS", "ADV", "ADV", "APPRART", "ADJA", "NN", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wann, Menschen-Freund, wann steht Dein Philadelphia", "tokens": ["Wann", ",", "Men\u00b7schen\u00b7Freund", ",", "wann", "steht", "Dein", "Phi\u00b7la\u00b7del\u00b7phia"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "$,", "NN", "$,", "PWAV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "In seiner Bruder-Lieb und Kinder-Einfalt da?", "tokens": ["In", "sei\u00b7ner", "Bru\u00b7der\u00b7Lieb", "und", "Kin\u00b7der\u00b7Ein\u00b7falt", "da", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und ach! was ist es nicht f\u00fcr ein gewisses Zeichen,", "tokens": ["Und", "ach", "!", "was", "ist", "es", "nicht", "f\u00fcr", "ein", "ge\u00b7wis\u00b7ses", "Zei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "PWS", "VAFIN", "PPER", "PTKNEG", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df du erz\u00fcrnet seyst, gerechter Jehova:", "tokens": ["Da\u00df", "du", "er\u00b7z\u00fcr\u00b7net", "seyst", ",", "ge\u00b7rech\u00b7ter", "Je\u00b7ho\u00b7va", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "VAFIN", "$,", "ADJD", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn so ein Ri\u00df geschicht, so ist der Fall gar nah,", "tokens": ["Wenn", "so", "ein", "Ri\u00df", "ge\u00b7schicht", ",", "so", "ist", "der", "Fall", "gar", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVPP", "$,", "ADV", "VAFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Fall, wo Stadt und Land aus ihrer Veste weichen,", "tokens": ["Der", "Fall", ",", "wo", "Stadt", "und", "Land", "aus", "ih\u00b7rer", "Ves\u00b7te", "wei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein l\u00f6blicher Regent von seiner Hut entr\u00fckt,", "tokens": ["Ein", "l\u00f6b\u00b7li\u00b7cher", "Re\u00b7gent", "von", "sei\u00b7ner", "Hut", "ent\u00b7r\u00fckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Bezeuget, da\u00df es sich zum Untergange schikt.", "tokens": ["Be\u00b7zeu\u00b7get", ",", "da\u00df", "es", "sich", "zum", "Un\u00b7ter\u00b7gan\u00b7ge", "schikt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Und wie so herzlich weh, wie weh ist ihr geschehen,", "tokens": ["Und", "wie", "so", "herz\u00b7lich", "weh", ",", "wie", "weh", "ist", "ihr", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "ADJD", "PTKVZ", "$,", "PWAV", "ADJD", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Frau Baase, da der Herr den lieben Ehemann", "tokens": ["Frau", "Baa\u00b7se", ",", "da", "der", "Herr", "den", "lie\u00b7ben", "E\u00b7he\u00b7mann"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "$,", "KOUS", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Von ihren H\u00e4upten nimt: Ich seh es also an,", "tokens": ["Von", "ih\u00b7ren", "H\u00e4up\u00b7ten", "nimt", ":", "Ich", "seh", "es", "al\u00b7so", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hier sey der schwere Rath des Herrn nicht abzusehen.", "tokens": ["Hier", "sey", "der", "schwe\u00b7re", "Rath", "des", "Herrn", "nicht", "ab\u00b7zu\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "ART", "NN", "PTKNEG", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Hier gilt es, hier bedarfs nicht Ueberwindens: Nein,", "tokens": ["Hier", "gilt", "es", ",", "hier", "be\u00b7darfs", "nicht", "Ue\u00b7ber\u00b7win\u00b7dens", ":", "Nein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PTKNEG", "ADV", "$.", "PTKANT", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Klag ist ihr verg\u00f6nnt: Es soll gef\u00fchlet seyn.", "tokens": ["Die", "Klag", "ist", "ihr", "ver\u00b7g\u00f6nnt", ":", "Es", "soll", "ge\u00b7f\u00fch\u00b7let", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$.", "PPER", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Mit Rechte kan sie sich im Staube niederlegen,", "tokens": ["Mit", "Rech\u00b7te", "kan", "sie", "sich", "im", "Stau\u00b7be", "nie\u00b7der\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "PRF", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um den Entschlafenen mit Thr\u00e4nen \u00fcbergehn,", "tokens": ["Um", "den", "Ent\u00b7schla\u00b7fe\u00b7nen", "mit", "Thr\u00e4\u00b7nen", "\u00fc\u00b7ber\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Es sey an ihrer Stirn das tiefste Leid zu sehn,", "tokens": ["Es", "sey", "an", "ih\u00b7rer", "Stirn", "das", "tiefs\u00b7te", "Leid", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit ihres Jammers Last den Unfall abzuw\u00e4gen:", "tokens": ["Mit", "ih\u00b7res", "Jam\u00b7mers", "Last", "den", "Un\u00b7fall", "ab\u00b7zu\u00b7w\u00e4\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NE", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Bricht uns, Gebeugete, das Br\u00fcderliche Herz", "tokens": ["Bricht", "uns", ",", "Ge\u00b7beu\u00b7ge\u00b7te", ",", "das", "Br\u00fc\u00b7der\u00b7li\u00b7che", "Herz"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.6": {"text": "Und ihr entsinkt das Haupt; wie tiefer dringt ihr Schmerz?", "tokens": ["Und", "ihr", "ent\u00b7sinkt", "das", "Haupt", ";", "wie", "tie\u00b7fer", "dringt", "ihr", "Schmerz", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$.", "PWAV", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Ihr, die ihr ehemals das angenehme Wesen,", "tokens": ["Ihr", ",", "die", "ihr", "e\u00b7he\u00b7mals", "das", "an\u00b7ge\u00b7neh\u00b7me", "We\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Heinrich, unser Freund, nur von Natur besa\u00df,", "tokens": ["Das", "Hein\u00b7rich", ",", "un\u00b7ser", "Freund", ",", "nur", "von", "Na\u00b7tur", "be\u00b7sa\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "PPOSAT", "NN", "$,", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Besonders hochgesch\u00e4tzt, und nur sein Gnaden-Maa\u00df,", "tokens": ["Be\u00b7son\u00b7ders", "hoch\u00b7ge\u00b7sch\u00e4tzt", ",", "und", "nur", "sein", "Gna\u00b7den\u00b7Maa\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "KON", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die neue Creatur, zu euerm Spott erlesen,", "tokens": ["Die", "neu\u00b7e", "Crea\u00b7tur