 "trochaic.tri"}, "line.3": {"text": "Nachtigall und Bienen,", "tokens": ["Nach\u00b7ti\u00b7gall", "und", "Bie\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Fl\u00f6tenton und Reihngesang!", "tokens": ["Fl\u00f6\u00b7ten\u00b7ton", "und", "Reihn\u00b7ge\u00b7sang", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "O wie jauchzt der Freude Klang!", "tokens": ["O", "wie", "jauchzt", "der", "Freu\u00b7de", "Klang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Herrlich jauchzt der Freude Klang!", "tokens": ["Herr\u00b7lich", "jauchzt", "der", "Freu\u00b7de", "Klang", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "O wie labt's, im Traum zu ruhn!", "tokens": ["O", "wie", "labt's", ",", "im", "Traum", "zu", "ruhn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "NE", "$,", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo durch Kies und Erlen", "tokens": ["Wo", "durch", "Kies", "und", "Er\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Leise Wellen perlen,", "tokens": ["Lei\u00b7se", "Wel\u00b7len", "per\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text":