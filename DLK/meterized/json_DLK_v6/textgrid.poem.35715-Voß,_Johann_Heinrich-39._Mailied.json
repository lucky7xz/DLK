{"textgrid.poem.35715": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "39. Mailied", "genre": "verse", "period": "N.A.", "pub_year": 1789, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O der sch\u00f6ne Maienmond!", "tokens": ["O", "der", "sch\u00f6\u00b7ne", "Mai\u00b7en\u00b7mond", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wann in Thal und H\u00f6hen", "tokens": ["Wann", "in", "Thal", "und", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Bl\u00fctenb\u00e4ume wehen,", "tokens": ["Bl\u00fc\u00b7ten\u00b7b\u00e4u\u00b7me", "we\u00b7hen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Und im Nest der Vogel wohnt!", "tokens": ["Und", "im", "Nest", "der", "Vo\u00b7gel", "wohnt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NE", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "O der sch\u00f6ne Maienmond!", "tokens": ["O", "der", "sch\u00f6\u00b7ne", "Mai\u00b7en\u00b7mond", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Herrlich sch\u00f6ner Maienmond!", "tokens": ["Herr\u00b7lich", "sch\u00f6\u00b7ner", "Mai\u00b7en\u00b7mond", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "O wie prangt die sch\u00f6ne Welt!", "tokens": ["O", "wie", "prangt", "die", "sch\u00f6\u00b7ne", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Br\u00e4unlich spro\u00dft die Eiche", "tokens": ["Br\u00e4un\u00b7lich", "spro\u00dft", "die", "Ei\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "An umgr\u00fcntem Teiche,", "tokens": ["An", "um\u00b7gr\u00fcn\u00b7tem", "Tei\u00b7che", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Graulich wogt das Roggenfeld!", "tokens": ["Grau\u00b7lich", "wogt", "das", "Rog\u00b7gen\u00b7feld", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "O wie prangt die sch\u00f6ne Welt!", "tokens": ["O", "wie", "prangt", "die", "sch\u00f6\u00b7ne", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Herrlich prangt die sch\u00f6ne Welt!", "tokens": ["Herr\u00b7lich", "prangt", "die", "sch\u00f6\u00b7ne", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "O wie frisch die Morgenluft!", "tokens": ["O", "wie", "frisch", "die", "Mor\u00b7gen\u00b7luft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Blume