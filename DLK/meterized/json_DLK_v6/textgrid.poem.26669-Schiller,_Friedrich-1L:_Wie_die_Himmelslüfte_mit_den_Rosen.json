{"textgrid.poem.26669": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie die Himmelsl\u00fcfte mit den Rosen", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie die Himmelsl\u00fcfte mit den Rosen", "tokens": ["Wie", "die", "Him\u00b7mel\u00b7sl\u00fcf\u00b7te", "mit", "den", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "An den Fr\u00fchlingsmorgen z\u00e4rtlich kosen,", "tokens": ["An", "den", "Fr\u00fch\u00b7lings\u00b7mor\u00b7gen", "z\u00e4rt\u00b7lich", "ko\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Kind, so schmeichelt dir", "tokens": ["Kind", ",", "so", "schmei\u00b7chelt", "dir"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPER"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Itzt das \u00e4u\u00dfre Gl\u00fcck in deinen Jugendtagen.", "tokens": ["Itzt", "das", "\u00e4u\u00df\u00b7re", "Gl\u00fcck", "in", "dei\u00b7nen", "Ju\u00b7gend\u00b7ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Tr\u00e4nen sahst du nur, noch rangen keine Klagen", "tokens": ["Tr\u00e4\u00b7nen", "sahst", "du", "nur", ",", "noch", "ran\u00b7gen", "kei\u00b7ne", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ADV", "$,", "ADV", "VVFIN", "PIAT", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Sich aus deiner Brust herf\u00fcr.", "tokens": ["Sich", "aus", "dei\u00b7ner", "Brust", "her\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aber sieh! der Hain, der kaum entz\u00fccket,", "tokens": ["A\u00b7ber", "sieh", "!", "der", "Hain", ",", "der", "kaum", "ent\u00b7z\u00fc\u00b7cket", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$.", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Neigt sich, pl\u00f6tzlich rast der Sturm, zerknicket", "tokens": ["Neigt", "sich", ",", "pl\u00f6tz\u00b7lich", "rast", "der", "Sturm", ",", "zer\u00b7kni\u00b7cket"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PRF", "$,", "ADJD", "VVFIN", "ART", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Liegt die Rosenblum!", "tokens": ["Liegt", "die", "Ro\u00b7sen\u00b7blum", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "O so ist es, Sohn, mit unsern Sinnesfreuden,", "tokens": ["O", "so", "ist", "es", ",", "Sohn", ",", "mit", "un\u00b7sern", "Sin\u00b7nes\u00b7freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VAFIN", "PPER", "$,", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Unserm Golde, unsern lichten Herrlichkeiten,", "tokens": ["Un\u00b7serm", "Gol\u00b7de", ",", "un\u00b7sern", "lich\u00b7ten", "Herr\u00b7lich\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "So mit unserm Flitterruhm.", "tokens": ["So", "mit", "un\u00b7serm", "Flit\u00b7ter\u00b7ruhm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nur des H\u00f6chsten Abglanz, der Gerechte,", "tokens": ["Nur", "des", "H\u00f6chs\u00b7ten", "Ab\u00b7glanz", ",", "der", "Ge\u00b7rech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Welcher in dem schr\u00f6cklichen Gefechte", "tokens": ["Wel\u00b7cher", "in", "dem", "schr\u00f6ck\u00b7li\u00b7chen", "Ge\u00b7fech\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAT", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Zwischen Lust und Pflicht", "tokens": ["Zwi\u00b7schen", "Lust", "und", "Pflicht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Jener sich entringt, der h\u00f6hern Weisheit Stimme", "tokens": ["Je\u00b7ner", "sich", "ent\u00b7ringt", ",", "der", "h\u00f6\u00b7hern", "Weis\u00b7heit", "Stim\u00b7me"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDAT", "PRF", "VVFIN", "$,", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Folget, trotz der Selbstsucht hei\u00dfem Grimme,", "tokens": ["Fol\u00b7get", ",", "trotz", "der", "Selbst\u00b7sucht", "hei\u00b7\u00dfem", "Grim\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Die sein Herz mit Schwertern sticht \u2013", "tokens": ["Die", "sein", "Herz", "mit", "Schwer\u00b7tern", "sticht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Dessen Wollust tr\u00e4gt von hier die Bahre", "tokens": ["Des\u00b7sen", "Wol\u00b7lust", "tr\u00e4gt", "von", "hier", "die", "Bah\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "VVFIN", "APPR", "ADV", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Nicht, es l\u00f6scht sie nicht der Strom der Jahre,", "tokens": ["Nicht", ",", "es", "l\u00f6scht", "sie", "nicht", "der", "Strom", "der", "Jah\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Nicht die Ewigkeit:", "tokens": ["Nicht", "die", "E\u00b7wig\u00b7keit", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Angeleuchtet k\u00f6nnt er in den letzten Blitzen", "tokens": ["An\u00b7ge\u00b7leuch\u00b7tet", "k\u00f6nnt", "er", "in", "den", "letz\u00b7ten", "Blit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Und vom Weltenumsturz angeschwungen sitzen", "tokens": ["Und", "vom", "Wel\u00b7ten\u00b7um\u00b7sturz", "an\u00b7ge\u00b7schwun\u00b7gen", "sit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVPP", "VVINF"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Ohne Menschenbangigkeit.", "tokens": ["Oh\u00b7ne", "Men\u00b7schen\u00b7ban\u00b7gig\u00b7keit", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}