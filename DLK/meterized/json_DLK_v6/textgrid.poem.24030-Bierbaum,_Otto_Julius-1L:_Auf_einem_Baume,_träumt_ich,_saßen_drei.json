{"textgrid.poem.24030": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf einem Baume, tr\u00e4umt ich, sa\u00dfen drei", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf einem Baume, tr\u00e4umt ich, sa\u00dfen drei", "tokens": ["Auf", "ei\u00b7nem", "Bau\u00b7me", ",", "tr\u00e4umt", "ich", ",", "sa\u00b7\u00dfen", "drei"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "VVFIN", "PPER", "$,", "VVFIN", "CARD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ganz kleine V\u00f6gel, rot und grau und gr\u00fcn,", "tokens": ["Ganz", "klei\u00b7ne", "V\u00f6\u00b7gel", ",", "rot", "und", "grau", "und", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit schwarzen Schn\u00e4beln, und sie zwitscherten.", "tokens": ["Mit", "schwar\u00b7zen", "Schn\u00e4\u00b7beln", ",", "und", "sie", "zwit\u00b7scher\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "KON", "PPER", "VVINF", "$."], "meter": "-+-+--+++-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ich stand am Stamm des Baums und h\u00f6rte zu,", "tokens": ["Ich", "stand", "am", "Stamm", "des", "Baums", "und", "h\u00f6r\u00b7te", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und ich verstand genau den Sinn des Sangs.", "tokens": ["Und", "ich", "ver\u00b7stand", "ge\u00b7nau", "den", "Sinn", "des", "Sangs", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und in mir blieb der Sinn.", "tokens": ["Und", "in", "mir", "blieb", "der", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}