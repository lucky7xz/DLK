{"textgrid.poem.46452": {"metadata": {"author": {"name": "Engelke, Gerrit", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mich aber schone, Tod,", "genre": "verse", "period": "N.A.", "pub_year": 1904, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mich aber schone, Tod,", "tokens": ["Mich", "a\u00b7ber", "scho\u00b7ne", ",", "Tod", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$,", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mir dampft noch Jugend blutstromrot, \u2013", "tokens": ["Mir", "dampft", "noch", "Ju\u00b7gend", "bluts\u00b7trom\u00b7rot", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch hab ich nicht mein Werk erf\u00fcllt,", "tokens": ["Noch", "hab", "ich", "nicht", "mein", "Werk", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch ist die Zukunft dunstverh\u00fcllt \u2013", "tokens": ["Noch", "ist", "die", "Zu\u00b7kunft", "dunst\u00b7ver\u00b7h\u00fcllt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Drum schone mich, Tod.", "tokens": ["Drum", "scho\u00b7ne", "mich", ",", "Tod", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Wenn sp\u00e4ter einst, Tod,", "tokens": ["Wenn", "sp\u00e4\u00b7ter", "einst", ",", "Tod", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ADJD", "ADV", "$,", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Mein Leben verlebt ist, verloht", "tokens": ["Mein", "Le\u00b7ben", "ver\u00b7lebt", "ist", ",", "ver\u00b7loht"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "VVPP", "VAFIN", "$,", "VVFIN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ins Werk \u2013 wenn das m\u00fcde Herz sich neigt,", "tokens": ["Ins", "Werk", "\u2013", "wenn", "das", "m\u00fc\u00b7de", "Herz", "sich", "neigt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$(", "KOUS", "ART", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wenn die Welt mir schweigt, \u2013", "tokens": ["Wenn", "die", "Welt", "mir", "schweigt", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "$,", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Dann trage mich fort, Tod.", "tokens": ["Dann", "tra\u00b7ge", "mich", "fort", ",", "Tod", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}