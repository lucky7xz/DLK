{"textgrid.poem.38645": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1. Der Genfersee", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr, die zur Wonne der G\u00f6tter und Menschen mein Alter verherrlicht,", "tokens": ["Ihr", ",", "die", "zur", "Won\u00b7ne", "der", "G\u00f6t\u00b7ter", "und", "Men\u00b7schen", "mein", "Al\u00b7ter", "ver\u00b7herr\u00b7licht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPRART", "NN", "ART", "NN", "KON", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Zinnen des Ufers! o g\u00f6nnt mir ein erheiterndes Wort.", "tokens": ["Zin\u00b7nen", "des", "U\u00b7fers", "!", "o", "g\u00f6nnt", "mir", "ein", "er\u00b7hei\u00b7tern\u00b7des", "Wort", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "FM", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Lange schon raubt mir die Geissel der grollenden Bise den Schlummer,", "tokens": ["Lan\u00b7ge", "schon", "raubt", "mir", "die", "Geis\u00b7sel", "der", "grol\u00b7len\u00b7den", "Bi\u00b7se", "den", "Schlum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Auch hat mich Frankreichs Kourier ganz hypochondrisch gemacht.", "tokens": ["Auch", "hat", "mich", "Fran\u00b7kreichs", "Kou\u00b7rier", "ganz", "hy\u00b7po\u00b7chond\u00b7risch", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NE", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+--+-++-+", "measure": "iambic.septa.relaxed"}}}}}