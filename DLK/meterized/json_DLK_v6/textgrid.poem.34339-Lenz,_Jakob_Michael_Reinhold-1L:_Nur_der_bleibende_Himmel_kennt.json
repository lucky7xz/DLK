{"textgrid.poem.34339": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nur der bleibende Himmel kennt", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nur der bleibende Himmel kennt", "tokens": ["Nur", "der", "blei\u00b7ben\u00b7de", "Him\u00b7mel", "kennt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Was er den schwachen Sterblichen g\u00f6nnt;", "tokens": ["Was", "er", "den", "schwa\u00b7chen", "Sterb\u00b7li\u00b7chen", "g\u00f6nnt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "All ihr Gl\u00fcck erstohlen von Quaalen;", "tokens": ["All", "ihr", "Gl\u00fcck", "er\u00b7stoh\u00b7len", "von", "Qua\u00b7a\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VVPP", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Hinter Wolken zitternde Stralen;", "tokens": ["Hin\u00b7ter", "Wol\u00b7ken", "zit\u00b7tern\u00b7de", "Stra\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Was ihr Herz sich gesteht und verheelt,", "tokens": ["Was", "ihr", "Herz", "sich", "ge\u00b7steht", "und", "ver\u00b7heelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "PRF", "VVPP", "KON", "VVFIN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Alles hat er ihnen zugez\u00e4hlt;", "tokens": ["Al\u00b7les", "hat", "er", "ih\u00b7nen", "zu\u00b7ge\u00b7z\u00e4hlt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Unerbittlich \u2013 all ihre Triebe,", "tokens": ["Un\u00b7er\u00b7bitt\u00b7lich", "\u2013", "all", "ih\u00b7re", "Trie\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$(", "PIAT", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Alle Gestalten und Grad' ihrer Liebe,", "tokens": ["Al\u00b7le", "Ge\u00b7stal\u00b7ten", "und", "Grad'", "ih\u00b7rer", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "PPOSAT", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.9": {"text": "Alle Fehler des Augenblicks,", "tokens": ["Al\u00b7le", "Feh\u00b7ler", "des", "Au\u00b7gen\u00b7blicks", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.10": {"text": "Oft die R\u00e4uber ewigen Gl\u00fccks,", "tokens": ["Oft", "die", "R\u00e4u\u00b7ber", "e\u00b7wi\u00b7gen", "Gl\u00fccks", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.11": {"text": "Allen Unverstand, Delikatessen,", "tokens": ["Al\u00b7len", "Un\u00b7ver\u00b7stand", ",", "De\u00b7li\u00b7ka\u00b7tes\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Wo sie nicht noth waren, Plumpheit, Vergessen", "tokens": ["Wo", "sie", "nicht", "noth", "wa\u00b7ren", ",", "Plum\u00b7pheit", ",", "Ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PWAV", "PPER", "PTKNEG", "NN", "VAFIN", "$,", "NN", "$,", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.13": {"text": "Seiner selbst, oder dessen was nie", "tokens": ["Sei\u00b7ner", "selbst", ",", "o\u00b7der", "des\u00b7sen", "was", "nie"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADV", "$,", "KON", "PDS", "PRELS", "ADV"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.14": {"text": "Gut gemacht wird, der Harmonie,", "tokens": ["Gut", "ge\u00b7macht", "wird", ",", "der", "Har\u00b7mo\u00b7nie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VAFIN", "$,", "ART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.15": {"text": "Die aller Wesen Wohlstand erh\u00e4lt,", "tokens": ["Die", "al\u00b7ler", "We\u00b7sen", "Wohl\u00b7stand", "er\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.16": {"text": "Dieses Himmels auf der Welt \u2013", "tokens": ["Die\u00b7ses", "Him\u00b7mels", "auf", "der", "Welt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "All das l\u00e4\u00dft er mit kindischem Schrein", "tokens": ["All", "das", "l\u00e4\u00dft", "er", "mit", "kin\u00b7di\u00b7schem", "Schrein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "PDS", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.18": {"text": "Uns in der Wiege schon prophezein.", "tokens": ["Uns", "in", "der", "Wie\u00b7ge", "schon", "pro\u00b7phe\u00b7zein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.19": {"text": "Reitzt nicht oft schon des S\u00e4uglings Stimme", "tokens": ["Reitzt", "nicht", "oft", "schon", "des", "S\u00e4ug\u00b7lings", "Stim\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "ADV", "ADV", "ART", "NN", "NN"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.20": {"text": "Seinen Zorn zum k\u00fcnftigen Grimme", "tokens": ["Sei\u00b7nen", "Zorn", "zum", "k\u00fcnf\u00b7ti\u00b7gen", "Grim\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPRART", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.21": {"text": "Und seiner stillen Thr\u00e4nen Geduld", "tokens": ["Und", "sei\u00b7ner", "stil\u00b7len", "Thr\u00e4\u00b7nen", "Ge\u00b7duld"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.22": {"text": "Seine Gnade zur k\u00fcnftigen Huld?", "tokens": ["Sei\u00b7ne", "Gna\u00b7de", "zur", "k\u00fcnf\u00b7ti\u00b7gen", "Huld", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.23": {"text": "Ach womit mu\u00df ich's versehen haben,", "tokens": ["Ach", "wo\u00b7mit", "mu\u00df", "ich's", "ver\u00b7se\u00b7hen", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWAV", "VMFIN", "PIS", "VVPP", "VAINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.24": {"text": "Da\u00df meine erste Liebe begraben?", "tokens": ["Da\u00df", "mei\u00b7ne", "ers\u00b7te", "Lie\u00b7be", "be\u00b7gra\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.25": {"text": "Da\u00df meines Herzens Unbestand", "tokens": ["Da\u00df", "mei\u00b7nes", "Her\u00b7zens", "Un\u00b7be\u00b7stand"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Nachher nirgends Ruhe fand?", "tokens": ["Nach\u00b7her", "nir\u00b7gends", "Ru\u00b7he", "fand", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Da\u00df deine k\u00f6stlichsten Sch\u00e4tze auf Erden", "tokens": ["Da\u00df", "dei\u00b7ne", "k\u00f6st\u00b7lichs\u00b7ten", "Sch\u00e4t\u00b7ze", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.28": {"text": "Mir nur im Fluge gewiesen werden;", "tokens": ["Mir", "nur", "im", "Flu\u00b7ge", "ge\u00b7wie\u00b7sen", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.29": {"text": "Und in dem schwimmenden Augenblick", "tokens": ["Und", "in", "dem", "schwim\u00b7men\u00b7den", "Au\u00b7gen\u00b7blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.30": {"text": "Des seeligen Genusses \u2013 beb' ich zur\u00fcck", "tokens": ["Des", "see\u00b7li\u00b7gen", "Ge\u00b7nus\u00b7ses", "\u2013", "beb'", "ich", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.31": {"text": "Fort in den furchtbaren Strudel des Geschickes;", "tokens": ["Fort", "in", "den", "furcht\u00b7ba\u00b7ren", "Stru\u00b7del", "des", "Ge\u00b7schi\u00b7ckes", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.32": {"text": "Fort fort ohne Hofnung des vorigen Gl\u00fcckes,", "tokens": ["Fort", "fort", "oh\u00b7ne", "Hof\u00b7nung", "des", "vo\u00b7ri\u00b7gen", "Gl\u00fc\u00b7ckes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.33": {"text": "Ohne Wiedererinnerung fort,", "tokens": ["Oh\u00b7ne", "Wie\u00b7de\u00b7re\u00b7rin\u00b7ne\u00b7rung", "fort", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.34": {"text": "Wo mein Leben in W\u00fcsten verdorrt,", "tokens": ["Wo", "mein", "Le\u00b7ben", "in", "W\u00fcs\u00b7ten", "ver\u00b7dorrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.35": {"text": "Wo niemand Theil nimmt, niemand mich kennet,", "tokens": ["Wo", "nie\u00b7mand", "Theil", "nimmt", ",", "nie\u00b7mand", "mich", "ken\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "NN", "VVFIN", "$,", "PIS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.36": {"text": "Niemand mir Theil zu nehmen g\u00f6nnet,", "tokens": ["Nie\u00b7mand", "mir", "Theil", "zu", "neh\u00b7men", "g\u00f6n\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PPER", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Und die Natur selbst k\u00e4lter scheint,", "tokens": ["Und", "die", "Na\u00b7tur", "selbst", "k\u00e4l\u00b7ter", "scheint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Weil sich niemand mit ihr befreundt?", "tokens": ["Weil", "sich", "nie\u00b7mand", "mit", "ihr", "be\u00b7freundt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PIS", "APPR", "PPOSAT", "VVPP", "$."], "meter": "+-+----+", "measure": "unknown.measure.tri"}, "line.39": {"text": "O gute G\u00f6tter! wie gl\u00fcckliche Stunden,", "tokens": ["O", "gu\u00b7te", "G\u00f6t\u00b7ter", "!", "wie", "gl\u00fcck\u00b7li\u00b7che", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "PWAV", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.40": {"text": "Wie schr\u00f6cklich leere sind mir verschwunden!", "tokens": ["Wie", "schr\u00f6ck\u00b7lich", "lee\u00b7re", "sind", "mir", "ver\u00b7schwun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADJA", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.41": {"text": "Ihr z\u00e4hlet sie alle. Bewilligt mir", "tokens": ["Ihr", "z\u00e4h\u00b7let", "sie", "al\u00b7le", ".", "Be\u00b7wil\u00b7ligt", "mir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "$.", "VVFIN", "PPER"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.42": {"text": "Nur eine Bitte: solltet ihr", "tokens": ["Nur", "ei\u00b7ne", "Bit\u00b7te", ":", "soll\u00b7tet", "ihr"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "NN", "$.", "VMFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Noch der gl\u00fccklichen \u00fcbrig haben,", "tokens": ["Noch", "der", "gl\u00fcck\u00b7li\u00b7chen", "\u00fcb\u00b7rig", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "ADJD", "VAINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.44": {"text": "Ach geht sparsam mit euren Gaben!", "tokens": ["Ach", "geht", "spar\u00b7sam", "mit", "eu\u00b7ren", "Ga\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.45": {"text": "Hieltet ihr aber doch nicht Hau\u00df,", "tokens": ["Hiel\u00b7tet", "ihr", "a\u00b7ber", "doch", "nicht", "Hau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PTKNEG", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.46": {"text": "Mir zur Strafe vielleicht, so halt ich", "tokens": ["Mir", "zur", "Stra\u00b7fe", "viel\u00b7leicht", ",", "so", "halt", "ich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "APPRART", "NN", "ADV", "$,", "ADV", "VVFIN", "PPER"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.47": {"text": "Wenigstens zu der Sterbestunde", "tokens": ["We\u00b7nigs\u00b7tens", "zu", "der", "Ster\u00b7be\u00b7stun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.48": {"text": "Mir ein St\u00fcndgen mit \u2013 aus.", "tokens": ["Mir", "ein", "St\u00fcnd\u00b7gen", "mit", "\u2013", "aus", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ART", "NN", "APPR", "$(", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}