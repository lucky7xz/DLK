{"textgrid.poem.43726": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So schlaf' in Ruh!", "tokens": ["So", "schlaf'", "in", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Die Zeitlos' und die Tulpe nickt,", "tokens": ["Die", "Zeit\u00b7los'", "und", "die", "Tul\u00b7pe", "nickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf da\u00df der Schlaf sie auch erquickt.", "tokens": ["Auf", "da\u00df", "der", "Schlaf", "sie", "auch", "er\u00b7quickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Aeugelein zu!", "tokens": ["Die", "A\u00b7e\u00b7u\u00b7ge\u00b7lein", "zu", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mein Kindlein du!", "tokens": ["Mein", "Kin\u00b7dlein", "du", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Nun schlaf' in Ruh!", "tokens": ["Nun", "schlaf'", "in", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "So schlaf' in Ruh!", "tokens": ["So", "schlaf'", "in", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Die L\u00e4mmlein sind jetzt m\u00fcd' und satt,", "tokens": ["Die", "L\u00e4mm\u00b7lein", "sind", "jetzt", "m\u00fcd'", "und", "satt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie suchen ihre Lagerstatt.", "tokens": ["Sie", "su\u00b7chen", "ih\u00b7re", "La\u00b7ger\u00b7statt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Aeugelein zu!", "tokens": ["Die", "A\u00b7e\u00b7u\u00b7ge\u00b7lein", "zu", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mein Kindlein du,", "tokens": ["Mein", "Kin\u00b7dlein", "du", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Nun schlaf' in Ruh!", "tokens": ["Nun", "schlaf'", "in", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "So schlaf' in Ruh!", "tokens": ["So", "schlaf'", "in", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Der Vogel fliegt zum Dornenstrauch:", "tokens": ["Der", "Vo\u00b7gel", "fliegt", "zum", "Dor\u00b7nen\u00b7strauch", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbjetzt ist es Nacht, drum schlaf' ich auch.\u00ab", "tokens": ["\u00bb", "jetzt", "ist", "es", "Nacht", ",", "drum", "schlaf'", "ich", "auch", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "NN", "$,", "PAV", "VVFIN", "PPER", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Aeugelein zu!", "tokens": ["Die", "A\u00b7e\u00b7u\u00b7ge\u00b7lein", "zu", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mein Kindlein du,", "tokens": ["Mein", "Kin\u00b7dlein", "du", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Nun schlaf' in Ruh!", "tokens": ["Nun", "schlaf'", "in", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "So schlaf' in Ruh!", "tokens": ["So", "schlaf'", "in", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Die Sterne leuchten hell und klar,", "tokens": ["Die", "Ster\u00b7ne", "leuch\u00b7ten", "hell", "und", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es kommt von dort der Engel Schaar,", "tokens": ["Es", "kommt", "von", "dort", "der", "En\u00b7gel", "Schaar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Aeugelein zu!", "tokens": ["Die", "A\u00b7e\u00b7u\u00b7ge\u00b7lein", "zu", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mein Kindlein du,", "tokens": ["Mein", "Kin\u00b7dlein", "du", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Nun schlaf' in Ruh!", "tokens": ["Nun", "schlaf'", "in", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "So schlaf' in Ruh!", "tokens": ["So", "schlaf'", "in", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Es kommt auch einer her und wacht,", "tokens": ["Es", "kommt", "auch", "ei\u00b7ner", "her", "und", "wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIS", "PTKVZ", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Kind, bei dir die ganze Nacht.", "tokens": ["Mein", "Kind", ",", "bei", "dir", "die", "gan\u00b7ze", "Nacht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Aeugelein zu!", "tokens": ["Die", "A\u00b7e\u00b7u\u00b7ge\u00b7lein", "zu", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mein Kindlein du,", "tokens": ["Mein", "Kin\u00b7dlein", "du", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Nun schlaf' in Ruh!", "tokens": ["Nun", "schlaf'", "in", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "So schlaf' in Ruh!", "tokens": ["So", "schlaf'", "in", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Er breitet seine Fl\u00fcgel aus,", "tokens": ["Er", "brei\u00b7tet", "sei\u00b7ne", "Fl\u00fc\u00b7gel", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und singt: Gott segne dieses Haus!", "tokens": ["Und", "singt", ":", "Gott", "seg\u00b7ne", "die\u00b7ses", "Haus", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "NN", "VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Aeugelein zu!", "tokens": ["Die", "A\u00b7e\u00b7u\u00b7ge\u00b7lein", "zu", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mein Kindlein du,", "tokens": ["Mein", "Kin\u00b7dlein", "du", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Nun schlaf' in Ruh!", "tokens": ["Nun", "schlaf'", "in", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}