{"textgrid.poem.45015": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[wir K\u00fcnstler, du, auch ich vielleicht]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir K\u00fcnstler, du, auch ich vielleicht,", "tokens": ["Wir", "K\u00fcnst\u00b7ler", ",", "du", ",", "auch", "ich", "viel\u00b7leicht", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PPER", "$,", "ADV", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir liegen an dem Strand", "tokens": ["Wir", "lie\u00b7gen", "an", "dem", "Strand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und schwimmen erst, wenn uns erreicht", "tokens": ["Und", "schwim\u00b7men", "erst", ",", "wenn", "uns", "er\u00b7reicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "$,", "KOUS", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Wassers h\u00f6chster Rand.", "tokens": ["Des", "Was\u00b7sers", "h\u00f6chs\u00b7ter", "Rand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wenn nun der Schnee in Bergen schmolz,", "tokens": ["Wenn", "nun", "der", "Schnee", "in", "Ber\u00b7gen", "schmolz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Strom die Ufer dr\u00e4ngt,", "tokens": ["Der", "Strom", "die", "U\u00b7fer", "dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Treibt alles, Kahn und Laub und Holz,", "tokens": ["Treibt", "al\u00b7les", ",", "Kahn", "und", "Laub", "und", "Holz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Schwalle bunt vermengt.", "tokens": ["Im", "Schwal\u00b7le", "bunt", "ver\u00b7mengt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ja, wohl am leichtsten schwimmt daher,", "tokens": ["Ja", ",", "wohl", "am", "leichts\u00b7ten", "schwimmt", "da\u00b7her", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "APPRART", "ADJA", "VVFIN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was ganz dem Zug sich gibt,", "tokens": ["Was", "ganz", "dem", "Zug", "sich", "gibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Indes das Schiff, beladen schwer,", "tokens": ["In\u00b7des", "das", "Schiff", ",", "be\u00b7la\u00b7den", "schwer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADJA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur langsam vorw\u00e4rts schiebt.", "tokens": ["Nur", "lang\u00b7sam", "vor\u00b7w\u00e4rts", "schiebt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}