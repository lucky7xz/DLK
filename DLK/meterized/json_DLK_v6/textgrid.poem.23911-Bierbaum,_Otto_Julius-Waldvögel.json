{"textgrid.poem.23911": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Waldv\u00f6gel", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein wohlbestelltes Mieder,", "tokens": ["Ein", "wohl\u00b7be\u00b7stell\u00b7tes", "Mie\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Backen rot gesund,", "tokens": ["Die", "Ba\u00b7cken", "rot", "ge\u00b7sund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Schnabel voller Lieder", "tokens": ["Den", "Schna\u00b7bel", "vol\u00b7ler", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und vorn und hinten rund.", "tokens": ["Und", "vorn", "und", "hin\u00b7ten", "rund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KON", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Zwei Augen glutend blaue", "tokens": ["Zwei", "Au\u00b7gen", "glu\u00b7tend", "blau\u00b7e"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "NN", "ADJD", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und eine kleine Hand,", "tokens": ["Und", "ei\u00b7ne", "klei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wohl mir, waldwilde Fraue,", "tokens": ["Wohl", "mir", ",", "wald\u00b7wil\u00b7de", "Frau\u00b7e", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Da\u00df ich dich einsten fand.", "tokens": ["Da\u00df", "ich", "dich", "eins\u00b7ten", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVIZU", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es war im tiefen Walde", "tokens": ["Es", "war", "im", "tie\u00b7fen", "Wal\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und Sommers war die Zeit,", "tokens": ["Und", "Som\u00b7mers", "war", "die", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In einem Wipfel balde", "tokens": ["In", "ei\u00b7nem", "Wip\u00b7fel", "bal\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nesthockten wir zu zweit", "tokens": ["Nest\u00b7hock\u00b7ten", "wir", "zu", "zweit"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und niemand hat gesehen", "tokens": ["Und", "nie\u00b7mand", "hat", "ge\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIS", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das sondre Vogelpaar,", "tokens": ["Das", "sond\u00b7re", "Vo\u00b7gel\u00b7paar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das hoch im Windewehen", "tokens": ["Das", "hoch", "im", "Win\u00b7de\u00b7we\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vor Gl\u00fccke schwindlig war.", "tokens": ["Vor", "Gl\u00fc\u00b7cke", "schwind\u00b7lig", "war", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}