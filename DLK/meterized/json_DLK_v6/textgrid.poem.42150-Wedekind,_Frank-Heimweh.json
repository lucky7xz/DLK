{"textgrid.poem.42150": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Heimweh", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcber bemooste Steine", "tokens": ["\u00dc\u00b7ber", "be\u00b7moos\u00b7te", "Stei\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "F\u00e4llt ein rauschender Quell,", "tokens": ["F\u00e4llt", "ein", "rau\u00b7schen\u00b7der", "Quell", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Glitzert im Mondenscheine,", "tokens": ["Glit\u00b7zert", "im", "Mon\u00b7den\u00b7schei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Funkelt so silberhell.", "tokens": ["Fun\u00b7kelt", "so", "sil\u00b7ber\u00b7hell", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Sinnend sa\u00df ich daneben,", "tokens": ["Sin\u00b7nend", "sa\u00df", "ich", "da\u00b7ne\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PAV", "$,"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Sah, wie die Welle sch\u00e4umt,", "tokens": ["Sah", ",", "wie", "die", "Wel\u00b7le", "sch\u00e4umt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Hab vom vergangenen Leben,", "tokens": ["Hab", "vom", "ver\u00b7gan\u00b7ge\u00b7nen", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Hab von der Zukunft getr\u00e4umt.", "tokens": ["Hab", "von", "der", "Zu\u00b7kunft", "ge\u00b7tr\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "In der Tiefe der Wogen", "tokens": ["In", "der", "Tie\u00b7fe", "der", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Sah ich gar mancherlei,", "tokens": ["Sah", "ich", "gar", "man\u00b7cher\u00b7lei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PIS", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Viele Gestalten zogen", "tokens": ["Vie\u00b7le", "Ge\u00b7stal\u00b7ten", "zo\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Gr\u00fc\u00dfend an mir vorbei.", "tokens": ["Gr\u00fc\u00b7\u00dfend", "an", "mir", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Waren die lieben Seelen,", "tokens": ["Wa\u00b7ren", "die", "lie\u00b7ben", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Die mich dereinst erfreut,", "tokens": ["Die", "mich", "de\u00b7reinst", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die meinem Herzen fehlen", "tokens": ["Die", "mei\u00b7nem", "Her\u00b7zen", "feh\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hier in der Einsamkeit.", "tokens": ["Hier", "in", "der", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Tausendmal la\u00df dir danken,", "tokens": ["Tau\u00b7send\u00b7mal", "la\u00df", "dir", "dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Lieblicher Silberbach,", "tokens": ["Lieb\u00b7li\u00b7cher", "Sil\u00b7ber\u00b7bach", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Da\u00df du den Heimwehkranken", "tokens": ["Da\u00df", "du", "den", "Heim\u00b7weh\u00b7kran\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Tr\u00f6stest im Ungemach;", "tokens": ["Tr\u00f6s\u00b7test", "im", "Un\u00b7ge\u00b7mach", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "Da\u00df du aus alten Tagen", "tokens": ["Da\u00df", "du", "aus", "al\u00b7ten", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Freundliches mir erz\u00e4hlt,", "tokens": ["Freund\u00b7li\u00b7ches", "mir", "er\u00b7z\u00e4hlt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "PPER", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Da\u00df ich dir durfte klagen,", "tokens": ["Da\u00df", "ich", "dir", "durf\u00b7te", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was meinem Herzen fehlt.", "tokens": ["Was", "mei\u00b7nem", "Her\u00b7zen", "fehlt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}