{"textgrid.poem.66226": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Der Hahn und der Adler", "genre": "verse", "period": "N.A.", "pub_year": 1774, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein F\u00fcrst war einem Hahnen hold \u2013", "tokens": ["Ein", "F\u00fcrst", "war", "ei\u00b7nem", "Hah\u00b7nen", "hold", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbwarum nicht gar! Was? einem Hahnen?\u00ab", "tokens": ["\u00bb", "wa\u00b7rum", "nicht", "gar", "!", "Was", "?", "ei\u00b7nem", "Hah\u00b7nen", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "PTKNEG", "ADV", "$.", "PWS", "$.", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ja, ja, er liebt ihn mehr, als seine Unterthanen.", "tokens": ["Ja", ",", "ja", ",", "er", "liebt", "ihn", "mehr", ",", "als", "sei\u00b7ne", "Un\u00b7ter\u00b7tha\u00b7nen", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PTKANT", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sein Kamm war Purpur, seine Federn Gold.", "tokens": ["Sein", "Kamm", "war", "Pur\u00b7pur", ",", "sei\u00b7ne", "Fe\u00b7dern", "Gold", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$,", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dumm war er zwar; jedoch sein Kikriki", "tokens": ["Dumm", "war", "er", "zwar", ";", "je\u00b7doch", "sein", "Kik\u00b7ri\u00b7ki"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "$.", "ADV", "PPOSAT", "NN"], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Galt an dem Hofe f\u00fcr Genie.", "tokens": ["Galt", "an", "dem", "Ho\u00b7fe", "f\u00fcr", "Ge\u00b7nie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Kein H\u00f6fling durfte sich erdreisten,", "tokens": ["Kein", "H\u00f6f\u00b7ling", "durf\u00b7te", "sich", "er\u00b7dreis\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dem Hahnen was zu thun. Ihn speisten", "tokens": ["Dem", "Hah\u00b7nen", "was", "zu", "thun", ".", "Ihn", "speis\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "PIS", "PTKZU", "VVINF", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Prinzessinnen mit eigner Hand,", "tokens": ["Prin\u00b7zes\u00b7sin\u00b7nen", "mit", "eig\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.10": {"text": "Und schm\u00fcckten seinen Hals mit einem goldnen Band.", "tokens": ["Und", "schm\u00fcck\u00b7ten", "sei\u00b7nen", "Hals", "mit", "ei\u00b7nem", "gold\u00b7nen", "Band", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der Hofmann ehrte ihn, der oft vor Neid erstickte,", "tokens": ["Der", "Hof\u00b7mann", "ehr\u00b7te", "ihn", ",", "der", "oft", "vor", "Neid", "er\u00b7stick\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "PRELS", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wenn sich die Dame niederb\u00fcckte", "tokens": ["Wenn", "sich", "die", "Da\u00b7me", "nie\u00b7der\u00b7b\u00fcck\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Und dann der Hahn den Marmorarm bepickte.", "tokens": ["Und", "dann", "der", "Hahn", "den", "Mar\u00b7mo\u00b7rarm", "be\u00b7pick\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "An einem Morgen flog der Hahn", "tokens": ["An", "ei\u00b7nem", "Mor\u00b7gen", "flog", "der", "Hahn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hinab zum Garten, schlug die Fl\u00fcgel", "tokens": ["Hin\u00b7ab", "zum", "Gar\u00b7ten", ",", "schlug", "die", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und kr\u00e4hete von einem Rasenh\u00fcgel", "tokens": ["Und", "kr\u00e4\u00b7he\u00b7te", "von", "ei\u00b7nem", "Ra\u00b7sen\u00b7h\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den goldnen Morgen an.", "tokens": ["Den", "gold\u00b7nen", "Mor\u00b7gen", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein Adler flog vorbei. Der stolze Haushahn schrie", "tokens": ["Ein", "Ad\u00b7ler", "flog", "vor\u00b7bei", ".", "Der", "stol\u00b7ze", "Haus\u00b7hahn", "schrie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$.", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "In seiner schmetternden Trompetenmelodie:", "tokens": ["In", "sei\u00b7ner", "schmet\u00b7tern\u00b7den", "Trom\u00b7pe\u00b7ten\u00b7me\u00b7lo\u00b7die", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.7": {"text": "Wohin, Herr Bruder! schon so fr\u00fch?", "tokens": ["Wo\u00b7hin", ",", "Herr", "Bru\u00b7der", "!", "schon", "so", "fr\u00fch", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "NN", "NN", "$.", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Qu\u00e4lst du dich noch mit Sonnenflug?", "tokens": ["Qu\u00e4lst", "du", "dich", "noch", "mit", "Son\u00b7nen\u00b7flug", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Zu deinem Gl\u00fcck ist's schon genug", "tokens": ["Zu", "dei\u00b7nem", "Gl\u00fcck", "ist's", "schon", "ge\u00b7nug"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "An einem Hahnenfl\u00fcgelschlage.", "tokens": ["An", "ei\u00b7nem", "Hah\u00b7nen\u00b7fl\u00fc\u00b7gel\u00b7schla\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Komm und genie\u00dfe goldne Tage!", "tokens": ["Komm", "und", "ge\u00b7nie\u00b7\u00dfe", "gold\u00b7ne", "Ta\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Die K\u00f6nige bewundern dich,", "tokens": ["Die", "K\u00f6\u00b7ni\u00b7ge", "be\u00b7wun\u00b7dern", "dich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Dich speisen F\u00fcrstinnen mit hoher Hand, wie mich,", "tokens": ["Dich", "spei\u00b7sen", "F\u00fcrs\u00b7tin\u00b7nen", "mit", "ho\u00b7her", "Hand", ",", "wie", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "ADJA", "NN", "$,", "PWAV", "PPER", "$,"], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.14": {"text": "Was willst du dich mit Donnerkeilen plagen?", "tokens": ["Was", "willst", "du", "dich", "mit", "Don\u00b7ner\u00b7kei\u00b7len", "pla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Kann Zeus sie denn nicht selber tragen?", "tokens": ["Kann", "Zeus", "sie", "denn", "nicht", "sel\u00b7ber", "tra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NE", "PPER", "ADV", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Schweig, sprach mit einem ernsten Blicke", "tokens": ["Schweig", ",", "sprach", "mit", "ei\u00b7nem", "erns\u00b7ten", "Bli\u00b7cke"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Sonnenflieger zu dem Hahn,", "tokens": ["Der", "Son\u00b7nen\u00b7flie\u00b7ger", "zu", "dem", "Hahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich fliege zu der Wolkenbahn;", "tokens": ["Ich", "flie\u00b7ge", "zu", "der", "Wol\u00b7ken\u00b7bahn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du aber bleibst im Staub zur\u00fccke.", "tokens": ["Du", "a\u00b7ber", "bleibst", "im", "Staub", "zu\u00b7r\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Schw\u00e4tzer, leer, wie du, ist's werth,", "tokens": ["Ein", "Schw\u00e4t\u00b7zer", ",", "leer", ",", "wie", "du", ",", "ist's", "werth", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "$,", "PWAV", "PPER", "$,", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df ihn der goldne H\u00f6fling ehrt.", "tokens": ["Da\u00df", "ihn", "der", "gold\u00b7ne", "H\u00f6f\u00b7ling", "ehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ihr Beifall und ein Band ziemt deinem Hahnenwitze;", "tokens": ["Ihr", "Bei\u00b7fall", "und", "ein", "Band", "ziemt", "dei\u00b7nem", "Hah\u00b7nen\u00b7wit\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ich aber fliege zu dem Sitze", "tokens": ["Ich", "a\u00b7ber", "flie\u00b7ge", "zu", "dem", "Sit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Des Donnerers, und trage Blitze,", "tokens": ["Des", "Don\u00b7ne\u00b7rers", ",", "und", "tra\u00b7ge", "Blit\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und der Olympos sieht mich l\u00e4chelnd an;", "tokens": ["Und", "der", "O\u00b7lym\u00b7pos", "sieht", "mich", "l\u00e4\u00b7chelnd", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Selbst Vater Zeus der donnern kann,", "tokens": ["Selbst", "Va\u00b7ter", "Zeus", "der", "don\u00b7nern", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "NE", "ART", "NN", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Giebt mir zum Lohne v\u00e4terliche Blicke", "tokens": ["Giebt", "mir", "zum", "Loh\u00b7ne", "v\u00e4\u00b7ter\u00b7li\u00b7che", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Dann eil' ich stolz zum Felsennest zur\u00fccke,", "tokens": ["Dann", "eil'", "ich", "stolz", "zum", "Fel\u00b7sen\u00b7nest", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Und Teuts erhabner Bardenchor", "tokens": ["Und", "Teuts", "er\u00b7hab\u00b7ner", "Bar\u00b7denc\u00b7hor"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Singt aus dem Eichenhain zu meinem Fels empor.", "tokens": ["Singt", "aus", "dem", "Ei\u00b7chen\u00b7hain", "zu", "mei\u00b7nem", "Fels", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Vor trunkner Wollust schlummr' ich hin,", "tokens": ["Vor", "trunk\u00b7ner", "Wol\u00b7lust", "schlumm\u00b7r'", "ich", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Und f\u00fchl's, da\u00df ich ein Adler bin.", "tokens": ["Und", "f\u00fchl's", ",", "da\u00df", "ich", "ein", "Ad\u00b7ler", "bin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Muse der Geschichte spricht:", "tokens": ["Die", "Mu\u00b7se", "der", "Ge\u00b7schich\u00b7te", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Cassel gilt die ganze Fabel nicht.", "tokens": ["In", "Cas\u00b7sel", "gilt", "die", "gan\u00b7ze", "Fa\u00b7bel", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}