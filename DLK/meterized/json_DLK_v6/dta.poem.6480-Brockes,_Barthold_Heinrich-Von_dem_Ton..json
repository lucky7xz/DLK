{"dta.poem.6480": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Ton.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Versp\u00fcret man ein f\u00fcrchterliches Knallen", "tokens": ["Ver\u00b7sp\u00fc\u00b7ret", "man", "ein", "f\u00fcrch\u00b7ter\u00b7li\u00b7ches", "Knal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von den zerst\u00f6r\u2019nden Ballen,", "tokens": ["Von", "den", "zer\u00b7st\u00f6r'n\u00b7den", "Bal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die fortgerissen sind durch des Salpeters Blitze.", "tokens": ["Die", "fort\u00b7ge\u00b7ris\u00b7sen", "sind", "durch", "des", "Sal\u00b7pe\u00b7ters", "Blit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+---+-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Durch solchen schnellen Druck erth\u00f6nt die Lufft und br\u00fcllt,", "tokens": ["Durch", "sol\u00b7chen", "schnel\u00b7len", "Druck", "er\u00b7th\u00f6nt", "die", "Lufft", "und", "br\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "VVFIN", "ART", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Von Circkeln, die sich drehn und wallen, angef\u00fcllt.", "tokens": ["Von", "Cir\u00b7ckeln", ",", "die", "sich", "drehn", "und", "wal\u00b7len", ",", "an\u00b7ge\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PRF", "VVINF", "KON", "VVINF", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch widersetzt sich offt der Wind den starcken Schallen.", "tokens": ["Doch", "wi\u00b7der\u00b7setzt", "sich", "offt", "der", "Wind", "den", "star\u00b7cken", "Schal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es drehen sich der L\u00fcffte Wellen", "tokens": ["Es", "dre\u00b7hen", "sich", "der", "L\u00fcff\u00b7te", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Nicht gleich an allen Stellen;", "tokens": ["Nicht", "gleich", "an", "al\u00b7len", "Stel\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Es treibt der Nord sie fort nach eigenem Gefallen.", "tokens": ["Es", "treibt", "der", "Nord", "sie", "fort", "nach", "ei\u00b7ge\u00b7nem", "Ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wenn wir dergleichen Schu\u00df mit unsern Augen sehn;", "tokens": ["Wenn", "wir", "derg\u00b7lei\u00b7chen", "Schu\u00df", "mit", "un\u00b7sern", "Au\u00b7gen", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So machen Winde, wenn sie wehn,", "tokens": ["So", "ma\u00b7chen", "Win\u00b7de", ",", "wenn", "sie", "wehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$,", "KOUS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Da\u00df sich an einer Seit das Knallen schnell verliert,", "tokens": ["Da\u00df", "sich", "an", "ei\u00b7ner", "Seit", "das", "Knal\u00b7len", "schnell", "ver\u00b7liert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "APPR", "ART", "NN", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Indem es an der andern Seite,", "tokens": ["In\u00b7dem", "es", "an", "der", "an\u00b7dern", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "In eine grosse Fern und Weite", "tokens": ["In", "ei\u00b7ne", "gros\u00b7se", "Fern", "und", "Wei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Mit ihnen selbst wird fortgef\u00fchrt.", "tokens": ["Mit", "ih\u00b7nen", "selbst", "wird", "fort\u00b7ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}