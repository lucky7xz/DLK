{"textgrid.poem.60007": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Freund, der Du am Kamine,", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freund, der Du am Kamine,", "tokens": ["Freund", ",", "der", "Du", "am", "Ka\u00b7mi\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "APPRART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Zu Dir, mit Chloens Miene,", "tokens": ["Zu", "Dir", ",", "mit", "Chloens", "Mie\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "NE", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Im leichten Hermeline", "tokens": ["Im", "leich\u00b7ten", "Her\u00b7me\u00b7li\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Weisheit kommen siehst;", "tokens": ["Die", "Weis\u00b7heit", "kom\u00b7men", "siehst", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und um Dich her durch Lieder", "tokens": ["Und", "um", "Dich", "her", "durch", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUI", "PRF", "APZR", "APPR", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "F\u00fcr sie des Amors Br\u00fcder", "tokens": ["F\u00fcr", "sie", "des", "A\u00b7mors", "Br\u00fc\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "NE", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Zu kleinen Weisen ziehst!", "tokens": ["Zu", "klei\u00b7nen", "Wei\u00b7sen", "ziehst", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Bestrafe doch die Thoren,", "tokens": ["Be\u00b7stra\u00b7fe", "doch", "die", "Tho\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Die, nicht f\u00fcr sie geboren,", "tokens": ["Die", ",", "nicht", "f\u00fcr", "sie", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PTKNEG", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Die sanfte Huldg\u00f6ttin,", "tokens": ["Die", "sanf\u00b7te", "Huld\u00b7g\u00f6t\u00b7tin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Im schulgelehrten Tone,", "tokens": ["Im", "schul\u00b7ge\u00b7lehr\u00b7ten", "To\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Zur m\u00fcrrischen Matrone,", "tokens": ["Zur", "m\u00fcr\u00b7ri\u00b7schen", "Mat\u00b7ro\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Zur strengen Richterin", "tokens": ["Zur", "stren\u00b7gen", "Rich\u00b7te\u00b7rin"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Unschuld'ger Freude machen;", "tokens": ["Un\u00b7schuld'\u00b7ger", "Freu\u00b7de", "ma\u00b7chen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.15": {"text": "Doch nein! sie nur belachen,", "tokens": ["Doch", "nein", "!", "sie", "nur", "be\u00b7la\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$.", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Und singen wollen wir.", "tokens": ["Und", "sin\u00b7gen", "wol\u00b7len", "wir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O Freund! es sagten mir", "tokens": ["O", "Freund", "!", "es", "sag\u00b7ten", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$.", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die mit den Charitinnen", "tokens": ["Die", "mit", "den", "Cha\u00b7ri\u00b7tin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vertrauten Pierinnen,", "tokens": ["Ver\u00b7trau\u00b7ten", "Pie\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Was wahre Weisheit sey,", "tokens": ["Was", "wah\u00b7re", "Weis\u00b7heit", "sey", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Von trockner Schulgesetze", "tokens": ["Von", "trock\u00b7ner", "Schul\u00b7ge\u00b7set\u00b7ze"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Verworrenem Geschw\u00e4tze,", "tokens": ["Ver\u00b7wor\u00b7re\u00b7nem", "Ge\u00b7schw\u00e4t\u00b7ze", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Von leeren Formeln frey.", "tokens": ["Von", "lee\u00b7ren", "For\u00b7meln", "frey", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Sie gleichet Deiner Leyer,", "tokens": ["Sie", "glei\u00b7chet", "Dei\u00b7ner", "Le\u00b7yer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Ist lauter Harmonie,", "tokens": ["Ist", "lau\u00b7ter", "Har\u00b7mo\u00b7nie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Gl\u00fcht oft von edelm Feuer,", "tokens": ["Gl\u00fcht", "oft", "von", "e\u00b7delm", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Oft aber scherzet sie.", "tokens": ["Oft", "a\u00b7ber", "scher\u00b7zet", "sie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Sie wei\u00df in kleinen Bildern", "tokens": ["Sie", "wei\u00df", "in", "klei\u00b7nen", "Bil\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Uns l\u00e4chelnd das zu schildern,", "tokens": ["Uns", "l\u00e4\u00b7chelnd", "das", "zu", "schil\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PDS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Was hundert Thoren qu\u00e4lt;", "tokens": ["Was", "hun\u00b7dert", "Tho\u00b7ren", "qu\u00e4lt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Sie lehrt uns, wenn wir klagen,", "tokens": ["Sie", "lehrt", "uns", ",", "wenn", "wir", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Da\u00df selbst den tr\u00fcben Tagen", "tokens": ["Da\u00df", "selbst", "den", "tr\u00fc\u00b7ben", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Nicht alle Freude fehlt.", "tokens": ["Nicht", "al\u00b7le", "Freu\u00b7de", "fehlt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Soll ich Dir wieder sagen,", "tokens": ["Soll", "ich", "Dir", "wie\u00b7der", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie auf dem alten Wagen", "tokens": ["Wie", "auf", "dem", "al\u00b7ten", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von St\u00fcrmen hergetragen,", "tokens": ["Von", "St\u00fcr\u00b7men", "her\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie mir den Winter zeigt?", "tokens": ["Sie", "mir", "den", "Win\u00b7ter", "zeigt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "An seinen Stab gebeugt,", "tokens": ["An", "sei\u00b7nen", "Stab", "ge\u00b7beugt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Lappl\u00e4ndisch wild behangen", "tokens": ["Lap\u00b7pl\u00e4n\u00b7disch", "wild", "be\u00b7han\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Mit H\u00e4uten mancher Art,", "tokens": ["Mit", "H\u00e4u\u00b7ten", "man\u00b7cher", "Art", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Steht er; um seine Wangen", "tokens": ["Steht", "er", ";", "um", "sei\u00b7ne", "Wan\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$.", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Starrt ein gefrorner Bart.", "tokens": ["Starrt", "ein", "ge\u00b7fror\u00b7ner", "Bart", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Wie scheu\u00dflich! dennoch h\u00fcpfen", "tokens": ["Wie", "scheu\u00df\u00b7lich", "!", "den\u00b7noch", "h\u00fcp\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADJD", "$.", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Die Scherze ganz vertraut", "tokens": ["Die", "Scher\u00b7ze", "ganz", "ver\u00b7traut"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Um ihn herum, und schl\u00fcpfen", "tokens": ["Um", "ihn", "he\u00b7rum", ",", "und", "schl\u00fcp\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUI", "PPER", "PTKVZ", "$,", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "In eine B\u00e4renhaut.", "tokens": ["In", "ei\u00b7ne", "B\u00e4\u00b7ren\u00b7haut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Da liegen sie, und schielen,", "tokens": ["Da", "lie\u00b7gen", "sie", ",", "und", "schie\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Wenn Hirt und Sch\u00e4ferin", "tokens": ["Wenn", "Hirt", "und", "Sch\u00e4\u00b7fe\u00b7rin"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Am Herde traulich spielen,", "tokens": ["Am", "Her\u00b7de", "trau\u00b7lich", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Muthwillig l\u00e4chelnd hin;", "tokens": ["Mut\u00b7hwil\u00b7lig", "l\u00e4\u00b7chelnd", "hin", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "Gesammelt werden Pf\u00e4nder;", "tokens": ["Ge\u00b7sam\u00b7melt", "wer\u00b7den", "Pf\u00e4n\u00b7der", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Das flatternde Gewand", "tokens": ["Das", "flat\u00b7tern\u00b7de", "Ge\u00b7wand"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.20": {"text": "Der M\u00e4dchen, Haar und B\u00e4nder", "tokens": ["Der", "M\u00e4d\u00b7chen", ",", "Haar", "und", "B\u00e4n\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Verrathen bald die Hand", "tokens": ["Ver\u00b7ra\u00b7then", "bald", "die", "Hand"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.22": {"text": "Der allzu dreisten Knaben,", "tokens": ["Der", "all\u00b7zu", "dreis\u00b7ten", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PTKA", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Die nicht ein jedes Pfand", "tokens": ["Die", "nicht", "ein", "je\u00b7des", "Pfand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PTKNEG", "ART", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.24": {"text": "Um Einen Ku\u00df nur gaben.", "tokens": ["Um", "Ei\u00b7nen", "Ku\u00df", "nur", "ga\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Schon sind die Felder wei\u00df,", "tokens": ["Schon", "sind", "die", "Fel\u00b7der", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und ein Palast von Eis", "tokens": ["Und", "ein", "Pa\u00b7last", "von", "Eis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Beherbergt die Najaden;", "tokens": ["Be\u00b7her\u00b7bergt", "die", "Na\u00b7ja\u00b7den", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie tr\u00f6sten sich, und laden,", "tokens": ["Sie", "tr\u00f6s\u00b7ten", "sich", ",", "und", "la\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "KON", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Um dennoch froh zu seyn,", "tokens": ["Um", "den\u00b7noch", "froh", "zu", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Zu bunten Maskeraden", "tokens": ["Zu", "bun\u00b7ten", "Mas\u00b7ke\u00b7ra\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Den alten Flu\u00dfgott ein.", "tokens": ["Den", "al\u00b7ten", "Flu\u00df\u00b7gott", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Des Faunus Kinder schleichen", "tokens": ["Des", "Fau\u00b7nus", "Kin\u00b7der", "schlei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Vergebens durch den Wald,", "tokens": ["Ver\u00b7ge\u00b7bens", "durch", "den", "Wald", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART",