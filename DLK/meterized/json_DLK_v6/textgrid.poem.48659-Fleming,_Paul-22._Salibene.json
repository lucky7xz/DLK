{"textgrid.poem.48659": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "22. Salibene", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wolte sie nur, wie sie solte,", "tokens": ["Wol\u00b7te", "sie", "nur", ",", "wie", "sie", "sol\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "$,", "PWAV", "PPER", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "und solt' ich nur, wie ich wolte,", "tokens": ["und", "solt'", "ich", "nur", ",", "wie", "ich", "wol\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "$,", "PWAV", "PPER", "VMFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "so w\u00e4r' ich und sie vergn\u00fcgt.", "tokens": ["so", "w\u00e4r'", "ich", "und", "sie", "ver\u00b7gn\u00fcgt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "KON", "PPER", "VVPP", "$."], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.4": {"text": "Ach! wie w\u00e4r' es wol gef\u00fcgt,", "tokens": ["Ach", "!", "wie", "w\u00e4r'", "es", "wol", "ge\u00b7f\u00fcgt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "wenn wir nicht so widerstrebten,", "tokens": ["wenn", "wir", "nicht", "so", "wi\u00b7der\u00b7streb\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "sondern itzt und f\u00fcr und f\u00fcr,", "tokens": ["son\u00b7dern", "itzt", "und", "f\u00fcr", "und", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KON", "APPR", "KON", "APPR", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "ich bei ihr und sie bei mir,", "tokens": ["ich", "bei", "ihr", "und", "sie", "bei", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "KON", "PPER", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "in verglichner Liebe lebten.", "tokens": ["in", "ver\u00b7glich\u00b7ner", "Lie\u00b7be", "leb\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "O wie w\u00fcrden unsre Heerden", "tokens": ["O", "wie", "w\u00fcr\u00b7den", "uns\u00b7re", "Heer\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "so geschwinde feister werden!", "tokens": ["so", "ge\u00b7schwin\u00b7de", "feis\u00b7ter", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Feld und Tal und Berg und Hain", "tokens": ["Feld", "und", "Tal", "und", "Berg", "und", "Hain"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "KON", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "w\u00fcrde mit uns fr\u00f6lich sein.", "tokens": ["w\u00fcr\u00b7de", "mit", "uns", "fr\u00f6\u00b7lich", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alle Nymphen w\u00fcrden lachen", "tokens": ["Al\u00b7le", "Nym\u00b7phen", "w\u00fcr\u00b7den", "la\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "und uns manchen sch\u00f6nen Tanz,", "tokens": ["und", "uns", "man\u00b7chen", "sch\u00f6\u00b7nen", "Tanz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "manchen sch\u00f6nen lieben Kranz", "tokens": ["man\u00b7chen", "sch\u00f6\u00b7nen", "lie\u00b7ben", "Kranz"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "in den bunten Wiesen machen.", "tokens": ["in", "den", "bun\u00b7ten", "Wie\u00b7sen", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich auch w\u00fcrd' auf meiner Pfeifen", "tokens": ["Ich", "auch", "w\u00fcrd'", "auf", "mei\u00b7ner", "Pfei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "ein erfreutes Liedlein greifen,", "tokens": ["ein", "er\u00b7freu\u00b7tes", "Lied\u00b7lein", "grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "wenn ich in der Liebsten Scho\u00df", "tokens": ["wenn", "ich", "in", "der", "Liebs\u00b7ten", "Scho\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "alles Kummers wurde los.", "tokens": ["al\u00b7les", "Kum\u00b7mers", "wur\u00b7de", "los", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn wolt' ich anstat des Klagen,", "tokens": ["Denn", "wolt'", "ich", "an\u00b7stat", "des", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "das mich itzt f\u00fcr seiner Pein", "tokens": ["das", "mich", "itzt", "f\u00fcr", "sei\u00b7ner", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "kaum l\u00e4\u00dft mich und meine sein,", "tokens": ["kaum", "l\u00e4\u00dft", "mich", "und", "mei\u00b7ne", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "PPOSAT", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "nur von lauter Wonne sagen.", "tokens": ["nur", "von", "lau\u00b7ter", "Won\u00b7ne", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "O du sch\u00f6ne ", "tokens": ["O", "du", "sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["NE", "NE", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "schau doch, wie sich Alles liebt", "tokens": ["schau", "doch", ",", "wie", "sich", "Al\u00b7les", "liebt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "$,", "PWAV", "PRF", "PIS", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "und in au\u00dfen Freuden \u00fcbt.", "tokens": ["und", "in", "au\u00b7\u00dfen", "Freu\u00b7den", "\u00fcbt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles wird durch Lust ger\u00fcret.", "tokens": ["Al\u00b7les", "wird", "durch", "Lust", "ge\u00b7r\u00fc\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wir nur g\u00f6nnen unsre Zeit", "tokens": ["Wir", "nur", "g\u00f6n\u00b7nen", "uns\u00b7re", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVINF", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "der versto\u00dfnen Einsamkeit.", "tokens": ["der", "ver\u00b7sto\u00df\u00b7nen", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Denk', ob di\u00df sich auch geb\u00fcret.", "tokens": ["Denk'", ",", "ob", "di\u00df", "sich", "auch", "ge\u00b7b\u00fc\u00b7ret", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "KOUS", "PDS", "PRF", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}