{"dta.poem.10208": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "( Reinigung. )", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Es singt ein Lied von Felix Mendelmaier", "tokens": ["Es", "singt", "ein", "Lied", "von", "Fe\u00b7lix", "Men\u00b7del\u00b7mai\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NE", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der lange Lieutenant mit dem Ordensb\u00e4ndel.", "tokens": ["Der", "lan\u00b7ge", "Lie\u00b7u\u00b7ten\u00b7ant", "mit", "dem", "Or\u00b7dens\u00b7b\u00e4n\u00b7del", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.3": {"text": "Das alte Fr\u00e4ulein br\u00fctet R\u00e4tseleier,", "tokens": ["Das", "al\u00b7te", "Fr\u00e4u\u00b7lein", "br\u00fc\u00b7tet", "R\u00e4t\u00b7sel\u00b7ei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Besorgt den Thee und duftet nach Lavendel.", "tokens": ["Be\u00b7sorgt", "den", "Thee", "und", "duf\u00b7tet", "nach", "La\u00b7ven\u00b7del", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN", "APPR", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "\u201eo Isis\u201c ba\u00dft der Rath, der liebe Schreier.", "tokens": ["\u201e", "o", "I\u00b7sis", "\u201c", "ba\u00dft", "der", "Rath", ",", "der", "lie\u00b7be", "Schrei\u00b7er", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "FM", "$(", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Weh mir, wie langsam schwingt der Abendpendel.", "tokens": ["Weh", "mir", ",", "wie", "lang\u00b7sam", "schwingt", "der", "A\u00b7bend\u00b7pen\u00b7del", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PWAV", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Zu Ende. Gott sei Dank. Ich atme freier,", "tokens": ["Zu", "En\u00b7de", ".", "Gott", "sei", "Dank", ".", "Ich", "at\u00b7me", "frei\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$.", "NN", "VAFIN", "NN", "$.", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und bade mich daheim in Bach und H\u00e4ndel.", "tokens": ["Und", "ba\u00b7de", "mich", "da\u00b7heim", "in", "Bach", "und", "H\u00e4n\u00b7del", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}