{"textgrid.poem.53232": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Steigt, ihr kluge Pregelinnen,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Steigt, ihr kluge Pregelinnen,", "tokens": ["Steigt", ",", "ihr", "klu\u00b7ge", "Pre\u00b7ge\u00b7lin\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Euer Stand euch recht zu Sinnen,", "tokens": ["Eu\u00b7er", "Stand", "euch", "recht", "zu", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df es hertzlich euch gef\u00e4lt,", "tokens": ["Da\u00df", "es", "hertz\u00b7lich", "euch", "ge\u00b7f\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn kein Unfall euch verletzet,", "tokens": ["Wenn", "kein", "Un\u00b7fall", "euch", "ver\u00b7let\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "So erhebt den teuren Heldt,", "tokens": ["So", "er\u00b7hebt", "den", "teu\u00b7ren", "Heldt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Der in diese Ruh' euch setzet.", "tokens": ["Der", "in", "die\u00b7se", "Ruh'", "euch", "set\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PDAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ihm solt ihr den Geist erh\u00f6hen,", "tokens": ["Ihm", "solt", "ihr", "den", "Geist", "er\u00b7h\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sein Geburts-Fest heut begehen,", "tokens": ["Sein", "Ge\u00b7burts\u00b7Fest", "heut", "be\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Heut hat sechs und zwantzig mahl", "tokens": ["Heut", "hat", "sechs", "und", "zwant\u00b7zig", "mahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "CARD", "KON", "CARD", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich das Jahr herumb begeben,", "tokens": ["Sich", "das", "Jahr", "he\u00b7rumb", "be\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "APZR", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df der sch\u00f6ne Himmels Saal", "tokens": ["Da\u00df", "der", "sch\u00f6\u00b7ne", "Him\u00b7mels", "Saal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihn geliehen diesem Leben.", "tokens": ["Ihn", "ge\u00b7lie\u00b7hen", "die\u00b7sem", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Damals ist in diesen Landen", "tokens": ["Da\u00b7mals", "ist", "in", "die\u00b7sen", "Lan\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Euer Auffenthalt entstanden,", "tokens": ["Eu\u00b7er", "Auf\u00b7fent\u00b7halt", "ent\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Damals ist der Himmel neu", "tokens": ["Da\u00b7mals", "ist", "der", "Him\u00b7mel", "neu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Uns zu gut durch ihn gebohren,", "tokens": ["Uns", "zu", "gut", "durch", "ihn", "ge\u00b7boh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKA", "ADJD", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Damals haben Lieb' und Treu", "tokens": ["Da\u00b7mals", "ha\u00b7ben", "Lieb'", "und", "Treu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sich mit Preussen hart verschworen.", "tokens": ["Sich", "mit", "Preus\u00b7sen", "hart", "ver\u00b7schwo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "F\u00fchrt, ihr Pregelinnen, T\u00e4ntze,", "tokens": ["F\u00fchrt", ",", "ihr", "Pre\u00b7ge\u00b7lin\u00b7nen", ",", "T\u00e4nt\u00b7ze", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Traget gr\u00fcne Lorbeer Kr\u00e4ntze,", "tokens": ["Tra\u00b7get", "gr\u00fc\u00b7ne", "Lor\u00b7beer", "Kr\u00e4nt\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lasset eurer Seiten Thon", "tokens": ["Las\u00b7set", "eu\u00b7rer", "Sei\u00b7ten", "Thon"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00dcber alle Berg' hin dringen,", "tokens": ["\u00dc\u00b7ber", "al\u00b7le", "Ber\u00b7g'", "hin", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Da\u00df gantz K\u00f6nigsberg davon,", "tokens": ["Da\u00df", "gantz", "K\u00f6\u00b7nigs\u00b7berg", "da\u00b7von", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "PAV", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "Ja die Wolcken selbst erklingen.", "tokens": ["Ja", "die", "Wol\u00b7cken", "selbst", "er\u00b7klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Last nicht ab mit euren Weisen", "tokens": ["Last", "nicht", "ab", "mit", "eu\u00b7ren", "Wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKNEG", "PTKVZ", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hiervor unserm Gott zu preisen,", "tokens": ["Hier\u00b7vor", "un\u00b7serm", "Gott", "zu", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und zu bitten, da\u00df Er hie", "tokens": ["Und", "zu", "bit\u00b7ten", ",", "da\u00df", "Er", "hie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PTKZU", "VVINF", "$,", "KOUS", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihn erhalt in sp\u00e4ten Jahren,", "tokens": ["Ihn", "er\u00b7halt", "in", "sp\u00e4\u00b7ten", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Weh' uns Armen! solt Er fr\u00fch", "tokens": ["Weh'", "uns", "Ar\u00b7men", "!", "solt", "Er", "fr\u00fch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "$.", "VMFIN", "PPER", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Uns in sein Gestirn entfahren.", "tokens": ["Uns", "in", "sein", "Ge\u00b7stirn", "ent\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}