{"textgrid.poem.54926": {"metadata": {"author": {"name": "Scheerbart, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Eremit ist dick und gro\u00df;", "genre": "verse", "period": "N.A.", "pub_year": 1889, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Eremit ist dick und gro\u00df;", "tokens": ["Der", "E\u00b7re\u00b7mit", "ist", "dick", "und", "gro\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er ha\u00dft die Nebenmenschen blo\u00df.", "tokens": ["Er", "ha\u00dft", "die", "Ne\u00b7ben\u00b7men\u00b7schen", "blo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er liebt nur seine Klause", "tokens": ["Er", "liebt", "nur", "sei\u00b7ne", "Klau\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und bleibt daher zu Hause.", "tokens": ["Und", "bleibt", "da\u00b7her", "zu", "Hau\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die ganze Welt ist ihm Pomade.", "tokens": ["Die", "gan\u00b7ze", "Welt", "ist", "ihm", "Po\u00b7ma\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Nebenmenschen sagen: schade!", "tokens": ["Die", "Ne\u00b7ben\u00b7men\u00b7schen", "sa\u00b7gen", ":", "scha\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$.", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Das aber r\u00fchrt den Teufel nicht.", "tokens": ["Das", "a\u00b7ber", "r\u00fchrt", "den", "Teu\u00b7fel", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Hat er nur stets sein Leibgericht,", "tokens": ["Hat", "er", "nur", "stets", "sein", "Leib\u00b7ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "So ist ihm alles piepe \u2013", "tokens": ["So", "ist", "ihm", "al\u00b7les", "pie\u00b7pe", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Der Ha\u00df und auch die Liepe.", "tokens": ["Der", "Ha\u00df", "und", "auch", "die", "Lie\u00b7pe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}