{"dta.poem.20936": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Heloisse an Abelarden.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Auf einen Brief von Blutt geh\u00f6rt ein Brief von", "tokens": ["Auf", "ei\u00b7nen", "Brief", "von", "Blutt", "ge\u00b7h\u00f6rt", "ein", "Brief", "von"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ADJD", "VVFIN", "ART", "NN", "APPR"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Thr\u00e4nen/", "tokens": ["Thr\u00e4\u00b7nen", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Ich f\u00fchle wie dein Schnitt mich auch zugleiche sticht", "tokens": ["Ich", "f\u00fch\u00b7le", "wie", "dein", "Schnitt", "mich", "auch", "zu\u00b7glei\u00b7che", "sticht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "PPOSAT", "NN", "PPER", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ach da\u00df der Himmel mich den Jam\u0303er l\u00e4st erwehnen/", "tokens": ["Ach", "da\u00df", "der", "Him\u00b7mel", "mich", "den", "Jam\u0303er", "l\u00e4st", "er\u00b7weh\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "ART", "NN", "PPER", "ART", "NN", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Und mir nicht auch dabey das matte Hertze bricht.", "tokens": ["Und", "mir", "nicht", "auch", "da\u00b7bey", "das", "mat\u00b7te", "Hert\u00b7ze", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKNEG", "ADV", "PAV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Kein Zug der Eitelkeit/ kein Dunst beflam\u0303ter L\u00fcste/", "tokens": ["Kein", "Zug", "der", "Ei\u00b7tel\u00b7keit", "/", "kein", "Dunst", "be\u00b7flam\u0303ter", "L\u00fcs\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$(", "PIAT", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Macht da\u00df ich deine Noth entzuckt beweinen mu\u00df/", "tokens": ["Macht", "da\u00df", "ich", "dei\u00b7ne", "Noth", "ent\u00b7zuckt", "be\u00b7wei\u00b7nen", "mu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PPER", "PPOSAT", "NN", "VVPP", "VVINF", "VMFIN", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.8": {"text": "Die Geister f\u00fchren mich in eine d\u00fcrre W\u00fcste/", "tokens": ["Die", "Geis\u00b7ter", "f\u00fch\u00b7ren", "mich", "in", "ei\u00b7ne", "d\u00fcr\u00b7re", "W\u00fcs\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Gedenck ich k\u00fcnfftig mehr an einen geilen Ku\u00df.", "tokens": ["Ge\u00b7denck", "ich", "k\u00fcnff\u00b7tig", "mehr", "an", "ei\u00b7nen", "gei\u00b7len", "Ku\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ich scheue mich zwar nicht in Schwachheit zu beken\u0303e\u0303", "tokens": ["Ich", "scheu\u00b7e", "mich", "zwar", "nicht", "in", "Schwach\u00b7heit", "zu", "be\u00b7ke\u00f1\u1ebd"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "APPR", "NN", "APPR", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Da\u00df deine k\u00fchne Faust mich in die Gluth gef\u00fchrt;", "tokens": ["Da\u00df", "dei\u00b7ne", "k\u00fch\u00b7ne", "Faust", "mich", "in", "die", "Gluth", "ge\u00b7f\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "PRF", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wie solte nicht ein Weib in ihren Geist entbrennen/", "tokens": ["Wie", "sol\u00b7te", "nicht", "ein", "Weib", "in", "ih\u00b7ren", "Geist", "ent\u00b7bren\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PTKNEG", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wann ihr ein Abelard so sch\u00f6ne Funcken r\u00fchrt?", "tokens": ["Wann", "ihr", "ein", "A\u00b7be\u00b7lard", "so", "sch\u00f6\u00b7ne", "Fun\u00b7cken", "r\u00fchrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Das Wort/ damit dein Mund mein Ohre hat be-", "tokens": ["Das", "Wort", "/", "da\u00b7mit", "dein", "Mund", "mein", "Oh\u00b7re", "hat", "be"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "KOUS", "PPOSAT", "NN", "PPOSAT", "NN", "VAFIN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "stritten/", "tokens": ["strit\u00b7ten", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.16": {"text": "Bezwang mir auch den Geist durch s\u00fcsse Zauberey.", "tokens": ["Be\u00b7zwang", "mir", "auch", "den", "Geist", "durch", "s\u00fcs\u00b7se", "Zau\u00b7be\u00b7rey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ich bin mein Edler Freund durch deine Hand geglit-", "tokens": ["Ich", "bin", "mein", "Ed\u00b7ler", "Freund", "durch", "dei\u00b7ne", "Hand", "ge\u00b7glit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und lebte sonder dich von allem Falle frey. ", "tokens": ["Und", "leb\u00b7te", "son\u00b7der", "dich", "von", "al\u00b7lem", "Fal\u00b7le", "frey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "PPER", "APPR", "PIS", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ich bin durch dich allein au\u00df dem Gewichte kom\u0303en/", "tokens": ["Ich", "bin", "durch", "dich", "al\u00b7lein", "au\u00df", "dem", "Ge\u00b7wich\u00b7te", "kom\u0303en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "ADV", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Doch wer durch Helde\u0303 f\u00e4lt/ der f\u00e4lt nicht ohne Ruhm", "tokens": ["Doch", "wer", "durch", "Held\u1ebd", "f\u00e4lt", "/", "der", "f\u00e4lt", "nicht", "oh\u00b7ne", "Ruhm"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "APPR", "NN", "VVFIN", "$(", "ART", "VVFIN", "PTKNEG", "APPR", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "Da\u00df du mich hast bekriegt/ und mir das Heft genom-", "tokens": ["Da\u00df", "du", "mich", "hast", "be\u00b7kriegt", "/", "und", "mir", "das", "Heft", "ge\u00b7nom"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "VAFIN", "VVPP", "$(", "KON", "PPER", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "men/", "tokens": ["men", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "-", "measure": "single.down"}, "line.23": {"text": "Das bleibt der beste Schatz von meinem Eigenthum.", "tokens": ["Das", "bleibt", "der", "bes\u00b7te", "Schatz", "von", "mei\u00b7nem", "Ei\u00b7gen\u00b7thum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Mein Einfalt sch\u00e4rftest du durch viel gelehrte K\u00fcsse", "tokens": ["Mein", "Ein\u00b7falt", "sch\u00e4rf\u00b7test", "du", "durch", "viel", "ge\u00b7lehr\u00b7te", "K\u00fcs\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Die Geilheit legtest du in bunde Schalen ein/", "tokens": ["Die", "Geil\u00b7heit", "leg\u00b7test", "du", "in", "bun\u00b7de", "Scha\u00b7len", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Es machte mir dein Ku\u00df Gall v. auch Wermut s\u00fcsse", "tokens": ["Es", "mach\u00b7te", "mir", "dein", "Ku\u00df", "Gall", "v.", "auch", "Wer\u00b7mut", "s\u00fcs\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "NE", "APPRART", "ADV", "NN", "VVFIN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "Du liest Vertrauligkeit der Keuschheit Wiege seyn.", "tokens": ["Du", "liest", "Ver\u00b7trau\u00b7lig\u00b7keit", "der", "Keuschheit", "Wie\u00b7ge", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ART", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.28": {"text": "Es war die Buhlerey mit Wei\u00dfheit \u00fcberzogen/", "tokens": ["Es", "war", "die", "Buh\u00b7le\u00b7rey", "mit", "Wei\u00df\u00b7heit", "\u00fc\u00b7berz\u00b7o\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ja unsre Geilheit selbst mit Keuschheit angethan", "tokens": ["Ja", "uns\u00b7re", "Geil\u00b7heit", "selbst", "mit", "Keuschheit", "an\u00b7ge\u00b7than"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "PPOSAT", "NN", "ADV", "APPR", "NN", "VVPP"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "Mit solcher Liebligkeit ward unser Lust gepflogen", "tokens": ["Mit", "sol\u00b7cher", "Lieb\u00b7lig\u00b7keit", "ward", "un\u00b7ser", "Lust", "ge\u00b7pflo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Da\u00df ich sie auch itzund nicht g\u00e4ntzlich tadeln kar.", "tokens": ["Da\u00df", "ich", "sie", "auch", "it\u00b7zund", "nicht", "g\u00e4ntz\u00b7lich", "ta\u00b7deln", "kar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADV", "PTKNEG", "ADJD", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Es gieng die Schl\u00fcpfrigkeit in einem reinen Kleide/", "tokens": ["Es", "gieng", "die", "Schl\u00fcpf\u00b7rig\u00b7keit", "in", "ei\u00b7nem", "rei\u00b7nen", "Klei\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ich ward von deiner Brunst geziehret/ nicht befleckt/", "tokens": ["Ich", "ward", "von", "dei\u00b7ner", "Brunst", "ge\u00b7zieh\u00b7ret", "/", "nicht", "be\u00b7fleckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$(", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Es war mein Purpur Rock nicht ohne weisse Seide.", "tokens": ["Es", "war", "mein", "Pur\u00b7pur", "Rock", "nicht", "oh\u00b7ne", "weis\u00b7se", "Sei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "NN", "PTKNEG", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Wer liebt die Speise nicht so nach der Tugend", "tokens": ["Wer", "liebt", "die", "Spei\u00b7se", "nicht", "so", "nach", "der", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "NN", "PTKNEG", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "schmeckt.", "tokens": ["schmeckt", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.37": {"text": "Als Monde wolt\u2019 ich nur durch dich o Son\u0303e scheinen.", "tokens": ["Als", "Mon\u00b7de", "wolt'", "ich", "nur", "durch", "dich", "o", "So\u00f1e", "schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VMFIN", "PPER", "ADV", "APPR", "PPER", "FM", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-++-", "measure": "unknown.measure.hexa"}, "line.38": {"text": "Mich schreckt auf deiner Scho\u00df kein Bild betr\u00fcbter", "tokens": ["Mich", "schreckt", "auf", "dei\u00b7ner", "Scho\u00df", "kein", "Bild", "be\u00b7tr\u00fcb\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "PIAT", "NN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Nacht (nen/", "tokens": ["Nacht", "(", "nen", "/"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$(", "VVINF", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.40": {"text": "Ich dacht\u2019 auf dieser Welt forthin nicht mehr zu wei-", "tokens": ["Ich", "dacht'", "auf", "die\u00b7ser", "Welt", "for\u00b7thin", "nicht", "mehr", "zu", "wei"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN", "ADV", "PTKNEG", "ADV", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Ach da\u00df sich unser Lust zur Unlust Mutter macht.", "tokens": ["Ach", "da\u00df", "sich", "un\u00b7ser", "Lust", "zur", "Un\u00b7lust", "Mut\u00b7ter", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "PRF", "PPOSAT", "NN", "APPRART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Du hattest mir so viel von Tugend f\u00fcrgestellet/", "tokens": ["Du", "hat\u00b7test", "mir", "so", "viel", "von", "Tu\u00b7gend", "f\u00fcr\u00b7ge\u00b7stel\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Da\u00df sich die Schelmerey dadurch nicht blicken lie\u00df.", "tokens": ["Da\u00df", "sich", "die", "Schel\u00b7me\u00b7rey", "da\u00b7durch", "nicht", "bli\u00b7cken", "lie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "PAV", "PTKNEG", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Mit solcher Liebligkeit ward ich durch dich gef\u00e4llet", "tokens": ["Mit", "sol\u00b7cher", "Lieb\u00b7lig\u00b7keit", "ward", "ich", "durch", "dich", "ge\u00b7f\u00e4l\u00b7let"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "PPER", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"te