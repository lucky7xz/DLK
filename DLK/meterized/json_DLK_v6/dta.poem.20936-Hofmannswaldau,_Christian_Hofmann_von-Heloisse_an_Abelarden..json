penta"}, "line.110": {"text": "Pfand.", "tokens": ["Pfand", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.111": {"text": "Vor deinen Schaden kan ich itzt kein Pflaster sende\u0303/", "tokens": ["Vor", "dei\u00b7nen", "Scha\u00b7den", "kan", "ich", "itzt", "kein", "Pflas\u00b7ter", "sen\u00b7d\u1ebd", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PPER", "ADV", "PIAT", "NN", "NE", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.112": {"text": "Wenn meine Wehmut man nicht deine Salbe heist.", "tokens": ["Wenn", "mei\u00b7ne", "Weh\u00b7mut", "man", "nicht", "dei\u00b7ne", "Sal\u00b7be", "heist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PIS", "PTKNEG", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.113": {"text": "Hiermit empfehl\u2019 ich dich des Him\u0303els treuen H\u00e4nde\u0303/", "tokens": ["Hier\u00b7mit", "emp\u00b7fehl'", "ich", "dich", "des", "Him\u0303els", "treu\u00b7en", "H\u00e4n\u00b7d\u1ebd", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+--+---", "measure": "iambic.tetra.relaxed"}, "line.114": {"text": "Der heile deinen Leib/ und st\u00e4rcke meinen Geist.", "tokens": ["Der", "hei\u00b7le", "dei\u00b7nen", "Leib", "/", "und", "st\u00e4r\u00b7cke", "mei\u00b7nen", "Geist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PPOSAT", "NN", "$(", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}