: "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "So nur zu leutern wei\u00df/ und nichts an uns befleckt.", "tokens": ["So", "nur", "zu", "leu\u00b7tern", "wei\u00df", "/", "und", "nichts", "an", "uns", "be\u00b7fleckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKZU", "VVINF", "VVFIN", "$(", "KON", "PIS", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Ich schlug in solcher Lust Geist und auch Auge nieder/", "tokens": ["Ich", "schlug", "in", "sol\u00b7cher", "Lust", "Geist", "und", "auch", "Au\u00b7ge", "nie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "NN", "KON", "ADV", "NN", "PTKVZ", "$("], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.52": {"text": "Wer Adlern gleiche sieht/ wird durch die Liebe blind/", "tokens": ["Wer", "Ad\u00b7lern", "glei\u00b7che", "sieht", "/", "wird", "durch", "die", "Lie\u00b7be", "blind", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "ADJA", "VVFIN", "$(", "VAFIN", "APPR", "ART", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Was ich aldar empfand/ bringt mir kein Monath", "tokens": ["Was", "ich", "al\u00b7dar", "emp\u00b7fand", "/", "bringt", "mir", "kein", "Mo\u00b7nath"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "$(", "VVFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.54": {"text": "wieder/", "tokens": ["wie\u00b7der", "/"], "token_info": ["word", "punct"], "pos": ["ADV", "$("], "meter": "+-", "measure": "trochaic.sing