": "iambic.penta"}, "line.70": {"text": "Man mu\u00df die Liebe nicht mit gleicher Ele messen/", "tokens": ["Man", "mu\u00df", "die", "Lie\u00b7be", "nicht", "mit", "glei\u00b7cher", "E\u00b7le", "mes\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "NN", "PTKNEG", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.71": {"text": "Gemeine Buhlerey sucht nichts als Fleisch un\u0303 Blut/", "tokens": ["Ge\u00b7mei\u00b7ne", "Buh\u00b7le\u00b7rey", "sucht", "nichts", "als", "Fleisch", "u\u00f1", "Blut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PIS", "KOKOM", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.72": {"text": "Doch der ein edler Geist das Hertze hat besessen/", "tokens": ["Doch", "der", "ein", "ed\u00b7ler", "Geist", "das", "Hert\u00b7ze", "hat", "be\u00b7ses\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "ADJA", "NN", "PDS", "VVFIN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.73": {"text": "Die l\u00e4st das Schlacken Werck/ und sucht ein h\u00f6her", "tokens": ["Die", "l\u00e4st", "das", "Schla\u00b7cken", "Werck", "/", "und", "sucht", "ein", "h\u00f6\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "NN", "$(", "KON", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.74": {"text": "Gut. (ret", "tokens": ["Gut", ".", "(", "ret"], "token_info": ["word", "punct", "punct", "word"], "pos": ["ADJD", "$.", "$(", "VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.75": {"text": "Hat mich dein Zucker M