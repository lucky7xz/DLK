{"textgrid.poem.24119": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Neujahrs-Besuch", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kleine H\u00e4nde, kleiner Mund,", "tokens": ["Klei\u00b7ne", "H\u00e4n\u00b7de", ",", "klei\u00b7ner", "Mund", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gro\u00dfe Augen blau und rund,", "tokens": ["Gro\u00b7\u00dfe", "Au\u00b7gen", "blau", "und", "rund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weiches, langes Ringelhaar,", "tokens": ["Wei\u00b7ches", ",", "lan\u00b7ges", "Rin\u00b7gel\u00b7haar", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Leise Stimme glockenklar \u2013:", "tokens": ["Lei\u00b7se", "Stim\u00b7me", "glo\u00b7cken\u00b7klar", "\u2013", ":"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "ADJD", "$(", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Also kam das neue Jahr", "tokens": ["Al\u00b7so", "kam", "das", "neu\u00b7e", "Jahr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Heute zu mir in mein Haus.", "tokens": ["Heu\u00b7te", "zu", "mir", "in", "mein", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Lieblich sahs und lustig aus.", "tokens": ["Lieb\u00b7lich", "sahs", "und", "lus\u00b7tig", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "KON", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Da\u00df es bleibe, wie es ist,", "tokens": ["Da\u00df", "es", "blei\u00b7be", ",", "wie", "es", "ist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00fcnsche ich als Mensch und Christ.", "tokens": ["W\u00fcn\u00b7sche", "ich", "als", "Mensch", "und", "Christ", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOUS", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00f6g es nie mit Wutgeberden", "tokens": ["M\u00f6g", "es", "nie", "mit", "Wut\u00b7ge\u00b7ber\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eine schrille Trulle werden,", "tokens": ["Ei\u00b7ne", "schril\u00b7le", "Trul\u00b7le", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die mit Zank und Zorn regiert", "tokens": ["Die", "mit", "Zank", "und", "Zorn", "re\u00b7giert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "KON", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und das Sch\u00f6ne molestiert.", "tokens": ["Und", "das", "Sch\u00f6\u00b7ne", "mo\u00b7les\u00b7tiert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Leise bleib es, klar und lind,", "tokens": ["Lei\u00b7se", "bleib", "es", ",", "klar", "und", "lind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Guter Gast und gutes Kind,", "tokens": ["Gu\u00b7ter", "Gast", "und", "gu\u00b7tes", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bring mir bald in gr\u00fcner Sch\u00fcssel", "tokens": ["Bring", "mir", "bald", "in", "gr\u00fc\u00b7ner", "Sch\u00fcs\u00b7sel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hohe, gelbe Himmelsschl\u00fcssel,", "tokens": ["Ho\u00b7he", ",", "gel\u00b7be", "Him\u00b7mels\u00b7schl\u00fcs\u00b7sel", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Rosen, wenn der Sommer gl\u00fcht,", "tokens": ["Ro\u00b7sen", ",", "wenn", "der", "Som\u00b7mer", "gl\u00fcht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wein, wenn bla\u00df die Aster bl\u00fcht,", "tokens": ["Wein", ",", "wenn", "bla\u00df", "die", "As\u00b7ter", "bl\u00fcht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und im Winter z\u00fcnd es dann", "tokens": ["Und", "im", "Win\u00b7ter", "z\u00fcnd", "es", "dann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Mir die Weihnachtskerzen an.", "tokens": ["Mir", "die", "Weih\u00b7nachts\u00b7ker\u00b7zen", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wird es dann von hinnen m\u00fcssen,", "tokens": ["Wird", "es", "dann", "von", "hin\u00b7nen", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "VVINF", "VMINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Werd ich ihm die H\u00e4nde k\u00fcssen,", "tokens": ["Werd", "ich", "ihm", "die", "H\u00e4n\u00b7de", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die mich so mit Gl\u00fcck beg\u00fctet", "tokens": ["Die", "mich", "so", "mit", "Gl\u00fcck", "be\u00b7g\u00fc\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "APPR", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und in Sch\u00f6nheit eingeh\u00fctet.", "tokens": ["Und", "in", "Sch\u00f6n\u00b7heit", "ein\u00b7ge\u00b7h\u00fc\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Willst du, Jahr? Die Kleine lacht.", "tokens": ["Willst", "du", ",", "Jahr", "?", "Die", "Klei\u00b7ne", "lacht", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "NN", "$.", "ART", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat mir einen Knix gemacht,", "tokens": ["Hat", "mir", "ei\u00b7nen", "Knix", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat noch einmal still genickt,", "tokens": ["Hat", "noch", "ein\u00b7mal", "still", "ge\u00b7nickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Eine Ku\u00dfhand mir geschickt,", "tokens": ["Ei\u00b7ne", "Ku\u00df\u00b7hand", "mir", "ge\u00b7schickt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und dann ist sie fortgesprungen.", "tokens": ["Und", "dann", "ist", "sie", "fort\u00b7ge\u00b7sprun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Springend hat sie dies gesungen:", "tokens": ["Sprin\u00b7gend", "hat", "sie", "dies", "ge\u00b7sun\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "PDS", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Zu Fl\u00f6ten und Geigen", "tokens": ["Zu", "Fl\u00f6\u00b7ten", "und", "Gei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Hintanz ich im Reigen,", "tokens": ["Hin\u00b7tanz", "ich", "im", "Rei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Habe Blumen im Haar.", "tokens": ["Ha\u00b7be", "Blu\u00b7men", "im", "Haar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPRART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Oh la\u00dft euch bewegen,", "tokens": ["Oh", "la\u00dft", "euch", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Ihr Tr\u00fcben und Tr\u00e4gen,", "tokens": ["Ihr", "Tr\u00fc\u00b7ben", "und", "Tr\u00e4\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Im Tanze ist Segen,", "tokens": ["Im", "Tan\u00b7ze", "ist", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Die Freude macht klar.", "tokens": ["Die", "Freu\u00b7de", "macht", "klar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.8": {"line.1": {"text": "Auf, wagt es, zu springen!", "tokens": ["Auf", ",", "wagt", "es", ",", "zu", "sprin\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "$,", "VVFIN", "PPER", "$,", "PTKZU", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Es mu\u00df euch gelingen,", "tokens": ["Es", "mu\u00df", "euch", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Was fr\u00f6hlich ihr schafft.", "tokens": ["Was", "fr\u00f6h\u00b7lich", "ihr", "schafft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Das gr\u00e4mliche Hocken", "tokens": ["Das", "gr\u00e4m\u00b7li\u00b7che", "Ho\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Bringt alles ins Stocken.", "tokens": ["Bringt", "al\u00b7les", "ins", "Sto\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPRART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Frei wehn meine Locken,", "tokens": ["Frei", "wehn", "mei\u00b7ne", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Die Freude macht Kraft.", "tokens": ["Die", "Freu\u00b7de", "macht", "Kraft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}