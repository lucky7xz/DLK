{"dta.poem.6400": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Geschmack.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Des Feuers heisse Gluht,", "tokens": ["Des", "Feu\u00b7ers", "heis\u00b7se", "Gluht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die, dem Geschmack zum besten, nimmer ruht,", "tokens": ["Die", ",", "dem", "Ge\u00b7schmack", "zum", "bes\u00b7ten", ",", "nim\u00b7mer", "ruht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "$,", "ART", "NN", "APPRART", "ADJA", "$,", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Erregt und w\u00fcrckt, auf eine seltne Weise,", "tokens": ["Er\u00b7regt", "und", "w\u00fcrckt", ",", "auf", "ei\u00b7ne", "selt\u00b7ne", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVFIN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bewegungen, in unsrer Speise,", "tokens": ["Be\u00b7we\u00b7gun\u00b7gen", ",", "in", "uns\u00b7rer", "Spei\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-++--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Sie kocht dieselbigen, eh\u2019, als der Magen, gar,", "tokens": ["Sie", "kocht", "die\u00b7sel\u00b7bi\u00b7gen", ",", "eh'", ",", "als", "der", "Ma\u00b7gen", ",", "gar", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "$,", "KOUS", "$,", "KOUS", "ART", "NN", "$,", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und, da sie von so mancherley-", "tokens": ["Und", ",", "da", "sie", "von", "so", "man\u00b7cher\u00b7ley"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "PPER", "APPR", "ADV", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und vielen C\u00f6rpern trennt die Fibern, ihren Safft", "tokens": ["Und", "vie\u00b7len", "C\u00f6r\u00b7pern", "trennt", "die", "Fi\u00b7bern", ",", "ih\u00b7ren", "Safft"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ART", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Erpresst, das Fleisch erweicht, zeugt sie zur Gasterey,", "tokens": ["Er\u00b7presst", ",", "das", "Fleisch", "er\u00b7weicht", ",", "zeugt", "sie", "zur", "Gas\u00b7te\u00b7rey", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "VVPP", "$,", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "So mancherley Geschmack, der vor darinn nicht war.", "tokens": ["So", "man\u00b7cher\u00b7ley", "Ge\u00b7schmack", ",", "der", "vor", "da\u00b7rinn", "nicht", "war", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,", "PRELS", "APPR", "PAV", "PTKNEG", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}