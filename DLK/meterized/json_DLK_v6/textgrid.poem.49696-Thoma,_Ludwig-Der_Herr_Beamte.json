{"textgrid.poem.49696": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der Herr Beamte", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was zieht nur der Herr Glasermeister", "tokens": ["Was", "zieht", "nur", "der", "Herr", "Gla\u00b7ser\u00b7meis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Hut devoten Angesichts?", "tokens": ["Den", "Hut", "de\u00b7vo\u00b7ten", "An\u00b7ge\u00b7sichts", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dort geht, und Joseph Meier hei\u00dft er,", "tokens": ["Dort", "geht", ",", "und", "Jo\u00b7se\u00b7ph", "Mei\u00b7er", "hei\u00dft", "er", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KON", "NE", "NE", "VVFIN", "PPER", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Vorstand eines Amtsgerichts.", "tokens": ["Der", "Vor\u00b7stand", "ei\u00b7nes", "Amts\u00b7ge\u00b7richts", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Apotheker bleibet stehen;", "tokens": ["Der", "A\u00b7pot\u00b7he\u00b7ker", "blei\u00b7bet", "ste\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Obwohl es stark geregnet hat,", "tokens": ["Ob\u00b7wohl", "es", "stark", "ge\u00b7reg\u00b7net", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "L\u00e4\u00dft er ihn doch vor\u00fcber gehen,", "tokens": ["L\u00e4\u00dft", "er", "ihn", "doch", "vor\u00b7\u00fc\u00b7ber", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Indem er in die Gosse trat.", "tokens": ["In\u00b7dem", "er", "in", "die", "Gos\u00b7se", "trat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Ehrfurcht dieser insgesamten", "tokens": ["Die", "Ehr\u00b7furcht", "die\u00b7ser", "ins\u00b7ge\u00b7sam\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PDAT", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Honoratioren in dem Ort", "tokens": ["Ho\u00b7no\u00b7ra\u00b7ti\u00b7o\u00b7ren", "in", "dem", "Ort"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Erfreuet unsern Staatsbeamten", "tokens": ["Er\u00b7freu\u00b7et", "un\u00b7sern", "Staats\u00b7be\u00b7am\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und lebt in seinem Herzen fort.", "tokens": ["Und", "lebt", "in", "sei\u00b7nem", "Her\u00b7zen", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "So ist er ideell entsch\u00e4digt", "tokens": ["So", "ist", "er", "i\u00b7deell", "ent\u00b7sch\u00e4\u00b7digt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VVPP"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "F\u00fcr das, was er entbehren mu\u00df,", "tokens": ["F\u00fcr", "das", ",", "was", "er", "ent\u00b7beh\u00b7ren", "mu\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PWS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und was er eigentlich ben\u00f6tigt,", "tokens": ["Und", "was", "er", "ei\u00b7gent\u00b7lich", "be\u00b7n\u00f6\u00b7tigt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und was ihm mangelt zum Genu\u00df.", "tokens": ["Und", "was", "ihm", "man\u00b7gelt", "zum", "Ge\u00b7nu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}