{"textgrid.poem.26583": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die scheue Kummerfalte,", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die scheue Kummerfalte,", "tokens": ["Die", "scheu\u00b7e", "Kum\u00b7mer\u00b7fal\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im Antlitz dir zu schauen,", "tokens": ["Im", "Ant\u00b7litz", "dir", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie mahnt mich an das alte,", "tokens": ["Sie", "mahnt", "mich", "an", "das", "al\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das herbe Loos der Frauen.", "tokens": ["Das", "her\u00b7be", "Loos", "der", "Frau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nach kurzen Jugendtagen", "tokens": ["Nach", "kur\u00b7zen", "Ju\u00b7gend\u00b7ta\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verschuldetes Entbehren \u2013", "tokens": ["Ver\u00b7schul\u00b7de\u00b7tes", "Ent\u00b7beh\u00b7ren", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Einen durch Versagen,", "tokens": ["Die", "Ei\u00b7nen", "durch", "Ver\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die And'ren durch Gew\u00e4hren.", "tokens": ["Die", "An\u00b7d'\u00b7ren", "durch", "Ge\u00b7w\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch wehe, wenn da Beides", "tokens": ["Doch", "we\u00b7he", ",", "wenn", "da", "Bei\u00b7des"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "KOUS", "ADV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Eins zusammenflie\u00dft,", "tokens": ["In", "Eins", "zu\u00b7sam\u00b7men\u00b7flie\u00dft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und so ein Meer des Leides", "tokens": ["Und", "so", "ein", "Meer", "des", "Lei\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die stumme Brust verschlie\u00dft!", "tokens": ["Die", "stum\u00b7me", "Brust", "ver\u00b7schlie\u00dft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}