{"dta.poem.458": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Lust im Kusse.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn keine S\u00fcssigkeit in denen K\u00fcssen stecket?", "tokens": ["Wenn", "kei\u00b7ne", "S\u00fcs\u00b7sig\u00b7keit", "in", "de\u00b7nen", "K\u00fcs\u00b7sen", "ste\u00b7cket", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "APPR", "PRELS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So sage ", "tokens": ["So", "sa\u00b7ge"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Und nicht vielmehr die Hand/ mit seinen Lippen decket?", "tokens": ["Und", "nicht", "viel\u00b7mehr", "die", "Hand", "/", "mit", "sei\u00b7nen", "Lip\u00b7pen", "de\u00b7cket", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "ART", "NN", "$(", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-++--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Mein Kind es heist/ sie macht nicht solche Anmuth kund.", "tokens": ["Mein", "Kind", "es", "heist", "/", "sie", "macht", "nicht", "sol\u00b7che", "An\u00b7muth", "kund", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN", "$(", "PPER", "VVFIN", "PTKNEG", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}