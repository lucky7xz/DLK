{"dta.poem.13017": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Siebendes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es wird im Sommer oft der allersch\u00f6nste Tag,", "tokens": ["Es", "wird", "im", "Som\u00b7mer", "oft", "der", "al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "70An dem die Sonne selbst ihr Aug erg\u00f6zen mag,", "tokens": ["dem", "die", "Son\u00b7ne", "selbst", "ihr", "Aug", "er\u00b7g\u00f6\u00b7zen", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "PPOSAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "In den begl\u00e4nztesten und angenehmsten Stunden", "tokens": ["In", "den", "be\u00b7gl\u00e4nz\u00b7tes\u00b7ten", "und", "an\u00b7ge\u00b7nehms\u00b7ten", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von dem geringsten Grau der Wolcken \u00fcberwunden.", "tokens": ["Von", "dem", "ge\u00b7rings\u00b7ten", "Grau", "der", "Wol\u00b7cken", "\u00fc\u00b7berw\u00b7un\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es steigt von ungef\u00e4hr ein solcher Schatten vor,", "tokens": ["Es", "steigt", "von", "un\u00b7ge\u00b7f\u00e4hr", "ein", "sol\u00b7cher", "Schat\u00b7ten", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJD", "ART", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Bedeckt, verbirgt die Luft als ein gespannter Flor.", "tokens": ["Be\u00b7deckt", ",", "ver\u00b7birgt", "die", "Luft", "als", "ein", "ge\u00b7spann\u00b7ter", "Flor", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "ART", "NN", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "75 Durch eine stille Macht zerstreuet sich die N\u00e4sse;", "tokens": ["Durch", "ei\u00b7ne", "stil\u00b7le", "Macht", "zer\u00b7streu\u00b7et", "sich", "die", "N\u00e4s\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wie wann der Sonne Glanz selbst in das Grau zerfl\u00f6sse:", "tokens": ["Wie", "wann", "der", "Son\u00b7ne", "Glanz", "selbst", "in", "das", "Grau", "zer\u00b7fl\u00f6s\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PWAV", "ART", "NN", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Nichts sieht man unerbleicht. In einem Augenblick", "tokens": ["Nichts", "sieht", "man", "un\u00b7er\u00b7bleicht", ".", "In", "ei\u00b7nem", "Au\u00b7gen\u00b7blick"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PIS", "ADJD", "$.", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wird die verh\u00fcllte Luft, der Tropfen-Strich so dick,", "tokens": ["Wird", "die", "ver\u00b7h\u00fcll\u00b7te", "Luft", ",", "der", "Trop\u00b7fen\u00b7Strich", "so", "dick", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Da\u00df Freude, Lust und Trost, so wie der Schein, verschwindet,", "tokens": ["Da\u00df", "Freu\u00b7de", ",", "Lust", "und", "Trost", ",", "so", "wie", "der", "Schein", ",", "ver\u00b7schwin\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "NN", "$,", "NN", "KON", "NN", "$,", "ADV", "KOKOM", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "80Und aller Gassen Raum sich \u00fcberschwemmt befindet.", "tokens": ["al\u00b7ler", "Gas\u00b7sen", "Raum", "sich", "\u00fc\u00b7bersc\u00b7hwemmt", "be\u00b7fin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "PRF", "VVPP", "VVFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.13": {"text": "Bis sich das blasse Licht der Sonne wieder zeigt,", "tokens": ["Bis", "sich", "das", "blas\u00b7se", "Licht", "der", "Son\u00b7ne", "wie\u00b7der", "zeigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "ADJA", "NN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und ihr noch feuchter Strahl durch solche Wolcke steigt,", "tokens": ["Und", "ihr", "noch", "feuch\u00b7ter", "Strahl", "durch", "sol\u00b7che", "Wol\u00b7cke", "steigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADJA", "NN", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die W\u00e4sser aus der Luft sich allgemach verquellen,", "tokens": ["Die", "W\u00e4s\u00b7ser", "aus", "der", "Luft", "sich", "all\u00b7ge\u00b7mach", "ver\u00b7quel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und alle Kreise sich durch neuen Schein erhellen.", "tokens": ["Und", "al\u00b7le", "Krei\u00b7se", "sich", "durch", "neu\u00b7en", "Schein", "er\u00b7hel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PRF", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}