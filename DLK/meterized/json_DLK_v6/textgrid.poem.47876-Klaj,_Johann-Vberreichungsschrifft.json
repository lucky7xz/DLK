{"textgrid.poem.47876": {"metadata": {"author": {"name": "Klaj, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Vberreichungsschrifft", "genre": "verse", "period": "N.A.", "pub_year": 1636, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So geht es in der Welt/ die Zeit in sich verbunden", "tokens": ["So", "geht", "es", "in", "der", "Welt", "/", "die", "Zeit", "in", "sich", "ver\u00b7bun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$(", "ART", "NN", "APPR", "PRF", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat alle hundert Jahr ein neues Werk erfunden/", "tokens": ["Hat", "al\u00b7le", "hun\u00b7dert", "Jahr", "ein", "neu\u00b7es", "Werk", "er\u00b7fun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "CARD", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Welt jemehr sie graut/ jemehr sie sinnet au\u00df/", "tokens": ["Die", "Welt", "je\u00b7mehr", "sie", "graut", "/", "je\u00b7mehr", "sie", "sin\u00b7net", "au\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPER", "VVFIN", "$(", "KOUS", "PPER", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es saget der Magnet/ wo unser Fichtenhau\u00df", "tokens": ["Es", "sa\u00b7get", "der", "Mag\u00b7net", "/", "wo", "un\u00b7ser", "Fich\u00b7ten\u00b7hau\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "PWAV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Hinl\u00e4ufft/ wenn Eolus den Wind au\u00df seinen H\u00f6len", "tokens": ["Hin\u00b7l\u00e4ufft", "/", "wenn", "E\u00b7o\u00b7lus", "den", "Wind", "au\u00df", "sei\u00b7nen", "H\u00f6\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$(", "KOUS", "NE", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Herf\u00fcr l\u00e4st/ da\u00df er kan das tode Meer beseelen/", "tokens": ["Her\u00b7f\u00fcr", "l\u00e4st", "/", "da\u00df", "er", "kan", "das", "to\u00b7de", "Meer", "be\u00b7see\u00b7len", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$(", "KOUS", "PPER", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Im Fall uns \u00fcberf\u00e4lt die kohlpechschwartze Nacht/", "tokens": ["Im", "Fall", "uns", "\u00fc\u00b7berf\u00b7\u00e4lt", "die", "kohl\u00b7pech\u00b7schwart\u00b7ze", "Nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Auf der erbosten See/ da\u00df alles knikt und kracht.", "tokens": ["Auf", "der", "er\u00b7bos\u00b7ten", "See", "/", "da\u00df", "al\u00b7les", "knikt", "und", "kracht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$(", "KOUS", "PIS", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Darnach hat man versucht de\u00df Pulvers Macht gen\u00fctzet/", "tokens": ["Dar\u00b7nach", "hat", "man", "ver\u00b7sucht", "de\u00df", "Pul\u00b7vers", "Macht", "ge\u00b7n\u00fct\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PIS", "VVFIN", "ART", "NN", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der grossen St\u00fckken Kern Carthaunen lo\u00df geblitzet/", "tokens": ["Der", "gros\u00b7sen", "St\u00fck\u00b7ken", "Kern", "Car\u00b7thau\u00b7nen", "lo\u00df", "ge\u00b7blit\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "NN", "PTKVZ", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Das eingezwengte Feur schmie\u00df au\u00df das runde Bley", "tokens": ["Das", "ein\u00b7ge\u00b7zweng\u00b7te", "Feur", "schmie\u00df", "au\u00df", "das", "run\u00b7de", "Bley"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Mit sehr ergrimten Zorn/ ri\u00df Ro\u00df und Mann entzwey.", "tokens": ["Mit", "sehr", "er\u00b7grim\u00b7ten", "Zorn", "/", "ri\u00df", "Ro\u00df", "und", "Mann", "ent\u00b7zwey", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$(", "VVFIN", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Es sind zweyhundert Jahr und viere noch verschwunden/", "tokens": ["Es", "sind", "zwey\u00b7hun\u00b7dert", "Jahr", "und", "vie\u00b7re", "noch", "ver\u00b7schwun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "CARD", "NN", "KON", "PIS", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Als erst die K\u00fcnste Kunst durch Himmelsgunst erfunden", "tokens": ["Als", "erst", "die", "K\u00fcns\u00b7te", "Kunst", "durch", "Him\u00b7mels\u00b7gunst", "er\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "De\u00df klugen Faustes Faust/ der Edle Gutenberg/", "tokens": ["De\u00df", "klu\u00b7gen", "Faus\u00b7tes", "Faust", "/", "der", "Ed\u00b7le", "Gu\u00b7ten\u00b7berg", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Das Wunder der Natur/ das sch\u00f6ne Drukkerwerk.", "tokens": ["Das", "Wun\u00b7der", "der", "Na\u00b7tur", "/", "das", "sch\u00f6\u00b7ne", "Druk\u00b7ker\u00b7werk", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$(", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Hernach hat Magellan der Wellen Saltz durchpfl\u00fcget/", "tokens": ["Her\u00b7nach", "hat", "Ma\u00b7gel\u00b7lan", "der", "Wel\u00b7len", "Saltz", "durch\u00b7pfl\u00fc\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "ART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Vnd in der neuen Welt das arme Volk besieget/", "tokens": ["Vnd", "in", "der", "neu\u00b7en", "Welt", "das", "ar\u00b7me", "Volk", "be\u00b7sie\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Geraubt der Erden Mark/ der Edlensteine Pracht/", "tokens": ["Ge\u00b7raubt", "der", "Er\u00b7den", "Mark", "/", "der", "Ed\u00b7len\u00b7stei\u00b7ne", "Pracht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Was Holland/ Spanien und andre reich gemacht.", "tokens": ["Was", "Hol\u00b7land", "/", "Spa\u00b7ni\u00b7en", "und", "and\u00b7re", "reich", "ge\u00b7macht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "$(", "NE", "KON", "PIS", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Drauf kam Lutherus an/ der Teutschen Ruhm und Ehre/", "tokens": ["Drauf", "kam", "Lu\u00b7the\u00b7rus", "an", "/", "der", "Teut\u00b7schen", "Ruhm", "und", "Eh\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "NE", "PTKVZ", "$(", "ART", "ADJA", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Stekt auf das helle Liecht der reinen Gottes Lehre/", "tokens": ["Stekt", "auf", "das", "hel\u00b7le", "Liecht", "der", "rei\u00b7nen", "Got\u00b7tes", "Leh\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ein unbewehrter Mann trotzt die Welt/ wie er will/", "tokens": ["Ein", "un\u00b7be\u00b7wehr\u00b7ter", "Mann", "trotzt", "die", "Welt", "/", "wie", "er", "will", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$(", "PWAV", "PPER", "VMFIN", "$("], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "Erleget Babylon mit einem Federkiel.", "tokens": ["Er\u00b7le\u00b7get", "Ba\u00b7by\u00b7lon", "mit", "ei\u00b7nem", "Fe\u00b7der\u00b7kiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Gleich hundert Jahr hernach ist aufgerichtet worden", "tokens": ["Gleich", "hun\u00b7dert", "Jahr", "her\u00b7nach", "ist", "auf\u00b7ge\u00b7rich\u00b7tet", "wor\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "CARD", "NN", "ADV", "VAFIN", "VVPP", "VAPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Die hochvertraute Zunfft der Teutschen Helden Orden/", "tokens": ["Die", "hoch\u00b7ver\u00b7trau\u00b7te", "Zunfft", "der", "Teut\u00b7schen", "Hel\u00b7den", "Or\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Durch derer Zuthun jetzt die Teutsche Sprache bl\u00fct/", "tokens": ["Durch", "de\u00b7rer", "Zu\u00b7thun", "jetzt", "die", "Teut\u00b7sche", "Spra\u00b7che", "bl\u00fct", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "NN", "ADV", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.28": {"text": "Wie lautet es so wol/ wie manches sch\u00f6nes Lied", "tokens": ["Wie", "lau\u00b7tet", "es", "so", "wol", "/", "wie", "man\u00b7ches", "sch\u00f6\u00b7nes", "Lied"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADV", "$(", "KOKOM", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Schalt jetzund weit und breit bey den ber\u00fchmten Fl\u00fcssen/", "tokens": ["Schalt", "je\u00b7tzund", "weit", "und", "breit", "bey", "den", "be\u00b7r\u00fchm\u00b7ten", "Fl\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "KON", "ADJD", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Die Elbe h\u00f6ret zu/ der Rhein mu\u00df reiner g\u00fcssen/", "tokens": ["Die", "El\u00b7be", "h\u00f6\u00b7ret", "zu", "/", "der", "Rhein", "mu\u00df", "rei\u00b7ner", "g\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "PTKZU", "$(", "ART", "NE", "VMFIN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Wer au\u00df den Bober trinkt (da Opitz war bekand/", "tokens": ["Wer", "au\u00df", "den", "Bo\u00b7ber", "trinkt", "(", "da", "O\u00b7pitz", "war", "be\u00b7kand", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "$(", "KOUS", "NE", "VAFIN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Wo jetzt Apelles sing) schreibt Verse von der Hand.", "tokens": ["Wo", "jetzt", "A\u00b7pel\u00b7les", "sing", ")", "schreibt", "Ver\u00b7se", "von", "der", "Hand", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NE", "VVFIN", "$(", "VVFIN", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.33": {"text": "Die Okker ist gelehrt/ den Teutschen Ruhm zu preisen/", "tokens": ["Die", "Ok\u00b7ker", "ist", "ge\u00b7lehrt", "/", "den", "Teut\u00b7schen", "Ruhm", "zu", "prei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$(", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Sie r\u00fchrt der Harfen Zier/ in j\u00fcngsterfundnen Weisen/", "tokens": ["Sie", "r\u00fchrt", "der", "Har\u00b7fen", "Zier", "/", "in", "j\u00fcngs\u00b7ter\u00b7fund\u00b7nen", "Wei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$(", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Die Pegnitz stimmet ein/ die s\u00fcsse Melodey", "tokens": ["Die", "Peg\u00b7nitz", "stim\u00b7met", "ein", "/", "die", "s\u00fcs\u00b7se", "Me\u00b7lo\u00b7dey"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Bezeugt/ da\u00df unser Land der K\u00fcnste Schauplatz sey.", "tokens": ["Be\u00b7zeugt", "/", "da\u00df", "un\u00b7ser", "Land", "der", "K\u00fcns\u00b7te", "Schau\u00b7platz", "sey", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$(", "KOUS", "PPOSAT", "NN", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Soll aber dieses Schiff das Gut wol \u00fcbertragen/", "tokens": ["Soll", "a\u00b7ber", "die\u00b7ses", "Schiff", "das", "Gut", "wol", "\u00fc\u00b7ber\u00b7tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PDAT", "NN", "ART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Mu\u00df es ein guter Wind hin in den Hafen jagen/", "tokens": ["Mu\u00df", "es", "ein", "gu\u00b7ter", "Wind", "hin", "in", "den", "Ha\u00b7fen", "ja\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Soll die nichthelle Glut recht geben einen Schein/", "tokens": ["Soll", "die", "nicht\u00b7hel\u00b7le", "Glut", "recht", "ge\u00b7ben", "ei\u00b7nen", "Schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "ADJD", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "So mu\u00df sie nach und nach recht aufgeblasen seyn.", "tokens": ["So", "mu\u00df", "sie", "nach", "und", "nach", "recht", "auf\u00b7ge\u00b7bla\u00b7sen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "KON", "APPR", "ADJD", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Virgil der h\u00e4tte nicht sein ewigs Buch geschrieben/", "tokens": ["Vir\u00b7gil", "der", "h\u00e4t\u00b7te", "nicht", "sein", "e\u00b7wigs", "Buch", "ge\u00b7schrie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "VAFIN", "PTKNEG", "PPOSAT", "ADJA", "NN