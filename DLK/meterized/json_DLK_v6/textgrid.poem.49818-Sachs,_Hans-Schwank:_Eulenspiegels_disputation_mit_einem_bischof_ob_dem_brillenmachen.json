{"textgrid.poem.49818": {"metadata": {"author": {"name": "Sachs, Hans", "birth": "N.A.", "death": "N.A."}, "title": "Schwank: Eulenspiegels disputation mit einem bischof ob dem brillenmachen", "genre": "verse", "period": "N.A.", "pub_year": 1554, "urn": "N.A.", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eulenspiegel etwan vor jaren,", "tokens": ["Eu\u00b7len\u00b7spie\u00b7gel", "et\u00b7wan", "vor", "ja\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "in aller schalkheit wol erfaren,", "tokens": ["in", "al\u00b7ler", "schalk\u00b7heit", "wol", "er\u00b7fa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "loff in eim winter \u00fcber felt,", "tokens": ["loff", "in", "eim", "win\u00b7ter", "\u00fc\u00b7ber", "felt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "het schlechte kleider und kein gelt;", "tokens": ["het", "schlech\u00b7te", "klei\u00b7der", "und", "kein", "gelt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "KON", "PIAT", "ADJD", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "in dem da sach er dort von weiten", "tokens": ["in", "dem", "da", "sach", "er", "dort", "von", "wei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "VVFIN", "PPER", "ADV", "APPR", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "ein reising zeug gegen im reiten.", "tokens": ["ein", "rei\u00b7sing", "zeug", "ge\u00b7gen", "im", "rei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "NN", "APPR", "APPRART", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "dasselbige ein bischof war;", "tokens": ["das\u00b7sel\u00b7bi\u00b7ge", "ein", "bi\u00b7schof", "war", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VAFIN", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.8": {"text": "derselbige wolt gen Worms dar,", "tokens": ["der\u00b7sel\u00b7bi\u00b7ge", "wolt", "gen", "Worms", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "alda solt werden ein reichstag,", "tokens": ["al\u00b7da", "solt", "wer\u00b7den", "ein", "reichs\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "VAFIN", "ART", "NN", "$,"], "meter": "+--+-+--", "measure": "iambic.tri.invert"}, "line.10": {"text": "und mancher f\u00fcrste darzu lag,", "tokens": ["und", "man\u00b7cher", "f\u00fcrs\u00b7te", "dar\u00b7zu", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "solten betrachten gmeinen nutz,", "tokens": ["sol\u00b7ten", "be\u00b7trach\u00b7ten", "gmei\u00b7nen", "nutz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.12": {"text": "r\u00f6mischem reich zu hilf und schutz,", "tokens": ["r\u00f6\u00b7mi\u00b7schem", "reich", "zu", "hilf", "und", "schutz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "ADJD", "APPR", "NE", "KON", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.13": {"text": "das auf di\u00dfmal vil anst\u00f6\u00df het.", "tokens": ["das", "auf", "di\u00df\u00b7mal", "vil", "an\u00b7st\u00f6\u00df", "het", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ADV", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "als er im nun begegnen tet,", "tokens": ["als", "er", "im", "nun", "be\u00b7geg\u00b7nen", "tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADV", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.15": {"text": "Eulenspiegel tet ab sein hut", "tokens": ["Eu\u00b7len\u00b7spie\u00b7gel", "tet", "ab", "sein", "hut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.16": {"text": "und neigt sich gen dem bischof gut;", "tokens": ["und", "neigt", "sich", "gen", "dem", "bi\u00b7schof", "gut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "der hielt, sach Eulenspiegel an,", "tokens": ["der", "hielt", ",", "sach", "Eu\u00b7len\u00b7spie\u00b7gel", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "$,", "VVFIN", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "merkt wol, das er war ein fatzman,", "tokens": ["merkt", "wol", ",", "das", "er", "war", "ein", "fatz\u00b7man", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PRELS", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "dacht im: ich h\u00f6rt bei allen tagen,", "tokens": ["dacht", "im", ":", "ich", "h\u00f6rt", "bei", "al\u00b7len", "ta\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "$.", "PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "kinder und narrn die warheit sagen,", "tokens": ["kin\u00b7der", "und", "narrn", "die", "war\u00b7heit", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.21": {"text": "ich will gleich disen reden an,", "tokens": ["ich", "will", "gleich", "di\u00b7sen", "re\u00b7den", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PDS", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "der wirt mir gar balt sagen tan,", "tokens": ["der", "wirt", "mir", "gar", "balt", "sa\u00b7gen", "tan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "PPER", "ADV", "ADV", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "was das gschrei ist von f\u00fcrstn und hern", "tokens": ["was", "das", "gschrei", "ist", "von", "f\u00fcrstn", "und", "hern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "VAFIN", "APPR", "NE", "KON", "VVINF"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.24": {"text": "bei dem gmein man nahet und fern.", "tokens": ["bei", "dem", "gmein", "man", "na\u00b7het", "und", "fern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PIS", "VVFIN", "KON", "ADJD", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}}}}