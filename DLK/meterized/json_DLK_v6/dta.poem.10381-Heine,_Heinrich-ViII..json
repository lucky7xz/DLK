{"dta.poem.10381": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "ViII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Es stehen unbeweglich", "tokens": ["Es", "ste\u00b7hen", "un\u00b7be\u00b7weg\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Sterne in der H\u00f6h',", "tokens": ["Die", "Ster\u00b7ne", "in", "der", "H\u00f6h'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Viel tausend Jahr', und schauen", "tokens": ["Viel", "tau\u00b7send", "Jahr'", ",", "und", "schau\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "CARD", "NN", "$,", "KON", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sich an mit Liebesweh.", "tokens": ["Sich", "an", "mit", "Lie\u00b7bes\u00b7weh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie sprechen eine Sprache,", "tokens": ["Sie", "spre\u00b7chen", "ei\u00b7ne", "Spra\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die ist so reich, so sch\u00f6n;", "tokens": ["Die", "ist", "so", "reich", ",", "so", "sch\u00f6n", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch keiner der Philologen", "tokens": ["Doch", "kei\u00b7ner", "der", "Phi\u00b7lo\u00b7lo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIS", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Kann diese Sprache verstehn.", "tokens": ["Kann", "die\u00b7se", "Spra\u00b7che", "ver\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDAT", "NN", "VVINF", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Ich aber hab' sie gelernet,", "tokens": ["Ich", "a\u00b7ber", "hab'", "sie", "ge\u00b7ler\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und ich vergesse sie nicht;", "tokens": ["Und", "ich", "ver\u00b7ges\u00b7se", "sie", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Mir diente als Grammatik", "tokens": ["Mir", "dien\u00b7te", "als", "Gram\u00b7ma\u00b7tik"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOUS", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der Herzallerliebsten Gesicht.", "tokens": ["Der", "Her\u00b7zal\u00b7ler\u00b7liebs\u00b7ten", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}