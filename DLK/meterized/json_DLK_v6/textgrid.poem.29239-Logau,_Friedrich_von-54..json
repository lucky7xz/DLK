{"textgrid.poem.29239": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "54.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Soll man dann am Himmel sehen,", "tokens": ["Soll", "man", "dann", "am", "Him\u00b7mel", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was hierunten soll geschehen?", "tokens": ["Was", "hier\u00b7un\u00b7ten", "soll", "ge\u00b7sche\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Soll der Himmel geben Blick", "tokens": ["Soll", "der", "Him\u00b7mel", "ge\u00b7ben", "Blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "VVINF", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auff so manches Schelmen-St\u00fcck?", "tokens": ["Auff", "so", "man\u00b7ches", "Schel\u00b7men\u00b7St\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer wird mehr den Himmel achten,", "tokens": ["Wer", "wird", "mehr", "den", "Him\u00b7mel", "ach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Drauff man sonst so schlecht wil trachten?", "tokens": ["Drauff", "man", "sonst", "so", "schlecht", "wil", "trach\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIS", "ADV", "ADV", "ADJD", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}