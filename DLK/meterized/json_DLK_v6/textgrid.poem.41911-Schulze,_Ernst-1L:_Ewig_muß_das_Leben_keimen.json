{"textgrid.poem.41911": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ewig mu\u00df das Leben keimen", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ewig mu\u00df das Leben keimen", "tokens": ["E\u00b7wig", "mu\u00df", "das", "Le\u00b7ben", "kei\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus dem dunkeln Schoo\u00df der Erde,", "tokens": ["Aus", "dem", "dun\u00b7keln", "Schoo\u00df", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df zu wandelbaren Tr\u00e4umen", "tokens": ["Da\u00df", "zu", "wan\u00b7del\u00b7ba\u00b7ren", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles, was wir liebten, werde.", "tokens": ["Al\u00b7les", ",", "was", "wir", "lieb\u00b7ten", ",", "wer\u00b7de", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "PPER", "VVFIN", "$,", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sehnst du dich, ein Bild zu halten,", "tokens": ["Sehnst", "du", "dich", ",", "ein", "Bild", "zu", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Das es bleibend dich erfreue,", "tokens": ["Das", "es", "blei\u00b7bend", "dich", "er\u00b7freu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADJD", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wird es fl\u00fcchtig sich auf's neue", "tokens": ["Wird", "es", "fl\u00fcch\u00b7tig", "sich", "auf's", "neu\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADJD", "PRF", "APPRART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In ein fremdes umgestalten.", "tokens": ["In", "ein", "frem\u00b7des", "um\u00b7ge\u00b7stal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wo die Knospen heut erwachten,", "tokens": ["Wo", "die", "Knos\u00b7pen", "heut", "er\u00b7wach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wirst du morgen Bl\u00e4tter finden,", "tokens": ["Wirst", "du", "mor\u00b7gen", "Bl\u00e4t\u00b7ter", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn die Bl\u00fcthen kaum dir lachten,", "tokens": ["Wenn", "die", "Bl\u00fc\u00b7then", "kaum", "dir", "lach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mu\u00df auch schon die Frucht sich r\u00fcnden;", "tokens": ["Mu\u00df", "auch", "schon", "die", "Frucht", "sich", "r\u00fcn\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "ART", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Fr\u00fchling, ach, wer kann dich sehen", "tokens": ["Fr\u00fch\u00b7ling", ",", "ach", ",", "wer", "kann", "dich", "se\u00b7hen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "ITJ", "$,", "PWS", "VMFIN", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und an deinem Glanz sich weiden?", "tokens": ["Und", "an", "dei\u00b7nem", "Glanz", "sich", "wei\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Bist ja nur ein ew'ges Scheiden,", "tokens": ["Bist", "ja", "nur", "ein", "ew'\u00b7ges", "Schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ew'ges Wechseln und Vergehen.", "tokens": ["Ew'\u00b7ges", "Wech\u00b7seln", "und", "Ver\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Blumen, welche schnell verbl\u00fchen,", "tokens": ["Blu\u00b7men", ",", "wel\u00b7che", "schnell", "ver\u00b7bl\u00fc\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heitres Blau in hohen Fernen,", "tokens": ["Heit\u00b7res", "Blau", "in", "ho\u00b7hen", "Fer\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ADJA", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wolken, die vor\u00fcberziehen", "tokens": ["Wol\u00b7ken", ",", "die", "vor\u00b7\u00fc\u00b7ber\u00b7zie\u00b7hen"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vor des Himmels festen Sternen,", "tokens": ["Vor", "des", "Him\u00b7mels", "fes\u00b7ten", "Ster\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Leise T\u00f6ne, die verschweben,", "tokens": ["Lei\u00b7se", "T\u00f6\u00b7ne", ",", "die", "ver\u00b7schwe\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PRELS", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Fremde Bilder, flucht'ger Schimmer,", "tokens": ["Frem\u00b7de", "Bil\u00b7der", ",", "flucht'\u00b7ger", "Schim\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Fr\u00fchling, ach, wohl irrt man immer,", "tokens": ["Fr\u00fch\u00b7ling", ",", "ach", ",", "wohl", "irrt", "man", "im\u00b7mer", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ITJ", "$,", "ADV", "VVFIN", "PIS", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nennt man Liebe dich und Leben!", "tokens": ["Nennt", "man", "Lie\u00b7be", "dich", "und", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "NN", "PPER", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}