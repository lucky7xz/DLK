{"textgrid.poem.61481": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du wei\u00dft, ein Leid aus Gottes Hand", "tokens": ["Du", "wei\u00dft", ",", "ein", "Leid", "aus", "Got\u00b7tes", "Hand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "APPR", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durchl\u00e4utert dich wie Feuerbrand.", "tokens": ["Durch\u00b7l\u00e4u\u00b7tert", "dich", "wie", "Feu\u00b7er\u00b7brand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So lerne, wenn dich Menschen kr\u00e4nken,", "tokens": ["So", "ler\u00b7ne", ",", "wenn", "dich", "Men\u00b7schen", "kr\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df Gott auch dies dir schickt, zu denken;", "tokens": ["Da\u00df", "Gott", "auch", "dies", "dir", "schickt", ",", "zu", "den\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "PDS", "PPER", "VVFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Das mindert zwar nicht ihr Verschulden,", "tokens": ["Das", "min\u00b7dert", "zwar", "nicht", "ihr", "Ver\u00b7schul\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Aber es reinigt dein Erdulden.", "tokens": ["A\u00b7ber", "es", "rei\u00b7nigt", "dein", "E\u00b7rdul\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}}}}