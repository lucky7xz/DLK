], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn wir aber dieses hier,", "tokens": ["Wenn", "wir", "a\u00b7ber", "die\u00b7ses", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PDAT", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Als von Dir formiert, erwegen,", "tokens": ["Als", "von", "Dir", "for\u00b7miert", ",", "er\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "VVPP", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und, in dieser Bluhmen-Pracht,", "tokens": ["Und", ",", "in", "die\u00b7ser", "Bluh\u00b7men\u00b7Pracht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Den, der sie so sch\u00f6n gemacht,", "tokens": ["Den", ",", "der", "sie", "so", "sch\u00f6n", "ge\u00b7macht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Mit vergn\u00fcgter Ehr-Furcht, sehn,", "tokens": ["Mit", "ver\u00b7gn\u00fcg\u00b7ter", "Ehr\u00b7Furcht", ",", "sehn", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ehren, lieben, und erh\u00f6hn;", "tokens": ["Eh\u00b7ren", ",", "lie\u00b7ben", ",", "und", "er\u00b7h\u00f6hn", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "KON", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Steckt in ihr zu aller Zeit,", "tokens": 