{"dta.poem.10049": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die gelbe Rose.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es sahe die Natur von ihren Meister-St\u00fccken", "tokens": ["Es", "sa\u00b7he", "die", "Na\u00b7tur", "von", "ih\u00b7ren", "Meis\u00b7ter\u00b7St\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der sch\u00f6nsten eins, die Rose, selbst mit Lust,", "tokens": ["Der", "sch\u00f6ns\u00b7ten", "eins", ",", "die", "Ro\u00b7se", ",", "selbst", "mit", "Lust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "$,", "ART", "NN", "$,", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch auch zugleich mit tr\u00fcben Blicken,", "tokens": ["Doch", "auch", "zu\u00b7gleich", "mit", "tr\u00fc\u00b7ben", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Unvergn\u00fcgen an. Jhr war nicht unbewust,", "tokens": ["Und", "Un\u00b7ver\u00b7gn\u00fc\u00b7gen", "an", ".", "Ihr", "war", "nicht", "un\u00b7be\u00b7wust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKVZ", "$.", "PPER", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da\u00df, ob sie gleich, die Menschen zu erquicken,", "tokens": ["Da\u00df", ",", "ob", "sie", "gleich", ",", "die", "Men\u00b7schen", "zu", "er\u00b7qui\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PPER", "ADV", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die rothe Rose selbst gewehlt,", "tokens": ["Die", "ro\u00b7the", "Ro\u00b7se", "selbst", "ge\u00b7wehlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und sich bem\u00fcht, sie pr\u00e4chtigst auszuschm\u00fccken;", "tokens": ["Und", "sich", "be\u00b7m\u00fcht", ",", "sie", "pr\u00e4ch\u00b7tigst", "aus\u00b7zu\u00b7schm\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "VVPP", "$,", "PPER", "VVFIN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Sie dennoch ihres Zwecks verfehlt.", "tokens": ["Sie", "den\u00b7noch", "ih\u00b7res", "Zwecks", "ver\u00b7fehlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Denn, sprach sie, iedermann", "tokens": ["Denn", ",", "sprach", "sie", ",", "ie\u00b7der\u00b7mann"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "$,", "VVFIN", "PPER", "$,", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Sieht Rosen, ja so wol, als andrer Bluhmen Pracht,", "tokens": ["Sieht", "Ro\u00b7sen", ",", "ja", "so", "wol", ",", "als", "an\u00b7drer", "Bluh\u00b7men", "Pracht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "ADV", "ADV", "ADV", "$,", "KOUS", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Mit, sie nicht sehnden, Augen an.", "tokens": ["Mit", ",", "sie", "nicht", "sehn\u00b7den", ",", "Au\u00b7gen", "an", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "$,", "PPER", "PTKNEG", "VVINF", "$,", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie z\u00fcrnte dennoch nicht: vielmehr war sie bedacht,", "tokens": ["Sie", "z\u00fcrn\u00b7te", "den\u00b7noch", "nicht", ":", "viel\u00b7mehr", "war", "sie", "be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "$.", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-++-+--+", "measure": "iambic.hexa.chol"}, "line.2": {"text": "Aus Lieb und Z\u00e4rtlichkeit, noch einmahl zu probiren,", "tokens": ["Aus", "Lieb", "und", "Z\u00e4rt\u00b7lich\u00b7keit", ",", "noch", "ein\u00b7mahl", "zu", "pro\u00b7bi\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ob es, den kalten Sinn der Menschen recht zu r\u00fchren,", "tokens": ["Ob", "es", ",", "den", "kal\u00b7ten", "Sinn", "der", "Men\u00b7schen", "recht", "zu", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ART", "ADJA", "NN", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Denn gar nicht m\u00f6glich sey.", "tokens": ["Denn", "gar", "nicht", "m\u00f6g\u00b7lich", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie fieng deswegen an auch weisse zu formiren,", "tokens": ["Sie", "fi\u00b7eng", "des\u00b7we\u00b7gen", "an", "auch", "weis\u00b7se", "zu", "for\u00b7mi\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "APPR", "ADV", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.6": {"text": "Nachher auch Wein- und E\u00dfig-Rosen,", "tokens": ["Nach\u00b7her", "auch", "Wein", "und", "E\u00b7\u00dfig\u00b7Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Um, durch Ver\u00e4nderung dem Auge liebzukosen.", "tokens": ["Um", ",", "durch", "Ver\u00b7\u00e4n\u00b7de\u00b7rung", "dem", "Au\u00b7ge", "lieb\u00b7zu\u00b7ko\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "$,", "APPR", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Allein es war und blieb stets einerley.", "tokens": ["Al\u00b7lein", "es", "war", "und", "blieb", "stets", "ei\u00b7ner\u00b7ley", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "KON", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Zuletzt (da ihr nicht unbekannt,", "tokens": ["Zu\u00b7letzt", "(", "da", "ihr", "nicht", "un\u00b7be\u00b7kannt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "KOUS", "PPER", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df die Begierde, reich zu werden,", "tokens": ["Da\u00df", "die", "Be\u00b7gier\u00b7de", ",", "reich", "zu", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der meisten Menschen Hertz auf Erden", "tokens": ["Der", "meis\u00b7ten", "Men\u00b7schen", "Hertz", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit solcher heissen Gold-Sucht f\u00fcllt,", "tokens": ["Mit", "sol\u00b7cher", "heis\u00b7sen", "Gold\u00b7Sucht", "f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df ihrer Seelen Licht, der denckende Verstand,", "tokens": ["Da\u00df", "ih\u00b7rer", "See\u00b7len", "Licht", ",", "der", "den\u00b7cken\u00b7de", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+---+", "measure": "unknown.measure.penta"}, "line.6": {"text": "Dadurch so sehr benebelt und verh\u00fcllt,", "tokens": ["Da\u00b7durch", "so", "sehr", "be\u00b7ne\u00b7belt", "und", "ver\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ADV", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Da\u00df, da er, ", "tokens": ["Da\u00df", ",", "da", "er", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PPER", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Er alle Creatur, sammt ihrem Schmuck, verachtet,", "tokens": ["Er", "al\u00b7le", "Crea\u00b7tur", ",", "sammt", "ih\u00b7rem", "Schmuck", ",", "ver\u00b7ach\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Und, blind f\u00fcr alles, nichts als Gold", "tokens": ["Und", ",", "blind", "f\u00fcr", "al\u00b7les", ",", "nichts", "als", "Gold"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "ADJD", "APPR", "PIS", "$,", "PIS", "KOKOM", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Zu sehn, und zu erlangen trachtet,)", "tokens": ["Zu", "sehn", ",", "und", "zu", "er\u00b7lan\u00b7gen", "trach\u00b7tet", ",", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKZU", "VVINF", "$,", "KON", "PTKZU", "VVINF", "VVFIN", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Beschlo\u00df sie, sich noch einmahl zu bem\u00fchn,", "tokens": ["Be\u00b7schlo\u00df", "sie", ",", "sich", "noch", "ein\u00b7mahl", "zu", "be\u00b7m\u00fchn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PRF", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und, \u00fcm, durch diesen Weg, ihn zu sich her zu ziehn,", "tokens": ["Und", ",", "\u00fcm", ",", "durch", "die\u00b7sen", "Weg", ",", "ihn", "zu", "sich", "her", "zu", "ziehn", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "FM.la", "$,", "APPR", "PDAT", "NN", "$,", "PPER", "APPR", "PRF", "APZR", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die Rosen-Bl\u00e4tter zu verg\u00fclden.", "tokens": ["Die", "Ro\u00b7sen\u00b7Bl\u00e4t\u00b7ter", "zu", "ver\u00b7g\u00fcl\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Man sahe sie demnach die gelbe Rose bilden:", "tokens": ["Man", "sa\u00b7he", "sie", "dem\u00b7nach", "die", "gel\u00b7be", "Ro\u00b7se", "bil\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PAV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein neues Wunder, dessen Pracht", "tokens": ["Ein", "neu\u00b7es", "Wun\u00b7der", ",", "des\u00b7sen", "Pracht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Schein im solchem gelb- und sch\u00f6nen Feuer gl\u00fchen,", "tokens": ["Und", "Schein", "im", "sol\u00b7chem", "gelb", "und", "sch\u00f6\u00b7nen", "Feu\u00b7er", "gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPRART", "ADJA", "TRUNC", "KON", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df der fast g\u00fcldne Glantz Chrysanders Augen", "tokens": ["Da\u00df", "der", "fast", "g\u00fcld\u00b7ne", "Glantz", "Chry\u00b7san\u00b7ders", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADV", "ADJA", "NN", "NE", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Auf eine Zeitlang gar auf sich zu ziehen,", "tokens": ["Auf", "ei\u00b7ne", "Zeit\u00b7lang", "gar", "auf", "sich", "zu", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "APPR", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und durch den gelben Schein, ihn zu ergetzen taugen,", "tokens": ["Und", "durch", "den", "gel\u00b7ben", "Schein", ",", "ihn", "zu", "er\u00b7get\u00b7zen", "tau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,", "PPER", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Zumahl er ein gedoppeltes Vergn\u00fcgen", "tokens": ["Zu\u00b7mahl", "er", "ein", "ge\u00b7dop\u00b7pel\u00b7tes", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Auf ihrer Bl\u00e4tter Fl\u00e4ch\u2019 erblickt,", "tokens": ["Auf", "ih\u00b7rer", "Bl\u00e4t\u00b7ter", "Fl\u00e4ch'", "er\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Da ausw\u00e4rts Cronen-Gold,", "tokens": ["Da", "aus\u00b7w\u00e4rts", "Cro\u00b7nen\u00b7Gold", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Und inw\u00e4rts (dem er noch am meisten hold)", "tokens": ["Und", "in\u00b7w\u00e4rts", "(", "dem", "er", "noch", "am", "meis\u00b7ten", "hold", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$(", "PRELS", "PPER", "ADV", "PTKA", "PIS", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "So gar Ducaten-Gold sie schm\u00fcckt.", "tokens": ["So", "gar", "Du\u00b7ca\u00b7ten\u00b7Gold", "sie", "schm\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NE", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Allein, indem es nur gar kurtze Zeit", "tokens": ["Al\u00b7lein", ",", "in\u00b7dem", "es", "nur", "gar", "kurt\u00b7ze", "Zeit"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADV", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Und er nicht die Natur, nicht ihren Sch\u00f6pfer ehrte;", "tokens": ["Und", "er", "nicht", "die", "Na\u00b7tur", ",", "nicht", "ih\u00b7ren", "Sch\u00f6p\u00b7fer", "ehr\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKNEG", "ART", "NN", "$,", "PTKNEG", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Schien ihr aufs neu ihr Unternehmen leid:", "tokens": ["Schien", "ihr", "aufs", "neu", "ihr", "Un\u00b7ter\u00b7neh\u00b7men", "leid", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJD", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Und, aus gerechter Reu und bill\u2019ger Traurigkeit,", "tokens": ["Und", ",", "aus", "ge\u00b7rech\u00b7ter", "Reu", "und", "bill'\u00b7ger", "Trau\u00b7rig\u00b7keit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ADJD", "NE", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Erweckt sie einen Wurm, der dieser Rosen Pracht,", "tokens": ["Er\u00b7weckt", "sie", "ei\u00b7nen", "Wurm", ",", "der", "die\u00b7ser", "Ro\u00b7sen", "Pracht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Als wie ihr steter Feind, fast stets zu nichte macht.", "tokens": ["Als", "wie", "ihr", "ste\u00b7ter", "Feind", ",", "fast", "stets", "zu", "nich\u00b7te", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "PPOSAT", "ADJA", "NN", "$,", "ADV", "ADV", "PTKA", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Daher man sie denn ietzt gar selten recht ger\u00fcndet,", "tokens": ["Da\u00b7her", "man", "sie", "denn", "ietzt", "gar", "sel\u00b7ten", "recht", "ge\u00b7r\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIS", "PPER", "ADV", "ADV", "ADV", "ADJD", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und in der lieblichen Vollkommenheit,", "tokens": ["Und", "in", "der", "lieb\u00b7li\u00b7chen", "Voll\u00b7kom\u00b7men\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "So wie vorher, nur ihrer wenig findet.", "tokens": ["So", "wie", "vor\u00b7her", ",", "nur", "ih\u00b7rer", "we\u00b7nig", "fin\u00b7det", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ADV", "$,", "ADV", "PPOSAT", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Mich deucht, da\u00df ich hierin zugleich noch eine Lehre", "tokens": ["Mich", "deucht", ",", "da\u00df", "ich", "hie\u00b7rin", "zu\u00b7gleich", "noch", "ei\u00b7ne", "Leh\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ADV", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und f\u00fcr den Geitz ein schreckend Sinn-Bild finde.", "tokens": ["Und", "f\u00fcr", "den", "Geitz", "ein", "schre\u00b7ckend", "Sinn\u00b7Bild", "fin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es scheint, da\u00df, wie der Geitz gar selten sonder S\u00fcnde,", "tokens": ["Es", "scheint", ",", "da\u00df", ",", "wie", "der", "Geitz", "gar", "sel\u00b7ten", "son\u00b7der", "S\u00fcn\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "$,", "PWAV", "ART", "NN", "ADV", "ADJD", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Er meistens einen Wurm auch im Gewissen n\u00e4hre.", "tokens": ["Er", "meis\u00b7tens", "ei\u00b7nen", "Wurm", "auch", "im", "Ge\u00b7wis\u00b7sen", "n\u00e4h\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Damit ich aber