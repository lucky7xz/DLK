{"textgrid.poem.41980": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach, mir schmerzet die Stirn, so sprachst du und wandtest den Blick ab,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, mir schmerzet die Stirn, so sprachst du und wandtest den Blick ab,", "tokens": ["Ach", ",", "mir", "schmer\u00b7zet", "die", "Stirn", ",", "so", "sprachst", "du", "und", "wand\u00b7test", "den", "Blick", "ab", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Und dein rosiger Mund weigerte z\u00fcrnend den Ku\u00df.", "tokens": ["Und", "dein", "ro\u00b7si\u00b7ger", "Mund", "wei\u00b7ger\u00b7te", "z\u00fcr\u00b7nend", "den", "Ku\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Trauernd sa\u00df ich und st\u00fctzte das Haupt und starrte den Tisch an,", "tokens": ["Trau\u00b7ernd", "sa\u00df", "ich", "und", "st\u00fctz\u00b7te", "das", "Haupt", "und", "starr\u00b7te", "den", "Tisch", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "KON", "VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+--+--+-+-++-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Und kein schmeichelndes Wort flehte von neuem zu dir.", "tokens": ["Und", "kein", "schmei\u00b7cheln\u00b7des", "Wort", "fleh\u00b7te", "von", "neu\u00b7em", "zu", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "APPR", "PPER", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Stumm nun harrten wir Beid' und z\u00fcrneten Beide; der Unmuth", "tokens": ["Stumm", "nun", "harr\u00b7ten", "wir", "Beid'", "und", "z\u00fcr\u00b7ne\u00b7ten", "Bei\u00b7de", ";", "der", "Un\u00b7muth"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "ADV", "VVFIN", "PPER", "NN", "KON", "VVFIN", "PIS", "$.", "ART", "NN"], "meter": "--+--+-+--+-+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Streckte die starrende Hand zwischen das schweigende Paar.", "tokens": ["Streck\u00b7te", "die", "star\u00b7ren\u00b7de", "Hand", "zwi\u00b7schen", "das", "schwei\u00b7gen\u00b7de", "Paar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Wahrlich, erblickt' ein Maler uns jetzt, schnell h\u00e4tt' er der Ehe", "tokens": ["Wahr\u00b7lich", ",", "er\u00b7blickt'", "ein", "Ma\u00b7ler", "uns", "jetzt", ",", "schnell", "h\u00e4tt'", "er", "der", "E\u00b7he"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "VVFIN", "ART", "NN", "PPER", "ADV", "$,", "ADJD", "VAFIN", "PPER", "ART", "NN"], "meter": "+--+-+-+-++--+-", "measure": "iambic.septa.invert"}, "line.8": {"text": "Treffendes Bild und der Treu' eiserne Bande gemalt.", "tokens": ["Tref\u00b7fen\u00b7des", "Bild", "und", "der", "Treu'", "ei\u00b7ser\u00b7ne", "Ban\u00b7de", "ge\u00b7malt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ART", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.9": {"text": "Sprich, was fehlet dir, lispeltest du jetzt endlich und wandtest", "tokens": ["Sprich", ",", "was", "feh\u00b7let", "dir", ",", "lis\u00b7pel\u00b7test", "du", "jetzt", "end\u00b7lich", "und", "wand\u00b7test"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "PWS", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "ADV", "ADV", "KON", "VVFIN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Halb das holde Gesicht, nahetest leise die Hand;", "tokens": ["Halb", "das", "hol\u00b7de", "Ge\u00b7sicht", ",", "na\u00b7he\u00b7test", "lei\u00b7se", "die", "Hand", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$,", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Ach mir schmerzet das Herz, so rief ich und wandte den Blick ab,", "tokens": ["Ach", "mir", "schmer\u00b7zet", "das", "Herz", ",", "so", "rief", "ich", "und", "wand\u00b7te", "den", "Blick", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.12": {"text": "Und der gebotenen Hand weigert' ich z\u00fcrnend die Hand.", "tokens": ["Und", "der", "ge\u00b7bo\u00b7te\u00b7nen", "Hand", "wei\u00b7gert'", "ich", "z\u00fcr\u00b7nend", "die", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PPER", "VVPP", "ART", "NN", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Jeglicher grollte nun sich und dem Anderen, weil er die Gabe,", "tokens": ["Jeg\u00b7li\u00b7cher", "groll\u00b7te", "nun", "sich", "und", "dem", "An\u00b7de\u00b7ren", ",", "weil", "er", "die", "Ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "PRF", "KON", "ART", "ADJA", "$,", "KOUS", "PPER", "ART", "NN", "$,"], "meter": "+--+-+-+-+-+-+-+-", "measure": "iambic.octa.plus.invert"}, "line.14": {"text": "Selber verscherzt, weil stolz Jener die Gabe verschm\u00e4ht.", "tokens": ["Sel\u00b7ber", "ver\u00b7scherzt", ",", "weil", "stolz", "Je\u00b7ner", "die", "Ga\u00b7be", "ver\u00b7schm\u00e4ht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "KOUS", "ADJD", "PDAT", "ART", "NN", "VVPP", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.15": {"text": "Oft nur sandten, ob Keinen die That wohl reute, wir Blicke", "tokens": ["Oft", "nur", "sand\u00b7ten", ",", "ob", "Kei\u00b7nen", "die", "That", "wohl", "reu\u00b7te", ",", "wir", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "$,", "KOUS", "NN", "ART", "NN", "ADV", "VVFIN", "$,", "PPER", "NN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.16": {"text": "Lauschend uns zu, doch sie flohn, wenn sie sich trafen, zur\u00fcck.", "tokens": ["Lau\u00b7schend", "uns", "zu", ",", "doch", "sie", "flohn", ",", "wenn", "sie", "sich", "tra\u00b7fen", ",", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "PPER", "PTKVZ", "$,", "KON", "PPER", "VVFIN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,", "PTKVZ", "$."], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.17": {"text": "Siehe, da brach dein Trotz! Holdselige, mu\u00dftest du l\u00e4ngst nicht", "tokens": ["Sie\u00b7he", ",", "da", "brach", "dein", "Trotz", "!", "Hold\u00b7se\u00b7li\u00b7ge", ",", "mu\u00df\u00b7test", "du", "l\u00e4ngst", "nicht"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "$.", "NN", "$,", "VMFIN", "PPER", "ADV", "PTKNEG"], "meter": "-+-+-++-+-+-+-+", "measure": "unknown.measure.octa.plus"}, "line.18": {"text": "F\u00fchlen, wie m\u00e4chtig das Weib herrsche durch zartere Huld?", "tokens": ["F\u00fch\u00b7len", ",", "wie", "m\u00e4ch\u00b7tig", "das", "Weib", "herr\u00b7sche", "durch", "zar\u00b7te\u00b7re", "Huld", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ADJD", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.19": {"text": "Traulicher nahtest du jetzt, halb bogst du die Wange, doch abw\u00e4rts", "tokens": ["Trau\u00b7li\u00b7cher", "nah\u00b7test", "du", "jetzt", ",", "halb", "bogst", "du", "die", "Wan\u00b7ge", ",", "doch", "ab\u00b7w\u00e4rts"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ADV", "$,", "ADJD", "VVFIN", "PPER", "ART", "NN", "$,", "ADV", "ADV"], "meter": "+--+-+-+-+-+--+-", "measure": "iambic.septa.invert"}, "line.20": {"text": "Schaute der Blick noch, und ich nahete leise wie du,", "tokens": ["Schau\u00b7te", "der", "Blick", "noch", ",", "und", "ich", "na\u00b7he\u00b7te", "lei\u00b7se", "wie", "du", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "$,", "KON", "PPER", "VVFIN", "ADJD", "KOKOM", "PPER", "$,"], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.21": {"text": "Und wir f\u00fchlten den Ku\u00df, fast eh wir ihn sahen, und nicht mehr", "tokens": ["Und", "wir", "f\u00fchl\u00b7ten", "den", "Ku\u00df", ",", "fast", "eh", "wir", "ihn", "sa\u00b7hen", ",", "und", "nicht", "mehr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,", "ADV", "KOUS", "PPER", "PPER", "VVFIN", "$,", "KON", "PTKNEG", "ADV"], "meter": "--+--+--+-+-+-+", "measure": "anapaest.tri.plus"}, "line.22": {"text": "Schmerzte die Stirn dir, und mir schmerzte nicht ferner das Herz.", "tokens": ["Schmerz\u00b7te", "die", "Stirn", "dir", ",", "und", "mir", "schmerz\u00b7te", "nicht", "fer\u00b7ner", "das", "Herz", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "$,", "KON", "PPER", "VVFIN", "PTKNEG", "ADV", "ART", "NN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}}}}}