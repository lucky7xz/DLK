{"textgrid.poem.33332": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "An den Mond", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr Mond, von mir erwart' er nicht,", "tokens": ["Herr", "Mond", ",", "von", "mir", "er\u00b7wart'", "er", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "APPR", "PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ich nach Dichterweise", "tokens": ["Da\u00df", "ich", "nach", "Dich\u00b7ter\u00b7wei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nun auch sein Alletagsgesicht", "tokens": ["Nun", "auch", "sein", "Al\u00b7le\u00b7tags\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus vollen Backen preise.", "tokens": ["Aus", "vol\u00b7len", "Ba\u00b7cken", "prei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich habe lang ihn observirt,", "tokens": ["Ich", "ha\u00b7be", "lang", "ihn", "ob\u00b7ser\u00b7virt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und wahrlich wenig ausgesp\u00fcrt,", "tokens": ["Und", "wahr\u00b7lich", "we\u00b7nig", "aus\u00b7ge\u00b7sp\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Was ihm gedieh' zur Ehre,", "tokens": ["Was", "ihm", "ge\u00b7dieh'", "zur", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und lobensw\u00fcrdig w\u00e4re.", "tokens": ["Und", "lo\u00b7bens\u00b7w\u00fcr\u00b7dig", "w\u00e4\u00b7re", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da pflegt er, wie ein kleines Kind,", "tokens": ["Da", "pflegt", "er", ",", "wie", "ein", "klei\u00b7nes", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit seinem Licht zu prahlen;", "tokens": ["Mit", "sei\u00b7nem", "Licht", "zu", "prah\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Allein, man wei\u00df ja wohl, es sind", "tokens": ["Al\u00b7lein", ",", "man", "wei\u00df", "ja", "wohl", ",", "es", "sind"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "$,", "PIS", "VVFIN", "ADV", "ADV", "$,", "PPER", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur seines Weibes Strahlen.", "tokens": ["Nur", "sei\u00b7nes", "Wei\u00b7bes", "Strah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "W\u00e4r' nicht sein Weib, es ging ihm dann", "tokens": ["W\u00e4r'", "nicht", "sein", "Weib", ",", "es", "ging", "ihm", "dann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gewi\u00df wie manchem Ehemann,", "tokens": ["Ge\u00b7wi\u00df", "wie", "man\u00b7chem", "E\u00b7he\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Den Niemand regardirte,", "tokens": ["Den", "Nie\u00b7mand", "re\u00b7gar\u00b7dir\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wenn nicht sein Weib brillirte.", "tokens": ["Wenn", "nicht", "sein", "Weib", "bril\u00b7lir\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und glaub' er ja nicht, da\u00df dies Licht", "tokens": ["Und", "glaub'", "er", "ja", "nicht", ",", "da\u00df", "dies", "Licht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKNEG", "$,", "KOUS", "PDS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihn so besonders kleide;", "tokens": ["Ihn", "so", "be\u00b7son\u00b7ders", "klei\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er hat darin ein bleich' Gesicht,", "tokens": ["Er", "hat", "da\u00b7rin", "ein", "bleich'", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als w\u00e4r's gemalt mit Kreide,", "tokens": ["Als", "w\u00e4r's", "ge\u00b7malt", "mit", "Krei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und gleichet dann bald einem Stier,", "tokens": ["Und", "glei\u00b7chet", "dann", "bald", "ei\u00b7nem", "Stier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bald einem Becken vom Barbier,", "tokens": ["Bald", "ei\u00b7nem", "Be\u00b7cken", "vom", "Bar\u00b7bier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und wird er voll und heller,", "tokens": ["Und", "wird", "er", "voll", "und", "hel\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Gar einem Suppenteller.", "tokens": ["Gar", "ei\u00b7nem", "Sup\u00b7pen\u00b7tel\u00b7ler", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Mit seinem Weib f\u00fchrt er von je", "tokens": ["Mit", "sei\u00b7nem", "Weib", "f\u00fchrt", "er", "von", "je"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "APPR", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein skandal\u00f6ses Leben;", "tokens": ["Ein", "skan\u00b7da\u00b7l\u00f6\u00b7ses", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kann man den M\u00e4nnern in der Eh'", "tokens": ["Kann", "man", "den", "M\u00e4n\u00b7nern", "in", "der", "Eh'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "ART", "NN", "APPR", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Ein schlechter Beispiel geben?", "tokens": ["Ein", "schlech\u00b7ter", "Bei\u00b7spiel", "ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Kaum k\u00f6mmt Madam nach Haus, so rennt", "tokens": ["Kaum", "k\u00f6mmt", "Ma\u00b7dam", "nach", "Haus", ",", "so", "rennt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "APPR", "NN", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er fort, und geht am Firmament", "tokens": ["Er", "fort", ",", "und", "geht", "am", "Fir\u00b7ma\u00b7ment"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "PTKVZ", "$,", "KON", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die ganze Nacht spazieren,", "tokens": ["Die", "gan\u00b7ze", "Nacht", "spa\u00b7zie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Um sie nicht zu geniren.", "tokens": ["Um", "sie", "nicht", "zu", "ge\u00b7ni\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.5": {"line.1": {"text": "Kein Hahnrei noch auf Erden war", "tokens": ["Kein", "Hahn\u00b7rei", "noch", "auf", "Er\u00b7den", "war"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "ADV", "APPR", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So ein publiker Lappe.", "tokens": ["So", "ein", "pub\u00b7li\u00b7ker", "Lap\u00b7pe", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Oft steckt er seinen Hauptschmuck zwar", "tokens": ["Oft", "steckt", "er", "sei\u00b7nen", "Haupt\u00b7schmuck", "zwar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In eine Nebelkappe;", "tokens": ["In", "ei\u00b7ne", "Ne\u00b7bel\u00b7kap\u00b7pe", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Allein vergi\u00dft er die zu Haus,", "tokens": ["Al\u00b7lein", "ver\u00b7gi\u00dft", "er", "die", "zu", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So geht er auch mit H\u00f6rnern aus,", "tokens": ["So", "geht", "er", "auch", "mit", "H\u00f6r\u00b7nern", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df manchen, die ihn sehen,", "tokens": ["Da\u00df", "man\u00b7chen", ",", "die", "ihn", "se\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Augen d'rob vergehen.", "tokens": ["Die", "Au\u00b7gen", "d'\u00b7rob", "ver\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Und macht Madam ihm dann und wann", "tokens": ["Und", "macht", "Ma\u00b7dam", "ihm", "dann", "und", "wann"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "NN", "PPER", "ADV", "KON", "PWAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu Haus zu viele Schw\u00e4nke,", "tokens": ["Zu", "Haus", "zu", "vie\u00b7le", "Schw\u00e4n\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So geht er, wie so mancher Mann,", "tokens": ["So", "geht", "er", ",", "wie", "so", "man\u00b7cher", "Mann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In der Frau Thetis Schenke,", "tokens": ["In", "der", "Frau", "The\u00b7tis", "Schen\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ers\u00e4uft im Meere seinen Groll,", "tokens": ["Er\u00b7s\u00e4uft", "im", "Mee\u00b7re", "sei\u00b7nen", "Groll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und k\u00f6mmt nicht selten toll und voll", "tokens": ["Und", "k\u00f6mmt", "nicht", "sel\u00b7ten", "toll", "und", "voll"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "ADJD", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zur\u00fcck vom vollen Glase", "tokens": ["Zu\u00b7r\u00fcck", "vom", "vol\u00b7len", "Gla\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mit einer Kupfernase.", "tokens": ["Mit", "ei\u00b7ner", "Kup\u00b7fer\u00b7na\u00b7se", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Bei all' dem Hauskreuz sucht er doch", "tokens": ["Bei", "all'", "dem", "Haus\u00b7kreuz", "sucht", "er", "doch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "ART", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Stets Herzen zu erweichen,", "tokens": ["Stets", "Her\u00b7zen", "zu", "er\u00b7wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und ist nebst allem diesem noch", "tokens": ["Und", "ist", "nebst", "al\u00b7lem", "die\u00b7sem", "noch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "PIS", "PDAT", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Kuppler ohne gleichen:", "tokens": ["Ein", "Kupp\u00b7ler", "oh\u00b7ne", "glei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er h\u00e4lt dem liebenden Gez\u00fccht", "tokens": ["Er", "h\u00e4lt", "dem", "lie\u00b7ben\u00b7den", "Ge\u00b7z\u00fccht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bei dunkler Nacht so lang das Licht,", "tokens": ["Bei", "dunk\u00b7ler", "Nacht", "so", "lang", "das", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Bis oft die guten Lappen", "tokens": ["Bis", "oft", "die", "gu\u00b7ten", "Lap\u00b7pen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Aus Inbrunst sich verschnappen.", "tokens": ["Aus", "In\u00b7brunst", "sich", "ver\u00b7schnap\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "VVINF", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.8": {"line.1": {"text": "Und dieser Liebeshehlerei", "tokens": ["Und", "die\u00b7ser", "Lie\u00b7bes\u00b7heh\u00b7le\u00b7rei"], "token_info": ["word", "word", "word"], "pos": ["KON", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geheimer Liebsgeschichtchen", "tokens": ["Ge\u00b7hei\u00b7mer", "Liebs\u00b7ge\u00b7schicht\u00b7chen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verdankt er manche R\u00e4umerei,", "tokens": ["Ver\u00b7dankt", "er", "man\u00b7che", "R\u00e4u\u00b7me\u00b7rei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und manches Lobgedichtchen;", "tokens": ["Und", "man\u00b7ches", "Lob\u00b7ge\u00b7dicht\u00b7chen", ";"], "token_info": ["word", "word", 