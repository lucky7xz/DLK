{"dta.poem.19503": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Das Gl\u00f6cklein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Er steht an ihrem Pf\u00fchl in herber Qual,", "tokens": ["Er", "steht", "an", "ih\u00b7rem", "Pf\u00fchl", "in", "her\u00b7ber", "Qual", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den jungen Busen mu\u00df er keuchen sehn \u2014", "tokens": ["Den", "jun\u00b7gen", "Bu\u00b7sen", "mu\u00df", "er", "keu\u00b7chen", "sehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er ist ein Arzt. Er wei\u00df, sein traut Gemahl", "tokens": ["Er", "ist", "ein", "Arzt", ".", "Er", "wei\u00df", ",", "sein", "traut", "Ge\u00b7mahl"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "PPER", "VVFIN", "$,", "PPOSAT", "VVFIN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Erbla\u00dft, sobald die Morgenschauer wehn.", "tokens": ["Er\u00b7bla\u00dft", ",", "so\u00b7bald", "die", "Mor\u00b7gen\u00b7schau\u00b7er", "wehn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sie hat ", "tokens": ["Sie", "hat"], "token_info": ["word", "word"], "pos": ["PPER", "VAFIN"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Mir tr\u00e4umte, da\u00df ich auf der Alpe war,", "tokens": ["Mir", "tr\u00e4um\u00b7te", ",", "da\u00df", "ich", "auf", "der", "Al\u00b7pe", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie sch\u00f6n mir tr\u00e4umte, das erz\u00e4hl ich dir \u2014", "tokens": ["Wie", "sch\u00f6n", "mir", "tr\u00e4um\u00b7te", ",", "das", "er\u00b7z\u00e4hl", "ich", "dir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "VVFIN", "$,", "PDS", "VVFIN", "PPER", "PPER", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Du schickst mich wieder hin das n\u00e4chste Jahr!", "tokens": ["Du", "schickst", "mich", "wie\u00b7der", "hin", "das", "n\u00e4chs\u00b7te", "Jahr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dort vor dem Dorf \u2014 du wei\u00dft den moos'gen Stein \u2014", "tokens": ["Dort", "vor", "dem", "Dorf", "du", "wei\u00dft", "den", "moos'\u00b7gen", "Stein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$(", "PPER", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Sa\u00df ich, umhallt von lauter Heerdget\u00f6n,", "tokens": ["Sa\u00df", "ich", ",", "um\u00b7hallt", "von", "lau\u00b7ter", "Heerd\u00b7ge\u00b7t\u00f6n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "An mir vor\u00fcber zogen mit Schalmein", "tokens": ["An", "mir", "vor\u00b7\u00fc\u00b7ber", "zo\u00b7gen", "mit", "Schal\u00b7mein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ADV", "VVFIN", "APPR", "NN"], "meter": "---+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die Heerden nieder von den Sommerh\u00f6hn.", "tokens": ["Die", "Heer\u00b7den", "nie\u00b7der", "von", "den", "Som\u00b7mer\u00b7h\u00f6hn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die Heerden kehren alle heut nach Haus \u2014", "tokens": ["Die", "Heer\u00b7den", "keh\u00b7ren", "al\u00b7le", "heut", "nach", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "ADV", "APPR", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das ist die letzte wohl? Nein, eine noch!", "tokens": ["Das", "ist", "die", "letz\u00b7te", "wohl", "?", "Nein", ",", "ei\u00b7ne", "noch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "ADV", "$.", "PTKANT", "$,", "ART", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Noch ein Gel\u00e4ut klingt an und eins klingt aus!", "tokens": ["Noch", "ein", "Ge\u00b7l\u00e4ut", "klingt", "an", "und", "eins", "klingt", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PTKVZ", "KON", "PIS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das endet nicht! Da kam das letzte doch!", "tokens": ["Das", "en\u00b7det", "nicht", "!", "Da", "kam", "das", "letz\u00b7te", "doch", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKNEG", "$.", "ADV", "VVFIN", "ART", "ADJA", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Mich \u00fcberfluthet' fliehend Abendroth,", "tokens": ["Mich", "\u00fc\u00b7ber\u00b7flu\u00b7thet'", "flie\u00b7hend", "A\u00b7ben\u00b7droth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Matten dunkelten so gr\u00fcn und rein,", "tokens": ["Die", "Mat\u00b7ten", "dun\u00b7kel\u00b7ten", "so", "gr\u00fcn", "und", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Firne brannten still \u2014 und lagen todt,", "tokens": ["Die", "Fir\u00b7ne", "brann\u00b7ten", "still", "und", "la\u00b7gen", "todt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$(", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Dar\u00fcber glomm ein leiser Sternenschein \u2014", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "glomm", "ein", "lei\u00b7ser", "Ster\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Da horch! ein Gl\u00f6cklein noch aus finst'rer Schlucht,", "tokens": ["Da", "horch", "!", "ein", "Gl\u00f6c\u00b7klein", "noch", "aus", "finst'\u00b7rer", "Schlucht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$.", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verirrt, versp\u00e4tet, wandert's ohne Ruh,", "tokens": ["Ver\u00b7irrt", ",", "ver\u00b7sp\u00e4\u00b7tet", ",", "wan\u00b7dert's", "oh\u00b7ne", "Ruh", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein armes Gl\u00f6cklein, das die Heerde sucht \u2014", "tokens": ["Ein", "ar\u00b7mes", "Gl\u00f6c\u00b7klein", ",", "das", "die", "Heer\u00b7de", "sucht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Auf wacht' ich dann und bei mir warest du!", "tokens": ["Auf", "wacht'", "ich", "dann", "und", "bei", "mir", "wa\u00b7rest", "du", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "ADV", "KON", "APPR", "PPER", "VAFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Mann, schick mich wieder auf die lieben H\u00f6hn \u2014", "tokens": ["Mann", ",", "schick", "mich", "wie\u00b7der", "auf", "die", "lie\u00b7ben", "H\u00f6hn"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sie haben, sagst du, mich gesund gemacht ...", "tokens": ["Sie", "ha\u00b7ben", ",", "sagst", "du", ",", "mich", "ge\u00b7sund", "ge\u00b7macht", "..."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "VVFIN", "PPER", "$,", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dort war es sch\u00f6n! Dort war es wundersch\u00f6n!", "tokens": ["Dort", "war", "es", "sch\u00f6n", "!", "Dort", "war", "es", "wun\u00b7der\u00b7sch\u00f6n", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$.", "ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das Gl\u00f6cklein! Wieder! H\u00f6rst du's? Gute Nacht ...\u201c", "tokens": ["Das", "Gl\u00f6c\u00b7klein", "!", "Wie\u00b7der", "!", "H\u00f6rst", "du's", "?", "Gu\u00b7te", "Nacht", "...", "\u201c"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$.", "ADV", "$.", "VVFIN", "PIS", "$.", "ADJA", "NN", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}