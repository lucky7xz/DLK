{"textgrid.poem.63139": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "33", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Dichters Mutter liegt vor dir im Staube,", "tokens": ["Des", "Dich\u00b7ters", "Mut\u00b7ter", "liegt", "vor", "dir", "im", "Stau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Maria, hohe Himmelsk\u00f6nigin,", "tokens": ["Ma\u00b7ria", ",", "ho\u00b7he", "Him\u00b7mels\u00b7k\u00f6\u00b7ni\u00b7gin", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Du bist mein Schild, mein Baldachin, mein Glaube,", "tokens": ["Du", "bist", "mein", "Schild", ",", "mein", "Bal\u00b7da\u00b7ch\u00b7in", ",", "mein", "Glau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Die ich um meinen Sohn voll Schmerzen bin.", "tokens": ["Die", "ich", "um", "mei\u00b7nen", "Sohn", "voll", "Schmer\u00b7zen", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "ADJD", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Als einst die Welt versank, sandt Noah eine Taube", "tokens": ["Als", "einst", "die", "Welt", "ver\u00b7sank", ",", "sandt", "Noah", "ei\u00b7ne", "Tau\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$,", "VVFIN", "NE", "ART", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Mit einem \u00d6lzweig \u00fcbers Wasser hin.", "tokens": ["Mit", "ei\u00b7nem", "\u00d6l\u00b7zweig", "\u00fc\u00b7bers", "Was\u00b7ser", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich sende dies Gebet: f\u00fcr meinen Knaben,", "tokens": ["Ich", "sen\u00b7de", "dies", "Ge\u00b7bet", ":", "f\u00fcr", "mei\u00b7nen", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "NN", "$.", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Den alle Furien zerrissen haben.", "tokens": ["Den", "al\u00b7le", "Fu\u00b7ri\u00b7en", "zer\u00b7ris\u00b7sen", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nichts will ich f\u00fcr mich selbst als seinen Frieden.", "tokens": ["Nichts", "will", "ich", "f\u00fcr", "mich", "selbst", "als", "sei\u00b7nen", "Frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "APPR", "PPER", "ADV", "KOUS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich lebe nur, weil mich sein Anblick h\u00e4lt.", "tokens": ["Ich", "le\u00b7be", "nur", ",", "weil", "mich", "sein", "An\u00b7blick", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "W\u00e4r ihm ein sanftes Eheweib beschieden", "tokens": ["W\u00e4r", "ihm", "ein", "sanf\u00b7tes", "E\u00b7he\u00b7weib", "be\u00b7schie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In einer kleinen, aber guten Welt!", "tokens": ["In", "ei\u00b7ner", "klei\u00b7nen", ",", "a\u00b7ber", "gu\u00b7ten", "Welt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch seine Sehnsucht seh ich zischend sieden.", "tokens": ["Doch", "sei\u00b7ne", "Sehn\u00b7sucht", "seh", "ich", "zi\u00b7schend", "sie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Er hustet Blut \u2013 und seine Stimme gellt.", "tokens": ["Er", "hus\u00b7tet", "Blut", "\u2013", "und", "sei\u00b7ne", "Stim\u00b7me", "gellt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$(", "KON", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Er w\u00fcnscht voll gl\u00fccklicher Gerechtigkeiten,", "tokens": ["Er", "w\u00fcnscht", "voll", "gl\u00fcck\u00b7li\u00b7cher", "Ge\u00b7rech\u00b7tig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die Menschen zur Vollkommenheit zu leiten.", "tokens": ["Die", "Men\u00b7schen", "zur", "Voll\u00b7kom\u00b7men\u00b7heit", "zu", "lei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch ist er herrisch. Und im Trotz entweiht er", "tokens": ["Doch", "ist", "er", "her\u00b7risch", ".", "Und", "im", "Trotz", "ent\u00b7weiht", "er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "$.", "KON", "APPRART", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Altar und Dom mit roher Rede Flu\u00df.", "tokens": ["Al\u00b7tar", "und", "Dom", "mit", "ro\u00b7her", "Re\u00b7de", "Flu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er steigt in N\u00e4chten auf die Himmelsleiter,", "tokens": ["Er", "steigt", "in", "N\u00e4ch\u00b7ten", "auf", "die", "Him\u00b7mels\u00b7lei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Weil er mit seinem Gotte ringen mu\u00df.", "tokens": ["Weil", "er", "mit", "sei\u00b7nem", "Got\u00b7te", "rin\u00b7gen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Er ist kein gegen S\u00fcnd und Zorn gefeiter,", "tokens": ["Er", "ist", "kein", "ge\u00b7gen", "S\u00fcnd", "und", "Zorn", "ge\u00b7fei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "APPR", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Gefeit nicht gegen W\u00fcrfelspiel und Ku\u00df.", "tokens": ["Ge\u00b7feit", "nicht", "ge\u00b7gen", "W\u00fcr\u00b7fel\u00b7spiel", "und", "Ku\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Doch h\u00f6rt ich, da\u00df selbst Theophil gerettet,", "tokens": ["Doch", "h\u00f6rt", "ich", ",", "da\u00df", "selbst", "Theo\u00b7phil", "ge\u00b7ret\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "ADV", "NE", "VVPP", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Ob er sich gleich dem Teufel angekettet.", "tokens": ["Ob", "er", "sich", "gleich", "dem", "Teu\u00b7fel", "an\u00b7ge\u00b7ket\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich bin ein armes Weib und ohne Wissen,", "tokens": ["Ich", "bin", "ein", "ar\u00b7mes", "Weib", "und", "oh\u00b7ne", "Wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich wei\u00df nur, da\u00df auch du einst Mutter warst,", "tokens": ["Ich", "wei\u00df", "nur", ",", "da\u00df", "auch", "du", "einst", "Mut\u00b7ter", "warst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "ADV", "PPER", "ADV", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als du von Kr\u00e4mpfen und von Wehn zerrissen", "tokens": ["Als", "du", "von", "Kr\u00e4mp\u00b7fen", "und", "von", "Wehn", "zer\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "KON", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Herrn Jesum, unsern Heiland, uns gebarst.", "tokens": ["Herrn", "Je\u00b7sum", ",", "un\u00b7sern", "Hei\u00b7land", ",", "uns", "ge\u00b7barst", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PPOSAT", "NN", "$,", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "La\u00df deine F\u00fc\u00dfe, M\u00fctterchen, mich k\u00fcssen,", "tokens": ["La\u00df", "dei\u00b7ne", "F\u00fc\u00b7\u00dfe", ",", "M\u00fct\u00b7ter\u00b7chen", ",", "mich", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "$,", "NN", "$,", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und dich erflehn, da\u00df meinen Sohn du scharst", "tokens": ["Und", "dich", "er\u00b7flehn", ",", "da\u00df", "mei\u00b7nen", "Sohn", "du", "scharst"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVINF", "$,", "KOUS", "PPOSAT", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "In jenen Reigen englischer Gestalten,", "tokens": ["In", "je\u00b7nen", "Rei\u00b7gen", "eng\u00b7li\u00b7scher", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die deines Kleides goldne Schleppe halten.", "tokens": ["Die", "dei\u00b7nes", "Klei\u00b7des", "gold\u00b7ne", "Schlep\u00b7pe", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}