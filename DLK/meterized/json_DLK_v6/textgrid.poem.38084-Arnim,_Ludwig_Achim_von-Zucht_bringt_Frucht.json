{"textgrid.poem.38084": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Zucht bringt Frucht", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es flohen drei Sterne wohl \u00fcber den Rhein,", "tokens": ["Es", "flo\u00b7hen", "drei", "Ster\u00b7ne", "wohl", "\u00fc\u00b7ber", "den", "Rhein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "NN", "ADV", "APPR", "ART", "NE", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Es h\u00e4tt' eine Wittwe drey T\u00f6chterlein;", "tokens": ["Es", "h\u00e4tt'", "ei\u00b7ne", "Witt\u00b7we", "drey", "T\u00f6ch\u00b7ter\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "CARD", "NN", "$."], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "Die eine starb wie es Abend war", "tokens": ["Die", "ei\u00b7ne", "starb", "wie", "es", "A\u00b7bend", "war"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "KOKOM", "PPER", "NN", "VAFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und die Sonne nicht mehr schiene klar,", "tokens": ["Und", "die", "Son\u00b7ne", "nicht", "mehr", "schie\u00b7ne", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKNEG", "ADV", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Die Andre um die Mitternacht,", "tokens": ["Die", "And\u00b7re", "um", "die", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Dritte um die Morgenwacht.", "tokens": ["Die", "Drit\u00b7te", "um", "die", "Mor\u00b7gen\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie nahmen sich all einander die H\u00e4nd", "tokens": ["Sie", "nah\u00b7men", "sich", "all", "ein\u00b7an\u00b7der", "die", "H\u00e4nd"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "PIAT", "PRF", "ART", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und kamen vor den Himmel behend,", "tokens": ["Und", "ka\u00b7men", "vor", "den", "Him\u00b7mel", "be\u00b7hend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie klopften leise an die Th\u00fcr,", "tokens": ["Sie", "klopf\u00b7ten", "lei\u00b7se", "an", "die", "Th\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sankt Petrus sprach: Wer ist daf\u00fcr?", "tokens": ["Sankt", "Pet\u00b7rus", "sprach", ":", "Wer", "ist", "da\u00b7f\u00fcr", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "FM", "VVFIN", "$.", "PWS", "VAFIN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es stehn drei arme Seelen hier,", "tokens": ["Es", "stehn", "drei", "ar\u00b7me", "See\u00b7len", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ach, macht bald auf die Himmelsth\u00fcr.", "tokens": ["Ach", ",", "macht", "bald", "auf", "die", "Him\u00b7mel\u00b7sth\u00fcr", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Er sprach: Ich mu\u00df erst zeigen an,", "tokens": ["Er", "sprach", ":", "Ich", "mu\u00df", "erst", "zei\u00b7gen", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VMFIN", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Welch' von euch soll in Himmel gahn,", "tokens": ["Welch'", "von", "euch", "soll", "in", "Him\u00b7mel", "gahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "APPR", "PPER", "VMFIN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Drauf ging er hin und fragte nach,", "tokens": ["Drauf", "ging", "er", "hin", "und", "frag\u00b7te", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Himmelsstimme also sprach:", "tokens": ["Die", "Him\u00b7mels\u00b7stim\u00b7me", "al\u00b7so", "sprach", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die \u00e4ltsten zwei sollen hier ein gehn,", "tokens": ["Die", "\u00e4lts\u00b7ten", "zwei", "sol\u00b7len", "hier", "ein", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "CARD", "VMFIN", "ADV", "ART", "VVINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die j\u00fcngste mu\u00df bleiben stehn.", "tokens": ["Die", "j\u00fcngs\u00b7te", "mu\u00df", "blei\u00b7ben", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VMFIN", "VVINF", "VVINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Sie schrie und sprach: Was hab ich gethan,", "tokens": ["Sie", "schrie", "und", "sprach", ":", "Was", "hab", "ich", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$.", "PWS", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Da\u00df ich hier bleiben soll bestahn?", "tokens": ["Da\u00df", "ich", "hier", "blei\u00b7ben", "soll", "be\u00b7stahn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sankt Petrus sprach: Weil du veracht", "tokens": ["Sankt", "Pet\u00b7rus", "sprach", ":", "Weil", "du", "ver\u00b7acht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "FM", "VVFIN", "$.", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gotts Wort, deine Seele nicht bedacht,", "tokens": ["Gotts", "Wort", ",", "dei\u00b7ne", "See\u00b7le", "nicht", "be\u00b7dacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPOSAT", "NN", "PTKNEG", "VVPP", "$,"], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.5": {"text": "So geh nun hin und siehe zu,", "tokens": ["So", "geh", "nun", "hin", "und", "sie\u00b7he", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PTKVZ", "KON", "VVIMP", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wo du findest in der H\u00f6llen Ruh.", "tokens": ["Wo", "du", "fin\u00b7dest", "in", "der", "H\u00f6l\u00b7len", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Denn wenn du in die Kirch solltst gehn,", "tokens": ["Denn", "wenn", "du", "in", "die", "Kirch", "solltst", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So bliebst du vor dem Spiegel stehn,", "tokens": ["So", "bliebst", "du", "vor", "dem", "Spie\u00b7gel", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Haupt bekr\u00f6nt, dein Haar geschmiert,", "tokens": ["Dein", "Haupt", "be\u00b7kr\u00f6nt", ",", "dein", "Haar", "ge\u00b7schmiert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und dich hoff\u00e4rtig ausgeziert:", "tokens": ["Und", "dich", "hof\u00b7f\u00e4r\u00b7tig", "aus\u00b7ge\u00b7ziert", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Drum geh nur fort und packe dich,", "tokens": ["Drum", "geh", "nur", "fort", "und", "pa\u00b7cke", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "PTKVZ", "KON", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die H\u00f6lle wird aufnehmen dich!", "tokens": ["Die", "H\u00f6l\u00b7le", "wird", "auf\u00b7neh\u00b7men", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVINF", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Als sie nun vor die H\u00f6lle kam,", "tokens": ["Als", "sie", "nun", "vor", "die", "H\u00f6l\u00b7le", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da klopfte sie gar grausam an,", "tokens": ["Da", "klopf\u00b7te", "sie", "gar", "grau\u00b7sam", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Satan sprach: Wer ist allhier?", "tokens": ["Der", "Sa\u00b7tan", "sprach", ":", "Wer", "ist", "all\u00b7hier", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PWS", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es ist eine arme Seel daf\u00fcr!", "tokens": ["Es", "ist", "ei\u00b7ne", "ar\u00b7me", "Seel", "da\u00b7f\u00fcr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "PAV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Drauf sprang er auf, und lie\u00df sie ein,", "tokens": ["Drauf", "sprang", "er", "auf", ",", "und", "lie\u00df", "sie", "ein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und schenkt ihr ein ein gl\u00fchnden Wein.", "tokens": ["Und", "schenkt", "ihr", "ein", "ein", "gl\u00fchn\u00b7den", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Als sie nun aus dem Becher trank,", "tokens": ["Als", "sie", "nun", "aus", "dem", "Be\u00b7cher", "trank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Das Blut ihr aus den N\u00e4geln sprang,", "tokens": ["Das", "Blut", "ihr", "aus", "den", "N\u00e4\u00b7geln", "sprang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er bracht sie in den h\u00f6llischen Pfuhl,", "tokens": ["Er", "bracht", "sie", "in", "den", "h\u00f6l\u00b7li\u00b7schen", "Pfuhl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und sezt sie auf ein gl\u00fchenden Stuhl,", "tokens": ["Und", "sezt", "sie", "auf", "ein", "gl\u00fc\u00b7hen\u00b7den", "Stuhl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Ja ihre Qual war \u00fcbergro\u00df,", "tokens": ["Ja", "ih\u00b7re", "Qual", "war", "\u00fc\u00b7ber\u00b7gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie kriegte manchen harten Sto\u00df.", "tokens": ["Sie", "krieg\u00b7te", "man\u00b7chen", "har\u00b7ten", "Sto\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Sie sprach: Das ist mein Mutter Schuld,", "tokens": ["Sie", "sprach", ":", "Das", "ist", "mein", "Mut\u00b7ter", "Schuld", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PDS", "VAFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df sie mein Bosheit hat erduldt,", "tokens": ["Da\u00df", "sie", "mein", "Bos\u00b7heit", "hat", "er\u00b7duldt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und mich in Frevel lassen gehn,", "tokens": ["Und", "mich", "in", "Fre\u00b7vel", "las\u00b7sen", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht einmal sauer drum gesehn,", "tokens": ["Nicht", "ein\u00b7mal", "sau\u00b7er", "drum", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJD", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da meine Schwestern im Himmelssaal,", "tokens": ["Da", "mei\u00b7ne", "Schwes\u00b7tern", "im", "Him\u00b7mels\u00b7saal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "So siz ich in der H\u00f6llen-Qual.", "tokens": ["So", "siz", "ich", "in", "der", "H\u00f6l\u00b7len\u00b7Qual", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Was hilft mir nun mein Uebermuth,", "tokens": ["Was", "hilft", "mir", "nun", "mein", "Ue\u00b7ber\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Reichthum, Ehre, Geld und Gut?", "tokens": ["Mein", "Reicht\u00b7hum", ",", "Eh\u00b7re", ",", "Geld", "und", "Gut", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was hilft mir nun all Zierd und Pracht?", "tokens": ["Was", "hilft", "mir", "nun", "all", "Zierd", "und", "Pracht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ach h\u00e4tt' ich nie daran gedacht!", "tokens": ["Ach", "h\u00e4tt'", "ich", "nie", "da\u00b7ran", "ge\u00b7dacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VAFIN", "PPER", "ADV", "PAV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So s\u00e4\u00df ich nicht in dieser Flammen,", "tokens": ["So", "s\u00e4\u00df", "ich", "nicht", "in", "die\u00b7ser", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da alle Qualen schlagen zusammen.", "tokens": ["Da", "al\u00b7le", "Qua\u00b7len", "schla\u00b7gen", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}