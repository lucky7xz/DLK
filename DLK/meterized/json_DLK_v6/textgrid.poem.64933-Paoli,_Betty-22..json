{"textgrid.poem.64933": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "22.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Born, d'raus Reiz und Tod in gleichem Schwalle quillt,", "tokens": ["Du", "Born", ",", "d'raus", "Reiz", "und", "Tod", "in", "glei\u00b7chem", "Schwal\u00b7le", "quillt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "$,", "ADV", "NN", "KON", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Womit vergleich ich dich? was gibt von dir ein Bild?", "tokens": ["Wo\u00b7mit", "ver\u00b7gleich", "ich", "dich", "?", "was", "gibt", "von", "dir", "ein", "Bild", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "PRF", "$.", "PWS", "VVFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dein wahrstes Bild seh ich in einem Dolch sich malen,", "tokens": ["Dein", "wahrs\u00b7tes", "Bild", "seh", "ich", "in", "ei\u00b7nem", "Dolch", "sich", "ma\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An dessen Griff Demant und Perlen leuchtend strahlen.", "tokens": ["An", "des\u00b7sen", "Griff", "De\u00b7mant", "und", "Per\u00b7len", "leuch\u00b7tend", "strah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN