{"dta.poem.7074": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mit welchem Vorrath ew\u2019ger Lust,", "tokens": ["Mit", "wel\u00b7chem", "Vor\u00b7rath", "ew'\u00b7ger", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In den Erfahrungen von ihres Sch\u00f6pfers Willen,", "tokens": ["In", "den", "Er\u00b7fah\u00b7run\u00b7gen", "von", "ih\u00b7res", "Sch\u00f6p\u00b7fers", "Wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.3": {"text": "Wird sich ihr gantzes Gantz dadurch erf\u00fcllen!", "tokens": ["Wird", "sich", "ihr", "gant\u00b7zes", "Gantz", "da\u00b7durch", "er\u00b7f\u00fcl\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "PPOSAT", "ADJA", "NN", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn sie nur blo\u00df die Huld, und die Gerechtigkeit,", "tokens": ["Wenn", "sie", "nur", "blo\u00df", "die", "Huld", ",", "und", "die", "Ge\u00b7rech\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ART", "NN", "$,", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Durch welche GOTT die Welt regieret,", "tokens": ["Durch", "wel\u00b7che", "GoTT", "die", "Welt", "re\u00b7gie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zu welchem Zweck er alles das gef\u00fchret,", "tokens": ["Zu", "wel\u00b7chem", "Zweck", "er", "al\u00b7les", "das", "ge\u00b7f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "PPER", "PIAT", "PDS", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Was ihnen vormahls in der Zeit", "tokens": ["Was", "ih\u00b7nen", "vor\u00b7mahls", "in", "der", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nicht billig schien, Verwund\u2019rungs voll erwegen,", "tokens": ["Nicht", "bil\u00b7lig", "schien", ",", "Ver\u00b7wun\u00b7d'\u00b7rungs", "voll", "er\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVFIN", "$,", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Wenn sie erstaunet \u00fcberlegen,", "tokens": ["Wenn", "sie", "er\u00b7stau\u00b7net", "\u00fc\u00b7berl\u00b7e\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Wie ihr vorher verfinsterter Verstand,", "tokens": ["Wie", "ihr", "vor\u00b7her", "ver\u00b7fins\u00b7ter\u00b7ter", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.11": {"text": "So offtmahls B\u00f6ses Gut, und Gutes B\u00f6s genannt,", "tokens": ["So", "offt\u00b7mahls", "B\u00f6\u00b7ses", "Gut", ",", "und", "Gu\u00b7tes", "B\u00f6s", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "NN", "$,", "KON", "NN", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wovon sie nun in stiller Andacht lesen,", "tokens": ["Wo\u00b7von", "sie", "nun", "in", "stil\u00b7ler", "An\u00b7dacht", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Wie herrlich allezeit die Absicht sey gewesen.", "tokens": ["Wie", "herr\u00b7lich", "al\u00b7le\u00b7zeit", "die", "Ab\u00b7sicht", "sey", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "ART", "NN", "VAFIN", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ja, wie sich, durch ein seeligs Ueberlegen", "tokens": ["Ja", ",", "wie", "sich", ",", "durch", "ein", "see\u00b7ligs", "Ue\u00b7ber\u00b7le\u00b7gen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PWAV", "PRF", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Der F\u00fchrung, welche GOTT blo\u00df ihrentwegen", "tokens": ["Der", "F\u00fch\u00b7rung", ",", "wel\u00b7che", "GoTT", "blo\u00df", "ih\u00b7rent\u00b7we\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "NE", "ADV", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Hier in der Welt gezeigt, ihr Heil vermehren,", "tokens": ["Hier", "in", "der", "Welt", "ge\u00b7zeigt", ",", "ihr", "Heil", "ver\u00b7meh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVPP", "$,", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Und immer gr\u00f6sser wird; so wird zu GOTTES Ehren", "tokens": ["Und", "im\u00b7mer", "gr\u00f6s\u00b7ser", "wird", ";", "so", "wird", "zu", "GoT\u00b7TES", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "$.", "ADV", "VAFIN", "APPR", "NE", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Dasselbe noch weit gr\u00f6sser werden;", "tokens": ["Das\u00b7sel\u00b7be", "noch", "weit", "gr\u00f6s\u00b7ser", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJD", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Wenn sie die wichtigen Begebenheiten,", "tokens": ["Wenn", "sie", "die", "wich\u00b7ti\u00b7gen", "Be\u00b7ge\u00b7ben\u00b7hei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Und die gewaltige Ver\u00e4nderung der Zeiten,", "tokens": ["Und", "die", "ge\u00b7wal\u00b7ti\u00b7ge", "Ver\u00b7\u00e4n\u00b7de\u00b7rung", "der", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Den Wachsthum und den Fall der Regiment\u2019 auf Erden,", "tokens": ["Den", "Wach\u00b7sthum", "und", "den", "Fall", "der", "Re\u00b7gi\u00b7ment'", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Erstaunend \u00fcbersehn:", "tokens": ["Er\u00b7stau\u00b7nend", "\u00fc\u00b7ber\u00b7sehn", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "Da sie denn, was kein Mensch auf Erden konnt\u2019 ergr\u00fcnden,", "tokens": ["Da", "sie", "denn", ",", "was", "kein", "Mensch", "auf", "Er\u00b7den", "konnt'", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "PRELS", "PIAT", "NN", "APPR", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Vor Lust entz\u00fccket, werden finden,", "tokens": ["Vor", "Lust", "ent\u00b7z\u00fc\u00b7cket", ",", "wer\u00b7den", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Da\u00df alles Gutes, was geschehen,", "tokens": ["Da\u00df", "al\u00b7les", "Gu\u00b7tes", ",", "was", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "$,", "PWS", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Allein geschehen sey, den weisen Willen", "tokens": ["Al\u00b7lein", "ge\u00b7sche\u00b7hen", "sey", ",", "den", "wei\u00b7sen", "Wil\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVPP", "VAFIN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Des herrlichen Beherrschers aller Welt,", "tokens": ["Des", "herr\u00b7li\u00b7chen", "Be\u00b7herr\u00b7schers", "al\u00b7ler", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Zum guten Endzweck, zu erf\u00fcllen.", "tokens": ["Zum", "gu\u00b7ten", "End\u00b7zweck", ",", "zu", "er\u00b7f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Sie werden deutlich k\u00f6nnen lesen;", "tokens": ["Sie", "wer\u00b7den", "deut\u00b7lich", "k\u00f6n\u00b7nen", "le\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Da\u00df alles, was gebaut und angefangen,", "tokens": ["Da\u00df", "al\u00b7les", ",", "was", "ge\u00b7baut", "und", "an\u00b7ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "PRELS", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Da\u00df alles, was verst\u00f6hret und vergangeu;", "tokens": ["Da\u00df", "al\u00b7les", ",", "was", "ver\u00b7st\u00f6h\u00b7ret", "und", "ver\u00b7gan\u00b7geu", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "PWS", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Vortrefflich, n\u00fctzlich, gut und n\u00f6thig sey gewesen.", "tokens": ["Vor\u00b7treff\u00b7lich", ",", "n\u00fctz\u00b7lich", ",", "gut", "und", "n\u00f6\u00b7thig", "sey", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD", "VAFIN", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Wie werden sie mit solcher Seelen-Speise", "tokens": ["Wie", "wer\u00b7den", "sie", "mit", "sol\u00b7cher", "See\u00b7len\u00b7Spei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Sich ewig und unendlich n\u00e4hren,", "tokens": ["Sich", "e\u00b7wig", "und", "un\u00b7end\u00b7lich", "n\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Auf welche angenehme Weise", "tokens": ["Auf", "wel\u00b7che", "an\u00b7ge\u00b7neh\u00b7me", "Wei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PWAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Wird von Verrichtungen der Engel auf der Erden,", "tokens": ["Wird", "von", "Ver\u00b7rich\u00b7tun\u00b7gen", "der", "En\u00b7gel", "auf", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Wenn sie manch Reich gest\u00fcrtzt, ein anders unterst\u00fctzet,", "tokens": ["Wenn", "sie", "manch", "Reich", "ge\u00b7st\u00fcrtzt", ",", "ein", "an\u00b7ders", "un\u00b7ter\u00b7st\u00fct\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVPP", "$,", "ART", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Hier eine Stadt verheert, ein\u2019 and\u2019re dort gesch\u00fctzet;", "tokens": ["Hier", "ei\u00b7ne", "Stadt", "ver\u00b7heert", ",", "ein'", "an\u00b7d'\u00b7re", "dort", "ge\u00b7sch\u00fct\u00b7zet", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$,", "ART", "ADJA", "ADV", "VVPP", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.39": {"text": "Den Seeligen erz\u00e4hlt und kund gemachet werden!", "tokens": ["Den", "See\u00b7li\u00b7gen", "er\u00b7z\u00e4hlt", "und", "kund", "ge\u00b7ma\u00b7chet", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ADJD", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Ja werden sie sich nicht in seel\u2019ger Lust verliehren,", "tokens": ["Ja", "wer\u00b7den", "sie", "sich", "nicht", "in", "seel'\u00b7ger", "Lust", "ver\u00b7lieh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VAFIN", "PPER", "PRF", "PTKNEG", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Wenn sie dergleichen Wunderwercke,", "tokens": ["Wenn", "sie", "derg\u00b7lei\u00b7chen", "Wun\u00b7der\u00b7wer\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "Und herrlichen Beweis, von GOTTES Lieb und St\u00e4rcke,", "tokens": ["Und", "herr\u00b7li\u00b7chen", "Be\u00b7weis", ",", "von", "GoT\u00b7TES", "Lieb", "und", "St\u00e4r\u00b7cke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "APPR", "NE", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "In hundert tausend Welten sp\u00fcren?", "tokens": ["In", "hun\u00b7dert", "tau\u00b7send", "Wel\u00b7ten", "sp\u00fc\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}