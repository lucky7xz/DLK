{"textgrid.poem.46504": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr, denen, was mein Haus von stillem Gl\u00fccke", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr, denen, was mein Haus von stillem Gl\u00fccke", "tokens": ["Ihr", ",", "de\u00b7nen", ",", "was", "mein", "Haus", "von", "stil\u00b7lem", "Gl\u00fc\u00b7cke"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "$,", "PRELS", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Umfa\u00dfte, stand in meinen Liedern offen!", "tokens": ["Um\u00b7fa\u00df\u00b7te", ",", "stand", "in", "mei\u00b7nen", "Lie\u00b7dern", "of\u00b7fen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Theilnehmend an so unscheinbaren Stoffen,", "tokens": ["Theil\u00b7neh\u00b7mend", "an", "so", "un\u00b7schein\u00b7ba\u00b7ren", "Stof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die nicht vertragen, da\u00df viel Kunst sie schm\u00fccke;", "tokens": ["Die", "nicht", "ver\u00b7tra\u00b7gen", ",", "da\u00df", "viel", "Kunst", "sie", "schm\u00fc\u00b7cke", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "VVFIN", "$,", "KOUS", "PIAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nehmt eure Theilnahm' itzt auch nicht zur\u00fccke", "tokens": ["Nehmt", "eu\u00b7re", "Theil\u00b7nahm'", "itzt", "auch", "nicht", "zu\u00b7r\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "ADV", "PTKNEG", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und la\u00dft f\u00fcr Beifallsl\u00e4cheln Thr\u00e4nen hoffen,", "tokens": ["Und", "la\u00dft", "f\u00fcr", "Bei\u00b7fall\u00b7sl\u00e4\u00b7cheln", "Thr\u00e4\u00b7nen", "hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "APPR", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Beim Schicksalsschlag, der so das Haus getroffen,", "tokens": ["Beim", "Schick\u00b7sals\u00b7schlag", ",", "der", "so", "das", "Haus", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df alles Glas der Freude ging in St\u00fccke!", "tokens": ["Da\u00df", "al\u00b7les", "Glas", "der", "Freu\u00b7de", "ging", "in", "St\u00fc\u00b7cke", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Vielleicht verschl\u00f6\u00df' ich besser solche Kl\u00e4nge;", "tokens": ["Viel\u00b7leicht", "ver\u00b7schl\u00f6\u00df'", "ich", "bes\u00b7ser", "sol\u00b7che", "Kl\u00e4n\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wahrlich nicht mit Lorbeer zu umweben", "tokens": ["Und", "wahr\u00b7lich", "nicht", "mit", "Lor\u00b7beer", "zu", "um\u00b7we\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PTKNEG", "APPR", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Denk' ich die Stirn durch klagende Ges\u00e4nge.", "tokens": ["Denk'", "ich", "die", "Stirn", "durch", "kla\u00b7gen\u00b7de", "Ge\u00b7s\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch wenn ich s\u00e4he meine Lieben leben", "tokens": ["Doch", "wenn", "ich", "s\u00e4\u00b7he", "mei\u00b7ne", "Lie\u00b7ben", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "PPOSAT", "ADJA", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In fremden Munde, dieses Schaugepr\u00e4nge", "tokens": ["In", "frem\u00b7den", "Mun\u00b7de", ",", "die\u00b7ses", "Schau\u00b7ge\u00b7pr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "K\u00f6nnt' ein'gen Trost f\u00fcr ihren Tod mir geben.", "tokens": ["K\u00f6nnt'", "ein'\u00b7gen", "Trost", "f\u00fcr", "ih\u00b7ren", "Tod", "mir", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}