{"textgrid.poem.58619": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "51.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Menschen, ach, wie plagt ihr euch, nach der Eitelkeit zu trachten;", "tokens": ["Men\u00b7schen", ",", "ach", ",", "wie", "plagt", "ihr", "euch", ",", "nach", "der", "Ei\u00b7tel\u00b7keit", "zu", "trach\u00b7ten", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ITJ", "$,", "PWAV", "VVFIN", "PPER", "PPER", "$,", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Denkt, ihr greift nach Schatten nur, denkt, der arme Geist mu\u00df schmachten,", "tokens": ["Denkt", ",", "ihr", "greift", "nach", "Schat\u00b7ten", "nur", ",", "denkt", ",", "der", "ar\u00b7me", "Geist", "mu\u00df", "schmach\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "APPR", "NN", "ADV", "$,", "VVFIN", "$,", "ART", "ADJA", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "H\u00f6rt, es ruft euch euer Gott: \u00bbKommet doch mit Herz und Mut!\u00ab", "tokens": ["H\u00f6rt", ",", "es", "ruft", "euch", "eu\u00b7er", "Gott", ":", "\u00bb", "Kom\u00b7met", "doch", "mit", "Herz", "und", "Mut", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "$(", "VVIMP", "ADV", "APPR", "NN", "KON", "NN", "$.", "$("], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Hungert doch nach ihm allein, ach, man hat's bei ihm so gut!", "tokens": ["Hun\u00b7gert", "doch", "nach", "ihm", "al\u00b7lein", ",", "ach", ",", "man", "hat's", "bei", "ihm", "so", "gut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPER", "ADV", "$,", "ITJ", "$,", "PIS", "VAFIN", "APPR", "PPER", "ADV", "ADJD", "$."], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.5": {"text": "Herr, ich neig' mein Ohr zu dir, la\u00df mich dich im Geist denn h\u00f6ren,", "tokens": ["Herr", ",", "ich", "neig'", "mein", "Ohr", "zu", "dir", ",", "la\u00df", "mich", "dich", "im", "Geist", "denn", "h\u00f6\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PPER", "$,", "VVIMP", "PPER", "PRF", "APPRART", "NN", "KON", "VVINF", "$,"], "meter": "----+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Ich will kommen, zieh mich nur, ganz in dich hineinzukehren,", "tokens": ["Ich", "will", "kom\u00b7men", ",", "zieh", "mich", "nur", ",", "ganz", "in", "dich", "hin\u00b7ein\u00b7zu\u00b7keh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "VVFIN", "PPER", "ADV", "$,", "ADV", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.7": {"text": "Gib dich mir, so bin ich satt, dann wird leben Seel' und Geist!", "tokens": ["Gib", "dich", "mir", ",", "so", "bin", "ich", "satt", ",", "dann", "wird", "le\u00b7ben", "Seel'", "und", "Geist", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$,", "ADV", "VAFIN", "VVINF", "NN", "KON", "NN", "$."], "meter": "+---+-+----+-+", "measure": "dactylic.init"}, "line.8": {"text": "Der i\u00dft ja das Gute recht, der das h\u00f6chste Gut geneu\u00dft.", "tokens": ["Der", "i\u00dft", "ja", "das", "Gu\u00b7te", "recht", ",", "der", "das", "h\u00f6chs\u00b7te", "Gut", "ge\u00b7neu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ART", "NN", "ADJD", "$,", "PRELS", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}