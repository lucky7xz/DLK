{"dta.poem.12302": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Vertraue .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist kein J\u00e4ger, er hat ein Schu\u00df,               ", "tokens": ["Es", "ist", "kein", "J\u00e4\u00b7ger", ",", "er", "hat", "ein", "Schu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Viel hundert Schrot auf einen Ku\u00df:", "tokens": ["Viel", "hun\u00b7dert", "Schrot", "auf", "ei\u00b7nen", "Ku\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201efeins Lieb, dich ruhig stelle,", "tokens": ["\u201e", "feins", "Lieb", ",", "dich", "ru\u00b7hig", "stel\u00b7le", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201eund willst du meinem Ku\u00df nicht stehn,", "tokens": ["\u201e", "und", "willst", "du", "mei\u00b7nem", "Ku\u00df", "nicht", "stehn", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VMFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u201eso k\u00fc\u00dft dich mein Geselle.\u201c", "tokens": ["\u201e", "so", "k\u00fc\u00dft", "dich", "mein", "Ge\u00b7sel\u00b7le", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u201emein Ku\u00df ist leicht, wiegt nur ein Loth,", "tokens": ["\u201e", "mein", "Ku\u00df", "ist", "leicht", ",", "wiegt", "nur", "ein", "Loth", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "ADJD", "$,", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201edu wirst nicht bleich, du wirst nicht roth,", "tokens": ["\u201e", "du", "wirst", "nicht", "bleich", ",", "du", "wirst", "nicht", "roth", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PTKNEG", "ADJD", "$,", "PPER", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201edu brauchst dich nicht zu sch\u00e4men,", "tokens": ["\u201e", "du", "brauchst", "dich", "nicht", "zu", "sch\u00e4\u00b7men", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201eich will den schwarzen Vogel dir", "tokens": ["\u201e", "ich", "will", "den", "schwar\u00b7zen", "Vo\u00b7gel", "dir"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VMFIN", "ART", "ADJA", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u201evom Haupt herunter nehmen.\u201c", "tokens": ["\u201e", "vom", "Haupt", "her\u00b7un\u00b7ter", "neh\u00b7men", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPRART", "NN", "APZR", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u201efeins Lieb sitz still im gr\u00fcnen Moos,", "tokens": ["\u201e", "feins", "Lieb", "sitz", "still", "im", "gr\u00fc\u00b7nen", "Moos", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "NE", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201eder Vogel f\u00e4llt in deinen Schoos,", "tokens": ["\u201e", "der", "Vo\u00b7gel", "f\u00e4llt", "in", "dei\u00b7nen", "Schoos", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201ewohl von des Baumes Spitzen;", "tokens": ["\u201e", "wohl", "von", "des", "Bau\u00b7mes", "Spit\u00b7zen", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "\u201ein deinem Schoose stirbt sich gut,", "tokens": ["\u201e", "in", "dei\u00b7nem", "Schoo\u00b7se", "stirbt", "sich", "gut", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPOSAT", "NN", "VVFIN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u201efeins Lieb bleib ruhig sitzen.\u201c", "tokens": ["\u201e", "feins", "Lieb", "bleib", "ru\u00b7hig", "sit\u00b7zen", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJA", "NN", "VVFIN", "ADJD", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie wollt nicht trauen auf sein Wort,", "tokens": ["Sie", "wollt", "nicht", "trau\u00b7en", "auf", "sein", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Brauns M\u00e4delein wollt springen fort,", "tokens": ["Brauns", "M\u00e4\u00b7de\u00b7lein", "wollt", "sprin\u00b7gen", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VMFIN", "VVINF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Schu\u00df schlug sie darnieder;", "tokens": ["Der", "Schu\u00df", "schlug", "sie", "dar\u00b7nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PAV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der schwarze Vogel von dem Baum", "tokens": ["Der", "schwar\u00b7ze", "Vo\u00b7gel", "von", "dem", "Baum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schwang weiter sein Gefieder.", "tokens": ["Schwang", "wei\u00b7ter", "sein", "Ge\u00b7fie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u201emein Ku\u00df ist leicht, wiegt nur ein Loth,", "tokens": ["\u201e", "mein", "Ku\u00df", "ist", "leicht", ",", "wiegt", "nur", "ein", "Loth", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "ADJD", "$,", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201edu wirst nicht bleich, du wirst nicht roth,", "tokens": ["\u201e", "du", "wirst", "nicht", "bleich", ",", "du", "wirst", "nicht", "roth", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PTKNEG", "ADJD", "$,", "PPER", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201ebrauchst dich nicht mehr zu sch\u00e4men,", "tokens": ["\u201e", "brauchst", "dich", "nicht", "mehr", "zu", "sch\u00e4\u00b7men", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "PTKNEG", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "\u201ein deinem Schoo\u00dfe stirbt sichs gut.\u201c", "tokens": ["\u201e", "in", "dei\u00b7nem", "Schoo\u00b7\u00dfe", "stirbt", "sichs", "gut", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "PPOSAT", "NN", "VVFIN", "PIS", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er th\u00e4t sichs Leben nehmen.", "tokens": ["Er", "th\u00e4t", "sichs", "Le\u00b7ben", "neh\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}