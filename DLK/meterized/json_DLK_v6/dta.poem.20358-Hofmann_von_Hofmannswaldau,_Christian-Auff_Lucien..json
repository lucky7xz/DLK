{"dta.poem.20358": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Auff Lucien.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die karge Lucie hat j\u00fcngst ein schwein geschlacht/", "tokens": ["Die", "kar\u00b7ge", "Lu\u00b7cie", "hat", "j\u00fcngst", "ein", "schwein", "ge\u00b7schlacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VAFIN", "ADV", "ART", "ADJD", "VVPP", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das sie ein gantzes jahr von eignem koth ernehret;", "tokens": ["Das", "sie", "ein", "gant\u00b7zes", "jahr", "von", "eig\u00b7nem", "koth", "er\u00b7neh\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hiern\u00e4chst hat sie ein huhn von speichel fett gemacht/", "tokens": ["Hier\u00b7n\u00e4chst", "hat", "sie", "ein", "huhn", "von", "spei\u00b7chel", "fett", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "APPR", "NE", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das t\u00e4glich ihr ein ey zur frischen kost gew\u00e4hret.", "tokens": ["Das", "t\u00e4g\u00b7lich", "ihr", "ein", "ey", "zur", "fri\u00b7schen", "kost", "ge\u00b7w\u00e4h\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "PPER", "ART", "NN", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Offt nimmt sie noch dazu von ihres schweines speck;", "tokens": ["Offt", "nimmt", "sie", "noch", "da\u00b7zu", "von", "ih\u00b7res", "schwei\u00b7nes", "speck", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PAV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wer sagt nun/ da\u00df sie nicht fri\u00dft ihren eignen dreck?", "tokens": ["Wer", "sagt", "nun", "/", "da\u00df", "sie", "nicht", "fri\u00dft", "ih\u00b7ren", "eig\u00b7nen", "dreck", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "$(", "KOUS", "PPER", "PTKNEG", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und da\u00df sie nicht vor geitz sich in sich selbst verzehret?", "tokens": ["Und", "da\u00df", "sie", "nicht", "vor", "geitz", "sich", "in", "sich", "selbst", "ver\u00b7zeh\u00b7ret", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PTKNEG", "APPR", "NE", "PRF", "APPR", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}