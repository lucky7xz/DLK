{"textgrid.poem.47856": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1852, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Glaubt ich doch, sie w\u00e4r es selber", "tokens": ["Glaubt", "ich", "doch", ",", "sie", "w\u00e4r", "es", "sel\u00b7ber"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PPER", "VAFIN", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": " \u2013Was nur das Gedanken sind! \u2013,", "tokens": ["\u2013", "Was", "nur", "das", "Ge\u00b7dan\u00b7ken", "sind", "!", "\u2013", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "PWS", "ADV", "ART", "NN", "VAFIN", "$.", "$(", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Frau Musika vom Himmel;", "tokens": ["Die", "Frau", "Mu\u00b7si\u00b7ka", "vom", "Him\u00b7mel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "APPRART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und nun ist' s ein Erdenkind!", "tokens": ["Und", "nun", "ist'", "s", "ein", "Er\u00b7den\u00b7kind", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Gestern, da sie stand am Brunnen,", "tokens": ["Ge\u00b7stern", ",", "da", "sie", "stand", "am", "Brun\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Zog ich flink den Hut zum Gru\u00df;", "tokens": ["Zog", "ich", "flink", "den", "Hut", "zum", "Gru\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und sie nickt' und sprach in Z\u00fcchten:", "tokens": ["Und", "sie", "nickt'", "und", "sprach", "in", "Z\u00fcch\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bbgr\u00fc\u00df dich Gott, Herr Musikus!\u00ab", "tokens": ["\u00bb", "gr\u00fc\u00df", "dich", "Gott", ",", "Herr", "Mu\u00b7si\u00b7kus", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "VVIMP", "PPER", "NN", "$,", "NN", "NE", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Zwar ich wu\u00dft, Marannle hei\u00dft sie,", "tokens": ["Zwar", "ich", "wu\u00dft", ",", "Ma\u00b7rann\u00b7le", "hei\u00dft", "sie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "$,", "NE", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und sie wohnt am Tore nah;", "tokens": ["Und", "sie", "wohnt", "am", "To\u00b7re", "nah", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch ich h\u00e4tt's nicht k\u00f6nnen lassen,", "tokens": ["Doch", "ich", "h\u00e4tt's", "nicht", "k\u00f6n\u00b7nen", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sprach: \u00bbGr\u00fc\u00df Gott, Frau Musika!\u00ab", "tokens": ["Sprach", ":", "\u00bb", "Gr\u00fc\u00df", "Gott", ",", "Frau", "Mu\u00b7si\u00b7ka", "!", "\u00ab"], "token_info": ["word", "punct", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "$.", "$(", "VVIMP", "NN", "$,", "NN", "NE", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Was sie da f\u00fcr Augen machte!", "tokens": ["Was", "sie", "da", "f\u00fcr", "Au\u00b7gen", "mach\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und was da mit mir geschah!", "tokens": ["Und", "was", "da", "mit", "mir", "ge\u00b7schah", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Stets nun klingt's mir vor den Ohren:", "tokens": ["Stets", "nun", "klingt's", "mir", "vor", "den", "Oh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Musikus und Musika!", "tokens": ["Mu\u00b7si\u00b7kus", "und", "Mu\u00b7si\u00b7ka", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}