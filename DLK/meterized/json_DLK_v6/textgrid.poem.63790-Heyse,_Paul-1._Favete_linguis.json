{"textgrid.poem.63790": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1. Favete linguis", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da ich ein junger Gesell, wie schalt mich oft die Geliebte,", "tokens": ["Da", "ich", "ein", "jun\u00b7ger", "Ge\u00b7sell", ",", "wie", "schalt", "mich", "oft", "die", "Ge\u00b7lieb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "$,", "PWAV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wenn ich in Schweigen versank mitten im lachendsten Gl\u00fcck,", "tokens": ["Wenn", "ich", "in", "Schwei\u00b7gen", "ver\u00b7sank", "mit\u00b7ten", "im", "la\u00b7chends\u00b7ten", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Um erst ferne von ihr in befl\u00fcgeltem Wort zu ergie\u00dfen", "tokens": ["Um", "erst", "fer\u00b7ne", "von", "ihr", "in", "be\u00b7fl\u00fc\u00b7gel\u00b7tem", "Wort", "zu", "er\u00b7gie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ADV", "ADV", "APPR", "PPER", "APPR", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "All der Gef\u00fchle Gewalt, die mir die Nahe geweckt.", "tokens": ["All", "der", "Ge\u00b7f\u00fch\u00b7le", "Ge\u00b7walt", ",", "die", "mir", "die", "Na\u00b7he", "ge\u00b7weckt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.5": {"text": "So auch wandelt' ich stumm vorbei an den holden Gebilden", "tokens": ["So", "auch", "wan\u00b7delt'", "ich", "stumm", "vor\u00b7bei", "an", "den", "hol\u00b7den", "Ge\u00b7bil\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADJD", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "S\u00fcdlicher Kunst; erst sp\u00e4t kam das Erlebnis zu Wort.", "tokens": ["S\u00fcd\u00b7li\u00b7cher", "Kunst", ";", "erst", "sp\u00e4t", "kam", "das", "Er\u00b7leb\u00b7nis", "zu", "Wort", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADV", "ADJD", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Ist doch Denken Erinnern, und Dichten ein inneres Anschaun;", "tokens": ["Ist", "doch", "Den\u00b7ken", "E\u00b7rin\u00b7nern", ",", "und", "Dich\u00b7ten", "ein", "in\u00b7ne\u00b7res", "An\u00b7schaun", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "VVFIN", "$,", "KON", "NN", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.8": {"text": "Worte beschw\u00f6ren den Geist, der sich den Sinnen entzog.", "tokens": ["Wor\u00b7te", "be\u00b7schw\u00f6\u00b7ren", "den", "Geist", ",", "der", "sich", "den", "Sin\u00b7nen", "ent\u00b7zog", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "PRELS", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.9": {"text": "Nachzubeleben entschwundenes Gl\u00fcck vermag die beseelte", "tokens": ["Nach\u00b7zu\u00b7be\u00b7le\u00b7ben", "ent\u00b7schwun\u00b7de\u00b7nes", "Gl\u00fcck", "ver\u00b7mag", "die", "be\u00b7seel\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "ART", "ADJA"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Rede; lebend'gem Genu\u00df gn\u00fcgt ein verworrenes Ach.", "tokens": ["Re\u00b7de", ";", "le\u00b7ben\u00b7d'\u00b7gem", "Ge\u00b7nu\u00df", "gn\u00fcgt", "ein", "ver\u00b7wor\u00b7re\u00b7nes", "Ach", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+-+-+--+", "measure": "trochaic.septa.relaxed"}}}}}