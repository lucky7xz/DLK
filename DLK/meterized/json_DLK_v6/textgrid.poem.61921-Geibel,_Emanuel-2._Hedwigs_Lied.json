{"textgrid.poem.61921": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "2. Hedwigs Lied", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Falk hat sich verflogen,", "tokens": ["Mein", "Falk", "hat", "sich", "ver\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verflogen \u00fcber Feld;", "tokens": ["Ver\u00b7flo\u00b7gen", "\u00fc\u00b7ber", "Feld", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Schatz ist fortgezogen", "tokens": ["Mein", "Schatz", "ist", "fort\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In die weite, weite Welt.", "tokens": ["In", "die", "wei\u00b7te", ",", "wei\u00b7te", "Welt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nun geht das dritte Jahr dahin,", "tokens": ["Nun", "geht", "das", "drit\u00b7te", "Jahr", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df ich in Sorgen harr' auf ihn", "tokens": ["Da\u00df", "ich", "in", "Sor\u00b7gen", "harr'", "auf", "ihn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "VAFIN", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und frohtun mu\u00df mit Schmerzen", "tokens": ["Und", "froh\u00b7tun", "mu\u00df", "mit", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "VMFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Im Herzen.", "tokens": ["Im", "Her\u00b7zen", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Ach, Liebster, weh tut Scheiden", "tokens": ["Ach", ",", "Liebs\u00b7ter", ",", "weh", "tut", "Schei\u00b7den"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "NN", "$,", "ADV", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ins fremde Land hinaus,", "tokens": ["Ins", "frem\u00b7de", "Land", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APZR", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch bittrer ist das Meiden", "tokens": ["Doch", "bit\u00b7trer", "ist", "das", "Mei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Daheim im \u00f6den Haus.", "tokens": ["Da\u00b7heim", "im", "\u00f6\u00b7den", "Haus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Von fr\u00fch bis sp\u00e4t den ganzen Tag", "tokens": ["Von", "fr\u00fch", "bis", "sp\u00e4t", "den", "gan\u00b7zen", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "APPR", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Denk' ich, wie dir's ergehen mag,", "tokens": ["Denk'", "ich", ",", "wie", "dir's", "er\u00b7ge\u00b7hen", "mag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "PWAV", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und sitze nachts alleine", "tokens": ["Und", "sit\u00b7ze", "nachts", "al\u00b7lei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und weine.", "tokens": ["Und", "wei\u00b7ne", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Der Fr\u00fchling kommt gegangen,", "tokens": ["Der", "Fr\u00fch\u00b7ling", "kommt", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kaum seh' ich's, wie er bl\u00fcht;", "tokens": ["Kaum", "seh'", "ich's", ",", "wie", "er", "bl\u00fcht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In Bangen und Verlangen", "tokens": ["In", "Ban\u00b7gen", "und", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verzehrt sich mein Gem\u00fct.", "tokens": ["Ver\u00b7zehrt", "sich", "mein", "Ge\u00b7m\u00fct", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O komm und bringe Trost und Gl\u00fcck", "tokens": ["O", "komm", "und", "brin\u00b7ge", "Trost", "und", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "KON", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und bring' mir meine Ruh' zur\u00fcck!", "tokens": ["Und", "bring'", "mir", "mei\u00b7ne", "Ruh'", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Fr\u00fchling kommt zum Walde \u2013", "tokens": ["Der", "Fr\u00fch\u00b7ling", "kommt", "zum", "Wal\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Komm balde!", "tokens": ["Komm", "bal\u00b7de", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADV", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}