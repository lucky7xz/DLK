{"textgrid.poem.39480": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Aus Versen ganz gebaut, beflaggt, bekabelt,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus Versen ganz gebaut, beflaggt, bekabelt,", "tokens": ["Aus", "Ver\u00b7sen", "ganz", "ge\u00b7baut", ",", "be\u00b7flaggt", ",", "be\u00b7ka\u00b7belt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVPP", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Holt in den Krieg f\u00fcr des Geschmackes Ehre", "tokens": ["Holt", "in", "den", "Krieg", "f\u00fcr", "des", "Ge\u00b7schma\u00b7ckes", "Eh\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "APPR", "ART", "NN", "NN"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.3": {"text": "Ein Schiff Cervantes sammt der Dichter Heere,", "tokens": ["Ein", "Schiff", "Cer\u00b7van\u00b7tes", "sammt", "der", "Dich\u00b7ter", "Hee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zum Berg der Dichtung, den sie selbst gefabelt.", "tokens": ["Zum", "Berg", "der", "Dich\u00b7tung", ",", "den", "sie", "selbst", "ge\u00b7fa\u00b7belt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.2": {"line.1": {"text": "Ein andres, wo sich Reimer, s\u00fc\u00df geschnabelt,", "tokens": ["Ein", "and\u00b7res", ",", "wo", "sich", "Rei\u00b7mer", ",", "s\u00fc\u00df", "ge\u00b7schna\u00b7belt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PIS", "$,", "PWAV", "PRF", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Herzugedr\u00e4ngt, versenkt Neptun im Meere;", "tokens": ["Her\u00b7zu\u00b7ge\u00b7dr\u00e4ngt", ",", "ver\u00b7senkt", "Nep\u00b7tun", "im", "Mee\u00b7re", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In Schl\u00e4uch' und K\u00fcrbse wandelt sie Cythere,", "tokens": ["In", "Schl\u00e4uch'", "und", "K\u00fcrb\u00b7se", "wan\u00b7delt", "sie", "Cy\u00b7the\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PPER", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df nicht der Gott ihr hohles Volk ergabelt.", "tokens": ["Da\u00df", "nicht", "der", "Gott", "ihr", "hoh\u00b7les", "Volk", "er\u00b7ga\u00b7belt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Bald sind besiegt, die den Parnass verwirrten;", "tokens": ["Bald", "sind", "be\u00b7siegt", ",", "die", "den", "Par\u00b7nass", "ver\u00b7wirr\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Man siehet neu die Poesie ergl\u00e4nzen,", "tokens": ["Man", "sie\u00b7het", "neu", "die", "Poe\u00b7sie", "er\u00b7gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "So wie die Sonn' aus sch\u00f6nen Morgenr\u00f6then.", "tokens": ["So", "wie", "die", "Sonn'", "aus", "sch\u00f6\u00b7nen", "Mor\u00b7gen\u00b7r\u00f6\u00b7then", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die Heldenthaten lohnt Apoll mit Kr\u00e4nzen", "tokens": ["Die", "Hel\u00b7den\u00b7tha\u00b7ten", "lohnt", "A\u00b7poll", "mit", "Kr\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NE", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und beut, um sie auf's beste zu bewirthen,", "tokens": ["Und", "beut", ",", "um", "sie", "auf's", "bes\u00b7te", "zu", "be\u00b7wirt\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUI", "PPER", "APPRART", "ADJA", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kastaliens Na\u00df den hungrigen Poeten.", "tokens": ["Kas\u00b7ta\u00b7li\u00b7ens", "Na\u00df", "den", "hung\u00b7ri\u00b7gen", "Po\u00b7et\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}}}}