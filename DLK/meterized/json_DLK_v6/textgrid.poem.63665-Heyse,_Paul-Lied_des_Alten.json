{"textgrid.poem.63665": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Lied des Alten", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Maientagen, im Jugenddrang,", "tokens": ["In", "Mai\u00b7en\u00b7ta\u00b7gen", ",", "im", "Ju\u00b7gend\u00b7drang", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da lebt' ich von Luft und Liebe.", "tokens": ["Da", "lebt'", "ich", "von", "Luft", "und", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich hoffte, da\u00df es den Sommer lang", "tokens": ["Ich", "hoff\u00b7te", ",", "da\u00df", "es", "den", "Som\u00b7mer", "lang"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "So lustige Lebzeit bliebe.", "tokens": ["So", "lus\u00b7ti\u00b7ge", "Leb\u00b7zeit", "blie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Der Sommer kam, der wu\u00dfte nichts", "tokens": ["Der", "Som\u00b7mer", "kam", ",", "der", "wu\u00df\u00b7te", "nichts"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "VVFIN", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von T\u00e4nzen, Kr\u00e4nzen und K\u00fcssen.", "tokens": ["Von", "T\u00e4n\u00b7zen", ",", "Kr\u00e4n\u00b7zen", "und", "K\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich hab' im Schwei\u00dfe des Angesichts", "tokens": ["Ich", "hab'", "im", "Schwei\u00b7\u00dfe", "des", "An\u00b7ge\u00b7sichts"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Den Tag mir verdienen m\u00fcssen.", "tokens": ["Den", "Tag", "mir", "ver\u00b7die\u00b7nen", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "VMINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Die Schlo\u00dfen st\u00fcrmten, es traf der Blitz,", "tokens": ["Die", "Schlo\u00b7\u00dfen", "st\u00fcrm\u00b7ten", ",", "es", "traf", "der", "Blitz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nun herbstet es schon in den Zweigen.", "tokens": ["Nun", "herbs\u00b7tet", "es", "schon", "in", "den", "Zwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Im Busen reift mir ein voller Besitz \u2013", "tokens": ["Im", "Bu\u00b7sen", "reift", "mir", "ein", "vol\u00b7ler", "Be\u00b7sitz", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie lang wohl bleibt er mein eigen?", "tokens": ["Wie", "lang", "wohl", "bleibt", "er", "mein", "ei\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Gleichviel! und friert es Stein und Bein,", "tokens": ["Gleich\u00b7viel", "!", "und", "friert", "es", "Stein", "und", "Bein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "KON", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Man ruht doch Winters im Hafen.", "tokens": ["Man", "ruht", "doch", "Win\u00b7ters", "im", "Ha\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wer wacker geschafft, darf m\u00fcde sein;", "tokens": ["Wer", "wa\u00b7cker", "ge\u00b7schafft", ",", "darf", "m\u00fc\u00b7de", "sein", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVPP", "$,", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie freu' ich mich, auszuschlafen!", "tokens": ["Wie", "freu'", "ich", "mich", ",", "aus\u00b7zu\u00b7schla\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "$,", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}