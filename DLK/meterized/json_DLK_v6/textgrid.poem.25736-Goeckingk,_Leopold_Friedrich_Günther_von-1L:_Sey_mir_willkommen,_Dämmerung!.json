{"textgrid.poem.25736": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sey mir willkommen, D\u00e4mmerung!", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sey mir willkommen, D\u00e4mmerung!", "tokens": ["Sey", "mir", "will\u00b7kom\u00b7men", ",", "D\u00e4m\u00b7me\u00b7rung", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Mir wird so leicht in deinem Schatten!", "tokens": ["Mir", "wird", "so", "leicht", "in", "dei\u00b7nem", "Schat\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und wenn die Fl\u00fcgel mir ermatten,", "tokens": ["Und", "wenn", "die", "Fl\u00fc\u00b7gel", "mir", "er\u00b7mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So gibst du ihnen neuen Schwung.", "tokens": ["So", "gibst", "du", "ih\u00b7nen", "neu\u00b7en", "Schwung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir m\u00fcssen leider einst uns trennen:", "tokens": ["Wir", "m\u00fcs\u00b7sen", "lei\u00b7der", "einst", "uns", "tren\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Doch, bis die Abschiedsstunde schl\u00e4gt,", "tokens": ["Doch", ",", "bis", "die", "Ab\u00b7schieds\u00b7stun\u00b7de", "schl\u00e4gt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Soll dann erst meine Lampe brennen,", "tokens": ["Soll", "dann", "erst", "mei\u00b7ne", "Lam\u00b7pe", "bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn dich die Finsterni\u00df verj\u00e4gt.", "tokens": ["Wenn", "dich", "die", "Fins\u00b7ter\u00b7ni\u00df", "ver\u00b7j\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Schon immer hab' ich dich geliebt;", "tokens": ["Schon", "im\u00b7mer", "hab'", "ich", "dich", "ge\u00b7liebt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Als Knabe unter deinem Schutze,", "tokens": ["Als", "Kna\u00b7be", "un\u00b7ter", "dei\u00b7nem", "Schut\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Des Argus Augen selbst zum Trutze,", "tokens": ["Des", "Ar\u00b7gus", "Au\u00b7gen", "selbst", "zum", "Trut\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "So manchen losen Streich ver\u00fcbt;", "tokens": ["So", "man\u00b7chen", "lo\u00b7sen", "Streich", "ver\u00b7\u00fcbt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "F\u00fcr Nachbar ", "tokens": ["F\u00fcr", "Nach\u00b7bar"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "+--", "measure": "dactylic.init"}, "line.14": {"text": "Geldtuten, angef\u00fcllt mit Sand,", "tokens": ["Geld\u00b7tu\u00b7ten", ",", "an\u00b7ge\u00b7f\u00fcllt", "mit", "Sand", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "So lang vor seiner Th\u00fcr' verloren,", "tokens": ["So", "lang", "vor", "sei\u00b7ner", "Th\u00fcr'", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Bis sein betrogner Geitz sie fand.", "tokens": ["Bis", "sein", "be\u00b7trog\u00b7ner", "Geitz", "sie", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Das allerlieblichste, was schier", "tokens": ["Das", "al\u00b7ler\u00b7lieb\u00b7lichs\u00b7te", ",", "was", "schier"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "PWS", "ADJA"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Mein Herz in seinen sch\u00f6nsten Stunden", "tokens": ["Mein", "Herz", "in", "sei\u00b7nen", "sch\u00f6ns\u00b7ten", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Bei meinem M\u00e4dchen hat empfunden,", "tokens": ["Bei", "mei\u00b7nem", "M\u00e4d\u00b7chen", "hat", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "O D\u00e4mmerung! verdankt es dir!", "tokens": ["O", "D\u00e4m\u00b7me\u00b7rung", "!", "ver\u00b7dankt", "es", "dir", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Des Lebens Nachen trieb gelinde", "tokens": ["Des", "Le\u00b7bens", "Na\u00b7chen", "trieb", "ge\u00b7lin\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "An einem Veilchenstrande fort;", "tokens": ["An", "ei\u00b7nem", "Veil\u00b7chen\u00b7stran\u00b7de", "fort", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Mir war \u2013 \u2013 Ich sinne, doch ich finde", "tokens": ["Mir", "war", "\u2013", "\u2013", "Ich", "sin\u00b7ne", ",", "doch", "ich", "fin\u00b7de"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$(", "$(", "PPER", "VVFIN", "$,", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "F\u00fcr diesen Wonnestand kein Wort.", "tokens": ["F\u00fcr", "die\u00b7sen", "Won\u00b7nes\u00b7tand", "kein", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Itzt l\u00e4ssest du bei Hespers Schein", "tokens": ["Itzt", "l\u00e4s\u00b7sest", "du", "bei", "Hes\u00b7pers", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Vom Himmel leise dich hernieder,", "tokens": ["Vom", "Him\u00b7mel", "lei\u00b7se", "dich", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Und fl\u00f6\u00dfest deinem Freunde Lieder,", "tokens": ["Und", "fl\u00f6\u00b7\u00dfest", "dei\u00b7nem", "Freun\u00b7de", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Und reine Lebensweisheit ein.", "tokens": ["Und", "rei\u00b7ne", "Le\u00b7bens\u00b7weis\u00b7heit", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Mein Geist f\u00e4ngt an empor zu schweben,", "tokens": ["Mein", "Geist", "f\u00e4ngt", "an", "em\u00b7por", "zu", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Mir wird, in schmelzendem Gef\u00fchl',", "tokens": ["Mir", "wird", ",", "in", "schmel\u00b7zen\u00b7dem", "Ge\u00b7f\u00fchl'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Zum Feenm\u00e4hrchen dieses Leben,", "tokens": ["Zum", "Feen\u00b7m\u00e4hr\u00b7chen", "die\u00b7ses", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PDAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.32": {"text": "Die Welt zum Marjonettenspiel'.", "tokens": ["Die", "Welt", "zum", "Mar\u00b7jo\u00b7net\u00b7ten\u00b7spiel'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}