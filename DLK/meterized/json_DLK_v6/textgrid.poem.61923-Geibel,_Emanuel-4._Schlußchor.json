{"textgrid.poem.61923": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "4. Schlu\u00dfchor", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun bringt mit Schall das volle Fa\u00df", "tokens": ["Nun", "bringt", "mit", "Schall", "das", "vol\u00b7le", "Fa\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hervor aus Kellerstiefen", "tokens": ["Her\u00b7vor", "aus", "Kel\u00b7ler\u00b7stie\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und la\u00dft ins gr\u00fcne R\u00f6merglas", "tokens": ["Und", "la\u00dft", "ins", "gr\u00fc\u00b7ne", "R\u00f6\u00b7mer\u00b7glas"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein fl\u00fcssig Feuer triefen!", "tokens": ["Sein", "fl\u00fcs\u00b7sig", "Feu\u00b7er", "trie\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wir haben Tag' und Monde lang", "tokens": ["Wir", "ha\u00b7ben", "Tag'", "und", "Mon\u00b7de", "lang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "KON", "NE", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In d\u00fcrrer Pein gelegen;", "tokens": ["In", "d\u00fcr\u00b7rer", "Pein", "ge\u00b7le\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Willkommen denn im \u00dcberschwang,", "tokens": ["Will\u00b7kom\u00b7men", "denn", "im", "\u00dc\u00b7bersc\u00b7hwang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Willkommen, goldner Segen!", "tokens": ["Will\u00b7kom\u00b7men", ",", "gold\u00b7ner", "Se\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Wein! Wein! Wein!", "tokens": ["Wein", "!", "Wein", "!", "Wein", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.10": {"text": "Du Tr\u00f6ster ohnegleichen,", "tokens": ["Du", "Tr\u00f6s\u00b7ter", "oh\u00b7ne\u00b7glei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Du tust dich kund an Herz und Mund", "tokens": ["Du", "tust", "dich", "kund", "an", "Herz", "und", "Mund"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Mit Wundern und mit Zeichen.", "tokens": ["Mit", "Wun\u00b7dern", "und", "mit", "Zei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Fledermaus, die unsern Sinn", "tokens": ["Die", "Fle\u00b7der\u00b7maus", ",", "die", "un\u00b7sern", "Sinn"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geschreckt mit b\u00f6sen Tr\u00e4umen,", "tokens": ["Ge\u00b7schreckt", "mit", "b\u00f6\u00b7sen", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die schwarze Sorge f\u00e4hrt dahin,", "tokens": ["Die", "schwar\u00b7ze", "Sor\u00b7ge", "f\u00e4hrt", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sobald die Becher sch\u00e4umen.", "tokens": ["So\u00b7bald", "die", "Be\u00b7cher", "sch\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Baum des Lebens bl\u00fcht und laubt", "tokens": ["Der", "Baum", "des", "Le\u00b7bens", "bl\u00fcht", "und", "laubt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von frischem Saft durchdrungen,", "tokens": ["Von", "fri\u00b7schem", "Saft", "durch\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und wer noch j\u00fcngst sich stumm geglaubt,", "tokens": ["Und", "wer", "noch", "j\u00fcngst", "sich", "stumm", "ge\u00b7glaubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "ADV", "PRF", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der jauchzt in hellen Zungen.", "tokens": ["Der", "jauchzt", "in", "hel\u00b7len", "Zun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Wein! Wein! Wein!", "tokens": ["Wein", "!", "Wein", "!", "Wein", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.10": {"text": "Du Tr\u00f6ster ohnegleichen,", "tokens": ["Du", "Tr\u00f6s\u00b7ter", "oh\u00b7ne\u00b7glei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Du tust dich kund an Herz und Mund", "tokens": ["Du", "tust", "dich", "kund", "an", "Herz", "und", "Mund"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Mit Wundern und mit Zeichen.", "tokens": ["Mit", "Wun\u00b7dern", "und", "mit", "Zei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wir f\u00fchrten heut mit Jubellaut", "tokens": ["Wir", "f\u00fchr\u00b7ten", "heut", "mit", "Ju\u00b7bel\u00b7laut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein treues Paar zusammen;", "tokens": ["Ein", "treu\u00b7es", "Paar", "zu\u00b7sam\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie Maienrosen gl\u00fcht die Braut,", "tokens": ["Wie", "Mai\u00b7en\u00b7ro\u00b7sen", "gl\u00fcht", "die", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des J\u00fcnglings Blick wie Flammen.", "tokens": ["Des", "J\u00fcng\u00b7lings", "Blick", "wie", "Flam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KOKOM", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Doch selbst Frau Minne tritt zur\u00fcck", "tokens": ["Doch", "selbst", "Frau", "Min\u00b7ne", "tritt", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "NN", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vor deinem Freudenschwalle;", "tokens": ["Vor", "dei\u00b7nem", "Freu\u00b7den\u00b7schwal\u00b7le", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "F\u00fcr Zwei nur ist der Liebe Gl\u00fcck,", "tokens": ["F\u00fcr", "Zwei", "nur", "ist", "der", "Lie\u00b7be", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADV", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das Trinken ist f\u00fcr alle.", "tokens": ["Das", "Trin\u00b7ken", "ist", "f\u00fcr", "al\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PIS", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Wein! Wein! Wein!", "tokens": ["Wein", "!", "Wein", "!", "Wein", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.10": {"text": "Du Tr\u00f6ster ohnegleichen,", "tokens": ["Du", "Tr\u00f6s\u00b7ter", "oh\u00b7ne\u00b7glei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Du tust dich kund an Herz und Mund", "tokens": ["Du", "tust", "dich", "kund", "an", "Herz", "und", "Mund"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Mit Wundern und mit Zeichen!", "tokens": ["Mit", "Wun\u00b7dern", "und", "mit", "Zei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}