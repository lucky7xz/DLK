{"textgrid.poem.42300": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Die beiden Maler", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zum Zeuxis prahlt' einst Agatharch, ein kleiner,", "tokens": ["Zum", "Zeu\u00b7xis", "prahlt'", "einst", "A\u00b7gat\u00b7harch", ",", "ein", "klei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADV", "NE", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Fixfingriger, behender Pinselmann:", "tokens": ["Fix\u00b7fing\u00b7ri\u00b7ger", ",", "be\u00b7hen\u00b7der", "Pin\u00b7sel\u00b7mann", ":"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbso schnell, wie ich, malt wohl so leicht nicht Einer!\u00ab \u2013", "tokens": ["\u00bb", "so", "schnell", ",", "wie", "ich", ",", "malt", "wohl", "so", "leicht", "nicht", "Ei\u00b7ner", "!", "\u00ab", "\u2013"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "ADV", "ADJD", "$,", "PWAV", "PPER", "$,", "VVFIN", "ADV", "ADV", "ADJD", "PTKNEG", "PIS", "$.", "$(", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u00bbund ich, hub Zeuxis ruhig an,", "tokens": ["\u00bb", "und", "ich", ",", "hub", "Zeu\u00b7xis", "ru\u00b7hig", "an", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PPER", "$,", "VVFIN", "NE", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich r\u00fchme mich, da\u00df ich so langsam malen kann!\u00ab \u2013", "tokens": ["Ich", "r\u00fch\u00b7me", "mich", ",", "da\u00df", "ich", "so", "lang\u00b7sam", "ma\u00b7len", "kann", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "ADJD", "VVINF", "VMFIN", "$.", "$(", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Den Fingerfix nennt jetzt fast keiner;", "tokens": ["Den", "Fin\u00b7ger\u00b7fix", "nennt", "jetzt", "fast", "kei\u00b7ner", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "PIS", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Den Zeuxis noch fast Jedermann.", "tokens": ["Den", "Zeu\u00b7xis", "noch", "fast", "Je\u00b7der\u00b7mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}