{"dta.poem.97": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Uber die Nacht-Lust bey  Chloris.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr hellen Sterne/ die ihr jetzt so feurig spielet/", "tokens": ["Ihr", "hel\u00b7len", "Ster\u00b7ne", "/", "die", "ihr", "jetzt", "so", "feu\u00b7rig", "spie\u00b7let", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "PRELS", "PPER", "ADV", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du silber-weisser Mond/ des Schimmer auf mich zielet/", "tokens": ["Du", "sil\u00b7ber\u00b7weis\u00b7ser", "Mond", "/", "des", "Schim\u00b7mer", "auf", "mich", "zie\u00b7let", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$(", "ART", "NN", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du blasse ", "tokens": ["Du", "blas\u00b7se"], "token_info": ["word", "word"], "pos": ["PPER", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Mi\u00df g\u00f6nnet mir die Lust/ die mir die ", "tokens": ["Mi\u00df", "g\u00f6n\u00b7net", "mir", "die", "Lust", "/", "die", "mir", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "$(", "PRELS", "PPER", "ART"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Ums Himmels Willen nicht.", "tokens": ["Ums", "Him\u00b7mels", "Wil\u00b7len", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Umh\u00fcll dich Mond mit Flor/ la\u00df braunen Schatten mahlen", "tokens": ["Um\u00b7h\u00fcll", "dich", "Mond", "mit", "Flor", "/", "la\u00df", "brau\u00b7nen", "Schat\u00b7ten", "mah\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "NN", "APPR", "NN", "$(", "VVIMP", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Den gantzen Erden-Kreys mit schwartzer Farbe an/", "tokens": ["Den", "gant\u00b7zen", "Er\u00b7den\u00b7Kreys", "mit", "schwart\u00b7zer", "Far\u00b7be", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Verstecke dich ins Meer/ dein Schein sey gantz verdunckelt/", "tokens": ["Ver\u00b7ste\u00b7cke", "dich", "ins", "Meer", "/", "dein", "Schein", "sey", "gantz", "ver\u00b7dun\u00b7ckelt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$(", "PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.10": {"text": "Als aller Sternen Glantz.", "tokens": ["Als", "al\u00b7ler", "Ster\u00b7nen", "Glantz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Ich sehe/ da\u00df der Mond sich schon zum Abzug lencket/", "tokens": ["Ich", "se\u00b7he", "/", "da\u00df", "der", "Mond", "sich", "schon", "zum", "Ab\u00b7zug", "len\u00b7cket", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "ART", "NN", "PRF", "ADV", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Weil er der ", "tokens": ["Weil", "er", "der"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "ART"], "meter": "+--", "measure": "dactylic.init"}, "line.13": {"text": "Die Sterne weichen auch/ nun ist es vollends Nacht/", "tokens": ["Die", "Ster\u00b7ne", "wei\u00b7chen", "auch", "/", "nun", "ist", "es", "vol\u00b7lends", "Nacht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$(", "ADV", "VAFIN", "PPER", "ADV", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Doch ", "tokens": ["Doch"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.15": {"text": "Und macht was finster Licht.", "tokens": ["Und", "macht", "was", "fins\u00b7ter", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PWS", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Ich sp\u00fchre keine Nacht in meiner ", "tokens": ["Ich", "sp\u00fch\u00b7re", "kei\u00b7ne", "Nacht", "in", "mei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPR", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Ich f\u00fchle keinen Frost/ sie kan mich wohl erwarmen", "tokens": ["Ich", "f\u00fch\u00b7le", "kei\u00b7nen", "Frost", "/", "sie", "kan", "mich", "wohl", "er\u00b7war\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$(", "PPER", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Sie dr\u00fcckt mich an die Brust/ und liege ich gleich blo\u00df", "tokens": ["Sie", "dr\u00fcckt", "mich", "an", "die", "Brust", "/", "und", "lie\u00b7ge", "ich", "gleich", "blo\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$(", "KON", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Empsind ich keinem Frost weil meiner ", "tokens": ["Emp\u00b7sind", "ich", "kei\u00b7nem", "Frost", "weil", "mei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PIAT", "NN", "KOUS", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Feur hegt im Myrthen Krantz.", "tokens": ["Feur", "hegt", "im", "Myr\u00b7then", "Krantz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}