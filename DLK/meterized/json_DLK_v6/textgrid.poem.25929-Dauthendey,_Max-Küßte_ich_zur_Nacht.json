{"textgrid.poem.25929": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "K\u00fc\u00dfte ich zur Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, wie fr\u00f6hlich und gesund", "tokens": ["Ach", ",", "wie", "fr\u00f6h\u00b7lich", "und", "ge\u00b7sund"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWAV", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mich die Liebe macht!", "tokens": ["Mich", "die", "Lie\u00b7be", "macht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Bin der beste Mensch am Tag,", "tokens": ["Bin", "der", "bes\u00b7te", "Mensch", "am", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00fc\u00dfte ich zur Nacht.", "tokens": ["K\u00fc\u00df\u00b7te", "ich", "zur", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Arbeit tut von selber gehn,", "tokens": ["Ar\u00b7beit", "tut", "von", "sel\u00b7ber", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADV", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Jeder Schritt ist Dank,", "tokens": ["Je\u00b7der", "Schritt", "ist", "Dank", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Reden, die ich reden mu\u00df,", "tokens": ["Re\u00b7den", ",", "die", "ich", "re\u00b7den", "mu\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Red' ich frei und frank.", "tokens": ["Red'", "ich", "frei", "und", "frank", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "KON", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Heller wird mir jeder Tag,", "tokens": ["Hel\u00b7ler", "wird", "mir", "je\u00b7der", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wei\u00df, wohin man sieht,", "tokens": ["Wei\u00df", ",", "wo\u00b7hin", "man", "sieht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wei\u00df, wenn's Abend werden will,", "tokens": ["Wei\u00df", ",", "wenn's", "A\u00b7bend", "wer\u00b7den", "will", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "NN", "VAINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wozu das geschieht.", "tokens": ["Wo\u00b7zu", "das", "ge\u00b7schieht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "VVFIN", "$."], "meter": "----+", "measure": "unknown.measure.single"}}, "stanza.4": {"line.1": {"text": "Herrlich kommt die dunkle Nacht,", "tokens": ["Herr\u00b7lich", "kommt", "die", "dunk\u00b7le", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die den Mund mir gibt,", "tokens": ["Die", "den", "Mund", "mir", "gibt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Der mich bis zum hellen Tag", "tokens": ["Der", "mich", "bis", "zum", "hel\u00b7len", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Unter K\u00fcssen liebt.", "tokens": ["Un\u00b7ter", "K\u00fcs\u00b7sen", "liebt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}