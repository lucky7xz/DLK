{"textgrid.poem.33922": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Eine Pflicht", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6nheit, wo ich dich erblicke,", "tokens": ["Sch\u00f6n\u00b7heit", ",", "wo", "ich", "dich", "er\u00b7bli\u00b7cke", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Huldige ich deinem Licht,", "tokens": ["Hul\u00b7di\u00b7ge", "ich", "dei\u00b7nem", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Und wie ich mich selbst erquicke,", "tokens": ["Und", "wie", "ich", "mich", "selbst", "er\u00b7qui\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "So erf\u00fcll' ich eine Pflicht.", "tokens": ["So", "er\u00b7f\u00fcll'", "ich", "ei\u00b7ne", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Hast du je dich selbst genossen,", "tokens": ["Hast", "du", "je", "dich", "selbst", "ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn man dich nicht erst genie\u00dft?", "tokens": ["Wenn", "man", "dich", "nicht", "erst", "ge\u00b7nie\u00dft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bleibst du nicht in dich verschlossen,", "tokens": ["Bleibst", "du", "nicht", "in", "dich", "ver\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn man sich vor dir verschlie\u00dft?", "tokens": ["Wenn", "man", "sich", "vor", "dir", "ver\u00b7schlie\u00dft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ja, durchschauert es nicht leise", "tokens": ["Ja", ",", "durch\u00b7schau\u00b7ert", "es", "nicht", "lei\u00b7se"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "VVFIN", "PPER", "PTKNEG", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch die lieblichste Gestalt,", "tokens": ["Auch", "die", "lieb\u00b7lichs\u00b7te", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn in einem bl\u00f6den Kreise", "tokens": ["Wenn", "in", "ei\u00b7nem", "bl\u00f6\u00b7den", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihr versagt die Allgewalt?", "tokens": ["Ihr", "ver\u00b7sagt", "die", "All\u00b7ge\u00b7walt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aber wenn sie, Lust erweckend,", "tokens": ["A\u00b7ber", "wenn", "sie", ",", "Lust", "er\u00b7we\u00b7ckend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "$,", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieser Lust sich selbst erfreut,", "tokens": ["Die\u00b7ser", "Lust", "sich", "selbst", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PRF", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und, des Zaubers Macht entdeckend,", "tokens": ["Und", ",", "des", "Zau\u00b7bers", "Macht", "ent\u00b7de\u00b7ckend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den sie \u00fcbt, ihn still erneut:", "tokens": ["Den", "sie", "\u00fcbt", ",", "ihn", "still", "er\u00b7neut", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "PPER", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Hebt sich da ihr Blick nicht freier,", "tokens": ["Hebt", "sich", "da", "ihr", "Blick", "nicht", "frei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "PPOSAT", "NN", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil er fremdes Feuer trinkt?", "tokens": ["Weil", "er", "frem\u00b7des", "Feu\u00b7er", "trinkt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00e4llt die Angst nicht, wie ein Schleier,", "tokens": ["F\u00e4llt", "die", "Angst", "nicht", ",", "wie", "ein", "Schlei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Erst bemerkt, indem er sinkt?", "tokens": ["Erst", "be\u00b7merkt", ",", "in\u00b7dem", "er", "sinkt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Drum ein ungetr\u00fcbter Spiegel,", "tokens": ["Drum", "ein", "un\u00b7ge\u00b7tr\u00fcb\u00b7ter", "Spie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6nheit, werd' ich stets dir sein;", "tokens": ["Sch\u00f6n\u00b7heit", ",", "werd'", "ich", "stets", "dir", "sein", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "VVFIN", "PPER", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der Vollendung Sternensiegel", "tokens": ["Der", "Vol\u00b7len\u00b7dung", "Ster\u00b7nen\u00b7sie\u00b7gel"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Kommt dir durch dein Bild allein!", "tokens": ["Kommt", "dir", "durch", "dein", "Bild", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}