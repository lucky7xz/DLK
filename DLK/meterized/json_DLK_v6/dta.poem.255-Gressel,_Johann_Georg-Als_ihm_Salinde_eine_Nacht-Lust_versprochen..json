{"dta.poem.255": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als ihm  Salinde  eine Nacht-Lust  \n versprochen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Es bricht die Nacht mit ihrem Schatten ein/", "tokens": ["Es", "bricht", "die", "Nacht", "mit", "ih\u00b7rem", "Schat\u00b7ten", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "ART", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Darin ich soll in deinem Schoosse seyn:", "tokens": ["Da\u00b7rin", "ich", "soll", "in", "dei\u00b7nem", "Schoos\u00b7se", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "VMFIN", "APPR", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Sternen Schein verliehrt sein helles Funckeln/", "tokens": ["Der", "Ster\u00b7nen", "Schein", "ver\u00b7liehrt", "sein", "hel\u00b7les", "Fun\u00b7ckeln", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Nun f\u00fchre du die sch\u00f6nsten Sterne her/", "tokens": ["Nun", "f\u00fch\u00b7re", "du", "die", "sch\u00f6ns\u00b7ten", "Ster\u00b7ne", "her", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Dein Auge la\u00df nicht seyn von Strahlen leer.", "tokens": ["Dein", "Au\u00b7ge", "la\u00df", "nicht", "seyn", "von", "Strah\u00b7len", "leer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVIMP", "PTKNEG", "VAINF", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dein Angesicht sey jetzt des Himmels-Bilde/", "tokens": ["Dein", "An\u00b7ge\u00b7sicht", "sey", "jetzt", "des", "Him\u00b7mels\u00b7Bil\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die sch\u00f6ne Brust ", "tokens": ["Die", "sch\u00f6\u00b7ne", "Brust"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Diana k\u00fc\u00dft Endymion bey Nacht/", "tokens": ["Di\u00b7a\u00b7na", "k\u00fc\u00dft", "En\u00b7dy\u00b7mi\u00b7on", "bey", "Nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Weil denn die Lust in ihrem Schatten lacht/", "tokens": ["Weil", "denn", "die", "Lust", "in", "ih\u00b7rem", "Schat\u00b7ten", "lacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Cythere l\u00e4st in d\u00fcstern Finsternissen", "tokens": ["Cy\u00b7the\u00b7re", "l\u00e4st", "in", "d\u00fcs\u00b7tern", "Fins\u00b7ter\u00b7nis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "In voller Lust sich von dem ", "tokens": ["In", "vol\u00b7ler", "Lust", "sich", "von", "dem"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PRF", "APPR", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So l\u00e4st die Nacht die s\u00fc\u00dfte Wollust zu/", "tokens": ["So", "l\u00e4st", "die", "Nacht", "die", "s\u00fc\u00df\u00b7te", "Wol\u00b7lust", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "PTKZU", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ein Liebender findt in dem Schoosse Ruh:", "tokens": ["Ein", "Lie\u00b7ben\u00b7der", "findt", "in", "dem", "Schoos\u00b7se", "Ruh", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Vers\u00fc\u00dfte Lust/ Freud\u2019 und Erg\u00f6tzlichkeiten/", "tokens": ["Ver\u00b7s\u00fc\u00df\u00b7te", "Lust", "/", "Freud'", "und", "Er\u00b7g\u00f6tz\u00b7lich\u00b7kei\u00b7ten", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Die sind bem\u00fcht den Vor-Rang zu erstreiten.", "tokens": ["Die", "sind", "be\u00b7m\u00fcht", "den", "Vor\u00b7Rang", "zu", "er\u00b7strei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VVPP", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Cupido zollt im dunckeln Seelen-Lust/", "tokens": ["Cu\u00b7pi\u00b7do", "zollt", "im", "dun\u00b7ckeln", "See\u00b7len\u00b7Lust", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und f\u00fcllt damit das Wohn-Haus unser Brust/", "tokens": ["Und", "f\u00fcllt", "da\u00b7mit", "das", "Wohn\u00b7Haus", "un\u00b7ser", "Brust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er setzet uns als Liebes-K\u00fcchenmeister", "tokens": ["Er", "set\u00b7zet", "uns", "als", "Lie\u00b7bes\u00b7K\u00fc\u00b7chen\u00b7meis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "KOUS", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Speisen vor/ so nur vor treue Geister.", "tokens": ["Die", "Spei\u00b7sen", "vor", "/", "so", "nur", "vor", "treu\u00b7e", "Geis\u00b7ter", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$(", "ADV", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So la\u00df auch mich anjetzt mein Engel ein/", "tokens": ["So", "la\u00df", "auch", "mich", "an\u00b7jetzt", "mein", "En\u00b7gel", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ADV", "PPER", "VVFIN", "PPOSAT", "NN", "ART", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Da uns die Zeit recht wil beh\u00fclfflich seyn;", "tokens": ["Da", "uns", "die", "Zeit", "recht", "wil", "be\u00b7h\u00fclf\u00b7flich", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Es locken uns die dunckel-braunen Schatten/", "tokens": ["Es", "lo\u00b7cken", "uns", "die", "dun\u00b7ckel\u00b7brau\u00b7nen", "Schat\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "In Lust sich mit einander zu begatten.", "tokens": ["In", "Lust", "sich", "mit", "ein\u00b7an\u00b7der", "zu", "be\u00b7gat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "APPR", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Du l\u00e4st mich nicht ein widrig Augen-sehn/", "tokens": ["Du", "l\u00e4st", "mich", "nicht", "ein", "wid\u00b7rig", "Au\u00b7gen\u00b7sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ART", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Besondern gleich den frohen Stern auffgehn/", "tokens": ["Be\u00b7son\u00b7dern", "gleich", "den", "fro\u00b7hen", "Stern", "auff\u00b7gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der mir verheist/ da\u00df ich bey der ", "tokens": ["Der", "mir", "ver\u00b7heist", "/", "da\u00df", "ich", "bey", "der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "$(", "KOUS", "PPER", "APPR", "ART"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Kan stracks den Port der Gl\u00fcckes-Insul finden:", "tokens": ["Kan", "stracks", "den", "Port", "der", "Gl\u00fc\u00b7ckes\u00b7In\u00b7sul", "fin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der Insul/ da ein Rosen Bette macht", "tokens": ["Der", "In\u00b7sul", "/", "da", "ein", "Ro\u00b7sen", "Bet\u00b7te", "macht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "KOUS", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die s\u00fcsse Lust/ und die Erg\u00f6tzung lacht;", "tokens": ["Die", "s\u00fcs\u00b7se", "Lust", "/", "und", "die", "Er\u00b7g\u00f6t\u00b7zung", "lacht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wo Anmuht sich in reiche Str\u00f6hme giesset/", "tokens": ["Wo", "An\u00b7muht", "sich", "in", "rei\u00b7che", "Str\u00f6h\u00b7me", "gies\u00b7set", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PRF", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und durch den Thau in unser Geister fliesset.", "tokens": ["Und", "durch", "den", "Thau", "in", "un\u00b7ser", "Geis\u00b7ter", "flies\u00b7set", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Schenck ", "tokens": ["Schenck"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Mach mir die Krafft der ", "tokens": ["Mach", "mir", "die", "Krafft", "der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Ach k\u00fchle mich/ und l\u00f6sche meine Flammen/", "tokens": ["Ach", "k\u00fch\u00b7le", "mich", "/", "und", "l\u00f6\u00b7sche", "mei\u00b7ne", "Flam\u00b7men", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "$(", "KON", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die keiner kan als ungerecht verdammen.", "tokens": ["Die", "kei\u00b7ner", "kan", "als", "un\u00b7ge\u00b7recht", "ver\u00b7dam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "KOKOM", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mich hat dein Strahl/ mein Engels-Kind/ verletzt/", "tokens": ["Mich", "hat", "dein", "Strahl", "/", "mein", "En\u00b7gels\u00b7Kind", "/", "ver\u00b7letzt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "$(", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und durch den Blitz in Brand und Gluht gesetzt.", "tokens": ["Und", "durch", "den", "Blitz", "in", "Brand", "und", "Gluht", "ge\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.8": {"text": "Daraus du nur/ mein Leben/ mich