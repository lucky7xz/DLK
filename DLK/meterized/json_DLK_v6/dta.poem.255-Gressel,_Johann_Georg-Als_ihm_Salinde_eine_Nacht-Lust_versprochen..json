", "will", "/", "ich", "will", "!", "dein", "s\u00fcs\u00b7ses", "M\u00fcnd\u00b7gen", "spricht", "/"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "$(", "PPER", "VMFIN", "$.", "PPOSAT", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "\u201edein matter Geist erh\u00e4lt ein neues Licht.", "tokens": ["\u201e", "dein", "mat\u00b7ter", "Geist", "er\u00b7h\u00e4lt", "ein", "neu\u00b7es", "Licht", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "\u201e", "tokens": ["\u201e"], "token_info": ["punct"], "pos": ["$("]}, "line.8": {"text": "\u201eein Busen-Oehl und Lippen-Safft erfunden.", "tokens": ["\u201e", "ein", "Bu\u00b7sen\u00b7Oehl", "und", "Lip\u00b7pen\u00b7\u00b7S\u00b7afft", "er\u00b7fun\u00b7den", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "So spricht mein Schatz/ so denckt mein Engels-Kind", "tokens": ["So", "spricht", "mein", "Schatz", "/", "so", "denckt", "mein", "En\u00b7gels\u00b7Kind"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$(", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und zeigt/ da\u00df sie mir H\u00fclffe stets ersinnt/", "tokens": ["Und", "zeigt", "/", "da\u00df", "sie", "mir", "H\u00fclf\u00b7fe", "stets", "er\u00b7sinnt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "KOUS", "PPER", "PPER