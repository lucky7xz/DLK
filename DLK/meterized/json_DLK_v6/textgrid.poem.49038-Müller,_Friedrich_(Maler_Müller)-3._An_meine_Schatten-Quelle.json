{"textgrid.poem.49038": {"metadata": {"author": {"name": "M\u00fcller, Friedrich (Maler M\u00fcller)", "birth": "N.A.", "death": "N.A."}, "title": "3. An meine Schatten-Quelle", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vom goldnen Becher rinnt der Saft der Reben,", "tokens": ["Vom", "gold\u00b7nen", "Be\u00b7cher", "rinnt", "der", "Saft", "der", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Indem mein froher Mund dich neugebohrne gr\u00fc\u00dft,", "tokens": ["In\u00b7dem", "mein", "fro\u00b7her", "Mund", "dich", "neu\u00b7ge\u00b7bohr\u00b7ne", "gr\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In deine Silberwelle, die so sanft und eben", "tokens": ["In", "dei\u00b7ne", "Sil\u00b7ber\u00b7wel\u00b7le", ",", "die", "so", "sanft", "und", "e\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "ADV", "ADJD", "KON", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durch Blumenb\u00fcsche schattigt flie\u00dft.", "tokens": ["Durch", "Blu\u00b7men\u00b7b\u00fc\u00b7sche", "schat\u00b7tigt", "flie\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O Quellen-K\u00f6nigin! \u2013 Voll klopfenden Verlangen", "tokens": ["O", "Quel\u00b7len\u00b7K\u00f6\u00b7ni\u00b7gin", "!", "\u2013", "Voll", "klop\u00b7fen\u00b7den", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$.", "$(", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Beth ich und opfere, damit es ihm gelingt", "tokens": ["Be\u00b7th", "ich", "und", "op\u00b7fe\u00b7re", ",", "da\u00b7mit", "es", "ihm", "ge\u00b7lingt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "$,", "KOUS", "PPER", "PPER", "VVFIN"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Dem vollen Mond, der mit erhitzten Wangen", "tokens": ["Dem", "vol\u00b7len", "Mond", ",", "der", "mit", "er\u00b7hitz\u00b7ten", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sich aus des Abgrunds Armen schwingt.", "tokens": ["Sich", "aus", "des", "Ab\u00b7grunds", "Ar\u00b7men", "schwingt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Schon blickt er auf in seinem Schimmerlichte; \u2013", "tokens": ["Schon", "blickt", "er", "auf", "in", "sei\u00b7nem", "Schim\u00b7mer\u00b7lich\u00b7te", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schon steigst du wieder aus der D\u00e4mmerung hervor", "tokens": ["Schon", "steigst", "du", "wie\u00b7der", "aus", "der", "D\u00e4m\u00b7me\u00b7rung", "her\u00b7vor"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Voll Herrlichkeit \u2013 zwar flattert deinem Schneegesichte", "tokens": ["Voll", "Herr\u00b7lich\u00b7keit", "\u2013", "zwar", "flat\u00b7tert", "dei\u00b7nem", "Schnee\u00b7ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "NN", "$(", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein Wittwen-Trauerschleyer vor.", "tokens": ["Ein", "Witt\u00b7wen\u00b7Trau\u00b7er\u00b7schley\u00b7er", "vor", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sey mir gegr\u00fc\u00dfet, die du aus dem Reich der N\u00e4chte", "tokens": ["Sey", "mir", "ge\u00b7gr\u00fc\u00b7\u00dfet", ",", "die", "du", "aus", "dem", "Reich", "der", "N\u00e4ch\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "PRELS", "PPER", "APPR", "ART", "NN", "ART", "NN"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Gestiegen, nun mit neuem m\u00e4chtgen Glanz!", "tokens": ["Ge\u00b7stie\u00b7gen", ",", "nun", "mit", "neu\u00b7em", "m\u00e4cht\u00b7gen", "Glanz", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Um deine Silberschl\u00e4fe will ich flechten,", "tokens": ["Um", "dei\u00b7ne", "Sil\u00b7ber\u00b7schl\u00e4\u00b7fe", "will", "ich", "flech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den blumumwundenen Binsenkranz.", "tokens": ["Den", "blu\u00b7mum\u00b7wun\u00b7de\u00b7nen", "Bin\u00b7sen\u00b7kranz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Wie damals er um deine Stirne sau\u00dfte,", "tokens": ["Wie", "da\u00b7mals", "er", "um", "dei\u00b7ne", "Stir\u00b7ne", "sau\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als f\u00fcrchterlich durch B\u00fcsche, neben deinem Haupt,", "tokens": ["Als", "f\u00fcrch\u00b7ter\u00b7lich", "durch", "B\u00fc\u00b7sche", ",", "ne\u00b7ben", "dei\u00b7nem", "Haupt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "APPR", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Des sch\u00f6nsten J\u00fcnglings Sieges-Wagen brau\u00dfte,", "tokens": ["Des", "sch\u00f6ns\u00b7ten", "J\u00fcng\u00b7lings", "Sie\u00b7ges\u00b7Wa\u00b7gen", "brau\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sein Streitro\u00df dampfend dich umschnaubt.", "tokens": ["Sein", "Strei\u00b7tro\u00df", "damp\u00b7fend", "dich", "um\u00b7schnaubt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der Morgen fand ihn, Sch\u00f6nste dich zu retten", "tokens": ["Der", "Mor\u00b7gen", "fand", "ihn", ",", "Sch\u00f6ns\u00b7te", "dich", "zu", "ret\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom Drachen, der mit stolzer Zaubermacht,", "tokens": ["Vom", "Dra\u00b7chen", ",", "der", "mit", "stol\u00b7zer", "Zau\u00b7ber\u00b7macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Neunmal neun Monden dich an goldnen Ketten,", "tokens": ["Neun\u00b7mal", "neun", "Mon\u00b7den", "dich", "an", "gold\u00b7nen", "Ket\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "In deiner Schattenfluth bewacht.", "tokens": ["In", "dei\u00b7ner", "Schat\u00b7ten\u00b7fluth", "be\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Voll Liebe brannt dein Herze zu dem Sch\u00f6nen", "tokens": ["Voll", "Lie\u00b7be", "brannt", "dein", "Her\u00b7ze", "zu", "dem", "Sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "NN", "VVFIN", "PPOSAT", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der wie ein Gott das Schwerd dem Kampf entgegen trug,", "tokens": ["Der", "wie", "ein", "Gott", "das", "Schwerd", "dem", "Kampf", "ent\u00b7ge\u00b7gen", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "ART", "NN", "ART", "NN", "ART", "NN", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da zitterte f\u00fcr ihn dein Aug in bangen Thr\u00e4nen;", "tokens": ["Da", "zit\u00b7ter\u00b7te", "f\u00fcr", "ihn", "dein", "Aug", "in", "ban\u00b7gen", "Thr\u00e4\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Erschrocken sankst du auf den Nymphen-Krug;", "tokens": ["Er\u00b7schro\u00b7cken", "sankst", "du", "auf", "den", "Nym\u00b7phen\u00b7Krug", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Bis da\u00df des Sieges hoher Ruf dich weckte,", "tokens": ["Bis", "da\u00df", "des", "Sie\u00b7ges", "ho\u00b7her", "Ruf", "dich", "weck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "NN", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den der Posaunen-Mund durchs Thal und Klippen stie\u00df,", "tokens": ["Den", "der", "Po\u00b7sau\u00b7nen\u00b7Mund", "durchs", "Thal", "und", "Klip\u00b7pen", "stie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPRART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und seine blutge Faust der J\u00fcngling nach dir streckte,", "tokens": ["Und", "sei\u00b7ne", "blut\u00b7ge", "Faust", "der", "J\u00fcng\u00b7ling", "nach", "dir", "streck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Fessel dir von deinem Nacken ri\u00df. \u2013", "tokens": ["Die", "Fes\u00b7sel", "dir", "von", "dei\u00b7nem", "Na\u00b7cken", "ri\u00df", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Frohlockend sprangst du aus der Silberwelle,", "tokens": ["Froh\u00b7lo\u00b7ckend", "sprangst", "du", "aus", "der", "Sil\u00b7ber\u00b7wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hiengst mit umschlungnem Arm, als wie das falbe Licht", "tokens": ["Hiengst", "mit", "um\u00b7schlung\u00b7nem", "Arm", ",", "als", "wie", "das", "fal\u00b7be", "Licht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,", "KOUS", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Des fr\u00fchen Morgensterns \u2013 so klar und helle", "tokens": ["Des", "fr\u00fc\u00b7hen", "Mor\u00b7gens\u00b7terns", "\u2013", "so", "klar", "und", "hel\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "ADV", "ADJD", "KON", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um deines J\u00fcnglings Angesicht!", "tokens": ["Um", "dei\u00b7nes", "J\u00fcng\u00b7lings", "An\u00b7ge\u00b7sicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Doch G\u00f6ttin, ach! betrogen vom Geschicke", "tokens": ["Doch", "G\u00f6t\u00b7tin", ",", "ach", "!", "be\u00b7tro\u00b7gen", "vom", "Ge\u00b7schi\u00b7cke"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "$,", "ITJ", "$.", "VVPP", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Genossest du nicht lange dieser s\u00fc\u00dfen Lust:", "tokens": ["Ge\u00b7nos\u00b7sest", "du", "nicht", "lan\u00b7ge", "die\u00b7ser", "s\u00fc\u00b7\u00dfen", "Lust", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn, ach! dein J\u00fcngling fiel mit starrem Blicke", "tokens": ["Denn", ",", "ach", "!", "dein", "J\u00fcng\u00b7ling", "fiel", "mit", "star\u00b7rem", "Bli\u00b7cke"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "ITJ", "$.", "PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Herab an deine G\u00f6tter-Brust!", "tokens": ["Her\u00b7ab", "an", "dei\u00b7ne", "G\u00f6t\u00b7ter\u00b7Brust", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Ach! damals horchten deiner Trauerklage", "tokens": ["Ach", "!", "da\u00b7mals", "horch\u00b7ten", "dei\u00b7ner", "Trau\u00b7er\u00b7kla\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des H\u00fcgels schnelle Nymphen und die Flu\u00dfg\u00f6ttin.", "tokens": ["Des", "H\u00fc\u00b7gels", "schnel\u00b7le", "Nym\u00b7phen", "und", "die", "Flu\u00df\u00b7g\u00f6t\u00b7tin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.3": {"text": "Mit ungek\u00e4mmtem Haar lagst du dreymal drey Tage,", "tokens": ["Mit", "un\u00b7ge\u00b7k\u00e4mm\u00b7tem", "Haar", "lagst", "du", "drey\u00b7mal", "drey", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ADV", "CARD", "NN", "$,"], "meter": "-+-+-+--+-+--", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und schluchz'st und jammertest um ihn.", "tokens": ["Und", "schluch\u00b7z'st", "und", "jam\u00b7mer\u00b7test", "um", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "APPR", "PPER", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.12": {"line.1": {"text": "Bald drangst du deine Wellen durch das Reich der Schatten", "tokens": ["Bald", "drangst", "du", "dei\u00b7ne", "Wel\u00b7len", "durch", "das", "Reich", "der", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zur\u00fcck, am Strohm der Finsterni\u00df", "tokens": ["Zu\u00b7r\u00fcck", ",", "am", "Strohm", "der", "Fins\u00b7ter\u00b7ni\u00df"], "token_info": ["word", "punct", "word", "word", "word", "word"], "po