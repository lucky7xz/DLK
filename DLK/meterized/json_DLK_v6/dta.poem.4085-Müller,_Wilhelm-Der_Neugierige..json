{"dta.poem.4085": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Neugierige.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich frage keinen Stern,", "tokens": ["Ich", "fra\u00b7ge", "kei\u00b7nen", "Stern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Sie k\u00f6nnen mir alle nicht sagen,", "tokens": ["Sie", "k\u00f6n\u00b7nen", "mir", "al\u00b7le", "nicht", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PIS", "PTKNEG", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Was ich erf\u00fchr' so gern.", "tokens": ["Was", "ich", "er\u00b7f\u00fchr'", "so", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich bin ja auch kein G\u00e4rtner,", "tokens": ["Ich", "bin", "ja", "auch", "kein", "G\u00e4rt\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Sterne stehn zu hoch;", "tokens": ["Die", "Ster\u00b7ne", "stehn", "zu", "hoch", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein B\u00e4chlein will ich fragen,", "tokens": ["Mein", "B\u00e4ch\u00b7lein", "will", "ich", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ob mich mein Herz belog.", "tokens": ["Ob", "mich", "mein", "Herz", "be\u00b7log", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "O B\u00e4chlein meiner Liebe,", "tokens": ["O", "B\u00e4ch\u00b7lein", "mei\u00b7ner", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie bist du heut so stumm!", "tokens": ["Wie", "bist", "du", "heut", "so", "stumm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Will ja nur Eines wissen,", "tokens": ["Will", "ja", "nur", "Ei\u00b7nes", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ja, hei\u00dft das eine W\u00f6rtchen,", "tokens": ["Ja", ",", "hei\u00dft", "das", "ei\u00b7ne", "W\u00f6rt\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "PDS", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das andre hei\u00dfet Nein,", "tokens": ["Das", "and\u00b7re", "hei\u00b7\u00dfet", "Nein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "PTKANT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die beiden W\u00f6rtchen schlie\u00dfen", "tokens": ["Die", "bei\u00b7den", "W\u00f6rt\u00b7chen", "schlie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die ganze Welt mir ein.", "tokens": ["Die", "gan\u00b7ze", "Welt", "mir", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "O B\u00e4chlein meiner Liebe,", "tokens": ["O", "B\u00e4ch\u00b7lein", "mei\u00b7ner", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was bist du wunderlich!", "tokens": ["Was", "bist", "du", "wun\u00b7der\u00b7lich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Will's ja nicht weiter sagen,", "tokens": ["Will's", "ja", "nicht", "wei\u00b7ter", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sag', B\u00e4chlein, liebt sie mich?", "tokens": ["Sag'", ",", "B\u00e4ch\u00b7lein", ",", "liebt", "sie", "mich", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}