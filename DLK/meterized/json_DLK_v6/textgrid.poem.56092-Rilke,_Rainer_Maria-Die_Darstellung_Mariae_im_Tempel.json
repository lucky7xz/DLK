{"textgrid.poem.56092": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Die Darstellung Mariae im Tempel", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Um zu begreifen, wie sie damals war,", "tokens": ["Um", "zu", "be\u00b7grei\u00b7fen", ",", "wie", "sie", "da\u00b7mals", "war", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PTKZU", "VVINF", "$,", "PWAV", "PPER", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "mu\u00dft du dich erst an eine Stelle rufen,", "tokens": ["mu\u00dft", "du", "dich", "erst", "an", "ei\u00b7ne", "Stel\u00b7le", "ru\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "wo S\u00e4ulen in dir wirken; wo du Stufen", "tokens": ["wo", "S\u00e4u\u00b7len", "in", "dir", "wir\u00b7ken", ";", "wo", "du", "Stu\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "NN", "APPR", "PPER", "VVINF", "$.", "PWAV", "PPER", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "nachf\u00fchlen kannst; wo Bogen voll Gefahr", "tokens": ["nach\u00b7f\u00fch\u00b7len", "kannst", ";", "wo", "Bo\u00b7gen", "voll", "Ge\u00b7fahr"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVINF", "VMFIN", "$.", "PWAV", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "den Abgrund eines Raumes \u00fcberbr\u00fccken,", "tokens": ["den", "Ab\u00b7grund", "ei\u00b7nes", "Rau\u00b7mes", "\u00fc\u00b7berb\u00b7r\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "der in dir blieb, weil er aus solchen St\u00fccken", "tokens": ["der", "in", "dir", "blieb", ",", "weil", "er", "aus", "sol\u00b7chen", "St\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPER", "VVFIN", "$,", "KOUS", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "get\u00fcrmt war, da\u00df du sie nicht mehr aus dir", "tokens": ["ge\u00b7t\u00fcrmt", "war", ",", "da\u00df", "du", "sie", "nicht", "mehr", "aus", "dir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "$,", "KOUS", "PPER", "PPER", "PTKNEG", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "ausheben kannst: du rissest dich denn ein.", "tokens": ["aus\u00b7he\u00b7ben", "kannst", ":", "du", "ris\u00b7sest", "dich", "denn", "ein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVINF", "VMFIN", "$.", "PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.9": {"text": "Bist du so weit, ist alles in dir Stein,", "tokens": ["Bist", "du", "so", "weit", ",", "ist", "al\u00b7les", "in", "dir", "Stein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$,", "VAFIN", "PIS", "APPR", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Wand, Aufgang, Durchblick, W\u00f6lbung \u2013, so probier", "tokens": ["Wand", ",", "Auf\u00b7gang", ",", "Durch\u00b7blick", ",", "W\u00f6l\u00b7bung", "\u2013", ",", "so", "pro\u00b7bier"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "punct", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$(", "$,", "ADV", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "den gro\u00dfen Vorhang, den du vor dir hast,", "tokens": ["den", "gro\u00b7\u00dfen", "Vor\u00b7hang", ",", "den", "du", "vor", "dir", "hast", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "APPR", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "ein wenig wegzuzerrn mit beiden H\u00e4nden:", "tokens": ["ein", "we\u00b7nig", "weg\u00b7zu\u00b7zerrn", "mit", "bei\u00b7den", "H\u00e4n\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "da gl\u00e4nzt es von ganz hohen Gegenst\u00e4nden", "tokens": ["da", "gl\u00e4nzt", "es", "von", "ganz", "ho\u00b7hen", "Ge\u00b7gen\u00b7st\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "und \u00fcbertrifft dir Atem und Getast.", "tokens": ["und", "\u00fc\u00b7bert\u00b7rifft", "dir", "A\u00b7tem", "und", "Ge\u00b7tast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Hinauf, hinab, Palast steht auf Palast,", "tokens": ["Hin\u00b7auf", ",", "hin\u00b7ab", ",", "Pa\u00b7last", "steht", "auf", "Pa\u00b7last", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PTKVZ", "$,", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Gel\u00e4nder str\u00f6men breiter aus Gel\u00e4ndern", "tokens": ["Ge\u00b7l\u00e4n\u00b7der", "str\u00f6\u00b7men", "brei\u00b7ter", "aus", "Ge\u00b7l\u00e4n\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "und tauchen oben auf an solchen R\u00e4ndern,", "tokens": ["und", "tau\u00b7chen", "o\u00b7ben", "auf", "an", "sol\u00b7chen", "R\u00e4n\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "da\u00df dich, wie du sie siehst, der Schwindel fa\u00dft.", "tokens": ["da\u00df", "dich", ",", "wie", "du", "sie", "siehst", ",", "der", "Schwin\u00b7del", "fa\u00dft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Dabei macht ein Gew\u00f6lk aus R\u00e4ucherst\u00e4ndern", "tokens": ["Da\u00b7bei", "macht", "ein", "Ge\u00b7w\u00f6lk", "aus", "R\u00e4u\u00b7cher\u00b7st\u00e4n\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "NN", "APPR", "NN"], "meter": "-+++-+-+-+-", "measure": "unknown.measure.hexa"}, "line.20": {"text": "die N\u00e4he tr\u00fcb; aber das Fernste zielt", "tokens": ["die", "N\u00e4\u00b7he", "tr\u00fcb", ";", "a\u00b7ber", "das", "Ferns\u00b7te", "zielt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "$.", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.21": {"text": "in dich hinein mit seinen graden Strahlen \u2013,", "tokens": ["in", "dich", "hin\u00b7ein", "mit", "sei\u00b7nen", "gra\u00b7den", "Strah\u00b7len", "\u2013", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPER", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$(", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "und wenn jetzt Schein aus klaren Flammenschalen", "tokens": ["und", "wenn", "jetzt", "Schein", "aus", "kla\u00b7ren", "Flam\u00b7men\u00b7scha\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "auf langsam nahenden Gew\u00e4ndern spielt:", "tokens": ["auf", "lang\u00b7sam", "na\u00b7hen\u00b7den", "Ge\u00b7w\u00e4n\u00b7dern", "spielt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.24": {"text": "wie h\u00e4ltst du's aus?", "tokens": ["wie", "h\u00e4ltst", "du's", "aus", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PIS", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Sie aber kam und hob", "tokens": ["Sie", "a\u00b7ber", "kam", "und", "hob"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "den Blick, um dieses alles anzuschauen.", "tokens": ["den", "Blick", ",", "um", "die\u00b7ses", "al\u00b7les", "an\u00b7zu\u00b7schau\u00b7en", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUI", "PDAT", "PIS", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "(ein Kind, ein kleines M\u00e4dchen zwischen Frauen.)", "tokens": ["(", "ein", "Kind", ",", "ein", "klei\u00b7nes", "M\u00e4d\u00b7chen", "zwi\u00b7schen", "Frau\u00b7en", ".", ")"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "$,", "ART", "ADJA", "NN", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dann stieg sie ruhig, voller Selbstvertrauen,", "tokens": ["Dann", "stieg", "sie", "ru\u00b7hig", ",", "vol\u00b7ler", "Selbst\u00b7ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "dem Aufwand zu, der sich verw\u00f6hnt verschob:", "tokens": ["dem", "Auf\u00b7wand", "zu", ",", "der", "sich", "ver\u00b7w\u00f6hnt", "ver\u00b7schob", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "PRELS", "PRF", "VVPP", "VVFIN", "$."], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.6": {"text": "So sehr war alles, was die Menschen bauen,", "tokens": ["So", "sehr", "war", "al\u00b7les", ",", "was", "die", "Men\u00b7schen", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PIS", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "schon \u00fcberwogen von dem Lob", "tokens": ["schon", "\u00fc\u00b7ber\u00b7wo\u00b7gen", "von", "dem", "Lob"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "in ihrem Herzen. Von der Lust", "tokens": ["in", "ih\u00b7rem", "Her\u00b7zen", ".", "Von", "der", "Lust"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$.", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "sich hinzugeben an die innern Zeichen:", "tokens": ["sich", "hin\u00b7zu\u00b7ge\u00b7ben", "an", "die", "in\u00b7nern", "Zei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Eltern meinten, sie hinaufzureichen,", "tokens": ["Die", "El\u00b7tern", "mein\u00b7ten", ",", "sie", "hin\u00b7auf\u00b7zu\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "der Drohende mit der Juwelenbrust", "tokens": ["der", "Dro\u00b7hen\u00b7de", "mit", "der", "Ju\u00b7we\u00b7len\u00b7brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "empfing sie scheinbar: Doch sie ging durch alle,", "tokens": ["emp\u00b7fing", "sie", "schein\u00b7bar", ":", "Doch", "sie", "ging", "durch", "al\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$.", "KON", "PPER", "VVFIN", "APPR", "PIS", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "klein wie sie war, aus jeder Hand hinaus", "tokens": ["klein", "wie", "sie", "war", ",", "aus", "je\u00b7der", "Hand", "hin\u00b7aus"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "PPER", "VAFIN", "$,", "APPR", "PIAT", "NN", "APZR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "und in ihr Los, das, h\u00f6her als die Halle,", "tokens": ["und", "in", "ihr", "Los", ",", "das", ",", "h\u00f6\u00b7her", "als", "die", "Hal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "$,", "PDS", "$,", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "schon fertig war, und schwerer als das Haus.", "tokens": ["schon", "fer\u00b7tig", "war", ",", "und", "schwe\u00b7rer", "als", "das", "Haus", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "$,", "KON", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}