"word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "VVFIN", "ADV", "APPR", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Jhr Kenner menschlicher Natur!", "tokens": ["Ihr", "Ken\u00b7ner", "menschli\u00b7cher", "Na\u00b7tur", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Jedoch mit diesem Unterschiede:", "tokens": ["Je\u00b7doch", "mit", "die\u00b7sem", "Un\u00b7ter\u00b7schie\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Jhr urtheilt durch die Zunge nur.", "tokens": ["Ihr", "ur\u00b7theilt", "durch", "die", "Zun\u00b7ge", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Hier schmeckt ein Trunk! der Hand Gelenke", "tokens": ["Hier", "schmeckt", "ein", "Trunk", "!", "der", "Hand", "Ge\u00b7len\u00b7ke"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kommt in Bewegung, her du Glas,", "tokens": ["Kommt", "in", "Be\u00b7we\u00b7gung", ",", "her", "du", "Glas", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,", "ADV", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Worein ich oft die Sorgen senke,", "tokens": ["Wo\u00b7rein", "ich", "oft", "die", "Sor\u00b7gen", "sen\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mir winkt dein r\u00f6thlichbraunes Na\u00df;", "tokens": ["Mir", "winkt", "dein", "r\u00f6th\u00b7lich\u00b7brau\u00b7nes", "Na\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich habe Lust, dich auszuleeren,", "tokens": ["Ich", "ha\u00b7be", "Lust", ",", "dich", "aus\u00b7zu\u00b7lee\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$,", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "We\u00dfwegen bist du sonsten da?", "tokens": ["We\u00df\u00b7we\u00b7gen", "bist", "du", "sons\u00b7ten", "da", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "VMFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dein Fu\u00df soll sich zu oberst kehren \u2012 \u2012", "tokens": ["Dein", "Fu\u00df", "soll", "sich", "zu", "o\u00b7berst", "keh\u00b7ren", "\u2012", "\u2012"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "PRF", "APPR", "ADV", "VVINF", "$(", "$("], "meter": "-+---+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Das k\u00fchlet unvergleichlich! ha!", "tokens": ["Das", "k\u00fch\u00b7let", "un\u00b7ver\u00b7gleich\u00b7lich", "!", "ha", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "$.", "ITJ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Fort! holla! hurtig! eingeschenket!", "tokens": ["Fort", "!", "hol\u00b7la", "!", "hur\u00b7tig", "!", "ein\u00b7ge\u00b7schen\u00b7ket", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "NE", "$.", "ADJD", "$.", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Noch eins, und denn das dritte noch!", "tokens": ["Noch", "eins", ",", "und", "denn", "das", "drit\u00b7te", "noch", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "$,", "KON", "ADV", "ART", "ADJA", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es lebe, was oft mein gedenket!", "tokens": ["Es", "le\u00b7be", ",", "was", "oft", "mein", "ge\u00b7den\u00b7ket", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "ADV", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es lebe, was mich liebet, hoch!", "tokens": ["Es", "le\u00b7be", ",", "was", "mich", "lie\u00b7bet", ",", "hoch", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn ich oft so die Lippen wasche,", "tokens": ["Wenn", "ich", "oft", "so", "die", "Lip\u00b7pen", "wa\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "So schmeckt es als der beste Wein", "tokens": ["So", "schmeckt", "es", "als", "der", "bes\u00b7te", "Wein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Aus Jupiters Burgunder-Flasche,", "tokens": ["Aus", "Ju\u00b7pi\u00b7ters", "Bur\u00b7gun\u00b7der\u00b7Fla\u00b7sche", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ich aber bin best\u00e4ndig mein.", "tokens": ["Ich", "a\u00b7ber", "bin", "be\u00b7st\u00e4n\u00b7dig", "mein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADJD", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Mich zwingt kein fremdes Wohlergehen,", "tokens": ["Mich", "zwingt", "kein", "frem\u00b7des", "Woh\u00b7ler\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Jtzt meinem eignen Tort zu thun,", "tokens": ["Jtzt", "mei\u00b7nem", "eig\u00b7nen", "Tort", "zu", "thun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer will Gesundheit trinken sehen,", "tokens": ["Wer", "will", "Ge\u00b7sund\u00b7heit", "trin\u00b7ken", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der lasse doch die meine ruhn!", "tokens": ["Der", "las\u00b7se", "doch", "die", "mei\u00b7ne", "ruhn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ART", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich bin geneigt, sie aufzuheben,", "tokens": ["Ich", "bin", "ge\u00b7neigt", ",", "sie", "auf\u00b7zu\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Man gab sie mir, sie ist es werth.", "tokens": ["Man", "gab", "sie", "mir", ",", "sie", "ist", "es", "werth", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PPER", "$,", "PPER", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Man isst und trinket, nur zu leben,", "tokens": ["Man", "isst", "und", "trin\u00b7ket", ",", "nur", "zu", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KON", "VVFIN", "$,", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mit nichten aber umgekehrt.", "tokens": ["Mit", "nich\u00b7ten", "a\u00b7ber", "um\u00b7ge\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Gottlob! mein K\u00f6rper ist versorget", "tokens": ["Gott\u00b7lob", "!", "mein", "K\u00f6r\u00b7per", "ist", "ver\u00b7sor\u00b7get"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "PPOSAT", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf einen ganzen langen Tag!", "tokens": ["Auf", "ei\u00b7nen", "gan\u00b7zen", "lan\u00b7gen", "Tag", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sein Unterhalt war nicht geborget,", "tokens": ["Sein", "Un\u00b7ter\u00b7halt", "war", "nicht", "ge\u00b7bor\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df er ihn morgen finden mag!", "tokens": ["Da\u00df", "er", "ihn", "mor\u00b7gen", "fin\u00b7den", "mag", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich bin vergn\u00fcgt, als wie ein K\u00f6nig,", "tokens": ["Ich", "bin", "ver\u00b7gn\u00fcgt", ",", "als", "wie", "ein", "K\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "KOUS", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und ebenfalls wie er so satt!", "tokens": ["Und", "e\u00b7ben\u00b7falls", "wie", "er", "so", "satt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOKOM", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein Mensch hat auch gewi\u00df nicht wenig,", "tokens": ["Ein", "Mensch", "hat", "auch", "ge\u00b7wi\u00df", "nicht", "we\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der frohen Muth und Speisen hat.", "tokens": ["Der", "fro\u00b7hen", "Muth", "und", "Spei\u00b7sen", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}