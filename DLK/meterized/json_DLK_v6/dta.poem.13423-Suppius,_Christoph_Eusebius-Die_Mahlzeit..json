{"dta.poem.13423": {"metadata": {"author": {"name": "Suppius, Christoph Eusebius", "birth": "N.A.", "death": "N.A."}, "title": "Die Mahlzeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1749", "urn": "urn:nbn:de:kobv:b4-20594-7", "language": ["de:0.85", "no:0.14"], "booktitle": "Suppius, Christoph Eusebius: Oden und Lieder. Gotha, 1749."}, "poem": {"stanza.1": {"line.1": {"text": "Fort, Hunger! komm! wir wollen essen!", "tokens": ["Fort", ",", "Hun\u00b7ger", "!", "komm", "!", "wir", "wol\u00b7len", "es\u00b7sen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$.", "VVFIN", "$.", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Tisch ist allbereits gedeckt!", "tokens": ["Mein", "Tisch", "ist", "all\u00b7be\u00b7reits", "ge\u00b7deckt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da sind die Polster zwar vergessen,", "tokens": ["Da", "sind", "die", "Pols\u00b7ter", "zwar", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf welche man die Glieder streckt,", "tokens": ["Auf", "wel\u00b7che", "man", "die", "Glie\u00b7der", "streckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch unser aufger\u00e4umt Gesichte", "tokens": ["Doch", "un\u00b7ser", "auf\u00b7ge\u00b7r\u00e4umt", "Ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "VVPP", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zeigt auch bey harten Bretern an,", "tokens": ["Zeigt", "auch", "bey", "har\u00b7ten", "Bre\u00b7tern", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df sich des K\u00f6rpers Gleichgewichte", "tokens": ["Da\u00df", "sich", "des", "K\u00f6r\u00b7pers", "Gleich\u00b7ge\u00b7wich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Demselben anvertrauen kann.", "tokens": ["Dem\u00b7sel\u00b7ben", "an\u00b7ver\u00b7trau\u00b7en", "kann", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie steht es aber um die Trachten?", "tokens": ["Wie", "steht", "es", "a\u00b7ber", "um", "die", "Trach\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie sieht es um die Gl\u00e4ser aus?", "tokens": ["Wie", "sieht", "es", "um", "die", "Gl\u00e4\u00b7ser", "aus", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O Mahl! das h\u00f6her noch zu achten,", "tokens": ["O", "Mahl", "!", "das", "h\u00f6\u00b7her", "noch", "zu", "ach\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ART", "ADJA", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als der Poeten G\u00f6tter-Schmaus;", "tokens": ["Als", "der", "Po\u00b7et\u00b7en", "G\u00f6t\u00b7ter\u00b7Schmaus", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nach der zuvor gemachten M\u00fche", "tokens": ["Nach", "der", "zu\u00b7vor", "ge\u00b7mach\u00b7ten", "M\u00fc\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.6": {"text": "Vers\u00f6hnen wir des Magens Wuth,", "tokens": ["Ver\u00b7s\u00f6h\u00b7nen", "wir", "des", "Ma\u00b7gens", "Wuth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da schmeckt uns auch die schwarze Br\u00fche", "tokens": ["Da", "schmeckt", "uns", "auch", "die", "schwar\u00b7ze", "Br\u00fc\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der scherzenden Spartaner gut.", "tokens": ["Der", "scher\u00b7zen\u00b7den", "Spar\u00b7ta\u00b7ner", "gut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Hat mit den jungen Elamiten", "tokens": ["Hat", "mit", "den", "jun\u00b7gen", "E\u00b7la\u00b7mi\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wohl Cyrus besser sich gepflegt?", "tokens": ["Wohl", "Cy\u00b7rus", "bes\u00b7ser", "sich", "ge\u00b7pflegt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "ADJD", "PRF", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die E\u00dfbegierde l\u00e4\u00dft sich bieten,", "tokens": ["Die", "E\u00df\u00b7be\u00b7gier\u00b7de", "l\u00e4\u00dft", "sich", "bie\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was ihr das Gl\u00fccke vorgelegt,", "tokens": ["Was", "ihr", "das", "Gl\u00fc\u00b7cke", "vor\u00b7ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ganz junge Kr\u00e4uter, die noch heute", "tokens": ["Ganz", "jun\u00b7ge", "Kr\u00e4u\u00b7ter", ",", "die", "noch", "heu\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "$,", "PRELS", "ADV", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Morgenthau begeistert hat,", "tokens": ["Der", "Mor\u00b7gent\u00b7hau", "be\u00b7geis\u00b7tert", "hat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sind ihre frische Mittags-Beute", "tokens": ["Sind", "ih\u00b7re", "fri\u00b7sche", "Mit\u00b7tags\u00b7Beu\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "In dem erleichternden Sallat.", "tokens": ["In", "dem", "er\u00b7leich\u00b7tern\u00b7den", "Sal\u00b7lat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Ich darf, die Zunge zu ergetzen,", "tokens": ["Ich", "darf", ",", "die", "Zun\u00b7ge", "zu", "er\u00b7get\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und andern Sinnen sanft zu thun,", "tokens": ["Und", "an\u00b7dern", "Sin\u00b7nen", "sanft", "zu", "thun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Heer nicht in Bewegung setzen,", "tokens": ["Ein", "Heer", "nicht", "in", "Be\u00b7we\u00b7gung", "set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Viel H\u00e4nde k\u00f6nnen vor mir ruhn;", "tokens": ["Viel", "H\u00e4n\u00b7de", "k\u00f6n\u00b7nen", "vor", "mir", "ruhn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Tr\u00e4gt mir die Erde nur noch Fr\u00fcchte,", "tokens": ["Tr\u00e4gt", "mir", "die", "Er\u00b7de", "nur", "noch", "Fr\u00fcch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "ADV", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "B\u00e4ckt mir der Ofen kr\u00e4ftig Brodt,", "tokens": ["B\u00e4ckt", "mir", "der", "O\u00b7fen", "kr\u00e4f\u00b7tig", "Brodt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So schickt der Mittag sein Gerichte,", "tokens": ["So", "schickt", "der", "Mit\u00b7tag", "sein", "Ge\u00b7rich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und \u00fcberhebt mich aller Noth.", "tokens": ["Und", "\u00fc\u00b7ber\u00b7hebt", "mich", "al\u00b7ler", "Noth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Nie saget mir der Haus-Calender,", "tokens": ["Nie", "sa\u00b7get", "mir", "der", "Haus\u00b7Ca\u00b7len\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}, "line.2": {"text": "Wenn Flu\u00df und Wald sein Wildpret h\u00e4uft:", "tokens": ["Wenn", "Flu\u00df", "und", "Wald", "sein", "Wild\u00b7pret", "h\u00e4uft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich mag nicht, da\u00df der Bratenwender", "tokens": ["Ich", "mag", "nicht", ",", "da\u00df", "der", "Bra\u00b7ten\u00b7wen\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sich bey mir aus dem Oden l\u00e4uft;", "tokens": ["Sich", "bey", "mir", "aus", "dem", "O\u00b7den", "l\u00e4uft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vergn\u00fcgt, in dem gelobten Lande", "tokens": ["Ver\u00b7gn\u00fcgt", ",", "in", "dem", "ge\u00b7lob\u00b7ten", "Lan\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Schmeckt mir auch halbgeschmelzter Kohl,", "tokens": ["Schmeckt", "mir", "auch", "halb\u00b7ge\u00b7schmelz\u00b7ter", "Kohl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Aus einer Sch\u00fcssel schlechtem Rande,", "tokens": ["Aus", "ei\u00b7ner", "Sch\u00fcs\u00b7sel", "schlech\u00b7tem", "Ran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mit einem St\u00fccke Rindfleisch wohl.", "tokens": ["Mit", "ei\u00b7nem", "St\u00fc\u00b7cke", "Rind\u00b7fleisch", "wohl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Zufriedenheit nimmt eine Stelle", "tokens": ["Zu\u00b7frie\u00b7den\u00b7heit", "nimmt", "ei\u00b7ne", "Stel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "An meines Tischgens Viereck ein;", "tokens": ["An", "mei\u00b7nes", "Tischgens", "Vier\u00b7eck", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Horaz mu\u00df auch ein Mitgeselle,", "tokens": ["Ho\u00b7raz", "mu\u00df", "auch", "ein", "Mit\u00b7ge\u00b7sel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ADV", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Mein Gast bey dieser Mahlzeit seyn:", "tokens": ["Mein", "Gast", "bey", "die\u00b7ser", "Mahl\u00b7zeit", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PDAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die M\u00e4ssigkeit steht hinter beyden,", "tokens": ["Die", "M\u00e4s\u00b7sig\u00b7keit", "steht", "hin\u00b7ter", "bey\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PIAT", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sie h\u00e4lt den Teller in der Hand,", "tokens": ["Sie", "h\u00e4lt", "den", "Tel\u00b7ler", "in", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wir k\u00f6nnen ihre Sprache leiden,", "tokens": ["Wir", "k\u00f6n\u00b7nen", "ih\u00b7re", "Spra\u00b7che", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Jhr Scherz ist lehrreich, voll Verstand.", "tokens": ["Ihr", "Scherz", "ist", "lehr\u00b7reich", ",", "voll", "Ver\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Kommt, Sybariten! gebt doch St\u00fchle!", "tokens": ["Kommt", ",", "Sy\u00b7ba\u00b7ri\u00b7ten", "!", "gebt", "doch", "St\u00fch\u00b7le", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$.", "VVFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Lucull! nimm auch vorlieb mit mir!", "tokens": ["Lu\u00b7cull", "!", "nimm", "auch", "vor\u00b7lieb", "mit", "mir", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVIMP", "ADV", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jhr Herrn! hier sind zwar keine Pf\u00fchle,", "tokens": ["Ihr", "Herrn", "!", "hier", "sind", "zwar", "kei\u00b7ne", "Pf\u00fch\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "ADV", "VAFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auch setz ich nur Gekochtes f\u00fcr \u2012 \u2012", "tokens": ["Auch", "setz", "ich", "nur", "Ge\u00b7koch\u00b7tes", "f\u00fcr", "\u2012", "\u2012"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "NN", "APPR", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie? was? ihr weltberufne Richter", "tokens": ["Wie", "?", "was", "?", "ihr", "welt\u00b7be\u00b7ruf\u00b7ne", "Rich\u00b7ter"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "$.", "PWS", "$.", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der besten Welt der Kocherey,", "tokens": ["Der", "bes\u00b7ten", "Welt", "der", "Ko\u00b7che\u00b7rey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zeugt durch verzogene G