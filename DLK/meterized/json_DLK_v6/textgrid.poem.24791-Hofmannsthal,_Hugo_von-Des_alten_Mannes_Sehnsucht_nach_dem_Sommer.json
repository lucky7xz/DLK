{"textgrid.poem.24791": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "Des alten Mannes Sehnsucht nach dem Sommer", "genre": "verse", "period": "N.A.", "pub_year": 1901, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn endlich Juli w\u00fcrde anstatt M\u00e4rz,", "tokens": ["Wenn", "end\u00b7lich", "Ju\u00b7li", "w\u00fcr\u00b7de", "an\u00b7statt", "M\u00e4rz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nichts hielte mich, ich n\u00e4hme einen Rand,", "tokens": ["Nichts", "hiel\u00b7te", "mich", ",", "ich", "n\u00e4h\u00b7me", "ei\u00b7nen", "Rand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zu Pferd, zu Wagen oder mit der Bahn", "tokens": ["Zu", "Pferd", ",", "zu", "Wa\u00b7gen", "o\u00b7der", "mit", "der", "Bahn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "K\u00e4m ich hinaus ins sch\u00f6ne H\u00fcgelland.", "tokens": ["K\u00e4m", "ich", "hin\u00b7aus", "ins", "sch\u00f6\u00b7ne", "H\u00fc\u00b7gel\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da st\u00fcnden Gruppen gro\u00dfer B\u00e4ume nah,", "tokens": ["Da", "st\u00fcn\u00b7den", "Grup\u00b7pen", "gro\u00b7\u00dfer", "B\u00e4u\u00b7me", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Platanen, R\u00fcster, Ahorn oder Eiche:", "tokens": ["Pla\u00b7ta\u00b7nen", ",", "R\u00fcs\u00b7ter", ",", "A\u00b7horn", "o\u00b7der", "Ei\u00b7che", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie lang ists, da\u00df ich keine solchen sah!", "tokens": ["Wie", "lang", "ists", ",", "da\u00df", "ich", "kei\u00b7ne", "sol\u00b7chen", "sah", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "$,", "KOUS", "PPER", "PIAT", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Da stiege ich vom Pferde oder riefe", "tokens": ["Da", "stie\u00b7ge", "ich", "vom", "Pfer\u00b7de", "o\u00b7der", "rie\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem Kutscher: Halt! und ginge ohne Ziel", "tokens": ["Dem", "Kut\u00b7scher", ":", "Halt", "!", "und", "gin\u00b7ge", "oh\u00b7ne", "Ziel"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "NN", "$.", "KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nach vorw\u00e4rts in des Sommerlandes Tiefe.", "tokens": ["Nach", "vor\u00b7w\u00e4rts", "in", "des", "Som\u00b7mer\u00b7lan\u00b7des", "Tie\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und unter solchen B\u00e4umen ruht ich aus;", "tokens": ["Und", "un\u00b7ter", "sol\u00b7chen", "B\u00e4u\u00b7men", "ruht", "ich", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In deren Wipfel w\u00e4re Tag und Nacht", "tokens": ["In", "de\u00b7ren", "Wip\u00b7fel", "w\u00e4\u00b7re", "Tag", "und", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "VAFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zugleich, und nicht so wie in diesem Haus,", "tokens": ["Zu\u00b7gleich", ",", "und", "nicht", "so", "wie", "in", "die\u00b7sem", "Haus", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KON", "PTKNEG", "ADV", "KOKOM", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Wo Tage manchmal \u00f6d sind wie die Nacht", "tokens": ["Wo", "Ta\u00b7ge", "manch\u00b7mal", "\u00f6d", "sind", "wie", "die", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "ADV", "ADJD", "VAFIN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und N\u00e4chte fahl und lauernd wie der Tag.", "tokens": ["Und", "N\u00e4ch\u00b7te", "fahl", "und", "lau\u00b7ernd", "wie", "der", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "KON", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dort w\u00e4re alles Leben, Glanz und Pracht.", "tokens": ["Dort", "w\u00e4\u00b7re", "al\u00b7les", "Le\u00b7ben", ",", "Glanz", "und", "Pracht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Und aus dem Schatten in des Abendlichts", "tokens": ["Und", "aus", "dem", "Schat\u00b7ten", "in", "des", "A\u00b7bend\u00b7lichts"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Begl\u00fcckung tret ich, und ein Hauch weht hin,", "tokens": ["Be\u00b7gl\u00fc\u00b7ckung", "tret", "ich", ",", "und", "ein", "Hauch", "weht", "hin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "KON", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch nirgend fl\u00fcsterts: \u00bbAlles dies ist nichts.\u00ab", "tokens": ["Doch", "nir\u00b7gend", "fl\u00fcs\u00b7terts", ":", "\u00bb", "Al\u00b7les", "dies", "ist", "nichts", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "ADV", "$.", "$(", "PIS", "PDS", "VAFIN", "PIS", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Das Tal wird dunkel, und wo H\u00e4user sind,", "tokens": ["Das", "Tal", "wird", "dun\u00b7kel", ",", "und", "wo", "H\u00e4u\u00b7ser", "sind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "KON", "PWAV", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sind Lichter, und das Dunkel weht mich an,", "tokens": ["Sind", "Lich\u00b7ter", ",", "und", "das", "Dun\u00b7kel", "weht", "mich", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "KON", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch nicht vom Sterben spricht der n\u00e4chtige Wind;", "tokens": ["Doch", "nicht", "vom", "Ster\u00b7ben", "spricht", "der", "n\u00e4ch\u00b7ti\u00b7ge", "Wind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.9": {"line.1": {"text": "Ich gehe \u00fcbern Friedhof hin und sehe", "tokens": ["Ich", "ge\u00b7he", "\u00fc\u00b7bern", "Fried\u00b7hof", "hin", "und", "se\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nur Blumen sich im letzten Scheine wiegen,", "tokens": ["Nur", "Blu\u00b7men", "sich", "im", "letz\u00b7ten", "Schei\u00b7ne", "wie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PRF", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von gar nichts anderm f\u00fchl ich eine N\u00e4he.", "tokens": ["Von", "gar", "nichts", "an\u00b7derm", "f\u00fchl", "ich", "ei\u00b7ne", "N\u00e4\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PIS", "PIS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Und zwischen Haselstr\u00e4uchern, die schon d\u00fcstern,", "tokens": ["Und", "zwi\u00b7schen", "Ha\u00b7sel\u00b7str\u00e4u\u00b7chern", ",", "die", "schon", "d\u00fcs\u00b7tern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Flie\u00dft Wasser hin, und wie ein Kind, so lausch ich", "tokens": ["Flie\u00dft", "Was\u00b7ser", "hin", ",", "und", "wie", "ein", "Kind", ",", "so", "lausch", "ich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "NN", "PTKVZ", "$,", "KON", "PWAV", "ART", "NN", "$,", "ADV", "ADJD", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und h\u00f6re kein \u00bbDies ist vergeblich\u00ab fl\u00fcstern!", "tokens": ["Und", "h\u00f6\u00b7re", "kein", "\u00bb", "Dies", "ist", "ver\u00b7geb\u00b7lich", "\u00ab", "fl\u00fcs\u00b7tern", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "$(", "PDS", "VAFIN", "ADJD", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Da ziehe ich mich hurtig aus und springe", "tokens": ["Da", "zie\u00b7he", "ich", "mich", "hur\u00b7tig", "aus", "und", "sprin\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADJD", "PTKVZ", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hinein, und wie ich dann den Kopf erhebe,", "tokens": ["Hin\u00b7ein", ",", "und", "wie", "ich", "dann", "den", "Kopf", "er\u00b7he\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KON", "PWAV", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ist Mond, indes ich mit dem B\u00e4chlein ringe.", "tokens": ["Ist", "Mond", ",", "in\u00b7des", "ich", "mit", "dem", "B\u00e4ch\u00b7lein", "rin\u00b7ge", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+++-+-+-", "measure": "unknown.measure.hexa"}}, "stanza.12": {"line.1": {"text": "Halb heb ich mich aus der eiskalten Welle,", "tokens": ["Halb", "heb", "ich", "mich", "aus", "der", "eis\u00b7kal\u00b7ten", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und einen glatten Kieselstein ins Land", "tokens": ["Und", "ei\u00b7nen", "glat\u00b7ten", "Kie\u00b7sel\u00b7stein", "ins", "Land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Weit schleudernd, steh ich in der Mondeshelle.", "tokens": ["Weit", "schleu\u00b7dernd", ",", "steh", "ich", "in", "der", "Mon\u00b7des\u00b7hel\u00b7le", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "Und auf das mondbegl\u00e4nzte Sommerland F\u00e4", "tokens": ["Und", "auf", "das", "mond\u00b7be\u00b7gl\u00e4nz\u00b7te", "Som\u00b7mer\u00b7land", "F\u00e4"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "F\u00e4llt weit ein Schatten: dieser, der so traurig", "tokens": ["F\u00e4llt", "weit", "ein", "Schat\u00b7ten", ":", "die\u00b7ser", ",", "der", "so", "trau\u00b7rig"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$.", "PDAT", "$,", "PRELS", "ADV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hier nickt, hier hinterm Kissen an der Wand?", "tokens": ["Hier", "nickt", ",", "hier", "hin\u00b7term", "Kis\u00b7sen", "an", "der", "Wand", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.14": {"line.1": {"text": "So tr\u00fcb und traurig, der halb aufrecht kauert", "tokens": ["So", "tr\u00fcb", "und", "trau\u00b7rig", ",", "der", "halb", "auf\u00b7recht", "kau\u00b7ert"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,", "PRELS", "ADJD", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vor Tag und b\u00f6se in das Fr\u00fchlicht starrt", "tokens": ["Vor", "Tag", "und", "b\u00f6\u00b7se", "in", "das", "Fr\u00fch\u00b7licht", "starrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "ADJD", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wei\u00df, da\u00df auf uns beide etwas lauert?", "tokens": ["Und", "wei\u00df", ",", "da\u00df", "auf", "uns", "bei\u00b7de", "et\u00b7was", "lau\u00b7ert", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "APPR", "PPER", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.15": {"line.1": {"text": "Er, den der b\u00f6se Wind in diesem M\u00e4rz", "tokens": ["Er", ",", "den", "der", "b\u00f6\u00b7se", "Wind", "in", "die\u00b7sem", "M\u00e4rz"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "ART", "ADJA", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So qu\u00e4lt, da\u00df er die N\u00e4chte nie sich legt,", "tokens": ["So", "qu\u00e4lt", ",", "da\u00df", "er", "die", "N\u00e4ch\u00b7te", "nie", "sich", "legt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "ADV", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gekrampft die schwarzen H\u00e4nde auf sein Herz?", "tokens": ["Ge\u00b7krampft", "die", "schwar\u00b7zen", "H\u00e4n\u00b7de", "auf", "sein", "Herz", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.16": {"line.1": {"text": "Ach, wo ist Juli und das Sommerland!", "tokens": ["Ach", ",", "wo", "ist", "Ju\u00b7li", "und", "das", "Som\u00b7mer\u00b7land", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VAFIN", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}