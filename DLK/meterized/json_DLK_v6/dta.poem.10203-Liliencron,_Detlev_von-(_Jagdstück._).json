{"dta.poem.10203": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "( Jagdst\u00fcck. )", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Der Edelhirsch hebt stolz die sechszehn Enden,", "tokens": ["Der", "E\u00b7del\u00b7hirsch", "hebt", "stolz", "die", "sechs\u00b7zehn", "En\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ART", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und sichert, thaubedeckt, in Morgenfunken.", "tokens": ["Und", "si\u00b7chert", ",", "thau\u00b7be\u00b7deckt", ",", "in", "Mor\u00b7gen\u00b7fun\u00b7ken", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Diana sch\u00fcrzt sich, um den Pfeil zu senden,", "tokens": ["Di\u00b7a\u00b7na", "sch\u00fcrzt", "sich", ",", "um", "den", "Pfeil", "zu", "sen\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "$,", "KOUI", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die R\u00fcdenhunde l\u00e4uten, todgiertrunken.", "tokens": ["Die", "R\u00fc\u00b7den\u00b7hun\u00b7de", "l\u00e4u\u00b7ten", ",", "tod\u00b7gier\u00b7trun\u00b7ken", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Durch Busch und Bruch, es neigt die Kraft der Lenden,", "tokens": ["Durch", "Busch", "und", "Bruch", ",", "es", "neigt", "die", "Kraft", "der", "Len\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Am stillen Waldteich ist er hingesunken.", "tokens": ["Am", "stil\u00b7len", "Wald\u00b7teich", "ist", "er", "hin\u00b7ge\u00b7sun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Halali, Zinkentusch und Jubelspenden,", "tokens": ["Ha\u00b7la\u00b7li", ",", "Zin\u00b7ken\u00b7tusch", "und", "Ju\u00b7bel\u00b7spen\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die Trauermesse halten Nix und Unken.", "tokens": ["Die", "Trau\u00b7er\u00b7mes\u00b7se", "hal\u00b7ten", "Nix", "und", "Un\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}