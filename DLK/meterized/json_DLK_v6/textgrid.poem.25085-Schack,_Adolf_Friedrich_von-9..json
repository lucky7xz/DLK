{"textgrid.poem.25085": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Tage bang und herzbeklommen", "tokens": ["Am", "Ta\u00b7ge", "bang", "und", "herz\u00b7be\u00b7klom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJD", "KON", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schreit' ich dahin auf \u00f6dem Pfad,", "tokens": ["Schreit'", "ich", "da\u00b7hin", "auf", "\u00f6\u00b7dem", "Pfad", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bis, wenn sein dreistes Licht verglommen,", "tokens": ["Bis", ",", "wenn", "sein", "dreis\u00b7tes", "Licht", "ver\u00b7glom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "KOUS", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die vielersehnte Stunde naht.", "tokens": ["Die", "vie\u00b7ler\u00b7sehn\u00b7te", "Stun\u00b7de", "naht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie, die im Tod mich nicht vergessen,", "tokens": ["Sie", ",", "die", "im", "Tod", "mich", "nicht", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPRART", "NN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf kurz dann darf ich wiedersehn;", "tokens": ["Auf", "kurz", "dann", "darf", "ich", "wie\u00b7der\u00b7sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Her\u00fcber von den Grabcypressen", "tokens": ["Her\u00b7\u00fc\u00b7ber", "von", "den", "Grab\u00b7cyp\u00b7res\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schwebt sie zu mir im Abendwehn.", "tokens": ["Schwebt", "sie", "zu", "mir", "im", "Ab\u00b7end\u00b7wehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Von ihrem Atemzug, dem reinen,", "tokens": ["Von", "ih\u00b7rem", "A\u00b7tem\u00b7zug", ",", "dem", "rei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Umhaucht f\u00fchl' ich mich wiederum;", "tokens": ["Um\u00b7haucht", "f\u00fchl'", "ich", "mich", "wie\u00b7de\u00b7rum", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PRF", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie dr\u00fcckt die Lippen auf die meinen,", "tokens": ["Sie", "dr\u00fcckt", "die", "Lip\u00b7pen", "auf", "die", "mei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Seele h\u00e4ngt an Seele stumm.", "tokens": ["Und", "See\u00b7le", "h\u00e4ngt", "an", "See\u00b7le", "stumm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie mahnend in mein Auge sieht sie", "tokens": ["Wie", "mah\u00b7nend", "in", "mein", "Au\u00b7ge", "sieht", "sie"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Legt ihre Hand in meine matt,", "tokens": ["Legt", "ih\u00b7re", "Hand", "in", "mei\u00b7ne", "matt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und leis zu sich hinab mich zieht sie", "tokens": ["Und", "leis", "zu", "sich", "hin\u00b7ab", "mich", "zieht", "sie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "PRF", "ADV", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In ihre dunkle Grabesstatt.", "tokens": ["In", "ih\u00b7re", "dunk\u00b7le", "Gra\u00b7bess\u00b7tatt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und wo ich nach des Lebens Streite", "tokens": ["Und", "wo", "ich", "nach", "des", "Le\u00b7bens", "Strei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ruhn soll im stillen Friedenshaus,", "tokens": ["Ruhn", "soll", "im", "stil\u00b7len", "Frie\u00b7dens\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dort unten tr\u00e4um' ich ihr zur Seite", "tokens": ["Dort", "un\u00b7ten", "tr\u00e4um'", "ich", "ihr", "zur", "Sei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Traum des Todes schon voraus.", "tokens": ["Den", "Traum", "des", "To\u00b7des", "schon", "vo\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}