{"textgrid.poem.53325": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sey getrost, O meine Seele,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sey getrost, O meine Seele,", "tokens": ["Sey", "ge\u00b7trost", ",", "O", "mei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "NE", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vnd bestreite ritterlich", "tokens": ["Vnd", "be\u00b7strei\u00b7te", "rit\u00b7ter\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dieses schwachen Leibes H\u00f6le,", "tokens": ["Die\u00b7ses", "schwa\u00b7chen", "Lei\u00b7bes", "H\u00f6\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die Erl\u00f6sung nahet sich,", "tokens": ["Die", "Er\u00b7l\u00f6\u00b7sung", "na\u00b7het", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da du aller Angst vnd Pein", "tokens": ["Da", "du", "al\u00b7ler", "Angst", "vnd", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PIAT", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Selig wirft entbunden seyn.", "tokens": ["Se\u00b7lig", "wirft", "ent\u00b7bun\u00b7den", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Christus selbst wird f\u00fcr dich k\u00e4mpfen.", "tokens": ["Chris\u00b7tus", "selbst", "wird", "f\u00fcr", "dich", "k\u00e4mp\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VAFIN", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Er, der rechte Sieges-Held,", "tokens": ["Er", ",", "der", "rech\u00b7te", "Sie\u00b7ges\u00b7Held", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lehrt vns alle Feinde d\u00e4mpffen,", "tokens": ["Lehrt", "vns", "al\u00b7le", "Fein\u00b7de", "d\u00e4mpf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die Er selber hat gef\u00e4llt,", "tokens": ["Die", "Er", "sel\u00b7ber", "hat", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Als Er mit dem Tode rangk,", "tokens": ["Als", "Er", "mit", "dem", "To\u00b7de", "rangk", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Vnd der H\u00f6llen Reich bezwangk.", "tokens": ["Vnd", "der", "H\u00f6l\u00b7len", "Reich", "be\u00b7zwangk", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "O wie werd' ich dort empfangen", "tokens": ["O", "wie", "werd'", "ich", "dort", "emp\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So gew\u00fcnschten Sieges-Lohn,", "tokens": ["So", "ge\u00b7w\u00fcnschten", "Sie\u00b7ges\u00b7Lohn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein verkl\u00e4hrtes Haupt wird prangen", "tokens": ["Mein", "ver\u00b7kl\u00e4hr\u00b7tes", "Haupt", "wird", "pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit der rechten Ehren-Krohn',", "tokens": ["Mit", "der", "rech\u00b7ten", "Eh\u00b7ren\u00b7Krohn'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alle Schwachheit vnd Verdru\u00df", "tokens": ["Al\u00b7le", "Schwach\u00b7heit", "vnd", "Ver\u00b7dru\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird seyn vnter meinem Fu\u00df.", "tokens": ["Wird", "seyn", "vn\u00b7ter", "mei\u00b7nem", "Fu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wessen ich mich stets befliessen,", "tokens": ["Wes\u00b7sen", "ich", "mich", "stets", "be\u00b7flies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PRF", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meines Hertzens gute Sach'", "tokens": ["Mei\u00b7nes", "Hert\u00b7zens", "gu\u00b7te", "Sach'"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vnd mein vnbefleckt Gewissen", "tokens": ["Vnd", "mein", "vn\u00b7be\u00b7fleckt", "Ge\u00b7wis\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Folgen vngeseumt mir nach,", "tokens": ["Fol\u00b7gen", "vn\u00b7ge\u00b7seumt", "mir", "nach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Also bald mein freyer Geist", "tokens": ["Al\u00b7so", "bald", "mein", "frey\u00b7er", "Geist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Au\u00df dem C\u00f6rper ist gereist.", "tokens": ["Au\u00df", "dem", "C\u00f6r\u00b7per", "ist", "ge\u00b7reist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Vnterdessen wil ich leiden", "tokens": ["Vn\u00b7ter\u00b7des\u00b7sen", "wil", "ich", "lei\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was mein GOTT mir aufferlegt,", "tokens": ["Was", "mein", "GoTT", "mir", "auf\u00b7fer\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seine Hand k\u00fcss' ich bescheiden,", "tokens": ["Sei\u00b7ne", "Hand", "k\u00fcss'", "ich", "be\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die mich v\u00e4terlich jetzt schl\u00e4gt,", "tokens": ["Die", "mich", "v\u00e4\u00b7ter\u00b7lich", "jetzt", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Seinen Zorn ertrag' ich still,", "tokens": ["Sei\u00b7nen", "Zorn", "er\u00b7trag'", "ich", "still", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Lass' Ihn schaffen was Er wil.", "tokens": ["Lass'", "Ihn", "schaf\u00b7fen", "was", "Er", "wil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "PWS", "PPER", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Er wird mich von allem B\u00f6sen,", "tokens": ["Er", "wird", "mich", "von", "al\u00b7lem", "B\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "PIS", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Es sey S\u00fcnde, Todt vnd Zeit", "tokens": ["Es", "sey", "S\u00fcn\u00b7de", ",", "Todt", "vnd", "Zeit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Selig noch zuletzt erl\u00f6sen", "tokens": ["Se\u00b7lig", "noch", "zu\u00b7letzt", "er\u00b7l\u00f6\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADV", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu dem Reich der Herrligkeit,", "tokens": ["Zu", "dem", "Reich", "der", "Herr\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Das Er vns nach dieser Welt", "tokens": ["Das", "Er", "vns", "nach", "die\u00b7ser", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PPER", "PRF", "APPR", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "In dem Himmel vor-beh\u00e4lt.", "tokens": ["In", "dem", "Him\u00b7mel", "vor\u00b7be\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Ihm sey Ehr' vnd Danck gegeben,", "tokens": ["Ihm", "sey", "Ehr'", "vnd", "Danck", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihn erheb' ich, wie ich weis,", "tokens": ["Ihn", "er\u00b7heb'", "ich", ",", "wie", "ich", "weis", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Beydes in vnd nach dem Leben,", "tokens": ["Bey\u00b7des", "in", "vnd", "nach", "dem", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "KON", "APPR", "ART", "NN", "$,"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Ihm allein sol Lob vnd Prei\u00df", "tokens": ["Ihm", "al\u00b7lein", "sol", "Lob", "vnd", "Prei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "PIAT", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gar von Ewigkeit her seyn", "tokens": ["Gar", "von", "E\u00b7wig\u00b7keit", "her", "seyn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "APZR", "VAINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Bis in Ewigkeit hinein.", "tokens": ["Bis", "in", "E\u00b7wig\u00b7keit", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}