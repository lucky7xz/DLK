{"textgrid.poem.34494": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1", "genre": "verse", "period": "N.A.", "pub_year": 1888, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Leidenschaftlichen Sinns und heissbegehrenden Herzens", "tokens": ["Lei\u00b7den\u00b7schaft\u00b7li\u00b7chen", "Sinns", "und", "heiss\u00b7be\u00b7geh\u00b7ren\u00b7den", "Her\u00b7zens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "haderst du still mit der Welt, weil sie dir nimmer gen\u00fcgt.", "tokens": ["ha\u00b7derst", "du", "still", "mit", "der", "Welt", ",", "weil", "sie", "dir", "nim\u00b7mer", "ge\u00b7n\u00fcgt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,", "KOUS", "PPER", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Lieber vertraue dir selbst! Es reifen die Fr\u00fcchte dem Manne,", "tokens": ["Lie\u00b7ber", "ver\u00b7trau\u00b7e", "dir", "selbst", "!", "Es", "rei\u00b7fen", "die", "Fr\u00fcch\u00b7te", "dem", "Man\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "$.", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "wenn er die Menschen beherrscht, weil er die Menschen durchschaut.", "tokens": ["wenn", "er", "die", "Men\u00b7schen", "be\u00b7herrscht", ",", "weil", "er", "die", "Men\u00b7schen", "durch\u00b7schaut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}}}}}