{"dta.poem.1525": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung funffzig-j\u00e4hrigen  \n Lebens-Lauffs.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "La\u00dft Solyms Mauren ", "tokens": ["La\u00dft", "So\u00b7lyms", "Mau\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "NE", "NN"], "meter": "---+-", "measure": "unknown.measure.single"}, "line.2": {"text": "Voll Freude stehen/", "tokens": ["Voll", "Freu\u00b7de", "ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVFIN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Ich will ins Trauren", "tokens": ["Ich", "will", "ins", "Trau\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Mit JEsu gehen.", "tokens": ["Mit", "Je\u00b7su", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Bey duncklem Schatten", "tokens": ["Bey", "dunck\u00b7lem", "Schat\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Mich bey ihm zeigen/", "tokens": ["Mich", "bey", "ihm", "zei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Durch Kidron watten/", "tokens": ["Durch", "Kid\u00b7ron", "wat\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Den Oel-Berg steigen.", "tokens": ["Den", "O\u00b7el\u00b7Berg", "stei\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-++-", "measure": "unknown.measure.tri"}}, "stanza.3": {"line.1": {"text": "Betracht/ o Hertze/", "tokens": ["Be\u00b7tracht", "/", "o", "Hert\u00b7ze", "/"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$(", "FM", "VVFIN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Mit Bu\u00df\u2019 erweichet/", "tokens": ["Mit", "Bu\u00df'", "er\u00b7wei\u00b7chet", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Ob auch ein Schmertze", "tokens": ["Ob", "auch", "ein", "Schmert\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Dem seinen gleichet!", "tokens": ["Dem", "sei\u00b7nen", "glei\u00b7chet", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Schau JEsum sitzen", "tokens": ["Schau", "Je\u00b7sum", "sit\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["NN", "NE", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Auff seinen Knihen/", "tokens": ["Auff", "sei\u00b7nen", "Kni\u00b7hen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und Blutt verschwitzen", "tokens": ["Und", "Blutt", "ver\u00b7schwit\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "F\u00fcr Angst-Bem\u00fchen.", "tokens": ["F\u00fcr", "Angst\u00b7Be\u00b7m\u00fc\u00b7hen", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}