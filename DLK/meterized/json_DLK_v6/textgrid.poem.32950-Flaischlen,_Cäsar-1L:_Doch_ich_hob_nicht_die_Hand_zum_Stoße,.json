{"textgrid.poem.32950": {"metadata": {"author": {"name": "Flaischlen, C\u00e4sar", "birth": "N.A.", "death": "N.A."}, "title": "1L: Doch ich hob nicht die Hand zum Sto\u00dfe,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Doch ich hob nicht die Hand zum Sto\u00dfe,", "tokens": ["Doch", "ich", "hob", "nicht", "die", "Hand", "zum", "Sto\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "ART", "NN", "APPRART", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "ich weinte still nur, eine Nacht ...", "tokens": ["ich", "wein\u00b7te", "still", "nur", ",", "ei\u00b7ne", "Nacht", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "$,", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann aber fing ich an zu lachen", "tokens": ["Dann", "a\u00b7ber", "fing", "ich", "an", "zu", "la\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PTKVZ", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "und lachte, bis ich's durchgelacht ...", "tokens": ["und", "lach\u00b7te", ",", "bis", "ich's", "durch\u00b7ge\u00b7lacht", "..."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und stie\u00df die Fackel in die Tr\u00fcmmer ...", "tokens": ["Und", "stie\u00df", "die", "Fa\u00b7ckel", "in", "die", "Tr\u00fcm\u00b7mer", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "hei, wie das aufschlug, tollen Brands!", "tokens": ["hei", ",", "wie", "das", "auf\u00b7schlug", ",", "tol\u00b7len", "Brands", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "PWAV", "PDS", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "und krachend barst die letzte S\u00e4ule", "tokens": ["und", "kra\u00b7chend", "barst", "die", "letz\u00b7te", "S\u00e4u\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "in lohewildem Flammenkranz ...", "tokens": ["in", "lo\u00b7he\u00b7wil\u00b7dem", "Flam\u00b7men\u00b7kranz", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dann ging ich ruhig von der St\u00e4tte", "tokens": ["Dann", "ging", "ich", "ru\u00b7hig", "von", "der", "St\u00e4t\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "und schritt hinein ins D\u00e4mmergraun,", "tokens": ["und", "schritt", "hin\u00b7ein", "ins", "D\u00e4m\u00b7mer\u00b7graun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und lie\u00df des Morgens Ostersonne", "tokens": ["und", "lie\u00df", "des", "Mor\u00b7gens", "Os\u00b7ter\u00b7son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADV", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "den Nachtfrost mir vom Herzen taun.", "tokens": ["den", "Nacht\u00b7frost", "mir", "vom", "Her\u00b7zen", "taun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Nun steh ich frei im freien Leben", "tokens": ["Nun", "steh", "ich", "frei", "im", "frei\u00b7en", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "und aus dem J\u00fcngling ward ein Mann ...", "tokens": ["und", "aus", "dem", "J\u00fcng\u00b7ling", "ward", "ein", "Mann", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und weitab liegt in Nacht und Nebel", "tokens": ["und", "weit\u00b7ab", "liegt", "in", "Nacht", "und", "Ne\u00b7bel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "was seine Jugend hielt im Bann!", "tokens": ["was", "sei\u00b7ne", "Ju\u00b7gend", "hielt", "im", "Bann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}