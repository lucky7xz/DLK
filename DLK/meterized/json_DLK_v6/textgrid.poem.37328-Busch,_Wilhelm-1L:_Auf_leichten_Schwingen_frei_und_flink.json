{"textgrid.poem.37328": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf leichten Schwingen frei und flink", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf leichten Schwingen frei und flink", "tokens": ["Auf", "leich\u00b7ten", "Schwin\u00b7gen", "frei", "und", "flink"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Lindenwipfel flog der Fink", "tokens": ["Zum", "Lin\u00b7den\u00b7wip\u00b7fel", "flog", "der", "Fink"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sang an dieser hohen Stelle", "tokens": ["Und", "sang", "an", "die\u00b7ser", "ho\u00b7hen", "Stel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Morgenlied so glockenhelle.", "tokens": ["Sein", "Mor\u00b7gen\u00b7lied", "so", "glo\u00b7cken\u00b7hel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein Frosch, ein dicker, der im Grase", "tokens": ["Ein", "Frosch", ",", "ein", "di\u00b7cker", ",", "der", "im", "Gra\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Am Boden hockt, erhob die Nase,", "tokens": ["Am", "Bo\u00b7den", "hockt", ",", "er\u00b7hob", "die", "Na\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Strich selbstgef\u00e4llig seinen Bauch", "tokens": ["Strich", "selbst\u00b7ge\u00b7f\u00e4l\u00b7lig", "sei\u00b7nen", "Bauch"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und denkt: Die K\u00fcnste kann ich auch.", "tokens": ["Und", "denkt", ":", "Die", "K\u00fcns\u00b7te", "kann", "ich", "auch", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ART", "NN", "VMFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Alsbald am rauhen Stamm der Linde", "tokens": ["Als\u00b7bald", "am", "rau\u00b7hen", "Stamm", "der", "Lin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN", "ART", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Begann er, wenn auch nicht geschwinde,", "tokens": ["Be\u00b7gann", "er", ",", "wenn", "auch", "nicht", "ge\u00b7schwin\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "ADV", "PTKNEG", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch mit Erfolg emporzusteigen,", "tokens": ["Doch", "mit", "Er\u00b7folg", "em\u00b7por\u00b7zu\u00b7stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bis er zuletzt von Zweig zu Zweigen,", "tokens": ["Bis", "er", "zu\u00b7letzt", "von", "Zweig", "zu", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wobei er freilich etwas keucht,", "tokens": ["Wo\u00b7bei", "er", "frei\u00b7lich", "et\u00b7was", "keucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den h\u00f6chsten Wipfelpunkt erreicht", "tokens": ["Den", "h\u00f6chs\u00b7ten", "Wip\u00b7fel\u00b7punkt", "er\u00b7reicht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und hier sein allersch\u00f6nstes Quaken", "tokens": ["Und", "hier", "sein", "al\u00b7ler\u00b7sch\u00f6ns\u00b7tes", "Qua\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ert\u00f6nen l\u00e4\u00dft aus vollen Backen.", "tokens": ["Er\u00b7t\u00f6\u00b7nen", "l\u00e4\u00dft", "aus", "vol\u00b7len", "Ba\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Fink, dem dieser Wettgesang", "tokens": ["Der", "Fink", ",", "dem", "die\u00b7ser", "Wett\u00b7ge\u00b7sang"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht recht gef\u00e4llt, entfloh und schwang", "tokens": ["Nicht", "recht", "ge\u00b7f\u00e4llt", ",", "ent\u00b7floh", "und", "schwang"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "VVPP", "$,", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sich auf das steile Kirchendach.", "tokens": ["Sich", "auf", "das", "stei\u00b7le", "Kir\u00b7chen\u00b7dach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wart, rief der Frosch, ich komme nach.", "tokens": ["Wart", ",", "rief", "der", "Frosch", ",", "ich", "kom\u00b7me", "nach", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und richtig ist er fortgeflogen,", "tokens": ["Und", "rich\u00b7tig", "ist", "er", "fort\u00b7ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das hei\u00dft, nach unten hin im Bogen,", "tokens": ["Das", "hei\u00dft", ",", "nach", "un\u00b7ten", "hin", "im", "Bo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "APPR", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So da\u00df er schnell und ohne S\u00e4umen,", "tokens": ["So", "da\u00df", "er", "schnell", "und", "oh\u00b7ne", "S\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADJD", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nach mehr als zwanzig Purzelb\u00e4umen,", "tokens": ["Nach", "mehr", "als", "zwan\u00b7zig", "Pur\u00b7zel\u00b7b\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "KOKOM", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zur Erde kam mit lautem Quak,", "tokens": ["Zur", "Er\u00b7de", "kam", "mit", "lau\u00b7tem", "Quak", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nicht ohne gro\u00dfes Unbehagen.", "tokens": ["Nicht", "oh\u00b7ne", "gro\u00b7\u00dfes", "Un\u00b7be\u00b7ha\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Er fiel zum Gl\u00fcck auf seinen Magen,", "tokens": ["Er", "fiel", "zum", "Gl\u00fcck", "auf", "sei\u00b7nen", "Ma\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den dicken, weichen Futtersack,", "tokens": ["Den", "di\u00b7cken", ",", "wei\u00b7chen", "Fut\u00b7ter\u00b7sack", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sonst h\u00e4tt er sicher sich verletzt.", "tokens": ["Sonst", "h\u00e4tt", "er", "si\u00b7cher", "sich", "ver\u00b7letzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "PRF", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Heil ihm! Er hat es durchgesetzt.", "tokens": ["Heil", "ihm", "!", "Er", "hat", "es", "durch\u00b7ge\u00b7setzt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "PPER", "VAFIN", "PPER", "VVPP", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}