{"textgrid.poem.44514": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[wenn man das Grab nicht kennt]", "genre": "verse", "period": "N.A.", "pub_year": 1841, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn man das Grab nicht kennt, in dem er Ruh erworben,", "tokens": ["Wenn", "man", "das", "Grab", "nicht", "kennt", ",", "in", "dem", "er", "Ruh", "er\u00b7wor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "PTKNEG", "VVFIN", "$,", "APPR", "PRELS", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wen, Freunde, \u00e4ngstet das? Ist er doch nicht gestorben!", "tokens": ["Wen", ",", "Freun\u00b7de", ",", "\u00e4ngs\u00b7tet", "das", "?", "Ist", "er", "doch", "nicht", "ge\u00b7stor\u00b7ben", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "NN", "$,", "VVFIN", "PDS", "$.", "VAFIN", "PPER", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Er lebt in aller Herzen, aller Sinn", "tokens": ["Er", "lebt", "in", "al\u00b7ler", "Her\u00b7zen", ",", "al\u00b7ler", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und schreitet jetzt durch unsre Reihen hin.", "tokens": ["Und", "schrei\u00b7tet", "jetzt", "durch", "uns\u00b7re", "Rei\u00b7hen", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Deshalb dem Lebenden, der sich am Dasein freute,", "tokens": ["Des\u00b7halb", "dem", "Le\u00b7ben\u00b7den", ",", "der", "sich", "am", "Da\u00b7sein", "freu\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "$,", "PRELS", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ihm sei kein leblos Totenopfer heute.", "tokens": ["Ihm", "sei", "kein", "leb\u00b7los", "To\u00b7ten\u00b7op\u00b7fer", "heu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "ADJD", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Hebt auf das Glas, das Mut und Frohsinn gibt,", "tokens": ["Hebt", "auf", "das", "Glas", ",", "das", "Mut", "und", "Froh\u00b7sinn", "gibt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und sprecht, es leerend, wie ers selbst geliebt:", "tokens": ["Und", "sprecht", ",", "es", "lee\u00b7rend", ",", "wie", "ers", "selbst", "ge\u00b7liebt", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "ADJD", "$,", "PWAV", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "\u00bbdem gro\u00dfen Meister in dem Reich der T\u00f6ne,", "tokens": ["\u00bb", "dem", "gro\u00b7\u00dfen", "Meis\u00b7ter", "in", "dem", "Reich", "der", "T\u00f6\u00b7ne", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der nie zu wenig tat und nie zu viel,", "tokens": ["Der", "nie", "zu", "we\u00b7nig", "tat", "und", "nie", "zu", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PTKA", "PIS", "VVFIN", "KON", "ADV", "PTKA", "PIS", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der stets erreicht, nie \u00fcberschritt sein Ziel,", "tokens": ["Der", "stets", "er\u00b7reicht", ",", "nie", "\u00fc\u00b7bersc\u00b7hritt", "sein", "Ziel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das mit ihm eins und einig war: das Sch\u00f6ne!\u00ab", "tokens": ["Das", "mit", "ihm", "eins", "und", "ei\u00b7nig", "war", ":", "das", "Sch\u00f6\u00b7ne", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PDS", "APPR", "PPER", "PIS", "KON", "ADJD", "VAFIN", "$.", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}