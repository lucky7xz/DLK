{"textgrid.poem.64341": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "62. Die Feier der Erde", "genre": "verse", "period": "N.A.", "pub_year": 1778, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alles unter dem Monde,", "tokens": ["Al\u00b7les", "un\u00b7ter", "dem", "Mon\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART", "NE", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Unter der himmelwandelnden", "tokens": ["Un\u00b7ter", "der", "him\u00b7mel\u00b7wan\u00b7deln\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+--+-+--", "measure": "iambic.tri.invert"}, "line.3": {"text": "Sonne, kennet und kannte", "tokens": ["Son\u00b7ne", ",", "ken\u00b7net", "und", "kann\u00b7te"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "KON", "VVFIN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Alles die Muse;", "tokens": ["Al\u00b7les", "die", "Mu\u00b7se", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Unter den Tiefen der Erde", "tokens": ["Un\u00b7ter", "den", "Tie\u00b7fen", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Schwebet ihr Fittich,", "tokens": ["Schwe\u00b7bet", "ihr", "Fit\u00b7tich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Und willkommen ist die k\u00fchne Fremdling auch oft", "tokens": ["Und", "will\u00b7kom\u00b7men", "ist", "die", "k\u00fch\u00b7ne", "Fremd\u00b7ling", "auch", "oft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "ART", "ADJA", "NN", "ADV", "ADV"], "meter": "--+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "Unter den Reigen der Himmlischen", "tokens": ["Un\u00b7ter", "den", "Rei\u00b7gen", "der", "Himm\u00b7li\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Dennoch erscheinet sie", "tokens": ["Den\u00b7noch", "er\u00b7schei\u00b7net", "sie"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Oft dem sterblichen Dichter;", "tokens": ["Oft", "dem", "sterb\u00b7li\u00b7chen", "Dich\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Eilet dem rufenden", "tokens": ["Ei\u00b7let", "dem", "ru\u00b7fen\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.4": {"text": "Z\u00fcrnend vorbei,", "tokens": ["Z\u00fcr\u00b7nend", "vor\u00b7bei", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Aber besuchet,", "tokens": ["A\u00b7ber", "be\u00b7su\u00b7chet", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Ungerufen und l\u00e4chelnd,", "tokens": ["Un\u00b7ge\u00b7ru\u00b7fen", "und", "l\u00e4\u00b7chelnd", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "Oft im bebenden Mondenschein,", "tokens": ["Oft", "im", "be\u00b7ben\u00b7den", "Mon\u00b7den\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Oft auf gl\u00fchendem Sonnenstrahl,", "tokens": ["Oft", "auf", "gl\u00fc\u00b7hen\u00b7dem", "Son\u00b7nen\u00b7strahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Deine ruhenden S\u00e4uglinge,", "tokens": ["Dei\u00b7ne", "ru\u00b7hen\u00b7den", "S\u00e4ug\u00b7lin\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.10": {"text": "Mutter Natur!", "tokens": ["Mut\u00b7ter", "Na\u00b7tur", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Staunend sah ich und froh,", "tokens": ["Stau\u00b7nend", "sah", "ich", "und", "froh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "KON", "ADJD", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Wogenumdonnertes Hellebek,", "tokens": ["Wo\u00b7gen\u00b7um\u00b7don\u00b7ner\u00b7tes", "Hel\u00b7le\u00b7bek", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Wie der Winter und der Sommer zugleich", "tokens": ["Wie", "der", "Win\u00b7ter", "und", "der", "Som\u00b7mer", "zu\u00b7gleich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "KON", "ART", "NN", "ADV"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Schm\u00fcckten dein rauschendes Haupt.", "tokens": ["Schm\u00fcck\u00b7ten", "dein", "rau\u00b7schen\u00b7des", "Haupt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Staunend und froh,", "tokens": ["Stau\u00b7nend", "und", "froh", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Weilten vor\u00fcberwallende", "tokens": ["Weil\u00b7ten", "vor\u00b7\u00fc\u00b7berw\u00b7al\u00b7len\u00b7de"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-+--", "measure": "iambic.tri.invert"}, "line.3": {"text": "Geister, die aus Orions", "tokens": ["Geis\u00b7ter", ",", "die", "aus", "O\u00b7rions"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "APPR", "NE"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Fluren zu den Inseln der Pleias", "tokens": ["Flu\u00b7ren", "zu", "den", "In\u00b7seln", "der", "Plei\u00b7as"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Schwebten, und erkannten kaum", "tokens": ["Schweb\u00b7ten", ",", "und", "er\u00b7kann\u00b7ten", "kaum"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KON", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Der Erde Antlitz, das sie oft schon sahn,", "tokens": ["Der", "Er\u00b7de", "Ant\u00b7litz", ",", "das", "sie", "oft", "schon", "sahn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Forschten nach des rollenden", "tokens": ["Forschten", "nach", "des", "rol\u00b7len\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.8": {"text": "Jahres Alter, denn sie sahn", "tokens": ["Jah\u00b7res", "Al\u00b7ter", ",", "denn", "sie", "sahn"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "KON", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Auf der grauen schneeigen Scheitel,", "tokens": ["Auf", "der", "grau\u00b7en", "schne\u00b7e\u00b7i\u00b7gen", "Schei\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Goldene, s\u00e4uselnde Locken des Hains!", "tokens": ["Gol\u00b7de\u00b7ne", ",", "s\u00e4u\u00b7seln\u00b7de", "Lo\u00b7cken", "des", "Hains", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.5": {"line.1": {"text": "Mir vertraute, sie vertraute mir,", "tokens": ["Mir", "ver\u00b7trau\u00b7te", ",", "sie", "ver\u00b7trau\u00b7te", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Die kundige Muse", "tokens": ["Die", "kun\u00b7di\u00b7ge", "Mu\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Das Geheimnis der Natur!", "tokens": ["Das", "Ge\u00b7heim\u00b7nis", "der", "Na\u00b7tur", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Es feiert die Erde", "tokens": ["Es", "fei\u00b7ert", "die", "Er\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Heute den Tag ihrer Geburt,", "tokens": ["Heu\u00b7te", "den", "Tag", "ih\u00b7rer", "Ge\u00b7burt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Den sie nach tausend", "tokens": ["Den", "sie", "nach", "tau\u00b7send"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "CARD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Rollenden Jahren", "tokens": ["Rol\u00b7len\u00b7den", "Jah\u00b7ren"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Immer feiert!", "tokens": ["Im\u00b7mer", "fei\u00b7ert", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.7": {"line.1": {"text": "Denn an diesem Tage", "tokens": ["Denn", "an", "die\u00b7sem", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PDAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Stieg sie zuerst,", "tokens": ["Stieg", "sie", "zu\u00b7erst", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "$,"], "meter": "++-+", "measure": "iambic.di"}, "line.3": {"text": "Aus der heimlichen Halle der alten Nacht,", "tokens": ["Aus", "der", "heim\u00b7li\u00b7chen", "Hal\u00b7le", "der", "al\u00b7ten", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "An der strahlenden Hand des ersten der Morgen,", "tokens": ["An", "der", "strah\u00b7len\u00b7den", "Hand", "des", "ers\u00b7ten", "der", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "ADJA", "ART", "NN", "$,"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "L\u00e4chelnd und err\u00f6tend, den Himmel hinan!", "tokens": ["L\u00e4\u00b7chelnd", "und", "er\u00b7r\u00f6\u00b7tend", ",", "den", "Him\u00b7mel", "hi\u00b7nan", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "VVPP", "$,", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}}, "stanza.8": {"line.1": {"text": "Es feiert die Erde", "tokens": ["Es", "fei\u00b7ert", "die", "Er\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Diesen Tag!", "tokens": ["Die\u00b7sen", "Tag", "!"], "token_info": ["word", "word", "punct"], "pos": ["PDAT", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Sie berief zur Feier", "tokens": ["Sie", "be\u00b7rief", "zur", "Fei\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Die S\u00f6hne des Jahrs!", "tokens": ["Die", "S\u00f6h\u00b7ne", "des", "Jahrs", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.9": {"line.1": {"text": "Es erhub sich im nordischen Thal", "tokens": ["Es", "er\u00b7hub", "sich", "im", "nor\u00b7di\u00b7schen", "Thal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "ADJA", "NN"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Der Winter nach kurzem Schlaf;", "tokens": ["Der", "Win\u00b7ter", "nach", "kur\u00b7zem", "Schlaf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sch\u00fcttelte sein Haupt, da ward bedeckt", "tokens": ["Sch\u00fct\u00b7tel\u00b7te", "sein", "Haupt", ",", "da", "ward", "be\u00b7deckt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "ADV", "VAFIN", "VVPP"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Der Boden mit Schnee;", "tokens": ["Der", "Bo\u00b7den", "mit", "Schnee", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Ging mit eilendem Riesenschritt,", "tokens": ["Ging", "mit", "ei\u00b7len\u00b7dem", "Rie\u00b7sen\u00b7schritt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Setzte den starrenden Strahlenfu\u00df", "tokens": ["Setz\u00b7te", "den", "star\u00b7ren\u00b7den", "Strah\u00b7len\u00b7fu\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Auf die t\u00fcrmenden Gipfel", "tokens": ["Auf", "die", "t\u00fcr\u00b7men\u00b7den", "Gip\u00b7fel"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Des hohen schwedischen Felsengebirgs;", "tokens": ["Des", "ho\u00b7hen", "schwe\u00b7di\u00b7schen", "Fel\u00b7sen\u00b7ge\u00b7birgs", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Schritt \u00fcbers Meer,", "tokens": ["Schritt", "\u00fc\u00b7bers", "Meer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Trat aufs Gestade,", "tokens": ["Trat", "aufs", "Ge\u00b7sta\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Wo sein Bruder, der Herbst,", "tokens": ["Wo", "sein", "Bru\u00b7der", ",", "der", "Herbst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.12": {"text": "Waltete im falben Hain,", "tokens": ["Wal\u00b7te\u00b7te", "im", "fal\u00b7ben", "Hain", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Wo sein Bruder, der Sommer,", "tokens": ["Wo", "sein", "Bru\u00b7der", ",", "der", "Som\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.14": {"text": "Weilte in der Eiche gr\u00fcnem Laub.", "tokens": ["Weil\u00b7te", "in", "der", "Ei\u00b7che", "gr\u00fc\u00b7nem", "Laub", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Es schm\u00fcckten die Br\u00fcder mit vereinter Hand", "tokens": ["Es", "schm\u00fcck\u00b7ten", "die", "Br\u00fc\u00b7der", "mit", "ver\u00b7ein\u00b7