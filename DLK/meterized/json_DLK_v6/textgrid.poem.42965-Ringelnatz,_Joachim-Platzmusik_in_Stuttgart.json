{"textgrid.poem.42965": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Platzmusik in Stuttgart", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das ist ein froher Sonntagsblick:", "tokens": ["Das", "ist", "ein", "fro\u00b7her", "Sonn\u00b7tags\u00b7blick", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Stuttgart, Studenten, Platzmusik.", "tokens": ["Stutt\u00b7gart", ",", "Stu\u00b7den\u00b7ten", ",", "Platz\u00b7mu\u00b7sik", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Da stehen sie in Gr\u00fcppchen", "tokens": ["Da", "ste\u00b7hen", "sie", "in", "Gr\u00fcpp\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nach Kopfbedeckung, gr\u00fcn, rot, blau", "tokens": ["Nach", "Kopf\u00b7be\u00b7de\u00b7ckung", ",", "gr\u00fcn", ",", "rot", ",", "blau"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "NN", "$,", "ADJD", "$,", "ADJD", "$,", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und l\u00f6ffeln sich ihr S\u00fcppchen", "tokens": ["Und", "l\u00f6f\u00b7feln", "sich", "ihr", "S\u00fcpp\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und wissen alle nichts genau.", "tokens": ["Und", "wis\u00b7sen", "al\u00b7le", "nichts", "ge\u00b7nau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "PIS", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Warum wird nicht gesungen,", "tokens": ["Wa\u00b7rum", "wird", "nicht", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Warum wird nicht marschiert zum Takt", "tokens": ["Wa\u00b7rum", "wird", "nicht", "mar\u00b7schiert", "zum", "Takt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PTKNEG", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Zeitmiliz, die alles packt?", "tokens": ["Der", "Zeit\u00b7mi\u00b7liz", ",", "die", "al\u00b7les", "packt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Es bummeln diese Jungen", "tokens": ["Es", "bum\u00b7meln", "die\u00b7se", "Jun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vorbei, ein wachsendes Geschlecht,", "tokens": ["Vor\u00b7bei", ",", "ein", "wach\u00b7sen\u00b7des", "Ge\u00b7schlecht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ART", "ADJA", "NN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Von keinem Zwang gezwungen.", "tokens": ["Von", "kei\u00b7nem", "Zwang", "ge\u00b7zwun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie haben recht.", "tokens": ["Sie", "ha\u00b7ben", "recht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Der Platz ist sch\u00f6n. Der Platz ist weit.", "tokens": ["Der", "Platz", "ist", "sch\u00f6n", ".", "Der", "Platz", "ist", "weit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Sonntagsvolksgewimmel", "tokens": ["Ein", "Sonn\u00b7tags\u00b7volks\u00b7ge\u00b7wim\u00b7mel"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hat seine eigene Einigkeit", "tokens": ["Hat", "sei\u00b7ne", "ei\u00b7ge\u00b7ne", "Ei\u00b7nig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und einen offiziellen Himmel.", "tokens": ["Und", "ei\u00b7nen", "of\u00b7fi\u00b7zi\u00b7el\u00b7len", "Him\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}}}}