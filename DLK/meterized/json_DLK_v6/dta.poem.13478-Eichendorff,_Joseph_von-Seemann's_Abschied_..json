{"dta.poem.13478": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Seemann's Abschied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ade, mein Schatz, du mocht'st mich nicht,", "tokens": ["A\u00b7de", ",", "mein", "Schatz", ",", "du", "mocht'st", "mich", "nicht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "PPER", "VMFIN", "PPER", "PTKNEG", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Ich war dir zu geringe.", "tokens": ["Ich", "war", "dir", "zu", "ge\u00b7rin\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Einst wandelst du bei Mondenlicht", "tokens": ["Einst", "wan\u00b7delst", "du", "bei", "Mon\u00b7den\u00b7licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und h\u00f6rst ein s\u00fc\u00dfes Klingen,", "tokens": ["Und", "h\u00f6rst", "ein", "s\u00fc\u00b7\u00dfes", "Klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein Meerweib singt, die Nacht ist lau,", "tokens": ["Ein", "Meer\u00b7weib", "singt", ",", "die", "Nacht", "ist", "lau", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die stillen Wolken wandern,", "tokens": ["Die", "stil\u00b7len", "Wol\u00b7ken", "wan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da denk' an mich, 's ist meine Frau,", "tokens": ["Da", "denk'", "an", "mich", ",", "'s", "ist", "mei\u00b7ne", "Frau", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Nun such dir einen andern!", "tokens": ["Nun", "such", "dir", "ei\u00b7nen", "an\u00b7dern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ade, ihr Landsknecht', Musketier'!", "tokens": ["A\u00b7de", ",", "ihr", "Lands\u00b7knecht'", ",", "Mus\u00b7ke\u00b7tier'", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wir ziehn auf wildem Rosse,", "tokens": ["Wir", "ziehn", "auf", "wil\u00b7dem", "Ros\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das b\u00e4umt und \u00fcberschl\u00e4gt sich schier", "tokens": ["Das", "b\u00e4umt", "und", "\u00fc\u00b7bersc\u00b7hl\u00e4gt", "sich", "schier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "PRF", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor manchem Felsenschlosse,", "tokens": ["Vor", "man\u00b7chem", "Fel\u00b7sen\u00b7schlos\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Wassermann bei Blitzesschein", "tokens": ["Der", "Was\u00b7ser\u00b7mann", "bei", "Blit\u00b7zes\u00b7schein"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Taucht auf in dunklen N\u00e4chten,", "tokens": ["Taucht", "auf", "in", "dunk\u00b7len", "N\u00e4ch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der Hayfisch schnappt, die M\u00f6wen schrein \u2014", "tokens": ["Der", "Hay\u00b7fisch", "schnappt", ",", "die", "M\u00f6\u00b7wen", "schrein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das ist ein lust'ges Fechten!", "tokens": ["Das", "ist", "ein", "lust'\u00b7ges", "Fech\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Streckt nur auf eurer B\u00e4renhaut", "tokens": ["Streckt", "nur", "auf", "eu\u00b7rer", "B\u00e4\u00b7ren\u00b7haut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Daheim die faulen Glieder,", "tokens": ["Da\u00b7heim", "die", "fau\u00b7len", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gott Vater aus dem Fenster schaut,", "tokens": ["Gott", "Va\u00b7ter", "aus", "dem", "Fens\u00b7ter", "schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schickt seine S\u00fcndflut wieder,", "tokens": ["Schickt", "sei\u00b7ne", "S\u00fcnd\u00b7flut", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Feldwebel, Reiter, Musketier", "tokens": ["Feld\u00b7we\u00b7bel", ",", "Rei\u00b7ter", ",", "Mus\u00b7ke\u00b7tier"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NE", "$,", "NN"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie m\u00fcssen all' ersaufen,", "tokens": ["Sie", "m\u00fcs\u00b7sen", "all'", "er\u00b7sau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Derweil mit frischem Winde wir", "tokens": ["Der\u00b7weil", "mit", "fri\u00b7schem", "Win\u00b7de", "wir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Im Paradies einlaufen.", "tokens": ["Im", "Pa\u00b7ra\u00b7dies", "ein\u00b7lau\u00b7fen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}