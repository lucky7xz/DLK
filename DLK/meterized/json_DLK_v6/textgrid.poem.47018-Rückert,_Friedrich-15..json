{"textgrid.poem.47018": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "15.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die tausend Schritte, die ich t\u00e4glich schreite,", "tokens": ["Die", "tau\u00b7send", "Schrit\u00b7te", ",", "die", "ich", "t\u00e4g\u00b7lich", "schrei\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Seitdem der tolle Wahn mein Herz besessen,", "tokens": ["Seit\u00b7dem", "der", "tol\u00b7le", "Wahn", "mein", "Herz", "be\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Stets auf dem Weg, den ich nicht kann vergessen,", "tokens": ["Stets", "auf", "dem", "Weg", ",", "den", "ich", "nicht", "kann", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VMFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bald in der Sonne, bald des Monds Geleite;", "tokens": ["Bald", "in", "der", "Son\u00b7ne", ",", "bald", "des", "Monds", "Ge\u00b7lei\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wenn ich im Geiste sie zusammenreihte,", "tokens": ["Wenn", "ich", "im", "Geis\u00b7te", "sie", "zu\u00b7sam\u00b7men\u00b7reih\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wieviel des Landes h\u00e4tt' ich wohl durchmessen,", "tokens": ["Wie\u00b7viel", "des", "Lan\u00b7des", "h\u00e4tt'", "ich", "wohl", "durch\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie vieles h\u00e4tt ich sehen wohl indessen", "tokens": ["Wie", "vie\u00b7les", "h\u00e4tt", "ich", "se\u00b7hen", "wohl", "in\u00b7des\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "VAFIN", "PPER", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und h\u00f6ren k\u00f6nnen in der Fern' und Weite!", "tokens": ["Und", "h\u00f6\u00b7ren", "k\u00f6n\u00b7nen", "in", "der", "Fern'", "und", "Wei\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "APPR", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Meinst du, da\u00df du versammelt alle Strahlen", "tokens": ["Meinst", "du", ",", "da\u00df", "du", "ver\u00b7sam\u00b7melt", "al\u00b7le", "Strah\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Sch\u00f6nheit habest so an deinem Bette,", "tokens": ["Der", "Sch\u00f6n\u00b7heit", "ha\u00b7best", "so", "an", "dei\u00b7nem", "Bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df all die Weit' dagegen leere Schalen?", "tokens": ["Da\u00df", "all", "die", "Weit'", "da\u00b7ge\u00b7gen", "lee\u00b7re", "Scha\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ART", "NN", "PAV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die Berge, W\u00e4lder, Str\u00f6me, Menschen, St\u00e4dte!", "tokens": ["Die", "Ber\u00b7ge", ",", "W\u00e4l\u00b7der", ",", "Str\u00f6\u00b7me", ",", "Men\u00b7schen", ",", "St\u00e4d\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Womit willst du das Leben mir bezahlen,", "tokens": ["Wo\u00b7mit", "willst", "du", "das", "Le\u00b7ben", "mir", "be\u00b7zah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das ich versitz' an deiner Liebe Kette?", "tokens": ["Das", "ich", "ver\u00b7sitz'", "an", "dei\u00b7ner", "Lie\u00b7be", "Ket\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}