{"textgrid.poem.52371": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "21. Dat smeckt dor \u00e4werst ok nah!", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "De Paster S\u00e4ger tau Salaten,", "tokens": ["De", "Pas\u00b7ter", "S\u00e4\u00b7ger", "tau", "Sa\u00b7la\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "De m\u00fc\u00dft sin Holt mal f\u00fchren laten.", "tokens": ["De", "m\u00fc\u00dft", "sin", "Holt", "mal", "f\u00fch\u00b7ren", "la\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPOSAT", "NN", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "De Burknecht hadden't s\u00fcs em f\u00fchrt,", "tokens": ["De", "Burk\u00b7necht", "had\u00b7den't", "s\u00fcs", "em", "f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Un hadden s't ran, kreg jedwerein", "tokens": ["Un", "had\u00b7den", "s't", "ran", ",", "kreg", "jed\u00b7wer\u00b7ein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VAFIN", "ADJD", "PTKVZ", "$,", "VVFIN", "PIAT"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "En Sluck un Botterbrod, as sick dat h\u00fcrt.", "tokens": ["En", "Sluck", "un", "Bot\u00b7ter\u00b7brod", ",", "as", "sick", "dat", "h\u00fcrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "NN", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Na, einmal \u00e4wer m\u00fc\u00dft dat scheihn,", "tokens": ["Na", ",", "ein\u00b7mal", "\u00e4\u00b7wer", "m\u00fc\u00dft", "dat", "scheihn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "ADJD", "VMFIN", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dat einer von de Knechts w\u00fcrd krank,", "tokens": ["Dat", "ei\u00b7ner", "von", "de", "Knechts", "w\u00fcrd", "krank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "APPR", "NE", "NE", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Un Vadder, de m\u00fc\u00dft s\u00fclwst mit mang,", "tokens": ["Un", "Vad\u00b7der", ",", "de", "m\u00fc\u00dft", "s\u00fclwst", "mit", "mang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Oll Brand m\u00fc\u00dft mit tau Holt karjolen.", "tokens": ["Oll", "Brand", "m\u00fc\u00dft", "mit", "tau", "Holt", "kar\u00b7jo\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VMFIN", "APPR", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "\u00bbna\u00ab, denkt de Paster, \u00bbwenn er selber f\u00e4hrt,", "tokens": ["\u00bb", "na", "\u00ab", ",", "denkt", "de", "Pas\u00b7ter", ",", "\u00bb", "wenn", "er", "sel\u00b7ber", "f\u00e4hrt", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$(", "$,", "VVFIN", "NE", "NN", "$,", "$(", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Dann mu\u00df ich wohl was Bessers holen;", "tokens": ["Dann", "mu\u00df", "ich", "wohl", "was", "Bes\u00b7sers", "ho\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PWS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "So'n Bauer hat en guten Ranzen,", "tokens": ["So'n", "Bau\u00b7er", "hat", "en", "gu\u00b7ten", "Ran\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "FM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Wo t\u00fcchtig was hinein geh\u00f6rt.\u00ab", "tokens": ["Wo", "t\u00fcch\u00b7tig", "was", "hin\u00b7ein", "ge\u00b7h\u00f6rt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ADJD", "PWS", "ADV", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Un em f\u00f6llt in, dat hei en ganzen", "tokens": ["Un", "em", "f\u00f6llt", "in", ",", "dat", "hei", "en", "gan\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "FM", "FM", "FM", "ADJA"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.15": {"text": "Limburger K\u00e4s' in'n Keller hett;", "tokens": ["Lim\u00b7bur\u00b7ger", "K\u00e4s'", "in'n", "Kel\u00b7ler", "hett", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPRART", "NN", "VAFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.16": {"text": "De w\u00fcrd nu v\u00f6r den Buren set't,", "tokens": ["De", "w\u00fcrd", "nu", "v\u00f6r", "den", "Bu\u00b7ren", "set't", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Un Botter, Brod und Sluck dortau.", "tokens": ["Un", "Bot\u00b7ter", ",", "Brod", "und", "Sluck", "dor\u00b7tau", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "KON", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "De Bur, de ett in gaude Rauh", "tokens": ["De", "Bur", ",", "de", "ett", "in", "gau\u00b7de", "Rauh"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "NE", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Un nimmt en Dr\u00fcddel von den Kes'.", "tokens": ["Un", "nimmt", "en", "Dr\u00fcd\u00b7del", "von", "den", "Kes'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "\u00bbna, Herr Pastur, so'n Kes' as des',", "tokens": ["\u00bb", "na", ",", "Herr", "Pas\u00b7tur", ",", "so'n", "Kes'", "as", "des'", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "NN", "NN", "$,", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "So masig un dorbi so fett,", "tokens": ["So", "ma\u00b7sig", "un", "dor\u00b7bi", "so", "fett", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "FM", "FM", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Dat is, as wenn ein Botter ett;", "tokens": ["Dat", "is", ",", "as", "wenn", "ein", "Bot\u00b7ter", "ett", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "$,", "NE", "KOUS", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Un'n w\u00fcrkliches Vergn\u00e4ugen is't!\u00ab", "tokens": ["Un'n", "w\u00fcrk\u00b7li\u00b7ches", "Ver\u00b7gn\u00e4u\u00b7gen", "is't", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "De Paster fr\u00f6ggt, ob hei ok w\u00fc\u00dft,", "tokens": ["De", "Pas\u00b7ter", "fr\u00f6ggt", ",", "ob", "hei", "ok", "w\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "$,", "KOUS", "NE", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Dat dat ", "tokens": ["Dat", "dat"], "token_info": ["word", "word"], "pos": ["FM.la", "FM.la"], "meter": "-+", "measure": "iambic.single"}, "line.26": {"text": "\u00bbja\u00ab, seggt oll Brand, \u00bbdor et ick em ok v\u00f6r.\u00ab", "tokens": ["\u00bb", "ja", "\u00ab", ",", "seggt", "oll", "Brand", ",", "\u00bb", "dor", "et", "ick", "em", "ok", "v\u00f6r", ".", "\u00ab"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PTKANT", "$(", "$,", "VVFIN", "ADJD", "NN", "$,", "$(", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "+--+--+---", "measure": "dactylic.tri.plus"}, "line.27": {"text": "Un deilt sick dat, wat v\u00f6r em st\u00fcnn,", "tokens": ["Un", "deilt", "sick", "dat", ",", "wat", "v\u00f6r", "em", "st\u00fcnn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.28": {"text": "Noch richtig in twei Dr\u00fcddel in", "tokens": ["Noch", "rich\u00b7tig", "in", "twei", "Dr\u00fcd\u00b7del", "in"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "CARD", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Un f\u00fchrt dat ein sick tau Gem\u00e4ud.", "tokens": ["Un", "f\u00fchrt", "dat", "ein", "sick", "tau", "Ge\u00b7m\u00e4ud", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "\u00bbja\u00ab, seggt hei, \u00bbas en N\u00e4tkarn s\u00e4ut!", "tokens": ["\u00bb", "ja", "\u00ab", ",", "seggt", "hei", ",", "\u00bb", "as", "en", "N\u00e4t\u00b7karn", "s\u00e4ut", "!"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$(", "$,", "VVFIN", "PTKVZ", "$,", "$(", "FM", "FM", "FM", "FM", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.31": {"text": "So'n Kes', den et ick gor tau girn!\u00ab", "tokens": ["So'n", "Kes'", ",", "den", "et", "ick", "gor", "tau", "girn", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NE", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "De Paster kickt so langs de N\u00e4s' \u2013", "tokens": ["De", "Pas\u00b7ter", "kickt", "so", "langs", "de", "N\u00e4s'", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "ADV", "ADV", "NE", "NE", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Zwei Drittel von dem sch\u00f6nen K\u00e4s'!", "tokens": ["Zwei", "Drit\u00b7tel", "von", "dem", "sch\u00f6\u00b7nen", "K\u00e4s'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Un f\u00e4ngt verluren an, so ganz von firn,", "tokens": ["Un", "f\u00e4ngt", "ver\u00b7lu\u00b7ren", "an", ",", "so", "ganz", "von", "firn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "VVFIN", "PTKVZ", "$,", "ADV", "ADV", "APPR", "NE", "$,"], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.35": {"text": "Dat't man ganz lege Tiden wir'n,", "tokens": ["Dat't", "man", "ganz", "le\u00b7ge", "Ti\u00b7den", "wir'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.36": {"text": "Hei w\u00fc\u00dft nich in un ut tau kamen,", "tokens": ["Hei", "w\u00fc\u00dft", "nich", "in", "un", "ut", "tau", "ka\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "APPR", "FM", "FM", "FM", "VVFIN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.37": {"text": "Sin Weiten, de leg alltausamen,", "tokens": ["Sin", "Wei\u00b7ten", ",", "de", "leg", "all\u00b7tau\u00b7sa\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NE", "NE", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.38": {"text": "Den Klewer w\u00fcrd'n de M\u00fcs' vertehren,", "tokens": ["Den", "Kle\u00b7wer", "w\u00fcrd'n", "de", "M\u00fcs'", "ver\u00b7teh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NE", "NE", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "De ein von sine beiden M\u00e4hren,", "tokens": ["De", "ein", "von", "si\u00b7ne", "bei\u00b7den", "M\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "APPR", "PPOSAT", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "De m\u00fc\u00dft sick letzt in'n Dik vers\u00f6pen;", "tokens": ["De", "m\u00fc\u00dft", "sick", "letzt", "in'n", "Dik", "ver\u00b7s\u00f6\u00b7pen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PRF", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Na, Botter m\u00fc\u00dft hei s\u00fclwen k\u00f6pen,", "tokens": ["Na", ",", "Bot\u00b7ter", "m\u00fc\u00dft", "hei", "s\u00fcl\u00b7wen", "k\u00f6\u00b7pen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "VMFIN", "ADJD", "VVINF", "VMINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.42": {"text": "Twei von sin S\u00e4gen hadden't F\u00fcr,", "tokens": ["Twei", "von", "sin", "S\u00e4\u00b7gen", "had\u00b7den't", "F\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "VVFIN", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.43": {"text": "Un all sin Kurn hadd Brand un Rost,", "tokens": ["Un", "all", "sin", "Kurn", "hadd", "Brand", "un", "Rost", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.44": {"text": "Un allens w\u00fcrd so gruglich d\u00fcr.", "tokens": ["Un", "al\u00b7lens", "w\u00