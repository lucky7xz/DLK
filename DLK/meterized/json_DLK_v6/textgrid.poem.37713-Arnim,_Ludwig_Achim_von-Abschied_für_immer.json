{"textgrid.poem.37713": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Abschied f\u00fcr immer", "genre": "verse", "period": "N.A.", "pub_year": 1807, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist's ein Wunder, da\u00df dich alle lieben,", "tokens": ["Ist's", "ein", "Wun\u00b7der", ",", "da\u00df", "dich", "al\u00b7le", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "KOUS", "PPER", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die nach meinem Scheiden sich dir nahen,", "tokens": ["Die", "nach", "mei\u00b7nem", "Schei\u00b7den", "sich", "dir", "na\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "PRF", "PPER", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Meine Seufzer sind bei dir geblieben", "tokens": ["Mei\u00b7ne", "Seuf\u00b7zer", "sind", "bei", "dir", "ge\u00b7blie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "PPER", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und als Luftgeist sehnlich dich umfahen,", "tokens": ["Und", "als", "Luft\u00b7geist", "sehn\u00b7lich", "dich", "um\u00b7fa\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Wer zu athmen wagt an deinem Munde", "tokens": ["Wer", "zu", "ath\u00b7men", "wagt", "an", "dei\u00b7nem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PTKZU", "VVINF", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Zieht sie unbewu\u00dft zu seinem Herzen,", "tokens": ["Zieht", "sie", "un\u00b7be\u00b7wu\u00dft", "zu", "sei\u00b7nem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Diese Seufzer mancher tr\u00fcben Stunden,", "tokens": ["Die\u00b7se", "Seuf\u00b7zer", "man\u00b7cher", "tr\u00fc\u00b7ben", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Diese Geister, mir entflohn in Schmerzen.", "tokens": ["Die\u00b7se", "Geis\u00b7ter", ",", "mir", "ent\u00b7flohn", "in", "Schmer\u00b7zen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Zu lebendig war des Herzens Hoffen,", "tokens": ["Zu", "le\u00b7ben\u00b7dig", "war", "des", "Her\u00b7zens", "Hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Es vergeht nicht mit den Schmerzenstagen,", "tokens": ["Es", "ver\u00b7geht", "nicht", "mit", "den", "Schmer\u00b7zens\u00b7ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ja es liegt die Welt jetzt vor mir offen,", "tokens": ["Ja", "es", "liegt", "die", "Welt", "jetzt", "vor", "mir", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Meine Liebe f\u00fchl' ich drinnen schlagen;", "tokens": ["Mei\u00b7ne", "Lie\u00b7be", "f\u00fchl'", "ich", "drin\u00b7nen", "schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Leb' ich nun in Andern, die dich lieben,", "tokens": ["Leb'", "ich", "nun", "in", "An\u00b7dern", ",", "die", "dich", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "ADJA", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Mag ich wohl der armen Mutter gleichen,", "tokens": ["Mag", "ich", "wohl", "der", "ar\u00b7men", "Mut\u00b7ter", "glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Die ihr Kind von ihrer Brust vertrieben,", "tokens": ["Die", "ihr", "Kind", "von", "ih\u00b7rer", "Brust", "ver\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Ihre Brust dem fremden Kind zu reichen.", "tokens": ["Ih\u00b7re", "Brust", "dem", "frem\u00b7den", "Kind", "zu", "rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Hoffnungsgeister, die mit sch\u00f6nen Bildern", "tokens": ["Hoff\u00b7nungs\u00b7geis\u00b7ter", ",", "die", "mit", "sch\u00f6\u00b7nen", "Bil\u00b7dern"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mich get\u00e4uschet wie die Jugendzeiten,", "tokens": ["Mich", "ge\u00b7t\u00e4u\u00b7schet", "wie", "die", "Ju\u00b7gend\u00b7zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Meiner N\u00e4chte Einsamkeit zu mildern,", "tokens": ["Mei\u00b7ner", "N\u00e4ch\u00b7te", "Ein\u00b7sam\u00b7keit", "zu", "mil\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ich entla\u00df euch in die blauen Weiten,", "tokens": ["Ich", "ent\u00b7la\u00df", "euch", "in", "die", "blau\u00b7en", "Wei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Einen H\u00e4ndedruck gebt noch zum Scheiden,", "tokens": ["Ei\u00b7nen", "H\u00e4n\u00b7de\u00b7druck", "gebt", "noch", "zum", "Schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Sei's die Jugend, die ich heut entlassen,", "tokens": ["Sei's", "die", "Ju\u00b7gend", ",", "die", "ich", "heut", "ent\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Was auch komme, nichts will ich vermeiden,", "tokens": ["Was", "auch", "kom\u00b7me", ",", "nichts", "will", "ich", "ver\u00b7mei\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$,", "PIS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Was vorbei, das l\u00e4\u00dft sich nicht mehr fassen.", "tokens": ["Was", "vor\u00b7bei", ",", "das", "l\u00e4\u00dft", "sich", "nicht", "mehr", "fas\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKVZ", "$,", "PDS", "VVFIN", "PRF", "PTKNEG", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}