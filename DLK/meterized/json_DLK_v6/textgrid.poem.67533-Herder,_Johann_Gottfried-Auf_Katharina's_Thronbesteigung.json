{"textgrid.poem.67533": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Auf Katharina's Thronbesteigung", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die unsre Mutter ist,", "tokens": ["Die", "uns\u00b7re", "Mut\u00b7ter", "ist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die Grazie auf Europens h\u00f6chstem Throne,", "tokens": ["Die", "Gra\u00b7zie", "auf", "Eu\u00b7ro\u00b7pens", "h\u00f6chs\u00b7tem", "Thro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Heldin in der Palmenkrone,", "tokens": ["Die", "Hel\u00b7din", "in", "der", "Pal\u00b7men\u00b7kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die von dem Throne stieg, und Riga k\u00fc\u00dft:", "tokens": ["Die", "von", "dem", "Thro\u00b7ne", "stieg", ",", "und", "Ri\u00b7ga", "k\u00fc\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$,", "KON", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die G\u00f6ttin singt mein patriotisch Lied!", "tokens": ["Die", "G\u00f6t\u00b7tin", "singt", "mein", "pat\u00b7ri\u00b7o\u00b7tisch", "Lied", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Erhebe Dich, Gesang! So wie der Adler gl\u00fcht,", "tokens": ["Er\u00b7he\u00b7be", "Dich", ",", "Ge\u00b7sang", "!", "So", "wie", "der", "Ad\u00b7ler", "gl\u00fcht", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$.", "ADV", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wenn er zur Sonne zielt, stark in ihr Feuer sieht,", "tokens": ["Wenn", "er", "zur", "Son\u00b7ne", "zielt", ",", "stark", "in", "ihr", "Feu\u00b7er", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und oben dann an Jovis Thron der Donner Last", "tokens": ["Und", "o\u00b7ben", "dann", "an", "Jo\u00b7vis", "Thron", "der", "Don\u00b7ner", "Last"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "APPR", "NE", "NN", "ART", "NN", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Mit k\u00fchnem Griffe fa\u00dft:", "tokens": ["Mit", "k\u00fch\u00b7nem", "Grif\u00b7fe", "fa\u00dft", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "So hebe Dich, mein Lied, im feierlichsten Tone", "tokens": ["So", "he\u00b7be", "Dich", ",", "mein", "Lied", ",", "im", "fei\u00b7er\u00b7lichs\u00b7ten", "To\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Zur tiefsten Stuf' an Katharinens Throne,", "tokens": ["Zur", "tiefs\u00b7ten", "Stuf'", "an", "Ka\u00b7tha\u00b7ri\u00b7nens", "Thro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Auf den Sie Sich heut schwang!", "tokens": ["Auf", "den", "Sie", "Sich", "heut", "schwang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPER", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Sie ging, Sie ging den k\u00f6niglichen Gang", "tokens": ["Sie", "ging", ",", "Sie", "ging", "den", "k\u00f6\u00b7nig\u00b7li\u00b7chen", "Gang"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Hinauf zum Thron und nahm die Kaiserkrone", "tokens": ["Hin\u00b7auf", "zum", "Thron", "und", "nahm", "die", "Kai\u00b7ser\u00b7kro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Und Ru\u00dflands Scepter in die Hand.", "tokens": ["Und", "Ru\u00df\u00b7lands", "Scep\u00b7ter", "in", "die", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "O jauchze dreimal, Land!", "tokens": ["O", "jauch\u00b7ze", "drei\u00b7mal", ",", "Land", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$,", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Den Scepter k\u00fc\u00dfte Sie und w\u00e4gt' ihn mit der Rechte", "tokens": ["Den", "Scep\u00b7ter", "k\u00fc\u00df\u00b7te", "Sie", "und", "w\u00e4gt'", "ihn", "mit", "der", "Rech\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und sprach: \u00bbDu sollst kein Stecken meiner Knechte,", "tokens": ["Und", "sprach", ":", "\u00bb", "Du", "sollst", "kein", "Ste\u00b7cken", "mei\u00b7ner", "Knech\u00b7te", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "PPER", "VMFIN", "PIAT", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Ein Gnadenscepter sollst Du sein!\u00ab", "tokens": ["Ein", "Gna\u00b7den\u00b7scep\u00b7ter", "sollst", "Du", "sein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VAINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Sie sprach's. Und Ru\u00dfland jauchzete darein,", "tokens": ["Sie", "sprach'", "s.", "Und", "Ru\u00df\u00b7land", "jauch\u00b7ze\u00b7te", "da\u00b7rein", ","], "token_info": ["word", "word", "abbreviation", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "KON", "NE", "VVFIN", "PAV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Vom Eismeer bis zu uns, vom Lena bis zum Belt;", "tokens": ["Vom", "Eis\u00b7meer", "bis", "zu", "uns", ",", "vom", "Le\u00b7na", "bis", "zum", "Belt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "APPR", "PPER", "$,", "APPRART", "NE", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Da jauchzte Katharinens Welt", "tokens": ["Da", "jauchz\u00b7te", "Ka\u00b7tha\u00b7ri\u00b7nens", "Welt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Und bebte nicht mehr. Und der Himmel brach,", "tokens": ["Und", "beb\u00b7te", "nicht", "mehr", ".", "Und", "der", "Him\u00b7mel", "brach", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADV", "$.", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Und Jovah sah herab und sprach:", "tokens": ["Und", "Jo\u00b7vah", "sah", "her\u00b7ab", "und", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "ADV", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "\u00bbdu, meines Thrones Tochter, sei mein Bild", "tokens": ["\u00bb", "du", ",", "mei\u00b7nes", "Thro\u00b7nes", "Toch\u00b7ter", ",", "sei", "mein", "Bild"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Und bitte, was Du willt!\u00ab", "tokens": ["Und", "bit\u00b7te", ",", "was", "Du", "willt", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PTKANT", "$,", "PWS", "PPER", "VMFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.27": {"text": "\u00bbnicht, Vater,\u00ab sprach Sie, \u00bbgieb mir Pracht,", "tokens": ["\u00bb", "nicht", ",", "Va\u00b7ter", ",", "\u00ab", "sprach", "Sie", ",", "\u00bb", "gieb", "mir", "Pracht", ","], "token_info": ["punct", "word", "punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PTKNEG", "$,", "NN", "$,", "$(", "VVFIN", "PPER", "$,", "$(", "VVIMP", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Die vom entnervten Mark des Landes gl\u00e4nzet,", "tokens": ["Die", "vom", "ent\u00b7nerv\u00b7ten", "Mark", "des", "Lan\u00b7des", "gl\u00e4n\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Nicht Lorbeer, der nur Menschenfeinde kr\u00e4nzet", "tokens": ["Nicht", "Lor\u00b7beer", ",", "der", "nur", "Men\u00b7schen\u00b7fein\u00b7de", "kr\u00e4n\u00b7zet"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "NE", "$,", "PRELS", "ADV", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Und, weil er blutig trieft, Tyrannen l\u00fcstern macht,", "tokens": ["Und", ",", "weil", "er", "blu\u00b7tig", "trieft", ",", "Ty\u00b7ran\u00b7nen", "l\u00fcs\u00b7tern", "macht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$,", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Nicht Reichthum, der vom Schwei\u00df des Armen gl\u00e4nzet", "tokens": ["Nicht", "Reicht\u00b7hum", ",", "der", "vom", "Schwei\u00df", "des", "Ar\u00b7men", "gl\u00e4n\u00b7zet"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "NN", "$,", "PRELS", "APPRART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Und nur f\u00fcr Schmeichler lacht \u2013", "tokens": ["Und", "nur", "f\u00fcr", "Schmeich\u00b7ler", "lacht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.33": {"text": "Nicht gieb mir dies! Doch soll ich etwas flehen,", "tokens": ["Nicht", "gieb", "mir", "dies", "!", "Doch", "soll", "ich", "et\u00b7was", "fle\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVIMP", "PPER", "PDS", "$.", "KON", "VMFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "F\u00fcr mich nicht \u2013 f\u00fcr die Kinder, f\u00fcr mein Land,", "tokens": ["F\u00fcr", "mich", "nicht", "\u2013", "f\u00fcr", "die", "Kin\u00b7der", ",", "f\u00fcr", "mein", "Land", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKNEG", "$(", "APPR", "ART", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "So gieb mir Mutterherz und Salomon's Verstand!\u00ab", "tokens": ["So", "gieb", "mir", "Mut\u00b7ter\u00b7herz", "und", "Sa\u00b7lo\u00b7mon's", "Ver\u00b7stand", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVIMP", "PPER", "NN", "KON", "NE", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Da feierten die Engel; da flo\u00df von Jovah's H\u00f6hen", "tokens": ["Da", "fei\u00b7er\u00b7ten", "die", "En\u00b7gel", ";", "da", "flo\u00df", "von", "Jo\u00b7vah's", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "ADV", "VVFIN", "APPR", "NE", "NN"], "meter": "-+---+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.37": {"text": "Der Weisheit Oel, wie Thau vom Hermon flie\u00dft,", "tokens": ["Der", "Weis\u00b7heit", "O\u00b7el", ",", "wie", "Thau", "vom", "Her\u00b7mon", "flie\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PWAV", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.38": {"text": "In Str\u00f6men auf Ihr Haupt, und \u2013 Sie ward, was Sie ist!", "tokens": ["In", "Str\u00f6\u00b7men", "auf", "Ihr", "Haupt", ",", "und", "\u2013", "Sie", "ward", ",", "was", "Sie", "ist", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPOSAT", "NN", "$,", "KON", "$(", "PPER", "VAFIN", "$,", "PWS", "PPER", "VAFIN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.39": {"text": "Monarchin, Mutter, Kaiserin,", "tokens": ["Mon\u00b7ar\u00b7chin", ",", "Mut\u00b7ter", ",", "Kai\u00b7se\u00b7rin", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Europens Schiedesrichterin,", "tokens": ["Eu\u00b7ro\u00b7pens", "Schie\u00b7des\u00b7rich\u00b7te\u00b7rin", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Die G\u00f6ttin Ru\u00dflands und der Glanz in Norden \u2013", "tokens": ["Die", "G\u00f6t\u00b7tin", "Ru\u00df\u00b7lands", "und", "der", "Glanz", "in", "Nor\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "KON", "ART", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "Das Alles und noch mehr ist Katharina worden.", "tokens": ["Das", "Al\u00b7les", "und", "noch", "mehr", "ist", "Ka\u00b7tha\u00b7ri\u00b7na", "wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "KON", "ADV", "ADV", "VAFIN", "NE", "VAPP", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.43": {"text": "Ihr Waffentr\u00e4ger, stark durch Ihre Macht,", "tokens": ["Ihr", "Waf\u00b7fen\u00b7tr\u00e4\u00b7ger", ",", "stark", "durch", "Ih\u00b7re", "Macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.44": {"text": "Ihr Adler, deckt in majest\u00e4t'scher Pracht", "tokens": ["Ihr", "Ad\u00b7ler", ",", "deckt", "in", "ma\u00b7je\u00b7st\u00e4t'\u00b7scher", "Pracht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.45": {"text": "Sein weites Reich mit Ruh", "tokens": ["Sein", "wei\u00b7tes", "Reich", "mit", "Ruh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.46": {"text": "Und eilt mit feuerdrohndem Blick,", "tokens": ["Und", "eilt", "mit", "feu\u00b7er\u00b7drohn\u00b7dem", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Voll Ihres Ruhms, den Sternen zu!", "tokens": ["Voll", "Ih\u00b7res", "Ruhms", ",", "den", "Ster\u00b7nen", "zu", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "$,", "ART", "NN", "PTK