{"dta.poem.11523": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Das verlassene M\u00e4gdlein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Eh' die Sternlein verschwinden,", "tokens": ["Eh'", "die", "Stern\u00b7lein", "ver\u00b7schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Mu\u00df ich am Herde stehn,", "tokens": ["Mu\u00df", "ich", "am", "Her\u00b7de", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mu\u00df Feuer z\u00fcnden.", "tokens": ["Mu\u00df", "Feu\u00b7er", "z\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Sch\u00f6n ist der Flammen Schein,", "tokens": ["Sch\u00f6n", "ist", "der", "Flam\u00b7men", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Es springen die Funken,", "tokens": ["Es", "sprin\u00b7gen", "die", "Fun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Ich schaue so drein,", "tokens": ["Ich", "schau\u00b7e", "so", "drein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "In Leid versunken.", "tokens": ["In", "Leid", "ver\u00b7sun\u00b7ken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Pl\u00f6tzlich, da kommt es mir,", "tokens": ["Pl\u00f6tz\u00b7lich", ",", "da", "kommt", "es", "mir", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADV", "VVFIN", "PPER", "PPER", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Treuloser Knabe,", "tokens": ["Treu\u00b7lo\u00b7ser", "Kna\u00b7be", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Da\u00df ich die Nacht von dir", "tokens": ["Da\u00df", "ich", "die", "Nacht", "von", "dir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Getr\u00e4umet habe.", "tokens": ["Ge\u00b7tr\u00e4u\u00b7met", "ha\u00b7be", "."], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Thr\u00e4ne auf Thr\u00e4ne dann", "tokens": ["Thr\u00e4\u00b7ne", "auf", "Thr\u00e4\u00b7ne", "dann"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "ADV"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "St\u00fcrzet hernieder,", "tokens": ["St\u00fcr\u00b7zet", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "So kommt der Tag heran, \u2014", "tokens": ["So", "kommt", "der", "Tag", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "O ging' er wieder!", "tokens": ["O", "ging'", "er", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}