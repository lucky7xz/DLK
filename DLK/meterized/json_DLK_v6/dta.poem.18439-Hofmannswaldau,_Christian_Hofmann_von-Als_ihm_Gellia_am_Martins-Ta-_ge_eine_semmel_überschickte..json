{"dta.poem.18439": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als ihm Gellia am Martins-Ta-  \n ge eine semmel \u00fcberschickte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du schickst mir/ Gellia/ von keinem horne nicht/", "tokens": ["Du", "schickst", "mir", "/", "Gel\u00b7lia", "/", "von", "kei\u00b7nem", "hor\u00b7ne", "nicht", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$(", "NE", "$(", "APPR", "PIAT", "NN", "PTKNEG", "$("], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und di\u00df nur/ was man sonst bey ieder malzeit bricht.", "tokens": ["Und", "di\u00df", "nur", "/", "was", "man", "sonst", "bey", "ie\u00b7der", "mal\u00b7zeit", "bricht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADV", "$(", "PWS", "PIS", "ADV", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du denckest bey dir selbst: itzt spar ich diese gaben/", "tokens": ["Du", "den\u00b7ckest", "bey", "dir", "selbst", ":", "itzt", "spar", "ich", "die\u00b7se", "ga\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ADV", "$.", "ADV", "ADJD", "PPER", "PDS", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bekomm ich einen mann/ der sol die h\u00f6rner haben.", "tokens": ["Be\u00b7komm", "ich", "ei\u00b7nen", "mann", "/", "der", "sol", "die", "h\u00f6r\u00b7ner", "ha\u00b7ben", "."], "token_info": ["word", "word",