{"textgrid.poem.41298": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Menor", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie weit ich Menors Herz besessen,", "tokens": ["Wie", "weit", "ich", "Me\u00b7nors", "Herz", "be\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das wei\u00df er freilich mehr als ich;", "tokens": ["Das", "wei\u00df", "er", "frei\u00b7lich", "mehr", "als", "ich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADV", "KOUS", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch hat er \u00f6fters sich vermessen,", "tokens": ["Doch", "hat", "er", "\u00f6f\u00b7ters", "sich", "ver\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mich lieb' er, und recht br\u00fcderlich.", "tokens": ["Mich", "lieb'", "er", ",", "und", "recht", "br\u00fc\u00b7der\u00b7lich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KON", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Als einen Feind w\u00fcrd' er mich nicht vergessen:", "tokens": ["Als", "ei\u00b7nen", "Feind", "w\u00fcrd'", "er", "mich", "nicht", "ver\u00b7ges\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VAFIN", "PPER", "PRF", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Als einen Freund vergi\u00dft er mich.", "tokens": ["Als", "ei\u00b7nen", "Freund", "ver\u00b7gi\u00dft", "er", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}