{"textgrid.poem.45056": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "M.G. Saphir", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was Wunder auch, da\u00df er hier gilt? Er galt", "tokens": ["Was", "Wun\u00b7der", "auch", ",", "da\u00df", "er", "hier", "gilt", "?", "Er", "galt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "NN", "ADV", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ja auch in andern L\u00e4ndern und Provinzen.", "tokens": ["Ja", "auch", "in", "an\u00b7dern", "L\u00e4n\u00b7dern", "und", "Pro\u00b7vin\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Allein den Lohn, womit man ihn bezahlt,", "tokens": ["Al\u00b7lein", "den", "Lohn", ",", "wo\u00b7mit", "man", "ihn", "be\u00b7zahlt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PWAV", "PIS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Schlug man in andern \u2013 andern M\u00fcnzen!", "tokens": ["Schlug", "man", "in", "an\u00b7dern", "\u2013", "an\u00b7dern", "M\u00fcn\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ADJA", "$(", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}