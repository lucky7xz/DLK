{"textgrid.poem.28755": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Freund, da ieder sich ietzt freut, da\u00df mit dieses Winters Fr\u00f6sten", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freund, da ieder sich ietzt freut, da\u00df mit dieses Winters Fr\u00f6sten", "tokens": ["Freund", ",", "da", "ie\u00b7der", "sich", "ietzt", "freut", ",", "da\u00df", "mit", "die\u00b7ses", "Win\u00b7ters", "Fr\u00f6s\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PIS", "PRF", "ADV", "VVFIN", "$,", "KOUS", "APPR", "PDAT", "NN", "NN"], "meter": "+-+---+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Auch de\u00df langen Krieges Ei\u00df werde schmeltzen und den L\u00fcsten", "tokens": ["Auch", "de\u00df", "lan\u00b7gen", "Krie\u00b7ges", "Ei\u00df", "wer\u00b7de", "schmelt\u00b7zen", "und", "den", "L\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "NN", "VAFIN", "VVINF", "KON", "ART", "NN"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Nechsten Fr\u00fchlings sich die Zier auch de\u00df Friedens mischen ein,", "tokens": ["Nechs\u00b7ten", "Fr\u00fch\u00b7lings", "sich", "die", "Zier", "auch", "de\u00df", "Frie\u00b7dens", "mi\u00b7schen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PRF", "ART", "NN", "ADV", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+---+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "O, so seh ich dein Gesicht tr\u00fcbe, bla\u00df, na\u00df, kr\u00e4ncklich seyn.", "tokens": ["O", ",", "so", "seh", "ich", "dein", "Ge\u00b7sicht", "tr\u00fc\u00b7be", ",", "bla\u00df", ",", "na\u00df", ",", "kr\u00e4n\u00b7ck\u00b7lich", "seyn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "VVFIN", "$,", "ADJD", "$,", "ADJD", "$,", "ADJD", "VAINF", "$."], "meter": "+-+-+-+--+-+--+", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Wolte Gott! noch dir noch mir w\u00e4r die Ursach also k\u00fcndig!", "tokens": ["Wol\u00b7te", "Gott", "!", "noch", "dir", "noch", "mir", "w\u00e4r", "die", "Ur\u00b7sach", "al\u00b7so", "k\u00fcn\u00b7dig", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "$.", "ADV", "PPER", "ADV", "PPER", "VAFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "+--+-+-+-+-+-+-", "measure": "iambic.septa.invert"}, "line.6": {"text": "Mir zwar ist sie in dem Sinn, aber dir, dir ist sie f\u00fcndig,", "tokens": ["Mir", "zwar", "ist", "sie", "in", "dem", "Sinn", ",", "a\u00b7ber", "dir", ",", "dir", "ist", "sie", "f\u00fcn\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "$,", "KON", "PPER", "$,", "PPER", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "Wo du hin gehst, sihst und stehst, was du denckest, was du thust;", "tokens": ["Wo", "du", "hin", "gehst", ",", "sihst", "und", "stehst", ",", "was", "du", "den\u00b7ckest", ",", "was", "du", "thust", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "--+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.8": {"text": "Dr\u00fcber mangelt leider dir deine Friedens-Fr\u00fchlings-Lust.", "tokens": ["Dr\u00fc\u00b7ber", "man\u00b7gelt", "lei\u00b7der", "dir", "dei\u00b7ne", "Frie\u00b7dens\u00b7Fr\u00fch\u00b7lings\u00b7Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.9": {"text": "Deine Frieden-Fr\u00fchlings-Lust hat de\u00df Krieges raues st\u00fcrmen", "tokens": ["Dei\u00b7ne", "Frie\u00b7den\u00b7Fr\u00fch\u00b7lings\u00b7Lust", "hat", "de\u00df", "Krie\u00b7ges", "rau\u00b7es", "st\u00fcr\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "ADJA", "VVINF"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Offt geblast, doch nie gest\u00fcrtzt; aber ach! de\u00df Grabes W\u00fcrmen", "tokens": ["Offt", "ge\u00b7blast", ",", "doch", "nie", "ge\u00b7st\u00fcrtzt", ";", "a\u00b7ber", "ach", "!", "de\u00df", "Gra\u00b7bes", "W\u00fcr\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVPP", "$,", "ADV", "ADV", "VVPP", "$.", "ADV", "ADV", "$.", "ART", "ADJA", "NN"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.11": {"text": "Gab der Tod zum Opffer sie, ohngeacht das halbe Theil", "tokens": ["Gab", "der", "Tod", "zum", "Opf\u00b7fer", "sie", ",", "ohn\u00b7ge\u00b7acht", "das", "hal\u00b7be", "Theil"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "PPER", "$,", "ADJD", "ART", "ADJA", "NN"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.12": {"text": "Deiner dran verbunden hing, auch wol gar dein sterblich Heil.", "tokens": ["Dei\u00b7ner", "dran", "ver\u00b7bun\u00b7den", "hing", ",", "auch", "wol", "gar", "dein", "sterb\u00b7lich", "Heil", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "PAV", "VVPP", "VVFIN", "$,", "ADV", "ADV", "ADV", "PPOSAT", "ADJD", "NN", "$."], "meter": "+-+-+-+-+--+-+", "measure": "trochaic.septa.relaxed"}, "line.13": {"text": "Weder Schatz, wie gro\u00df er sey, ist uns M\u00e4nnern so ersprie\u00dflich,", "tokens": ["We\u00b7der", "Schatz", ",", "wie", "gro\u00df", "er", "sey", ",", "ist", "uns", "M\u00e4n\u00b7nern", "so", "er\u00b7sprie\u00df\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "$,", "VAFIN", "PPER", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "Weder Freund, wie gut er sey, ist uns M\u00e4nnern so geni\u00dflich,", "tokens": ["We\u00b7der", "Freund", ",", "wie", "gut", "er", "sey", ",", "ist", "uns", "M\u00e4n\u00b7nern", "so", "ge\u00b7ni\u00df\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "$,", "VAFIN", "PPER", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.15": {"text": "Als der uns in Armen schlieff; dann die angetreute Treu", "tokens": ["Als", "der", "uns", "in", "Ar\u00b7men", "schlieff", ";", "dann", "die", "an\u00b7ge\u00b7treu\u00b7te", "Treu"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "PPER", "APPR", "NN", "VVFIN", "$.", "ADV", "ART", "ADJA", "NN"], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.16": {"text": "Herrschet \u00fcber Leid und Zeit, wird durch alt seyn immer neu.", "tokens": ["Herr\u00b7schet", "\u00fc\u00b7ber", "Leid", "und", "Zeit", ",", "wird", "durch", "alt", "seyn", "im\u00b7mer", "neu", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$,", "VAFIN", "APPR", "ADJD", "VAINF", "ADV", "ADJD", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.17": {"text": "Wem ist mehr als mir bewust, wie die Jugend eurer Liebe", "tokens": ["Wem", "ist", "mehr", "als", "mir", "be\u00b7wust", ",", "wie", "die", "Ju\u00b7gend", "eu\u00b7rer", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ADV", "KOUS", "PPER", "VVFIN", "$,", "PWAV", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+--+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Erstlich wuchs und weiter wuchs? Aller Grund, worauff sie bliebe,", "tokens": ["Erst\u00b7lich", "wuchs", "und", "wei\u00b7ter", "wuchs", "?", "Al\u00b7ler", "Grund", ",", "wo\u00b7rauff", "sie", "blie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "KON", "ADV", "VVFIN", "$.", "ADJA", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.19": {"text": "War die Treu und Redligkeit. Anders was das tauret nicht;", "tokens": ["War", "die", "Treu", "und", "Red\u00b7lig\u00b7keit", ".", "An\u00b7ders", "was", "das", "tau\u00b7ret", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "KON", "NN", "$.", "ADV", "PWS", "PDS", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.20": {"text": "Was sich auff verg\u00e4nglich Ding st\u00fctzet, das verf\u00e4llt und bricht;", "tokens": ["Was", "sich", "auff", "ver\u00b7g\u00e4ng\u00b7lich", "Ding", "st\u00fct\u00b7zet", ",", "das", "ver\u00b7f\u00e4llt", "und", "bricht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPR", "ADJD", "NN", "VVFIN", "$,", "PDS", "VVFIN", "KON", "VVFIN", "$."], "meter": "--+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "Was die Tugend baut, das steht. Wann ich dencke mehr zur\u00fccke", "tokens": ["Was", "die", "Tu\u00b7gend", "baut", ",", "das", "steht", ".", "Wann", "ich", "den\u00b7cke", "mehr", "zu\u00b7r\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,", "PDS", "VVFIN", "$.", "PWAV", "PPER", "VVFIN", "ADV", "VVFIN"], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "Auff die nun verrauchte Zeit, auff mein mir begrabnes Gl\u00fccke,", "tokens": ["Auff", "die", "nun", "ver\u00b7rauch\u00b7te", "Zeit", ",", "auff", "mein", "mir", "be\u00b7grab\u00b7nes", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN", "$,", "APPR", "PPOSAT", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.23": {"text": "Denck ich auch zugleiche fort auff der Freundschafft Schwesterschafft,", "tokens": ["Denck", "ich", "auch", "zu\u00b7glei\u00b7che", "fort", "auff", "der", "Freund\u00b7schafft", "Schwes\u00b7ter\u00b7schafft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADJA", "PTKVZ", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.24": {"text": "Drinnen dein und meine Lust unverbr\u00fcchlich war verhafft,", "tokens": ["Drin\u00b7nen", "dein", "und", "mei\u00b7ne", "Lust", "un\u00b7ver\u00b7br\u00fcch\u00b7lich", "war", "ver\u00b7hafft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "KON", "PPOSAT", "NN", "ADJD", "VAFIN", "VVPP", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.25": {"text": "Wie sich dein und meine Lieb unter sich so lieblich liebten,", "tokens": ["Wie", "sich", "dein", "und", "mei\u00b7ne", "Lieb", "un\u00b7ter", "sich", "so", "lieb\u00b7lich", "lieb\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "PPOSAT", "KON", "PPOSAT", "NN", "APPR", "PRF", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.26": {"text": "Auch de\u00df Blutes nahe Pflicht durch vertraute Sinnen \u00fcbten.", "tokens": ["Auch", "de\u00df", "Blu\u00b7tes", "na\u00b7he", "Pflicht", "durch", "ver\u00b7trau\u00b7te", "Sin\u00b7nen", "\u00fcb\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.27": {"text": "Als der Tod mein erste Treu gleich verbarg in frischen Sand,", "tokens": ["Als", "der", "Tod", "mein", "ers\u00b7te", "Treu", "gleich", "ver\u00b7barg", "in", "fri\u00b7schen", "Sand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "ADJA", "NN", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "--+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "Dennoch hat das liebe Mensch ein vertrautes Freundschaffts Band", "tokens": ["Den\u00b7noch", "hat", "das", "lie\u00b7be", "Mensch", "ein", "ver\u00b7trau\u00b7tes", "Freund\u00b7schaffts", "Band"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.29": {"text": "Auff die Meinen unverf\u00e4lscht immer fort und fort erstrecket,", "tokens": ["Auff", "die", "Mei\u00b7nen", "un\u00b7ver\u00b7f\u00e4lscht", "im\u00b7mer", "fort", "und", "fort", "er\u00b7stre\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "ADV", "PTKVZ", "KON", "PTKVZ", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.30": {"text": "Bi\u00df nun auch de\u00df Todes Neid ihr das letzte Ziel gestecket.", "tokens": ["Bi\u00df", "nun", "auch", "de\u00df", "To\u00b7des", "Neid", "ihr", "das", "letz\u00b7te", "Ziel", "ge\u00b7ste\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADV", "ART", "NN", "NN", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.31": {"text": "Sey gesichert, treuer Freund, da\u00df dein Augen nicht allein,", "tokens": ["Sey", "ge\u00b7si\u00b7chert", ",", "treu\u00b7er", "Freund", ",", "da\u00df", "dein", "Au\u00b7gen", "nicht", "al\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "ADJA", "NN", "$,", "KOUS", "PPOSAT", "NN", "PTKNEG", "ADV", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.32": {"text": "Sondern mir und meinem Haus in Gesellschafft w\u00e4\u00dfrig seyn.", "tokens": ["Son\u00b7dern", "mir", "und", "mei\u00b7nem", "Haus", "in", "Ge\u00b7sell\u00b7schafft", "w\u00e4\u00df\u00b7rig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "KON", "PPOSAT", "NN", "APPR", "NN", "ADJD", "VAINF", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.33": {"text": "Welcher das gemeine Falsch, das die Welt f\u00fcr Witz verhandelt,", "tokens": ["Wel\u00b7cher", "das", "ge\u00b7mei\u00b7ne", "Falsch", ",", "das", "die", "Welt", "f\u00fcr", "Witz", "ver\u00b7han\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.34": {"text": "Kennt und hasst, dem wird sein Hertz auff betr\u00fcbten Mut gewandelt,", "tokens": ["Kennt", "und", "hasst", ",", "dem", "wird", "sein", "Hertz", "auff", "be\u00b7tr\u00fcb\u00b7ten", "Mut", "ge\u00b7wan\u00b7delt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "$,", "PDS", "VAFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.35": {"text": "Wann ein redlich-fromer Christ hin sich sichert in den Sarck,", "tokens": ["Wann", "ein", "red\u00b7lich\u00b7fro\u00b7mer", "Christ", "hin", "sich", "si\u00b7chert", "in", "den", "Sarck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ADV", "PRF", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.36": {"text": "Weil das From geschw\u00e4cht dadurch und verst\u00e4rcket wird das Arg.", "tokens": ["Weil", "das", "From", "ge\u00b7schw\u00e4cht", "da\u00b7durch", "und", "ver\u00b7st\u00e4r\u00b7cket", "wird", "das", "Arg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "PAV", "KON", "VVFIN", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.37": {"text": "Nun was hilffts? Es mu\u00df so seyn! in der Welt von Kindes Beinen", "tokens": ["Nun", "was", "hilffts", "?", "Es", "mu\u00df", "so", "seyn", "!", "in", "der", "Welt", "von", "Kin\u00b7des", "Bei\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PWS", "VVFIN", "$.", "PPER", "VMFIN", "ADV", "VAINF", "$.", "APPR", "ART", "NN", "APPR", "NN", "NN"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.38": {"text": "Hat man, da\u00df der Mensch verstarb, h\u00f6ren klagen, sehen weinen;", "tokens": ["Hat", "man", ",", "da\u00df", "der", "Mensch", "ver\u00b7starb", ",", "h\u00f6\u00b7ren", "kla\u00b7gen", ",", "se\u00b7hen", "wei\u00b7nen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "$,", "KOUS", "ART", "NN", "VVFIN", "$,", "VVINF", "VVINF", "$,", "VVINF", "VVINF", "$."], "meter": "--+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.39": {"text": "Nun sie auff der Gruben geht, wird es anders wol nicht seyn,", "tokens": ["Nun", "sie", "auff", "der", "Gru\u00b7ben", "geht", ",", "wird", "es", "an\u00b7ders", "wol", "nicht", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "ART", "NN", "VVFIN", "$,", "VAFIN", "PPER", "ADV", "ADV", "PTKNEG", "VAINF", "$,"], "meter": "--+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.40": {"text": "Als da\u00df iederman in ihr, sie auch k\u00fcrtzlich selbst geht ein.", "tokens": ["Als", "da\u00df", "ie\u00b7der\u00b7man", "in", "ihr", ",", "sie", "auch", "k\u00fcrtz\u00b7lich", "selbst", "geht", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PIS", "APPR", "PPER", "$,", "PPER", "ADV", "ADJD", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.41": {"text": "Ey gar gut! was d\u00fcnckt uns wol, wann wir stetig solten leben,", "tokens": ["Ey", "gar", "gut", "!", "was", "d\u00fcnckt", "uns", "wol", ",", "wann", "wir", "ste\u00b7tig", "sol\u00b7ten", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "$.", "PWS", "VVFIN", "PPER", "ADV", "$,", "PWAV", "PPER", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.42": {"text": "Solten stets der Teuffeley dieser Welt seyn untergeben?", "tokens": ["Sol\u00b7ten", "stets", "der", "Teuf\u00b7fe\u00b7ley", "die\u00b7ser", "Welt", "seyn", "un\u00b7ter\u00b7ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "PDAT", "NN", "VAINF", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.43": {"text": "Nemen wir noch eine Welt und best\u00fcnden noch einmal,", "tokens": ["Ne\u00b7men", "wir", "noch", "ei\u00b7ne", "Welt", "und", "be\u00b7st\u00fcn\u00b7den", "noch", "ein\u00b7mal", ","], "token_info": ["word", "word", "