{"dta.poem.5494": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Croceon auton.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.71", "cy:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Kann das wol m\u00f6glich seyn!", "tokens": ["Kann", "das", "wol", "m\u00f6g\u00b7lich", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDS", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Sprach ich, als j\u00fcngst mein G\u00e4rtner mir,", "tokens": ["Sprach", "ich", ",", "als", "j\u00fcngst", "mein", "G\u00e4rt\u00b7ner", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KOUS", "ADV", "PPOSAT", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In einer purpur-farbnen Zier,", "tokens": ["In", "ei\u00b7ner", "pur\u00b7pur\u00b7fa\u00b7rb\u00b7nen", "Zier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und einem weissen Silber Schein,", "tokens": ["Und", "ei\u00b7nem", "weis\u00b7sen", "Sil\u00b7ber", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Croceon avton mit diesen Worten gab:", "tokens": ["Ein", "Cro\u00b7ce\u00b7on", "av\u00b7ton", "mit", "die\u00b7sen", "Wor\u00b7ten", "gab", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NE", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Durch blosse Luft allein gen\u00e4hret werde.", "tokens": ["Durch", "blos\u00b7se", "Luft", "al\u00b7lein", "ge\u00b7n\u00e4h\u00b7ret", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich setzte sie demnach, um dieses zu probiren,", "tokens": ["Ich", "setz\u00b7te", "sie", "dem\u00b7nach", ",", "um", "die\u00b7ses", "zu", "pro\u00b7bi\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PAV", "$,", "KOUI", "PDS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und von der Wahrheit de\u00df mich selbst zu \u00fcberf\u00fchren,", "tokens": ["Und", "von", "der", "Wahr\u00b7heit", "de\u00df", "mich", "selbst", "zu", "\u00fc\u00b7berf\u00b7\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gleich vor mein Fenster hin, und fand es w\u00fcrcklich wahr.", "tokens": ["Gleich", "vor", "mein", "Fens\u00b7ter", "hin", ",", "und", "fand", "es", "w\u00fcrck\u00b7lich", "wahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Blume w\u00e4chs\u2019t und bl\u00fcht an diesem Ort", "tokens": ["Die", "Blu\u00b7me", "w\u00e4chs't", "und", "bl\u00fcht", "an", "die\u00b7sem", "Ort"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ohn Erd\u2019 und sonder Na\u00df best\u00e4ndig fort.", "tokens": ["Ohn", "Erd'", "und", "son\u00b7der", "Na\u00df", "be\u00b7st\u00e4n\u00b7dig", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mein Leser, sprich mit mir: ist die\u00df nicht wunderbar?", "tokens": ["Mein", "Le\u00b7ser", ",", "sprich", "mit", "mir", ":", "ist", "die\u00df", "nicht", "wun\u00b7der\u00b7bar", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "APPR", "PPER", "$.", "VAFIN", "PDS", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wir haben erst vor wenig Jahren,", "tokens": ["Wir", "ha\u00b7ben", "erst", "vor", "we\u00b7nig", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df Blumen, setzt man sie nur auf ein Glas,", "tokens": ["Da\u00df", "Blu\u00b7men", ",", "setzt", "man", "sie", "nur", "auf", "ein", "Glas", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "VVFIN", "PIS", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ohn Erde, blo\u00df allein durchs Na\u00df", "tokens": ["Ohn", "Er\u00b7de", ",", "blo\u00df", "al\u00b7lein", "durchs", "Na\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "ADV", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gedeyen, gantz erstaunt erfahren:", "tokens": ["Ge\u00b7de\u00b7yen", ",", "gantz", "er\u00b7staunt", "er\u00b7fah\u00b7ren", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Hier stellt der Sch\u00f6pfer uns ein neues Wunder dar,", "tokens": ["Hier", "stellt", "der", "Sch\u00f6p\u00b7fer", "uns", "ein", "neu\u00b7es", "Wun\u00b7der", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und zeigt in dieser Blum uns klar", "tokens": ["Und", "zeigt", "in", "die\u00b7ser", "Blum", "uns", "klar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PDAT", "NE", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und \u00fcberzeuglich an,", "tokens": ["Und", "\u00fc\u00b7berz\u00b7eug\u00b7lich", "an", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Zu seinem Lobe, Ruhm und Preise,", "tokens": ["Zu", "sei\u00b7nem", "Lo\u00b7be", ",", "Ruhm", "und", "Prei\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und seines grossen Nahmens Ehren,", "tokens": ["Und", "sei\u00b7nes", "gros\u00b7sen", "Nah\u00b7mens", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Da\u00df er, auf ungezehlte Weise,", "tokens": ["Da\u00df", "er", ",", "auf", "un\u00b7ge\u00b7zehl\u00b7te", "Wei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Die Creatur erschaffen, n\u00e4hren,", "tokens": ["Die", "Crea\u00b7tur", "er\u00b7schaf\u00b7fen", ",", "n\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Versorgen und erhalten kann.", "tokens": ["Ver\u00b7sor\u00b7gen", "und", "er\u00b7hal\u00b7ten", "kann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Ach m\u00f6gten wir demnach, ohn dich, HErr, zu erh\u00f6hen,", "tokens": ["Ach", "m\u00f6g\u00b7ten", "wir", "dem\u00b7nach", ",", "ohn", "dich", ",", "Herr", ",", "zu", "er\u00b7h\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PAV", "$,", "KOUI", "PPER", "$,", "NN", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Die\u00df seltne Bl\u00fcmchen nimmer sehen.", "tokens": ["Die\u00df", "selt\u00b7ne", "Bl\u00fcm\u00b7chen", "nim\u00b7mer", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}