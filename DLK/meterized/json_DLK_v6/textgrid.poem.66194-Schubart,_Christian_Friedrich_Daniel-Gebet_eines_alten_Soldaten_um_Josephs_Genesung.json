{"textgrid.poem.66194": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Gebet eines alten Soldaten um Josephs Genesung", "genre": "verse", "period": "N.A.", "pub_year": 1789, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gro\u00dfer Gott in deinem Himmel,", "tokens": ["Gro\u00b7\u00dfer", "Gott", "in", "dei\u00b7nem", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueber alles hoch erh\u00f6ht;", "tokens": ["Ue\u00b7ber", "al\u00b7les", "hoch", "er\u00b7h\u00f6ht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Herrscher \u00fcber F\u00fcrstenthronen,", "tokens": ["Herr\u00b7scher", "\u00fc\u00b7ber", "F\u00fcrs\u00b7ten\u00b7thro\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kaiser aller Nationen,", "tokens": ["Kai\u00b7ser", "al\u00b7ler", "Na\u00b7ti\u00b7o\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "H\u00f6re mein Gebet!", "tokens": ["H\u00f6\u00b7re", "mein", "Ge\u00b7bet", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Joseph, unser guter Kaiser,", "tokens": ["Jo\u00b7se\u00b7ph", ",", "un\u00b7ser", "gu\u00b7ter", "Kai\u00b7ser", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Ist in t\u00f6dtlicher Gefahr!", "tokens": ["Ist", "in", "t\u00f6dt\u00b7li\u00b7cher", "Ge\u00b7fahr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Joseph, unser V\u00f6lkervater,", "tokens": ["Jo\u00b7se\u00b7ph", ",", "un\u00b7ser", "V\u00f6l\u00b7ker\u00b7va\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Der im feindlichen Geschwader", "tokens": ["Der", "im", "feind\u00b7li\u00b7chen", "Ge\u00b7schwa\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Unser F\u00fchrer war;", "tokens": ["Un\u00b7ser", "F\u00fch\u00b7rer", "war", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Der auf seinem Atlasr\u00fccken", "tokens": ["Der", "auf", "sei\u00b7nem", "At\u00b7las\u00b7r\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lasten ganzer Welten trug.", "tokens": ["Las\u00b7ten", "gan\u00b7zer", "Wel\u00b7ten", "trug", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Er, der erste F\u00fcrst der Erde,", "tokens": ["Er", ",", "der", "ers\u00b7te", "F\u00fcrst", "der", "Er\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der den Feind bald mit dem Schwerte,", "tokens": ["Der", "den", "Feind", "bald", "mit", "dem", "Schwer\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bald mit Weisheit schlug;", "tokens": ["Bald", "mit", "Weis\u00b7heit", "schlug", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Der mit Adlerschnelle eilte", "tokens": ["Der", "mit", "Ad\u00b7ler\u00b7schnel\u00b7le", "eil\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von der That zur andern That.", "tokens": ["Von", "der", "That", "zur", "an\u00b7dern", "That", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, der mich mit H\u00e4ndedr\u00fccken", "tokens": ["Ach", ",", "der", "mich", "mit", "H\u00e4n\u00b7de\u00b7dr\u00fc\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PRELS", "PRF", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und mit Zunder in den Blicken", "tokens": ["Und", "mit", "Zun\u00b7der", "in", "den", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nannte: Kamerad!", "tokens": ["Nann\u00b7te", ":", "Ka\u00b7me\u00b7rad", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$.", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Der soll sterben? Todesengel,", "tokens": ["Der", "soll", "ster\u00b7ben", "?", "To\u00b7de\u00b7sen\u00b7gel", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VMFIN", "VVINF", "$.", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df von meinem Joseph ab.", "tokens": ["La\u00df", "von", "mei\u00b7nem", "Jo\u00b7se\u00b7ph", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "PPOSAT", "NE", "PTKVZ", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Nimm aus unsern Kriegerschaaren", "tokens": ["Nimm", "aus", "un\u00b7sern", "Krie\u00b7ger\u00b7schaa\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schleppe mich ins Grab!", "tokens": ["Schlep\u00b7pe", "mich", "ins", "Grab", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Wunden haben mich entkr\u00e4ftet,", "tokens": ["Wun\u00b7den", "ha\u00b7ben", "mich", "ent\u00b7kr\u00e4f\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine Knochen sind schon m\u00fcrb;", "tokens": ["Mei\u00b7ne", "Kno\u00b7chen", "sind", "schon", "m\u00fcrb", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meinst, ich zittre vor dem Tode?", "tokens": ["Meinst", ",", "ich", "zitt\u00b7re", "vor", "dem", "To\u00b7de", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nein, sprich nur, du Gottesbote;", "tokens": ["Nein", ",", "sprich", "nur", ",", "du", "Got\u00b7tes\u00b7bo\u00b7te", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "ADV", "$,", "PPER", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Alter Krieger, stirb!", "tokens": ["Al\u00b7ter", "Krie\u00b7ger", ",", "stirb", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "NN", "$,", "VVIMP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "La\u00df nur unsern Joseph leben!", "tokens": ["La\u00df", "nur", "un\u00b7sern", "Jo\u00b7se\u00b7ph", "le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Ha, schon ruht dein Flammenschwert!", "tokens": ["Ha", ",", "schon", "ruht", "dein", "Flam\u00b7men\u00b7schwert", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Preiset Gott, ihr Kameraden,", "tokens": ["Prei\u00b7set", "Gott", ",", "ihr", "Ka\u00b7me\u00b7ra\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcr die h\u00f6chste seiner Gnaden,", "tokens": ["F\u00fcr", "die", "h\u00f6chs\u00b7te", "sei\u00b7ner", "Gna\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn wir sind erh\u00f6rt.", "tokens": ["Denn", "wir", "sind", "er\u00b7h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}