{"textgrid.poem.60636": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf einem Baume sa\u00df ein alter Hahn,", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf einem Baume sa\u00df ein alter Hahn,", "tokens": ["Auf", "ei\u00b7nem", "Bau\u00b7me", "sa\u00df", "ein", "al\u00b7ter", "Hahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein schlauer Kopf in allen Lebenslagen.", "tokens": ["Ein", "schlau\u00b7er", "Kopf", "in", "al\u00b7len", "Le\u00b7bens\u00b7la\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbfreund,\u00ab sprach ein Fuchs und d\u00e4mpfte sein Organ,", "tokens": ["\u00bb", "freund", ",", "\u00ab", "sprach", "ein", "Fuchs", "und", "d\u00e4mpf\u00b7te", "sein", "Or\u00b7gan", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "$(", "VVFIN", "ART", "NE", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+--+-+-++-", "measure": "iambic.penta.invert"}, "line.4": {"text": "\u00bbwir wollen uns von heute an vertragen,", "tokens": ["\u00bb", "wir", "wol\u00b7len", "uns", "von", "heu\u00b7te", "an", "ver\u00b7tra\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "PPER", "APPR", "ADV", "APPR", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein allgemeiner Friede ist befohlen.", "tokens": ["Ein", "all\u00b7ge\u00b7mei\u00b7ner", "Frie\u00b7de", "ist", "be\u00b7foh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ich komme, dir\u2019s zu k\u00fcnden; steige nieder", "tokens": ["Ich", "kom\u00b7me", ",", "dir's", "zu", "k\u00fcn\u00b7den", ";", "stei\u00b7ge", "nie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PIS", "PTKZU", "VVINF", "$.", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und k\u00fcsse mich, von nun an sind wir Br\u00fcder.", "tokens": ["Und", "k\u00fcs\u00b7se", "mich", ",", "von", "nun", "an", "sind", "wir", "Br\u00fc\u00b7der", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "APPR", "ADV", "APZR", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Gleich mu\u00df ich weiter auf beschwingten Sohlen,", "tokens": ["Gleich", "mu\u00df", "ich", "wei\u00b7ter", "auf", "be\u00b7schwing\u00b7ten", "Soh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Noch zwanzig andre Leute aufzusuchen.", "tokens": ["Noch", "zwan\u00b7zig", "and\u00b7re", "Leu\u00b7te", "auf\u00b7zu\u00b7su\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "ADJA", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Komm nur herab, dort unter jenen Buchen", "tokens": ["Komm", "nur", "her\u00b7ab", ",", "dort", "un\u00b7ter", "je\u00b7nen", "Bu\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "$,", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Kannst du mit deinen Kindern K\u00e4fer picken.", "tokens": ["Kannst", "du", "mit", "dei\u00b7nen", "Kin\u00b7dern", "K\u00e4\u00b7fer", "pi\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Komm schnell, da\u00df wir uns in die Augen blicken", "tokens": ["Komm", "schnell", ",", "da\u00df", "wir", "uns", "in", "die", "Au\u00b7gen", "bli\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "$,", "KOUS", "PPER", "PRF", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Und herzlich k\u00fcssen, weil nun Friede ist.\u00ab", "tokens": ["Und", "herz\u00b7lich", "k\u00fcs\u00b7sen", ",", "weil", "nun", "Frie\u00b7de", "ist", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "VVINF", "$,", "KOUS", "ADV", "NN", "VAFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "\u00bbfreund,\u00ab sprach der Hahn, \u00bbes h\u00e4tte nie", "tokens": ["\u00bb", "freund", ",", "\u00ab", "sprach", "der", "Hahn", ",", "\u00bb", "es", "h\u00e4t\u00b7te", "nie"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["$(", "ADJD", "$,", "$(", "VVFIN", "ART", "NN", "$,", "$(", "PPER", "VAFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Mir s\u00fc\u00dfre Botschaft werden k\u00f6nnen", "tokens": ["Mir", "s\u00fc\u00df\u00b7re", "Bot\u00b7schaft", "wer\u00b7den", "k\u00f6n\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "VAINF", "VMFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Als diese Friedensmelodie.", "tokens": ["Als", "die\u00b7se", "Frie\u00b7dens\u00b7me\u00b7lo\u00b7die", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.17": {"text": "Wie sch\u00f6n, da\u00df du ihr \u00dcberbringer bist!", "tokens": ["Wie", "sch\u00f6n", ",", "da\u00df", "du", "ihr", "\u00dc\u00b7berb\u00b7rin\u00b7ger", "bist", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Dort seh ich noch zwei Hunde rennen,", "tokens": ["Dort", "seh", "ich", "noch", "zwei", "Hun\u00b7de", "ren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Vermutlich w\u00e4hlte man die schnellen Tiere", "tokens": ["Ver\u00b7mut\u00b7lich", "w\u00e4hl\u00b7te", "man", "die", "schnel\u00b7len", "Tie\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PIS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Zur Friedensbotschaft als Kuriere.", "tokens": ["Zur", "Frie\u00b7dens\u00b7bot\u00b7schaft", "als", "Ku\u00b7rie\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KOUS", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Sie fliegen fast, gleich sind sie hier;", "tokens": ["Sie", "flie\u00b7gen", "fast", ",", "gleich", "sind", "sie", "hier", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ADV", "VAFIN", "PPER", "ADV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.22": {"text": "Dann k\u00fcssen wir uns alle vier!\u00ab", "tokens": ["Dann", "k\u00fcs\u00b7sen", "wir", "uns", "al\u00b7le", "vier", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PIAT", "CARD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "\u00bblebwohl,\u00ab rief da der Fuchs, \u00bbmein Weg ist weit;", "tokens": ["\u00bb", "leb\u00b7wohl", ",", "\u00ab", "rief", "da", "der", "Fuchs", ",", "\u00bb", "mein", "Weg", "ist", "weit", ";"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "$(", "VVFIN", "ADV", "ART", "NE", "$,", "$(", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Wir wollen zu gelegenerer Zeit", "tokens": ["Wir", "wol\u00b7len", "zu", "ge\u00b7le\u00b7ge\u00b7ne\u00b7rer", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.25": {"text": "Die Freude feiern.\u00ab Und der Bursche nahm", "tokens": ["Die", "Freu\u00b7de", "fei\u00b7ern", ".", "\u00ab", "Und", "der", "Bur\u00b7sche", "nahm"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "$.", "$(", "KON", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Rei\u00dfaus so schnell, man glaubt es kaum,", "tokens": ["Rei\u00df\u00b7aus", "so", "schnell", ",", "man", "glaubt", "es", "kaum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "$,", "PIS", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Betr\u00fcbt, da\u00df er um seine Beute kam.", "tokens": ["Be\u00b7tr\u00fcbt", ",", "da\u00df", "er", "um", "sei\u00b7ne", "Beu\u00b7te", "kam", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Der alte Hahn sa\u00df lange noch im Baum", "tokens": ["Der", "al\u00b7te", "Hahn", "sa\u00df", "lan\u00b7ge", "noch", "im", "Baum"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Und bog sich kr\u00e4hend vor Vergn\u00fcgen.", "tokens": ["Und", "bog", "sich", "kr\u00e4\u00b7hend", "vor", "Ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Welch doppelter Genu\u00df, Betr\u00fcger zu betr\u00fcgen!", "tokens": ["Welch", "dop\u00b7pel\u00b7ter", "Ge\u00b7nu\u00df", ",", "Be\u00b7tr\u00fc\u00b7ger", "zu", "be\u00b7tr\u00fc\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}