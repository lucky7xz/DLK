{"dta.poem.12587": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Erster Theil.  \n Von den Tisch-Liedern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905199128", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dieser, wie er auch nun heisse, welcher dieses Lied erfand,", "tokens": ["Die\u00b7ser", ",", "wie", "er", "auch", "nun", "heis\u00b7se", ",", "wel\u00b7cher", "die\u00b7ses", "Lied", "er\u00b7fand", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWAV", "PPER", "ADV", "ADV", "VVFIN", "$,", "PRELS", "PDAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Hat mit Recht die erste Stelle der Gesundheit zuerkannt.", "tokens": ["Hat", "mit", "Recht", "die", "ers\u00b7te", "Stel\u00b7le", "der", "Ge\u00b7sund\u00b7heit", "zu\u00b7er\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Aber, Sch\u00f6nheit! dir die andre, Reichthum! dir die dritte weih\u2019n,", "tokens": ["A\u00b7ber", ",", "Sch\u00f6n\u00b7heit", "!", "dir", "die", "and\u00b7re", ",", "Reicht\u00b7hum", "!", "dir", "die", "drit\u00b7te", "weih'n", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$.", "PPER", "ART", "ADJA", "$,", "NN", "$.", "PPER", "ART", "ADJA", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Warlich, eine solche Theilung scheint mir ungereimt zu seyn.", "tokens": ["War\u00b7lich", ",", "ei\u00b7ne", "sol\u00b7che", "Thei\u00b7lung", "scheint", "mir", "un\u00b7ge\u00b7reimt", "zu", "seyn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ART", "PIAT", "NN", "VVFIN", "PPER", "ADJD", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Nein! den Platz nach der Gesundheit, G\u00fcter! den verdienet ihr.", "tokens": ["Nein", "!", "den", "Platz", "nach", "der", "Ge\u00b7sund\u00b7heit", ",", "G\u00fc\u00b7ter", "!", "den", "ver\u00b7die\u00b7net", "ihr", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ART", "NN", "APPR", "ART", "NN", "$,", "NN", "$.", "ART", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Eine Sch\u00f6nheit, welche hungert, ist ein l\u00e4cherliches Thier.", "tokens": ["Ei\u00b7ne", "Sch\u00f6n\u00b7heit", ",", "wel\u00b7che", "hun\u00b7gert", ",", "ist", "ein", "l\u00e4\u00b7cher\u00b7li\u00b7ches", "Thier", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "$,", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}