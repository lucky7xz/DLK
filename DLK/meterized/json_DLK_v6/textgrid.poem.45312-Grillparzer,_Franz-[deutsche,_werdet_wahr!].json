{"textgrid.poem.45312": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[deutsche, werdet wahr!]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Deutsche, werdet wahr!", "tokens": ["Deut\u00b7sche", ",", "wer\u00b7det", "wahr", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJA", "$,", "VAFIN", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Ihr seids vielleicht gegen andre,", "tokens": ["Ihr", "seids", "viel\u00b7leicht", "ge\u00b7gen", "and\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PIS", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Doch nicht gegen euch selbst.", "tokens": ["Doch", "nicht", "ge\u00b7gen", "euch", "selbst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "PPER", "ADV", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Die L\u00fcge gegen andre ist S\u00fcnde,", "tokens": ["Die", "L\u00fc\u00b7ge", "ge\u00b7gen", "and\u00b7re", "ist", "S\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIS", "VAFIN", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Die L\u00fcge gegen sich Verkehrtheit,", "tokens": ["Die", "L\u00fc\u00b7ge", "ge\u00b7gen", "sich", "Ver\u00b7kehrt\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PRF", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Trotz Wissen und trotz Gelehrtheit.", "tokens": ["Trotz", "Wis\u00b7sen", "und", "trotz", "Ge\u00b7lehr\u00b7theit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}