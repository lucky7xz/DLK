{"dta.poem.865": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "LiI.  Auff den Sontag de\u00df vor vns sorgenden  \n Sch\u00f6pffers/ oder  XV.  Sontag nach dem Fest  \n der H. Dreyeinigkeit. Matth. 6.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.85", "cy:0.14"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Weg welt! wegtrawrig seyn/ weg Teufel/ Fleisch vnd ", "tokens": ["Weg", "welt", "!", "weg\u00b7traw\u00b7rig", "seyn", "/", "weg", "Teu\u00b7fel", "/", "Fleisch", "vnd"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "$.", "ADJD", "VAINF", "$(", "ADV", "NN", "$(", "NN", "KON"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "zagen/ ", "tokens": ["za\u00b7gen", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Weg Eitelkeit vnd Furcht/ weg was mich offt so krenckt!", "tokens": ["Weg", "Ei\u00b7tel\u00b7keit", "vnd", "Furcht", "/", "weg", "was", "mich", "offt", "so", "krenckt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "$(", "ADV", "PWS", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Mein Vater/ der auff Gras vnd wilde Thiere denckt", "tokens": ["Mein", "Va\u00b7ter", "/", "der", "auff", "Gras", "vnd", "wil\u00b7de", "Thie\u00b7re", "denckt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "ART", "APPR", "NN", "KON", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der wird mir was ich darff zu keiner zeit versagen.", "tokens": ["Der", "wird", "mir", "was", "ich", "darff", "zu", "kei\u00b7ner", "zeit", "ver\u00b7sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PWS", "PPER", "VMFIN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der vor die V\u00f6gel sorgt/ wird ja mehr kummer tragen", "tokens": ["Der", "vor", "die", "V\u00f6\u00b7gel", "sorgt", "/", "wird", "ja", "mehr", "kum\u00b7mer", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$(", "VAFIN", "ADV", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vor mich sein Ebenbild/ der mir di\u00df Leben schenckt", "tokens": ["Vor", "mich", "sein", "E\u00b7ben\u00b7bild", "/", "der", "mir", "di\u00df", "Le\u00b7ben", "schenckt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "PPOSAT", "NN", "$(", "PRELS", "PPER", "PDS", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wird/ was zu leben noth/ vnd der mich speis\u2019t vnd tr\u00e4nckt", "tokens": ["Wird", "/", "was", "zu", "le\u00b7ben", "noth", "/", "vnd", "der", "mich", "speis't", "vnd", "tr\u00e4nckt"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "$(", "PWS", "PTKZU", "VVINF", "NN", "$(", "KON", "ART", "PPER", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wird meiner Bl\u00f6\u00df ein Kleidt ja nimmermehr abschlagen.", "tokens": ["Wird", "mei\u00b7ner", "Bl\u00f6\u00df", "ein", "Kleidt", "ja", "nim\u00b7mer\u00b7mehr", "ab\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ART", "NN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Drumb weg! was jrrdisch ist! wer stets nach Erden tracht", "tokens": ["Drumb", "weg", "!", "was", "jrr\u00b7disch", "ist", "!", "wer", "stets", "nach", "Er\u00b7den", "tracht"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "PTKVZ", "$.", "PWS", "PPER", "VAFIN", "$.", "PWS", "ADV", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd nur den Mammon ehrt/ mag forthin Tag vnd Nacht", "tokens": ["Vnd", "nur", "den", "Mam\u00b7mon", "ehrt", "/", "mag", "for\u00b7thin", "Tag", "vnd", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "$(", "VMFIN", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Sich k\u00fcmmern wie er m\u00f6g ein eehl\u2019 jhm selbst zusetzen.", "tokens": ["Sich", "k\u00fcm\u00b7mern", "wie", "er", "m\u00f6g", "ein", "e\u00b7ehl'", "jhm", "selbst", "zu\u00b7set\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "PWAV", "PPER", "VMFIN", "ART", "NN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "La\u00dft Erd vnd Welt vergehn! wenn mir der Him\u0303el bleibt: ", "tokens": ["La\u00dft", "Erd", "vnd", "Welt", "ver\u00b7gehn", "!", "wenn", "mir", "der", "Him\u0303el", "bleibt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "KON", "NN", "VVINF", "$.", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Schlo\u00df der Ewigkeit! das ", "tokens": ["Das", "Schlo\u00df", "der", "E\u00b7wig\u00b7keit", "!", "das"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "ART", "NN", "$.", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "schreibt:", "tokens": ["schreibt", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.4": {"text": "So bin ich ewig-reich! vnd ewig-grog zu sch\u00e4tzen. ", "tokens": ["So", "bin", "ich", "e\u00b7wig\u00b7reich", "!", "vnd", "e\u00b7wig\u00b7grog", "zu", "sch\u00e4t\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$.", "KON", "NE", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}