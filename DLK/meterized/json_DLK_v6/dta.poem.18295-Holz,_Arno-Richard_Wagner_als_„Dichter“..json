{"dta.poem.18295": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Richard Wagner als \u201eDichter\u201c.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Das unser Jahrhundert geboren;", "tokens": ["Das", "un\u00b7ser", "Jahr\u00b7hun\u00b7dert", "ge\u00b7bo\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schon beim Anh\u00f6ren seiner Hotth\u00fcpoesie", "tokens": ["Schon", "beim", "An\u00b7h\u00f6\u00b7ren", "sei\u00b7ner", "Hott\u00b7h\u00fc\u00b7po\u00b7e\u00b7sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Verl\u00e4ngern sich unsre Ohren!", "tokens": ["Ver\u00b7l\u00e4n\u00b7gern", "sich", "uns\u00b7re", "Oh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Der deutschen Sprache spie dreist ins Gesicht", "tokens": ["Der", "deut\u00b7schen", "Spra\u00b7che", "spie", "dreist", "ins", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Seines Stabreims Eiapopeia \u2014", "tokens": ["Sei\u00b7nes", "Stab\u00b7reims", "Ei\u00b7a\u00b7po\u00b7pei\u00b7a"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ein demokratischer Krebs, der Verse verbricht:", "tokens": ["Ein", "de\u00b7mo\u00b7kra\u00b7ti\u00b7scher", "Krebs", ",", "der", "Ver\u00b7se", "ver\u00b7bricht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "VVPP", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.4": {"text": "Wigala Wagala Weia!", "tokens": ["Wi\u00b7ga\u00b7la", "Wa\u00b7ga\u00b7la", "Wei\u00b7a", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}