{"textgrid.poem.59320": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Kranke", "genre": "verse", "period": "N.A.", "pub_year": 1820, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "V\u00f6gelein munter", "tokens": ["V\u00f6\u00b7ge\u00b7lein", "mun\u00b7ter"], "token_info": ["word", "word"], "pos": ["NN", "ADJD"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Singen so sch\u00f6n,", "tokens": ["Sin\u00b7gen", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "La\u00dft mich hinunter", "tokens": ["La\u00dft", "mich", "hin\u00b7un\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "PPER", "PTKVZ"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Spazierengehn!", "tokens": ["Spa\u00b7zie\u00b7ren\u00b7gehn", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "\u00bbnacht ist's ja drau\u00dfen;", "tokens": ["\u00bb", "nacht", "ist's", "ja", "drau\u00b7\u00dfen", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVPP", "VAFIN", "ADV", "ADV", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "'s war nur der Sturm,", "tokens": ["'s", "war", "nur", "der", "Sturm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Den du h\u00f6rst sausen", "tokens": ["Den", "du", "h\u00f6rst", "sau\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Droben vom Turm.\u00ab", "tokens": ["Dro\u00b7ben", "vom", "Turm", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPRART", "NN", "$.", "$("], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Liebchen im Garten", "tokens": ["Lieb\u00b7chen", "im", "Gar\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Seh ich dort stehn,", "tokens": ["Seh", "ich", "dort", "stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Lang mu\u00dft sie warten,", "tokens": ["Lang", "mu\u00dft", "sie", "war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "O la\u00dft mich gehn.", "tokens": ["O", "la\u00dft", "mich", "gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "\u00bbstill nur, der blasse", "tokens": ["\u00bb", "still", "nur", ",", "der", "blas\u00b7se"], "token_info": ["punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "ADJD", "ADV", "$,", "ART", "ADJA"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Tod ist's, der sacht", "tokens": ["Tod", "ist's", ",", "der", "sacht"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VAFIN", "$,", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Dort durch die Gasse", "tokens": ["Dort", "durch", "die", "Gas\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Schleicht in der Nacht.\u00ab", "tokens": ["Schleicht", "in", "der", "Nacht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$.", "$("], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Wie mir ergraute,", "tokens": ["Wie", "mir", "er\u00b7grau\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Bleiches Gesicht!", "tokens": ["Blei\u00b7ches", "Ge\u00b7sicht", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Gebt mir die Laute,", "tokens": ["Gebt", "mir", "die", "Lau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Mir wird so licht!", "tokens": ["Mir", "wird", "so", "licht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "\u00bbwas willst du singen", "tokens": ["\u00bb", "was", "willst", "du", "sin\u00b7gen"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "PWS", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "In tiefster Not?", "tokens": ["In", "tiefs\u00b7ter", "Not", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Lenz, Lust vergingen,", "tokens": ["Lenz", ",", "Lust", "ver\u00b7gin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VVFIN", "$,"], "meter": "++-+-", "measure": "iambic.di"}, "line.4": {"text": "Liebchen ist tot!\u00ab \u2013", "tokens": ["Lieb\u00b7chen", "ist", "tot", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["NE", "VAFIN", "ADJD", "$.", "$(", "$("], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.7": {"line.1": {"text": "La\u00dft mich, Gespenster,", "tokens": ["La\u00dft", "mich", ",", "Ge\u00b7spens\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Lied, riegl auf die Gruft!", "tokens": ["Lied", ",", "riegl", "auf", "die", "Gruft", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "\u00d6ffnet die Fenster,", "tokens": ["\u00d6ff\u00b7net", "die", "Fens\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Luft, frische freie Luft!", "tokens": ["Luft", ",", "fri\u00b7sche", "frei\u00b7e", "Luft", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}