{"textgrid.poem.57252": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Liebe Schmerzen, nicht der erwartenden", "genre": "verse", "period": "N.A.", "pub_year": 1753, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Liebe Schmerzen, nicht der erwartenden", "tokens": ["Der", "Lie\u00b7be", "Schmer\u00b7zen", ",", "nicht", "der", "er\u00b7war\u00b7ten\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "PTKNEG", "ART", "ADJA"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Noch ungeliebten, die Schmerzen nicht,", "tokens": ["Noch", "un\u00b7ge\u00b7lieb\u00b7ten", ",", "die", "Schmer\u00b7zen", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "$,", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Denn ich liebe, so liebte", "tokens": ["Denn", "ich", "lie\u00b7be", ",", "so", "lieb\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "ADV", "VVFIN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Keiner! so werd ich geliebt!", "tokens": ["Kei\u00b7ner", "!", "so", "werd", "ich", "ge\u00b7liebt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$.", "ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Die sanftern Schmerzen, welche zum Wiedersehn", "tokens": ["Die", "sanf\u00b7tern", "Schmer\u00b7zen", ",", "wel\u00b7che", "zum", "Wie\u00b7der\u00b7sehn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Hinblicken, welche zum Wiedersehn", "tokens": ["Hin\u00b7bli\u00b7cken", ",", "wel\u00b7che", "zum", "Wie\u00b7der\u00b7sehn"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Tief aufathmen, doch lispelt", "tokens": ["Tief", "auf\u00b7ath\u00b7men", ",", "doch", "lis\u00b7pelt"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVPP", "$,", "ADV", "VVFIN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Stammelnde Freude mit auf!", "tokens": ["Stam\u00b7meln\u00b7de", "Freu\u00b7de", "mit", "auf", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Die Schmerzen wollt ich singen. Ich h\u00f6rte schon", "tokens": ["Die", "Schmer\u00b7zen", "wollt", "ich", "sin\u00b7gen", ".", "Ich", "h\u00f6r\u00b7te", "schon"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$.", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des Abschieds Thr\u00e4nen am Rosenbusch", "tokens": ["Des", "Ab\u00b7schieds", "Thr\u00e4\u00b7nen", "am", "Ro\u00b7sen\u00b7busch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "APPRART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Weinen! weinen der Thr\u00e4nen", "tokens": ["Wei\u00b7nen", "!", "wei\u00b7nen", "der", "Thr\u00e4\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "VVINF", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Stimme die Saiten herab!", "tokens": ["Stim\u00b7me", "die", "Sai\u00b7ten", "her\u00b7ab", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Doch schnell verbot ich meinem zu leisen Ohr", "tokens": ["Doch", "schnell", "ver\u00b7bot", "ich", "mei\u00b7nem", "zu", "lei\u00b7sen", "Ohr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PPOSAT", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Zur\u00fcck zu horchen! die Z\u00e4hre schwieg,", "tokens": ["Zu\u00b7r\u00fcck", "zu", "hor\u00b7chen", "!", "die", "Z\u00e4h\u00b7re", "schwieg", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$.", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und schon waren die Saiten", "tokens": ["Und", "schon", "wa\u00b7ren", "die", "Sai\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Klage zu singen verstumt!", "tokens": ["Kla\u00b7ge", "zu", "sin\u00b7gen", "ver\u00b7stumt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "VVPP", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.5": {"line.1": {"text": "Denn ach, ich sah dich! trank die Vergessenheit", "tokens": ["Denn", "ach", ",", "ich", "sah", "dich", "!", "trank", "die", "Ver\u00b7ges\u00b7sen\u00b7heit"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "XY", "$,", "PPER", "VVFIN", "PPER", "$.", "VVFIN", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der s\u00fcssen T\u00e4uschung mit feurigem", "tokens": ["Der", "s\u00fcs\u00b7sen", "T\u00e4u\u00b7schung", "mit", "feu\u00b7ri\u00b7gem"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Durste! Cidli, ich sahe", "tokens": ["Durs\u00b7te", "!", "Cid\u00b7li", ",", "ich", "sa\u00b7he"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$.", "XY", "$,", "PPER", "VVFIN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Dich, du Geliebte! dich Selbst!", "tokens": ["Dich", ",", "du", "Ge\u00b7lieb\u00b7te", "!", "dich", "Selbst", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "NN", "$.", "PPER", "ADV", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.6": {"line.1": {"text": "Wie standst du vor mir, Cidli, wie hing mein Herz", "tokens": ["Wie", "standst", "du", "vor", "mir", ",", "Cid\u00b7li", ",", "wie", "hing", "mein", "Herz"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "APPR", "PPER", "$,", "XY", "$,", "PWAV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "An deinem Herzen, Geliebtere,", "tokens": ["An", "dei\u00b7nem", "Her\u00b7zen", ",", "Ge\u00b7lieb\u00b7te\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Als die Liebenden lieben!", "tokens": ["Als", "die", "Lie\u00b7ben\u00b7den", "lie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "O die ich suchet', und fand!", "tokens": ["O", "die", "ich", "su\u00b7chet'", ",", "und", "fand", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PRELS", "PPER", "VVFIN", "$,", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}