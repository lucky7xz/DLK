{"dta.poem.12965": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Das sechste capitel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So bald der schwache mensch dem hitzigen verlangen", "tokens": ["So", "bald", "der", "schwa\u00b7che", "mensch", "dem", "hit\u00b7zi\u00b7gen", "ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "ART", "ADJA", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des blinden fleisches folgt, so geht die unruh an.", "tokens": ["Des", "blin\u00b7den", "flei\u00b7sches", "folgt", ",", "so", "geht", "die", "un\u00b7ruh", "an", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "VVFIN", "$,", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn hoffart, lust und geitz sind drey vergiffte schlangen,", "tokens": ["Denn", "hof\u00b7fart", ",", "lust", "und", "geitz", "sind", "drey", "ver\u00b7giff\u00b7te", "schlan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "KON", "ADJD", "VAFIN", "CARD", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vor denen, wo sie sind, kein friede wohnen kan.", "tokens": ["Vor", "de\u00b7nen", ",", "wo", "sie", "sind", ",", "kein", "frie\u00b7de", "woh\u00b7nen", "kan", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "$,", "PWAV", "PPER", "VAFIN", "$,", "PIAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wer aber arm am geist und niedrig am gem\u00fcthe,", "tokens": ["Wer", "a\u00b7ber", "arm", "am", "geist", "und", "nied\u00b7rig", "am", "ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "APPRART", "NN", "KON", "ADJD", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nach Christi willen ist, der kommt zur wahren ruh.", "tokens": ["Nach", "Chris\u00b7ti", "wil\u00b7len", "ist", ",", "der", "kommt", "zur", "wah\u00b7ren", "ruh", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VAFIN", "$,", "PRELS", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Denn solchen d\u00fcrfftigen wirfft GOttes reiche g\u00fcte", "tokens": ["Denn", "sol\u00b7chen", "d\u00fcr\u00b7ff\u00b7ti\u00b7gen", "wirfft", "Got\u00b7tes", "rei\u00b7che", "g\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN", "NN", "ADJA", "NN"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Den unverg\u00e4nglichen und sichern frieden zu.", "tokens": ["Den", "un\u00b7ver\u00b7g\u00e4ng\u00b7li\u00b7chen", "und", "si\u00b7chern", "frie\u00b7den", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ein mensch, der sich noch selbst nicht v\u00f6llig \u00fcberwunden,", "tokens": ["Ein", "mensch", ",", "der", "sich", "noch", "selbst", "nicht", "v\u00f6l\u00b7lig", "\u00fc\u00b7berw\u00b7un\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "ADV", "ADV", "PTKNEG", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Thut den versuchungen gar schlechten widerstand;", "tokens": ["Thut", "den", "ver\u00b7su\u00b7chun\u00b7gen", "gar", "schlech\u00b7ten", "wi\u00b7der\u00b7stand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "ADV", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wenn der begierden macht nur mit ihm angebunden,", "tokens": ["Wenn", "der", "be\u00b7gier\u00b7den", "macht", "nur", "mit", "ihm", "an\u00b7ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "VVFIN", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So haben sie ihn auch schon wieder in der hand.", "tokens": ["So", "ha\u00b7ben", "sie", "ihn", "auch", "schon", "wie\u00b7der", "in", "der", "hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADV", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Denn wer dem geiste nach nicht satsam zugensmmen,", "tokens": ["Denn", "wer", "dem", "geis\u00b7te", "nach", "nicht", "sat\u00b7sam", "zu\u00b7gensm\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "ADJA", "APPR", "PTKNEG", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Und mit dem hertzen noch sehr an dem fleische hengt,", "tokens": ["Und", "mit", "dem", "hert\u00b7zen", "noch", "sehr", "an", "dem", "flei\u00b7sche", "hengt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "ADV", "APPR", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Dem f\u00e4llt es allzuschwer den l\u00fcsten zu entkommen,", "tokens": ["Dem", "f\u00e4llt", "es", "all\u00b7zu\u00b7schwer", "den", "l\u00fcs\u00b7ten", "zu", "ent\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "ART", "ADJA", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "In deren abgrund er mit vollem z\u00fcgel sprengt.", "tokens": ["In", "de\u00b7ren", "ab\u00b7grund", "er", "mit", "vol\u00b7lem", "z\u00fc\u00b7gel", "sprengt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Gesetzt, er lasse sich das wort der gnade halten,", "tokens": ["Ge\u00b7setzt", ",", "er", "las\u00b7se", "sich", "das", "wort", "der", "gna\u00b7de", "hal\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PPER", "VVFIN", "PRF", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "So wird er doch betr\u00fcbt, da\u00df er gehorchen soll.", "tokens": ["So", "wird", "er", "doch", "be\u00b7tr\u00fcbt", ",", "da\u00df", "er", "ge\u00b7hor\u00b7chen", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ein lauer eifer kan im augenblick erkalten;", "tokens": ["Ein", "lau\u00b7er", "ei\u00b7fer", "kan", "im", "au\u00b7gen\u00b7blick", "er\u00b7kal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Drum wache, k\u00e4mpff und bet\u2019, und \u00fcberwinde wohl.", "tokens": ["Drum", "wa\u00b7che", ",", "k\u00e4mpff", "und", "bet'", ",", "und", "\u00fc\u00b7berw\u00b7in\u00b7de", "wohl", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "ADJA", "$,", "VVFIN", "KON", "VVFIN", "$,", "KON", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wilst du dich aber nicht in GOttes ordnung schicken,", "tokens": ["Wilst", "du", "dich", "a\u00b7ber", "nicht", "in", "Got\u00b7tes", "ord\u00b7nung", "schi\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "PTKNEG", "APPR", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und z\u00f6rnest \u00fcber dem, das dir im wege steht,", "tokens": ["Und", "z\u00f6r\u00b7nest", "\u00fc\u00b7ber", "dem", ",", "das", "dir", "im", "we\u00b7ge", "steht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Schl\u00e4gst du des Geistes joch und sauffte band\u2019 in st\u00fccken,", "tokens": ["Schl\u00e4gst", "du", "des", "Geis\u00b7tes", "joch", "und", "sauff\u00b7te", "band'", "in", "st\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "KON", "VVFIN", "ADJD", "APPR", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und rennst, wohin dein wunsch und eigner wille geht;", "tokens": ["Und", "rennst", ",", "wo\u00b7hin", "dein", "wunsch", "und", "eig\u00b7ner", "wil\u00b7le", "geht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PPOSAT", "ADJD", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "So wirst du bald darauf in dem gewissen f\u00fchlen,", "tokens": ["So", "wirst", "du", "bald", "da\u00b7rauf", "in", "dem", "ge\u00b7wis\u00b7sen", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PAV", "APPR", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Da\u00df eigensinn und lust die sch\u00e4rffsten hencker sind,", "tokens": ["Da\u00df", "ei\u00b7gen\u00b7sinn", "und", "lust", "die", "sch\u00e4rffs\u00b7ten", "hen\u00b7cker", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "VVFIN", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Die \u00e4rger als ein b\u00e4r in marck und adern w\u00fchlen,", "tokens": ["Die", "\u00e4r\u00b7ger", "als", "ein", "b\u00e4r", "in", "marck", "und", "a\u00b7dern", "w\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KOKOM", "ART", "NN", "APPR", "NN", "KON", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "So bald uns ihre list und schmeicheley gewinnt.", "tokens": ["So", "bald", "uns", "ih\u00b7re", "list", "und", "schmei\u00b7che\u00b7ley", "ge\u00b7winnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PPOSAT", "NN", "KON", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Drum bleibt es ausgemacht, da\u00df der, so seinem willen", "tokens": ["Drum", "bleibt", "es", "aus\u00b7ge\u00b7macht", ",", "da\u00df", "der", ",", "so", "sei\u00b7nem", "wil\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "VVPP", "$,", "KOUS", "ART", "$,", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und den begierden folgt, den frieden niemals kriegt.", "tokens": ["Und", "den", "be\u00b7gier\u00b7den", "folgt", ",", "den", "frie\u00b7den", "nie\u00b7mals", "kriegt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "VVFIN", "$,", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Den aber wird der HErr gewi\u00df mit fried erf\u00fcllen,", "tokens": ["Den", "a\u00b7ber", "wird", "der", "Herr", "ge\u00b7wi\u00df", "mit", "fried", "er\u00b7f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VAFIN", "ART", "NN", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Wer sich der welt entrei\u00dft, und seine lust besiegt.", "tokens": ["Wer", "sich", "der", "welt", "ent\u00b7rei\u00dft", ",", "und", "sei\u00b7ne", "lust", "be\u00b7siegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ART", "NN", "VVFIN", "$,", "KON", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Flieht nun die wahre ruh vor irrdischen gem\u00fcthern,", "tokens": ["Flieht", "nun", "die", "wah\u00b7re", "ruh", "vor", "irr\u00b7di\u00b7schen", "ge\u00b7m\u00fc\u00b7thern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "So la\u00df dem Geiste doch, o seele! freye macht,", "tokens": ["So", "la\u00df", "dem", "Geis\u00b7te", "doch", ",", "o", "see\u00b7le", "!", "frey\u00b7e", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ART", "NN", "ADV", "$,", "FM", "FM", "$.", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Und gieb dem eigensinn des fleisches, und den g\u00fctern", "tokens": ["Und", "gieb", "dem", "ei\u00b7gen\u00b7sinn", "des", "flei\u00b7sches", ",", "und", "den", "g\u00fc\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVIMP", "ART", "NN", "ART", "ADJA", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Der unruh-vollen welt im glauben gute nacht.", "tokens": ["Der", "un\u00b7ruh\u00b7vol\u00b7len", "welt", "im", "glau\u00b7ben", "gu\u00b7te", "nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}