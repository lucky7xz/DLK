{"textgrid.poem.59690": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Allein", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin allein, in weiter Welt allein,", "tokens": ["Ich", "bin", "al\u00b7lein", ",", "in", "wei\u00b7ter", "Welt", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "APPR", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "All meine Sterne schlossen ihren Himmel,", "tokens": ["All", "mei\u00b7ne", "Ster\u00b7ne", "schlos\u00b7sen", "ih\u00b7ren", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im dichten Menschenstrudel ganz allein,", "tokens": ["Im", "dich\u00b7ten", "Men\u00b7schen\u00b7stru\u00b7del", "ganz", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Allein im bunten, wilden Erdgewimmel \u2013", "tokens": ["Al\u00b7lein", "im", "bun\u00b7ten", ",", "wil\u00b7den", "Erd\u00b7ge\u00b7wim\u00b7mel", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "$,", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Zum Ozean des Nichts wie treibst du fort!", "tokens": ["Zum", "O\u00b7ze\u00b7an", "des", "Nichts", "wie", "treibst", "du", "fort", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "PIS", "KOKOM", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der meine jungen Tage einst umgl\u00e4nzte?", "tokens": ["Der", "mei\u00b7ne", "jun\u00b7gen", "Ta\u00b7ge", "einst", "um\u00b7gl\u00e4nz\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So fl\u00fcchtig, s\u00fc\u00dfe Freude, warest du,", "tokens": ["So", "fl\u00fcch\u00b7tig", ",", "s\u00fc\u00b7\u00dfe", "Freu\u00b7de", ",", "wa\u00b7rest", "du", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJA", "NN", "$,", "VAFIN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die meinen Fr\u00fchling einst mit Rosen kr\u00e4nzte?", "tokens": ["Die", "mei\u00b7nen", "Fr\u00fch\u00b7ling", "einst", "mit", "Ro\u00b7sen", "kr\u00e4nz\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Einsam der Mensch und ohne Heim und Ort?", "tokens": ["Ein\u00b7sam", "der", "Mensch", "und", "oh\u00b7ne", "Heim", "und", "Ort", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "KON", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Einsam der Mensch? Du faselst, dunkler Tor \u2013", "tokens": ["Ein\u00b7sam", "der", "Mensch", "?", "Du", "fa\u00b7selst", ",", "dunk\u00b7ler", "Tor", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$.", "PPER", "VVFIN", "$,", "ADJA", "NN", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Lockt nicht die Sonne mit den alten Strahlen?", "tokens": ["Lockt", "nicht", "die", "Son\u00b7ne", "mit", "den", "al\u00b7ten", "Strah\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Lockt nicht die Wiese mit dem Blumenflor,", "tokens": ["Lockt", "nicht", "die", "Wie\u00b7se", "mit", "dem", "Blu\u00b7men\u00b7flor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein zweites Eden vor dir hinzumalen?", "tokens": ["Ein", "zwei\u00b7tes", "E\u00b7den", "vor", "dir", "hin\u00b7zu\u00b7ma\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Spricht Gott nicht in dem Stein und Gras und Strauch,", "tokens": ["Spricht", "Gott", "nicht", "in", "dem", "Stein", "und", "Gras", "und", "Strauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PTKNEG", "APPR", "ART", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Im Sternenschimmer und im Bl\u00fctenhauch?", "tokens": ["Im", "Ster\u00b7nen\u00b7schim\u00b7mer", "und", "im", "Bl\u00fc\u00b7ten\u00b7hauch", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Spricht Gott in dir nicht? Ja, wenn Kerkernacht", "tokens": ["Spricht", "Gott", "in", "dir", "nicht", "?", "Ja", ",", "wenn", "Ker\u00b7ker\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "NN", "APPR", "PPER", "PTKNEG", "$.", "PTKANT", "$,", "KOUS", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Im Moder fern von Sonn' und Mond dich hielte,", "tokens": ["Im", "Mo\u00b7der", "fern", "von", "Sonn'", "und", "Mond", "dich", "hiel\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "APPR", "NN", "KON", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wenn des Satans sch\u00e4rfste H\u00f6llenmacht", "tokens": ["Und", "wenn", "des", "Sa\u00b7tans", "sch\u00e4rfs\u00b7te", "H\u00f6l\u00b7len\u00b7macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit allen Zweifelssch\u00fcssen auf dich zielte,", "tokens": ["Mit", "al\u00b7len", "Zwei\u00b7fels\u00b7sch\u00fcs\u00b7sen", "auf", "dich", "ziel\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wo Gott und Liebe spricht, wie k\u00f6nnt' es sein?", "tokens": ["Wo", "Gott", "und", "Lie\u00b7be", "spricht", ",", "wie", "k\u00f6nnt'", "es", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VVFIN", "$,", "PWAV", "VMFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mit Gott und Liebe bleibt kein Mensch allein.", "tokens": ["Mit", "Gott", "und", "Lie\u00b7be", "bleibt", "kein", "Mensch", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Gleich fliegen her die Myriaden Geister", "tokens": ["Gleich", "flie\u00b7gen", "her", "die", "My\u00b7ria\u00b7den", "Geis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "NN"], "meter": "-+-+----+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Und jagen alle d\u00fcstern Spuke fort", "tokens": ["Und", "ja\u00b7gen", "al\u00b7le", "d\u00fcs\u00b7tern", "Spu\u00b7ke", "fort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIS", "VVFIN", "NE", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und werden aller b\u00f6sen Tr\u00e4ume Meister,", "tokens": ["Und", "wer\u00b7den", "al\u00b7ler", "b\u00f6\u00b7sen", "Tr\u00e4u\u00b7me", "Meis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIAT", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und fr\u00f6hlich tagt's wie junger Morgenschein:", "tokens": ["Und", "fr\u00f6h\u00b7lich", "tagt's", "wie", "jun\u00b7ger", "Mor\u00b7gen\u00b7schein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "O Gott und Liebe! O du Liebesheld!", "tokens": ["O", "Gott", "und", "Lie\u00b7be", "!", "O", "du", "Lie\u00b7bes\u00b7held", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "$.", "NE", "NE", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du Stiller alles Jammers, aller Klagen!", "tokens": ["Du", "Stil\u00b7ler", "al\u00b7les", "Jam\u00b7mers", ",", "al\u00b7ler", "Kla\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "PIAT", "NN", "$,", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du Helfer und Befreier aller Welt,", "tokens": ["Du", "Hel\u00b7fer", "und", "Be\u00b7frei\u00b7er", "al\u00b7ler", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der auch f\u00fcr mich den Dornenkranz getragen \u2013", "tokens": ["Der", "auch", "f\u00fcr", "mich", "den", "Dor\u00b7nen\u00b7kranz", "ge\u00b7tra\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPER", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Bescheinst du mich, du h\u00f6chster Liebesschein,", "tokens": ["Be\u00b7scheinst", "du", "mich", ",", "du", "h\u00f6chs\u00b7ter", "Lie\u00b7bes\u00b7schein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ist alle Erde, aller Himmel mein.", "tokens": ["Ist", "al\u00b7le", "Er\u00b7de", ",", "al\u00b7ler", "Him\u00b7mel", "mein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "$,", "PIAT", "NN", "PPOSAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}