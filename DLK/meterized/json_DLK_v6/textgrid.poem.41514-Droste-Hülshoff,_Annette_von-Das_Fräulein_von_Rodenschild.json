{"textgrid.poem.41514": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Das Fr\u00e4ulein von Rodenschild", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sind denn so schw\u00fcl die N\u00e4cht' im April?", "tokens": ["Sind", "denn", "so", "schw\u00fcl", "die", "N\u00e4cht'", "im", "Ap\u00b7ril", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADJD", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Oder ist so siedend jungfr\u00e4ulich Blut?", "tokens": ["O\u00b7der", "ist", "so", "sie\u00b7dend", "jung\u00b7fr\u00e4u\u00b7lich", "Blut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "ADJD", "NN", "$."], "meter": "+-+-+-++-+", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Sie schlie\u00dft die Wimper, sie liegt so still,", "tokens": ["Sie", "schlie\u00dft", "die", "Wim\u00b7per", ",", "sie", "liegt", "so", "still", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und horcht des Herzens pochender Flut.", "tokens": ["Und", "horcht", "des", "Her\u00b7zens", "po\u00b7chen\u00b7der", "Flut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "\u00bbo will es denn nimmer und nimmer tagen!", "tokens": ["\u00bb", "o", "will", "es", "denn", "nim\u00b7mer", "und", "nim\u00b7mer", "ta\u00b7gen", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "VMFIN", "PPER", "ADV", "ADV", "KON", "ADV", "VVINF", "$."], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "O will denn nicht endlich die Stunde schlagen!", "tokens": ["O", "will", "denn", "nicht", "end\u00b7lich", "die", "Stun\u00b7de", "schla\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ADV", "PTKNEG", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Ich wache, und selbst der Seiger ruht!", "tokens": ["Ich", "wa\u00b7che", ",", "und", "selbst", "der", "Sei\u00b7ger", "ruht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Doch horch! es summt, eins, zwei und drei, \u2013", "tokens": ["Doch", "horch", "!", "es", "summt", ",", "eins", ",", "zwei", "und", "drei", ",", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVIMP", "$.", "PPER", "VVFIN", "$,", "PIS", "$,", "CARD", "KON", "CARD", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch immer fort? \u2013 sechs, sieben und acht,", "tokens": ["Noch", "im\u00b7mer", "fort", "?", "\u2013", "sechs", ",", "sie\u00b7ben", "und", "acht", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKVZ", "$.", "$(", "CARD", "$,", "CARD", "KON", "CARD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Elf, zw\u00f6lf, \u2013 o Himmel, war das ein Schrei?", "tokens": ["Elf", ",", "zw\u00f6lf", ",", "\u2013", "o", "Him\u00b7mel", ",", "war", "das", "ein", "Schrei", "?"], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "CARD", "$,", "$(", "FM", "NN", "$,", "VAFIN", "PDS", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Doch nein, Gesang steigt \u00fcber der Wacht,", "tokens": ["Doch", "nein", ",", "Ge\u00b7sang", "steigt", "\u00fc\u00b7ber", "der", "Wacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$,", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Nun wird mir's klar, mit frommem Munde", "tokens": ["Nun", "wird", "mir's", "klar", ",", "mit", "from\u00b7mem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "NE", "ADJD", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Begr\u00fc\u00dft das Hausgesinde die Stunde,", "tokens": ["Be\u00b7gr\u00fc\u00dft", "das", "Haus\u00b7ge\u00b7sin\u00b7de", "die", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Anbrach die hochheilige Osternacht.\u00ab", "tokens": ["An\u00b7brach", "die", "hoch\u00b7hei\u00b7li\u00b7ge", "Os\u00b7ter\u00b7nacht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$.", "$("], "meter": "+--++--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Seitab das Fr\u00e4ulein die Kissen st\u00f6\u00dft,", "tokens": ["Seit\u00b7ab", "das", "Fr\u00e4u\u00b7lein", "die", "Kis\u00b7sen", "st\u00f6\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und wie eine Hinde vom Lager setzt,", "tokens": ["Und", "wie", "ei\u00b7ne", "Hin\u00b7de", "vom", "La\u00b7ger", "setzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Sie hat des Mieders Schleifen gel\u00f6st,", "tokens": ["Sie", "hat", "des", "Mie\u00b7ders", "Schlei\u00b7fen", "ge\u00b7l\u00f6st", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Ins H\u00e4ubchen dr\u00e4ngt sie die Locken jetzt,", "tokens": ["Ins", "H\u00e4ub\u00b7chen", "dr\u00e4ngt", "sie", "die", "Lo\u00b7cken", "jetzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Dann leise das Fenster \u00f6ffnend, leise,", "tokens": ["Dann", "lei\u00b7se", "das", "Fens\u00b7ter", "\u00f6ff\u00b7nend", ",", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "VVPP", "$,", "ADJD", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Horcht sie der m\u00e4hlich schwellenden Weise,", "tokens": ["Horcht", "sie", "der", "m\u00e4h\u00b7lich", "schwel\u00b7len\u00b7den", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Vom wimmernden Schrei der Eule durchsetzt.", "tokens": ["Vom", "wim\u00b7mern\u00b7den", "Schrei", "der", "Eu\u00b7le", "durch\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "O dunkel die Nacht! und schaurig der Wind!", "tokens": ["O", "dun\u00b7kel", "die", "Nacht", "!", "und", "schau\u00b7rig", "der", "Wind", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ART", "NN", "$.", "KON", "ADJD", "ART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Fahnen wirbeln am knarrenden Tor, \u2013", "tokens": ["Die", "Fah\u00b7nen", "wir\u00b7beln", "am", "knar\u00b7ren\u00b7den", "Tor", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da tritt aus der Halle das Hausgesind'", "tokens": ["Da", "tritt", "aus", "der", "Hal\u00b7le", "das", "Haus\u00b7ge\u00b7sind'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Mit Blendlaternen und einzeln vor.", "tokens": ["Mit", "Blend\u00b7la\u00b7ter\u00b7nen", "und", "ein\u00b7zeln", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJD", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Der Pf\u00f6rtner dehnet sich, halb schon tr\u00e4umend,", "tokens": ["Der", "Pf\u00f6rt\u00b7ner", "deh\u00b7net", "sich", ",", "halb", "schon", "tr\u00e4u\u00b7mend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$,", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Am Dochte zupfet der J\u00e4ger s\u00e4umend,", "tokens": ["Am", "Doch\u00b7te", "zup\u00b7fet", "der", "J\u00e4\u00b7ger", "s\u00e4u\u00b7mend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und wie ein Oger g\u00e4hnet der Mohr.", "tokens": ["Und", "wie", "ein", "O\u00b7ger", "g\u00e4h\u00b7net", "der", "Mohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "Was ist? \u2013 wie das auseinanderschnellt!", "tokens": ["Was", "ist", "?", "\u2013", "wie", "das", "aus\u00b7ein\u00b7an\u00b7der\u00b7schnellt", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$.", "$(", "KOKOM", "PDS", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "In Reihen ordnen die M\u00e4nner sich,", "tokens": ["In", "Rei\u00b7hen", "ord\u00b7nen", "die", "M\u00e4n\u00b7ner", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "PRF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und eine Wacht vor die Dirnen stellt", "tokens": ["Und", "ei\u00b7ne", "Wacht", "vor", "die", "Dir\u00b7nen", "stellt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die graue Zofe sich ehrbarlich,", "tokens": ["Die", "grau\u00b7e", "Zo\u00b7fe", "sich", "ehr\u00b7bar\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "ADJD", "$,"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "\u00bbward ich gesehn an des Vorhangs L\u00fccke?", "tokens": ["\u00bb", "ward", "ich", "ge\u00b7sehn", "an", "des", "Vor\u00b7hangs", "L\u00fc\u00b7cke", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "VVPP", "APPR", "ART", "NN", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.6": {"text": "Doch nein, zum Balkone starren die Blicke,", "tokens": ["Doch", "nein", ",", "zum", "Bal\u00b7ko\u00b7ne", "star\u00b7ren", "die", "Bli\u00b7cke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$,", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Nun langsam wenden die H\u00e4upter sich.", "tokens": ["Nun", "lang\u00b7sam", "wen\u00b7den", "die", "H\u00e4up\u00b7ter", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "PRF", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "O weh meine Augen! bin ich verr\u00fcckt?", "tokens": ["O", "weh", "mei\u00b7ne", "Au\u00b7gen", "!", "bin", "ich", "ver\u00b7r\u00fcckt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "PPOSAT", "NN", "$.", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Was gleitet entlang das Treppengel\u00e4nd?", "tokens": ["Was", "glei\u00b7tet", "ent\u00b7lang", "das", "Trep\u00b7pen\u00b7ge\u00b7l\u00e4nd", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Hab' ich nicht so aus dem Spiegel geblickt?", "tokens": ["Hab'", "ich", "nicht", "so", "aus", "dem", "Spie\u00b7gel", "ge\u00b7blickt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Das sind meine Glieder, \u2013 welch ein Geblend'!", "tokens": ["Das", "sind", "mei\u00b7ne", "Glie\u00b7der", ",", "\u2013", "welch", "ein", "Ge\u00b7blend'", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$,", "$(", "PWAT", "ART", "NN", "$."], "meter": "+-+-+-++-+", "measure": "unknown.measure.hexa"}, "line.5": {"text": "Nun hebt es die H\u00e4nde, wie Zwirnes Flocken,", "tokens": ["Nun", "hebt", "es", "die", "H\u00e4n\u00b7de", ",", "wie", "Zwir\u00b7nes", "Flo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "PWAV", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Das ist mein Strich \u00fcber Stirn und Locken! \u2013", "tokens": ["Das", "ist", "mein", "Strich", "\u00fc\u00b7ber", "Stirn", "und", "Lo\u00b7cken", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Weh, bin ich toll, oder nahet mein End'!\u00ab", "tokens": ["Weh", ",", "bin", "ich", "toll", ",", "o\u00b7der", "na\u00b7het", "mein", "End'", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "ADJD", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.7": {"line.1": {"text": "Das Fr\u00e4ulein erbleicht und wieder ergl\u00fcht,", "tokens": ["Das", "Fr\u00e4u\u00b7lein", "er\u00b7bleicht", "und", "wie\u00b7der", "er\u00b7gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "KON", "ADV", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das Fr\u00e4ulein wendet die Blicke nicht,", "tokens": ["Das", "Fr\u00e4u\u00b7lein", "wen\u00b7det", "die", "Bli\u00b7cke", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und leise r\u00fchrend die Stufen zieht", "tokens": ["Und", "lei\u00b7se", "r\u00fch\u00b7rend", "die", "Stu\u00b7fen", "zieht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVPP", "ART", "NN", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Am Steingel\u00e4nde das Nebelgesicht,", "tokens": ["Am", "Stein\u00b7ge\u00b7l\u00e4n\u00b7de", "das", "Ne\u00b7bel\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "In seiner Rechten tr\u00e4gt es die Lampe,", "tokens": ["In", "sei\u00b7ner", "Rech\u00b7ten", "tr\u00e4gt", "es", "die", "Lam\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ihr Fl\u00e4mmchen zittert \u00fcber der Rampe,", "tokens": ["Ihr", "Fl\u00e4mm\u00b7chen", "zit\u00b7tert", "\u00fc\u00b7ber", "der", "Ram\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Verd\u00e4mmernd, blau, wie ein Elfenlicht.", "tokens": ["Ver\u00b7d\u00e4m\u00b7mernd", ",", "blau", ",", "wie", "ein", "El\u00b7fen\u00b7licht", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "$,", "PWAV", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Nun schwebt es unter dem Sternendom,", "tokens": ["Nun", "schwebt", "es", "un\u00b7ter", "dem", "Ster\u00b7nen\u00b7dom", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nachtwandlern gleich in Traumes Geleit,", "tokens": ["Nacht\u00b7wand\u00b7lern", "gleich", "in", "Trau\u00b7mes", "Ge\u00b7leit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "++-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Nun durch die Reihen zieht das Phantom,", "tokens": ["Nun", "durch", "die", "Rei\u00b7hen", "zieht", "das", "Phan\u00b7tom", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und jeder tritt einen Schritt zur Seit'. \u2013", "tokens": ["Und", "je\u00b7der", "tritt", "ei\u00b7nen", "Schritt", "zur", "Seit'", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "NN", "APPRART", "NN", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Nun lautlos gleitet's \u00fcber die Schwelle, \u2013", "tokens": ["Nun", "laut\u00b7los", "glei\u00b7tet's", "\u00fc\u00b7ber", "die", "Schwel\u00b7le", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "APPR", "ART", "NN", "$,", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Nun wieder drinnen erscheint die Helle,", "tokens": ["Nun", "wie\u00b7der", "drin\u00b7nen", "er\u00b7scheint", "die", "Hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Hinauf sich windend die Stiegen breit.", "tokens": ["Hin\u00b7auf", "sich", "win\u00b7dend", "die", "Stie\u00b7gen", "breit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "ADJD", "ART", "NN", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Das Fr\u00e4ulein h\u00f6rt das Gemurmel nicht,", "tokens": ["Das", "Fr\u00e4u\u00b7lein", "h\u00f6rt", "das", "Ge\u00b7mur\u00b7mel", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sieht nicht die Blicke, stier und verscheucht,", "tokens": ["Sieht", "nicht", "die", "Bli\u00b7cke", ",", "stier", "und", "ver\u00b7scheucht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["V