"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dr\u00fcckt z\u00f6gernd sie an der Spalte Rand,", "tokens": ["Dr\u00fcckt", "z\u00f6\u00b7gernd", "sie", "an", "der", "Spal\u00b7te", "Rand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Tiefdunkel drinnen \u2013 doch einem Rauschen", "tokens": ["Tief\u00b7dun\u00b7kel", "drin\u00b7nen", "\u2013", "doch", "ei\u00b7nem", "Rau\u00b7schen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "ADV", "$(", "ADV", "ART", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Der Pergamente glaubt sie zu lauschen,", "tokens": ["Der", "Per\u00b7ga\u00b7men\u00b7te", "glaubt", "sie", "zu", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und einem Streichen entlang der Wand.", "tokens": ["Und", "ei\u00b7nem", "Strei\u00b7chen", "ent\u00b7lang", "der", "Wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPO", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.12": {"line.1": {"text": "So niederk\u00e4mpfend des Herzens Schlag,", "tokens": ["So", "nie\u00b7der\u00b7k\u00e4mp\u00b7fend", "des", "Her\u00b7zens", "Schlag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "H\u00e4lt sie den Odem, sie lauscht, sie neigt \u2013", "tokens": ["H\u00e4lt", "sie", "den", "O\u00b7dem", ",", "sie", "lauscht", ",", "sie", "neigt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Was d\u00e4mmert ihr zur Seite gemach?", "tokens": ["Was", "d\u00e4m\u00b7mert", "ihr", "zur", "Sei\u00b7te", "ge\u00b7mach", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Ein Gl\u00fchwurmleuchten \u2013 es schwillt, es steigt,", "tokens": ["Ein", "Gl\u00fch\u00b7wurm\u00b7leuch\u00b7ten", "\u2013", "es", "schwillt", ",", "es", "steigt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und Arm an Arme, auf Schrittes Weite,", "tokens": ["Und", "Arm", "an", "Ar\u00b7me", ",", "auf", "Schrit\u00b7tes", "Wei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Lehnt das Gespenst an der Pforte Breite,", "tokens": ["Lehnt", "das", "Ge\u00b7spenst", "an", "der", "Pfor\u00b7te", "Brei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.7": {"text": "Gleich ihr zur Nachbarspalte gebeugt.", "tokens": ["Gleich", "ihr", "zur", "Nach\u00b7bar\u00b7spal\u00b7te", "ge\u00b7beugt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.13": {"line.1": {"text": "Sie f\u00e4hrt zur\u00fcck, \u2013 das Gebilde auch \u2013", "tokens": ["Sie", "f\u00e4hrt", "zu\u00b7r\u00fcck", ",", "\u2013", "das", "Ge\u00b7bil\u00b7de", "auch", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "$(", "ART", "NN", "ADV", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dann tritt sie n\u00e4her \u2013 so die Gestalt \u2013", "tokens": ["Dann", "tritt", "sie", "n\u00e4\u00b7her", "\u2013", "so", "die", "Ge\u00b7stalt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$(", "ADV", "ART", "NN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Nun stehen die beiden, Auge in Aug,", "tokens": ["Nun", "ste\u00b7hen", "die", "bei\u00b7den", ",", "Au\u00b7ge", "in", "Aug", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "PIAT", "$,", "NN", "APPR", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und bohren sich an mit Vampyres Gewalt.", "tokens": ["Und", "boh\u00b7ren", "sich", "an", "mit", "Vam\u00b7py\u00b7res", "Ge\u00b7walt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Das gleiche H\u00e4ubchen decket die Locken,", "tokens": ["Das", "glei\u00b7che", "H\u00e4ub\u00b7chen", "de\u00b7cket", "die", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "meas