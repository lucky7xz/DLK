{"textgrid.poem.59515": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Elegie", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bist du es, Traum der Jugend mit all deinen lieblichen Bl\u00fcten,", "tokens": ["Bist", "du", "es", ",", "Traum", "der", "Ju\u00b7gend", "mit", "all", "dei\u00b7nen", "lieb\u00b7li\u00b7chen", "Bl\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "$,", "NN", "ART", "NN", "APPR", "PIAT", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "All deiner sprossenden Lust, all deiner Hoffnungen Glanz?", "tokens": ["All", "dei\u00b7ner", "spros\u00b7sen\u00b7den", "Lust", ",", "all", "dei\u00b7ner", "Hoff\u00b7nun\u00b7gen", "Glanz", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$,", "PIAT", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "F\u00fchrst du mit Wehmut zur\u00fcck im Schleier schimmernder N\u00e4chte", "tokens": ["F\u00fchrst", "du", "mit", "Weh\u00b7mut", "zu\u00b7r\u00fcck", "im", "Schlei\u00b7er", "schim\u00b7mern\u00b7der", "N\u00e4ch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN", "PTKVZ", "APPRART", "NN", "ADJA", "NN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Holde Gestalten, die l\u00e4ngst bei den Entschlafenen ruhn?", "tokens": ["Hol\u00b7de", "Ge\u00b7stal\u00b7ten", ",", "die", "l\u00e4ngst", "bei", "den", "Ent\u00b7schla\u00b7fe\u00b7nen", "ruhn", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.5": {"text": "Ach! oft schwirret dein Laut s\u00fc\u00dft\u00f6nend in goldene Saiten.", "tokens": ["Ach", "!", "oft", "schwir\u00b7ret", "dein", "Laut", "s\u00fc\u00df\u00b7t\u00f6\u00b7nend", "in", "gol\u00b7de\u00b7ne", "Sai\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "PPOSAT", "APPR", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Mir, von Tr\u00e4nen genetzt, weigert die Laute den Klang,", "tokens": ["Mir", ",", "von", "Tr\u00e4\u00b7nen", "ge\u00b7netzt", ",", "wei\u00b7gert", "die", "Lau\u00b7te", "den", "Klang", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "NN", "VVPP", "$,", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Und es zittert die Hand, die Arme spreiten Umarmung", "tokens": ["Und", "es", "zit\u00b7tert", "die", "Hand", ",", "die", "Ar\u00b7me", "sprei\u00b7ten", "Um\u00b7ar\u00b7mung"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "NN"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Aus, doch die weichende Luft nimmt ihre Schatten zur\u00fcck.", "tokens": ["Aus", ",", "doch", "die", "wei\u00b7chen\u00b7de", "Luft", "nimmt", "ih\u00b7re", "Schat\u00b7ten", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "ADV", "ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "---+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Nein, ein gr\u00fcnendes Grab, von sp\u00e4teren Rasen gew\u00f6lbet,", "tokens": ["Nein", ",", "ein", "gr\u00fc\u00b7nen\u00b7des", "Grab", ",", "von", "sp\u00e4\u00b7te\u00b7ren", "Ra\u00b7sen", "ge\u00b7w\u00f6l\u00b7bet", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.10": {"text": "Sp\u00e4teren Tr\u00e4nen benetzt, sendet den liebenden Geist.", "tokens": ["Sp\u00e4\u00b7te\u00b7ren", "Tr\u00e4\u00b7nen", "be\u00b7netzt", ",", "sen\u00b7det", "den", "lie\u00b7ben\u00b7den", "Geist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.11": {"text": "Ja, du bist es, du Weib, das mich geboren, du k\u00fchnes,", "tokens": ["Ja", ",", "du", "bist", "es", ",", "du", "Weib", ",", "das", "mich", "ge\u00b7bo\u00b7ren", ",", "du", "k\u00fch\u00b7nes", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "PPER", "$,", "PPER", "NN", "$,", "PRELS", "PPER", "VVPP", "$,", "PPER", "ADJA", "$,"], "meter": "-+---+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Hohes und mutiges Herz, welches mich liebend umschwebt.", "tokens": ["Ho\u00b7hes", "und", "mu\u00b7ti\u00b7ges", "Herz", ",", "wel\u00b7ches", "mich", "lie\u00b7bend", "um\u00b7schwebt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "KON", "ADJA", "NN", "$,", "PRELS", "PRF", "ADJD", "VVFIN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.13": {"text": "M\u00e4chtiger f\u00fchle ich mich, zu ringen mit Schwert und mit Leier,", "tokens": ["M\u00e4ch\u00b7ti\u00b7ger", "f\u00fch\u00b7le", "ich", "mich", ",", "zu", "rin\u00b7gen", "mit", "Schwert", "und", "mit", "Lei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PRF", "$,", "PTKZU", "VVFIN", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.14": {"text": "F\u00fcr das Vaterland frisch nehm' ich den blutigen Tod,", "tokens": ["F\u00fcr", "das", "Va\u00b7ter\u00b7land", "frisch", "nehm'", "ich", "den", "blu\u00b7ti\u00b7gen", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.15": {"text": "F\u00fcr die Tugend, das M\u00e4rchen der Schn\u00f6den, m\u00e4nnlicher duldend", "tokens": ["F\u00fcr", "die", "Tu\u00b7gend", ",", "das", "M\u00e4r\u00b7chen", "der", "Schn\u00f6\u00b7den", ",", "m\u00e4nn\u00b7li\u00b7cher", "dul\u00b7dend"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "$,", "ADJA", "NN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.16": {"text": "Wandl' ich mutig den Pfad, welcher zu Himmlischen f\u00fchrt.", "tokens": ["Wandl'", "ich", "mu\u00b7tig", "den", "Pfad", ",", "wel\u00b7cher", "zu", "Himm\u00b7li\u00b7schen", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.17": {"text": "Solches wehet von dir und str\u00f6met aus heiliger N\u00e4he,", "tokens": ["Sol\u00b7ches", "we\u00b7het", "von", "dir", "und", "str\u00f6\u00b7met", "aus", "hei\u00b7li\u00b7ger", "N\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPER", "KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.18": {"text": "Was du dem Knaben oft, \u00f6fter dem J\u00fcngling gelehrt.", "tokens": ["Was", "du", "dem", "Kna\u00b7ben", "oft", ",", "\u00f6f\u00b7ter", "dem", "J\u00fcng\u00b7ling", "ge\u00b7lehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "ADV", "$,", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Herrliches Weib, wo ist dein Leben nach der Verwandlung,", "tokens": ["Herr\u00b7li\u00b7ches", "Weib", ",", "wo", "ist", "dein", "Le\u00b7ben", "nach", "der", "Ver\u00b7wand\u00b7lung", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWAV", "VAFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Welche, Tod genannt, frischeres Leben verj\u00fcngt?", "tokens": ["Wel\u00b7che", ",", "Tod", "ge\u00b7nannt", ",", "fri\u00b7sche\u00b7res", "Le\u00b7ben", "ver\u00b7j\u00fcngt", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAT", "$,", "NN", "VVPP", "$,", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.3": {"text": "Bist du die Stimme der Nacht, der Klang sehns\u00fcchtigen Lenzes,", "tokens": ["Bist", "du", "die", "Stim\u00b7me", "der", "Nacht", ",", "der", "Klang", "sehn\u00b7s\u00fcch\u00b7ti\u00b7gen", "Len\u00b7zes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "NE", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Philomele, die sonst oft um den Schlaf dich betrog?", "tokens": ["Phi\u00b7lo\u00b7me\u00b7le", ",", "die", "sonst", "oft", "um", "den", "Schlaf", "dich", "be\u00b7trog", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ADV", "ADV", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Bist du der Blumen, der Nachtviolen, der z\u00fcchtigen Veilchen,", "tokens": ["Bist", "du", "der", "Blu\u00b7men", ",", "der", "Nacht\u00b7vi\u00b7o\u00b7len", ",", "der", "z\u00fcch\u00b7ti\u00b7gen", "Veil\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Deren Gespielin du oft warst in einsamer Nacht?", "tokens": ["De\u00b7ren", "Ge\u00b7spie\u00b7lin", "du", "oft", "warst", "in", "ein\u00b7sa\u00b7mer", "Nacht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "PPER", "ADV", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Oder der z\u00e4rtliche Geist des L\u00fcftchens, der, sanft durch die Bl\u00e4tter", "tokens": ["O\u00b7der", "der", "z\u00e4rt\u00b7li\u00b7che", "Geist", "des", "L\u00fcft\u00b7chens", ",", "der", ",", "sanft", "durch", "die", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN", "$,", "PRELS", "$,", "ADJD", "APPR", "ART", "NN"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Rieselnd, Demut und Ernst haucht in die lauschende Brust?", "tokens": ["Rie\u00b7selnd", ",", "De\u00b7mut", "und", "Ernst", "haucht", "in", "die", "lau\u00b7schen\u00b7de", "Brust", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "NN", "KON", "NE", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Bist du des Abendrots, der leuchtenden Flamme des Morgens", "tokens": ["Bist", "du", "des", "A\u00b7ben\u00b7drots", ",", "der", "leuch\u00b7ten\u00b7den", "Flam\u00b7me", "des", "Mor\u00b7gens"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Ein lebendiger Teil, heiligsten Lebens ein Teil?", "tokens": ["Ein", "le\u00b7ben\u00b7di\u00b7ger", "Teil", ",", "hei\u00b7ligs\u00b7ten", "Le\u00b7bens", "ein", "Teil", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "O du hubest mich oft, den lallenden Knaben, zum Lichte,", "tokens": ["O", "du", "hu\u00b7best", "mich", "oft", ",", "den", "lal\u00b7len\u00b7den", "Kna\u00b7ben", ",", "zum", "Lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "PPER", "ADV", "$,", "ART", "ADJA", "NN", "$,", "APPRART", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.12": {"text": "Zu den G\u00f6ttern hinauf, segnend und betend zugleich,", "tokens": ["Zu", "den", "G\u00f6t\u00b7tern", "hin\u00b7auf", ",", "seg\u00b7nend", "und", "be\u00b7tend", "zu\u00b7gleich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,", "VVPP", "KON", "ADJD", "ADV", "$,"], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Hingst mit sehnendem Blick dem\u00fctig hoffender Liebe,", "tokens": ["Hingst", "mit", "seh\u00b7nen\u00b7dem", "Blick", "de\u00b7m\u00fc\u00b7tig", "hof\u00b7fen\u00b7der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.14": {"text": "Schauend ins tiefe Blau, selig am Sternenglanz.", "tokens": ["Schau\u00b7end", "ins", "tie\u00b7fe", "Blau", ",", "se\u00b7lig", "am", "Ster\u00b7nen\u00b7glanz", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "$,", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wo du auch bist und was du auch bist, dich ehren nicht Tr\u00e4nen,", "tokens": ["Wo", "du", "auch", "bist", "und", "was", "du", "auch", "bist", ",", "dich", "eh\u00b7ren", "nicht", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VAFIN", "KON", "PWS", "PPER", "ADV", "VAFIN", "$,", "PPER", "VVFIN", "PTKNEG", "NN", "$,"], "meter": "+---+-+-+-+--+-", "measure": "trochaic.hexa.relaxed"}, "line.16": {"text": "Nein, ein m\u00e4nnliches Herz, nein, ein r\u00fcstiger Lauf.", "tokens": ["Nein", ",", "ein", "m\u00e4nn\u00b7li\u00b7ches", "Herz", ",", "nein", ",", "ein", "r\u00fcs\u00b7ti\u00b7ger", "Lauf", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "ADJA", "NN", "$,", "PTKANT", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.17": {"text": "Gib dem Brennenden denn die heilige Weihe, da\u00df oben", "tokens": ["Gib", "dem", "Bren\u00b7nen\u00b7den", "denn", "die", "hei\u00b7li\u00b7ge", "Wei\u00b7he", ",", "da\u00df", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "ART", "NN", "KON", "ART", "ADJA", "NN", "$,", "KOUS", "ADV"], "meter": "+-+----+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.18": {"text": "Bei den Himmlischen einst Licht sich vereine dem Licht.", "tokens": ["Bei", "den", "Himm\u00b7li\u00b7schen", "einst", "Licht", "sich", "ver\u00b7ei\u00b7ne", "dem", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "NN", "PRF", "VVFIN", "ART", "NN", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}}}}}