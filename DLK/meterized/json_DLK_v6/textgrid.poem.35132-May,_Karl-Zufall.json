{"textgrid.poem.35132": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Zufall", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gieb dich nicht hin dem irrigen Gedanken,", "tokens": ["Gieb", "dich", "nicht", "hin", "dem", "ir\u00b7ri\u00b7gen", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df du ein Spielball blinden Looses seist.", "tokens": ["Da\u00df", "du", "ein", "Spiel\u00b7ball", "blin\u00b7den", "Loo\u00b7ses", "seist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Befreie dich von deinen engen Schranken,", "tokens": ["Be\u00b7frei\u00b7e", "dich", "von", "dei\u00b7nen", "en\u00b7gen", "Schran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und such nach ihm, der f\u00fcr dich Zufall hei\u00dft.", "tokens": ["Und", "such", "nach", "ihm", ",", "der", "f\u00fcr", "dich", "Zu\u00b7fall", "hei\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "$,", "PRELS", "APPR", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du wirst sehr bald ein g\u00f6ttlich Walten sp\u00fcren,", "tokens": ["Du", "wirst", "sehr", "bald", "ein", "g\u00f6tt\u00b7lich", "Wal\u00b7ten", "sp\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wohin du blickst, sei nah es oder fern,", "tokens": ["Wo\u00b7hin", "du", "blickst", ",", "sei", "nah", "es", "o\u00b7der", "fern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "VAFIN", "ADJD", "PPER", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und dies Empfinden wird dich weiter f\u00fchren,", "tokens": ["Und", "dies", "Emp\u00b7fin\u00b7den", "wird", "dich", "wei\u00b7ter", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "NN", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bis du sie deutlich f\u00fchlst, die Hand des Herrn.", "tokens": ["Bis", "du", "sie", "deut\u00b7lich", "f\u00fchlst", ",", "die", "Hand", "des", "Herrn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VVFIN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Zwar wird von ihr dem Unverstande nimmer", "tokens": ["Zwar", "wird", "von", "ihr", "dem", "Un\u00b7ver\u00b7stan\u00b7de", "nim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "PPOSAT", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das, was er will, schnell in den Schoo\u00df gelegt,", "tokens": ["Das", ",", "was", "er", "will", ",", "schnell", "in", "den", "Schoo\u00df", "ge\u00b7legt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWS", "PPER", "VMFIN", "$,", "ADJD", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch kennt die Weisheit und die Liebe immer", "tokens": ["Doch", "kennt", "die", "Weis\u00b7heit", "und", "die", "Lie\u00b7be", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "KON", "ART", "NN", "ADV"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Den Wunsch, der sich in deinem Herzen regt.", "tokens": ["Den", "Wunsch", ",", "der", "sich", "in", "dei\u00b7nem", "Her\u00b7zen", "reg\u00b7t."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "APPR", "PPOSAT", "NN", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und ist die Sonne heute dir entschwunden,", "tokens": ["Und", "ist", "die", "Son\u00b7ne", "heu\u00b7te", "dir", "ent\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So wirst du sie schon morgen wiederschaun.", "tokens": ["So", "wirst", "du", "sie", "schon", "mor\u00b7gen", "wie\u00b7der\u00b7schaun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es h\u00e4ngt der Rathschlu\u00df Gottes nicht an Stunden;", "tokens": ["Es", "h\u00e4ngt", "der", "Rath\u00b7schlu\u00df", "Got\u00b7tes", "nicht", "an", "Stun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er fordert nur Gehorsam und Vertraun.", "tokens": ["Er", "for\u00b7dert", "nur", "Ge\u00b7hor\u00b7sam", "und", "Ver\u00b7traun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}