{"textgrid.poem.60765": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "55.", "genre": "verse", "period": "N.A.", "pub_year": 1829, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4hrend Blut in reichen Str\u00f6men flo\u00df dem Wahne, flo\u00df der Zeit,", "tokens": ["W\u00e4h\u00b7rend", "Blut", "in", "rei\u00b7chen", "Str\u00f6\u00b7men", "flo\u00df", "dem", "Wah\u00b7ne", ",", "flo\u00df", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Standst du, Held, auf beiden Ufern, ragend als Kolo\u00df der Zeit!", "tokens": ["Standst", "du", ",", "Held", ",", "auf", "bei\u00b7den", "U\u00b7fern", ",", "ra\u00b7gend", "als", "Ko\u00b7lo\u00df", "der", "Zeit", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "APPR", "PIAT", "NN", "$,", "ADJD", "KOKOM", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Tief zu sich herabgezogen alles Gro\u00dfe hatten sie,", "tokens": ["Tief", "zu", "sich", "her\u00b7ab\u00b7ge\u00b7zo\u00b7gen", "al\u00b7les", "Gro\u00b7\u00dfe", "hat\u00b7ten", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PRF", "VVFIN", "PIAT", "ADJA", "VAFIN", "PPER", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Doch du kamst und herrschtest m\u00e4chtig \u00fcberm kleinen Tro\u00df der Zeit:", "tokens": ["Doch", "du", "kamst", "und", "herrschtest", "m\u00e4ch\u00b7tig", "\u00fc\u00b7berm", "klei\u00b7nen", "Tro\u00df", "der", "Zeit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "ADJD", "APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "--+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "F\u00fcrsten hielten dir den B\u00fcgel, Kaiser dir den Baldachin,", "tokens": ["F\u00fcrs\u00b7ten", "hiel\u00b7ten", "dir", "den", "B\u00fc\u00b7gel", ",", "Kai\u00b7ser", "dir", "den", "Bal\u00b7da\u00b7ch\u00b7in", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "$,", "NN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Unter deinem Schenkel st\u00f6hnte das gez\u00e4hmte Ro\u00df der Zeit.", "tokens": ["Un\u00b7ter", "dei\u00b7nem", "Schen\u00b7kel", "st\u00f6hn\u00b7te", "das", "ge\u00b7z\u00e4hm\u00b7te", "Ro\u00df", "der", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Was nur Scheinverdienst erheuchelt, tratst du nieder in den Staub,", "tokens": ["Was", "nur", "Schein\u00b7ver\u00b7dienst", "er\u00b7heu\u00b7chelt", ",", "tratst", "du", "nie\u00b7der", "in", "den", "Staub", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "NN", "VVFIN", "$,", "VVFIN", "PPER", "PTKVZ", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Nahmst des Gl\u00fccks Tribut zum Opfer, nahmst den Zoll und Scho\u00df der Zeit:", "tokens": ["Nahmst", "des", "Gl\u00fccks", "Tri\u00b7but", "zum", "Op\u00b7fer", ",", "nahmst", "den", "Zoll", "und", "Scho\u00df", "der", "Zeit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "APPRART", "NN", "$,", "VVFIN", "ART", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Sei das Gl\u00fcck denn laut gepriesen, samt den Gaben, die's verschenkt,", "tokens": ["Sei", "das", "Gl\u00fcck", "denn", "laut", "ge\u00b7prie\u00b7sen", ",", "samt", "den", "Ga\u00b7ben", ",", "die's", "ver\u00b7schenkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "KON", "ADJD", "VVPP", "$,", "APPR", "ART", "NN", "$,", "PDS", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.10": {"text": "Wer's gewann, geno\u00df des Lebens, wer's erfuhr, geno\u00df der Zeit!", "tokens": ["Wer's", "ge\u00b7wann", ",", "ge\u00b7no\u00df", "des", "Le\u00b7bens", ",", "wer's", "er\u00b7fuhr", ",", "ge\u00b7no\u00df", "der", "Zeit", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "VVFIN", "ART", "NN", "$,", "PWS", "VVFIN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.11": {"text": "Aber h\u00fctet euch, Begl\u00fcckte; denn die Menge rast um euch,", "tokens": ["A\u00b7ber", "h\u00fc\u00b7tet", "euch", ",", "Be\u00b7gl\u00fcck\u00b7te", ";", "denn", "die", "Men\u00b7ge", "rast", "um", "euch", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "VVFIN", "$.", "KON", "ART", "NN", "VVFIN", "APPR", "PPER", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Stets belagert sie den stolzen Kastellan im Schlo\u00df der Zeit.", "tokens": ["Stets", "be\u00b7la\u00b7gert", "sie", "den", "stol\u00b7zen", "Kas\u00b7tel\u00b7lan", "im", "Schlo\u00df", "der", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.13": {"text": "Mancher Pfeil, o Held, durchbohrte deine starke Brust von Erz;", "tokens": ["Man\u00b7cher", "Pfeil", ",", "o", "Held", ",", "durch\u00b7bohr\u00b7te", "dei\u00b7ne", "star\u00b7ke", "Brust", "von", "Erz", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "FM", "NN", "$,", "VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.14": {"text": "Aber Namen, gro\u00df wie deiner, f\u00fcrchten kein Gescho\u00df der Zeit!", "tokens": ["A\u00b7ber", "Na\u00b7men", ",", "gro\u00df", "wie", "dei\u00b7ner", ",", "f\u00fcrch\u00b7ten", "kein", "Ge\u00b7scho\u00df", "der", "Zeit", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ADJD", "KOKOM", "PPOSAT", "$,", "VVFIN", "PIAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}