{"textgrid.poem.31646": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "18. Was ist der Mensch! da\u00df du sein gedenkest", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach was ist der Arme Mensch / da\u00df du seiner so gedenkest.", "tokens": ["Ach", "was", "ist", "der", "Ar\u00b7me", "Mensch", "/", "da\u00df", "du", "sei\u00b7ner", "so", "ge\u00b7den\u00b7kest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWS", "VAFIN", "ART", "NN", "NN", "$(", "KOUS", "PPER", "PPOSAT", "ADV", "VVPP", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "ihm so reiche Wunder-Gnad / H\u00fclffe / Schutz / und Rettung schenkest?", "tokens": ["ihm", "so", "rei\u00b7che", "Wun\u00b7der\u00b7Gnad", "/", "H\u00fclf\u00b7fe", "/", "Schutz", "/", "und", "Ret\u00b7tung", "schen\u00b7kest", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJA", "NN", "$(", "NN", "$(", "NN", "$(", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-++-+-+--+", "measure": "iambic.octa.plus.octa.plus.chol"}, "line.3": {"text": "der nichts kan als dich erz\u00fcrnen / \u00fcbertrit all dein Gebot:", "tokens": ["der", "nichts", "kan", "als", "dich", "er\u00b7z\u00fcr\u00b7nen", "/", "\u00fc\u00b7bert\u00b7rit", "all", "dein", "Ge\u00b7bot", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "KOUS", "PPER", "VVINF", "$(", "APPR", "PIAT", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "dennoch du dich ihm erzeigest / als ein Lieb'-ergebner Gott.", "tokens": ["den\u00b7noch", "du", "dich", "ihm", "er\u00b7zei\u00b7gest", "/", "als", "ein", "Lie\u00b7b'\u00b7er\u00b7geb\u00b7ner", "Gott", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PRF", "PPER", "VVFIN", "$(", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+--+-+", "measure": "iambic.septa.relaxed"}, "line.5": {"text": "Kan aus diesem Gnaden-Werk / wie aus einem B\u00fcchlein / lesen", "tokens": ["Kan", "aus", "die\u00b7sem", "Gna\u00b7den\u00b7Werk", "/", "wie", "aus", "ei\u00b7nem", "B\u00fcch\u00b7lein", "/", "le\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["VMFIN", "APPR", "PDAT", "NN", "$(", "KOKOM", "APPR", "ART", "NN", "$(", "VVINF"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "deiner Gottheit Eigenschafft / Art / und recht selbst\u00e4ndigs Wesen:", "tokens": ["dei\u00b7ner", "Got\u00b7theit", "Ei\u00b7gen\u00b7schafft", "/", "Art", "/", "und", "recht", "selb\u00b7st\u00e4n\u00b7digs", "We\u00b7sen", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$(", "NN", "$(", "KON", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "da\u00df du g\u00fctig / liebreich / heilig / und derwegen Gott must seyn.", "tokens": ["da\u00df", "du", "g\u00fc\u00b7tig", "/", "lieb\u00b7reich", "/", "hei\u00b7lig", "/", "und", "der\u00b7we\u00b7gen", "Gott", "must", "seyn", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "$(", "ADJD", "$(", "ADJD", "$(", "KON", "PAV", "NN", "VMFIN", "VAINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Aus der unergr\u00fcndten Liebe / leucht der Gottheit heller Schein.", "tokens": ["Aus", "der", "un\u00b7er\u00b7gr\u00fcnd\u00b7ten", "Lie\u00b7be", "/", "leucht", "der", "Got\u00b7theit", "hel\u00b7ler", "Schein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$(", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Auch die unaufh\u00f6rlich G\u00fct / uns was \u00fcbermenschlichs lehret:", "tokens": ["Auch", "die", "un\u00b7auf\u00b7h\u00f6r\u00b7lich", "G\u00fct", "/", "uns", "was", "\u00fc\u00b7ber\u00b7menschlichs", "leh\u00b7ret", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJD", "NN", "$(", "PPER", "PIS", "APPRART", "VVFIN", "$."], "meter": "--+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "weil sie nur in Gott allein / unverdient gleich-Liebreich w\u00e4hret.", "tokens": ["weil", "sie", "nur", "in", "Gott", "al\u00b7lein", "/", "un\u00b7ver\u00b7di\u00b7ent", "gleich\u00b7Lieb\u00b7reich", "w\u00e4h\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN", "ADV", "$(", "ADJD", "ADJD", "VVFIN", "$."], "meter": "+-+-+--+-+--+-+-", "measure": "trochaic.septa.relaxed"}, "line.11": {"text": "Ja die allerheiligst W\u00fcrkung / ohne Selbnutz uns zu gut /", "tokens": ["Ja", "die", "al\u00b7ler\u00b7hei\u00b7ligst", "W\u00fcr\u00b7kung", "/", "oh\u00b7ne", "Selb\u00b7nutz", "uns", "zu", "gut", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ART", "ADV", "NN", "$(", "APPR", "NN", "PPER", "PTKA", "ADJD", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.12": {"text": "zeigt / es sey was hoch-erhabnes / \u00fcber aller Menschen Muht.", "tokens": ["zeigt", "/", "es", "sey", "was", "hoch\u00b7er\u00b7hab\u00b7nes", "/", "\u00fc\u00b7ber", "al\u00b7ler", "Men\u00b7schen", "Muht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "PPER", "VAFIN", "PIS", "ADJA", "$(", "APPR", "PIAT", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}