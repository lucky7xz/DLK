{"textgrid.poem.46500": {"metadata": {"author": {"name": "Engelke, Gerrit", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hinaus aufs Meer! die glatten Wellen funkeln.", "genre": "verse", "period": "N.A.", "pub_year": 1917, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hinaus aufs Meer! die glatten Wellen funkeln.", "tokens": ["Hin\u00b7aus", "aufs", "Meer", "!", "die", "glat\u00b7ten", "Wel\u00b7len", "fun\u00b7keln", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$.", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Froh durch des Gartens saftige Bukette,", "tokens": ["Froh", "durch", "des", "Gar\u00b7tens", "saf\u00b7ti\u00b7ge", "Bu\u00b7ket\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+--+--", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Darin im Licht die Rosen r\u00f6ter dunkeln.", "tokens": ["Da\u00b7rin", "im", "Licht", "die", "Ro\u00b7sen", "r\u00f6\u00b7ter", "dun\u00b7keln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPRART", "NN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es knirscht der Ufersand am Landungsbrette:", "tokens": ["Es", "knirscht", "der", "U\u00b7fer\u00b7sand", "am", "Lan\u00b7dungs\u00b7bret\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hurra, mein Boot! ihr braunlackierte Planken,", "tokens": ["Hur\u00b7ra", ",", "mein", "Boot", "!", "ihr", "braun\u00b7la\u00b7ckier\u00b7te", "Plan\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Du ungeduldig Tanzboot an der Kette,", "tokens": ["Du", "un\u00b7ge\u00b7dul\u00b7dig", "Tanz\u00b7boot", "an", "der", "Ket\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dir schl\u00e4gt der Schaum die schmalgew\u00f6lbten Flanken;", "tokens": ["Dir", "schl\u00e4gt", "der", "Schaum", "die", "schmal\u00b7ge\u00b7w\u00f6lb\u00b7ten", "Flan\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vertrauter Seegenosse, du mein Freund,", "tokens": ["Ver\u00b7trau\u00b7ter", "See\u00b7ge\u00b7nos\u00b7se", ",", "du", "mein", "Freund", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie manche Wetterfahrt kann ich dir danken;", "tokens": ["Wie", "man\u00b7che", "Wet\u00b7ter\u00b7fahrt", "kann", "ich", "dir", "dan\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Bin auch wie du von Brand und Salz gebr\u00e4unt.", "tokens": ["Bin", "auch", "wie", "du", "von", "Brand", "und", "Salz", "ge\u00b7br\u00e4unt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "KOKOM", "PPER", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Riemen raus und uferab gestrebt", "tokens": ["Die", "Rie\u00b7men", "raus", "und", "uf\u00b7er\u00b7ab", "ge\u00b7strebt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "KON", "ADV", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit kr\u00e4ft'gem Schlag \u2013 vom Gartengr\u00fcn umz\u00e4unt", "tokens": ["Mit", "kr\u00e4ft'\u00b7gem", "Schlag", "\u2013", "vom", "Gar\u00b7ten\u00b7gr\u00fcn", "um\u00b7z\u00e4unt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$(", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Winkt w\u00e4rmeflimmernd wei\u00df das Haus und schwebt", "tokens": ["Winkt", "w\u00e4r\u00b7me\u00b7flim\u00b7mernd", "wei\u00df", "das", "Haus", "und", "schwebt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "VVFIN", "ART", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf prallbemaltem hei\u00dfen Uferstriche:", "tokens": ["Auf", "prall\u00b7be\u00b7mal\u00b7tem", "hei\u00b7\u00dfen", "Uf\u00b7er\u00b7stri\u00b7che", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Bild von Herdlichkeit, doch erdgeklebt.", "tokens": ["Ein", "Bild", "von", "Herd\u00b7lich\u00b7keit", ",", "doch", "erd\u00b7ge\u00b7klebt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Nun \u00c4rmel aufgekrempt; die mittagliche", "tokens": ["Nun", "\u00c4r\u00b7mel", "auf\u00b7ge\u00b7krempt", ";", "die", "mit\u00b7tag\u00b7li\u00b7che"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "NN", "VVFIN", "$.", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vorm durst'gen Blick weitoffne Flut gewinnen,", "tokens": ["Vorm", "dur\u00b7st'\u00b7gen", "Blick", "weit\u00b7off\u00b7ne", "Flut", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Die hin und her sich wirft im Sonnenstiche.", "tokens": ["Die", "hin", "und", "her", "sich", "wirft", "im", "Son\u00b7nen\u00b7sti\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKVZ", "KON", "ADV", "PRF", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Getauchte Ruder \u00e4chzen in den Pinnen,", "tokens": ["Ge\u00b7tauch\u00b7te", "Ru\u00b7der", "\u00e4ch\u00b7zen", "in", "den", "Pin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein salzgew\u00fcrzter Wind springt herrlich auf,", "tokens": ["Ein", "salz\u00b7ge\u00b7w\u00fcrz\u00b7ter", "Wind", "springt", "herr\u00b7lich", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "L\u00e4\u00dft spr\u00fchend Schaumgespritz vom Buge rinnen.", "tokens": ["L\u00e4\u00dft", "spr\u00fc\u00b7hend", "Schaum\u00b7ge\u00b7spritz", "vom", "Bu\u00b7ge", "rin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Gl\u00fcckhei\u00dfer Tag! leuchtblau emporgerundet \u2013", "tokens": ["Gl\u00fcck\u00b7hei\u00b7\u00dfer", "Tag", "!", "leucht\u00b7blau", "em\u00b7por\u00b7ge\u00b7run\u00b7det", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Darein die braune Stirn, die Brust getaucht", "tokens": ["Da\u00b7rein", "die", "brau\u00b7ne", "Stirn", ",", "die", "Brust", "ge\u00b7taucht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "ART", "ADJA", "NN", "$,", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und lichtflutvoll sich atmend froh gesundet.", "tokens": ["Und", "licht\u00b7flut\u00b7voll", "sich", "at\u00b7mend", "froh", "ge\u00b7sun\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PRF", "ADJD", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "O wie mit jedem Ruck, von Kraft gestaucht,", "tokens": ["O", "wie", "mit", "je\u00b7dem", "Ruck", ",", "von", "Kraft", "ge\u00b7staucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "APPR", "PIAT", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Horizont sich immer gr\u00f6\u00dfer weitet \u2013", "tokens": ["Der", "Ho\u00b7ri\u00b7zont", "sich", "im\u00b7mer", "gr\u00f6\u00b7\u00dfer", "wei\u00b7tet", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die wei\u00dfen Wolken bl\u00fchn, das Wasser raucht \u2013", "tokens": ["Die", "wei\u00b7\u00dfen", "Wol\u00b7ken", "bl\u00fchn", ",", "das", "Was\u00b7ser", "raucht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Lichtmeer ist \u00fcber Wassermeer gebreitet \u2013", "tokens": ["Licht\u00b7meer", "ist", "\u00fc\u00b7ber", "Was\u00b7ser\u00b7meer", "ge\u00b7brei\u00b7tet", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "NN", "VVPP", "$("], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.2": {"text": "Ich zieh die glanzbestr\u00f6mten Ruder ein", "tokens": ["Ich", "zieh", "die", "glanz\u00b7be\u00b7str\u00f6m\u00b7ten", "Ru\u00b7der", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ART"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und horch wie unterm Kiel das Wasser gleitet,", "tokens": ["Und", "horch", "wie", "un\u00b7term", "Kiel", "das", "Was\u00b7ser", "glei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "KOKOM", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Wie in den hohen k\u00f6niglichen Schein,", "tokens": ["Wie", "in", "den", "ho\u00b7hen", "k\u00f6\u00b7nig\u00b7li\u00b7chen", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie in das gro\u00dfe Wolkensausen oben", "tokens": ["Wie", "in", "das", "gro\u00b7\u00dfe", "Wol\u00b7ken\u00b7sau\u00b7sen", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das k\u00fchle Glucksen z\u00e4rtlich singt hinein.", "tokens": ["Das", "k\u00fch\u00b7le", "Gluck\u00b7sen", "z\u00e4rt\u00b7lich", "singt", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Fanfaren schmettern G\u00f6ttertag da droben,", "tokens": ["Fan\u00b7fa\u00b7ren", "schmet\u00b7tern", "G\u00f6t\u00b7ter\u00b7tag", "da", "dro\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Umarmend, ewigkeitlich unbewu\u00dft \u2013", "tokens": ["Um\u00b7ar\u00b7mend", ",", "e\u00b7wig\u00b7keit\u00b7lich", "un\u00b7be\u00b7wu\u00dft", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O Meer, o Himmel, Licht in Licht gehoben!", "tokens": ["O", "Meer", ",", "o", "Him\u00b7mel", ",", "Licht", "in", "Licht", "ge\u00b7ho\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "NN", "$,", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "O Mensch, entbl\u00f6\u00dfe gl\u00fccklich deine Brust!", "tokens": ["O", "Mensch", ",", "ent\u00b7bl\u00f6\u00b7\u00dfe", "gl\u00fcck\u00b7lich", "dei\u00b7ne", "Brust", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wirf ab, wirf ab das Kleid, so grau und trennend,", "tokens": ["Wirf", "ab", ",", "wirf", "ab", "das", "Kleid", ",", "so", "grau", "und", "tren\u00b7nend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "VVIMP", "APPR", "ART", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "St\u00fcrz ein in diese unsagbare Lust!", "tokens": ["St\u00fcrz", "ein", "in", "die\u00b7se", "un\u00b7sag\u00b7ba\u00b7re", "Lust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.14": {"line.1": {"text": "O nun ich nackt, mitbrausend und mitbrennend,", "tokens": ["O", "nun", "ich", "nackt", ",", "mit\u00b7brau\u00b7send", "und", "mit\u00b7bren\u00b7nend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPER", "ADJD", "$,", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Best\u00fcrm ich denkend, singend, br\u00fcnstig dich,", "tokens": ["Be\u00b7st\u00fcrm", "ich", "den\u00b7kend", ",", "sin\u00b7gend", ",", "br\u00fcns\u00b7tig", "dich", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "VVPP", "$,", "ADJD", "$,", "ADJD", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dich Gott und Tagerleuchter, Vater nennend.", "tokens": ["Dich", "Gott", "und", "Ta\u00b7ger\u00b7leuch\u00b7ter", ",", "Va\u00b7ter", "nen\u00b7nend", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "$,", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.15": {"line.1": {"text": "Wie w\u00fc\u00dft ich diesem Gl\u00fcck, so sonderlich,", "tokens": ["Wie", "w\u00fc\u00dft", "ich", "die\u00b7sem", "Gl\u00fcck", ",", "so", "son\u00b7der\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PDAT", "NN", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Denn andern vollen Namen als den deinen,", "tokens": ["Denn", "an\u00b7dern", "vol\u00b7len", "Na\u00b7men", "als", "den", "dei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADJA", "NN", "KOKOM", "ART", "PPOSAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der alle Dinge w\u00e4rmet v\u00e4terlich.", "tokens": ["Der", "al\u00b7le", "Din\u00b7ge", "w\u00e4r\u00b7met", "v\u00e4\u00b7ter\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.16": {"line.1": {"text": "Du willst auch g\u00fctig jetzt um mich, den einen,", "tokens": ["Du", "willst", "auch", "g\u00fc\u00b7tig", "jetzt", "um", "mich", ",", "den", "ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADJD", "ADV", "APPR", "PPER", "$,", "PRELS", "ART", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der kam vom flachen Ufer Mann und arm,", "tokens": ["Der", "kam", "vom", "fla\u00b7chen", "U\u00b7fer", "Mann", "und", "arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "