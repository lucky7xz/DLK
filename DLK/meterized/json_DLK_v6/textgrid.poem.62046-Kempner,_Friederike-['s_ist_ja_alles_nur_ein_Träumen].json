{"textgrid.poem.62046": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "['s ist ja alles nur ein Tr\u00e4umen]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "'s ist ja alles nur ein Tr\u00e4umen,", "tokens": ["'s", "ist", "ja", "al\u00b7les", "nur", "ein", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIS", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nur ein silberwei\u00dfes Sch\u00e4umen", "tokens": ["Nur", "ein", "sil\u00b7ber\u00b7wei\u00b7\u00dfes", "Sch\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Von dem Meere, das erst wird.", "tokens": ["Von", "dem", "Mee\u00b7re", ",", "das", "erst", "wird", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADV", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie ein Degen, der da klirrt,", "tokens": ["Wie", "ein", "De\u00b7gen", ",", "der", "da", "klirrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Eh' er aus der Scheide irrt", "tokens": ["Eh'", "er", "aus", "der", "Schei\u00b7de", "irrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zu den tatengro\u00dfen R\u00e4umen.", "tokens": ["Zu", "den", "ta\u00b7ten\u00b7gro\u00b7\u00dfen", "R\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie der Aar die Schwingen hebt,", "tokens": ["Wie", "der", "Aar", "die", "Schwin\u00b7gen", "hebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Wenn er noch im Neste lebt,", "tokens": ["Wenn", "er", "noch", "im", "Nes\u00b7te", "lebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eh' er auf gen Himmel schwebt.", "tokens": ["Eh'", "er", "auf", "gen", "Him\u00b7mel", "schwebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}