{"textgrid.poem.59620": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das Schwert ist gefeget,", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Schwert ist gefeget,", "tokens": ["Das", "Schwert", "ist", "ge\u00b7fe\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Der S\u00e4bel ist blank,", "tokens": ["Der", "S\u00e4\u00b7bel", "ist", "blank", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Der Speer ist umleget", "tokens": ["Der", "Speer", "ist", "um\u00b7le\u00b7get"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Mit Stahl breit und lang,", "tokens": ["Mit", "Stahl", "breit", "und", "lang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Der Mut ist gewetzet,", "tokens": ["Der", "Mut", "ist", "ge\u00b7wet\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Das Herz sich erletzet", "tokens": ["Das", "Herz", "sich", "er\u00b7let\u00b7zet"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PRF", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Mit Trommeln und Pfeifen", "tokens": ["Mit", "Trom\u00b7meln", "und", "Pfei\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Im kriegrischen Klang.", "tokens": ["Im", "krie\u00b7gri\u00b7schen", "Klang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Nun her, ihr Franzosen!", "tokens": ["Nun", "her", ",", "ihr", "Fran\u00b7zo\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "PPOSAT", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Hieher in das Feld!", "tokens": ["Hie\u00b7her", "in", "das", "Feld", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Hier tanzet auf Rosen!", "tokens": ["Hier", "tan\u00b7zet", "auf", "Ro\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Musik ist bestellt;", "tokens": ["Mu\u00b7sik", "ist", "be\u00b7stellt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Schon klingen die Saiten", "tokens": ["Schon", "klin\u00b7gen", "die", "Sai\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Des Reigens von weitem;", "tokens": ["Des", "Rei\u00b7gens", "von", "wei\u00b7tem", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Versuchet, wer heute", "tokens": ["Ver\u00b7su\u00b7chet", ",", "wer", "heu\u00b7te"], "token_info": ["word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "PWS", "ADV"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Den Vortanz erh\u00e4lt.", "tokens": ["Den", "Vor\u00b7tanz", "er\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Die Braut hei\u00dfet Ehre,", "tokens": ["Die", "Braut", "hei\u00b7\u00dfet", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$,"], "meter": "-++-+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Sie f\u00fchret den Tanz", "tokens": ["Sie", "f\u00fch\u00b7ret", "den", "Tanz"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und schreitet dem Heere", "tokens": ["Und", "schrei\u00b7tet", "dem", "Hee\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Voran mit dem Kranz;", "tokens": ["Vo\u00b7ran", "mit", "dem", "Kranz", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Sie mahnet zur Rache", "tokens": ["Sie", "mah\u00b7net", "zur", "Ra\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "F\u00fcr heilige Sache", "tokens": ["F\u00fcr", "hei\u00b7li\u00b7ge", "Sa\u00b7che"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Und hat ihn gef\u00e4rbet", "tokens": ["Und", "hat", "ihn", "ge\u00b7f\u00e4r\u00b7bet"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Mit blutigem Glanz.", "tokens": ["Mit", "blu\u00b7ti\u00b7gem", "Glanz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Das Brautm\u00e4dchen springet", "tokens": ["Das", "Braut\u00b7m\u00e4d\u00b7chen", "sprin\u00b7get"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "So tapfer daher,", "tokens": ["So", "tap\u00b7fer", "da\u00b7her", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PAV", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Hei\u00dft Freiheit und schwinget", "tokens": ["Hei\u00dft", "Frei\u00b7heit", "und", "schwin\u00b7get"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "NN", "KON", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Den m\u00e4chtigen Speer;", "tokens": ["Den", "m\u00e4ch\u00b7ti\u00b7gen", "Speer", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Sie kann nicht erbleichen,", "tokens": ["Sie", "kann", "nicht", "er\u00b7blei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "++--+-", "measure": "trochaic.tri.relaxed"}, "line.6": {"text": "Auf Tr\u00fcmmern und Leichen", "tokens": ["Auf", "Tr\u00fcm\u00b7mern", "und", "Lei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Da f\u00fchrt sie als Heldin", "tokens": ["Da", "f\u00fchrt", "sie", "als", "Hel\u00b7din"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KOUS", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Das vorderste Heer.", "tokens": ["Das", "vor\u00b7ders\u00b7te", "Heer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Drum frisch, Kameraden!", "tokens": ["Drum", "frisch", ",", "Ka\u00b7me\u00b7ra\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PAV", "ADJD", "$,", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Wer greifet den Kranz?", "tokens": ["Wer", "grei\u00b7fet", "den", "Kranz", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Seid alle geladen", "tokens": ["Seid", "al\u00b7le", "ge\u00b7la\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["VAIMP", "PIAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Zum Spiel und zum Tanz;", "tokens": ["Zum", "Spiel", "und", "zum", "Tanz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Die Trommeln erklingen,", "tokens": ["Die", "Trom\u00b7meln", "er\u00b7klin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Die Fahnen sich schwingen \u2013", "tokens": ["Die", "Fah\u00b7nen", "sich", "schwin\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVINF", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Juchheisa! Juchheisa!", "tokens": ["Juch\u00b7hei\u00b7sa", "!", "Juch\u00b7hei\u00b7sa", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$.", "NE", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Zum lustigen Tanz!", "tokens": ["Zum", "lus\u00b7ti\u00b7gen", "Tanz", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}