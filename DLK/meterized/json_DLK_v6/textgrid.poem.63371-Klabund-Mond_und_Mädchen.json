{"textgrid.poem.63371": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Mond und M\u00e4dchen", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es kriecht der kahle Mond durch Zweigge\u00e4der,", "tokens": ["Es", "kriecht", "der", "kah\u00b7le", "Mond", "durch", "Zweig\u00b7ge\u00e4\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Ob wo im Haus ein M\u00e4dchen wohnt,", "tokens": ["Ob", "wo", "im", "Haus", "ein", "M\u00e4d\u00b7chen", "wohnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWAV", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein warmes Bett, ein daunenweicher Leib,", "tokens": ["Ein", "war\u00b7mes", "Bett", ",", "ein", "dau\u00b7nen\u00b7wei\u00b7cher", "Leib", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Es w\u00e4rmt zur Winternacht sich gern ein jeder..", "tokens": ["Es", "w\u00e4rmt", "zur", "Win\u00b7ter\u00b7nacht", "sich", "gern", "ein", "je\u00b7der", ".."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "PRF", "ADV", "ART", "PIS", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "O M\u00e4del, bleib, du schlanke Zeder!", "tokens": ["O", "M\u00e4\u00b7del", ",", "bleib", ",", "du", "schlan\u00b7ke", "Ze\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Mond tastet am Fensterglase", "tokens": ["Der", "Mond", "tas\u00b7tet", "am", "Fens\u00b7ter\u00b7gla\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Und zittert vor Begier und Frost...", "tokens": ["Und", "zit\u00b7tert", "vor", "Be\u00b7gier", "und", "Frost", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das M\u00e4del schl\u00e4gt ihm vor der Nase", "tokens": ["Das", "M\u00e4\u00b7del", "schl\u00e4gt", "ihm", "vor", "der", "Na\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Die L\u00e4den zu und h\u00f6hnt: Gib ruh!", "tokens": ["Die", "L\u00e4\u00b7den", "zu", "und", "h\u00f6hnt", ":", "Gib", "ruh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "KON", "VVFIN", "$.", "VVIMP", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Alten Gliedern ziemt nicht junger Most!", "tokens": ["Al\u00b7ten", "Glie\u00b7dern", "ziemt", "nicht", "jun\u00b7ger", "Most", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PTKNEG", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Er aber hat den Finger in der Fensterspalte,", "tokens": ["Er", "a\u00b7ber", "hat", "den", "Fin\u00b7ger", "in", "der", "Fens\u00b7ter\u00b7spal\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob ihrer Kissen eine Falte er nicht ersp\u00e4he,", "tokens": ["Ob", "ih\u00b7rer", "Kis\u00b7sen", "ei\u00b7ne", "Fal\u00b7te", "er", "nicht", "er\u00b7sp\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Er ihre Blicke, braune Rehe,", "tokens": ["Er", "ih\u00b7re", "Bli\u00b7cke", ",", "brau\u00b7ne", "Re\u00b7he", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u00dcber der Br\u00fcste Sommerh\u00fcgel", "tokens": ["\u00dc\u00b7ber", "der", "Br\u00fcs\u00b7te", "Som\u00b7mer\u00b7h\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Z\u00e4rtlich schreiten sehe.", "tokens": ["Z\u00e4rt\u00b7lich", "schrei\u00b7ten", "se\u00b7he", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}