{"textgrid.poem.58427": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "465. Es dr\u00e4ngt die Pein zu Gott hinein", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Herr will meinen Weg mit Dornen fest verschlie\u00dfen,", "tokens": ["Der", "Herr", "will", "mei\u00b7nen", "Weg", "mit", "Dor\u00b7nen", "fest", "ver\u00b7schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPOSAT", "NN", "APPR", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es dr\u00e4ngt das liebe Kreuz, ich kann mich regen kaum;", "tokens": ["Es", "dr\u00e4ngt", "das", "lie\u00b7be", "Kreuz", ",", "ich", "kann", "mich", "re\u00b7gen", "kaum", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "PPER", "VMFIN", "PPER", "ADJA", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "H\u00f6r, Seele, wie du kannst die Bitterkeit vers\u00fc\u00dfen:", "tokens": ["H\u00f6r", ",", "See\u00b7le", ",", "wie", "du", "kannst", "die", "Bit\u00b7ter\u00b7keit", "ver\u00b7s\u00fc\u00b7\u00dfen", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "PWAV", "PPER", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Aus allem kehr in Gott, so kriegst du weiten Raum!", "tokens": ["Aus", "al\u00b7lem", "kehr", "in", "Gott", ",", "so", "kriegst", "du", "wei\u00b7ten", "Raum", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "APPR", "NN", "$,", "ADV", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}