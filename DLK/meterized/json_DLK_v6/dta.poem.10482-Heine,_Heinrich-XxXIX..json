{"dta.poem.10482": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XxXIX.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Das Herz ist mir bedr\u00fcckt, und sehnlich", "tokens": ["Das", "Herz", "ist", "mir", "be\u00b7dr\u00fcckt", ",", "und", "sehn\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVFIN", "$,", "KON", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gedenke ich der alten Zeit;", "tokens": ["Ge\u00b7den\u00b7ke", "ich", "der", "al\u00b7ten", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Welt war damals noch so w\u00f6hnlich,", "tokens": ["Die", "Welt", "war", "da\u00b7mals", "noch", "so", "w\u00f6hn\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ruhig lebten hin die Leut'.", "tokens": ["Und", "ru\u00b7hig", "leb\u00b7ten", "hin", "die", "Leut'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch jetzt ist alles wie verschoben,", "tokens": ["Doch", "jetzt", "ist", "al\u00b7les", "wie", "ver\u00b7scho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PIS", "KOKOM", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das ist ein Dr\u00e4ngen! eine Noth!", "tokens": ["Das", "ist", "ein", "Dr\u00e4n\u00b7gen", "!", "ei\u00b7ne", "Noth", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$.", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gestorben ist der Herrgott oben,", "tokens": ["Ge\u00b7stor\u00b7ben", "ist", "der", "Herr\u00b7gott", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und unten ist der Teufel todt.", "tokens": ["Und", "un\u00b7ten", "ist", "der", "Teu\u00b7fel", "todt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und Alles schaut so gr\u00e4mlich tr\u00fcbe,", "tokens": ["Und", "Al\u00b7les", "schaut", "so", "gr\u00e4m\u00b7lich", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADV", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und krausverwirrt und morsch und kalt,", "tokens": ["Und", "kraus\u00b7ver\u00b7wirrt", "und", "morsch", "und", "kalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und w\u00e4re nicht das bischen Liebe,", "tokens": ["Und", "w\u00e4\u00b7re", "nicht", "das", "bi\u00b7schen", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So g\u00e4b' es nirgends einen Halt.", "tokens": ["So", "g\u00e4b'", "es", "nir\u00b7gends", "ei\u00b7nen", "Halt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}