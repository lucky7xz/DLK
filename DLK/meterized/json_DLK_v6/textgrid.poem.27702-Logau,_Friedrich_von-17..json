{"textgrid.poem.27702": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "17.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer wil Pertunda stoltz, hochtr\u00e4chtig auch wol nennen?", "tokens": ["Wer", "wil", "Per\u00b7tun\u00b7da", "stoltz", ",", "hoch\u00b7tr\u00e4ch\u00b7tig", "auch", "wol", "nen\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "NE", "NE", "$,", "ADJD", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Er gibt genug an Tag, er mu\u00df sie recht nicht kennen.", "tokens": ["Er", "gibt", "ge\u00b7nug", "an", "Tag", ",", "er", "mu\u00df", "sie", "recht", "nicht", "ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "$,", "PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Heist dieses denn wol stoltz? Sie bleibet unten an", "tokens": ["Heist", "die\u00b7ses", "denn", "wol", "stoltz", "?", "Sie", "blei\u00b7bet", "un\u00b7ten", "an"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PDAT", "KON", "ADV", "ADJD", "$.", "PPER", "VVFIN", "ADV", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und duldet \u00fcber ihr so leichtlich iedermann.", "tokens": ["Und", "dul\u00b7det", "\u00fc\u00b7ber", "ihr", "so", "leicht\u00b7lich", "ie\u00b7der\u00b7mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ADV", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}