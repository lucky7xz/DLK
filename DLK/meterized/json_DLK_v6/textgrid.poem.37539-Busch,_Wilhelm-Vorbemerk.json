{"textgrid.poem.37539": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Vorbemerk", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vater werden ist nicht schwer,", "tokens": ["Va\u00b7ter", "wer\u00b7den", "ist", "nicht", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAINF", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Vater sein dagegen sehr.", "tokens": ["Va\u00b7ter", "sein", "da\u00b7ge\u00b7gen", "sehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAINF", "PAV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ersteres wird gern ge\u00fcbt,", "tokens": ["Ers\u00b7te\u00b7res", "wird", "gern", "ge\u00b7\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Weil es allgemein beliebt.", "tokens": ["Weil", "es", "all\u00b7ge\u00b7mein", "be\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Selbst der Lasterhafte zeigt,", "tokens": ["Selbst", "der", "Las\u00b7ter\u00b7haf\u00b7te", "zeigt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df er gar nicht abgeneigt;", "tokens": ["Da\u00df", "er", "gar", "nicht", "ab\u00b7ge\u00b7neigt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nur will er mit seinen S\u00fcnden", "tokens": ["Nur", "will", "er", "mit", "sei\u00b7nen", "S\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Keinen guten Zweck verbinden,", "tokens": ["Kei\u00b7nen", "gu\u00b7ten", "Zweck", "ver\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sondern, wenn die Kosten kommen,", "tokens": ["Son\u00b7dern", ",", "wenn", "die", "Kos\u00b7ten", "kom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "F\u00fchlet er sich angstbeklommen.", "tokens": ["F\u00fch\u00b7let", "er", "sich", "angst\u00b7be\u00b7klom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Dieserhalb besonders scheut", "tokens": ["Die\u00b7ser\u00b7halb", "be\u00b7son\u00b7ders", "scheut"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADV", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Er die fromme Geistlichkeit,", "tokens": ["Er", "die", "from\u00b7me", "Geist\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Denn ihm sagt ein stilles Grauen:", "tokens": ["Denn", "ihm", "sagt", "ein", "stil\u00b7les", "Grau\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Das sind Leute, welche trauen. \u2013", "tokens": ["Das", "sind", "Leu\u00b7te", ",", "wel\u00b7che", "trau\u00b7en", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "NN", "$,", "PRELS", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "So ein b\u00f6ser Mensch verbleibt", "tokens": ["So", "ein", "b\u00f6\u00b7ser", "Mensch", "ver\u00b7bleibt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Lieber g\u00e4nzlich unbeweibt. \u2013", "tokens": ["Lie\u00b7ber", "g\u00e4nz\u00b7lich", "un\u00b7be\u00b7weibt", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADJD", "ADJD", "ADJD", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Ohne einen hochgesch\u00e4tzten", "tokens": ["Oh\u00b7ne", "ei\u00b7nen", "hoch\u00b7ge\u00b7sch\u00e4tz\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Tugendsamen Vorgesetzten", "tokens": ["Tu\u00b7gend\u00b7sa\u00b7men", "Vor\u00b7ge\u00b7setz\u00b7ten"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Irrt er in der Welt umher,", "tokens": ["Irrt", "er", "in", "der", "Welt", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Hat kein reines Hemde mehr,", "tokens": ["Hat", "kein", "rei\u00b7nes", "Hem\u00b7de", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Wird am Ende krumm und faltig,", "tokens": ["Wird", "am", "En\u00b7de", "krumm", "und", "fal\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Grimmig, greulich, ungestaltig,", "tokens": ["Grim\u00b7mig", ",", "greu\u00b7lich", ",", "un\u00b7ge\u00b7stal\u00b7tig", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Bis ihn dann bei Nacht und Tag", "tokens": ["Bis", "ihn", "dann", "bei", "Nacht", "und", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ADV", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Gar kein M\u00e4dchen leiden mag.", "tokens": ["Gar", "kein", "M\u00e4d\u00b7chen", "lei\u00b7den", "mag."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "NE"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.23": {"text": "Onkel hei\u00dft er g\u00fcnst'gen Falles,", "tokens": ["On\u00b7kel", "hei\u00dft", "er", "g\u00fcnst'\u00b7gen", "Fal\u00b7les", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Aber dieses ist auch alles. \u2013", "tokens": ["A\u00b7ber", "die\u00b7ses", "ist", "auch", "al\u00b7les", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADV", "PIS", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Oh, wie anders ist der Gute!", "tokens": ["Oh", ",", "wie", "an\u00b7ders", "ist", "der", "Gu\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "ADV", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Er erlegt mit frischem Mute", "tokens": ["Er", "er\u00b7legt", "mit", "fri\u00b7schem", "Mu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die gesetzlichen Geb\u00fchren,", "tokens": ["Die", "ge\u00b7setz\u00b7li\u00b7chen", "Ge\u00b7b\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "L\u00e4\u00dft sich redlich kopulieren,", "tokens": ["L\u00e4\u00dft", "sich", "red\u00b7lich", "ko\u00b7pu\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Tut im stillen hocherfreut", "tokens": ["Tut", "im", "stil\u00b7len", "ho\u00b7cher\u00b7freut"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Das, was seine Schuldigkeit,", "tokens": ["Das", ",", "was", "sei\u00b7ne", "Schul\u00b7dig\u00b7keit", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Steht dann eines Morgens da", "tokens": ["Steht", "dann", "ei\u00b7nes", "Mor\u00b7gens", "da"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Als ein Vater und Papa", "tokens": ["Als", "ein", "Va\u00b7ter", "und", "Pa\u00b7pa"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Und ist froh aus Herzensgrund,", "tokens": ["Und", "ist", "froh", "aus", "Her\u00b7zens\u00b7grund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Da\u00df er dies so gut gekunnt.", "tokens": ["Da\u00df", "er", "dies", "so", "gut", "ge\u00b7kunnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}