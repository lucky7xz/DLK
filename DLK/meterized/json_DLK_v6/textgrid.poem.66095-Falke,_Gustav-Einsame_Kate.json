{"textgrid.poem.66095": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Einsame Kate", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Nebel braut \u00fcber nacktem Land,", "tokens": ["Der", "Ne\u00b7bel", "braut", "\u00fc\u00b7ber", "nack\u00b7tem", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Man sieht die schwarzen Schollen kaum.", "tokens": ["Man", "sieht", "die", "schwar\u00b7zen", "Schol\u00b7len", "kaum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie eine dicke graue Wand", "tokens": ["Wie", "ei\u00b7ne", "di\u00b7cke", "grau\u00b7e", "Wand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ragt der Wald \u00fcberm Ackersaum.", "tokens": ["Ragt", "der", "Wald", "\u00fc\u00b7berm", "A\u00b7cker\u00b7saum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Hinter kahlen Hecken versteckt,", "tokens": ["Hin\u00b7ter", "kah\u00b7len", "He\u00b7cken", "ver\u00b7steckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Kauert eine Kate, niedrig und schief,", "tokens": ["Kau\u00b7ert", "ei\u00b7ne", "Ka\u00b7te", ",", "nied\u00b7rig", "und", "schief", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Als ob sie, vom Nebel zugedeckt,", "tokens": ["Als", "ob", "sie", ",", "vom", "Ne\u00b7bel", "zu\u00b7ge\u00b7deckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Den ganzen Winter so verschlief.", "tokens": ["Den", "gan\u00b7zen", "Win\u00b7ter", "so", "ver\u00b7schlief", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Zwei Weiden str\u00e4uben ihr sp\u00e4rlich Ge\u00e4st", "tokens": ["Zwei", "Wei\u00b7den", "str\u00e4u\u00b7ben", "ihr", "sp\u00e4r\u00b7lich", "Ge\u00b7\u00e4st"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "PPER", "ADJD", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vor der T\u00fcr, ein morsches Stumpfenpaar.", "tokens": ["Vor", "der", "T\u00fcr", ",", "ein", "mor\u00b7sches", "Stum\u00b7pfen\u00b7paar", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Eine alte Kr\u00e4he hockt hier fest,", "tokens": ["Ei\u00b7ne", "al\u00b7te", "Kr\u00e4\u00b7he", "hockt", "hier", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Als hockte sie hier das ganze Jahr.", "tokens": ["Als", "hock\u00b7te", "sie", "hier", "das", "gan\u00b7ze", "Jahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Sie r\u00fchrt sich nicht, den Balg gebl\u00e4ht,", "tokens": ["Sie", "r\u00fchrt", "sich", "nicht", ",", "den", "Balg", "ge\u00b7bl\u00e4ht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKNEG", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den grauen Kopf tief eingeduckt.", "tokens": ["Den", "grau\u00b7en", "Kopf", "tief", "ein\u00b7ge\u00b7duckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur ihr schl\u00e4friges Blinzeln verr\u00e4t,", "tokens": ["Nur", "ihr", "schl\u00e4f\u00b7ri\u00b7ges", "Blin\u00b7zeln", "ver\u00b7r\u00e4t", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Da\u00df sie der Tod noch nicht verschluckt!", "tokens": ["Da\u00df", "sie", "der", "Tod", "noch", "nicht", "ver\u00b7schluckt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Einmal wippt sie ein wenig vor,", "tokens": ["Ein\u00b7mal", "wippt", "sie", "ein", "we\u00b7nig", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "PIS", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Hockt aber gleich wieder hin. Es war", "tokens": ["Hockt", "a\u00b7ber", "gleich", "wie\u00b7der", "hin", ".", "Es", "war"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "ADV", "PTKVZ", "$.", "PPER", "VAFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ja nur das alte Katentor,", "tokens": ["Ja", "nur", "das", "al\u00b7te", "Ka\u00b7ten\u00b7tor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das knarrte und quarrte. Keine Gefahr.", "tokens": ["Das", "knarr\u00b7te", "und", "quarr\u00b7te", ".", "Kei\u00b7ne", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "$.", "PIAT", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Ein altes M\u00fctterchen hinkt aus dem Haus,", "tokens": ["Ein", "al\u00b7tes", "M\u00fct\u00b7ter\u00b7chen", "hinkt", "aus", "dem", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Blinzt bl\u00f6de in die Nebelwelt", "tokens": ["Blinzt", "bl\u00f6\u00b7de", "in", "die", "Ne\u00b7bel\u00b7welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJA", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und streicht sich das Haar aus der Stirn heraus,", "tokens": ["Und", "streicht", "sich", "das", "Haar", "aus", "der", "Stirn", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Gelbgraue Str\u00e4hne. Das f\u00e4llt, wies f\u00e4llt.", "tokens": ["Gelb\u00b7grau\u00b7e", "Str\u00e4h\u00b7ne", ".", "Das", "f\u00e4llt", ",", "wies", "f\u00e4llt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PDS", "VVFIN", "$,", "VVFIN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Ein winziges Beet, schwarz und feucht,", "tokens": ["Ein", "win\u00b7zi\u00b7ges", "Beet", ",", "schwarz", "und", "feucht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Liegt vor der T\u00fcr. Sie scharrt dabei", "tokens": ["Liegt", "vor", "der", "T\u00fcr", ".", "Sie", "scharrt", "da\u00b7bei"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "$.", "PPER", "VVFIN", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Herum und purrt um ein wei\u00dfes Geleucht,", "tokens": ["He\u00b7rum", "und", "purrt", "um", "ein", "wei\u00b7\u00dfes", "Ge\u00b7leucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Erste Schneegl\u00f6ckchen, zwei, drei.", "tokens": ["Ers\u00b7te", "Schnee\u00b7gl\u00f6ck\u00b7chen", ",", "zwei", ",", "drei", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "CARD", "$,", "CARD", "$."], "meter": "+-+---+", "measure": "unknown.measure.tri"}}, "stanza.8": {"line.1": {"text": "Sie schn\u00e4uzt sich, f\u00e4hrt mit der flachen Hand", "tokens": ["Sie", "schn\u00e4uzt", "sich", ",", "f\u00e4hrt", "mit", "der", "fla\u00b7chen", "Hand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "$,", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u00dcber das welke Runzelgesicht,", "tokens": ["\u00dc\u00b7ber", "das", "wel\u00b7ke", "Run\u00b7zel\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Hustet und spuckt in den nassen Sand.", "tokens": ["Hus\u00b7tet", "und", "spuckt", "in", "den", "nas\u00b7sen", "Sand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Der Nebel ist gar zu schwer und dicht.", "tokens": ["Der", "Ne\u00b7bel", "ist", "gar", "zu", "schwer", "und", "dicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "H\u00fcstelnd hinkt sie ins Haus. Das Tor", "tokens": ["H\u00fcs\u00b7telnd", "hinkt", "sie", "ins", "Haus", ".", "Das", "Tor"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPRART", "NN", "$.", "ART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Kreischt kl\u00e4glich, wie kleine Kinder schrein.", "tokens": ["Kreischt", "kl\u00e4g\u00b7lich", ",", "wie", "klei\u00b7ne", "Kin\u00b7der", "schrein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "PWAV", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Kr\u00e4he wippt ein wenig vor,", "tokens": ["Die", "Kr\u00e4\u00b7he", "wippt", "ein", "we\u00b7nig", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schl\u00e4gt mit den Fl\u00fcgeln und duckt wieder ein.", "tokens": ["Schl\u00e4gt", "mit", "den", "Fl\u00fc\u00b7geln", "und", "duckt", "wie\u00b7der", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "KON", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.10": {"line.1": {"text": "Kein Laut. Die alte Kate liegt", "tokens": ["Kein", "Laut", ".", "Die", "al\u00b7te", "Ka\u00b7te", "liegt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "$.", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie tot, es piept nicht eine Maus.", "tokens": ["Wie", "tot", ",", "es", "piept", "nicht", "ei\u00b7ne", "Maus", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "PPER", "VVFIN", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jetzt ein Kr\u00e4chzen. Schwerf\u00e4llig fliegt", "tokens": ["Jetzt", "ein", "Kr\u00e4ch\u00b7zen", ".", "Schwer\u00b7f\u00e4l\u00b7lig", "fliegt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "NN", "$.", "ADJD", "VVFIN"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Die Kr\u00e4he in den Nebel hinaus.", "tokens": ["Die", "Kr\u00e4\u00b7he", "in", "den", "Ne\u00b7bel", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}}}}