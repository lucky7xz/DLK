{"textgrid.poem.53241": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Unterth\u00e4nigste Gl\u00fccks-Vermuthung, bey Chur-Printzl. Durchl. zu Brandenb. Hn. Hn. Carl Aemyl, unsers gn\u00e4digsten Chur-Printzen und Herren, den 30. OsterM. A.C. 1655. zu C\u00f6lln an der Spree angestellten Heil. Tauff-Feyer, zu K\u00f6nigsberg in Preussen dem\u00fchtigst gesch\u00f6pffet", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott entbrennet im Gem\u00fcte", "tokens": ["Gott", "ent\u00b7bren\u00b7net", "im", "Ge\u00b7m\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nie so hefftig auff ein Land,", "tokens": ["Nie", "so", "heff\u00b7tig", "auff", "ein", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df nicht V\u00e4terliche G\u00fcte", "tokens": ["Da\u00df", "nicht", "V\u00e4\u00b7ter\u00b7li\u00b7che", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stets dabey werd' eingewandt:", "tokens": ["Stets", "da\u00b7bey", "werd'", "ein\u00b7ge\u00b7wandt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da ist Straff auff unsre Schuld,", "tokens": ["Da", "ist", "Straff", "auff", "uns\u00b7re", "Schuld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Hie Erbarmen und Gedult.", "tokens": ["Hie", "Er\u00b7bar\u00b7men", "und", "Ge\u00b7dult", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Mitten in dem Krieges-Fewer,", "tokens": ["Mit\u00b7ten", "in", "dem", "Krie\u00b7ge\u00b7sFe\u00b7wer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der K\u00fcmmerlichen Zeit,", "tokens": ["In", "der", "K\u00fcm\u00b7mer\u00b7li\u00b7chen", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die uns alles Ungehewer", "tokens": ["Die", "uns", "al\u00b7les", "Un\u00b7ge\u00b7he\u00b7wer"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "PIAT", "NN"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Pl\u00fcndern, Brand und Elend dreut,", "tokens": ["Pl\u00fcn\u00b7dern", ",", "Brand", "und", "E\u00b7lend", "dreut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "So wirst du, der V\u00f6lcker Heil,", "tokens": ["So", "wirst", "du", ",", "der", "V\u00f6l\u00b7cker", "Heil", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "ART", "NN", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Chur-Printz, eben uns zu Theil,", "tokens": ["Chur\u00b7Printz", ",", "e\u00b7ben", "uns", "zu", "Theil", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Machst, da\u00df wir die Furcht gelosen,", "tokens": ["Machst", ",", "da\u00df", "wir", "die", "Furcht", "ge\u00b7lo\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So uns scheint zu \u00fcberziehn.", "tokens": ["So", "uns", "scheint", "zu", "\u00fc\u00b7ber\u00b7ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Also tr\u00e4gt ein Dorn-Pusch Rosen,", "tokens": ["Al\u00b7so", "tr\u00e4gt", "ein", "Dorn\u00b7Pusch", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "S\u00fcssen Honig eine Bien'", "tokens": ["S\u00fcs\u00b7sen", "Ho\u00b7nig", "ei\u00b7ne", "Bi\u00b7en'"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "ART", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Und in einem wilden Streit", "tokens": ["Und", "in", "ei\u00b7nem", "wil\u00b7den", "Streit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Bl\u00fcht die Siegs-Kron' allezeit.", "tokens": ["Bl\u00fcht", "die", "Siegs\u00b7Kron'", "al\u00b7le\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Lang ist schon umb dich gebehten,", "tokens": ["Lang", "ist", "schon", "umb", "dich", "ge\u00b7beh\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn sind unsre Seufftzer nicht", "tokens": ["Wenn", "sind", "uns\u00b7re", "Seufft\u00b7zer", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "PPOSAT", "NN", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wegen dein vor Gott getreten?", "tokens": ["We\u00b7gen", "dein", "vor", "Gott", "ge\u00b7tre\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nein, nun schier der Mastbaum bricht,", "tokens": ["Nein", ",", "nun", "schier", "der", "Mast\u00b7baum", "bricht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schafft der h\u00f6chste Gott, da\u00df man", "tokens": ["Schafft", "der", "h\u00f6chs\u00b7te", "Gott", ",", "da\u00df", "man"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "KOUS", "PIS"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dich, O Stern, erblicken kan.", "tokens": ["Dich", ",", "O", "Stern", ",", "er\u00b7bli\u00b7cken", "kan", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "NE", "NN", "$,", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Zwar von unsers Wandels Sachen,", "tokens": ["Zwar", "von", "un\u00b7sers", "Wan\u00b7dels", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der nur Frevel, k\u00f6nnen wir", "tokens": ["Der", "nur", "Fre\u00b7vel", ",", "k\u00f6n\u00b7nen", "wir"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADV", "NN", "$,", "VMFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Uns gar schlechte Rechnung machen,", "tokens": ["Uns", "gar", "schlech\u00b7te", "Rech\u00b7nung", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch k\u00f6mpt dieser Trost mit dir,", "tokens": ["Doch", "k\u00f6mpt", "die\u00b7ser", "Trost", "mit", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDAT", "NN", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df Gott mitten in der Pein", "tokens": ["Da\u00df", "Gott", "mit\u00b7ten", "in", "der", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nicht zu streng' uns werde seyn,", "tokens": ["Nicht", "zu", "streng'", "uns", "wer\u00b7de", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PTKZU", "VVFIN", "PPER", "VAFIN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Uns nicht g\u00e4ntzlich \u00fcbergeben", "tokens": ["Uns", "nicht", "g\u00e4ntz\u00b7lich", "\u00fc\u00b7ber\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "PTKNEG", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In Verw\u00fcstung, Raub und Schwerd.", "tokens": ["In", "Ver\u00b7w\u00fcs\u00b7tung", ",", "Raub", "und", "Schwerd", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Er verleihe dir nur Leben", "tokens": ["Er", "ver\u00b7lei\u00b7he", "dir", "nur", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und des Geistes hohen Werth,", "tokens": ["Und", "des", "Geis\u00b7tes", "ho\u00b7hen", "Werth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der dich \u00fcber alles hebt,", "tokens": ["Der", "dich", "\u00fc\u00b7ber", "al\u00b7les", "hebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Was sonst hoch auff Erden schwebt.", "tokens": ["Was", "sonst", "hoch", "auff", "Er\u00b7den", "schwebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Der, wenn alles Glut wird fassen,", "tokens": ["Der", ",", "wenn", "al\u00b7les", "Glut", "wird", "fas\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PIS", "NN", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was wir sehen weit und breit,", "tokens": ["Was", "wir", "se\u00b7hen", "weit", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dich auch dort wird herschen lassen", "tokens": ["Dich", "auch", "dort", "wird", "her\u00b7schen", "las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADV", "VAFIN", "VVINF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In der g\u00fcldnen Ewigkeit,", "tokens": ["In", "der", "g\u00fcld\u00b7nen", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und verschaffen, da\u00df auch hier", "tokens": ["Und", "ver\u00b7schaf\u00b7fen", ",", "da\u00df", "auch", "hier"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVINF", "$,", "KOUS", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nichts dir gleich' an Pracht und Zier.", "tokens": ["Nichts", "dir", "gleich'", "an", "Pracht", "und", "Zier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PPER", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Hierzu nam der Bund der Gnaden", "tokens": ["Hier\u00b7zu", "nam", "der", "Bund", "der", "Gna\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dich in diesen Tagen auff,", "tokens": ["Dich", "in", "die\u00b7sen", "Ta\u00b7gen", "auff", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PDAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der der Erb-Schuld dich entladen,", "tokens": ["Der", "der", "Er\u00b7bSchuld", "dich", "ent\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Dich geheiligt durch die Tauff',", "tokens": ["Dich", "ge\u00b7hei\u00b7ligt", "durch", "die", "Tauff'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ohne die vor Gott August", "tokens": ["Oh\u00b7ne", "die", "vor", "Gott", "Au\u00b7gust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "APPR", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Selber ist nur Koth und Wust.", "tokens": ["Sel\u00b7ber", "ist", "nur", "Koth", "und", "Wust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Wer nicht so wird new gebohren,", "tokens": ["Wer", "nicht", "so", "wird", "new", "ge\u00b7boh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "ADV", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mu\u00df vergehn, und w\u00e4r' er gleich", "tokens": ["Mu\u00df", "ver\u00b7gehn", ",", "und", "w\u00e4r'", "er", "gleich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "VVINF", "$,", "KON", "VAFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mehr als K\u00f6niglich erkohren", "tokens": ["Mehr", "als", "K\u00f6\u00b7nig\u00b7lich", "er\u00b7koh\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "KOKOM", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und h\u00e4tt' aller Erden Reich,", "tokens": ["Und", "h\u00e4tt'", "al\u00b7ler", "Er\u00b7den", "Reich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo die Sonne fr\u00fch' entsteht", "tokens": ["Wo", "die", "Son\u00b7ne", "fr\u00fch'", "ent\u00b7steht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJD", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und des Abends untergeht.", "tokens": ["Und", "des", "A\u00b7bends", "un\u00b7ter\u00b7geht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Hiedurch lernt ein F\u00fcrst sich zwingen,", "tokens": ["Hie\u00b7durch", "lernt", "ein", "F\u00fcrst", "sich", "zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eh' er wird der V\u00f6lcker Zwang,", "tokens": ["Eh'", "er", "wird", "der", "V\u00f6l\u00b7cker", "Zwang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird und bleibt f\u00fcr allen Dingen", "tokens": ["Wird", "und", "bleibt", "f\u00fcr", "al\u00b7len", "Din\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "KON", "VVFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seiner L\u00fcste Zucht und Drang,", "tokens": ["Sei\u00b7ner", "L\u00fcs\u00b7te", "Zucht", "und", "Drang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Eh' er \u00fcber Leut' und Land", "tokens": ["Eh'", "er", "\u00fc\u00b7ber", "Leut'", "und", "Land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Streckt die Herrschafft seiner Hand,", "tokens": ["Streckt", "die", "Herr\u00b7schafft", "sei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "H\u00e4lt von Gottes Bahn geschritten", "tokens": ["H\u00e4lt", "von", "Got\u00b7tes", "Bahn", "ge\u00b7schrit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fcr die allerh\u00f6chste Pein,", "tokens": ["F\u00fcr", "die", "al\u00b7ler\u00b7h\u00f6chs\u00b7te", "Pein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gottes, welcher seiner Sitten", "tokens": ["Got\u00b7tes", ",", "wel\u00b7cher", "sei\u00b7ner", "Sit\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Richtscheid und Patron mu\u00df seyn", "tokens": ["Richt\u00b7scheid", "und", "Pat\u00b7ron", "mu\u00df", "seyn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VMFIN", "PPOSAT"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Und ein Licht, das seinem Fu\u00df", "tokens": ["Und", "ein", "Licht", ",", "das", "sei\u00b7nem", "Fu\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nimmer untergehen mu\u00df.", "tokens": ["Nim\u00b7mer", "un\u00b7ter\u00b7ge\u00b7hen", "mu\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Wol uns, wol, wenn nun bey Zeiten", "tokens": ["Wol", "uns", ",", "wol", ",", "wenn", "nun", "bey", "Zei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "$,", "ADV", "$,", "KOUS", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dich, O Kind, des Himmels Pfand,", "tokens": ["Dich", ",", "O", "Kind", ",", "des", "Him\u00b7mels", "Pfand", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NE", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Selbst die Gottesfurcht wird leiten", "tokens": ["Selbst", "die", "Got\u00b7tes\u00b7furcht", "wird", "lei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VAFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An der Unschuld-reichen Hand,", "tokens": ["An", "der", "Un\u00b7schuld\u00b7rei\u00b7chen", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und die Tritte, die du thust,", "tokens": ["Und", "die", "Trit\u00b7te", ",", "die", "du", "thust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "Werden seyn nur Tugend-Lust!", "tokens": ["Wer\u00b7den", "seyn", "nur", "Tu\u00b7gen\u00b7dLust", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Dieses wird uns ba\u00df gefallen,", "tokens": ["Die\u00b7ses", "wird", "uns", "ba\u00df", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ba\u00df uns stillen, als wenn du", "tokens": ["Ba\u00df", "uns", "stil\u00b7len", ",", "als", "wenn", "du"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "KOKOM", "KOUS", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00f6chtest reissen stracks f\u00fcr allen", "tokens": ["M\u00f6ch\u00b7test", "reis\u00b7sen", "stracks", "f\u00fcr", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "APPR", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auff die blancken Degen zu,", "tokens": ["Auff", "die", "blan\u00b7cken", "De\u00b7gen", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Welches man zwar allermeist", "tokens": ["Wel\u00b7ches", "man", "zwar", "al\u00b7ler\u00b7meist"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "PIS", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Tapffer, doch auch grausam heist.", "tokens": ["Tapf\u00b7fer", ",", "doch", "auch", "grau\u00b7sam", "heist", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "ADV", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Wolte Gott, dein gantzes Leben", "tokens": ["Wol\u00b7te", "Gott", ",", "dein", "gant\u00b7zes", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00e4re nichts als Sicherheit,", "tokens": ["W\u00e4\u00b7re", "nichts", "als", "Si\u00b7cher\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "KOKOM", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df kein Schwerdt w\u00e4r' auffzuheben,", "tokens": ["Da\u00df", "kein", "Schwerdt", "w\u00e4r'", "auff\u00b7zu\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ohn wenn selbst das Recht gebeut", "tokens": ["Ohn", "wenn", "selbst", "das", "Recht", "ge\u00b7beut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ADV", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und die Laster, die allein", "tokens": ["Und", "die", "Las\u00b7ter", ",", "die", "al\u00b7lein"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "So geb\u00fcsset wollen seyn.", "tokens": ["So", "ge\u00b7b\u00fcs\u00b7set", "wol\u00b7len", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VMFIN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Nichts ist so gew\u00fcndscht auff Erden,", "tokens": ["Nichts", "ist", "so", "ge\u00b7w\u00fcnd\u00b7scht", "auff", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "HeRR, wir hoffen es von dir,", "tokens": ["HeRR", ",", "wir", "hof\u00b7fen", "es", "von", "dir", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "PPER", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn du nicht wirst anders werden,", "tokens": ["Denn", "du", "nicht", "wirst", "an\u00b7ders", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKNEG", "VAFIN", "ADV", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als die hohe Tugend-Zier", "tokens": ["Als", "die", "ho\u00b7he", "Tu\u00b7gen\u00b7dZier"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Deiner Eltern, die das Licht", "tokens": ["Dei\u00b7ner", "El\u00b7tern", ",", "die", "das", "Licht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dir ertheilen, uns verspricht.", "tokens": ["Dir", "er\u00b7thei\u00b7len", ",", "uns", "ver\u00b7spricht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Ihr Verstand und thewre Gaben,", "tokens": ["Ihr", "Ver\u00b7stand", "und", "thew\u00b7re", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die weit \u00fcber Menschen sind", "tokens": ["Die", "weit", "\u00fc\u00b7ber", "Men\u00b7schen", "sind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPR", "NN", "VAFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und au\u00df Gott den Ursprung haben,", "tokens": ["Und", "au\u00df", "Gott", "den", "Ur\u00b7sprung", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schaffen, da\u00df auch du, O Kind,", "tokens": ["Schaf\u00b7fen", ",", "da\u00df", "auch", "du", ",", "O", "Kind", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ADV", "PPER", "$,", "NE", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Also bald du lernest stehn,", "tokens": ["Al\u00b7so", "bald", "du", "ler\u00b7nest", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "M\u00f6gest ihren Fu\u00dfpfad gehn.", "tokens": ["M\u00f6\u00b7gest", "ih\u00b7ren", "Fu\u00df\u00b7pfad", "gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "O der hochgew\u00fcnschten Stunden,", "tokens": ["O", "der", "hoch\u00b7ge\u00b7w\u00fcnschten", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Wenn der grosse Vater nun", "tokens": ["Wenn", "der", "gros\u00b7se", "Va\u00b7ter", "nun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seiner SorgenLast entbunden,", "tokens": ["Sei\u00b7ner", "Sor\u00b7gen", "Last", "ent\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dir sein Stamm-Hau\u00df kunt wird thun", "tokens": ["Dir", "sein", "Stam\u00b7mHau\u00df", "kunt", "wird", "thun"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "NN", "PTKVZ", "VAFIN", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit Bericht, da\u00df dieses frey", "tokens": ["Mit", "Be\u00b7richt", ",", "da\u00df", "die\u00b7ses", "frey"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "KOUS", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Aller Tugend Schauplatz sey!", "tokens": ["Al\u00b7ler", "Tu\u00b7gend", "Schau\u00b7platz", "sey", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "Wenn der hohen Mutter G\u00fcte", "tokens": ["Wenn", "der", "ho\u00b7hen", "Mut\u00b7ter", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch nach ihres Hauses Art", "tokens": ["Auch", "nach", "ih\u00b7res", "Hau\u00b7ses", "Art"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dir wird bilden das Gem\u00fcte,", "tokens": ["Dir", "wird", "bil\u00b7den", "das", "Ge\u00b7m\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVINF", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Du dabey auch jung und zart", "tokens": ["Du", "da\u00b7bey", "auch", "jung", "und", "zart"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PAV", "ADV", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dahin beugsam wirst gesp\u00fcrt,", "tokens": ["Da\u00b7hin", "beug\u00b7sam", "wirst", "ge\u00b7sp\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJD", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo dich ihre Zucht hinf\u00fchrt!", "tokens": ["Wo", "dich", "ih\u00b7re", "Zucht", "hin\u00b7f\u00fchrt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.19": {"line.1": {"text": "Grosse Lichter dieser Erden,", "tokens": ["Gros\u00b7se", "Lich\u00b7ter", "die\u00b7ser", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Churf\u00fcrst und auch Churf\u00fcrstinn,", "tokens": ["Chur\u00b7f\u00fcrst", "und", "auch", "Chur\u00b7f\u00fcrs\u00b7tinn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADV", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Schutz und Zuflucht ewrer Herden,", "tokens": ["Schutz", "und", "Zu\u00b7flucht", "ew\u00b7rer", "Her\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Haltet \u00fcber diesem Sinn,", "tokens": ["Hal\u00b7tet", "\u00fc\u00b7ber", "die\u00b7sem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Macht, da\u00df seine Schritt' allein", "tokens": ["Macht", ",", "da\u00df", "sei\u00b7ne", "Schritt'", "al\u00b7lein"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPOSAT", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Unschuld, Recht und Liebe seyn.", "tokens": ["Un\u00b7schuld", ",", "Recht", "und", "Lie\u00b7be", "seyn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "NN", "KON", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.20": {"line.1": {"text": "Mehrt in ihm der Tugend Flammen,", "tokens": ["Mehrt", "in", "ihm", "der", "Tu\u00b7gend", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mischet Gottesfurcht und Treu'", "tokens": ["Mi\u00b7schet", "Got\u00b7tes\u00b7furcht", "und", "Treu'"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Allzeit in die Milch der Ammen,", "tokens": ["All\u00b7zeit", "in", "die", "Milch", "der", "Am\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bringt die Einbildung ihm bey,", "tokens": ["Bringt", "die", "Ein\u00b7bil\u00b7dung", "ihm", "bey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Da\u00df er Gott mit Lust und Wahn", "tokens": ["Da\u00df", "er", "Gott", "mit", "Lust", "und", "Wahn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Einig seyn m\u00fcss' unterthan,", "tokens": ["Ei\u00b7nig", "seyn", "m\u00fcss'", "un\u00b7ter\u00b7than", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAINF", "VMFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.21": {"line.1": {"text": "Da\u00df er seinen Untersassen", "tokens": ["Da\u00df", "er", "sei\u00b7nen", "Un\u00b7ter\u00b7sas\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00fcss' ein Bild seyn jederzeit,", "tokens": ["M\u00fcss'", "ein", "Bild", "seyn", "je\u00b7der\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die von ihm Exempel fassen.", "tokens": ["Die", "von", "ihm", "Ex\u00b7em\u00b7pel", "fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "NN", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Liebet Er Gerechtigkeit,", "tokens": ["Lie\u00b7bet", "Er", "Ge\u00b7rech\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wird Gewalt und Unrecht nie", "tokens": ["Wird", "Ge\u00b7walt", "und", "Un\u00b7recht", "nie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "KON", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Leichtlich herrschen \u00fcber sie.", "tokens": ["Leicht\u00b7lich", "herr\u00b7schen", "\u00fc\u00b7ber", "sie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.22": {"line.1": {"text": "Ist Er feind der Venus S\u00fcnden,", "tokens": ["Ist", "Er", "feind", "der", "Ve\u00b7nus", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Es wird sich der Unzucht Ha\u00df", "tokens": ["Es", "wird", "sich", "der", "Un\u00b7zucht", "Ha\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PRF", "ART", "NN", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Auch bey ihnen leichtlich finden:", "tokens": ["Auch", "bey", "ih\u00b7nen", "leicht\u00b7lich", "fin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wer liebt wol Ges\u00f6ff und Fra\u00df,", "tokens": ["Wer", "liebt", "wol", "Ge\u00b7s\u00f6ff", "und", "Fra\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Sieht er nie von vielem Wein", "tokens": ["Sieht", "er", "nie", "von", "vie\u00b7lem", "Wein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PIS", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Seinen F\u00fcrsten truncken seyn?", "tokens": ["Sei\u00b7nen", "F\u00fcrs\u00b7ten", "trun\u00b7cken", "seyn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.23": {"line.1": {"text": "Ja kein Schiffer sieht so eben", "tokens": ["Ja", "kein", "Schif\u00b7fer", "sieht", "so", "e\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "PIAT", "NN", "VVFIN", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach dem Nord-Stern auff der Fluth,", "tokens": ["Nach", "dem", "Nord\u00b7S\u00b7tern", "auff", "der", "Fluth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als wir s\u00e4mptlich Achtung geben", "tokens": ["Als", "wir", "s\u00e4mpt\u00b7lich", "Ach\u00b7tung", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auff den F\u00fcrsten, was er thut,", "tokens": ["Auff", "den", "F\u00fcrs\u00b7ten", ",", "was", "er", "thut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ihn sieht allzeit jederman", "tokens": ["Ihn", "sieht", "all\u00b7zeit", "je\u00b7der\u00b7man"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PIS"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Wol mit hundert Augen an.", "tokens": ["Wol", "mit", "hun\u00b7dert", "Au\u00b7gen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "CARD", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.24": {"line.1": {"text": "Nirgends kan er seyn verborgen,", "tokens": ["Nir\u00b7gends", "kan", "er", "seyn", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VAINF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Allenthalben nimmt man war", "tokens": ["Al\u00b7len\u00b7thal\u00b7ben", "nimmt", "man", "war"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seiner Wercke, seiner Sorgen.", "tokens": ["Sei\u00b7ner", "Wer\u00b7cke", ",", "sei\u00b7ner", "Sor\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn die Sonne scheint doch klar,", "tokens": ["Denn", "die", "Son\u00b7ne", "scheint", "doch", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn sie sich gleich umb die Nacht", "tokens": ["Wenn", "sie", "sich", "gleich", "umb", "die", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Fern von hinnen hat gemacht.", "tokens": ["Fern", "von", "hin\u00b7nen", "hat", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADV", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.25": {"line.1": {"text": "Herren, welche die\u00df bedencken,", "tokens": ["Her\u00b7ren", ",", "wel\u00b7che", "die\u00df", "be\u00b7den\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PDS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Werden ihren Sinn so bald", "tokens": ["Wer\u00b7den", "ih\u00b7ren", "Sinn", "so", "bald"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht auff etwas b\u00f6ses lencken.", "tokens": ["Nicht", "auff", "et\u00b7was", "b\u00f6\u00b7ses", "len\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PIAT", "ADJA", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Numa stellt ihm diesen Halt,", "tokens": ["Nu\u00b7ma", "stellt", "ihm", "die\u00b7sen", "Halt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Darumb nam auch Rom an Ruh',", "tokens": ["Da\u00b7rumb", "nam", "auch", "Rom", "an", "Ruh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "NE", "APPR", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Ansehn', Macht und Segen zu.", "tokens": ["An\u00b7sehn'", ",", "Macht", "und", "Se\u00b7gen", "zu", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.26": {"line.1": {"text": "Constantin und Carl die Grossen", "tokens": ["Con\u00b7stan\u00b7tin", "und", "Carl", "die", "Gros\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KON", "NE", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Haben auch durch dieses Mal", "tokens": ["Ha\u00b7ben", "auch", "durch", "die\u00b7ses", "Mal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihre h\u00f6chst-Gewalt umbschlossen,", "tokens": ["Ih\u00b7re", "h\u00f6chst\u00b7Ge\u00b7walt", "umbsc\u00b7hlos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich gem\u00e4ssigt \u00fcberall,", "tokens": ["Sich", "ge\u00b7m\u00e4s\u00b7sigt", "\u00fc\u00b7be\u00b7rall", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "VVPP", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Darumb ihre Macht auch trat", "tokens": ["Da\u00b7rumb", "ih\u00b7re", "Macht", "auch", "trat"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "PPOSAT", "NN", "ADV", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Weit, weit \u00fcber den Euphrat.", "tokens": ["Weit", ",", "weit", "\u00fc\u00b7ber", "den", "Eu\u00b7ph\u00b7rat", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "APPR", "ART", "NE", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.27": {"line.1": {"text": "Sie sind friedlich hingestorben,", "tokens": ["Sie", "sind", "fried\u00b7lich", "hin\u00b7ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo man sie nur sterblich h\u00e4lt,", "tokens": ["Wo", "man", "sie", "nur", "sterb\u00b7lich", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ihr Lob, das sie erworben,", "tokens": ["Und", "ihr", "Lob", ",", "das", "sie", "er\u00b7wor\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcllt noch jetzund alle Welt,", "tokens": ["F\u00fcllt", "noch", "je\u00b7tzund", "al\u00b7le", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da hingegen Tolch und Gifft", "tokens": ["Da", "hin\u00b7ge\u00b7gen", "Tolch", "und", "Gifft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "NN", "KON", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Meistentheils Tyrannen trifft,", "tokens": ["Meis\u00b7ten\u00b7theils", "Ty\u00b7ran\u00b7nen", "trifft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.28": {"line.1": {"text": "Das nicht schadet frommen Herren,", "tokens": ["Das", "nicht", "scha\u00b7det", "from\u00b7men", "Her\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil die Liebe sie bewacht.", "tokens": ["Weil", "die", "Lie\u00b7be", "sie", "be\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nichts verf\u00e4ngt doch sich versperren", "tokens": ["Nichts", "ver\u00b7f\u00e4ngt", "doch", "sich", "ver\u00b7sper\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch der Partisanen Macht,", "tokens": ["Durch", "der", "Par\u00b7ti\u00b7sa\u00b7nen", "Macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ist der Untherthanen Treu',", "tokens": ["Ist", "der", "Un\u00b7ther\u00b7tha\u00b7nen", "Treu'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Huld' und Liebe nicht dabey,", "tokens": ["Huld'", "und", "Lie\u00b7be", "nicht", "da\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "PTKNEG", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.29": {"line.1": {"text": "Welche dich, Kind, wird bewachen,", "tokens": ["Wel\u00b7che", "dich", ",", "Kind", ",", "wird", "be\u00b7wa\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "$,", "NN", "$,", "VAFIN", "VVINF", "$,"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Weil der Unschuld Zucht allein", "tokens": ["Weil", "der", "Un\u00b7schuld", "Zucht", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deine Gn\u00fcg' in allen Sachen,", "tokens": ["Dei\u00b7ne", "Gn\u00fcg'", "in", "al\u00b7len", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deine Frewd und Lust wird seyn,", "tokens": ["Dei\u00b7ne", "Frewd", "und", "Lust", "wird", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VAFIN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und der H\u00f6chst' ohn unterla\u00df", "tokens": ["Und", "der", "H\u00f6chst'", "ohn", "un\u00b7ter\u00b7la\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Deiner Werck' und Sinnen Ma\u00df.", "tokens": ["Dei\u00b7ner", "Werck", "und", "Sin\u00b7nen", "Ma\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}