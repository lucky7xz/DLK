{"textgrid.poem.52858": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1. Gesang", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Zirkelschmied beim Sch\u00f6pplein sa\u00df;", "tokens": ["Der", "Zir\u00b7kelsc\u00b7hmied", "beim", "Sch\u00f6p\u00b7plein", "sa\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jetzt, Schwanenwirth, f\u00fcllt mir das gro\u00dfe Glas,", "tokens": ["Jetzt", ",", "Schwa\u00b7nen\u00b7wirth", ",", "f\u00fcllt", "mir", "das", "gro\u00b7\u00dfe", "Glas", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Ich will den Herrn was erz\u00e4hlen,", "tokens": ["Ich", "will", "den", "Herrn", "was", "er\u00b7z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PIS", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "'s ist was f\u00fcr durstige Kehlen!", "tokens": ["'s", "ist", "was", "f\u00fcr", "durs\u00b7ti\u00b7ge", "Keh\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Heut Nacht war der Schwanenwirth, so begann", "tokens": ["Heut", "Nacht", "war", "der", "Schwa\u00b7nen\u00b7wirth", ",", "so", "be\u00b7gann"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "NN", "VAFIN", "ART", "NN", "$,", "ADV", "VVFIN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Zirkelschmied, ein gewaltiger Mann;", "tokens": ["Der", "Zir\u00b7kelsc\u00b7hmied", ",", "ein", "ge\u00b7wal\u00b7ti\u00b7ger", "Mann", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Mir tr\u00e4umte, er war der Noah -", "tokens": ["Mir", "tr\u00e4um\u00b7te", ",", "er", "war", "der", "Noah"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "ART", "NE", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "\"will nit hoffen - Tohu wab - oha!\"", "tokens": ["\"", "will", "nit", "hof\u00b7fen", "To\u00b7hu", "wab", "o\u00b7ha", "!", "\""], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VMFIN", "PTKNEG", "VVINF", "$(", "XY", "XY", "$(", "NE", "$.", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.3": {"line.1": {"text": "Der Noah war er, und wohlbekannt", "tokens": ["Der", "Noah", "war", "er", ",", "und", "wohl\u00b7be\u00b7kannt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NE", "VAFIN", "PPER", "$,", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als Schwanenwirth im Assyrerland,", "tokens": ["Als", "Schwa\u00b7nen\u00b7wirth", "im", "As\u00b7sy\u00b7rer\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich, auf des Herrn F\u00f6rsters Schimmel,", "tokens": ["Ich", ",", "auf", "des", "Herrn", "F\u00f6rs\u00b7ters", "Schim\u00b7mel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "ART", "NN", "NE", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ich war der Juppitter vom Himmel.", "tokens": ["Ich", "war", "der", "Jup\u00b7pit\u00b7ter", "vom", "Him\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Alle Hagel! Ihr seid gut aufger\u00e4umt,", "tokens": ["Al\u00b7le", "Ha\u00b7gel", "!", "Ihr", "seid", "gut", "auf\u00b7ge\u00b7r\u00e4umt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$.", "PPER", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Erz\u00e4hlt doch weiter, was Ihr getr\u00e4umt.", "tokens": ["Er\u00b7z\u00e4hlt", "doch", "wei\u00b7ter", ",", "was", "Ihr", "ge\u00b7tr\u00e4umt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Mach't nicht viel Federlesen,", "tokens": ["Mach't", "nicht", "viel", "Fe\u00b7der\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PIAT", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Was ist denn der F\u00f6rster gewesen?", "tokens": ["Was", "ist", "denn", "der", "F\u00f6rs\u00b7ter", "ge\u00b7we\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ART", "NN", "VAPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "Der F\u00f6rster? Das ist den Herren klar,", "tokens": ["Der", "F\u00f6rs\u00b7ter", "?", "Das", "ist", "den", "Her\u00b7ren", "klar", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PDS", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da\u00df der des Schwanenwirths Hausknecht war,", "tokens": ["Da\u00df", "der", "des", "Schwa\u00b7nen\u00b7wirths", "Haus\u00b7knecht", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ART", "NN", "NN", "VAFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "So durft' er die S\u00fcndfluth erleben,", "tokens": ["So", "durft'", "er", "die", "S\u00fcnd\u00b7fluth", "er\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Nun h\u00f6ret, wie das sich begeben.", "tokens": ["Nun", "h\u00f6\u00b7ret", ",", "wie", "das", "sich", "be\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWAV", "PDS", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Gar oft ritt der Hergott der freundliche Greis", "tokens": ["Gar", "oft", "ritt", "der", "Her\u00b7gott", "der", "freund\u00b7li\u00b7che", "Greis"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Am Schwanen vor\u00fcber und fragte: Was Neu's?", "tokens": ["Am", "Schwa\u00b7nen", "vor\u00b7\u00fc\u00b7ber", "und", "frag\u00b7te", ":", "Was", "Neu's", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "KON", "VVFIN", "$.", "PWS", "NE", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Gew\u00f6hnlich jedoch blieb er h\u00e4ngen,", "tokens": ["Ge\u00b7w\u00f6hn\u00b7lich", "je\u00b7doch", "blieb", "er", "h\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weil ihn die Gesch\u00e4fte nicht dr\u00e4ngen.", "tokens": ["Weil", "ihn", "die", "Ge\u00b7sch\u00e4f\u00b7te", "nicht", "dr\u00e4n\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.7": {"line.1": {"text": "Da schmauchte mit Dem, der die Welt regiert,", "tokens": ["Da", "schmauch\u00b7te", "mit", "Dem", ",", "der", "die", "Welt", "re\u00b7giert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Vortrefflichen Knaster der Schwanenwirth,", "tokens": ["Vor\u00b7treff\u00b7li\u00b7chen", "Knas\u00b7ter", "der", "Schwa\u00b7nen\u00b7wirth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und der Amtmann auch und der Helfer,", "tokens": ["Und", "der", "Amt\u00b7mann", "auch", "und", "der", "Hel\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "KON", "ART", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und tranken Achter und Zw\u00f6lfer.", "tokens": ["Und", "tran\u00b7ken", "Ach\u00b7ter", "und", "Zw\u00f6l\u00b7fer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Zumeist doch Achter, aus Sparsamkeit,", "tokens": ["Zu\u00b7meist", "doch", "Ach\u00b7ter", ",", "aus", "Spar\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Des Beispiels wegen wie and're Leut.", "tokens": ["Des", "Bei\u00b7spiels", "we\u00b7gen", "wie", "an\u00b7d'\u00b7re", "Leut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es gab noch keinen Burgunder -", "tokens": ["Es", "gab", "noch", "kei\u00b7nen", "Bur\u00b7gun\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "War aufgespart f\u00fcr ein Wunder.", "tokens": ["War", "auf\u00b7ge\u00b7spart", "f\u00fcr", "ein", "Wun\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Wenn Ihr mir recht brav und recht flei\u00dfig bleibt,", "tokens": ["Wenn", "Ihr", "mir", "recht", "brav", "und", "recht", "flei\u00b7\u00dfig", "bleibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADJD", "KON", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und immer so fort anst\u00e4ndig kneipt,", "tokens": ["Und", "im\u00b7mer", "so", "fort", "an\u00b7st\u00e4n\u00b7dig", "kneipt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PTKVZ", "ADJD", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Hat oft sich Gottvatter erboten,", "tokens": ["Hat", "oft", "sich", "Gott\u00b7vat\u00b7ter", "er\u00b7bo\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PRF", "NN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Dann, Kinder, dann gibt's auch Rothen!", "tokens": ["Dann", ",", "Kin\u00b7der", ",", "dann", "gibt's", "auch", "Ro\u00b7then", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "ADV", "VVFIN", "ADV", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "So ein Weinchen, g\u00f6ttlich! Ich bin nur noch", "tokens": ["So", "ein", "Wein\u00b7chen", ",", "g\u00f6tt\u00b7lich", "!", "Ich", "bin", "nur", "noch"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "ADJD", "$.", "PPER", "VAFIN", "ADV", "ADV"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit dem Jahrgang nicht im Reinen, jedoch", "tokens": ["Mit", "dem", "Jahr\u00b7gang", "nicht", "im", "Rei\u00b7nen", ",", "je\u00b7doch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "PTKNEG", "APPRART", "NN", "$,", "ADV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Soll er verj\u00fcngen die Alten", "tokens": ["Soll", "er", "ver\u00b7j\u00fcn\u00b7gen", "die", "Al\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "VVFIN", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Und jung die Jungen erhalten.", "tokens": ["Und", "jung", "die", "Jun\u00b7gen", "er\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.11": {"line.1": {"text": "Nun hatte der Noah sechs T\u00f6chter im Haus,", "tokens": ["Nun", "hat\u00b7te", "der", "Noah", "sechs", "T\u00f6ch\u00b7ter", "im", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NE", "CARD", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die sahen gar g\u00fctig und rothbackig aus,", "tokens": ["Die", "sa\u00b7hen", "gar", "g\u00fc\u00b7tig", "und", "roth\u00b7ba\u00b7ckig", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und waren gar fromm und anstellig,", "tokens": ["Und", "wa\u00b7ren", "gar", "fromm", "und", "an\u00b7stel\u00b7lig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-++-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und Gott und den Menschen gef\u00e4llig.", "tokens": ["Und", "Gott", "und", "den", "Men\u00b7schen", "ge\u00b7f\u00e4l\u00b7lig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "ART", "NN", "ADJD", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.12": {"line.1": {"text": "Der Alte war gn\u00e4dig und schmunzelte oft,", "tokens": ["Der", "Al\u00b7te", "war", "gn\u00e4\u00b7dig", "und", "schmun\u00b7zel\u00b7te", "oft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "VVFIN", "ADV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und kneift in die Backen sie unverhofft", "tokens": ["Und", "kneift", "in", "die", "Ba\u00b7cken", "sie", "un\u00b7ver\u00b7hofft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PPER", "ADJD"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und fragte sie auch, wie sie hei\u00dfen,", "tokens": ["Und", "frag\u00b7te", "sie", "auch", ",", "wie", "sie", "hei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "PWAV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ob sie beim K\u00fcssen nit bei\u00dfen?", "tokens": ["Und", "ob", "sie", "beim", "K\u00fcs\u00b7sen", "nit", "bei\u00b7\u00dfen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPRART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.13": {"line.1": {"text": "Es hielten die Jungfern nicht viel auf Credit,", "tokens": ["Es", "hiel\u00b7ten", "die", "Jung\u00b7fern", "nicht", "viel", "auf", "Cre\u00b7dit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "ADV", "APPR", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Drum bracht' er der Br\u00e4vsten auch immer was mit,", "tokens": ["Drum", "bracht'", "er", "der", "Br\u00e4vs\u00b7ten", "auch", "im\u00b7mer", "was", "mit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN", "ADV", "ADV", "PIS", "PTKVZ", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die that viel kochen und opfern -", "tokens": ["Die", "that", "viel", "ko\u00b7chen", "und", "op\u00b7fern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "VVFIN", "KON", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sie durfte die Pfeife ihm stopfen.", "tokens": ["Sie", "durf\u00b7te", "die", "Pfei\u00b7fe", "ihm", "stop\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}