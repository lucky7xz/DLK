{"textgrid.poem.24008": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Walzer", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Walzer hebt sich s\u00e4uselnd an", "tokens": ["Ein", "Wal\u00b7zer", "hebt", "sich", "s\u00e4u\u00b7selnd", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJD", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit h\u00fcpfendem Bogenspringen,", "tokens": ["Mit", "h\u00fcp\u00b7fen\u00b7dem", "Bo\u00b7gen\u00b7sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "In breitem, rauschendem Striche dann", "tokens": ["In", "brei\u00b7tem", ",", "rau\u00b7schen\u00b7dem", "Stri\u00b7che", "dann"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Beginnt er sein lockendes Singen.", "tokens": ["Be\u00b7ginnt", "er", "sein", "lo\u00b7cken\u00b7des", "Sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Er schmeichelt in das Herz sich ein", "tokens": ["Er", "schmei\u00b7chelt", "in", "das", "Herz", "sich", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PRF", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den zart beschwippsten M\u00e4dchen,", "tokens": ["Den", "zart", "be\u00b7schwipps\u00b7ten", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Nun ist die Ruhe bittre Pein", "tokens": ["Nun", "ist", "die", "Ru\u00b7he", "bitt\u00b7re", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Elektrisierten W\u00e4dchen.", "tokens": ["E\u00b7lekt\u00b7ri\u00b7sier\u00b7ten", "W\u00e4d\u00b7chen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Frei und geheim ist hier die Wahl;", "tokens": ["Frei", "und", "ge\u00b7heim", "ist", "hier", "die", "Wahl", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Such, Freund, dir irgend eine", "tokens": ["Such", ",", "Freund", ",", "dir", "ir\u00b7gend", "ei\u00b7ne"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "NN", "$,", "PPER", "ADV", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und schwenke sie rundum im Saal,", "tokens": ["Und", "schwen\u00b7ke", "sie", "run\u00b7dum", "im", "Saal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Stehst fest du noch auf dem Beine.", "tokens": ["Stehst", "fest", "du", "noch", "auf", "dem", "Bei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}