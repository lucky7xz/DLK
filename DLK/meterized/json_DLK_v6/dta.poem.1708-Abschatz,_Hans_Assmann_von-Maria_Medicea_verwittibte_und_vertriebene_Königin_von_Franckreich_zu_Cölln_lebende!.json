{"dta.poem.1708": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Maria Medicea/ verwittibte und vertriebene  \n K\u00f6nigin von Franckreich/ zu C\u00f6lln  \n lebende!", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Geschencke wird mir von drey K\u00f6nigen gegeben:", "tokens": ["Ge\u00b7schen\u00b7cke", "wird", "mir", "von", "drey", "K\u00f6\u00b7ni\u00b7gen", "ge\u00b7ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "APPR", "CARD", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der in Castilien regiert/ giebt Gold zu leben/", "tokens": ["Der", "in", "Cas\u00b7ti\u00b7li\u00b7en", "re\u00b7giert", "/", "giebt", "Gold", "zu", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "VVFIN", "$(", "VVFIN", "NN", "PTKZU", "VVINF", "$("], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Mit Hoffnungs-Weyhrauch n\u00e4hrt der F\u00fcrst aus Albion/", "tokens": ["Mit", "Hoff\u00b7nungs\u00b7Weyhrauch", "n\u00e4hrt", "der", "F\u00fcrst", "aus", "Al\u00b7bi\u00b7on", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "APPR", "NE", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und Myrrhen von Pari\u00df gew\u00e4hrt der eigne Sohn.", "tokens": ["Und", "Myr\u00b7rhen", "von", "Pa\u00b7ri\u00df", "ge\u00b7w\u00e4hrt", "der", "eig\u00b7ne", "Sohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NE", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}