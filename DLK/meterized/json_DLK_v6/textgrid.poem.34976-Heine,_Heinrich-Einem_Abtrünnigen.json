{"textgrid.poem.34976": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Einem Abtr\u00fcnnigen", "genre": "verse", "period": "N.A.", "pub_year": 1825, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O des heil'gen Jugendmutes!", "tokens": ["O", "des", "heil'\u00b7gen", "Ju\u00b7gend\u00b7mu\u00b7tes", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Oh, wie schnell bist du geb\u00e4ndigt!", "tokens": ["Oh", ",", "wie", "schnell", "bist", "du", "ge\u00b7b\u00e4n\u00b7digt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "ADJD", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und du hast dich, k\u00fchlern Blutes,", "tokens": ["Und", "du", "hast", "dich", ",", "k\u00fch\u00b7lern", "Blu\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit den lieben Herrn verst\u00e4ndigt.", "tokens": ["Mit", "den", "lie\u00b7ben", "Herrn", "ver\u00b7st\u00e4n\u00b7digt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und du bist zu Kreuz gekrochen,", "tokens": ["Und", "du", "bist", "zu", "Kreuz", "ge\u00b7kro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu dem Kreuz, das du verachtest,", "tokens": ["Zu", "dem", "Kreuz", ",", "das", "du", "ver\u00b7ach\u00b7test", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Das du noch vor wenig Wochen", "tokens": ["Das", "du", "noch", "vor", "we\u00b7nig", "Wo\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In den Staub zu treten dachtest!", "tokens": ["In", "den", "Staub", "zu", "tre\u00b7ten", "dach\u00b7test", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Oh, das tut das viele Lesen", "tokens": ["Oh", ",", "das", "tut", "das", "vie\u00b7le", "Le\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PDS", "VVFIN", "ART", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jener Schlegel, Haller, Burke \u2013", "tokens": ["Je\u00b7ner", "Schle\u00b7gel", ",", "Hal\u00b7ler", ",", "Bur\u00b7ke", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PDAT", "NN", "$,", "NE", "$,", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gestern noch ein Held gewesen,", "tokens": ["Ge\u00b7stern", "noch", "ein", "Held", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "VAPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ist man heute schon ein Schurke.", "tokens": ["Ist", "man", "heu\u00b7te", "schon", "ein", "Schur\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}