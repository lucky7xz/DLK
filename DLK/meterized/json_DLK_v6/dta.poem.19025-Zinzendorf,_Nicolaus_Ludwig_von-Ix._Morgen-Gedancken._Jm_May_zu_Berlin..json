{"dta.poem.19025": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Ix.   Morgen-Gedancken.   Jm May zu Berlin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Glantz der Ewigkeit,", "tokens": ["Glantz", "der", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Gott und HErr der Zeit!", "tokens": ["Gott", "und", "Herr", "der", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Sey von allen Creaturen", "tokens": ["Sey", "von", "al\u00b7len", "Crea\u00b7tu\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PIAT", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Vor die neu erregte Spuren", "tokens": ["Vor", "die", "neu", "er\u00b7reg\u00b7te", "Spu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Deiner G\u00fctigkeit", "tokens": ["Dei\u00b7ner", "G\u00fc\u00b7tig\u00b7keit"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Hoch gebenedeyt.", "tokens": ["Hoch", "ge\u00b7be\u00b7ne\u00b7deyt", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Diese finstre Nacht", "tokens": ["Die\u00b7se", "finst\u00b7re", "Nacht"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Ist zum Schlu\u00df gebracht,", "tokens": ["Ist", "zum", "Schlu\u00df", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und die Strahlen heitrer Sonne", "tokens": ["Und", "die", "Strah\u00b7len", "hei\u00b7trer", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Brechen, zur gemeinen Wonne,", "tokens": ["Bre\u00b7chen", ",", "zur", "ge\u00b7mei\u00b7nen", "Won\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Durch die dunckle Macht", "tokens": ["Durch", "die", "dunck\u00b7le", "Macht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Der vergangnen Nacht.", "tokens": ["Der", "ver\u00b7gang\u00b7nen", "Nacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Sehen wir denn nicht", "tokens": ["Se\u00b7hen", "wir", "denn", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "In dem Morgen-Licht", "tokens": ["In", "dem", "Mor\u00b7gen\u00b7Licht"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Einen Strahl von gr\u00f6ssern Kr\u00e4fften,", "tokens": ["Ei\u00b7nen", "Strahl", "von", "gr\u00f6s\u00b7sern", "Kr\u00e4ff\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und durchdringendern Gesch\u00e4fften?", "tokens": ["Und", "durch\u00b7drin\u00b7gen\u00b7dern", "Ge\u00b7sch\u00e4ff\u00b7ten", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Sehen wir dich nicht,", "tokens": ["Se\u00b7hen", "wir", "dich", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKNEG", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Zions Sonnen-Licht?", "tokens": ["Zi\u00b7ons", "Son\u00b7nen\u00b7Licht", "?"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Ach! du blinckest zwar;", "tokens": ["Ach", "!", "du", "blin\u00b7ckest", "zwar", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Aber unser Staar,", "tokens": ["A\u00b7ber", "un\u00b7ser", "Staar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Unsre Blindheit mu\u00df mit Schrecken", "tokens": ["Uns\u00b7re", "Blind\u00b7heit", "mu\u00df", "mit", "Schre\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich vor deinem Blitz verstecken:", "tokens": ["Sich", "vor", "dei\u00b7nem", "Blitz", "ver\u00b7ste\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Unsrer Augen-Staar", "tokens": ["Uns\u00b7rer", "Au\u00b7gen\u00b7Staar"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Wird dich nicht gewahr.", "tokens": ["Wird", "dich", "nicht", "ge\u00b7wahr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Eile doch herbey,", "tokens": ["Ei\u00b7le", "doch", "her\u00b7bey", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Mit der Artzeney:", "tokens": ["Mit", "der", "Art\u00b7ze\u00b7ney", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "R\u00e4ume weg die dicken Felle,", "tokens": ["R\u00e4u\u00b7me", "weg", "die", "di\u00b7cken", "Fel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mache unsre Augen helle,", "tokens": ["Ma\u00b7che", "uns\u00b7re", "Au\u00b7gen", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sonst ist unsre Noth", "tokens": ["Sonst", "ist", "uns\u00b7re", "Noth"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Aerger als der Tod.", "tokens": ["A\u00b7er\u00b7ger", "als", "der", "Tod", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "Und weil in der Zeit", "tokens": ["Und", "weil", "in", "der", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Nacht und Dunckelheit", "tokens": ["Nacht", "und", "Dun\u00b7ckel\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Unser Licht so hefftig schw\u00e4chen,", "tokens": ["Un\u00b7ser", "Licht", "so", "heff\u00b7tig", "schw\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und so offte unterbrechen;", "tokens": ["Und", "so", "off\u00b7te", "un\u00b7ter\u00b7bre\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Voller Dunckelheit:", "tokens": ["Vol\u00b7ler", "Dun\u00b7ckel\u00b7heit", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "So verkl\u00e4re bald", "tokens": ["So", "ver\u00b7kl\u00e4\u00b7re", "bald"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Deines Lichts-Gestalt:", "tokens": ["Dei\u00b7nes", "Lichts\u00b7Ge\u00b7stalt", ":"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Oeffne die verschlo\u00dfne Siegel,", "tokens": ["Oeff\u00b7ne", "die", "ver\u00b7schlo\u00df\u00b7ne", "Sie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Brich den unvollkommnen Spiegel,", "tokens": ["Brich", "den", "un\u00b7voll\u00b7komm\u00b7nen", "Spie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und verkl\u00e4re bald", "tokens": ["Und", "ver\u00b7kl\u00e4\u00b7re", "bald"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Unsere Gestalt.", "tokens": ["Un\u00b7se\u00b7re", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Doch wenn dirs gef\u00e4llt,", "tokens": ["Doch", "wenn", "dirs", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Da\u00df wir auf der Welt", "tokens": ["Da\u00df", "wir", "auf", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "L\u00e4nger noch mit lahmen F\u00fcssen", "tokens": ["L\u00e4n\u00b7ger", "noch", "mit", "lah\u00b7men", "F\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unsre Strasse wandeln m\u00fcssen;", "tokens": ["Uns\u00b7re", "Stras\u00b7se", "wan\u00b7deln", "m\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "VMINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "O so zeig uns nur", "tokens": ["O", "so", "zeig", "uns", "nur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VVFIN", "PPER", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Die gerade Spur.", "tokens": ["Die", "ge\u00b7ra\u00b7de", "Spur", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Richte unser Hertz", "tokens": ["Rich\u00b7te", "un\u00b7ser", "Hertz"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Zeitlich Himmel-w\u00e4rts,", "tokens": ["Zeit\u00b7lich", "Him\u00b7mel\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$,"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.3": {"text": "Da\u00df die Zeichen dieser Zeiten", "tokens": ["Da\u00df", "die", "Zei\u00b7chen", "die\u00b7ser", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Uns zur letzten Zeit bereiten,", "tokens": ["Uns", "zur", "letz\u00b7ten", "Zeit", "be\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Richte unsern Sinn", "tokens": ["Rich\u00b7te", "un\u00b7sern", "Sinn"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Auf das Ende hin.", "tokens": ["Auf", "das", "En\u00b7de", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "Gibt es in der Zeit", "tokens": ["Gibt", "es", "in", "der", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Schein-Vergn\u00fcglichkeit:", "tokens": ["Schein\u00b7Ver\u00b7gn\u00fcg\u00b7lich\u00b7keit", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "So verleide uns ein Leben,", "tokens": ["So", "ver\u00b7lei\u00b7de", "uns", "ein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das kein wahres Wohlseyn geben,", "tokens": ["Das", "kein", "wah\u00b7res", "Wohl\u00b7seyn", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Noch den letzten Tag", "tokens": ["Noch", "den", "letz\u00b7ten", "Tag"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Uns vers\u00fcssen mag.", "tokens": ["Uns", "ver\u00b7s\u00fcs\u00b7sen", "mag."], "token_info": ["word", "word", "abbreviation"], "pos": ["PPER", "VVFIN", "NE"], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.11": {"line.1": {"text": "Solls uns harte gehn,", "tokens": ["Solls", "uns", "har\u00b7te", "gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "VVINF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "La\u00df uns feste stehn,", "tokens": ["La\u00df", "uns", "fes\u00b7te", "stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJA", "VVINF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und so gar in schweren Tagen", "tokens": ["Und", "so", "gar", "in", "schwe\u00b7ren", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Niemahls \u00fcber Lasten klagen;", "tokens": ["Nie\u00b7mahls", "\u00fc\u00b7ber", "Las\u00b7ten", "kla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn das ist der Weg,", "tokens": ["Denn", "das", "ist", "der", "Weg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Zu der Sternen-Steg.", "tokens": ["Zu", "der", "Ster\u00b7nen\u00b7Steg", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "--+-+", "measure": "anapaest.init"}}, "stanza.12": {"line.1": {"text": "Kracht der H\u00fctten Thor,", "tokens": ["Kracht", "der", "H\u00fct\u00b7ten", "Thor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Zeuch den Geist hervor,", "tokens": ["Zeuch", "den", "Geist", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "La\u00df ihn zu den frohen Schaaren,", "tokens": ["La\u00df", "ihn", "zu", "den", "fro\u00b7hen", "Schaa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der erl\u00f6\u00dften Geister fahren,", "tokens": ["Der", "er\u00b7l\u00f6\u00df\u00b7ten", "Geis