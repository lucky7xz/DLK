{"dta.poem.9812": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Das spiel.  \n C. H. v. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist ein schalck der zeit/ ein zunder zu dem zancken/", "tokens": ["Es", "ist", "ein", "schalck", "der", "zeit", "/", "ein", "zun\u00b7der", "zu", "dem", "zan\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "$(", "ART", "NN", "APPR", "ART", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein rechtes freundschafts-gifft/ ein hencker der gedancken/", "tokens": ["Ein", "rech\u00b7tes", "freund\u00b7schafts\u00b7gifft", "/", "ein", "hen\u00b7cker", "der", "ge\u00b7dan\u00b7cken", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Des krieges ebenbild/ ein diebstahl den man liebt/", "tokens": ["Des", "krie\u00b7ges", "e\u00b7ben\u00b7bild", "/", "ein", "dieb\u00b7stahl", "den", "man", "liebt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "NN", "ART", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein wesen/ so kein christ recht mit gewissen \u00fcbt/", "tokens": ["Ein", "we\u00b7sen", "/", "so", "kein", "christ", "recht", "mit", "ge\u00b7wis\u00b7sen", "\u00fcbt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "PIAT", "NN", "ADJD", "APPR", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein s\u00fcsses hexenwerck/ ein ungewisses lachen/", "tokens": ["Ein", "s\u00fcs\u00b7ses", "he\u00b7xen\u00b7werck", "/", "ein", "un\u00b7ge\u00b7wis\u00b7ses", "la\u00b7chen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und da\u00df ich nicht zu viel darff von dem spiele machen/", "tokens": ["Und", "da\u00df", "ich", "nicht", "zu", "viel", "darff", "von", "dem", "spie\u00b7le", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PTKNEG", "PTKA", "PIS", "VMFIN", "APPR", "ART", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wenn der ber\u00fchmte tag wirfft erd und himmel ein/", "tokens": ["Wenn", "der", "be\u00b7r\u00fchm\u00b7te", "tag", "wirfft", "erd", "und", "him\u00b7mel", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "NN", "KON", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So wird das spielen doch der teufel labsal seyn.", "tokens": ["So", "wird", "das", "spie\u00b7len", "doch", "der", "teu\u00b7fel", "lab\u00b7sal", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PDS", "VVFIN", "ADV", "ART", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}