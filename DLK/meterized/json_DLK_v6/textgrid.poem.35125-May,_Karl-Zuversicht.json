{"textgrid.poem.35125": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Zuversicht", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin in Gottes Hand, wo ich auch geh und steh;", "tokens": ["Ich", "bin", "in", "Got\u00b7tes", "Hand", ",", "wo", "ich", "auch", "geh", "und", "steh", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "NN", "$,", "PWAV", "PPER", "ADV", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Seit meinem ersten Tag bin ich geborgen.", "tokens": ["Seit", "mei\u00b7nem", "ers\u00b7ten", "Tag", "bin", "ich", "ge\u00b7bor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er kennt mein Herz mit allem seinem Weh,", "tokens": ["Er", "kennt", "mein", "Herz", "mit", "al\u00b7lem", "sei\u00b7nem", "Weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PIS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit seinen gro\u00dfen, seinen kleinen Sorgen.", "tokens": ["Mit", "sei\u00b7nen", "gro\u00b7\u00dfen", ",", "sei\u00b7nen", "klei\u00b7nen", "Sor\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Es sch\u00fctzen stetig mich bei Tag und Nacht", "tokens": ["Es", "sch\u00fct\u00b7zen", "ste\u00b7tig", "mich", "bei", "Tag", "und", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PRF", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die lichten Engel, die er mir gesandt;", "tokens": ["Die", "lich\u00b7ten", "En\u00b7gel", ",", "die", "er", "mir", "ge\u00b7sandt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Drum giebts f\u00fcr mich nichts, was mich bange macht;", "tokens": ["Drum", "giebts", "f\u00fcr", "mich", "nichts", ",", "was", "mich", "ban\u00b7ge", "macht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "APPR", "PPER", "PIS", "$,", "PWS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ich wei\u00df es ja, ich steh in Gottes Hand.", "tokens": ["Ich", "wei\u00df", "es", "ja", ",", "ich", "steh", "in", "Got\u00b7tes", "Hand", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PPER", "VVFIN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich bin in Gottes Hand, die mich so sicher stellt,", "tokens": ["Ich", "bin", "in", "Got\u00b7tes", "Hand", ",", "die", "mich", "so", "si\u00b7cher", "stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "NN", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df keinem Feind ich in die H\u00e4nde falle.", "tokens": ["Da\u00df", "kei\u00b7nem", "Feind", "ich", "in", "die", "H\u00e4n\u00b7de", "fal\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Drum f\u00fcrcht ich mich nicht vor der ganzen Welt,", "tokens": ["Drum", "f\u00fcrcht", "ich", "mich", "nicht", "vor", "der", "gan\u00b7zen", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "PTKNEG", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So lang ich gl\u00e4ubig seine Pfade walle.", "tokens": ["So", "lang", "ich", "gl\u00e4u\u00b7big", "sei\u00b7ne", "Pfa\u00b7de", "wal\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich bebe nicht, mag kommen was da will;", "tokens": ["Ich", "be\u00b7be", "nicht", ",", "mag", "kom\u00b7men", "was", "da", "will", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "VMFIN", "VVINF", "PWS", "ADV", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ich zittre nicht selbst an des Abgrunds Rand;", "tokens": ["Ich", "zitt\u00b7re", "nicht", "selbst", "an", "des", "Ab\u00b7grunds", "Rand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Er f\u00fchrt mich doch dahin, wohin er will;", "tokens": ["Er", "f\u00fchrt", "mich", "doch", "da\u00b7hin", ",", "wo\u00b7hin", "er", "will", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PAV", "$,", "PWAV", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ich wei\u00df es ja, ich steh in Gottes Hand.", "tokens": ["Ich", "wei\u00df", "es", "ja", ",", "ich", "steh", "in", "Got\u00b7tes", "Hand", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PPER", "VVFIN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich bin in Gottes Hand. Sie h\u00e4lt mich treu und fest", "tokens": ["Ich", "bin", "in", "Got\u00b7tes", "Hand", ".", "Sie", "h\u00e4lt", "mich", "treu", "und", "fest"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "NN", "$.", "PPER", "VVFIN", "PPER", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn andre H\u00e4nde gierig nach mir fassen.", "tokens": ["Wenn", "and\u00b7re", "H\u00e4n\u00b7de", "gie\u00b7rig", "nach", "mir", "fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ADJD", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da sein Erbarmen nimmer mich verl\u00e4\u00dft,", "tokens": ["Da", "sein", "Er\u00b7bar\u00b7men", "nim\u00b7mer", "mich", "ver\u00b7l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So m\u00fcssen sie doch endlich von mir lassen.", "tokens": ["So", "m\u00fcs\u00b7sen", "sie", "doch", "end\u00b7lich", "von", "mir", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mit ihm vereinigt mich f\u00fcr alle Zeit", "tokens": ["Mit", "ihm", "ver\u00b7ei\u00b7nigt", "mich", "f\u00fcr", "al\u00b7le", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "PRF", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mein Glaube als ein unzerrei\u00dfbar Band.", "tokens": ["Mein", "Glau\u00b7be", "als", "ein", "un\u00b7zer\u00b7rei\u00df\u00b7bar", "Band", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KOKOM", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sein Eigenthum bin ich in Ewigkeit;", "tokens": ["Sein", "Ei\u00b7gen\u00b7thum", "bin", "ich", "in", "E\u00b7wig\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ich steh und bleib in meines Gottes Hand.", "tokens": ["Ich", "steh", "und", "bleib", "in", "mei\u00b7nes", "Got\u00b7tes", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}