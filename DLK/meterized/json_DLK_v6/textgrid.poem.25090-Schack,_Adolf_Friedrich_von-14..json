{"textgrid.poem.25090": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf morgen mir ein Wiedersehen", "tokens": ["Auf", "mor\u00b7gen", "mir", "ein", "Wie\u00b7der\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verhie\u00dfest du mit letztem Wort;", "tokens": ["Ver\u00b7hie\u00b7\u00dfest", "du", "mit", "letz\u00b7tem", "Wort", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da ri\u00df des Schicksals Sturmeswehen", "tokens": ["Da", "ri\u00df", "des", "Schick\u00b7sals", "Stur\u00b7mes\u00b7we\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dich unerbittlich von mir fort.", "tokens": ["Dich", "un\u00b7er\u00b7bitt\u00b7lich", "von", "mir", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Umsonst durchforscht' ich L\u00e4nder, St\u00e4dte,", "tokens": ["Um\u00b7sonst", "durch\u00b7forscht'", "ich", "L\u00e4n\u00b7der", ",", "St\u00e4d\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo deine Spur auf Erden sei;", "tokens": ["Wo", "dei\u00b7ne", "Spur", "auf", "Er\u00b7den", "sei", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Statt deiner zog, so viel ich sp\u00e4hte,", "tokens": ["Statt", "dei\u00b7ner", "zog", ",", "so", "viel", "ich", "sp\u00e4h\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "VVFIN", "$,", "ADV", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die fremde, kalte Welt vorbei.", "tokens": ["Die", "frem\u00b7de", ",", "kal\u00b7te", "Welt", "vor\u00b7bei", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Von Orte trieb es mich zu Orte,", "tokens": ["Von", "Or\u00b7te", "trieb", "es", "mich", "zu", "Or\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "An alle H\u00e4user klopft' ich an;", "tokens": ["An", "al\u00b7le", "H\u00e4u\u00b7ser", "klopft'", "ich", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch immer wurde mir die Pforte", "tokens": ["Doch", "im\u00b7mer", "wur\u00b7de", "mir", "die", "Pfor\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von fremden H\u00e4nden aufgethan.", "tokens": ["Von", "frem\u00b7den", "H\u00e4n\u00b7den", "auf\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und ob zum fernsten der Gestade", "tokens": ["Und", "ob", "zum", "ferns\u00b7ten", "der", "Ge\u00b7sta\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPRART", "ADJA", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wir schweifen \u00fcber Land und Meer,", "tokens": ["Wir", "schwei\u00b7fen", "\u00fc\u00b7ber", "Land", "und", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht einer f\u00fchrt der Erdenpfade", "tokens": ["Nicht", "ei\u00b7ner", "f\u00fchrt", "der", "Er\u00b7den\u00b7pfa\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "PIS", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mich zu dir hin, dich zu mir her.", "tokens": ["Mich", "zu", "dir", "hin", ",", "dich", "zu", "mir", "her", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "PTKVZ", "$,", "PRF", "APPR", "PPER", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}