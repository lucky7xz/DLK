{"textgrid.poem.46851": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[ich wei\u00df nicht, ob es mich heute]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df nicht, ob es mich heute", "tokens": ["Ich", "wei\u00df", "nicht", ",", "ob", "es", "mich", "heu\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "PRF", "ADV"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Betr\u00fcbte mehr oder freute,", "tokens": ["Be\u00b7tr\u00fcb\u00b7te", "mehr", "o\u00b7der", "freu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Als ich ging durch die Kammer;", "tokens": ["Als", "ich", "ging", "durch", "die", "Kam\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "(es war eine Freud' im Jammer)", "tokens": ["(", "es", "war", "ei\u00b7ne", "Freud'", "im", "Jam\u00b7mer", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "APPRART", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wo noch stehn aufgeschlagen", "tokens": ["Wo", "noch", "stehn", "auf\u00b7ge\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VVINF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Bettchen, in welchen lagen", "tokens": ["Die", "Bett\u00b7chen", ",", "in", "wel\u00b7chen", "la\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "PWAT", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Die Beiden, die nun liegen", "tokens": ["Die", "Bei\u00b7den", ",", "die", "nun", "lie\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In \u00fcbergr\u00fcnten Wiegen.", "tokens": ["In", "\u00fc\u00b7berg\u00b7r\u00fcn\u00b7ten", "Wie\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Ohne dahin zu sehen,", "tokens": ["Oh\u00b7ne", "da\u00b7hin", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PAV", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.10": {"text": "Wollt ich vor\u00fcbergehen.", "tokens": ["Wollt", "ich", "vor\u00b7\u00fc\u00b7ber\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Da traf mein Ohr ein Girren,", "tokens": ["Da", "traf", "mein", "Ohr", "ein", "Gir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Ein sanftes Rauschen und Schwirren,", "tokens": ["Ein", "sanf\u00b7tes", "Rau\u00b7schen", "und", "Schwir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "Und hin mu\u00dft ich mich wenden.", "tokens": ["Und", "hin", "mu\u00dft", "ich", "mich", "wen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Da sah ich an den Enden", "tokens": ["Da", "sah", "ich", "an", "den", "En\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Der Bettlein (soll ich's glauben", "tokens": ["Der", "Bet\u00b7tlein", "(", "soll", "ich's", "glau\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$(", "VMFIN", "PIS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Den Augen?) ein Paar Tauben,", "tokens": ["Den", "Au\u00b7gen", "?", ")", "ein", "Paar", "Tau\u00b7ben", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "$(", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Die sich in Eintracht wiegen,", "tokens": ["Die", "sich", "in", "Ein\u00b7tracht", "wie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Sich an einander schmiegen,", "tokens": ["Sich", "an", "ein\u00b7an\u00b7der", "schmie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Und die K\u00f6pflein mit Schweigen", "tokens": ["Und", "die", "K\u00f6\u00b7pflein", "mit", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.20": {"text": "Gegen einander neigen,", "tokens": ["Ge\u00b7gen", "ein\u00b7an\u00b7der", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PRF", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.21": {"text": "Ganz wie einst jene thaten,", "tokens": ["Ganz", "wie", "einst", "je\u00b7ne", "tha\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ADV", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Die ich schwer mu\u00df entrathen.", "tokens": ["Die", "ich", "schwer", "mu\u00df", "ent\u00b7ra\u00b7then", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VMFIN", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.23": {"text": "Bild der Geschwisterliebe,", "tokens": ["Bild", "der", "Ge\u00b7schwis\u00b7ter\u00b7lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.24": {"text": "Bist du ein Schein, zerstiebe!", "tokens": ["Bist", "du", "ein", "Schein", ",", "zer\u00b7stie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Doch lebet ihr und leibet,", "tokens": ["Doch", "le\u00b7bet", "ihr", "und", "lei\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "So saget mir, und bleibet,", "tokens": ["So", "sa\u00b7get", "mir", ",", "und", "blei\u00b7bet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "Wo seid ihr her gekommen?", "tokens": ["Wo", "seid", "ihr", "her", "ge\u00b7kom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "Doch ich hab' es vernommen:", "tokens": ["Doch", "ich", "hab'", "es", "ver\u00b7nom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "VVPP", "$."], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.29": {"text": "F\u00fcr die K\u00fcche gekaufet,", "tokens": ["F\u00fcr", "die", "K\u00fc\u00b7che", "ge\u00b7kau\u00b7fet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.30": {"text": "Seid ihr nur ungeraufet", "tokens": ["Seid", "ihr", "nur", "un\u00b7ger\u00b7au\u00b7fet"], "token_info": ["word", "word", "word", "word"], "pos": ["VAIMP", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": "Darum bisher geblieben,", "tokens": ["Da\u00b7rum", "bis\u00b7her", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.32": {"text": "Weil ihr Spiel mit euch trieben,", "tokens": ["Weil", "ihr", "Spiel", "mit", "euch", "trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.33": {"text": "Und dieses Nest euch gaben", "tokens": ["Und", "die\u00b7ses", "Nest", "euch", "ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VVFIN", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "Die unbefangnen Knaben,", "tokens": ["Die", "un\u00b7be\u00b7fang\u00b7nen", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.35": {"text": "Und hier euch reichten Futter,", "tokens": ["Und", "hier", "euch", "reich\u00b7ten", "Fut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.36": {"text": "Mit halbem Willen der Mutter,", "tokens": ["Mit", "hal\u00b7bem", "Wil\u00b7len", "der", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.37": {"text": "Hinschiebend von Tag zu Tage", "tokens": ["Hin\u00b7schie\u00b7bend", "von", "Tag", "zu", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "APPR", "NN", "APPR", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.38": {"text": "Euere Niederlage.", "tokens": ["Eu\u00b7e\u00b7re", "Nie\u00b7der\u00b7la\u00b7ge", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.39": {"text": "Nun aber, dem Ort zu Ehren,", "tokens": ["Nun", "a\u00b7ber", ",", "dem", "Ort", "zu", "Eh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.40": {"text": "Will ich euch ganz abwehren", "tokens": ["Will", "ich", "euch", "ganz", "ab\u00b7weh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.41": {"text": "Das Messer von der Kehle", "tokens": ["Das", "Mes\u00b7ser", "von", "der", "Keh\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.42": {"text": "Mit Hausvaterbefehle.", "tokens": ["Mit", "Haus\u00b7va\u00b7ter\u00b7be\u00b7feh\u00b7le", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.43": {"text": "Der heiligen Freistatt wegen", "tokens": ["Der", "hei\u00b7li\u00b7gen", "Freis\u00b7tatt", "we\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.44": {"text": "Sollen die Br\u00fcder euch pflegen", "tokens": ["Sol\u00b7len", "die", "Br\u00fc\u00b7der", "euch", "pfle\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "PPER", "VVINF"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.45": {"text": "Stets mit dem reifsten Korne,", "tokens": ["Stets", "mit", "dem", "reifs\u00b7ten", "Kor\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.46": {"text": "Und dem frischesten Borne,", "tokens": ["Und", "dem", "fri\u00b7sches\u00b7ten", "Bor\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.47": {"text": "Da\u00df ihr tunket und picket,", "tokens": ["Da\u00df", "ihr", "tun\u00b7ket", "und", "pi\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.48": {"text": "Schlucket und euch erquicket,", "tokens": ["Schluc\u00b7ket", "und", "euch", "er\u00b7quic\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.49": {"text": "Und danket mit Geflister", "tokens": ["Und", "dan\u00b7ket", "mit", "Ge\u00b7flis\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.50": {"text": "Wie ihre rechten Geschwister.", "tokens": ["Wie", "ih\u00b7re", "rech\u00b7ten", "Ge\u00b7schwis\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.51": {"text": "Sie sollen auch vor der Tatze", "tokens": ["Sie", "sol\u00b7len", "auch", "vor", "der", "Tat\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.52": {"text": "Der taubenmordenden Katze", "tokens": ["Der", "tau\u00b7ben\u00b7mor\u00b7den\u00b7den", "Kat\u00b7ze"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.53": {"text": "Fein euere Schwelle h\u00fcten,", "tokens": ["Fein", "eu\u00b7e\u00b7re", "Schwel\u00b7le", "h\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.54": {"text": "Bis ihr gro\u00df seid zum Br\u00fcten.", "tokens": ["Bis", "ihr", "gro\u00df", "seid", "zum", "Br\u00fc\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJD", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.55": {"text": "Dann br\u00fctet hier, wenn ihr wollet;", "tokens": ["Dann", "br\u00fc\u00b7tet", "hier", ",", "wenn", "ihr", "wol\u00b7let", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "KOUS", "PPER", "VMFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.56": {"text": "Doch wenn ihr's wo anders sollet,", "tokens": ["Doch", "wenn", "ih\u00b7r's", "wo", "an\u00b7ders", "sol\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "PWAV", "ADV", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.57": {"text": "Und m\u00f6gt nicht bei uns bleiben,", "tokens": ["Und", "m\u00f6gt", "nicht", "bei", "uns", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.58": {"text": "Dort durch die gebrochnen Scheiben", "tokens": ["Dort", "durch", "die", "ge\u00b7broch\u00b7nen", "Schei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.59": {"text": "Entfliegt zum Himmelsbogen,", "tokens": ["Ent\u00b7fliegt", "zum", "Him\u00b7mels\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.60": {"text": "Wie jene uns einst entflogen.", "tokens": ["Wie", "je\u00b7ne", "uns", "einst", "ent\u00b7flo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}