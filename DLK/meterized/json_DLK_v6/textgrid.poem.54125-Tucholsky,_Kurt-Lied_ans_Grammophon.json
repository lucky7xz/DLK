{"textgrid.poem.54125": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Lied ans Grammophon", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun komm, du kleine N\u00e4hmaschine,", "tokens": ["Nun", "komm", ",", "du", "klei\u00b7ne", "N\u00e4h\u00b7ma\u00b7schi\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "und n\u00e4h mir leise einen vor.", "tokens": ["und", "n\u00e4h", "mir", "lei\u00b7se", "ei\u00b7nen", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ART", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich denke dann an Clementine,", "tokens": ["Ich", "den\u00b7ke", "dann", "an", "Cle\u00b7men\u00b7ti\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "du s\u00e4uselst sanft mir in das Ohr.", "tokens": ["du", "s\u00e4u\u00b7selst", "sanft", "mir", "in", "das", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und am Klavier ohn Unterla\u00df", "tokens": ["Und", "am", "Kla\u00b7vier", "ohn", "Un\u00b7ter\u00b7la\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "APPR", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "f\u00fchrt rhythmisch einer seinen Ba\u00df.", "tokens": ["f\u00fchrt", "rhyth\u00b7misch", "ei\u00b7ner", "sei\u00b7nen", "Ba\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie war so lieb. Kocht ich im Grimme,", "tokens": ["Sie", "war", "so", "lieb", ".", "Kocht", "ich", "im", "Grim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$.", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "weil jemand mich ge\u00e4rgert hat,", "tokens": ["weil", "je\u00b7mand", "mich", "ge\u00b7\u00e4r\u00b7gert", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "dann sang sie mit der Oberstimme", "tokens": ["dann", "sang", "sie", "mit", "der", "O\u00b7bers\u00b7tim\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "und strich mir alle Falten glatt.", "tokens": ["und", "strich", "mir", "al\u00b7le", "Fal\u00b7ten", "glatt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und am Klavier ohn Unterla\u00df", "tokens": ["Und", "am", "Kla\u00b7vier", "ohn", "Un\u00b7ter\u00b7la\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "APPR", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "f\u00fchrt rhythmisch einer seinen Ba\u00df.", "tokens": ["f\u00fchrt", "rhyth\u00b7misch", "ei\u00b7ner", "sei\u00b7nen", "Ba\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "pom-pom", "tokens": ["pom\u00b7pom"], "token_info": ["word"], "pos": ["XY"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.3": {"line.1": {"text": "Still sah sie immer nach dem Rechten", "tokens": ["Still", "sah", "sie", "im\u00b7mer", "nach", "dem", "Rech\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "und stellte alles so nett hin.", "tokens": ["und", "stell\u00b7te", "al\u00b7les", "so", "nett", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Am Tage k\u00fchl. Doch in den N\u00e4chten", "tokens": ["Am", "Ta\u00b7ge", "k\u00fchl", ".", "Doch", "in", "den", "N\u00e4ch\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJD", "$.", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "zerschmolz die s\u00fc\u00dfe Schaffnerin.", "tokens": ["zer\u00b7schmolz", "die", "s\u00fc\u00b7\u00dfe", "Schaff\u00b7ne\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "pom-pom", "tokens": ["pom\u00b7pom"], "token_info": ["word"], "pos": ["XY"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.4": {"line.1": {"text": "O spiele weiter!", "tokens": ["O", "spie\u00b7le", "wei\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Clementine", "tokens": ["Cle\u00b7men\u00b7ti\u00b7ne"], "token_info": ["word"], "pos": ["NE"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "war ihrerseits aus Brandenburch.", "tokens": ["war", "ih\u00b7rer\u00b7seits", "aus", "Bran\u00b7den\u00b7burch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie trog mich mit der Unschuldsmiene", "tokens": ["Sie", "trog", "mich", "mit", "der", "Un\u00b7schulds\u00b7mie\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "und ging mit einem Dichter durch.", "tokens": ["und", "ging", "mit", "ei\u00b7nem", "Dich\u00b7ter", "durch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Bei dem ist sie bis heut geblieben.", "tokens": ["Bei", "dem", "ist", "sie", "bis", "heut", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Gewi\u00df . . . der Mann hat keinen Bauch.", "tokens": ["Ge\u00b7wi\u00df", ".", ".", ".", "der", "Mann", "hat", "kei\u00b7nen", "Bauch", "."], "token_info": ["word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "$.", "$.", "ART", "NN", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und er hat alles klein geschrieben;", "tokens": ["Und", "er", "hat", "al\u00b7les", "klein", "ge\u00b7schrie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PIS", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "stefan george tut das auch;", "tokens": ["ste\u00b7fan", "ge\u00b7or\u00b7ge", "tut", "das", "auch", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "PDS", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "und im klavier ohn unterla\u00df", "tokens": ["und", "im", "kla\u00b7vier", "ohn", "un\u00b7ter\u00b7la\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "f\u00fchrt rhythmisch einer seinen ba\u00df.", "tokens": ["f\u00fchrt", "rhyth\u00b7misch", "ei\u00b7ner", "sei\u00b7nen", "ba\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "PPOSAT", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Du spielst. Ich mu\u00df mich still besaufen.", "tokens": ["Du", "spielst", ".", "Ich", "mu\u00df", "mich", "still", "be\u00b7sau\u00b7fen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VMFIN", "PRF", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Voll ist das Glas und wieder leer.", "tokens": ["Voll", "ist", "das", "Glas", "und", "wie\u00b7der", "leer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "KON", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "He! Holla! Du bist abgelaufen . . .", "tokens": ["He", "!", "Hol\u00b7la", "!", "Du", "bist", "ab\u00b7ge\u00b7lau\u00b7fen", ".", ".", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ITJ", "$.", "NE", "$.", "PPER", "VAFIN", "VVPP", "$.", "$.", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Nadel knirscht. Du singst nicht mehr.", "tokens": ["Die", "Na\u00b7del", "knirscht", ".", "Du", "singst", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VAFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In meinem Ohr ohn Unterla\u00df", "tokens": ["In", "mei\u00b7nem", "Ohr", "ohn", "Un\u00b7ter\u00b7la\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "rauscht rhythmisch unser Schicksalsba\u00df;", "tokens": ["rauscht", "rhyth\u00b7misch", "un\u00b7ser", "Schick\u00b7sals\u00b7ba\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "pom-pom", "tokens": ["pom\u00b7pom"], "token_info": ["word"], "pos": ["XY"], "meter": "+-", "measure": "trochaic.single"}}}}}