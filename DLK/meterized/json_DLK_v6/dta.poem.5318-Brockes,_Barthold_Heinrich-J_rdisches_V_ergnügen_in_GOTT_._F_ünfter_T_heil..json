{"dta.poem.5318": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Zufall r\u00fchrte mich, und glaubt\u2019 ich, da\u00df, zur", "tokens": ["Der", "Zu\u00b7fall", "r\u00fchr\u00b7te", "mich", ",", "und", "glaubt'", "ich", ",", "da\u00df", ",", "zur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "KON", "VVFIN", "PPER", "$,", "KOUS", "$,", "APPRART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Lehre,", "tokens": ["Leh\u00b7re", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Er n\u00fctzlich anzuwenden w\u00e4re.", "tokens": ["Er", "n\u00fctz\u00b7lich", "an\u00b7zu\u00b7wen\u00b7den", "w\u00e4\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVIZU", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Eulchen schiene mir der Seelen Bild zu seyn;", "tokens": ["Das", "Eul\u00b7chen", "schie\u00b7ne", "mir", "der", "See\u00b7len", "Bild", "zu", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das Scheiben-Glas des C\u00f6rpers; durch den Schein", "tokens": ["Das", "Schei\u00b7ben\u00b7Glas", "des", "C\u00f6r\u00b7pers", ";", "durch", "den", "Schein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$.", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Des Lichtes schiene mir die GOttheit; S\u00fcnd\u2019 und", "tokens": ["Des", "Lich\u00b7tes", "schie\u00b7ne", "mir", "die", "Got\u00b7theit", ";", "S\u00fcnd'", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "$.", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Welt", "tokens": ["Welt"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Durch das Geweb\u2019 und durch die Spinne, vorge-", "tokens": ["Durch", "das", "Ge\u00b7web'", "und", "durch", "die", "Spin\u00b7ne", ",", "vor\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "KON", "APPR", "ART", "NN", "$,", "TRUNC"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.9": {"text": "stellt.", "tokens": ["stellt", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}}}}}