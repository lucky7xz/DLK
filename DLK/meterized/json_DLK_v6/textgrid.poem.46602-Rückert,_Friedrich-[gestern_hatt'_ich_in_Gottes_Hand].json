{"textgrid.poem.46602": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[gestern hatt' ich in Gottes Hand]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gestern hatt' ich in Gottes Hand", "tokens": ["Ge\u00b7stern", "hatt'", "ich", "in", "Got\u00b7tes", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "APPR", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Leben \u00fcbergeben,", "tokens": ["Sein", "Le\u00b7ben", "\u00fc\u00b7ber\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So da\u00df ich kaum darein mich fand", "tokens": ["So", "da\u00df", "ich", "kaum", "da\u00b7rein", "mich", "fand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "ADV", "PAV", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihn noch zu haben am Leben.", "tokens": ["Ihn", "noch", "zu", "ha\u00b7ben", "am", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKZU", "VAINF", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Nun wird, da er geschenkt mir schien,", "tokens": ["Nun", "wird", ",", "da", "er", "ge\u00b7schenkt", "mir", "schien", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "KOUS", "PPER", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir wieder das Opfer gehei\u00dfen,", "tokens": ["Mir", "wie\u00b7der", "das", "Op\u00b7fer", "ge\u00b7hei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Um nur vom blutenden Herzen ihn", "tokens": ["Um", "nur", "vom", "blu\u00b7ten\u00b7den", "Her\u00b7zen", "ihn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ADV", "APPRART", "ADJA", "NN", "PPER"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Noch einmal loszurei\u00dfen.", "tokens": ["Noch", "ein\u00b7mal", "los\u00b7zu\u00b7rei\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}