{"textgrid.poem.41112": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verl\u00f6r' ich ganz der Augen Licht,", "tokens": ["Ver\u00b7l\u00f6r'", "ich", "ganz", "der", "Au\u00b7gen", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "W\u00fcrd' dennoch mich nicht Nacht umgeben,", "tokens": ["W\u00fcrd'", "den\u00b7noch", "mich", "nicht", "Nacht", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "PTKNEG", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Solange du, mein lichtes Leben,", "tokens": ["So\u00b7lan\u00b7ge", "du", ",", "mein", "lich\u00b7tes", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du, meine Sonne! scheidest nicht.", "tokens": ["Du", ",", "mei\u00b7ne", "Son\u00b7ne", "!", "schei\u00b7dest", "nicht", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$.", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dein Herz treibt meines Herzens Schlag,", "tokens": ["Dein", "Herz", "treibt", "mei\u00b7nes", "Her\u00b7zens", "Schlag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weil es das meine ganz umfangen,", "tokens": ["Weil", "es", "das", "mei\u00b7ne", "ganz", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "VVFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und meine Augen blind empfangen", "tokens": ["Und", "mei\u00b7ne", "Au\u00b7gen", "blind", "emp\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von deinen Augen ihren Tag.", "tokens": ["Von", "dei\u00b7nen", "Au\u00b7gen", "ih\u00b7ren", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nicht Nacht, ein lichtes Morgenrot", "tokens": ["Nicht", "Nacht", ",", "ein", "lich\u00b7tes", "Mor\u00b7gen\u00b7rot"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird, weil du lebest, vor mir stehen;", "tokens": ["Wird", ",", "weil", "du", "le\u00b7best", ",", "vor", "mir", "ste\u00b7hen", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KOUS", "PPER", "VVFIN", "$,", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Werd' einst statt dessen Nacht ich sehen,", "tokens": ["Werd'", "einst", "statt", "des\u00b7sen", "Nacht", "ich", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PRELAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Werd' ich erkennen, da\u00df du tot.", "tokens": ["Werd'", "ich", "er\u00b7ken\u00b7nen", ",", "da\u00df", "du", "tot", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVINF", "$,", "KOUS", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}