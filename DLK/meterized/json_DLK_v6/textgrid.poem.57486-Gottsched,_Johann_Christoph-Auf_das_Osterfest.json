{"textgrid.poem.57486": {"metadata": {"author": {"name": "Gottsched, Johann Christoph", "birth": "N.A.", "death": "N.A."}, "title": "Auf das Osterfest", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Flieht, d\u00fcstre Trauerwolken, flieht!", "tokens": ["Flieht", ",", "d\u00fcst\u00b7re", "Trau\u00b7er\u00b7wol\u00b7ken", ",", "flieht", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Denn der erstandne Heiland zieht", "tokens": ["Denn", "der", "er\u00b7stand\u00b7ne", "Hei\u00b7land", "zieht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aus dem verschlo\u00dfnen Felsengrabe.", "tokens": ["Aus", "dem", "ver\u00b7schlo\u00df\u00b7nen", "Fel\u00b7sen\u00b7gra\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Thr\u00e4nen! fahret hin,", "tokens": ["Ihr", "Thr\u00e4\u00b7nen", "!", "fah\u00b7ret", "hin", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df den bisher betr\u00fcbten Sinn", "tokens": ["Da\u00df", "den", "bis\u00b7her", "be\u00b7tr\u00fcb\u00b7ten", "Sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Stral der Freuden labe.", "tokens": ["Ein", "Stral", "der", "Freu\u00b7den", "la\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}