{"textgrid.poem.43001": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Spielball", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es weint ein Kind.", "tokens": ["Es", "weint", "ein", "Kind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Ein Luftballon mit d\u00fcnnem Zopf", "tokens": ["Ein", "Luft\u00b7bal\u00b7lon", "mit", "d\u00fcn\u00b7nem", "Zopf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und kleiner als des Kindes Kopf", "tokens": ["Und", "klei\u00b7ner", "als", "des", "Kin\u00b7des", "Kopf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KOKOM", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Entflieht im Wind.", "tokens": ["Ent\u00b7flieht", "im", "Wind", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Und reist und steigt verwegen.", "tokens": ["Und", "reist", "und", "steigt", "ver\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Nebel wallt.", "tokens": ["Ein", "Ne\u00b7bel", "wallt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ein Fehlschu\u00df knallt.", "tokens": ["Ein", "Fehl\u00b7schu\u00df", "knallt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Dann f\u00e4llt ein sanfter Regen.", "tokens": ["Dann", "f\u00e4llt", "ein", "sanf\u00b7ter", "Re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Rundrote Riesenbeere", "tokens": ["Rund\u00b7ro\u00b7te", "Rie\u00b7sen\u00b7bee\u00b7re"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Rollt m\u00fcde und verschrumpft", "tokens": ["Rollt", "m\u00fc\u00b7de", "und", "ver\u00b7schrumpft"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In einem Wipfelmeere,", "tokens": ["In", "ei\u00b7nem", "Wip\u00b7fel\u00b7mee\u00b7re", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hat austriumpht.", "tokens": ["Hat", "aus\u00b7tri\u00b7umpht", "."], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Witziger Kr\u00e4herich", "tokens": ["Wit\u00b7zi\u00b7ger", "Kr\u00e4\u00b7he\u00b7rich"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Bringt seinem Br\u00e4utchen", "tokens": ["Bringt", "sei\u00b7nem", "Br\u00e4ut\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Ein hohles H\u00e4utchen,", "tokens": ["Ein", "hoh\u00b7les", "H\u00e4ut\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Die aber \u00e4rgert sich.", "tokens": ["Die", "a\u00b7ber", "\u00e4r\u00b7gert", "sich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}