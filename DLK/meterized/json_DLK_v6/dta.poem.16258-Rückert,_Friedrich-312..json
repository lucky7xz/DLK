{"dta.poem.16258": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "312.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn du im Gl\u00fccke schwimmst, das Ungl\u00fcck nur vernimmst", "tokens": ["Wenn", "du", "im", "Gl\u00fc\u00b7cke", "schwimmst", ",", "das", "Un\u00b7gl\u00fcck", "nur", "ver\u00b7nimmst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,", "ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von au\u00dfen, ists nicht fein da\u00df du den Ton anstimmst", "tokens": ["Von", "au\u00b7\u00dfen", ",", "ists", "nicht", "fein", "da\u00df", "du", "den", "Ton", "an\u00b7stimmst"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "$,", "VAFIN", "PTKNEG", "ADJD", "KOUS", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Von Gl\u00fcckes Nichtigkeit, Ungl\u00fccks Unwichtigkeit;", "tokens": ["Von", "Gl\u00fc\u00b7ckes", "Nich\u00b7tig\u00b7keit", ",", "Un\u00b7gl\u00fccks", "Un\u00b7wich\u00b7tig\u00b7keit", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dein thatlos guter Rath ist ohne Richtigkeit.", "tokens": ["Dein", "that\u00b7los", "gu\u00b7ter", "Rath", "ist", "oh\u00b7ne", "Rich\u00b7tig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "ADJA", "NN", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nur was du selbst vermagst zu tragen, zu entbehren,", "tokens": ["Nur", "was", "du", "selbst", "ver\u00b7magst", "zu", "tra\u00b7gen", ",", "zu", "ent\u00b7beh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PWS", "PPER", "ADV", "ADJD", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kanst du mit ein'gem Recht an andre auch begehren.", "tokens": ["Kanst", "du", "mit", "ein'\u00b7gem", "Recht", "an", "and\u00b7re", "auch", "be\u00b7geh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ADJA", "NN", "APPR", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und selber da mu\u00dft du den Schwachen Nachsicht g\u00f6nnen,", "tokens": ["Und", "sel\u00b7ber", "da", "mu\u00dft", "du", "den", "Schwa\u00b7chen", "Nach\u00b7sicht", "g\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VMFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn sie, was leicht dir wird, so leicht nicht nehmen k\u00f6nnen.", "tokens": ["Wenn", "sie", ",", "was", "leicht", "dir", "wird", ",", "so", "leicht", "nicht", "neh\u00b7men", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PRELS", "ADJD", "PPER", "VAFIN", "$,", "ADV", "ADJD", "PTKNEG", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}