{"dta.poem.16193": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "247.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Noch sorgen andere, mein Kind, f\u00fcr dich und wachen;", "tokens": ["Noch", "sor\u00b7gen", "an\u00b7de\u00b7re", ",", "mein", "Kind", ",", "f\u00fcr", "dich", "und", "wa\u00b7chen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "PPOSAT", "NN", "$,", "APPR", "PPER", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bald es f\u00fcr dich zu thun mu\u00dft du dich fertig machen.", "tokens": ["Bald", "es", "f\u00fcr", "dich", "zu", "thun", "mu\u00dft", "du", "dich", "fer\u00b7tig", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "PPER", "PTKZU", "VVINF", "VMFIN", "PPER", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und bist du f\u00fcr dich selbst von Sorgen einst geborgen,", "tokens": ["Und", "bist", "du", "f\u00fcr", "dich", "selbst", "von", "Sor\u00b7gen", "einst", "ge\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "APPR", "PPER", "ADV", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fcr andre hast du dann zu wachen und zu sorgen.", "tokens": ["F\u00fcr", "and\u00b7re", "hast", "du", "dann", "zu", "wa\u00b7chen", "und", "zu", "sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VAFIN", "PPER", "ADV", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der Mensch wird niemals frei von dieser Sorgenwacht,", "tokens": ["Der", "Mensch", "wird", "nie\u00b7mals", "frei", "von", "die\u00b7ser", "Sor\u00b7gen\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die er bald anderen, und bald sich selber macht.", "tokens": ["Die", "er", "bald", "an\u00b7de\u00b7ren", ",", "und", "bald", "sich", "sel\u00b7ber", "macht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVINF", "$,", "KON", "ADV", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}