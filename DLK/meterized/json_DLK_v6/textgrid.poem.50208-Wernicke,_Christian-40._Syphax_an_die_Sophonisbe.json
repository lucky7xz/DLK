{"textgrid.poem.50208": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "40. Syphax an die Sophonisbe", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du hast dich etwas \u00fcbereilet", "tokens": ["Du", "hast", "dich", "et\u00b7was", "\u00fc\u00b7be\u00b7rei\u00b7let"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im Raht, den Du mir mitgetheilet,", "tokens": ["Im", "Raht", ",", "den", "Du", "mir", "mit\u00b7ge\u00b7thei\u00b7let", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Frau ", "tokens": ["Frau"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Und darum bist Du ", "tokens": ["Und", "da\u00b7rum", "bist", "Du"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PAV", "VAFIN", "PPER"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Sey ", "tokens": ["Sey"], "token_info": ["word"], "pos": ["VAIMP"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Die Sach' ist warlich allzuwichtig;", "tokens": ["Die", "Sach'", "ist", "war\u00b7lich", "all\u00b7zu\u00b7wich\u00b7tig", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Drum wenn der ", "tokens": ["Drum", "wenn", "der"], "token_info": ["word", "word", "word"], "pos": ["PAV", "KOUS", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Die Th\u00fcr' ihm vor der Nase zu.", "tokens": ["Die", "Th\u00fcr'", "ihm", "vor", "der", "Na\u00b7se", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Gesetzt dass er, wenn er's empfindet,", "tokens": ["Ge\u00b7setzt", "dass", "er", ",", "wenn", "er's", "emp\u00b7fin\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "$,", "KOUS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Mir ", "tokens": ["Mir"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.11": {"text": "Doch wil ich lieber mit viel Pein", "tokens": ["Doch", "wil", "ich", "lie\u00b7ber", "mit", "viel", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}