{"dta.poem.21316": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput V.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eich m\u00f6chte sie gerne wiedersehn,", "tokens": ["\u201e", "ich", "m\u00f6ch\u00b7te", "sie", "ger\u00b7ne", "wie\u00b7der\u00b7sehn", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Doch f\u00fcrcht\u2019 ich die Persifflage,", "tokens": ["Doch", "f\u00fcrcht'", "ich", "die", "Per\u00b7sif\u00b7fla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Von wegen des verw\u00fcnschten Lieds,", "tokens": ["Von", "we\u00b7gen", "des", "ver\u00b7w\u00fcnschten", "Lieds", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Von wegen der Blamage.", "tokens": ["Von", "we\u00b7gen", "der", "Bla\u00b7ma\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "$."], "meter": "-+--++-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "\u201eder Alphred de M\u00fcsset, der Gassenbub,", "tokens": ["\u201e", "der", "Al\u00b7ph\u00b7red", "de", "M\u00fcs\u00b7set", ",", "der", "Gas\u00b7sen\u00b7bub", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ART", "NE", "NE", "NE", "$,", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der kommt an ihrer Spitze", "tokens": ["Der", "kommt", "an", "ih\u00b7rer", "Spit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vielleicht als Tambour, und trommelt mir vor", "tokens": ["Viel\u00b7leicht", "als", "Tam\u00b7bour", ",", "und", "trom\u00b7melt", "mir", "vor"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "NN", "$,", "KON", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "All seine schlechten Witze.\u201c", "tokens": ["All", "sei\u00b7ne", "schlech\u00b7ten", "Wit\u00b7ze", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "So klagte der arme Vater Rhein,", "tokens": ["So", "klag\u00b7te", "der", "ar\u00b7me", "Va\u00b7ter", "Rhein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "NE", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Konnt sich nicht zufrieden geben.", "tokens": ["Konnt", "sich", "nicht", "zu\u00b7frie\u00b7den", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich sprach zu ihm manch tr\u00f6stendes Wort,", "tokens": ["Ich", "sprach", "zu", "ihm", "manch", "tr\u00f6s\u00b7ten\u00b7des", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Um ihm das Herz zu heben:", "tokens": ["Um", "ihm", "das", "Herz", "zu", "he\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "O, f\u00fcrchte nicht, mein Vater Rhein,", "tokens": ["O", ",", "f\u00fcrch\u00b7te", "nicht", ",", "mein", "Va\u00b7ter", "Rhein", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PTKNEG", "$,", "PPOSAT", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den sp\u00f6ttelnden Scherz der Franzosen;", "tokens": ["Den", "sp\u00f6t\u00b7teln\u00b7den", "Scherz", "der", "Fran\u00b7zo\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Sie sind die alten Franzosen nicht mehr,", "tokens": ["Sie", "sind", "die", "al\u00b7ten", "Fran\u00b7zo\u00b7sen", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Auch tragen sie andere Hosen.", "tokens": ["Auch", "tra\u00b7gen", "sie", "an\u00b7de\u00b7re", "Ho\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}