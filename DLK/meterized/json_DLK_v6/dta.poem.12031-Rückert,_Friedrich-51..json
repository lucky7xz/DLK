{"dta.poem.12031": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "51.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Vollkommen lieb' ich nicht die Menschen, streng und heilig;", "tokens": ["Voll\u00b7kom\u00b7men", "lieb'", "ich", "nicht", "die", "Men\u00b7schen", ",", "streng", "und", "hei\u00b7lig", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$,", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie w\u00e4ren unbequem und w\u00e4ren auch langweilig.", "tokens": ["Sie", "w\u00e4\u00b7ren", "un\u00b7be\u00b7quem", "und", "w\u00e4\u00b7ren", "auch", "lang\u00b7wei\u00b7lig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Einseitig lieb' ich sie, nat\u00fcrlich und beschrenkt,", "tokens": ["Ein\u00b7sei\u00b7tig", "lieb'", "ich", "sie", ",", "na\u00b7t\u00fcr\u00b7lich", "und", "be\u00b7schrenkt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "$,", "ADV", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht \u00fcbertrieben, krank, gebrechlich und verrenkt.", "tokens": ["Nicht", "\u00fc\u00b7bert\u00b7rie\u00b7ben", ",", "krank", ",", "ge\u00b7brech\u00b7lich", "und", "ver\u00b7renkt", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$,", "ADJD", "$,", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So lieb' ich sie sich dar mir stellend in der Welt,", "tokens": ["So", "lieb'", "ich", "sie", "sich", "dar", "mir", "stel\u00b7lend", "in", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PRF", "PTKVZ", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und also fordr' ich sie vom Dichter dargestellt.", "tokens": ["Und", "al\u00b7so", "fordr'", "ich", "sie", "vom", "Dich\u00b7ter", "dar\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wenn anders sie mir zeigt die Welt, mu\u00df ichs in Ruh", "tokens": ["Wenn", "an\u00b7ders", "sie", "mir", "zeigt", "die", "Welt", ",", "mu\u00df", "ichs", "in", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPER", "PPER", "VVFIN", "ART", "NN", "$,", "VMFIN", "PIS", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ertragen, aber wenn das Buch, so mach' ichs zu.", "tokens": ["Er\u00b7tra\u00b7gen", ",", "a\u00b7ber", "wenn", "das", "Buch", ",", "so", "mach'", "ichs", "zu", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "KOUS", "ART", "NN", "$,", "ADV", "VVFIN", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}