{"dta.poem.21758": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Die Venus steht ger\u00fcst", "tokens": ["Die", "Ve\u00b7nus", "steht", "ge\u00b7r\u00fcst"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "weil sie beym Mavors lieber ist", "tokens": ["weil", "sie", "beym", "Ma\u00b7vors", "lie\u00b7ber", "ist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ADV", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "als bey dem hinkendem Vulkan", "tokens": ["als", "bey", "dem", "hin\u00b7ken\u00b7dem", "Vul\u00b7kan"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "der kaum das Feur hat au\u00dfgetahn", "tokens": ["der", "kaum", "das", "Feur", "hat", "au\u00df\u00b7ge\u00b7tahn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "NN", "VAFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "so f\u00e4ngt er an zu schnarchen", "tokens": ["so", "f\u00e4ngt", "er", "an", "zu", "schnar\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "des Morgens steht er auff zu fr\u00fch", "tokens": ["des", "Mor\u00b7gens", "steht", "er", "auff", "zu", "fr\u00fch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "VVFIN", "PPER", "APPR", "PTKA", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "mit den Ziklopen ie und ie", "tokens": ["mit", "den", "Zik\u00b7lo\u00b7pen", "ie", "und", "ie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "KON", "NE"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "und schmiedet Waffen.", "tokens": ["und", "schmie\u00b7det", "Waf\u00b7fen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Was hat denn Amor hie zuschaffen?", "tokens": ["Was", "hat", "denn", "A\u00b7mor", "hie", "zu\u00b7schaf\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "NE", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Er spricht den Filidor an umb ein", "tokens": ["Er", "spricht", "den", "Fi\u00b7li\u00b7dor", "an", "umb", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "APPR", "ART"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Werbe-Geld", "tokens": ["Wer\u00b7be\u00b7Geld"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.12": {"text": "und zieht auch mit zu Feld.", "tokens": ["und", "zieht", "auch", "mit", "zu", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Ei lieber! weistu wol wor zu?", "tokens": ["Ei", "lie\u00b7ber", "!", "weis\u00b7tu", "wol", "wor", "zu", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$.", "VVFIN", "ADV", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "meinstu er wolle lernen fechten?", "tokens": ["meins\u00b7tu", "er", "wol\u00b7le", "ler\u00b7nen", "fech\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VMFIN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Ja! er ist einer von den rechten.", "tokens": ["Ja", "!", "er", "ist", "ei\u00b7ner", "von", "den", "rech\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPER", "VAFIN", "ART", "APPR", "ART", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Er l\u00e4st dem Filidor nicht Ruh\u2019", "tokens": ["Er", "l\u00e4st", "dem", "Fi\u00b7li\u00b7dor", "nicht", "Ruh'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "er mu\u00df auff Liebes-sachen", "tokens": ["er", "mu\u00df", "auff", "Lie\u00b7bes\u00b7sa\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "ihm Verse machen.", "tokens": ["ihm", "Ver\u00b7se", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.19": {"text": "Sich/ Filidor/", "tokens": ["Sich", "/", "Fi\u00b7li\u00b7dor", "/"], "token_info": ["word", "punct", "word", "punct"], "pos": ["PRF", "$(", "NE", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.20": {"text": "dich vor/", "tokens": ["dich", "vor", "/"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$("], "meter": "-+", "measure": "iambic.single"}, "line.21": {"text": "da\u00df du nicht wirst ber\u00fckket.", "tokens": ["da\u00df", "du", "nicht", "wirst", "be\u00b7r\u00fck\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VAFIN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Der Schalk hat lo\u00df gedr\u00fckket", "tokens": ["Der", "Schalk", "hat", "lo\u00df", "ge\u00b7dr\u00fck\u00b7ket"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "der Pfeil geht dir ins Hertz hinein.", "tokens": ["der", "Pfeil", "geht", "dir", "ins", "Hertz", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Doch la\u00df es sein:", "tokens": ["Doch", "la\u00df", "es", "sein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.25": {"text": "Nu er dich hat getroffen;", "tokens": ["Nu", "er", "dich", "hat", "ge\u00b7trof\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "steht dir der Weg zum Musen offen.", "tokens": ["steht", "dir", "der", "Weg", "zum", "Mu\u00b7sen", "of\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}