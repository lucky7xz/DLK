{"textgrid.poem.32468": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "Auf das Heldengedichte Herrmann", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dem Dichter, welcher uns den Herrmann hergesungen,", "tokens": ["Dem", "Dich\u00b7ter", ",", "wel\u00b7cher", "uns", "den", "Herr\u00b7mann", "her\u00b7ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist wahrlich, G** sagts, ein Meisterst\u00fcck gelungen.", "tokens": ["Ist", "wahr\u00b7lich", ",", "G", "*", "*", "sagts", ",", "ein", "Meis\u00b7ter\u00b7st\u00fcck", "ge\u00b7lun\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "XY", "XY", "XY", "VVFIN", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und ich, ich sag es auch. Wir m\u00fcssen es verstehn.", "tokens": ["Und", "ich", ",", "ich", "sag", "es", "auch", ".", "Wir", "m\u00fcs\u00b7sen", "es", "ver\u00b7stehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPER", "VVFIN", "PPER", "ADV", "$.", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nur w\u00fcnscht ich vom Geschick, noch eins von ihm zu sehn.", "tokens": ["Nur", "w\u00fcnscht", "ich", "vom", "Ge\u00b7schick", ",", "noch", "eins", "von", "ihm", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$,", "ADV", "PIS", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und was? Ein Trauerspiel. Ein Trauerspiel? Wovon?", "tokens": ["Und", "was", "?", "Ein", "Trau\u00b7er\u00b7spiel", ".", "Ein", "Trau\u00b7er\u00b7spiel", "?", "Wo\u00b7von", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PWS", "$.", "ART", "NN", "$.", "ART", "NN", "$.", "PWAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn mein Rat etwas gilt, so seis vom Phaeton.", "tokens": ["Wenn", "mein", "Rat", "et\u00b7was", "gilt", ",", "so", "seis", "vom", "Phae\u00b7ton", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "VVFIN", "$,", "ADV", "ADV", "APPRART", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}