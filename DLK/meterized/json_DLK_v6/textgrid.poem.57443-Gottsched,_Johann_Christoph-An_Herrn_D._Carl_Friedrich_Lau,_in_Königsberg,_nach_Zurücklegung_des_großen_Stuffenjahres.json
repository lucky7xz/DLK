{"textgrid.poem.57443": {"metadata": {"author": {"name": "Gottsched, Johann Christoph", "birth": "N.A.", "death": "N.A."}, "title": "An Herrn D. Carl Friedrich Lau, in K\u00f6nigsberg, nach Zur\u00fccklegung des gro\u00dfen Stuffenjahres", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Aberglaubens Anker bricht,", "tokens": ["Des", "A\u00b7berg\u00b7lau\u00b7bens", "An\u00b7ker", "bricht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein tiefbesch\u00e4mtes Angesicht", "tokens": ["Sein", "tief\u00b7be\u00b7sch\u00e4m\u00b7tes", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mu\u00df sich je mehr und mehr mit bl\u00f6der R\u00f6the f\u00e4rben.", "tokens": ["Mu\u00df", "sich", "je", "mehr", "und", "mehr", "mit", "bl\u00f6\u00b7der", "R\u00f6\u00b7the", "f\u00e4r\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ADV", "ADV", "KON", "ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der aufgekl\u00e4rte Geist der Welt,", "tokens": ["Der", "auf\u00b7ge\u00b7kl\u00e4r\u00b7te", "Geist", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dem keine Thorheit mehr gef\u00e4llt,", "tokens": ["Dem", "kei\u00b7ne", "Thor\u00b7heit", "mehr", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wird nun nicht, wie vorhin, vor eitler Angst verderben.", "tokens": ["Wird", "nun", "nicht", ",", "wie", "vor\u00b7hin", ",", "vor", "eit\u00b7ler", "Angst", "ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "$,", "PWAV", "ADV", "$,", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie bebte vormals Stadt und Land,", "tokens": ["Wie", "beb\u00b7te", "vor\u00b7mals", "Stadt", "und", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn eine freche Zauberhand", "tokens": ["Wenn", "ei\u00b7ne", "fre\u00b7che", "Zau\u00b7ber\u00b7hand"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sich murmelnd in den Kreis beschworner Zeichen zirkte?", "tokens": ["Sich", "mur\u00b7melnd", "in", "den", "Kreis", "be\u00b7schwor\u00b7ner", "Zei\u00b7chen", "zirk\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn ", "tokens": ["Wenn"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Und zur Beschimpfung der Natur", "tokens": ["Und", "zur", "Be\u00b7schimp\u00b7fung", "der", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Mehr Wunder in der Welt, als ", "tokens": ["Mehr", "Wun\u00b7der", "in", "der", "Welt", ",", "als"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "$,", "KOUS"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nun steht der kahle Blocksberg leer,", "tokens": ["Nun", "steht", "der", "kah\u00b7le", "Blocks\u00b7berg", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Hexen K\u00f6rper ist zu schwer,", "tokens": ["Der", "He\u00b7xen", "K\u00f6r\u00b7per", "ist", "zu", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kein Geist kan solche Last durch leichte L\u00fcfte f\u00fchren:", "tokens": ["Kein", "Geist", "kan", "sol\u00b7che", "Last", "durch", "leich\u00b7te", "L\u00fcf\u00b7te", "f\u00fch\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PIAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kein hei\u00dfer Scheiterhaufen schmaucht,", "tokens": ["Kein", "hei\u00b7\u00dfer", "Schei\u00b7ter\u00b7hau\u00b7fen", "schmaucht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Kein angeflammter Holzsto\u00df raucht,", "tokens": ["Kein", "an\u00b7ge\u00b7flamm\u00b7ter", "Holz\u00b7sto\u00df", "raucht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es ist itzt keine Spur der Zauberey zu sp\u00fcren.", "tokens": ["Es", "ist", "itzt", "kei\u00b7ne", "Spur", "der", "Zau\u00b7be\u00b7rey", "zu", "sp\u00fc\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wie zitterte die Vorderwelt!", "tokens": ["Wie", "zit\u00b7ter\u00b7te", "die", "Vor\u00b7der\u00b7welt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie? Sah man nicht den gr\u00f6\u00dften Held", "tokens": ["Wie", "?", "Sah", "man", "nicht", "den", "gr\u00f6\u00df\u00b7ten", "Held"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$.", "VVFIN", "PIS", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die n\u00e4chtliche Gewalt der Poltergeister glauben?", "tokens": ["Die", "n\u00e4cht\u00b7li\u00b7che", "Ge\u00b7walt", "der", "Pol\u00b7ter\u00b7geis\u00b7ter", "glau\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Denn alles fiel, und nichts zerbrach,", "tokens": ["Denn", "al\u00b7les", "fiel", ",", "und", "nichts", "zer\u00b7brach", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$,", "KON", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Wort, das man von sp\u00fccken sprach,", "tokens": ["Ein", "Wort", ",", "das", "man", "von", "sp\u00fc\u00b7cken", "sprach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "APPR", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "War stark und kr\u00e4ftig gnug uns Herz und Muth zu rauben.", "tokens": ["War", "stark", "und", "kr\u00e4f\u00b7tig", "gnug", "uns", "Herz", "und", "Muth", "zu", "rau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ADJD", "ADV", "PPER", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Kein Kind entsetzt sich mehr davor,", "tokens": ["Kein", "Kind", "ent\u00b7setzt", "sich", "mehr", "da\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PRF", "ADV", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es scheint, da\u00df itzo unser Ohr", "tokens": ["Es", "scheint", ",", "da\u00df", "it\u00b7zo", "un\u00b7ser", "Ohr"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In diesem Absehn taub, das Auge blind geworden.", "tokens": ["In", "die\u00b7sem", "Ab\u00b7sehn", "taub", ",", "das", "Au\u00b7ge", "blind", "ge\u00b7wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJD", "$,", "ART", "NN", "ADJD", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gespenster sind uns unbekannt,", "tokens": ["Ge\u00b7spens\u00b7ter", "sind", "uns", "un\u00b7be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Poltergeister ausgebannt,", "tokens": ["Die", "Pol\u00b7ter\u00b7geis\u00b7ter", "aus\u00b7ge\u00b7bannt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Drum wird Betrug und Angst itzt keinen Menschen morden.", "tokens": ["Drum", "wird", "Be\u00b7trug", "und", "Angst", "itzt", "kei\u00b7nen", "Men\u00b7schen", "mor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "NN", "KON", "NN", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Noch mehr! ein andrer Irrthum schwindt,", "tokens": ["Noch", "mehr", "!", "ein", "an\u00b7drer", "Irr\u00b7thum", "schwindt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$.", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der sich bey feigen Seelen findt,", "tokens": ["Der", "sich", "bey", "fei\u00b7gen", "See\u00b7len", "findt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn sie in ihrer Zeit gewisse Stuffen z\u00e4hlen.", "tokens": ["Wenn", "sie", "in", "ih\u00b7rer", "Zeit", "ge\u00b7wis\u00b7se", "Stuf\u00b7fen", "z\u00e4h\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Man nennet es ein Stuffenjahr,", "tokens": ["Man", "nen\u00b7net", "es", "ein", "Stuf\u00b7fen\u00b7jahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und pflegt mit Krankheit und Gefahr", "tokens": ["Und", "pflegt", "mit", "Krank\u00b7heit", "und", "Ge\u00b7fahr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit schwerer Todesfurcht die bange Brust zu qu\u00e4len.", "tokens": ["Mit", "schwe\u00b7rer", "To\u00b7des\u00b7furcht", "die", "ban\u00b7ge", "Brust", "zu", "qu\u00e4\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Mein Lau! dein eigen Beyspiel weist,", "tokens": ["Mein", "Lau", "!", "dein", "ei\u00b7gen", "Bey\u00b7spiel", "weist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df sich der oft betrogne Geist", "tokens": ["Da\u00df", "sich", "der", "oft", "be\u00b7trog\u00b7ne", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verirrter Sterblichen mit leeren Aengsten plaget.", "tokens": ["Ver\u00b7irr\u00b7ter", "Sterb\u00b7li\u00b7chen", "mit", "lee\u00b7ren", "A\u00b7engs\u00b7ten", "pla\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Kein Stuffenjahr erschreckte dich,", "tokens": ["Kein", "Stuf\u00b7fen\u00b7jahr", "er\u00b7schreck\u00b7te", "dich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dein gro\u00dfer Geist erh\u00f6hte sich,", "tokens": ["Dein", "gro\u00b7\u00dfer", "Geist", "er\u00b7h\u00f6h\u00b7te", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn mancher bl\u00f6de Sinn aus fr\u00fcher Furcht verzaget.", "tokens": ["Wenn", "man\u00b7cher", "bl\u00f6\u00b7de", "Sinn", "aus", "fr\u00fc\u00b7her", "Furcht", "ver\u00b7za\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Begl\u00fccktes Haupt! das seine Zeit,", "tokens": ["Be\u00b7gl\u00fcck\u00b7tes", "Haupt", "!", "das", "sei\u00b7ne", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ART", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht durch vergebne Traurigkeit,", "tokens": ["Nicht", "durch", "ver\u00b7geb\u00b7ne", "Trau\u00b7rig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit selbst gemachter Angst und eigner Schuld verk\u00fcrzet.", "tokens": ["Mit", "selbst", "ge\u00b7mach\u00b7ter", "Angst", "und", "eig\u00b7ner", "Schuld", "ver\u00b7k\u00fcr\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gesetzter Muth! der seine Zahl", "tokens": ["Ge\u00b7setz\u00b7ter", "Muth", "!", "der", "sei\u00b7ne", "Zahl"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "$.", "ART", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nicht mindert, nicht durch Gram und Qual", "tokens": ["Nicht", "min\u00b7dert", ",", "nicht", "durch", "Gram", "und", "Qual"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVPP", "$,", "PTKNEG", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sich schleunig in den Schlund des offnen Grabes st\u00fcrzet.", "tokens": ["Sich", "schleu\u00b7nig", "in", "den", "Schlund", "des", "off\u00b7nen", "Gra\u00b7bes", "st\u00fcr\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Dein theures Haus ist froh dabey", "tokens": ["Dein", "theu\u00b7res", "Haus", "ist", "froh", "da\u00b7bey"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADJD", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wird von allem Kummer frey,", "tokens": ["Und", "wird", "von", "al\u00b7lem", "Kum\u00b7mer", "frey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PIS", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da heute wiederum dein Wiegenfest erschienen.", "tokens": ["Da", "heu\u00b7te", "wie\u00