{"textgrid.poem.25811": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "[sieh die hundert kleinen Menschen]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieh die hundert kleinen Menschen,", "tokens": ["Sieh", "die", "hun\u00b7dert", "klei\u00b7nen", "Men\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "CARD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fr\u00fchling bricht in alle Gassen.", "tokens": ["Fr\u00fch\u00b7ling", "bricht", "in", "al\u00b7le", "Gas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die kleinen Menschen rennen,", "tokens": ["Und", "die", "klei\u00b7nen", "Men\u00b7schen", "ren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die kleinen Herzen klopfen,", "tokens": ["Und", "die", "klei\u00b7nen", "Her\u00b7zen", "klop\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Freigelassen, wie die tausend kleinen wilden Wassertropfen.", "tokens": ["Frei\u00b7ge\u00b7las\u00b7sen", ",", "wie", "die", "tau\u00b7send", "klei\u00b7nen", "wil\u00b7den", "Was\u00b7ser\u00b7trop\u00b7fen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "CARD", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Und die roten, frischen K\u00f6pfe", "tokens": ["Und", "die", "ro\u00b7ten", ",", "fri\u00b7schen", "K\u00f6p\u00b7fe"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eilen durch die Fr\u00fchlingsgassen,", "tokens": ["Ei\u00b7len", "durch", "die", "Fr\u00fch\u00b7lings\u00b7gas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Tausend tote Augen lachen,", "tokens": ["Tau\u00b7send", "to\u00b7te", "Au\u00b7gen", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Selbst die Augen, die sonst hassen.", "tokens": ["Selbst", "die", "Au\u00b7gen", ",", "die", "sonst", "has\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und die jungen, blauen Herzen,", "tokens": ["Und", "die", "jun\u00b7gen", ",", "blau\u00b7en", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aufgewirbelt von der Sonne,", "tokens": ["Auf\u00b7ge\u00b7wir\u00b7belt", "von", "der", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Klopfen an den blauen Himmel,", "tokens": ["Klop\u00b7fen", "an", "den", "blau\u00b7en", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Himmeltore tauen.", "tokens": ["Und", "die", "Him\u00b7mel\u00b7to\u00b7re", "tau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sieh die tausend warmen Lippen,", "tokens": ["Sieh", "die", "tau\u00b7send", "war\u00b7men", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "CARD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liebe! Liebe! h\u00f6r ich sagen.", "tokens": ["Lie\u00b7be", "!", "Lie\u00b7be", "!", "h\u00f6r", "ich", "sa\u00b7gen", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "VVFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gerne m\u00f6cht ich Erde werden,", "tokens": ["Ger\u00b7ne", "m\u00f6cht", "ich", "Er\u00b7de", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alle diese Liebe tragen.", "tokens": ["Al\u00b7le", "die\u00b7se", "Lie\u00b7be", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "PDAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}