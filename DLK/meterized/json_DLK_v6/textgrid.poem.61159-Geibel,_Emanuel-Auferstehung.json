{"textgrid.poem.61159": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Auferstehung", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn einer starb, den du geliebt hienieden,", "tokens": ["Wenn", "ei\u00b7ner", "starb", ",", "den", "du", "ge\u00b7liebt", "hien\u00b7ie\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "$,", "PRELS", "PPER", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So trag hinaus zur Einsamkeit dein Wehe,", "tokens": ["So", "trag", "hin\u00b7aus", "zur", "Ein\u00b7sam\u00b7keit", "dein", "We\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df ernst und still es sich mit dir ergehe", "tokens": ["Da\u00df", "ernst", "und", "still", "es", "sich", "mit", "dir", "er\u00b7ge\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "KON", "ADJD", "PPER", "PRF", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Wald, am Meer, auf Steigen l\u00e4ngst gemieden.", "tokens": ["Im", "Wald", ",", "am", "Meer", ",", "auf", "Stei\u00b7gen", "l\u00e4ngst", "ge\u00b7mie\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$,", "APPR", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da f\u00fchlst du bald, da\u00df jener, der geschieden,", "tokens": ["Da", "f\u00fchlst", "du", "bald", ",", "da\u00df", "je\u00b7ner", ",", "der", "ge\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PDS", "$,", "PRELS", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Lebendig dir im Herzen auferstehe;", "tokens": ["Le\u00b7ben\u00b7dig", "dir", "im", "Her\u00b7zen", "auf\u00b7er\u00b7ste\u00b7he", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "In Luft und Schatten sp\u00fcrst du seine N\u00e4he,", "tokens": ["In", "Luft", "und", "Schat\u00b7ten", "sp\u00fcrst", "du", "sei\u00b7ne", "N\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und aus den Tr\u00e4nen bl\u00fcht ein tiefer Frieden.", "tokens": ["Und", "aus", "den", "Tr\u00e4\u00b7nen", "bl\u00fcht", "ein", "tie\u00b7fer", "Frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ja, ", "tokens": ["Ja", ","], "token_info": ["word", "punct"], "pos": ["PTKANT", "$,"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Ums Haupt der Schmerzverkl\u00e4rung lichten Schein,", "tokens": ["Ums", "Haupt", "der", "Schmerz\u00b7ver\u00b7kl\u00e4\u00b7rung", "lich\u00b7ten", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}}, "stanza.4": {"line.1": {"text": "Das Herz auch hat sein Ostern, wo der Stein", "tokens": ["Das", "Herz", "auch", "hat", "sein", "Os\u00b7tern", ",", "wo", "der", "Stein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VAFIN", "PPOSAT", "NN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vom Grabe springt, dem wir den Staub nur weihten;", "tokens": ["Vom", "Gra\u00b7be", "springt", ",", "dem", "wir", "den", "Staub", "nur", "weih\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "PRELS", "PPER", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und was du ewig liebst, ist ewig dein.", "tokens": ["Und", "was", "du", "e\u00b7wig", "liebst", ",", "ist", "e\u00b7wig", "dein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADJD", "VVFIN", "$,", "VAFIN", "ADJD", "PPOSAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}