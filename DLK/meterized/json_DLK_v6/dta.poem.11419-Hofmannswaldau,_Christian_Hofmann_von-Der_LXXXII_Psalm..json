{"dta.poem.11419": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der  LXXXII  Psalm.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gott stehet allezeit, und sieht auf die gemeine:", "tokens": ["Gott", "ste\u00b7het", "al\u00b7le\u00b7zeit", ",", "und", "sieht", "auf", "die", "ge\u00b7mei\u00b7ne", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "$,", "KON", "VVFIN", "APPR", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wi\u00dft, g\u00f6tter! da\u00df der HErr der g\u00f6tter richter heist.", "tokens": ["Wi\u00dft", ",", "g\u00f6t\u00b7ter", "!", "da\u00df", "der", "Herr", "der", "g\u00f6t\u00b7ter", "rich\u00b7ter", "heist", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "$.", "KOUS", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie kommt es, da\u00df ihr euch des rechtes nicht befleist?", "tokens": ["Wie", "kommt", "es", ",", "da\u00df", "ihr", "euch", "des", "rech\u00b7tes", "nicht", "be\u00b7fleist", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PRF", "ART", "ADJA", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein jeder nimmt geschenck, und siehet auf das seine.", "tokens": ["Ein", "je\u00b7der", "nimmt", "ge\u00b7schenck", ",", "und", "sie\u00b7het", "auf", "das", "sei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ADJD", "$,", "KON", "VVFIN", "APPR", "ART", "PPOSAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie lange richtet ihr aus gunst und heucheley?", "tokens": ["Wie", "lan\u00b7ge", "rich\u00b7tet", "ihr", "aus", "gunst", "und", "heu\u00b7che\u00b7ley", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "APPR", "NN", "KON", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Schafft einst den armen recht, und springt den waisen bey!", "tokens": ["Schafft", "einst", "den", "ar\u00b7men", "recht", ",", "und", "springt", "den", "wai\u00b7sen", "bey", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Helfft dem, der d\u00fcrfftig ist, und straft die falscheu t\u00fccken,", "tokens": ["Helfft", "dem", ",", "der", "d\u00fcr\u00b7ff\u00b7tig", "ist", ",", "und", "straft", "die", "fal\u00b7scheu", "t\u00fc\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "$,", "PRELS", "ADJD", "VAFIN", "$,", "KON", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.8": {"text": "Damit die m\u00e4chtigen nicht den geringen dr\u00fccken!", "tokens": ["Da\u00b7mit", "die", "m\u00e4ch\u00b7ti\u00b7gen", "nicht", "den", "ge\u00b7rin\u00b7gen", "dr\u00fc\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "PTKNEG", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wiewohl! ihr bleibet taub, und lasset euch nichts sagen:", "tokens": ["Wie\u00b7wohl", "!", "ihr", "blei\u00b7bet", "taub", ",", "und", "las\u00b7set", "euch", "nichts", "sa\u00b7gen", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$.", "PPER", "VVFIN", "ADJD", "$,", "KON", "VVFIN", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jhr ha\u00dft das reine licht, und wolt im finstern seyn.", "tokens": ["Ihr", "ha\u00dft", "das", "rei\u00b7ne", "licht", ",", "und", "wolt", "im", "fins\u00b7tern", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "KON", "VMFIN", "APPRART", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Drum geht der feste grund des landes wohlfarth ein:", "tokens": ["Drum", "geht", "der", "fes\u00b7te", "grund", "des", "lan\u00b7des", "wohl\u00b7farth", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Drum h\u00f6ret man das volck auf allen gassen klagen.", "tokens": ["Drum", "h\u00f6\u00b7ret", "man", "das", "volck", "auf", "al\u00b7len", "gas\u00b7sen", "kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIS", "ART", "NN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der HErr hat wohl gesagt: Da\u00df ihr die obrigkeit,", "tokens": ["Der", "Herr", "hat", "wohl", "ge\u00b7sagt", ":", "Da\u00df", "ihr", "die", "ob\u00b7rig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$.", "KOUS", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df ihr des H\u00f6chsten s\u00f6hn\u2019, und da\u00df ihr g\u00f6tter seyd;", "tokens": ["Da\u00df", "ihr", "des", "H\u00f6chs\u00b7ten", "s\u00f6hn'", ",", "und", "da\u00df", "ihr", "g\u00f6t\u00b7ter", "seyd", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKVZ", "$,", "KON", "KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Jhr werdet aber doch, wie schlechte menschen sterben,", "tokens": ["Ihr", "wer\u00b7det", "a\u00b7ber", "doch", ",", "wie", "schlech\u00b7te", "men\u00b7schen", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "$,", "PWAV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und als tyrannen einst in tieffer angst verderben.", "tokens": ["Und", "als", "ty\u00b7ran\u00b7nen", "einst", "in", "tief\u00b7fer", "angst", "ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NN", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Drum mache dich selbst auf, und h\u00f6re das gewinsel,", "tokens": ["Drum", "ma\u00b7che", "dich", "selbst", "auf", ",", "und", "h\u00f6\u00b7re", "das", "ge\u00b7win\u00b7sel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O GOtt! weil niemand h\u00f6rt, der nicht geschencke kriegt,", "tokens": ["O", "Gott", "!", "weil", "nie\u00b7mand", "h\u00f6rt", ",", "der", "nicht", "ge\u00b7schen\u00b7cke", "kriegt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "KOUS", "PIS", "VVFIN", "$,", "PRELS", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du siehest, wie das land fast in dem tode liegt.", "tokens": ["Du", "sie\u00b7hest", ",", "wie", "das", "land", "fast", "in", "dem", "to\u00b7de", "liegt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ART", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das elend, das hier herrscht, entwirfft kein zarter pinsel;", "tokens": ["Das", "e\u00b7lend", ",", "das", "hier", "herrscht", ",", "ent\u00b7wirfft", "kein", "zar\u00b7ter", "pin\u00b7sel", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "$,", "PRELS", "ADV", "VVPP", "$,", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der reichen herrschens-sucht und ungerechtigkeit", "tokens": ["Der", "rei\u00b7chen", "herr\u00b7schens\u00b7sucht", "und", "un\u00b7ge\u00b7rech\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ist deinen heiligen ein unaussprechlich leyd.", "tokens": ["Ist", "dei\u00b7nen", "hei\u00b7li\u00b7gen", "ein", "un\u00b7aus\u00b7sprech\u00b7lich", "leyd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "ART", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Damit es nun vor sie nicht unertr\u00e4glich werde,", "tokens": ["Da\u00b7mit", "es", "nun", "vor", "sie", "nicht", "un\u00b7er\u00b7tr\u00e4g\u00b7lich", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPER", "PTKNEG", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So komm, o HErr der welt! und richte selbst die erde!", "tokens": ["So", "komm", ",", "o", "Herr", "der", "welt", "!", "und", "rich\u00b7te", "selbst", "die", "er\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "FM", "NN", "ART", "NN", "$.", "KON", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}