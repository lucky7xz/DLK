{"textgrid.poem.67128": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "G\u00f6tter wandelten einst ...", "genre": "verse", "period": "N.A.", "pub_year": 1799, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "G\u00f6tter wandelten einst bei Menschen, die herrlichen Musen", "tokens": ["G\u00f6t\u00b7ter", "wan\u00b7del\u00b7ten", "einst", "bei", "Men\u00b7schen", ",", "die", "herr\u00b7li\u00b7chen", "Mu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "APPR", "NN", "$,", "ART", "ADJA", "NN"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Und der J\u00fcngling, Apoll, heilend, begeisternd wie du.", "tokens": ["Und", "der", "J\u00fcng\u00b7ling", ",", "A\u00b7poll", ",", "hei\u00b7lend", ",", "be\u00b7geis\u00b7ternd", "wie", "du", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "NE", "$,", "VVPP", "$,", "VVPP", "KOKOM", "PPER", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und du bist mir, wie sie, als h\u00e4tte der Seligen Einer", "tokens": ["Und", "du", "bist", "mir", ",", "wie", "sie", ",", "als", "h\u00e4t\u00b7te", "der", "Se\u00b7li\u00b7gen", "Ei\u00b7ner"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "PPER", "$,", "PWAV", "PPER", "$,", "KOKOM", "VAFIN", "ART", "ADJA", "NN"], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Mich ins Leben gesandt, geh ich, es wandelt das Bild", "tokens": ["Mich", "ins", "Le\u00b7ben", "ge\u00b7sandt", ",", "geh", "ich", ",", "es", "wan\u00b7delt", "das", "Bild"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "APPRART", "NN", "VVPP", "$,", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Meiner Heldin mit mir, wo ich duld und bilde, mit Liebe", "tokens": ["Mei\u00b7ner", "Hel\u00b7din", "mit", "mir", ",", "wo", "ich", "duld", "und", "bil\u00b7de", ",", "mit", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "$,", "PWAV", "PPER", "ADJD", "KON", "VVFIN", "$,", "APPR", "NN"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Bis in den Tod, denn dies lernt ich und hab ich von ihr.", "tokens": ["Bis", "in", "den", "Tod", ",", "denn", "dies", "lernt", "ich", "und", "hab", "ich", "von", "ihr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "$,", "KON", "PDS", "VVFIN", "PPER", "KON", "VAFIN", "PPER", "APPR", "PPOSAT", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "La\u00df uns leben, o du, mit der ich leide, mit der ich", "tokens": ["La\u00df", "uns", "le\u00b7ben", ",", "o", "du", ",", "mit", "der", "ich", "lei\u00b7de", ",", "mit", "der", "ich"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "VVINF", "$,", "FM", "PPER", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,", "APPR", "PRELS", "PPER"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Innig und glaubig und treu ringe nach sch\u00f6nerer Zeit.", "tokens": ["In\u00b7nig", "und", "glau\u00b7big", "und", "treu", "rin\u00b7ge", "nach", "sch\u00f6\u00b7ne\u00b7rer", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "KON", "ADJD", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Sind doch wirs! und w\u00fc\u00dften sie noch in kommenden Jahren", "tokens": ["Sind", "doch", "wirs", "!", "und", "w\u00fc\u00df\u00b7ten", "sie", "noch", "in", "kom\u00b7men\u00b7den", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PIS", "$.", "KON", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+--+--+-+--+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Von uns beiden, wenn einst wieder der Genius gilt,", "tokens": ["Von", "uns", "bei\u00b7den", ",", "wenn", "einst", "wie\u00b7der", "der", "Ge\u00b7nius", "gilt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PIAT", "$,", "KOUS", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Spr\u00e4chen sie: es schufen sich einst die Einsamen liebend", "tokens": ["Spr\u00e4\u00b7chen", "sie", ":", "es", "schu\u00b7fen", "sich", "einst", "die", "Ein\u00b7sa\u00b7men", "lie\u00b7bend"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$.", "PPER", "VVFIN", "PRF", "ADV", "ART", "NN", "ADJD"], "meter": "+-+-+-++-+--+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Nur von G\u00f6ttern gekannt ihre geheimere Welt.", "tokens": ["Nur", "von", "G\u00f6t\u00b7tern", "ge\u00b7kannt", "ih\u00b7re", "ge\u00b7hei\u00b7me\u00b7re", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVPP", "PPOSAT", "ADJA", "NN", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Denn die Sterbliches nur besorgt, es empf\u00e4ngt sie die Erde,", "tokens": ["Denn", "die", "Sterb\u00b7li\u00b7ches", "nur", "be\u00b7sorgt", ",", "es", "emp\u00b7f\u00e4ngt", "sie", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "ADJD", "$,", "PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Aber n\u00e4her zum Licht wandern, zum Aether hinauf", "tokens": ["A\u00b7ber", "n\u00e4\u00b7her", "zum", "Licht", "wan\u00b7dern", ",", "zum", "A\u00b7e\u00b7ther", "hin\u00b7auf"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "APPRART", "NN", "VVFIN", "$,", "APPRART", "NN", "PTKVZ"], "meter": "+-+--++--+---+", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "Sie, die inniger Liebe treu, und g\u00f6ttlichem Geiste", "tokens": ["Sie", ",", "die", "in\u00b7ni\u00b7ger", "Lie\u00b7be", "treu", ",", "und", "g\u00f6tt\u00b7li\u00b7chem", "Geis\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "ADJD", "NN", "ADJD", "$,", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Hoffend und duldend und still \u00fcber das Schicksal gesiegt.", "tokens": ["Hof\u00b7fend", "und", "dul\u00b7dend", "und", "still", "\u00fc\u00b7ber", "das", "Schick\u00b7sal", "ge\u00b7siegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "KON", "ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}}}}}