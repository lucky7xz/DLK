{"textgrid.poem.25318": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Abend dampft in den gef\u00fcllten Schalen", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Abend dampft in den gef\u00fcllten Schalen", "tokens": ["Der", "A\u00b7bend", "dampft", "in", "den", "ge\u00b7f\u00fcll\u00b7ten", "Scha\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und schwillt aus Glocken blauumkr\u00e4nzter Weiten", "tokens": ["und", "schwillt", "aus", "Glo\u00b7cken", "blau\u00b7um\u00b7kr\u00e4nz\u00b7ter", "Wei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "die Brunnen gl\u00fchn wie Ketten von Opalen.", "tokens": ["die", "Brun\u00b7nen", "gl\u00fchn", "wie", "Ket\u00b7ten", "von", "O\u00b7pa\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KOKOM", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Aus strahlend offnen Toren l\u00e4chelnd schreiten", "tokens": ["Aus", "strah\u00b7lend", "off\u00b7nen", "To\u00b7ren", "l\u00e4\u00b7chelnd", "schrei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "in langen Z\u00fcgen blasse ferne Frauen:", "tokens": ["in", "lan\u00b7gen", "Z\u00fc\u00b7gen", "blas\u00b7se", "fer\u00b7ne", "Frau\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "die schlanken Kr\u00fcge l\u00e4ssig wiegend gleiten", "tokens": ["die", "schlan\u00b7ken", "Kr\u00fc\u00b7ge", "l\u00e4s\u00b7sig", "wie\u00b7gend", "glei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVPP", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "sie in den warmen Sommerglanz der Auen", "tokens": ["sie", "in", "den", "war\u00b7men", "Som\u00b7mer\u00b7glanz", "der", "Au\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und schwimmen hin im Duft verlorner Lieder ...", "tokens": ["und", "schwim\u00b7men", "hin", "im", "Duft", "ver\u00b7lor\u00b7ner", "Lie\u00b7der", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und aus dem s\u00fc\u00df gewellten Haar der grauen", "tokens": ["Und", "aus", "dem", "s\u00fc\u00df", "ge\u00b7well\u00b7ten", "Haar", "der", "grau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJD", "ADJA", "NN", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Zypressen rieseln schon die Schatten nieder.", "tokens": ["Zyp\u00b7res\u00b7sen", "rie\u00b7seln", "schon", "die", "Schat\u00b7ten", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}}}}