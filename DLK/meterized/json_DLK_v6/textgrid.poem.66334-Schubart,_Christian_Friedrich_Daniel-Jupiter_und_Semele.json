{"textgrid.poem.66334": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Jupiter und Semele", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von des Olympos Donnerh\u00f6he sah", "tokens": ["Von", "des", "O\u00b7lym\u00b7pos", "Don\u00b7ner\u00b7h\u00f6\u00b7he", "sah"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Einst Jupiter die sch\u00f6ne Semele,", "tokens": ["Einst", "Ju\u00b7pi\u00b7ter", "die", "sch\u00f6\u00b7ne", "Se\u00b7me\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+---+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wie sie im Thale Wiesenbl\u00fcmchen pfl\u00fcckte.", "tokens": ["Wie", "sie", "im", "Tha\u00b7le", "Wie\u00b7sen\u00b7bl\u00fcm\u00b7chen", "pfl\u00fcck\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie l\u00e4chelte im Rosenflor der Jugend,", "tokens": ["Sie", "l\u00e4\u00b7chel\u00b7te", "im", "Ro\u00b7sen\u00b7flor", "der", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und Sch\u00f6nheit warf um sie den Silberschleier,", "tokens": ["Und", "Sch\u00f6n\u00b7heit", "warf", "um", "sie", "den", "Sil\u00b7ber\u00b7schlei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Aus Morgengold und Maienlicht gewebt.", "tokens": ["Aus", "Mor\u00b7gen\u00b7gold", "und", "Mai\u00b7en\u00b7licht", "ge\u00b7webt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Des G\u00f6tterk\u00f6nigs und des Menschenvaters", "tokens": ["Des", "G\u00f6t\u00b7ter\u00b7k\u00f6\u00b7nigs", "und", "des", "Men\u00b7schen\u00b7va\u00b7ters"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von Liebe trunkne Seele fluthet auf,", "tokens": ["Von", "Lie\u00b7be", "trunk\u00b7ne", "See\u00b7le", "flut\u00b7het", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den Donnerkeil legt' er aus seiner Rechten,", "tokens": ["Den", "Don\u00b7ner\u00b7keil", "legt'", "er", "aus", "sei\u00b7ner", "Rech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er streifte von sich ab die G\u00f6tterh\u00fclle,", "tokens": ["Er", "streif\u00b7te", "von", "sich", "ab", "die", "G\u00f6t\u00b7ter\u00b7h\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die den Olymp mit Einem Wink ersch\u00fcttert", "tokens": ["Die", "den", "O\u00b7lymp", "mit", "Ei\u00b7nem", "Wink", "er\u00b7sch\u00fct\u00b7tert"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und unsre Erdax' knarren macht.", "tokens": ["Und", "uns\u00b7re", "Er\u00b7da\u00b7x'", "knar\u00b7ren", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Als goldgelockter J\u00fcngling kam er nun", "tokens": ["Als", "gold\u00b7ge\u00b7lock\u00b7ter", "J\u00fcng\u00b7ling", "kam", "er", "nun"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und trat vor Semele im leichten J\u00e4gerkleide.", "tokens": ["Und", "trat", "vor", "Se\u00b7me\u00b7le", "im", "leich\u00b7ten", "J\u00e4\u00b7ger\u00b7klei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NE", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Doch Semele war tugendhaft; sie liebte", "tokens": ["Doch", "Se\u00b7me\u00b7le", "war", "tu\u00b7gend\u00b7haft", ";", "sie", "lieb\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "NE", "VAFIN", "ADJD", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Den sch\u00f6nen J\u00fcngling zwar; jedoch den G\u00fcrtel", "tokens": ["Den", "sch\u00f6\u00b7nen", "J\u00fcng\u00b7ling", "zwar", ";", "je\u00b7doch", "den", "G\u00fcr\u00b7tel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "$.", "ADV", "ART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Der Keuschheit ihr zu l\u00f6sen, verstattet sie ihm nicht.", "tokens": ["Der", "Keuschheit", "ihr", "zu", "l\u00f6\u00b7sen", ",", "ver\u00b7stat\u00b7tet", "sie", "ihm", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKZU", "VVINF", "$,", "VVFIN", "PPER", "PPER", "PTKNEG", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.12": {"text": "Doch leicht gewinnt ein Gott des M\u00e4dchens Herz.", "tokens": ["Doch", "leicht", "ge\u00b7winnt", "ein", "Gott", "des", "M\u00e4d\u00b7chens", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Zeus bin ich, sprach der Menschgestaltete,", "tokens": ["Zeus", "bin", "ich", ",", "sprach", "der", "Menschge\u00b7stal\u00b7te\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.14": {"text": "Mit Welten lohn' ich dir der Unschuld Opfer.", "tokens": ["Mit", "Wel\u00b7ten", "lohn'", "ich", "dir", "der", "Un\u00b7schuld", "Op\u00b7fer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Viele s\u00fc\u00dfe Stunden flogen nun", "tokens": ["Vie\u00b7le", "s\u00fc\u00b7\u00dfe", "Stun\u00b7den", "flo\u00b7gen", "nun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "In seiner Semele Umarmungen", "tokens": ["In", "sei\u00b7ner", "Se\u00b7me\u00b7le", "Um\u00b7ar\u00b7mun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dem Gott der G\u00f6tter goldgeschwingt,", "tokens": ["Dem", "Gott", "der", "G\u00f6t\u00b7ter", "gold\u00b7ge\u00b7schwingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Himmelsfr\u00fchlinge, vor\u00fcber;", "tokens": ["Wie", "Him\u00b7mels\u00b7fr\u00fch\u00b7lin\u00b7ge", ",", "vor\u00b7\u00fc\u00b7ber", ";"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PWAV", "NN", "$,", "PTKVZ", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Die grollende Saturnia erfuhr's.", "tokens": ["Die", "grol\u00b7len\u00b7de", "Sa\u00b7tur\u00b7nia", "er\u00b7fuhr'", "s."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NE"], "meter": "-+---+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Die sann auf Rache. Weh' dir Semele!", "tokens": ["Die", "sann", "auf", "Ra\u00b7che", ".", "Weh'", "dir", "Se\u00b7me\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "$.", "NN", "PPER", "NE", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Der G\u00f6tter K\u00f6nigin ist deine Feindin!", "tokens": ["Der", "G\u00f6t\u00b7ter", "K\u00f6\u00b7ni\u00b7gin", "ist", "dei\u00b7ne", "Fein\u00b7din", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Zu einer alten Base schr\u00fcmpfte sich", "tokens": ["Zu", "ei\u00b7ner", "al\u00b7ten", "Ba\u00b7se", "schr\u00fcmpf\u00b7te", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der hohen Juno G\u00f6tterbild zusammen;", "tokens": ["Der", "ho\u00b7hen", "Ju\u00b7no", "G\u00f6t\u00b7ter\u00b7bild", "zu\u00b7sam\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durch Schmeicheleien und durch Trug gewann", "tokens": ["Durch", "Schmei\u00b7che\u00b7lei\u00b7en", "und", "durch", "Trug", "ge\u00b7wann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sie bald des offnen M\u00e4dchens Herz.", "tokens": ["Sie", "bald", "des", "off\u00b7nen", "M\u00e4d\u00b7chens", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zeus liebt mich! sprach sie. Die verstellte", "tokens": ["Zeus", "liebt", "mich", "!", "sprach", "sie", ".", "Die", "ver\u00b7stell\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$.", "VVFIN", "PPER", "$.", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Saturnia lacht hoch darob \u2013 Zeus meinst du?", "tokens": ["Sa\u00b7tur\u00b7nia", "lacht", "hoch", "da\u00b7rob", "\u2013", "Zeus", "meinst", "du", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "PAV", "$(", "NE", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Zeus liebe dich? sagt boshaft l\u00e4chelnd sie:", "tokens": ["Zeus", "lie\u00b7be", "dich", "?", "sagt", "bos\u00b7haft", "l\u00e4\u00b7chelnd", "sie", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$.", "VVFIN", "ADJD", "ADJD", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ha, ein Verf\u00fchrer", "tokens": ["Ha", ",", "ein", "Ver\u00b7f\u00fch\u00b7rer"], "token_info": ["word", "punct", "word", "word"], "pos": ["ITJ", "$,", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Will unter dieser Larve dir das Gold", "tokens": ["Will", "un\u00b7ter", "die\u00b7ser", "Lar\u00b7ve", "dir", "das", "Gold"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "PDAT", "NN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Der Unschuld rauben. M\u00e4dchen, traue nicht.", "tokens": ["Der", "Un\u00b7schuld", "rau\u00b7ben", ".", "M\u00e4d\u00b7chen", ",", "trau\u00b7e", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$.", "NN", "$,", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Versuch' ihn, sag', er m\u00f6ge sich einmal", "tokens": ["Ver\u00b7such'", "ihn", ",", "sag'", ",", "er", "m\u00f6\u00b7ge", "sich", "ein\u00b7mal"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "$,", "VVFIN", "$,", "PPER", "VMFIN", "PRF", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "In seiner furchtbarn Majest\u00e4t dir zeigen!", "tokens": ["In", "sei\u00b7ner", "furcht\u00b7barn", "Ma\u00b7jes\u00b7t\u00e4t", "dir", "zei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "An seiner Erdenschw\u00e4che wirst du bald", "tokens": ["An", "sei\u00b7ner", "Er\u00b7den\u00b7schw\u00e4\u00b7che", "wirst", "du", "bald"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Des eingeschleirten Gottes Trug erkennen.", "tokens": ["Des", "ein\u00b7ge\u00b7schleir\u00b7ten", "Got\u00b7tes", "Trug", "er\u00b7ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Saturnia entfernte sich und lie\u00df", "tokens": ["Sa\u00b7tur\u00b7nia", "ent\u00b7fern\u00b7te", "sich", "und", "lie\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PRF", "KON", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das zitternde M\u00e4dchen mit dem Dolche", "tokens": ["Das", "zit\u00b7tern\u00b7de", "M\u00e4d\u00b7chen", "mit", "dem", "Dol\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Des Kummers in der hohen Halle stehen.", "tokens": ["Des", "Kum\u00b7mers", "in", "der", "ho\u00b7hen", "Hal\u00b7le", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und Zeus erschien in der gewohnten H\u00fclle.", "tokens": ["Und", "Zeus", "er\u00b7schien", "in", "der", "ge\u00b7wohn\u00b7ten", "H\u00fcl\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du bist nicht aufger\u00e4umt, o Semele!", "tokens": ["Du", "bist", "nicht", "auf\u00b7ge\u00b7r\u00e4umt", ",", "o", "Se\u00b7me\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "VVPP", "$,", "FM", "FM", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich mu\u00df es wohl, denn du hast mich betrogen.", "tokens": ["Ich", "mu\u00df", "es", "wohl", ",", "denn", "du", "hast", "mich", "be\u00b7tro\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "$,", "KON", "PPER", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Gott? dich? \u2013 Ein Gott w\u00e4rst du? o geh Betr\u00fcger,", "tokens": ["Ein", "Gott", "?", "dich", "?", "\u2013", "Ein", "Gott", "w\u00e4rst", "du", "?", "o", "geh", "Be\u00b7tr\u00fc\u00b7ger", ","], "token_info": ["word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PPER", "$.", "$(", "ART", "NN", "VAFIN", "PPER", "$.", "FM", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Du bist ein erdgeschaffner Mensch, und ach,", "tokens": ["Du", "bist", "ein", "erd\u00b7ge\u00b7schaff\u00b7ner", "Mensch", ",", "und", "ach", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "KON", "XY", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Das Gold der U