{"textgrid.poem.62304": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "Der Savoyardenknabe", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kennt ihr den braunen Buben,", "tokens": ["Kennt", "ihr", "den", "brau\u00b7nen", "Bu\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im Berner Oberland,", "tokens": ["Im", "Ber\u00b7ner", "O\u00b7ber\u00b7land", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit strahlend schwarzen Augen", "tokens": ["Mit", "strah\u00b7lend", "schwar\u00b7zen", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Reicht er euch hin die Hand.", "tokens": ["Reicht", "er", "euch", "hin", "die", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Der allerliebste Junge,", "tokens": ["Der", "al\u00b7ler\u00b7liebs\u00b7te", "Jun\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist j\u00fcnger noch als jung,", "tokens": ["Ist", "j\u00fcn\u00b7ger", "noch", "als", "jung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "KOUS", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er st\u00fcrzt in die Luzine", "tokens": ["Er", "st\u00fcrzt", "in", "die", "Lu\u00b7zi\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und holt sich einen Trunk.", "tokens": ["Und", "holt", "sich", "ei\u00b7nen", "Trunk", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er schl\u00e4ft bei Alpenrosen", "tokens": ["Er", "schl\u00e4ft", "bei", "Al\u00b7pen\u00b7ro\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf einem harten Stein", "tokens": ["Auf", "ei\u00b7nem", "har\u00b7ten", "Stein"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und manchmal auch vor Hunger", "tokens": ["Und", "manch\u00b7mal", "auch", "vor", "Hun\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bei Eisesgrotte ein.", "tokens": ["Bei", "Ei\u00b7ses\u00b7grot\u00b7te", "ein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Hunger, ja das Essen", "tokens": ["Der", "Hun\u00b7ger", ",", "ja", "das", "Es\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bek\u00f6mmt man nur f\u00fcr Geld,", "tokens": ["Be\u00b7k\u00f6mmt", "man", "nur", "f\u00fcr", "Geld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Drum sp\u00e4h't er aller Orten", "tokens": ["Drum", "sp\u00e4h't", "er", "al\u00b7ler", "Or\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ob nicht ein Wagen h\u00e4lt.", "tokens": ["Ob", "nicht", "ein", "Wa\u00b7gen", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ein Wagen, Reisewagen,", "tokens": ["Ein", "Wa\u00b7gen", ",", "Rei\u00b7se\u00b7wa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da st\u00fcrzt er hin wie toll", "tokens": ["Da", "st\u00fcrzt", "er", "hin", "wie", "toll"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "KOKOM", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und strecket beide H\u00e4nde", "tokens": ["Und", "stre\u00b7cket", "bei\u00b7de", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nach einem Hungerzoll.", "tokens": ["Nach", "ei\u00b7nem", "Hun\u00b7ger\u00b7zoll", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}