{"textgrid.poem.43091": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Links die zischelnden Kom\u00f6dianten,", "tokens": ["Links", "die", "zi\u00b7scheln\u00b7den", "Ko\u00b7m\u00f6\u00b7di\u00b7an\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Rechts von mir mein Br\u00e4utigam;", "tokens": ["Rechts", "von", "mir", "mein", "Br\u00e4u\u00b7ti\u00b7gam", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "PPOSAT", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hinter ihm die Anverwandten", "tokens": ["Hin\u00b7ter", "ihm", "die", "An\u00b7ver\u00b7wand\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zucken sich die Achseln lahm.", "tokens": ["Zu\u00b7cken", "sich", "die", "Ach\u00b7seln", "lahm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Vor mir mild der greise Priester,", "tokens": ["Vor", "mir", "mild", "der", "grei\u00b7se", "Pries\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In mir keine Harmonie,", "tokens": ["In", "mir", "kei\u00b7ne", "Har\u00b7mo\u00b7nie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf den blonden lichten Locken", "tokens": ["Auf", "den", "blon\u00b7den", "lich\u00b7ten", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gr\u00fcne Myrthenironie.", "tokens": ["Gr\u00fc\u00b7ne", "Myr\u00b7the\u00b7ni\u00b7ro\u00b7nie", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}