{"textgrid.poem.49739": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Sexuelle Aufkl\u00e4rung", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der alte Storch wird nun begraben.", "tokens": ["Der", "al\u00b7te", "Storch", "wird", "nun", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Kinder lernt im Unterricht,", "tokens": ["Ihr", "Kin\u00b7der", "lernt", "im", "Un\u00b7ter\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Warum wir dies und jenes haben,", "tokens": ["Wa\u00b7rum", "wir", "dies", "und", "je\u00b7nes", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PDS", "KON", "PDS", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und es verbreitet sich das Licht.", "tokens": ["Und", "es", "ver\u00b7brei\u00b7tet", "sich", "das", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Zu meiner Zeit, du gro\u00dfe G\u00fcte!", "tokens": ["Zu", "mei\u00b7ner", "Zeit", ",", "du", "gro\u00b7\u00dfe", "G\u00fc\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da herrschte tiefe Geistesnacht.", "tokens": ["Da", "herrschte", "tie\u00b7fe", "Geis\u00b7tes\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Man ahnte manches im Gem\u00fcte", "tokens": ["Man", "ahn\u00b7te", "man\u00b7ches", "im", "Ge\u00b7m\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PIS", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und hat sich selber was gedacht.", "tokens": ["Und", "hat", "sich", "sel\u00b7ber", "was", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PRF", "ADV", "PIS", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mich lehrte dieses kein Professer;", "tokens": ["Mich", "lehr\u00b7te", "die\u00b7ses", "kein", "Pro\u00b7fes\u00b7ser", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nur eine gute, dicke Magd", "tokens": ["Nur", "ei\u00b7ne", "gu\u00b7te", ",", "di\u00b7cke", "Magd"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nahm meine Unschuld unters Messer", "tokens": ["Nahm", "mei\u00b7ne", "Un\u00b7schuld", "un\u00b7ters", "Mes\u00b7ser"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und machte auf dieselbe Jagd.", "tokens": ["Und", "mach\u00b7te", "auf", "die\u00b7sel\u00b7be", "Jagd", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ihr Unterricht war nicht \u00e4sthetisch,", "tokens": ["Ihr", "Un\u00b7ter\u00b7richt", "war", "nicht", "\u00e4s\u00b7the\u00b7tisch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Im Gegenteil, sehr weit entfernt.", "tokens": ["Im", "Ge\u00b7gen\u00b7teil", ",", "sehr", "weit", "ent\u00b7fernt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und doch, wenn auch nicht theoretisch,", "tokens": ["Und", "doch", ",", "wenn", "auch", "nicht", "the\u00b7o\u00b7re\u00b7tisch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "--+-+-+--", "measure": "anapaest.init"}, "line.4": {"text": "Ich hab' es ziemlich gut gelernt.", "tokens": ["Ich", "hab'", "es", "ziem\u00b7lich", "gut", "ge\u00b7lernt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}