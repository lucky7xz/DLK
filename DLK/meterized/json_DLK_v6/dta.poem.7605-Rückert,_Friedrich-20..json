{"dta.poem.7605": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "20.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Im Meer gen S\u00fcden wohnt auf Inseln ein Geschlecht,", "tokens": ["Im", "Meer", "gen", "S\u00fc\u00b7den", "wohnt", "auf", "In\u00b7seln", "ein", "Ge\u00b7schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "VVFIN", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Reich in Zufriedenheit, in Einfalt, schlicht und recht.", "tokens": ["Reich", "in", "Zu\u00b7frie\u00b7den\u00b7heit", ",", "in", "Ein\u00b7falt", ",", "schlicht", "und", "recht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "APPR", "NN", "$,", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Inselgruppen sind alswie ein Kranz gewunden,", "tokens": ["Die", "In\u00b7sel\u00b7grup\u00b7pen", "sind", "als\u00b7wie", "ein", "Kranz", "ge\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "KOKOM", "ART", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da wohnen sie zerstreut, getrennt und doch verbunden.", "tokens": ["Da", "woh\u00b7nen", "sie", "zer\u00b7streut", ",", "ge\u00b7trennt", "und", "doch", "ver\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "$,", "VVPP", "KON", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Auf jeder Insel wohnt ein kleines Volk beisammen,", "tokens": ["Auf", "je\u00b7der", "In\u00b7sel", "wohnt", "ein", "klei\u00b7nes", "Volk", "bei\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In Frieden, alle die von einem Vater stammen.", "tokens": ["In", "Frie\u00b7den", ",", "al\u00b7le", "die", "von", "ei\u00b7nem", "Va\u00b7ter", "stam\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PIS", "ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und \u00fcber alle herrscht die Inselk\u00f6niginn,", "tokens": ["Und", "\u00fc\u00b7ber", "al\u00b7le", "herrscht", "die", "In\u00b7sel\u00b7k\u00f6\u00b7ni\u00b7ginn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die hat nicht Waffenmacht, und friedlich ist ihr Sinn.", "tokens": ["Die", "hat", "nicht", "Waf\u00b7fen\u00b7macht", ",", "und", "fried\u00b7lich", "ist", "ihr", "Sinn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKNEG", "NN", "$,", "KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und friedlich ist der Sinn von ihren Unterthanen,", "tokens": ["Und", "fried\u00b7lich", "ist", "der", "Sinn", "von", "ih\u00b7ren", "Un\u00b7ter\u00b7tha\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie folgen nicht des Kriegs und nicht des Ruhmes Fahnen.", "tokens": ["Sie", "fol\u00b7gen", "nicht", "des", "Kriegs", "und", "nicht", "des", "Ruh\u00b7mes", "Fah\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "KON", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Ihr Waffen ist Gebet, ihr Ruhm Gesang und Psalmen,", "tokens": ["Ihr", "Waf\u00b7fen", "ist", "Ge\u00b7bet", ",", "ihr", "Ruhm", "Ge\u00b7sang", "und", "Psal\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$,", "PPOSAT", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Tempel der Natur gesungen unter Palmen.", "tokens": ["Im", "Tem\u00b7pel", "der", "Na\u00b7tur", "ge\u00b7sun\u00b7gen", "un\u00b7ter", "Pal\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Die Palmen sind ihr Dach, das Blatt ist ihr Gewand,", "tokens": ["Die", "Pal\u00b7men", "sind", "ihr", "Dach", ",", "das", "Blatt", "ist", "ihr", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und mit den Fr\u00fcchten f\u00e4llt die Speis' in ihre Hand.", "tokens": ["Und", "mit", "den", "Fr\u00fcch\u00b7ten", "f\u00e4llt", "die", "Speis'", "in", "ih\u00b7re", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "In dieser Frucht ist Oel und Milch und Honigtrank,", "tokens": ["In", "die\u00b7ser", "Frucht", "ist", "O\u00b7el", "und", "Milch", "und", "Ho\u00b7nig\u00b7trank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der heiter sie berauscht, und nie l\u00e4\u00dft werden krank.", "tokens": ["Der", "hei\u00b7ter", "sie", "be\u00b7rauscht", ",", "und", "nie", "l\u00e4\u00dft", "wer\u00b7den", "krank", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "VVFIN", "$,", "KON", "ADV", "VVFIN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Die Palmen leben, gleich den Menschen, hundert Jahr,", "tokens": ["Die", "Pal\u00b7men", "le\u00b7ben", ",", "gleich", "den", "Men\u00b7schen", ",", "hun\u00b7dert", "Jahr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ADV", "ART", "NN", "$,", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und bringen eine Ernt' in jedem Monat dar.", "tokens": ["Und", "brin\u00b7gen", "ei\u00b7ne", "Ernt'", "in", "je\u00b7dem", "Mo\u00b7nat", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "F\u00e4llt dann, vom Alter hohl, ein Schaft am Meeresrande,", "tokens": ["F\u00e4llt", "dann", ",", "vom", "Al\u00b7ter", "hohl", ",", "ein", "Schaft", "am", "Mee\u00b7res\u00b7ran\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "APPRART", "NN", "ADJD", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dient er zum Nachen, der sie schifft von Strand zu Strande,", "tokens": ["Dient", "er", "zum", "Na\u00b7chen", ",", "der", "sie", "schifft", "von", "Strand", "zu", "Stran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,", "PRELS", "PPER", "VVFIN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Wobei sie Ruder nicht, noch Stang' und Segel brauchen,", "tokens": ["Wo\u00b7bei", "sie", "Ru\u00b7der", "nicht", ",", "noch", "Stang'", "und", "Se\u00b7gel", "brau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "PTKNEG", "$,", "ADV", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil \u00fcber Spiegelflut die L\u00fcfte spielend hauchen.", "tokens": ["Weil", "\u00fc\u00b7ber", "Spie\u00b7gel\u00b7flut", "die", "L\u00fcf\u00b7te", "spie\u00b7lend", "hau\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Zum Gastgeschenk, wohin sie zum Besuche wallen,", "tokens": ["Zum", "Gast\u00b7ge\u00b7schenk", ",", "wo\u00b7hin", "sie", "zum", "Be\u00b7su\u00b7che", "wal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Pfl\u00fccken sie unterwegs nur aus der Flut Korallen;", "tokens": ["Pfl\u00fc\u00b7cken", "sie", "un\u00b7ter\u00b7wegs", "nur", "aus", "der", "Flut", "Ko\u00b7ral\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.13": {"line.1": {"text": "Die unterm Wasser bleich an weichen Zacken bl\u00fchn,", "tokens": ["Die", "un\u00b7term", "Was\u00b7ser", "bleich", "an", "wei\u00b7chen", "Za\u00b7cken", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ADJD", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und h\u00e4rtend an der Luft in hohen Farben gl\u00fchn.", "tokens": ["Und", "h\u00e4r\u00b7tend", "an", "der", "Luft", "in", "ho\u00b7hen", "Far\u00b7ben", "gl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.14": {"line.1": {"text": "Geld aber f\u00fchren sie kein andres, als soviele", "tokens": ["Geld", "a\u00b7ber", "f\u00fch\u00b7ren", "sie", "kein", "and\u00b7res", ",", "als", "so\u00b7vie\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ADV", "VVFIN", "PPER", "PIAT", "PIS", "$,", "KOUS", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie Muscheln sammelten von buntem Farbenspiele.", "tokens": ["Sie", "Mu\u00b7scheln", "sam\u00b7mel\u00b7ten", "von", "bun\u00b7tem", "Far\u00b7ben\u00b7spie\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "--+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.15": {"line.1": {"text": "Doch weil sie selber Krieg nie f\u00fchren, kommen ihnen", "tokens": ["Doch", "weil", "sie", "sel\u00b7ber", "Krieg", "nie", "f\u00fch\u00b7ren", ",", "kom\u00b7men", "ih\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "NN", "ADV", "VVINF", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dazu denn Fremde, die zu solchem Schauspiel dienen.", "tokens": ["Da\u00b7zu", "denn", "Frem\u00b7de", ",", "die", "zu", "sol\u00b7chem", "Schau\u00b7spiel", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "KON", "NN", "$,", "PRELS", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.16": {"line.1": {"text": "Seer\u00e4uber suchen auf mit kriegerischen Truppen", "tokens": ["See\u00b7r\u00e4u\u00b7ber", "su\u00b7chen", "auf", "mit", "krie\u00b7ge\u00b7ri\u00b7schen", "Trup\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Meereswindungen der Friedensinselgruppen.", "tokens": ["Die", "Mee\u00b7res\u00b7win\u00b7dun\u00b7gen", "der", "Frie\u00b7den\u00b7sin\u00b7sel\u00b7grup\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.17": {"line.1": {"text": "Allein sie schlagen sich nur mit sich selbst herum,", "tokens": ["Al\u00b7lein", "sie", "schla\u00b7gen", "sich", "nur", "mit", "sich", "selbst", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PRF", "ADV", "APPR", "PRF", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und tasten niemals an der Inseln Eigenthum.", "tokens": ["Und", "tas\u00b7ten", "nie\u00b7mals", "an", "der", "In\u00b7seln", "Ei\u00b7gen\u00b7thum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.18": {"line.1": {"text": "Denn sie erfuhren es und glaubens, da\u00df belastet", "tokens": ["Denn", "sie", "er\u00b7fuh\u00b7ren", "es", "und", "glau\u00b7bens", ",", "da\u00df", "be\u00b7las\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "KON", "NN", "$,", "KOUS", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Fluch des Himmels den, der es h\u00e4tt' angetastet.", "tokens": ["Ein", "Fluch", "des", "Him\u00b7mels", "den", ",", "der", "es", "h\u00e4tt'", "an\u00b7ge\u00b7tas\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ART", "$,", "PRELS", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.19": {"line.1": {"text": "Durch diesen Glauben blieb das Volk der Inseln frei,", "tokens": ["Durch", "die\u00b7sen", "Glau\u00b7ben", "blieb", "das", "Volk", "der", "In\u00b7seln", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das, wehrlos wie es ist, sonst fiel in Sklaverei.", "tokens": ["Das", ",", "wehr\u00b7los", "wie", "es", "ist", ",", "sonst", "fiel", "in", "Skla\u00b7ve\u00b7rei", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "ADJD", "KOKOM", "PPER", "VAFIN", "$,", "ADV", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.20": {"line.1": {"text": "Doch wenn ein Schiffer kommt, ein friedlicher, von ferne,", "tokens": ["Doch", "wenn", "ein", "Schif\u00b7fer", "kommt", ",", "ein", "fried\u00b7li\u00b7cher", ",", "von", "fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$,", "ART", "ADJA", "$,", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit dem verm\u00e4hlen sich die Inselt\u00f6chter gerne.", "tokens": ["Mit", "dem", "ver\u00b7m\u00e4h\u00b7len", "sich", "die", "In\u00b7sel\u00b7t\u00f6ch\u00b7ter", "ger\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVFIN", "PRF", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.i