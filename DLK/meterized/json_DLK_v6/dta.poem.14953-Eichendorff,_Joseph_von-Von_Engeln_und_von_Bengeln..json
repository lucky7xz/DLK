{"dta.poem.14953": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Von Engeln und von Bengeln.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Die Engel weinten und lachten", "tokens": ["Die", "En\u00b7gel", "wein\u00b7ten", "und", "lach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und wu\u00dften nicht, wie ihn'n gescheh'n, \u2014", "tokens": ["Und", "wu\u00df\u00b7ten", "nicht", ",", "wie", "ihn'n", "ge\u00b7scheh'n", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "VVPP", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die einen doch bald sich bedachten", "tokens": ["Die", "ei\u00b7nen", "doch", "bald", "sich", "be\u00b7dach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "ADV", "ADV", "PRF", "VVINF"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und meinten: das wird wohl geh'n?", "tokens": ["Und", "mein\u00b7ten", ":", "das", "wird", "wohl", "geh'n", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PDS", "VAFIN", "ADV", "VVINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}