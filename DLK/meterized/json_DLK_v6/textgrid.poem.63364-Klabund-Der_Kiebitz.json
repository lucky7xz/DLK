{"textgrid.poem.63364": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Der Kiebitz", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es geht wohl immer einer neben dir,", "tokens": ["Es", "geht", "wohl", "im\u00b7mer", "ei\u00b7ner", "ne\u00b7ben", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er sieht dir in das aufgeschlagne Blatt,", "tokens": ["Er", "sieht", "dir", "in", "das", "auf\u00b7ge\u00b7schlag\u00b7ne", "Blatt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er l\u00e4uft am Wagen als das f\u00fcnfte Rad,", "tokens": ["Er", "l\u00e4uft", "am", "Wa\u00b7gen", "als", "das", "f\u00fcnf\u00b7te", "Rad", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und trinkt mit dir aus einem Glase Bier.", "tokens": ["Und", "trinkt", "mit", "dir", "aus", "ei\u00b7nem", "Gla\u00b7se", "Bier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Er ist dein Schatten, und du bist sein Tier.", "tokens": ["Er", "ist", "dein", "Schat\u00b7ten", ",", "und", "du", "bist", "sein", "Tier", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "KON", "PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Was du auch schlingst, er sagt sich niemals satt.", "tokens": ["Was", "du", "auch", "schlingst", ",", "er", "sagt", "sich", "nie\u00b7mals", "satt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "$,", "PPER", "VVFIN", "PRF", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dein ganzes Dasein scheint ihm schal und matt", "tokens": ["Dein", "gan\u00b7zes", "Da\u00b7sein", "scheint", "ihm", "schal", "und", "matt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und er verlangt ", "tokens": ["Und", "er", "ver\u00b7langt"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPER", "VVFIN"], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Wohin du auch die m\u00fcden Schritte lenkst,", "tokens": ["Wo\u00b7hin", "du", "auch", "die", "m\u00fc\u00b7den", "Schrit\u00b7te", "lenkst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie eine Bremse schwirrt er stets um dich.", "tokens": ["Wie", "ei\u00b7ne", "Brem\u00b7se", "schwirrt", "er", "stets", "um", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und was du tust und was du auch bedenkst:", "tokens": ["Und", "was", "du", "tust", "und", "was", "du", "auch", "be\u00b7denkst", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "KON", "PWS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Er zehrt von deinem Ansehn br\u00fcderlich.", "tokens": ["Er", "zehrt", "von", "dei\u00b7nem", "An\u00b7sehn", "br\u00fc\u00b7der\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wenn du dich in des Todes Masse mengst:", "tokens": ["Wenn", "du", "dich", "in", "des", "To\u00b7des", "Mas\u00b7se", "mengst", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er bleibt am Leben: geil und l\u00fcderlich.", "tokens": ["Er", "bleibt", "am", "Le\u00b7ben", ":", "geil", "und", "l\u00fc\u00b7der\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$.", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}