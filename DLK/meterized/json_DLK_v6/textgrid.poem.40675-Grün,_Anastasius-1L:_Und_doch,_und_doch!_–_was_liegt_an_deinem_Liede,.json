{"textgrid.poem.40675": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und doch, und doch! \u2013 was liegt an deinem Liede,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und doch, und doch! \u2013 was liegt an deinem Liede,", "tokens": ["Und", "doch", ",", "und", "doch", "!", "\u2013", "was", "liegt", "an", "dei\u00b7nem", "Lie\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KON", "ADV", "$.", "$(", "PWS", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn r\u00fcst'gen Tagwerks Hammerschl\u00e4ge fallen,", "tokens": ["Wenn", "r\u00fcst'\u00b7gen", "Tag\u00b7werks", "Ham\u00b7mer\u00b7schl\u00e4\u00b7ge", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die edle Form zu schaffen Vielen, Allen,", "tokens": ["Die", "ed\u00b7le", "Form", "zu", "schaf\u00b7fen", "Vie\u00b7len", ",", "Al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "VMINF", "$,", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Drin M\u00e4nnerw\u00fcrde lebt und inn'rer Friede?!", "tokens": ["Drin", "M\u00e4n\u00b7ner\u00b7w\u00fcr\u00b7de", "lebt", "und", "inn'\u00b7rer", "Frie\u00b7de", "?!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nicht Hausrath blos, auch Waffen zum Entschiede,", "tokens": ["Nicht", "Haus\u00b7rath", "blos", ",", "auch", "Waf\u00b7fen", "zum", "Ent\u00b7schie\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "ADV", "$,", "ADV", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auch Schild und Schwert entstammen den Metallen,", "tokens": ["Auch", "Schild", "und", "Schwert", "ent\u00b7stam\u00b7men", "den", "Me\u00b7tal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df sie die H\u00fctten schirmen, wie die Hallen,", "tokens": ["Da\u00df", "sie", "die", "H\u00fct\u00b7ten", "schir\u00b7men", ",", "wie", "die", "Hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihr Gut und Recht; \u2013 drum h\u00e4mmre fort und schmiede!", "tokens": ["Ihr", "Gut", "und", "Recht", ";", "\u2013", "drum", "h\u00e4mm\u00b7re", "fort", "und", "schmie\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$.", "$(", "PAV", "VVFIN", "PTKVZ", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wohnt in den Th\u00e4lern einst das Gl\u00fcck beim Volke,", "tokens": ["Wohnt", "in", "den", "Th\u00e4\u00b7lern", "einst", "das", "Gl\u00fcck", "beim", "Vol\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.2": {"text": "Dann zieht die Sehnsucht euch nicht mehr zur Wolke,", "tokens": ["Dann", "zieht", "die", "Sehn\u00b7sucht", "euch", "nicht", "mehr", "zur", "Wol\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "PTKNEG", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dann missest gern auch du die Adlerschwinge;", "tokens": ["Dann", "mis\u00b7sest", "gern", "auch", "du", "die", "Ad\u00b7ler\u00b7schwin\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und euer Werk verkl\u00e4rt zum Ehrenmale,", "tokens": ["Und", "eu\u00b7er", "Werk", "ver\u00b7kl\u00e4rt", "zum", "Eh\u00b7ren\u00b7ma\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Statt deines Grubenlichts, mit vollerm Strahle", "tokens": ["Statt", "dei\u00b7nes", "Gru\u00b7ben\u00b7lichts", ",", "mit", "vol\u00b7lerm", "Strah\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Weltensonne! \u2013 O da\u00df es gelinge!", "tokens": ["Die", "Wel\u00b7ten\u00b7son\u00b7ne", "!", "\u2013", "O", "da\u00df", "es", "ge\u00b7lin\u00b7ge", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "$(", "NE", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}