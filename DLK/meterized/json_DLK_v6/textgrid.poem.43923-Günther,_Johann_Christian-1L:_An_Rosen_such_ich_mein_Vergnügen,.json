{"textgrid.poem.43923": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: An Rosen such ich mein Vergn\u00fcgen,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An Rosen such ich mein Vergn\u00fcgen,", "tokens": ["An", "Ro\u00b7sen", "such", "ich", "mein", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "An Rosen, die die Herzen ziehn,", "tokens": ["An", "Ro\u00b7sen", ",", "die", "die", "Her\u00b7zen", "ziehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An Rosen, die den Frost besiegen", "tokens": ["An", "Ro\u00b7sen", ",", "die", "den", "Frost", "be\u00b7sie\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NE", "$,", "PRELS", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und hier das ganze Jahr durch bl\u00fchn,", "tokens": ["Und", "hier", "das", "gan\u00b7ze", "Jahr", "durch", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "APPR", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "An Rosen, die wir bey den Linden,", "tokens": ["An", "Ro\u00b7sen", ",", "die", "wir", "bey", "den", "Lin\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "PRELS", "PPER", "APPR", "ART", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sonst nirgends leicht so reizend finden.", "tokens": ["Sonst", "nir\u00b7gends", "leicht", "so", "rei\u00b7zend", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Man lobt die br\u00e4unlichen Violen,", "tokens": ["Man", "lobt", "die", "br\u00e4un\u00b7li\u00b7chen", "Vi\u00b7o\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie sind auch ihres Lobes werth;", "tokens": ["Sie", "sind", "auch", "ih\u00b7res", "Lo\u00b7bes", "werth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch weil sie nur die Kinder holen,", "tokens": ["Doch", "weil", "sie", "nur", "die", "Kin\u00b7der", "ho\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So bin ich nicht vor sie erkl\u00e4rt", "tokens": ["So", "bin", "ich", "nicht", "vor", "sie", "er\u00b7kl\u00e4rt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wehle mir die holden Strahlen,", "tokens": ["Und", "weh\u00b7le", "mir", "die", "hol\u00b7den", "Strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Womit die vollen Rosen prahlen.", "tokens": ["Wo\u00b7mit", "die", "vol\u00b7len", "Ro\u00b7sen", "prah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Erhebt mir nicht die Kaysercronen,", "tokens": ["Er\u00b7hebt", "mir", "nicht", "die", "Kay\u00b7ser\u00b7cro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die sonder Kraft und Balsam sind;", "tokens": ["Die", "son\u00b7der", "Kraft", "und", "Bal\u00b7sam", "sind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Entfernt euch mit den Anemonen,", "tokens": ["Ent\u00b7fernt", "euch", "mit", "den", "A\u00b7ne\u00b7mo\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Nahm und Ruhm ist nichts als Wind;", "tokens": ["Ihr", "Nahm", "und", "Ruhm", "ist", "nichts", "als", "Wind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VAFIN", "PIS", "KOKOM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Narcissen sind im besten Lande", "tokens": ["Nar\u00b7cis\u00b7sen", "sind", "im", "bes\u00b7ten", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Abri\u00df von dem Unbestande.", "tokens": ["Ein", "Ab\u00b7ri\u00df", "von", "dem", "Un\u00b7be\u00b7stan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Rose tr\u00e4gt das Blut der G\u00f6tter", "tokens": ["Die", "Ro\u00b7se", "tr\u00e4gt", "das", "Blut", "der", "G\u00f6t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und ist der Blumen K\u00f6nigin,", "tokens": ["Und", "ist", "der", "Blu\u00b7men", "K\u00f6\u00b7ni\u00b7gin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr Antliz sticht das sch\u00f6nste Wetter", "tokens": ["Ihr", "Ant\u00b7liz", "sticht", "das", "sch\u00f6ns\u00b7te", "Wet\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und selbst Aurorens Wangen hin,", "tokens": ["Und", "selbst", "Au\u00b7ro\u00b7rens", "Wan\u00b7gen", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NE", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie ist ein Stern der milden Erden", "tokens": ["Sie", "ist", "ein", "Stern", "der", "mil\u00b7den", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und kan von nichts verfinstert werden.", "tokens": ["Und", "kan", "von", "nichts", "ver\u00b7fins\u00b7tert", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "PIS", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Ros erquickt die bl\u00f6den Sinnen", "tokens": ["Die", "Ros", "er\u00b7quickt", "die", "bl\u00f6\u00b7den", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und hat das beste Zuckerrohr;", "tokens": ["Und", "hat", "das", "bes\u00b7te", "Zu\u00b7cker\u00b7rohr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr g\u00f6ldner Umfang bricht von innen", "tokens": ["Ihr", "g\u00f6ld\u00b7ner", "Um\u00b7fang", "bricht", "von", "in\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So wie die Sonn aus Nacht hervor;", "tokens": ["So", "wie", "die", "Sonn", "aus", "Nacht", "her\u00b7vor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Rose n\u00e4hrt die s\u00fc\u00dfen Triebe", "tokens": ["Die", "Ro\u00b7se", "n\u00e4hrt", "die", "s\u00fc\u00b7\u00dfen", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und reizt die Liebe selbst zur Liebe.", "tokens": ["Und", "reizt", "die", "Lie\u00b7be", "selbst", "zur", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Mit Rosen schm\u00fcck ich Haupt und Haare,", "tokens": ["Mit", "Ro\u00b7sen", "schm\u00fcck", "ich", "Haupt", "und", "Haa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Rosen tauch ich in den Wein,", "tokens": ["Die", "Ro\u00b7sen", "tauch", "ich", "in", "den", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Rose soll vor meine Jahre", "tokens": ["Die", "Ro\u00b7se", "soll", "vor", "mei\u00b7ne", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die allerbeste St\u00e4rckung seyn,", "tokens": ["Die", "al\u00b7ler\u00b7bes\u00b7te", "St\u00e4r\u00b7ckung", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Rose zieret meine Fl\u00f6then", "tokens": ["Die", "Ro\u00b7se", "zie\u00b7ret", "mei\u00b7ne", "Fl\u00f6\u00b7then"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und cr\u00f6nt mich m\u00e4chtigen Poeten.", "tokens": ["Und", "cr\u00f6nt", "mich", "m\u00e4ch\u00b7ti\u00b7gen", "Po\u00b7et\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Auf Rosen mach ich gute Reime,", "tokens": ["Auf", "Ro\u00b7sen", "mach", "ich", "gu\u00b7te", "Rei\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf Rosen schl\u00e4fet meine Brust,", "tokens": ["Auf", "Ro\u00b7sen", "schl\u00e4\u00b7fet", "mei\u00b7ne", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf Rosen hab ich sanfte Tr\u00e4ume", "tokens": ["Auf", "Ro\u00b7sen", "hab", "ich", "sanf\u00b7te", "Tr\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "PPER", "VVFIN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von still- und warm- und weicher Lust,", "tokens": ["Von", "still", "und", "war\u00b7m", "und", "wei\u00b7cher", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "TRUNC", "KON", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und wenn ich einst von hinnen fahre,", "tokens": ["Und", "wenn", "ich", "einst", "von", "hin\u00b7nen", "fah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So w\u00fcntsch ich Rosen auf die Baare.", "tokens": ["So", "w\u00fcnt\u00b7sch", "ich", "Ro\u00b7sen", "auf", "die", "Baa\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "O d\u00f6rft ich nur bey einer Rose", "tokens": ["O", "d\u00f6rft", "ich", "nur", "bey", "ei\u00b7ner", "Ro\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Bienen Honig naschen gehn!", "tokens": ["Wie", "Bie\u00b7nen", "Ho\u00b7nig", "na\u00b7schen", "gehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich lie\u00dfe warlich unserm Bose", "tokens": ["Ich", "lie\u00b7\u00dfe", "war\u00b7lich", "un\u00b7serm", "Bo\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den sch\u00f6n- und theuren Garthen stehn", "tokens": ["Den", "sch\u00f6n", "und", "theu\u00b7ren", "Gar\u00b7then", "stehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "TRUNC", "KON", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wollt es mir bald angew\u00f6hnen,", "tokens": ["Und", "wollt", "es", "mir", "bald", "an\u00b7ge\u00b7w\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mich nie nach fremder Kost zu sehnen.", "tokens": ["Mich", "nie", "nach", "frem\u00b7der", "Kost", "zu", "seh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Mit dieser Rose will ich scherzen,", "tokens": ["Mit", "die\u00b7ser", "Ro\u00b7se", "will", "ich", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und hier erschr\u00f6ckt mich nicht der Dorn;", "tokens": ["Und", "hier", "er\u00b7schr\u00f6ckt", "mich", "nicht", "der", "Dorn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn bey verliebt- und sch\u00f6nen Herzen", "tokens": ["Denn", "bey", "ver\u00b7lieb\u00b7t", "und", "sch\u00f6\u00b7nen", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "TRUNC", "KON", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Erg\u00f6zt uns oft ein kleiner Zorn,", "tokens": ["Er\u00b7g\u00f6zt", "uns", "oft", "ein", "klei\u00b7ner", "Zorn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und so viel Anmuth abzubrechen,", "tokens": ["Und", "so", "viel", "An\u00b7muth", "ab\u00b7zu\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Verachtet man ein kurzes Stechen.", "tokens": ["Ver\u00b7ach\u00b7tet", "man", "ein", "kur\u00b7zes", "Ste\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}