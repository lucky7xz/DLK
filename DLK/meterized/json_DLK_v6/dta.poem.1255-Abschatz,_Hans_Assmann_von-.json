{"dta.poem.1255": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Wer will hinfort best\u00e4ndig bleiben/", "tokens": ["Wer", "will", "hin\u00b7fort", "be\u00b7st\u00e4n\u00b7dig", "blei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn alles voller Unbestand?", "tokens": ["Wenn", "al\u00b7les", "vol\u00b7ler", "Un\u00b7be\u00b7stand", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer will in sein Ged\u00e4chtnis schreiben", "tokens": ["Wer", "will", "in", "sein", "Ge\u00b7d\u00e4cht\u00b7nis", "schrei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was andre zeichnen in den Sand?", "tokens": ["Was", "and\u00b7re", "zeich\u00b7nen", "in", "den", "Sand", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was macht ein Celadon auff Erden/", "tokens": ["Was", "macht", "ein", "Ce\u00b7la\u00b7don", "auff", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn jeder will ein Hylas werden?", "tokens": ["Wenn", "je\u00b7der", "will", "ein", "Hy\u00b7las", "wer\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VMFIN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Was will man sich mit Treue plagen?", "tokens": ["Was", "will", "man", "sich", "mit", "Treu\u00b7e", "pla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PIS", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Cupidens Fl\u00fcgel sind bekandt/", "tokens": ["Cu\u00b7pi\u00b7dens", "Fl\u00fc\u00b7gel", "sind", "be\u00b7kandt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "ADJD", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Die Venus hat von ihrem Wagen", "tokens": ["Die", "Ve\u00b7nus", "hat", "von", "ih\u00b7rem", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vorl\u00e4ngst den alten Zug verbannt/", "tokens": ["Vor\u00b7l\u00e4ngst", "den", "al\u00b7ten", "Zug", "ver\u00b7bannt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "F\u00fcr Schwan und Taube sieht man Raben", "tokens": ["F\u00fcr", "Schwan", "und", "Tau\u00b7be", "sieht", "man", "Ra\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PIS", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und Sperling\u2019 um die Deichsel draben.", "tokens": ["Und", "Sper\u00b7ling'", "um", "die", "Deich\u00b7sel", "dra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich kan ja die von Hertzen lieben/", "tokens": ["Ich", "kan", "ja", "die", "von", "Hert\u00b7zen", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und jen\u2019 aus Pflicht und H\u00f6fligkeit/", "tokens": ["Und", "jen'", "aus", "Pflicht", "und", "H\u00f6f\u00b7lig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bey dieser mein Vergn\u00fcgen \u00fcben/", "tokens": ["Bey", "die\u00b7ser", "mein", "Ver\u00b7gn\u00fc\u00b7gen", "\u00fc\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit jener schliessen meine Zeit:", "tokens": ["Mit", "je\u00b7ner", "schlies\u00b7sen", "mei\u00b7ne", "Zeit", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "An Ort und Art/ Gestalt und Stunden", "tokens": ["An", "Ort", "und", "Art", "/", "Ge\u00b7stalt", "und", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$(", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ist unser Lieben nicht gebunden.", "tokens": ["Ist", "un\u00b7ser", "Lie\u00b7ben", "nicht", "ge\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "So pflegt manch leichter Sinn zu sagen/", "tokens": ["So", "pflegt", "manch", "leich\u00b7ter", "Sinn", "zu", "sa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "ADJA", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der sich mit Schaden luftig macht/", "tokens": ["Der", "sich", "mit", "Scha\u00b7den", "luf\u00b7tig", "macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verbotnen Raub darvon zu tragen", "tokens": ["Ver\u00b7bot\u00b7nen", "Raub", "dar\u00b7von", "zu", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "PAV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit tausend L\u00fcsten lebt bedacht.", "tokens": ["Mit", "tau\u00b7send", "L\u00fcs\u00b7ten", "lebt", "be\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wer sich der Treue will befleissen/", "tokens": ["Wer", "sich", "der", "Treu\u00b7e", "will", "be\u00b7fleis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mu\u00df alber oder einsam heissen.", "tokens": ["Mu\u00df", "al\u00b7ber", "o\u00b7der", "ein\u00b7sam", "heis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Was aber fragt nach solchem Schm\u00e4hen", "tokens": ["Was", "a\u00b7ber", "fragt", "nach", "sol\u00b7chem", "Schm\u00e4\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Harnisch tugendvoller Brust.", "tokens": ["Der", "Har\u00b7nisch", "tu\u00b7gend\u00b7vol\u00b7ler", "Brust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Ausgang wird uns lassen sehen/", "tokens": ["Der", "Aus\u00b7gang", "wird", "uns", "las\u00b7sen", "se\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auff wen noch wart die beste Lust.", "tokens": ["Auff", "wen", "noch", "wart", "die", "bes\u00b7te", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWS", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn Stein und Gicht die Glieder brechen", "tokens": ["Wenn", "Stein", "und", "Gicht", "die", "Glie\u00b7der", "bre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wird sie an ihm der Nachbar r\u00e4chen.", "tokens": ["Wird", "sie", "an", "ihm", "der", "Nach\u00b7bar", "r\u00e4\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "ART", "NN", "VVINF", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}}}}