{"textgrid.poem.49766": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Des Dichters Klage", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was bin ich f\u00fcr ein gro\u00dfer Lump!", "tokens": ["Was", "bin", "ich", "f\u00fcr", "ein", "gro\u00b7\u00dfer", "Lump", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich leb' das ganze Jahr auf Pump,", "tokens": ["Ich", "leb'", "das", "gan\u00b7ze", "Jahr", "auf", "Pump", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich stecke tief in Schulden.", "tokens": ["Ich", "ste\u00b7cke", "tief", "in", "Schul\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O Himmel, Herrschaft, Sapperlott!", "tokens": ["O", "Him\u00b7mel", ",", "Herr\u00b7schaft", ",", "Sap\u00b7per\u00b7lott", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich treibe mit dem H\u00f6chsten Spott.", "tokens": ["Ich", "trei\u00b7be", "mit", "dem", "H\u00f6chs\u00b7ten", "Spott", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie lange wird man's dulden?", "tokens": ["Wie", "lan\u00b7ge", "wird", "man's", "dul\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Tante sprach: \u00bbMein liebes Kind,", "tokens": ["Die", "Tan\u00b7te", "sprach", ":", "\u00bb", "Mein", "lie\u00b7bes", "Kind", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sei nicht so, wie die andern sind!", "tokens": ["Sei", "nicht", "so", ",", "wie", "die", "an\u00b7dern", "sind", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "$,", "PWAV", "ART", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mich freut nicht, was ich sehe.", "tokens": ["Mich", "freut", "nicht", ",", "was", "ich", "se\u00b7he", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nimm dir ein h\u00fcbsches M\u00e4dchen nur", "tokens": ["Nimm", "dir", "ein", "h\u00fcb\u00b7sches", "M\u00e4d\u00b7chen", "nur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit voller, \u00fcppiger Figur;", "tokens": ["Mit", "vol\u00b7ler", ",", "\u00fcp\u00b7pi\u00b7ger", "Fi\u00b7gur", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Begib dich in die Ehe!\u00ab", "tokens": ["Be\u00b7gib", "dich", "in", "die", "E\u00b7he", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und gestern abend der Herr Rat,", "tokens": ["Und", "ge\u00b7stern", "a\u00b7bend", "der", "Herr", "Rat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der sagte: \u00bbEs ist wirklich schad',", "tokens": ["Der", "sag\u00b7te", ":", "\u00bb", "Es", "ist", "wirk\u00b7lich", "schad'", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$.", "$(", "PPER", "VAFIN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie haben doch Talente!", "tokens": ["Sie", "ha\u00b7ben", "doch", "Ta\u00b7len\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie w\u00fcrden sicher Sekret\u00e4r", "tokens": ["Sie", "w\u00fcr\u00b7den", "si\u00b7cher", "Se\u00b7kre\u00b7t\u00e4r"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und sp\u00e4ter auch noch etwas mehr", "tokens": ["Und", "sp\u00e4\u00b7ter", "auch", "noch", "et\u00b7was", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "ADV", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit einem Staatspatente.\u00ab", "tokens": ["Mit", "ei\u00b7nem", "Staats\u00b7pa\u00b7ten\u00b7te", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "$.", "$("], "meter": "-+--+--", "measure": "iambic.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Mein Onkel, der ist ziemlich k\u00fchl;", "tokens": ["Mein", "On\u00b7kel", ",", "der", "ist", "ziem\u00b7lich", "k\u00fchl", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn er mich sieht, dann wird ihm schw\u00fcl,", "tokens": ["Wenn", "er", "mich", "sieht", ",", "dann", "wird", "ihm", "schw\u00fcl", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er geht mir durch die Lappen.", "tokens": ["Er", "geht", "mir", "durch", "die", "Lap\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er sieht sich nach 'nem Laden um,", "tokens": ["Er", "sieht", "sich", "nach", "'nem", "La\u00b7den", "um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er geht geschwinde hintenrum,", "tokens": ["Er", "geht", "ge\u00b7schwin\u00b7de", "hin\u00b7ten\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er glaubt, er mu\u00df berappen.", "tokens": ["Er", "glaubt", ",", "er", "mu\u00df", "be\u00b7rap\u00b7pen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ich bin entgleist als Existenz", "tokens": ["Ich", "bin", "ent\u00b7gleist", "als", "E\u00b7xis\u00b7tenz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "KOKOM", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und kenne selbst die Konsequenz", "tokens": ["Und", "ken\u00b7ne", "selbst", "die", "Kon\u00b7se\u00b7quenz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In unserm Staatsverbande;", "tokens": ["In", "un\u00b7serm", "Staats\u00b7ver\u00b7ban\u00b7de", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit mir, da geht's noch einmal schief,", "tokens": ["Mit", "mir", ",", "da", "geht's", "noch", "ein\u00b7mal", "schief", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "ADV", "VVFIN", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich sinke noch einmal sehr tief;", "tokens": ["Ich", "sin\u00b7ke", "noch", "ein\u00b7mal", "sehr", "tief", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es ist 'ne Affenschande.", "tokens": ["Es", "ist", "'ne", "Af\u00b7fen\u00b7schan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}