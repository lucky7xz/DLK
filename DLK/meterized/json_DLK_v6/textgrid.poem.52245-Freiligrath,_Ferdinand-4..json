{"textgrid.poem.52245": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und andre noch will ich dir r\u00fchmend zeigen;", "tokens": ["Und", "and\u00b7re", "noch", "will", "ich", "dir", "r\u00fch\u00b7mend", "zei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADV", "VMFIN", "PPER", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie kreuzten wohl und kehren jetzt vom Zuge;", "tokens": ["Sie", "kreuz\u00b7ten", "wohl", "und", "keh\u00b7ren", "jetzt", "vom", "Zu\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KON", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie wehn heran mit majest\u00e4t'schem Fluge:", "tokens": ["Sie", "wehn", "he\u00b7ran", "mit", "ma\u00b7je\u00b7st\u00e4t'\u00b7schem", "Flu\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u00bbder Alexander Humboldt\u00ab f\u00fchrt den Reigen!", "tokens": ["\u00bb", "der", "A\u00b7lex\u00b7an\u00b7der", "Hum\u00b7boldt", "\u00ab", "f\u00fchrt", "den", "Rei\u00b7gen", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NE", "NE", "$(", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ha, sieh den \u00bbGoethe\u00ab tief sein Bugspreit neigen!", "tokens": ["Ha", ",", "sieh", "den", "\u00bb", "Goe\u00b7the", "\u00ab", "tief", "sein", "Bug\u00b7spreit", "nei\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVIMP", "ART", "$(", "NE", "$(", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihm nach \u00bbder Schiller\u00ab, auch mit tiefem Buge!", "tokens": ["Ihm", "nach", "\u00bb", "der", "Schil\u00b7ler", "\u00ab", ",", "auch", "mit", "tie\u00b7fem", "Bu\u00b7ge", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "$(", "ART", "NN", "$(", "$,", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbdie freie Presse\u00ab l\u00e4\u00dft mit gutem Fuge", "tokens": ["\u00bb", "die", "frei\u00b7e", "Pres\u00b7se", "\u00ab", "l\u00e4\u00dft", "mit", "gu\u00b7tem", "Fu\u00b7ge"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ART", "ADJA", "NN", "$(", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Leuchtende Kuglen in die L\u00fcfte steigen!", "tokens": ["Leuch\u00b7ten\u00b7de", "Kug\u00b7len", "in", "die", "L\u00fcf\u00b7te", "stei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Die fernsten dr\u00fcben kann ich nicht erraten!", "tokens": ["Die", "ferns\u00b7ten", "dr\u00fc\u00b7ben", "kann", "ich", "nicht", "er\u00b7ra\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "La\u00df ungenannt sie vor dem Winde laufen!", "tokens": ["La\u00df", "un\u00b7ge\u00b7nannt", "sie", "vor", "dem", "Win\u00b7de", "lau\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJD", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Eins ist gewi\u00df: sie haben t\u00fccht'ge Paten!", "tokens": ["Eins", "ist", "ge\u00b7wi\u00df", ":", "sie", "ha\u00b7ben", "t\u00fccht'\u00b7ge", "Pa\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "$.", "PPER", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wir brauchen Namen wahrlich nicht zu kaufen!", "tokens": ["Wir", "brau\u00b7chen", "Na\u00b7men", "wahr\u00b7lich", "nicht", "zu", "kau\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ADV", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wir haben M\u00e4nner, haben Tage, Taten: \u2013", "tokens": ["Wir", "ha\u00b7ben", "M\u00e4n\u00b7ner", ",", "ha\u00b7ben", "Ta\u00b7ge", ",", "Ta\u00b7ten", ":", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "NN", "$,", "VAFIN", "NN", "$,", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mehr Schiffe nur! Wir wollen sie schon taufen!", "tokens": ["Mehr", "Schif\u00b7fe", "nur", "!", "Wir", "wol\u00b7len", "sie", "schon", "tau\u00b7fen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "$.", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}