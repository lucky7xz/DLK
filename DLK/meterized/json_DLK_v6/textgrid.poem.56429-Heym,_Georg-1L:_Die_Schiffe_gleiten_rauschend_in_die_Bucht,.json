{"textgrid.poem.56429": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Schiffe gleiten rauschend in die Bucht,", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Schiffe gleiten rauschend in die Bucht,", "tokens": ["Die", "Schif\u00b7fe", "glei\u00b7ten", "rau\u00b7schend", "in", "die", "Bucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von hundert Schultern in das Meer geschoben.", "tokens": ["Von", "hun\u00b7dert", "Schul\u00b7tern", "in", "das", "Meer", "ge\u00b7scho\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Menge dr\u00e4ngt sich an den Borden oben.", "tokens": ["Die", "Men\u00b7ge", "dr\u00e4ngt", "sich", "an", "den", "Bor\u00b7den", "o\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Schiffe sinken tief von Last und Wucht.", "tokens": ["Die", "Schif\u00b7fe", "sin\u00b7ken", "tief", "von", "Last", "und", "Wucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Segel schwanken auf von den Verdecken,", "tokens": ["Die", "Se\u00b7gel", "schwan\u00b7ken", "auf", "von", "den", "Ver\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu hundert ziehn sie auf sie an den Seilen.", "tokens": ["Zu", "hun\u00b7dert", "ziehn", "sie", "auf", "sie", "an", "den", "Sei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "VVFIN", "PPER", "APPR", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Ankerketten kappen sie mit Beilen.", "tokens": ["Die", "An\u00b7ker\u00b7ket\u00b7ten", "kap\u00b7pen", "sie", "mit", "Bei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit Stangen sto\u00dfen ab sie von den Hecken.", "tokens": ["Mit", "Stan\u00b7gen", "sto\u00b7\u00dfen", "ab", "sie", "von", "den", "He\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "APPR", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Es f\u00fcllen Raum und Deck sich mit dem Trosse.", "tokens": ["Es", "f\u00fcl\u00b7len", "Raum", "und", "Deck", "sich", "mit", "dem", "Tros\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Eunuchen, Sklaven, Priester und Soldaten.", "tokens": ["Eu\u00b7nu\u00b7chen", ",", "Skla\u00b7ven", ",", "Pries\u00b7ter", "und", "Sol\u00b7da\u00b7ten", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Rennen, Schreien, wahnsinnig Gebaren.", "tokens": ["Ein", "Ren\u00b7nen", ",", "Schrei\u00b7en", ",", "wahn\u00b7sin\u00b7nig", "Ge\u00b7ba\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "ADJD", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Ein Kampf und ziellos Durcheinanderfahren.", "tokens": ["Ein", "Kampf", "und", "ziel\u00b7los", "Durch\u00b7ein\u00b7an\u00b7der\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Elefanten durch die Fluten waten.", "tokens": ["Die", "E\u00b7lef\u00b7an\u00b7ten", "durch", "die", "Flu\u00b7ten", "wa\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Schiffe sto\u00dfen w\u00fctend die Kolosse.", "tokens": ["Die", "Schif\u00b7fe", "sto\u00b7\u00dfen", "w\u00fc\u00b7tend", "die", "Ko\u00b7los\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}