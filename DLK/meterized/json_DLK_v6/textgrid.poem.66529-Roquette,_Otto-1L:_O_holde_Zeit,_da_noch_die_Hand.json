{"textgrid.poem.66529": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: O holde Zeit, da noch die Hand", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O holde Zeit, da noch die Hand", "tokens": ["O", "hol\u00b7de", "Zeit", ",", "da", "noch", "die", "Hand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "KOUS", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Um Wunden bangt vom Rosendorn!", "tokens": ["Um", "Wun\u00b7den", "bangt", "vom", "Ro\u00b7sen\u00b7dorn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da, mit sich selber unbekannt,", "tokens": ["Da", ",", "mit", "sich", "sel\u00b7ber", "un\u00b7be\u00b7kannt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "PRF", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Herz entbrennt in kleinem Zorn!", "tokens": ["Das", "Herz", "ent\u00b7brennt", "in", "klei\u00b7nem", "Zorn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und dennoch mag vom Rosenstrauch", "tokens": ["Und", "den\u00b7noch", "mag", "vom", "Ro\u00b7sen\u00b7strauch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit banger Lust die Hand nicht lassen.", "tokens": ["Mit", "ban\u00b7ger", "Lust", "die", "Hand", "nicht", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So war seit alter Zeit der Brauch", "tokens": ["So", "war", "seit", "al\u00b7ter", "Zeit", "der", "Brauch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Bei Buben und bei M\u00e4dchen auch.", "tokens": ["Bei", "Bu\u00b7ben", "und", "bei", "M\u00e4d\u00b7chen", "auch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O harte Noth um einen Blick!", "tokens": ["O", "har\u00b7te", "Noth", "um", "ei\u00b7nen", "Blick", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Ha\u00df um eines Kusses Raub!", "tokens": ["O", "Ha\u00df", "um", "ei\u00b7nes", "Kus\u00b7ses", "Raub", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es braucht das Herz sein Mi\u00dfgeschick,", "tokens": ["Es", "braucht", "das", "Herz", "sein", "Mi\u00df\u00b7ge\u00b7schick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und hofft sich der Vers\u00f6hnung taub.", "tokens": ["Und", "hofft", "sich", "der", "Ver\u00b7s\u00f6h\u00b7nung", "taub", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wo Feuer brennt, da giebt es Rauch,", "tokens": ["Wo", "Feu\u00b7er", "brennt", ",", "da", "giebt", "es", "Rauch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und Jugend spielt mit Feuer gerne.", "tokens": ["Und", "Ju\u00b7gend", "spielt", "mit", "Feu\u00b7er", "ger\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "NN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So war seit alter Zeit der Brauch", "tokens": ["So", "war", "seit", "al\u00b7ter", "Zeit", "der", "Brauch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Bei Buben und bei M\u00e4dchen auch.", "tokens": ["Bei", "Bu\u00b7ben", "und", "bei", "M\u00e4d\u00b7chen", "auch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und hat der Groll recht wild gezehrt,", "tokens": ["Und", "hat", "der", "Groll", "recht", "wild", "ge\u00b7zehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und hat man bitter sich geplagt,", "tokens": ["Und", "hat", "man", "bit\u00b7ter", "sich", "ge\u00b7plagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIS", "ADJD", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann kommt ein Seufzer, tief beschwert,", "tokens": ["Dann", "kommt", "ein", "Seuf\u00b7zer", ",", "tief", "be\u00b7schwert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Blick, der wieder fr\u00f6hlich wagt.", "tokens": ["Ein", "Blick", ",", "der", "wie\u00b7der", "fr\u00f6h\u00b7lich", "wagt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In Bl\u00fcthen steht der Rosenstrauch,", "tokens": ["In", "Bl\u00fc\u00b7then", "steht", "der", "Ro\u00b7sen\u00b7strauch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es spotten Herz und Hand der Dornen.", "tokens": ["Es", "spot\u00b7ten", "Herz", "und", "Hand", "der", "Dor\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So war seit alter Zeit der Brauch", "tokens": ["So", "war", "seit", "al\u00b7ter", "Zeit", "der", "Brauch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Bei Buben und bei M\u00e4dchen auch.", "tokens": ["Bei", "Bu\u00b7ben", "und", "bei", "M\u00e4d\u00b7chen", "auch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}