{"textgrid.poem.39746": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "8. Im Lauterthal", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was lachen mich die M\u00e4nner,", "tokens": ["Was", "la\u00b7chen", "mich", "die", "M\u00e4n\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die schmucken M\u00e4gdlein aus,", "tokens": ["Die", "schmu\u00b7cken", "M\u00e4gd\u00b7lein", "aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df ich so eifrig schaue", "tokens": ["Da\u00df", "ich", "so", "eif\u00b7rig", "schau\u00b7e"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nach dem zerfallnen Haus?", "tokens": ["Nach", "dem", "zer\u00b7fall\u00b7nen", "Haus", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da\u00df ich so sehnlich folge", "tokens": ["Da\u00df", "ich", "so", "sehn\u00b7lich", "fol\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Flusses krummem Lauf,", "tokens": ["Des", "Flus\u00b7ses", "krum\u00b7mem", "Lauf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df ich so r\u00fcstig steige", "tokens": ["Da\u00df", "ich", "so", "r\u00fcs\u00b7tig", "stei\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den hohen Berg hinauf?", "tokens": ["Den", "ho\u00b7hen", "Berg", "hin\u00b7auf", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie m\u00f6gen es nicht glauben,", "tokens": ["Sie", "m\u00f6\u00b7gen", "es", "nicht", "glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df mir durch Thal und H\u00f6hn", "tokens": ["Da\u00df", "mir", "durch", "Thal", "und", "H\u00f6hn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Lust den Schritt befl\u00fcgelt", "tokens": ["Die", "Lust", "den", "Schritt", "be\u00b7fl\u00fc\u00b7gelt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bei dieser St\u00fcrme Wehn;", "tokens": ["Bei", "die\u00b7ser", "St\u00fcr\u00b7me", "Wehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie loben Stadt und Ebne", "tokens": ["Sie", "lo\u00b7ben", "Stadt", "und", "Eb\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und schielen halb mit Neid", "tokens": ["Und", "schie\u00b7len", "halb", "mit", "Neid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf meine weichen H\u00e4nde", "tokens": ["Auf", "mei\u00b7ne", "wei\u00b7chen", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und auf mein st\u00e4dtisch Kleid.", "tokens": ["Und", "auf", "mein", "st\u00e4d\u00b7tisch", "Kleid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ihr M\u00e4nner des Gebirges!", "tokens": ["Ihr", "M\u00e4n\u00b7ner", "des", "Ge\u00b7bir\u00b7ges", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es thut mir herzlich weh,", "tokens": ["Es", "thut", "mir", "herz\u00b7lich", "weh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df ihr die Nahrung k\u00e4rglich", "tokens": ["Da\u00df", "ihr", "die", "Nah\u00b7rung", "k\u00e4rg\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Abzwinget eurem Schnee;", "tokens": ["Ab\u00b7zwin\u00b7get", "eu\u00b7rem", "Schnee", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Da\u00df euren schlanken T\u00f6chtern", "tokens": ["Da\u00df", "eu\u00b7ren", "schlan\u00b7ken", "T\u00f6ch\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Last den R\u00fccken beugt,", "tokens": ["Die", "Last", "den", "R\u00fc\u00b7cken", "beugt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und euer Berg dem Durste", "tokens": ["Und", "eu\u00b7er", "Berg", "dem", "Durs\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kein Tr\u00f6pfchen Weins erzeugt.", "tokens": ["Kein", "Tr\u00f6pf\u00b7chen", "Weins", "er\u00b7zeugt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Doch d\u00e4ucht mir noch viel bittrer", "tokens": ["Doch", "d\u00e4ucht", "mir", "noch", "viel", "bit\u00b7trer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PIAT", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als euer Durst und Schwei\u00df,", "tokens": ["Als", "eu\u00b7er", "Durst", "und", "Schwei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df euer Geist vom Sch\u00f6nen,", "tokens": ["Da\u00df", "eu\u00b7er", "Geist", "vom", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von Gottes Bild nichts wei\u00df.", "tokens": ["Von", "Got\u00b7tes", "Bild", "nichts", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "PIS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Die Noth, an der ihr zehret,", "tokens": ["Die", "Noth", ",", "an", "der", "ihr", "zeh\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der euer Leib sich b\u00fcckt,", "tokens": ["Der", "eu\u00b7er", "Leib", "sich", "b\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hat euch ins Herz gefressen,", "tokens": ["Hat", "euch", "ins", "Herz", "ge\u00b7fres\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hat euch den Sinn erdr\u00fcckt!", "tokens": ["Hat", "euch", "den", "Sinn", "er\u00b7dr\u00fcckt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "In Seiner Leidenswoche", "tokens": ["In", "Sei\u00b7ner", "Lei\u00b7dens\u00b7wo\u00b7che"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durchwandl' ich dieses Thal:", "tokens": ["Durch\u00b7wandl'", "ich", "die\u00b7ses", "Thal", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er kennet jeden Kummer,", "tokens": ["Er", "ken\u00b7net", "je\u00b7den", "Kum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er heilet jede Qual!", "tokens": ["Er", "hei\u00b7let", "je\u00b7de", "Qual", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Geb' Er dem Jahre Segen,", "tokens": ["Geb'", "Er", "dem", "Jah\u00b7re", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df es euch tr\u00e4nkt und speist,", "tokens": ["Da\u00df", "es", "euch", "tr\u00e4nkt", "und", "speist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und l\u00f6se dann die Binde", "tokens": ["Und", "l\u00f6\u00b7se", "dann", "die", "Bin\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von dem verh\u00fcllten Geist!", "tokens": ["Von", "dem", "ver\u00b7h\u00fcll\u00b7ten", "Geist", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}