{"textgrid.poem.45070": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[mit Volkslied und Mittelhochdeutsch]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit Volkslied und Mittelhochdeutsch", "tokens": ["Mit", "Volks\u00b7lied", "und", "Mit\u00b7tel\u00b7hoch\u00b7deutsch"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wei\u00df ich f\u00fcrwahr nichts zu machen.", "tokens": ["Wei\u00df", "ich", "f\u00fcr\u00b7wahr", "nichts", "zu", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PIS", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer trinkt, solang es Brunnen gibt,", "tokens": ["Wer", "trinkt", ",", "so\u00b7lang", "es", "Brun\u00b7nen", "gibt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus Radspur gern und Lachen.", "tokens": ["Aus", "Rad\u00b7spur", "gern", "und", "La\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}