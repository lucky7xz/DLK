{"textgrid.poem.52841": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "M\u00fcnchen", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn Einer sich was Gutes w\u00fcnscht", "tokens": ["Wenn", "Ei\u00b7ner", "sich", "was", "Gu\u00b7tes", "w\u00fcnscht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PRF", "PWS", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und hat nicht viel zu w\u00fcnschen,", "tokens": ["Und", "hat", "nicht", "viel", "zu", "w\u00fcn\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der thut sich einen gro\u00dfen Dienst", "tokens": ["Der", "thut", "sich", "ei\u00b7nen", "gro\u00b7\u00dfen", "Dienst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und geht sogleich nach M\u00fcnchen.", "tokens": ["Und", "geht", "sog\u00b7leich", "nach", "M\u00fcn\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein' Rausch zu kaufen, ist nicht schwier'g", "tokens": ["Ein'", "Rausch", "zu", "kau\u00b7fen", ",", "ist", "nicht", "schwier'g"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,", "VAFIN", "PTKNEG", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das zweit Haus ist ein Wirthshaus,", "tokens": ["Das", "zweit", "Haus", "ist", "ein", "Wirths\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Das erste Haus ist eine Kirch", "tokens": ["Das", "ers\u00b7te", "Haus", "ist", "ei\u00b7ne", "Kirch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Oder sonst ein Seelenhirtshaus.", "tokens": ["O\u00b7der", "sonst", "ein", "See\u00b7len\u00b7hirts\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Drum wer nicht auf der Stra\u00df will stehn,", "tokens": ["Drum", "wer", "nicht", "auf", "der", "Stra\u00df", "will", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PWS", "PTKNEG", "APPR", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat auch nicht lang zu w\u00e4hlen,", "tokens": ["Hat", "auch", "nicht", "lang", "zu", "w\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Auch lassen sich die M\u00e4dels sehn,", "tokens": ["Auch", "las\u00b7sen", "sich", "die", "M\u00e4\u00b7dels", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Fast lauter sch\u00f6ne Seelen.", "tokens": ["Fast", "lau\u00b7ter", "sch\u00f6\u00b7ne", "See\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Viel Wesens macht man aus der Kunst", "tokens": ["Viel", "We\u00b7sens", "macht", "man", "aus", "der", "Kunst"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "PIS", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In h\u00f6hern Regionen,", "tokens": ["In", "h\u00f6\u00b7hern", "Re\u00b7gi\u00b7o\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und H\u00e4ndel hat man ganz umsonst", "tokens": ["Und", "H\u00e4n\u00b7del", "hat", "man", "ganz", "um\u00b7sonst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VAFIN", "PIS", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach allen Dimensionen.", "tokens": ["Nach", "al\u00b7len", "Di\u00b7men\u00b7si\u00b7o\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Auch gibt es nebenher Gewinn", "tokens": ["Auch", "gibt", "es", "ne\u00b7ben\u00b7her", "Ge\u00b7winn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bei Menschen, welche malen,", "tokens": ["Bei", "Men\u00b7schen", ",", "wel\u00b7che", "ma\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die stellen Einen nackig hin", "tokens": ["Die", "stel\u00b7len", "Ei\u00b7nen", "na\u00b7ckig", "hin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "ADJD", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und thun es gut bezahlen.", "tokens": ["Und", "thun", "es", "gut", "be\u00b7zah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Dann kommt man oft bei Tag und Nacht", "tokens": ["Dann", "kommt", "man", "oft", "bei", "Tag", "und", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In's \u00e4rgste Regenwetter", "tokens": ["In's", "\u00e4rgs\u00b7te", "Re\u00b7gen\u00b7wet\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und nimmt man sich nicht recht in Acht,", "tokens": ["Und", "nimmt", "man", "sich", "nicht", "recht", "in", "Acht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PRF", "PTKNEG", "ADJD", "APPR", "CARD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch in die fliegenden Bl\u00e4tter.", "tokens": ["Auch", "in", "die", "flie\u00b7gen\u00b7den", "Bl\u00e4t\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}