{"textgrid.poem.31647": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "19. Dreyst\u00e4ndige Reimen", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Gottes Herz! l\u00e4st dich dann betend nicht durchdringen?", "tokens": ["O", "Got\u00b7tes", "Herz", "!", "l\u00e4st", "dich", "dann", "be\u00b7tend", "nicht", "durch\u00b7drin\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "NN", "$.", "VVFIN", "PPER", "ADV", "ADJD", "PTKNEG", "VVINF", "$."], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "bist von Demand / la\u00df dich des Gottes-L\u00e4mmleins Blut", "tokens": ["bist", "von", "De\u00b7mand", "/", "la\u00df", "dich", "des", "Got\u00b7tes\u00b7L\u00e4mm\u00b7leins", "Blut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "NE", "$(", "VVIMP", "PPER", "ART", "NN", "NN"], "meter": "+-+----+-+-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "erweichen doch / und mir zu Trost und H\u00fclff verbinden.", "tokens": ["er\u00b7wei\u00b7chen", "doch", "/", "und", "mir", "zu", "Trost", "und", "H\u00fclff", "ver\u00b7bin\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$(", "KON", "PPER", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Heldenglaub / kont er doch sonst den Himmel zwingen.", "tokens": ["Der", "Hel\u00b7den\u00b7glaub", "/", "kont", "er", "doch", "sonst", "den", "Him\u00b7mel", "zwin\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "VMFIN", "PPER", "ADV", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich hab den nicht: ein glimmend t\u00e4chtlein ist auch gut.", "tokens": ["Ich", "hab", "den", "nicht", ":", "ein", "glim\u00b7mend", "t\u00e4cht\u00b7lein", "ist", "auch", "gut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "PTKNEG", "$.", "ART", "ADJD", "ADJD", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mein Schwachheit nimm / Herr / wollest damit \u00fcberwinden.", "tokens": ["Mein", "Schwach\u00b7heit", "nimm", "/", "Herr", "/", "wol\u00b7lest", "da\u00b7mit", "\u00fc\u00b7berw\u00b7in\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVIMP", "$(", "NN", "$(", "VMFIN", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}