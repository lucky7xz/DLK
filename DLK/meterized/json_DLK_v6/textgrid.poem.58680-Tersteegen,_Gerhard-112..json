{"textgrid.poem.58680": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "112.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich war der K\u00f6nig selbst, durch Hochmut ganz verjaget", "tokens": ["Ich", "war", "der", "K\u00f6\u00b7nig", "selbst", ",", "durch", "Hoch\u00b7mut", "ganz", "ver\u00b7ja\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "$,", "APPR", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus Lust und Land und Stand und gar zum Vieh gemacht,", "tokens": ["Aus", "Lust", "und", "Land", "und", "Stand", "und", "gar", "zum", "Vieh", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "KON", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In dummer Unvernunft mit falscher Lust geplaget;", "tokens": ["In", "dum\u00b7mer", "Un\u00b7ver\u00b7nunft", "mit", "fal\u00b7scher", "Lust", "ge\u00b7pla\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie herrlich stund ich doch, nun kriech' ich so veracht't!", "tokens": ["Wie", "herr\u00b7lich", "stund", "ich", "doch", ",", "nun", "kriech'", "ich", "so", "ver\u00b7acht't", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "ADV", "$,", "ADV", "VVFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Herr, wann doch werden sich die sieben Zeiten enden,", "tokens": ["Herr", ",", "wann", "doch", "wer\u00b7den", "sich", "die", "sie\u00b7ben", "Zei\u00b7ten", "en\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ADV", "VAFIN", "PRF", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Bis ich mein tiefes Nichts und deine Macht erfahr'?", "tokens": ["Bis", "ich", "mein", "tie\u00b7fes", "Nichts", "und", "dei\u00b7ne", "Macht", "er\u00b7fahr'", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "KON", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wann sich gen Himmel nur die Demutsaugen wenden,", "tokens": ["Wann", "sich", "gen", "Him\u00b7mel", "nur", "die", "De\u00b7mu\u00b7tsau\u00b7gen", "wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "APPR", "NN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dann werd ich wieder klug und herrlich, wie ich war.", "tokens": ["Dann", "werd", "ich", "wie\u00b7der", "klug", "und", "herr\u00b7lich", ",", "wie", "ich", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "$,", "PWAV", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}