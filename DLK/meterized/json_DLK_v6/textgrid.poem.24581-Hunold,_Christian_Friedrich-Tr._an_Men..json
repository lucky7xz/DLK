{"textgrid.poem.24581": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Tr. an Men.", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du l\u00e4\u00dft mich allezeit ein Straff-Gedichte lesen/", "tokens": ["Du", "l\u00e4\u00dft", "mich", "al\u00b7le\u00b7zeit", "ein", "Straff\u00b7Ge\u00b7dich\u00b7te", "le\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und schreibest: wie mein Hertz der Lieb ergeben sey.", "tokens": ["Und", "schrei\u00b7best", ":", "wie", "mein", "Hertz", "der", "Lieb", "er\u00b7ge\u00b7ben", "sey", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "KOKOM", "PPOSAT", "NN", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du schreibst so/ wie du bist/ nicht wie du bist gewesen;", "tokens": ["Du", "schreibst", "so", "/", "wie", "du", "bist", "/", "nicht", "wie", "du", "bist", "ge\u00b7we\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$(", "PWAV", "PPER", "VAFIN", "$(", "PTKNEG", "PWAV", "PPER", "VAFIN", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sonst legtest du/ wie ich/ noch ", "tokens": ["Sonst", "leg\u00b7test", "du", "/", "wie", "ich", "/", "noch"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PPER", "$(", "PWAV", "PPER", "$(", "ADV"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Allein/ geehrter Freund/ die Zeit nur zu vertreiben/", "tokens": ["Al\u00b7lein", "/", "ge\u00b7ehr\u00b7ter", "Freund", "/", "die", "Zeit", "nur", "zu", "ver\u00b7trei\u00b7ben", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "ADJA", "NN", "$(", "ART", "NN", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Kan ich von lieben zwar/ doch ohne Liebe schreiben.", "tokens": ["Kan", "ich", "von", "lie\u00b7ben", "zwar", "/", "doch", "oh\u00b7ne", "Lie\u00b7be", "schrei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ADJA", "ADV", "$(", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}