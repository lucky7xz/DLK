{"dta.poem.7520": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "19.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer Schranken denkend setzt, die wirklich nicht vorhanden,", "tokens": ["Wer", "Schran\u00b7ken", "den\u00b7kend", "setzt", ",", "die", "wirk\u00b7lich", "nicht", "vor\u00b7han\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVPP", "VVFIN", "$,", "PRELS", "ADJD", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dann hinweg sie denkt, der hat die Welt verstanden.", "tokens": ["Und", "dann", "hin\u00b7weg", "sie", "denkt", ",", "der", "hat", "die", "Welt", "ver\u00b7stan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APZR", "PPER", "VVFIN", "$,", "PRELS", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Alswie Geometrie in ihren Liniennetzen", "tokens": ["Als\u00b7wie", "Geo\u00b7me\u00b7trie", "in", "ih\u00b7ren", "Li\u00b7ni\u00b7en\u00b7net\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Den Raum, so f\u00e4ngt sich selbst das Denken in Gesetzen.", "tokens": ["Den", "Raum", ",", "so", "f\u00e4ngt", "sich", "selbst", "das", "Den\u00b7ken", "in", "Ge\u00b7set\u00b7zen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "VVFIN", "PRF", "ADV", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Anschaulich macht man uns die Welt durch L\u00e4ndercharten,", "tokens": ["An\u00b7schau\u00b7lich", "macht", "man", "uns", "die", "Welt", "durch", "L\u00e4n\u00b7der\u00b7char\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PIS", "PRF", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nun m\u00fcssen wir des Geists Sterncharten noch erwarten.", "tokens": ["Nun", "m\u00fcs\u00b7sen", "wir", "des", "Geists", "Stern\u00b7char\u00b7ten", "noch", "er\u00b7war\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Inde\u00df geht, auf Gefahr den Richtweg zu verlieren,", "tokens": ["In\u00b7de\u00df", "geht", ",", "auf", "Ge\u00b7fahr", "den", "Richt\u00b7weg", "zu", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "APPR", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Geist durch sein Gebiet, wie wir durchs Feld spazieren.", "tokens": ["Der", "Geist", "durch", "sein", "Ge\u00b7biet", ",", "wie", "wir", "durchs", "Feld", "spa\u00b7zie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,", "PWAV", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}