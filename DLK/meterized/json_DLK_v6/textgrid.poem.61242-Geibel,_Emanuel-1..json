{"textgrid.poem.61242": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es saust der Wind im dunkeln Wald,", "tokens": ["Es", "saust", "der", "Wind", "im", "dun\u00b7keln", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df hoch die Wipfel schwanken;", "tokens": ["Da\u00df", "hoch", "die", "Wip\u00b7fel", "schwan\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wohl \u00fcber den Wald, wohl \u00fcber die Flur", "tokens": ["Wohl", "\u00fc\u00b7ber", "den", "Wald", ",", "wohl", "\u00fc\u00b7ber", "die", "Flur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "ADV", "APPR", "ART", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Verweht er meine Gedanken.", "tokens": ["Ver\u00b7weht", "er", "mei\u00b7ne", "Ge\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Er tr\u00e4gt sie hin zum Grafenschlo\u00df,", "tokens": ["Er", "tr\u00e4gt", "sie", "hin", "zum", "Gra\u00b7fen\u00b7schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da klingen Fl\u00f6ten und Geigen,", "tokens": ["Da", "klin\u00b7gen", "Fl\u00f6\u00b7ten", "und", "Gei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Bei Kerzenschimmer perlt der Wein,", "tokens": ["Bei", "Ker\u00b7zen\u00b7schim\u00b7mer", "perlt", "der", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Saale braust der Reigen.", "tokens": ["Im", "Saa\u00b7le", "braust", "der", "Rei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Das ist das Fest der sch\u00f6nsten Maid,", "tokens": ["Das", "ist", "das", "Fest", "der", "sch\u00f6ns\u00b7ten", "Maid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Fest der wei\u00dfen Rose;", "tokens": ["Das", "Fest", "der", "wei\u00b7\u00dfen", "Ro\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Man bringt ihr manchen Becher dar,", "tokens": ["Man", "bringt", "ihr", "man\u00b7chen", "Be\u00b7cher", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Manch Spr\u00fcchlein bunt und lose.", "tokens": ["Manch", "Spr\u00fcch\u00b7lein", "bunt", "und", "lo\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie steht im Tanz und hat nicht acht,", "tokens": ["Sie", "steht", "im", "Tanz", "und", "hat", "nicht", "acht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "KON", "VAFIN", "PTKNEG", "CARD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df sie die Weise lerne;", "tokens": ["Da\u00df", "sie", "die", "Wei\u00b7se", "ler\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie l\u00e4chelt still in sich hinein,", "tokens": ["Sie", "l\u00e4\u00b7chelt", "still", "in", "sich", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als w\u00e4r' ihr Sinn in der Ferne.", "tokens": ["Als", "w\u00e4r'", "ihr", "Sinn", "in", "der", "Fer\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Ich wei\u00df es nicht, ist an ihr Ohr", "tokens": ["Ich", "wei\u00df", "es", "nicht", ",", "ist", "an", "ihr", "Ohr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Lieds ein Ton gedrungen,", "tokens": ["Des", "Lieds", "ein", "Ton", "ge\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das weit von ihr im dunkeln Wald", "tokens": ["Das", "weit", "von", "ihr", "im", "dun\u00b7keln", "Wald"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADJD", "APPR", "PPOSAT", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der J\u00e4gersmann gesungen?", "tokens": ["Der", "J\u00e4\u00b7gers\u00b7mann", "ge\u00b7sun\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}