{"dta.poem.744": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XxX.  An  Melanien.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr glaubet warlich nicht wie sch\u00f6n' es sey zu sehen ", "tokens": ["Ihr", "glau\u00b7bet", "war\u00b7lich", "nicht", "wie", "sch\u00f6n'", "es", "sey", "zu", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "KOKOM", "VVFIN", "PPER", "VAFIN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn jhr den krummen Hals noch dreymal kr\u00fcmmer ", "tokens": ["Wenn", "jhr", "den", "krum\u00b7men", "Hals", "noch", "drey\u00b7mal", "kr\u00fcm\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ADV", "ADV", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "macht.", "tokens": ["macht", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Vnd durch den weiten Mund so wunderlieblich lacht", "tokens": ["Vnd", "durch", "den", "wei\u00b7ten", "Mund", "so", "wun\u00b7der\u00b7lieb\u00b7lich", "lacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der sonst nichts kan/ denn nur frisch ligen vnd gut schm\u00e4hen. ", "tokens": ["Der", "sonst", "nichts", "kan", "/", "denn", "nur", "frisch", "li\u00b7gen", "vnd", "gut", "schm\u00e4\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIS", "VMFIN", "$(", "KON", "ADV", "ADJD", "VVINF", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Euch d\u00fcnckt der wisse nicht/ wie jhm doch sey geschehen:", "tokens": ["Euch", "d\u00fcnckt", "der", "wis\u00b7se", "nicht", "/", "wie", "jhm", "doch", "sey", "ge\u00b7sche\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "VVFIN", "PTKNEG", "$(", "PWAV", "PPER", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der zihe n\u00e4rrisch auff mit seiner newen Tracht ", "tokens": ["Der", "zi\u00b7he", "n\u00e4r\u00b7risch", "auff", "mit", "sei\u00b7ner", "ne\u00b7wen", "Tracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADJD", "APPR", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So hab euch jener nicht de\u00df grusses wehrt geacht ", "tokens": ["So", "hab", "euch", "je\u00b7ner", "nicht", "de\u00df", "grus\u00b7ses", "wehrt", "ge\u00b7acht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PDS", "PTKNEG", "ART", "ADJA", "VVFIN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dem must jhr seine sprach vnd jedes wort bej\u00e4hen/ ", "tokens": ["Dem", "must", "jhr", "sei\u00b7ne", "sprach", "vnd", "je\u00b7des", "wort", "be\u00b7j\u00e4\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PPOSAT", "VVFIN", "KON", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Dem mangelts an der Stirn/ vnd jener siht nicht recht ", "tokens": ["Dem", "man\u00b7gelts", "an", "der", "Stirn", "/", "vnd", "je\u00b7ner", "siht", "nicht", "recht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$(", "KON", "PDS", "VVFIN", "PTKNEG", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd der ist gar zu sch\u00f6n/ vnd dieser gar zu schlecht/", "tokens": ["Vnd", "der", "ist", "gar", "zu", "sch\u00f6n", "/", "vnd", "die\u00b7ser", "gar", "zu", "schlecht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADV", "PTKA", "ADJD", "$(", "KON", "PDS", "ADV", "PTKA", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der kan den Degen nicht recht an die Seite binden.", "tokens": ["Der", "kan", "den", "De\u00b7gen", "nicht", "recht", "an", "die", "Sei\u00b7te", "bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "PTKNEG", "ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Habt jhr den Spiegel auch der dort hing ander wandt ", "tokens": ["Habt", "jhr", "den", "Spie\u00b7gel", "auch", "der", "dort", "hing", "an\u00b7der", "wandt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "ART", "ADV", "VVFIN", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Melanie wol je genommen in die Handt? ", "tokens": ["Me\u00b7la\u00b7nie", "wol", "je", "ge\u00b7nom\u00b7men", "in", "die", "Handt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "VVPP", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Ey liebe! schawt hinein/ da ist was guts zu finden.", "tokens": ["Ey", "lie\u00b7be", "!", "schawt", "hin\u00b7ein", "/", "da", "ist", "was", "guts", "zu", "fin\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$.", "VVFIN", "PTKVZ", "$(", "ADV", "VAFIN", "PIS", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}