{"textgrid.poem.44365": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Am Morgen nach einem Sturm", "genre": "verse", "period": "N.A.", "pub_year": 1819, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hast einmal wieder gest\u00fcrmt?", "tokens": ["Hast", "ein\u00b7mal", "wie\u00b7der", "ge\u00b7st\u00fcrmt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Wildes, tobendes Element,", "tokens": ["Wil\u00b7des", ",", "to\u00b7ben\u00b7des", "E\u00b7le\u00b7ment", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Wider Erd und Himmel", "tokens": ["Wi\u00b7der", "Erd", "und", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Feindlich k\u00e4mpfend angerennt?", "tokens": ["Feind\u00b7lich", "k\u00e4mp\u00b7fend", "an\u00b7ge\u00b7rennt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "T\u00f6richt! Fruchtlos!", "tokens": ["T\u00f6\u00b7richt", "!", "Frucht\u00b7los", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Sieh! Die Erde steht unbewegt", "tokens": ["Sieh", "!", "Die", "Er\u00b7de", "steht", "un\u00b7be\u00b7wegt"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "ART", "NN", "VVFIN", "ADJD"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Und der Himmel w\u00f6lbt sich, heiter gl\u00e4nzend,", "tokens": ["Und", "der", "Him\u00b7mel", "w\u00f6lbt", "sich", ",", "hei\u00b7ter", "gl\u00e4n\u00b7zend", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PRF", "$,", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "L\u00e4chelnd \u00fcber sie und dich.", "tokens": ["L\u00e4\u00b7chelnd", "\u00fc\u00b7ber", "sie", "und", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "KON", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Du aber bist tr\u00fcb und d\u00fcster", "tokens": ["Du", "a\u00b7ber", "bist", "tr\u00fcb", "und", "d\u00fcs\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "ADJD", "KON", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Und warst doch sch\u00f6n wie sie.", "tokens": ["Und", "warst", "doch", "sch\u00f6n", "wie", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "KOKOM", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Feinde nicht die Erde an,", "tokens": ["Fein\u00b7de", "nicht", "die", "Er\u00b7de", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil sie fest und gr\u00fcnend,", "tokens": ["Weil", "sie", "fest", "und", "gr\u00fc\u00b7nend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKVZ", "KON", "ADJD", "$,"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Beneide nicht den Himmel,", "tokens": ["Be\u00b7nei\u00b7de", "nicht", "den", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Weil er blau und hell:", "tokens": ["Weil", "er", "blau", "und", "hell", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Bist du minder fest als jene,", "tokens": ["Bist", "du", "min\u00b7der", "fest", "als", "je\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "KOKOM", "PDS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bist du heller doch, als sie;", "tokens": ["Bist", "du", "hel\u00b7ler", "doch", ",", "als", "sie", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "ADV", "$,", "KOUS", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Bist du minder hell als dieser,", "tokens": ["Bist", "du", "min\u00b7der", "hell", "als", "die\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "KOKOM", "PDS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Bist du fester doch, als er,", "tokens": ["Bist", "du", "fes\u00b7ter", "doch", ",", "als", "er", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "ADV", "$,", "KOUS", "PPER", "$,"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.9": {"text": "Und beide \u2013 willst du ruhig quellen \u2013", "tokens": ["Und", "bei\u00b7de", "\u2013", "willst", "du", "ru\u00b7hig", "quel\u00b7len", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$(", "VMFIN", "PPER", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Spiegeln sich vereint in deinen Wellen.", "tokens": ["Spie\u00b7geln", "sich", "ver\u00b7eint", "in", "dei\u00b7nen", "Wel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Drum gib auf nur die Beschwerde,", "tokens": ["Drum", "gib", "auf", "nur", "die", "Be\u00b7schwer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "APPR", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Sei erst ruhig und dann schau,", "tokens": ["Sei", "erst", "ru\u00b7hig", "und", "dann", "schau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "KON", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Ob du gr\u00fcn nicht wie die Erde,", "tokens": ["Ob", "du", "gr\u00fcn", "nicht", "wie", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PTKNEG", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Wie der Himmel blau.", "tokens": ["Wie", "der", "Him\u00b7mel", "blau", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}