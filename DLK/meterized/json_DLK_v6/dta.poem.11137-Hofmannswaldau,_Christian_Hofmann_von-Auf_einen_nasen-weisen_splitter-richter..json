{"dta.poem.11137": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen nasen-weisen splitter-richter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.71", "nl:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Kein mensch hat recht geredt, kein mensch hat recht gethan;", "tokens": ["Kein", "mensch", "hat", "recht", "ge\u00b7redt", ",", "kein", "mensch", "hat", "recht", "ge\u00b7than", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADJD", "VVPP", "$,", "PIAT", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An allen findest du zu beissen und zu nagen.", "tokens": ["An", "al\u00b7len", "fin\u00b7dest", "du", "zu", "beis\u00b7sen", "und", "zu", "na\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "PPER", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch wo du rath bedarfst, so la\u00df dir dieses sagen:", "tokens": ["Doch", "wo", "du", "rath", "be\u00b7darfst", ",", "so", "la\u00df", "dir", "die\u00b7ses", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "NN", "VVFIN", "$,", "ADV", "VVIMP", "PPER", "PDS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Greiff deinen busen vor, hernach den besen an.", "tokens": ["Greiff", "dei\u00b7nen", "bu\u00b7sen", "vor", ",", "her\u00b7nach", "den", "be\u00b7sen", "an", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word",