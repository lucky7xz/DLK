{"textgrid.poem.56331": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Lord Byrons letzte Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Byron ist erschienen, der Kam\u00f6nen", "tokens": ["By\u00b7ron", "ist", "er\u00b7schie\u00b7nen", ",", "der", "Ka\u00b7m\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VAFIN", "VVPP", "$,", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und des Ares Z\u00f6gling strahlt, ein Held,", "tokens": ["Und", "des", "A\u00b7res", "Z\u00f6g\u00b7ling", "strahlt", ",", "ein", "Held", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Unter Hellas heldenm\u00fct'gen S\u00f6hnen", "tokens": ["Un\u00b7ter", "Hel\u00b7las", "hel\u00b7den\u00b7m\u00fct'\u00b7gen", "S\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Auf dem blutged\u00fcngten Freiheitsfeld.", "tokens": ["Auf", "dem", "blut\u00b7ge\u00b7d\u00fcng\u00b7ten", "Frei\u00b7heits\u00b7feld", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Und ihm schlagen aller Griechen Herzen \u2013", "tokens": ["Und", "ihm", "schla\u00b7gen", "al\u00b7ler", "Grie\u00b7chen", "Her\u00b7zen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "NN", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Eines nicht, nach welchem er doch ringt;", "tokens": ["Ei\u00b7nes", "nicht", ",", "nach", "wel\u00b7chem", "er", "doch", "ringt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PTKNEG", "$,", "APPR", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und er schafft sich unabl\u00e4ssig Schmerzen,", "tokens": ["Und", "er", "schafft", "sich", "un\u00b7ab\u00b7l\u00e4s\u00b7sig", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wo er selbst das Heil den V\u00f6lkern bringt.", "tokens": ["Wo", "er", "selbst", "das", "Heil", "den", "V\u00f6l\u00b7kern", "bringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "\u00bbwie mein Volk, so will ich dich verehren!\u00ab", "tokens": ["\u00bb", "wie", "mein", "Volk", ",", "so", "will", "ich", "dich", "ver\u00b7eh\u00b7ren", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KOKOM", "PPOSAT", "NN", "$,", "ADV", "VMFIN", "PPER", "PRF", "VVFIN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mild, doch unger\u00fchrt die Jungfrau spricht:", "tokens": ["Mild", ",", "doch", "un\u00b7ge\u00b7r\u00fchrt", "die", "Jung\u00b7frau", "spricht", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "\u00bbmagst die Krone von Byzanz begehren,", "tokens": ["\u00bb", "magst", "die", "Kro\u00b7ne", "von", "By\u00b7zanz", "be\u00b7geh\u00b7ren", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "ART", "NN", "APPR", "NE", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Meine Liebe nur begehre nicht!\u00ab", "tokens": ["Mei\u00b7ne", "Lie\u00b7be", "nur", "be\u00b7geh\u00b7re", "nicht", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "PTKNEG", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Eilig ward er einst zu ihr entboten,", "tokens": ["Ei\u00b7lig", "ward", "er", "einst", "zu", "ihr", "ent\u00b7bo\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die der Stern ist seiner innern Nacht;", "tokens": ["Die", "der", "Stern", "ist", "sei\u00b7ner", "in\u00b7nern", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "St\u00fcrmend folgt er, ahnungsvoll, dem Boten, \u2013", "tokens": ["St\u00fcr\u00b7mend", "folgt", "er", ",", "ah\u00b7nungs\u00b7voll", ",", "dem", "Bo\u00b7ten", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "ADJD", "$,", "ART", "NN", "$,", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Welch ein Schreckensbild vor ihm erwacht!", "tokens": ["Welch", "ein", "Schre\u00b7ckens\u00b7bild", "vor", "ihm", "er\u00b7wacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Starr lag, regungslos, die Schmerzenreiche,", "tokens": ["Starr", "lag", ",", "re\u00b7gungs\u00b7los", ",", "die", "Schmer\u00b7zen\u00b7rei\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "ADJD", "$,", "ART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Um ein Schwert die rechte Hand geballt;", "tokens": ["Um", "ein", "Schwert", "die", "rech\u00b7te", "Hand", "ge\u00b7ballt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Langsam richtet sich empor die bleiche,", "tokens": ["Lang\u00b7sam", "rich\u00b7tet", "sich", "em\u00b7por", "die", "blei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "PTKVZ", "ART", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Geisterartig herrliche Gestalt.", "tokens": ["Geis\u00b7ter\u00b7ar\u00b7tig", "herr\u00b7li\u00b7che", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Sie beginnt: \u00bbDu sollst es jetzt erfahren:", "tokens": ["Sie", "be\u00b7ginnt", ":", "\u00bb", "Du", "sollst", "es", "jetzt", "er\u00b7fah\u00b7ren", ":"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Fr\u00fche traf ich schon der Liebe Wahl,", "tokens": ["Fr\u00fc\u00b7he", "traf", "ich", "schon", "der", "Lie\u00b7be", "Wahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Gab sein Schwert auch meinem Palikaren,", "tokens": ["Gab", "sein", "Schwert", "auch", "mei\u00b7nem", "Pa\u00b7li\u00b7ka\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Als das Vaterland es mir befahl.", "tokens": ["Als", "das", "Va\u00b7ter\u00b7land", "es", "mir", "be\u00b7fahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Scheidend sprach ich ernst in ernster Stunde:", "tokens": ["Schei\u00b7dend", "sprach", "ich", "ernst", "in", "erns\u00b7ter", "Stun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "\u203asieg nur oder Tod, das wissen wir;", "tokens": ["\u203a", "sieg", "nur", "o\u00b7der", "Tod", ",", "das", "wis\u00b7sen", "wir", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "ADV", "KON", "NN", "$,", "PDS", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Auf denn! und ein Wort aus treuem Munde:", "tokens": ["Auf", "denn", "!", "und", "ein", "Wort", "aus", "treu\u00b7em", "Mun\u00b7de", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "$.", "KON", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Stirbst du unserm Volke, sterb ich dir.\u2039", "tokens": ["Stirbst", "du", "un\u00b7serm", "Vol\u00b7ke", ",", "sterb", "ich", "dir", ".", "\u2039"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "PPER", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Du nun siehst mich dem Gestorbnen sterben;", "tokens": ["Du", "nun", "siehst", "mich", "dem", "Ge\u00b7storb\u00b7nen", "ster\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PRF", "ART", "NN", "VVINF", "$."], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Fallend sandt er mir zur\u00fcck sein Schwert;", "tokens": ["Fal\u00b7lend", "sandt", "er", "mir", "zu\u00b7r\u00fcck", "sein", "Schwert", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Nimm es hin, du Dichterheld, zum Erben", "tokens": ["Nimm", "es", "hin", ",", "du", "Dich\u00b7ter\u00b7held", ",", "zum", "Er\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "PPER", "PTKVZ", "$,", "PPER", "NN", "$,", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Solchen Gutes bist nur du mir wert!\u00ab", "tokens": ["Sol\u00b7chen", "Gu\u00b7tes", "bist", "nur", "du", "mir", "wert", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "PPER", "PPER", "ADJD", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Mit Entsetzen forscht er \u2013 und gelassen", "tokens": ["Mit", "Ent\u00b7set\u00b7zen", "forscht", "er", "\u2013", "und", "ge\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "$(", "KON", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Spricht sie: \u00bbGift!\u00ab \u2013 und atmet, merklich kaum,", "tokens": ["Spricht", "sie", ":", "\u00bb", "Gift", "!", "\u00ab", "\u2013", "und", "at\u00b7met", ",", "merk\u00b7lich", "kaum", ","], "token_info": ["word", "word", "punct", "punct", "word", "punct", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "$(", "NN", "$.", "$(", "$(", "KON", "VVFIN", "$,", "ADJD", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und vollbracht ist's; \u2013 seine Arme fassen", "tokens": ["Und", "voll\u00b7bracht", "ist's", ";", "\u2013", "sei\u00b7ne", "Ar\u00b7me", "fas\u00b7sen"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["KON", "VVPP", "VAFIN", "$.", "$(", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Erst als Leiche seines Lebens Traum.", "tokens": ["Erst", "als", "Lei\u00b7che", "sei\u00b7nes", "Le\u00b7bens", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "NN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Byrons Z\u00fcge seit der Stunde waren", "tokens": ["By\u00b7rons", "Z\u00fc\u00b7ge", "seit", "der", "Stun\u00b7de", "wa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "APPR", "ART", "NN", "VAFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Tr\u00fcb und n\u00e4chtlich, wie sein d\u00fcstres Los;", "tokens": ["Tr\u00fcb", "und", "n\u00e4cht\u00b7lich", ",", "wie", "sein", "d\u00fcst\u00b7res", "Los", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$,", "PWAV", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und er nahm das Schwert des Palikaren", "tokens": ["Und", "er", "nahm", "das", "Schwert", "des", "Pa\u00b7li\u00b7ka\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Bald mit sich hinab in Grabes Scho\u00df.", "tokens": ["Bald", "mit", "sich", "hin\u00b7ab", "in", "Gra\u00b7bes", "Scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PRF", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}