{"textgrid.poem.30187": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1651, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann die Buler ihrer selbsten solten zu Narcissen werden,", "tokens": ["Wann", "die", "Bu\u00b7ler", "ih\u00b7rer", "selbs\u00b7ten", "sol\u00b7ten", "zu", "Nar\u00b7cis\u00b7sen", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "ADJA", "VMFIN", "APPR", "NN", "VAINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Hett es fast so viel Narcissen, als es Menschen hat auff Erden.", "tokens": ["Hett", "es", "fast", "so", "viel", "Nar\u00b7cis\u00b7sen", ",", "als", "es", "Men\u00b7schen", "hat", "auff", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "PIAT", "NN", "$,", "KOUS", "PPER", "NN", "VAFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}