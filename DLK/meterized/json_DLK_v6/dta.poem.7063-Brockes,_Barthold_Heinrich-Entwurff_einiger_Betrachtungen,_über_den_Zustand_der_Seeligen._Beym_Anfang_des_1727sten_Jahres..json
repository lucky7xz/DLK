{"dta.poem.7063": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie gro\u00df allein ist nicht das Gl\u00fcck, von Kummer, Leiden,", "tokens": ["Wie", "gro\u00df", "al\u00b7lein", "ist", "nicht", "das", "Gl\u00fcck", ",", "von", "Kum\u00b7mer", ",", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "VAFIN", "PTKNEG", "ART", "NN", "$,", "APPR", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von Noth, Verdrie\u00dflichkeit, von Schmertzen, Plag und Pein,", "tokens": ["Von", "Noth", ",", "Ver\u00b7drie\u00df\u00b7lich\u00b7keit", ",", "von", "Schmert\u00b7zen", ",", "Plag", "und", "Pein", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auf einmal frey, erl\u00f6s\u2019t, ja gar in ew\u2019gen Freuden,", "tokens": ["Auf", "ein\u00b7mal", "frey", ",", "er\u00b7l\u00f6s't", ",", "ja", "gar", "in", "ew'\u00b7gen", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJD", "$,", "VVFIN", "$,", "ADV", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ohn End\u2019 und ewiglich versetzt zu seyn!", "tokens": ["Ohn", "End'", "und", "e\u00b7wig\u00b7lich", "ver\u00b7setzt", "zu", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJD", "VVPP", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es ist nicht nur Furcht, Zweifel, Angst und Qu\u00e4len,", "tokens": ["Es", "ist", "nicht", "nur", "Furcht", ",", "Zwei\u00b7fel", ",", "Angst", "und", "Qu\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sammt allen Widrigen, was sie auf Erden nagte,", "tokens": ["Sammt", "al\u00b7len", "Wid\u00b7ri\u00b7gen", ",", "was", "sie", "auf", "Er\u00b7den", "nag\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$,", "PRELS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und allen Schr\u00f6cklichen, was sie auf Erden plagte,", "tokens": ["Und", "al\u00b7len", "Schr\u00f6ck\u00b7li\u00b7chen", ",", "was", "sie", "auf", "Er\u00b7den", "plag\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sodann als wie ein Dunst, verschwunden und dahin;", "tokens": ["So\u00b7dann", "als", "wie", "ein", "Dunst", ",", "ver\u00b7schwun\u00b7den", "und", "da\u00b7hin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "KOKOM", "ART", "NN", "$,", "VVPP", "KON", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Verspottung, Tyranney, Beschimpffung, Zanck und Neid,", "tokens": ["Ver\u00b7spot\u00b7tung", ",", "Ty\u00b7ran\u00b7ney", ",", "Be\u00b7schimpf\u00b7fung", ",", "Zanck", "und", "Neid", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Verfolgung, L\u00e4sterung, Verleumdung, Ha\u00df und Streit", "tokens": ["Ver\u00b7fol\u00b7gung", ",", "L\u00e4s\u00b7te\u00b7rung", ",", "Ver\u00b7leum\u00b7dung", ",", "Ha\u00df", "und", "Streit"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Zerfoltern ferner nicht dem immer heitern Sinn.", "tokens": ["Zer\u00b7fol\u00b7tern", "fer\u00b7ner", "nicht", "dem", "im\u00b7mer", "hei\u00b7tern", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKNEG", "ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Von dem nichtswerthem Theil der Menschen sind sie nun,", "tokens": ["Von", "dem", "nichts\u00b7wert\u00b7hem", "Theil", "der", "Men\u00b7schen", "sind", "sie", "nun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Umringt von seel\u2019ger Ruh, erf\u00fcllt mit ew\u2019gen Frieden,", "tokens": ["Um\u00b7ringt", "von", "seel'\u00b7ger", "Ruh", ",", "er\u00b7f\u00fcllt", "mit", "ew'\u00b7gen", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Durch eine grosse Klufft geschieden:", "tokens": ["Durch", "ei\u00b7ne", "gros\u00b7se", "Klufft", "ge\u00b7schie\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Mit niemand haben sie nunmehr zu thun,", "tokens": ["Mit", "nie\u00b7mand", "ha\u00b7ben", "sie", "nun\u00b7mehr", "zu", "thun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VAFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Als blo\u00df allein mit solchen Seelen,", "tokens": ["Als", "blo\u00df", "al\u00b7lein", "mit", "sol\u00b7chen", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Die auch, wie sie, befreyt von allen Qu\u00e4len,", "tokens": ["Die", "auch", ",", "wie", "sie", ",", "be\u00b7freyt", "von", "al\u00b7len", "Qu\u00e4\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "PWAV", "PPER", "$,", "VVPP", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Mit ihnen einen Zweck und eine Neigung hegen.", "tokens": ["Mit", "ih\u00b7nen", "ei\u00b7nen", "Zweck", "und", "ei\u00b7ne", "Nei\u00b7gung", "he\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "KON", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}