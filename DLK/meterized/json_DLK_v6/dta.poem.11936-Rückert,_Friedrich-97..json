{"dta.poem.11936": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "97.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du w\u00e4rest gerne reich, umh\u00e4uft von Ueberflu\u00df,", "tokens": ["Du", "w\u00e4\u00b7rest", "ger\u00b7ne", "reich", ",", "um\u00b7h\u00e4uft", "von", "Ue\u00b7berf\u00b7lu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und gern auch arm zugleich, zufrieden im Genu\u00df.", "tokens": ["Und", "gern", "auch", "arm", "zu\u00b7gleich", ",", "zu\u00b7frie\u00b7den", "im", "Ge\u00b7nu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJD", "ADV", "$,", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du w\u00e4rest gern ber\u00fchmt, von aller Welt genannt,", "tokens": ["Du", "w\u00e4\u00b7rest", "gern", "be\u00b7r\u00fchmt", ",", "von", "al\u00b7ler", "Welt", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und gern auch ungest\u00f6rt, von Niemand gar gekannt.", "tokens": ["Und", "gern", "auch", "un\u00b7ge\u00b7st\u00f6rt", ",", "von", "Nie\u00b7mand", "gar", "ge\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJD", "$,", "APPR", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Du h\u00e4ttest gern zugleich den Himmel und die Erde;", "tokens": ["Du", "h\u00e4t\u00b7test", "gern", "zu\u00b7gleich", "den", "Him\u00b7mel", "und", "die", "Er\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich f\u00fcrchte, da\u00df dir so von beiden keines werde.", "tokens": ["Ich", "f\u00fcrch\u00b7te", ",", "da\u00df", "dir", "so", "von", "bei\u00b7den", "kei\u00b7nes", "wer\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ADV", "APPR", "PIAT", "PIS", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}