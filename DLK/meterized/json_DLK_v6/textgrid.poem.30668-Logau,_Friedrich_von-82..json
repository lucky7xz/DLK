{"textgrid.poem.30668": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "82.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man soll dir die Nativit\u00e4t, Tenebrio, au\u00dfrechen.", "tokens": ["Man", "soll", "dir", "die", "Na\u00b7ti\u00b7vi\u00b7t\u00e4t", ",", "Te\u00b7ne\u00b7brio", ",", "au\u00df\u00b7re\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "ART", "NN", "$,", "NE", "$,", "VVIZU", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Zu rechnen, wer dein Vater sey, das wil den Kopff zerbrechen;", "tokens": ["Zu", "rech\u00b7nen", ",", "wer", "dein", "Va\u00b7ter", "sey", ",", "das", "wil", "den", "Kopff", "zer\u00b7bre\u00b7chen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PWS", "PPOSAT", "NN", "VAFIN", "$,", "PDS", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Wann, wo, au\u00df wem du wordest jung, das kan man noch wol sprechen.", "tokens": ["Wann", ",", "wo", ",", "au\u00df", "wem", "du", "wor\u00b7dest", "jung", ",", "das", "kan", "man", "noch", "wol", "spre\u00b7chen", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "PWAV", "$,", "APPR", "PWS", "PPER", "ADV", "ADJD", "$,", "PDS", "VMFIN", "PIS", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}}}}