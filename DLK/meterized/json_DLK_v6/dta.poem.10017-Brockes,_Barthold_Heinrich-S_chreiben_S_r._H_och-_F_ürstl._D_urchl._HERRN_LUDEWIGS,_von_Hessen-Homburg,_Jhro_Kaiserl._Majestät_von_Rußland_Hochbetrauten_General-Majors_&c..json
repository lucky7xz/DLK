{"dta.poem.10017": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "S chreiben  \n  S r.  H och- F \u00fcrstl.  D urchl.  \n  HERRN  \n  LUDEWIGS,  \n von Hessen-Homburg,  \n Jhro Kaiserl. Majest\u00e4t von Ru\u00dfland  \n Hochbetrauten  General-Majors  &c.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin, Gesch\u00e4tzter Brocks, durch Deinen", "tokens": ["Ich", "bin", ",", "Ge\u00b7sch\u00e4tz\u00b7ter", "Brocks", ",", "durch", "Dei\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "NN", "NN", "$,", "APPR", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Brief ger\u00fchrt,", "tokens": ["Brief", "ge\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Der so verbindlich sch\u00f6n, als fliessend wol geschrieben.", "tokens": ["Der", "so", "ver\u00b7bind\u00b7lich", "sch\u00f6n", ",", "als", "flies\u00b7send", "wol", "ge\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "ADJD", "$,", "KOUS", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Jedoch der Beyschlu\u00df hat mich selbsten mir entf\u00fchrt,", "tokens": ["Je\u00b7doch", "der", "Bey\u00b7schlu\u00df", "hat", "mich", "selbs\u00b7ten", "mir", "ent\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "PPER", "VVFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und, wie im Traum entz\u00fcckt, zur Dichterey getrieben.", "tokens": ["Und", ",", "wie", "im", "Traum", "ent\u00b7z\u00fcckt", ",", "zur", "Dich\u00b7te\u00b7rey", "ge\u00b7trie\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "APPRART", "NN", "VVPP", "$,", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich f\u00fchle seltne Lust, und einen fremden Zug;", "tokens": ["Ich", "f\u00fch\u00b7le", "selt\u00b7ne", "Lust", ",", "und", "ei\u00b7nen", "frem\u00b7den", "Zug", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Worte werden mir durch s\u00fcssen Zwang gebunden:", "tokens": ["Die", "Wor\u00b7te", "wer\u00b7den", "mir", "durch", "s\u00fcs\u00b7sen", "Zwang", "ge\u00b7bun\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und kurtz: ich habe schon, durch \u00fcbereilten Flug,", "tokens": ["Und", "kurtz", ":", "ich", "ha\u00b7be", "schon", ",", "durch", "\u00fc\u00b7be\u00b7reil\u00b7ten", "Flug", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$.", "PPER", "VAFIN", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die H\u00f6he von dem Pol der Wissenschaft gefunden;", "tokens": ["Die", "H\u00f6\u00b7he", "von", "dem", "Pol", "der", "Wis\u00b7sen\u00b7schaft", "ge\u00b7fun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ich dichte, doch mein Werck ist wie dort ", "tokens": ["Ich", "dich\u00b7te", ",", "doch", "mein", "Werck", "ist", "wie", "dort"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "PPOSAT", "NN", "VAFIN", "KOKOM", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.11": {"text": "Bild,", "tokens": ["Bild", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.12": {"text": "Das Regung, Thon und Krafft von ", "tokens": ["Das", "Re\u00b7gung", ",", "Thon", "und", "Krafft", "von"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "NN", "KON", "NN", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "len leihet:", "tokens": ["len", "lei\u00b7het", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "Und was, ", "tokens": ["Und", "was", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "PWS", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.15": {"text": "quillt,", "tokens": ["quillt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.16": {"text": "Ist Deiner Weisheit Born, zur Danck-Begier, ge-", "tokens": ["Ist", "Dei\u00b7ner", "Weis\u00b7heit", "Born", ",", "zur", "Dan\u00b7ck\u00b7Be\u00b7gier", ",", "ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "NE", "$,", "APPRART", "NN", "$,", "TRUNC"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "weihet.", "tokens": ["wei\u00b7het", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.18": {"text": "Aus dieser Quelle r\u00fchrt der s\u00fcsse Uberflu\u00df,", "tokens": ["Aus", "die\u00b7ser", "Quel\u00b7le", "r\u00fchrt", "der", "s\u00fcs\u00b7se", "U\u00b7ber\u00b7flu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Der meinen Geist bestr\u00f6mt mit reinen Andachts-", "tokens": ["Der", "mei\u00b7nen", "Geist", "be\u00b7str\u00f6mt", "mit", "rei\u00b7nen", "An\u00b7dachts"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Trieben,", "tokens": ["Trie\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.21": {"text": "Indem Dein netter Kiel, zum wahren Dienst-Genu\u00df,", "tokens": ["In\u00b7dem", "Dein", "net\u00b7ter", "Kiel", ",", "zum", "wah\u00b7ren", "Dienst\u00b7Ge\u00b7nu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Das Wasser-Reich vielmehr gebildet, als beschrieben.", "tokens": ["Das", "Was\u00b7ser\u00b7Reich", "viel\u00b7mehr", "ge\u00b7bil\u00b7det", ",", "als", "be\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$,", "KOUS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ja, ja, es wallt in mir der Adern geistigs Blut,", "tokens": ["Ja", ",", "ja", ",", "es", "wallt", "in", "mir", "der", "A\u00b7dern", "geis\u00b7tigs", "Blut", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PTKANT", "$,", "PPER", "VVFIN", "APPR", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Wie deine Wellen sich, mit sanften Schl\u00e4gen, brechen.", "tokens": ["Wie", "dei\u00b7ne", "Wel\u00b7len", "sich", ",", "mit", "sanf\u00b7ten", "Schl\u00e4\u00b7gen", ",", "bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PRF", "$,", "APPR", "ADJA", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Mich deucht, ich h\u00f6re noch die Tropfen in der Fluth,", "tokens": ["Mich", "deucht", ",", "ich", "h\u00f6\u00b7re", "noch", "die", "Trop\u00b7fen", "in", "der", "Fluth", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Von ihres Sch\u00f6pfers Macht sich, lispelnde, besprechen.", "tokens": ["Von", "ih\u00b7res", "Sch\u00f6p\u00b7fers", "Macht", "sich", ",", "lis\u00b7peln\u00b7de", ",", "be\u00b7spre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "PRF", "$,", "ADJA", "$,", "VVINF", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "Dein Lied vergleicht sich selbst dem allersch\u00f6nsten Flu\u00df,", "tokens": ["Dein", "Lied", "ver\u00b7gleicht", "sich", "selbst", "dem", "al\u00b7ler\u00b7sch\u00f6ns\u00b7ten", "Flu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Der tieff, doch aber klar und unverschl\u00e4mmet schiesset,", "tokens": ["Der", "tieff", ",", "doch", "a\u00b7ber", "klar", "und", "un\u00b7ver\u00b7schl\u00e4m\u00b7met", "schies\u00b7set", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "ADV", "ADV", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Und allgemach zuletzt, bey seiner Ufern Schlu\u00df,", "tokens": ["Und", "all\u00b7ge\u00b7mach", "zu\u00b7letzt", ",", "bey", "sei\u00b7ner", "U\u00b7fern", "Schlu\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Sich in sein Ursprungs-Meer mit Nutz und Lust ergiesset.", "tokens": ["Sich", "in", "sein", "Ur\u00b7sprungs\u00b7Meer", "mit", "Nutz", "und", "Lust", "er\u00b7gies\u00b7set", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Strahlt Deines Geistes Feur auf Feder und Papier;", "tokens": ["Strahlt", "Dei\u00b7nes", "Geis\u00b7tes", "Feur", "auf", "Fe\u00b7der", "und", "Pa\u00b7pier", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Scheint iede Sylbe mir im Gegenschein verg\u00fcldet:", "tokens": ["Scheint", "ie\u00b7de", "Syl\u00b7be", "mir", "im", "Ge\u00b7gen\u00b7schein", "ver\u00b7g\u00fcl\u00b7det", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "So wie das Sonnen Licht, stellt es Dein Pinsel f\u00fcr", "tokens": ["So", "wie", "das", "Son\u00b7nen", "Licht", ",", "stellt", "es", "Dein", "Pin\u00b7sel", "f\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NN", "NN", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Sich in dem Spiegel-Glas der glatten Fluthen bildet.", "tokens": ["Sich", "in", "dem", "Spie\u00b7gel\u00b7Glas", "der", "glat\u00b7ten", "Flut\u00b7hen", "bil\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Du mahlest, Du belebst die Sch\u00f6nheit der Natur,", "tokens": ["Du", "mah\u00b7lest", ",", "Du", "be\u00b7lebst", "die", "Sch\u00f6n\u00b7heit", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und was nur reitzendes in ihren Kreisen stecket.", "tokens": ["Und", "was", "nur", "reit\u00b7zen\u00b7des", "in", "ih\u00b7ren", "Krei\u00b7sen", "ste\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "ADJA", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Du hast, durch so beliebt-als sichtbarliche Spur,", "tokens": ["Du", "hast", ",", "durch", "so", "be\u00b7lieb\u00b7tals", "sicht\u00b7bar\u00b7li\u00b7che", "Spur", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "APPR", "ADV", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Die fast verborgne Bahn zum Himmel uns entdecket.", "tokens": ["Die", "fast", "ver\u00b7borg\u00b7ne", "Bahn", "zum", "Him\u00b7mel", "uns", "ent\u00b7de\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Was Augen, Zung, Geruch, Gef\u00fchl und Ohr gef\u00e4llt,", "tokens": ["Was", "Au\u00b7gen", ",", "Zung", ",", "Ge\u00b7ruch", ",", "Ge\u00b7f\u00fchl", "und", "Ohr", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Was selbst den innern Kern, die Krafft der Seelen,", "tokens": ["Was", "selbst", "den", "in\u00b7nern", "Kern", ",", "die", "Krafft", "der", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "ADJA", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "r\u00fchret,", "tokens": ["r\u00fch\u00b7ret", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.42": {"text": "Die\u00df zeigt das Schau-Ger\u00fcst der neu-entdeckten Welt,", "tokens": ["Die\u00df", "zeigt", "das", "Schau\u00b7Ge\u00b7r\u00fcst", "der", "neu\u00b7ent\u00b7deck\u00b7ten", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Von Deiner Wunder-Hand so lebhaft aufgef\u00fchret.", "tokens": ["Von", "Dei\u00b7ner", "Wun\u00b7der\u00b7Hand", "so", "leb\u00b7haft", "auf\u00b7ge\u00b7f\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Wann ", "tokens": ["Wann"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}, "line.45": {"text": "schaft,", "tokens": ["schaft", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "-", "measure": "single.down"}, "line.46": {"text": "(wie wol es Fabel-Spiel) die wilde Thiere z\u00e4hmet;", "tokens": ["(", "wie", "wol", "es", "Fa\u00b7bel\u00b7Spiel", ")", "die", "wil\u00b7de", "Thie\u00b7re", "z\u00e4h\u00b7met", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOKOM", "ADV", "PPER", "NN", "$(", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "So wird, erregest Du der Elementen Krafft,", "tokens": ["So", "wird", ",", "er\u00b7re\u00b7gest", "Du", "der", "E\u00b7le\u00b7men\u00b7ten", "Krafft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV"