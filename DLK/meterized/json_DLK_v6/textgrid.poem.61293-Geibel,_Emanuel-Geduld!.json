{"textgrid.poem.61293": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Geduld!", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So schwankst du wieder als ein Rohr dahin,", "tokens": ["So", "schwankst", "du", "wie\u00b7der", "als", "ein", "Rohr", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "KOKOM", "ART", "NN", "PAV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gegeben in des Windes Zorn und Huld?", "tokens": ["Ge\u00b7ge\u00b7ben", "in", "des", "Win\u00b7des", "Zorn", "und", "Huld", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hast du noch immer nicht, mein trotz'ger Sinn,", "tokens": ["Hast", "du", "noch", "im\u00b7mer", "nicht", ",", "mein", "trotz'\u00b7ger", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "PTKNEG", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Erlernt Geduld?", "tokens": ["Er\u00b7lernt", "Ge\u00b7duld", "?"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Magst du in goldnen Zukunftstr\u00e4umen stehn,", "tokens": ["Magst", "du", "in", "gold\u00b7nen", "Zu\u00b7kunfts\u00b7tr\u00e4u\u00b7men", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Magst hin du weinen sonder Licht und Rat:", "tokens": ["Magst", "hin", "du", "wei\u00b7nen", "son\u00b7der", "Licht", "und", "Rat", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPER", "VVINF", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Geduld! Geduld! \u2013 die ew'gen Sterne gehn", "tokens": ["Ge\u00b7duld", "!", "Ge\u00b7duld", "!", "\u2013", "die", "ew'\u00b7gen", "Ster\u00b7ne", "gehn"], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "NN", "$.", "$(", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch ihren Pfad.", "tokens": ["Doch", "ih\u00b7ren", "Pfad", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Und der die Bahnen ihnen auserw\u00e4hlt", "tokens": ["Und", "der", "die", "Bah\u00b7nen", "ih\u00b7nen", "au\u00b7ser\u00b7w\u00e4hlt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und sie bewegt mit seines Mundes Hauch,", "tokens": ["Und", "sie", "be\u00b7wegt", "mit", "sei\u00b7nes", "Mun\u00b7des", "Hauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er hat die Tr\u00e4nen deines Volks gez\u00e4hlt", "tokens": ["Er", "hat", "die", "Tr\u00e4\u00b7nen", "dei\u00b7nes", "Volks", "ge\u00b7z\u00e4hlt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und deine auch.", "tokens": ["Und", "dei\u00b7ne", "auch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Er h\u00e4lt der Zeiten Wag' und w\u00e4gt genau,", "tokens": ["Er", "h\u00e4lt", "der", "Zei\u00b7ten", "Wag'", "und", "w\u00e4gt", "ge\u00b7nau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und was sie sinnen, er nur gibt den Schlu\u00df;", "tokens": ["Und", "was", "sie", "sin\u00b7nen", ",", "er", "nur", "gibt", "den", "Schlu\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVINF", "$,", "PPER", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kein Stein wird fallen, der f\u00fcr seinen Bau", "tokens": ["Kein", "Stein", "wird", "fal\u00b7len", ",", "der", "f\u00fcr", "sei\u00b7nen", "Bau"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "VVINF", "$,", "PRELS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nicht fallen mu\u00df.", "tokens": ["Nicht", "fal\u00b7len", "mu\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Stehst du mit ihm in Frieden, magst du fest", "tokens": ["Stehst", "du", "mit", "ihm", "in", "Frie\u00b7den", ",", "magst", "du", "fest"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "APPR", "NN", "$,", "VMFIN", "PPER", "PTKVZ"], "meter": "+----+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Des Weltgangs Brausen h\u00f6ren fern und nah:", "tokens": ["Des", "Welt\u00b7gangs", "Brau\u00b7sen", "h\u00f6\u00b7ren", "fern", "und", "nah", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dir ist der Tag, was er auch werden l\u00e4\u00dft,", "tokens": ["Dir", "ist", "der", "Tag", ",", "was", "er", "auch", "wer\u00b7den", "l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PWS", "PPER", "ADV", "VAINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zum Segen da.", "tokens": ["Zum", "Se\u00b7gen", "da", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Drum hoff' auf ", "tokens": ["Drum", "hoff'", "auf"], "token_info": ["word", "word", "word"], "pos": ["PAV", "VVFIN", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Und was dir fehlschlug, hoffe stets aufs neu':", "tokens": ["Und", "was", "dir", "fehl\u00b7schlug", ",", "hof\u00b7fe", "stets", "aufs", "neu'", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "VVFIN", "ADV", "APPRART", "ADJA", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sein Nam' ist Kraft und Wunder, und er ist", "tokens": ["Sein", "Nam'", "ist", "Kraft", "und", "Wun\u00b7der", ",", "und", "er", "ist"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "KON", "NN", "$,", "KON", "PPER", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Allein getreu.", "tokens": ["Al\u00b7lein", "ge\u00b7treu", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}