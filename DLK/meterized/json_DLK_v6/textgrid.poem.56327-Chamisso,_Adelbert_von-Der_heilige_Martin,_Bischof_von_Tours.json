{"textgrid.poem.56327": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Der heilige Martin, Bischof von Tours", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbdiesen Martin\u00ab, rief der Satan, \u2013", "tokens": ["\u00bb", "die\u00b7sen", "Mar\u00b7tin", "\u00ab", ",", "rief", "der", "Sa\u00b7tan", ",", "\u2013"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PDAT", "NN", "$(", "$,", "VVFIN", "ART", "NN", "$,", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "\u00bbf\u00fcrchtet nichts, ihr H\u00f6llengeister,", "tokens": ["\u00bb", "f\u00fcrch\u00b7tet", "nichts", ",", "ihr", "H\u00f6l\u00b7len\u00b7geis\u00b7ter", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PIS", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fcrchtet nichts und h\u00f6rt den Rat an,", "tokens": ["F\u00fcrch\u00b7tet", "nichts", "und", "h\u00f6rt", "den", "Rat", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den geschmiedet euer Meister, \u2013", "tokens": ["Den", "ge\u00b7schmie\u00b7det", "eu\u00b7er", "Meis\u00b7ter", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "VVPP", "PPOSAT", "NN", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Diesen Martin, der, geplaget,", "tokens": ["Die\u00b7sen", "Mar\u00b7tin", ",", "der", ",", "ge\u00b7pla\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PRELS", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Angefochten, \u2013 unverzaget,", "tokens": ["An\u00b7ge\u00b7foch\u00b7ten", ",", "\u2013", "un\u00b7ver\u00b7za\u00b7get", ","], "token_info": ["word", "punct", "punct", "word", "punct"], "pos": ["NN", "$,", "$(", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Unverf\u00e4hrdet, uns zum Hohn,", "tokens": ["Un\u00b7ver\u00b7f\u00e4hr\u00b7det", ",", "uns", "zum", "Hohn", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wiederbringt die Kreaturen,", "tokens": ["Wie\u00b7der\u00b7bringt", "die", "Kre\u00b7a\u00b7tu\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Die zu unsern Zeichen schwuren,", "tokens": ["Die", "zu", "un\u00b7sern", "Zei\u00b7chen", "schwu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Dem verha\u00dften Menschensohn,", "tokens": ["Dem", "ver\u00b7ha\u00df\u00b7ten", "Men\u00b7schen\u00b7sohn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Diesen gilt es zu verderben;", "tokens": ["Die\u00b7sen", "gilt", "es", "zu", "ver\u00b7der\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKZU", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Also will um ihn ich werben,", "tokens": ["Al\u00b7so", "will", "um", "ihn", "ich", "wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "APPR", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Z\u00e4hlt ihn zu den Unsern schon.\u00ab", "tokens": ["Z\u00e4hlt", "ihn", "zu", "den", "Un\u00b7sern", "schon", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "ADV", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Redend hat der Geist der L\u00fcge", "tokens": ["Re\u00b7dend", "hat", "der", "Geist", "der", "L\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Form und K\u00f6rper angenommen,", "tokens": ["Form", "und", "K\u00f6r\u00b7per", "an\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es sind des Heilands Z\u00fcge,", "tokens": ["Und", "es", "sind", "des", "Hei\u00b7lands", "Z\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Welche seiner Arglist frommen, \u2013", "tokens": ["Wel\u00b7che", "sei\u00b7ner", "Arg\u00b7list", "from\u00b7men", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWAT", "PPOSAT", "NN", "ADJA", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "F\u00fcrchtet nichts, o Vielgetreue,", "tokens": ["F\u00fcrch\u00b7tet", "nichts", ",", "o", "Viel\u00b7ge\u00b7treu\u00b7e", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "FM", "FM", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "F\u00fcrchtet nichts, wenn euch aufs neue", "tokens": ["F\u00fcrch\u00b7tet", "nichts", ",", "wenn", "euch", "aufs", "neu\u00b7e"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "$,", "KOUS", "PPER", "APPRART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Tief verha\u00dft der Anblick kr\u00e4nkt;", "tokens": ["Tief", "ver\u00b7ha\u00dft", "der", "An\u00b7blick", "kr\u00e4nkt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "F\u00fcrchtet nichts, ich bin der Alte,", "tokens": ["F\u00fcrch\u00b7tet", "nichts", ",", "ich", "bin", "der", "Al\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Der, wie er sein Antlitz falte,", "tokens": ["Der", ",", "wie", "er", "sein", "Ant\u00b7litz", "fal\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Alten Grolles nur gedenkt;", "tokens": ["Al\u00b7ten", "Grol\u00b7les", "nur", "ge\u00b7denkt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Ihm, den sie den Heil'gen schelten,", "tokens": ["Ihm", ",", "den", "sie", "den", "Heil'\u00b7gen", "schel\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Will ich f\u00fcr den Juden gelten,", "tokens": ["Will", "ich", "f\u00fcr", "den", "Ju\u00b7den", "gel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Bis er seine Seel uns schenkt.\u00ab", "tokens": ["Bis", "er", "sei\u00b7ne", "Seel", "uns", "schenkt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "PPER", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und in Purpur prunkt er eitel,", "tokens": ["Und", "in", "Pur\u00b7pur", "prunkt", "er", "ei\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gleich den K\u00f6nigen der Erde,", "tokens": ["Gleich", "den", "K\u00f6\u00b7ni\u00b7gen", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die Tiar' auf seiner Scheitel,", "tokens": ["Die", "Ti\u00b7a\u00b7r'", "auf", "sei\u00b7ner", "Schei\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Stolz und Hochmut die Geberde.", "tokens": ["Stolz", "und", "Hoch\u00b7mut", "die", "Ge\u00b7ber\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ART", "NN", "$."], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.5": {"text": "Und die Teufel fa\u00dft ein Grauen,", "tokens": ["Und", "die", "Teu\u00b7fel", "fa\u00dft", "ein", "Grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Wie das Schreckenbild sie schauen,", "tokens": ["Wie", "das", "Schre\u00b7cken\u00b7bild", "sie", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und ein Weheruf erschallt;", "tokens": ["Und", "ein", "We\u00b7he\u00b7ruf", "er\u00b7schallt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Heulend st\u00fcrzen sie vonsammen,", "tokens": ["Heu\u00b7lend", "st\u00fcr\u00b7zen", "sie", "von\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Suchen Schutz in ew'gen Flammen", "tokens": ["Su\u00b7chen", "Schutz", "in", "ew'\u00b7gen", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Vor des R\u00e4chers Allgewalt;", "tokens": ["Vor", "des", "R\u00e4\u00b7chers", "All\u00b7ge\u00b7walt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Und mit Angst erf\u00fcllt nicht minder", "tokens": ["Und", "mit", "Angst", "er\u00b7f\u00fcllt", "nicht", "min\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VVFIN", "PTKNEG", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Auch den argen Trugs-Erfinder", "tokens": ["Auch", "den", "ar\u00b7gen", "Trugs\u00b7Er\u00b7fin\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Die erfrevelte Gestalt.", "tokens": ["Die", "er\u00b7fre\u00b7vel\u00b7te", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-++--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Bischof Martin liegt indessen,", "tokens": ["Bi\u00b7schof", "Mar\u00b7tin", "liegt", "in\u00b7des\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lieb im Herzen, Hoffnung, Glaube,", "tokens": ["Lieb", "im", "Her\u00b7zen", ",", "Hoff\u00b7nung", ",", "Glau\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Tief in Demut, selbstvergessen,", "tokens": ["Tief", "in", "De\u00b7mut", ",", "selbst\u00b7ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vor dem Kruzifix im Staube:", "tokens": ["Vor", "dem", "Kru\u00b7zi\u00b7fix", "im", "Stau\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00bbder du starbst uns zu erl\u00f6sen,", "tokens": ["\u00bb", "der", "du", "starbst", "uns", "zu", "er\u00b7l\u00f6\u00b7sen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PRELS", "PPER", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sieh uns Schwache, von dem B\u00f6sen,", "tokens": ["Sieh", "uns", "Schwa\u00b7che", ",", "von", "dem", "B\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADJA", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Von der S\u00fcnde Garn umstellt;", "tokens": ["Von", "der", "S\u00fcn\u00b7de", "Garn", "um\u00b7stellt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Straf uns nicht in deinem Zorne,", "tokens": ["Straf", "uns", "nicht", "in", "dei\u00b7nem", "Zor\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wasch uns rein im Gnadenborne", "tokens": ["Wasch", "uns", "rein", "im", "Gna\u00b7den\u00b7bor\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADJD", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Von der Schuld, die auf uns f\u00e4llt.\u00ab", "tokens": ["Von", "der", "Schuld", ",", "die", "auf", "uns", "f\u00e4llt", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Und es tritt der Geist der L\u00fcge", "tokens": ["Und", "es", "tritt", "der", "Geist", "der", "L\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Vor ihn hin, er tr\u00e4gt die Z\u00fcge", "tokens": ["Vor", "ihn", "hin", ",", "er", "tr\u00e4gt", "die", "Z\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "PTKVZ", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Des Erl\u00f6sers dieser Welt.", "tokens": ["Des", "Er\u00b7l\u00f6\u00b7sers", "die\u00b7ser", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und in Purpur prunkt er eitel,", "tokens": ["Und", "in", "Pur\u00b7pur", "prunkt", "er", "ei\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gleich den K\u00f6nigen der Erde,", "tokens": ["Gleich", "den", "K\u00f6\u00b7ni\u00b7gen", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die Tiar' auf seiner Scheitel,", "tokens": ["Die", "Ti\u00b7a\u00b7r'", "auf", "sei\u00b7ner", "Schei\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Stolz und Hochmut die Geberde:", "tokens": ["Stolz", "und", "Hoch\u00b7mut", "die", "Ge\u00b7ber\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ART", "NN", "$."], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.5": {"text": "\u00bbmartin, sieh, ich bin der wahre", "tokens": ["\u00bb", "mar\u00b7tin", ",", "sieh", ",", "ich", "bin", "der", "wah\u00b7re"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "NE", "$,", "VVFIN", "$,", "PPER", "VAFIN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Christus, und ich offenbare", "tokens": ["Chris\u00b7tus", ",", "und", "ich", "of\u00b7fen\u00b7ba\u00b7re"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "KON", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Dem mich, der zu mir sich neigt;", "tokens": ["Dem", "mich", ",", "der", "zu", "mir", "sich", "neigt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "PRELS", "APPR", "PPER", "PRF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und es ist dir anbefohlen,", "tokens": ["Und", "es", "ist", "dir", "an\u00b7be\u00b7foh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Anzubeten unverhohlen,", "tokens": ["An\u00b7zu\u00b7be\u00b7ten", "un\u00b7ver\u00b7hoh\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Der sich deinen Augen zeigt.\u00ab", "tokens": ["Der", "sich", "dei\u00b7nen", "Au\u00b7gen", "zeigt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PRF", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Martin starrt, die Augen offen,", "tokens": ["Mar\u00b7tin", 