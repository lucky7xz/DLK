{"dta.poem.10224": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Die Kapelle zum finstern Stern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Herzog Abel schwor beim Himmel weit", "tokens": ["Her\u00b7zog", "A\u00b7bel", "schwor", "beim", "Him\u00b7mel", "weit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "VVFIN", "APPRART", "NN", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Und lie\u00df dem M\u00f6rder wenig Zeit,", "tokens": ["Und", "lie\u00df", "dem", "M\u00f6r\u00b7der", "we\u00b7nig", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Herzog Abel schob nichts auf die lange Bank,", "tokens": ["Her\u00b7zog", "A\u00b7bel", "schob", "nichts", "auf", "die", "lan\u00b7ge", "Bank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "PIS", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "In die K\u00f6nigsburg ritt er frech und frank,", "tokens": ["In", "die", "K\u00f6\u00b7nigs\u00b7burg", "ritt", "er", "frech", "und", "frank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADJD", "KON", "VVFIN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}}}}}