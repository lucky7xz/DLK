{"textgrid.poem.63770": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Politisches", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Welch toller L\u00e4rm? Was hat sich nur begeben?", "tokens": ["Welch", "tol\u00b7ler", "L\u00e4rm", "?", "Was", "hat", "sich", "nur", "be\u00b7ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$.", "PWS", "VAFIN", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Steht wieder vor den Toren Hannibal?", "tokens": ["Steht", "wie\u00b7der", "vor", "den", "To\u00b7ren", "Han\u00b7ni\u00b7bal", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nein, nur ein Sammetsessel kam zu Fall:", "tokens": ["Nein", ",", "nur", "ein", "Sam\u00b7met\u00b7ses\u00b7sel", "kam", "zu", "Fall", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Im Parlament gab's ein Ministerbeben.", "tokens": ["Im", "Par\u00b7la\u00b7ment", "gab's", "ein", "Mi\u00b7nis\u00b7ter\u00b7be\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das dritte schon, das wir in Rom erleben:", "tokens": ["Das", "drit\u00b7te", "schon", ",", "das", "wir", "in", "Rom", "er\u00b7le\u00b7ben", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "$,", "PRELS", "PPER", "APPR", "NE", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zuerst Nicotera mit sanfterm Schall,", "tokens": ["Zu\u00b7erst", "Ni\u00b7co\u00b7te\u00b7ra", "mit", "sanf\u00b7term", "Schall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Herr Crispi dann mit skandal\u00f6sem Knall,", "tokens": ["Herr", "Cris\u00b7pi", "dann", "mit", "skan\u00b7da\u00b7l\u00f6\u00b7sem", "Knall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und Patriarch Depretis gleich daneben.", "tokens": ["Und", "Pat\u00b7ri\u00b7arch", "De\u00b7pre\u00b7tis", "gleich", "da\u00b7ne\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NE", "ADV", "PAV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und alle von der Linken. La\u00dft das Flunkern,", "tokens": ["Und", "al\u00b7le", "von", "der", "Lin\u00b7ken", ".", "La\u00dft", "das", "Flun\u00b7kern", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "ART", "NN", "$.", "VVIMP", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als ob das Vaterland gef\u00e4hrdet w\u00e4re!", "tokens": ["Als", "ob", "das", "Va\u00b7ter\u00b7land", "ge\u00b7f\u00e4hr\u00b7det", "w\u00e4\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hier hei\u00dft's ja nur: Steh auf, la\u00df mich hier sitzen!", "tokens": ["Hier", "hei\u00dft's", "ja", "nur", ":", "Steh", "auf", ",", "la\u00df", "mich", "hier", "sit\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "$.", "VVFIN", "PTKVZ", "$,", "VVIMP", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nur gro\u00dfer Kampf reift gro\u00dfe Charaktere.", "tokens": ["Nur", "gro\u00b7\u00dfer", "Kampf", "reift", "gro\u00b7\u00dfe", "Cha\u00b7rak\u00b7te\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Euch fehlt's an Pfaffen, Sozialisten, Junkern", "tokens": ["Euch", "fehlt's", "an", "Pfaf\u00b7fen", ",", "So\u00b7zi\u00b7a\u00b7lis\u00b7ten", ",", "Jun\u00b7kern"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und andrer schwerer Not, die ", "tokens": ["Und", "an\u00b7drer", "schwe\u00b7rer", "Not", ",", "die"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADJA", "ADJA", "NN", "$,", "PRELS"], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}