{"textgrid.poem.62786": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vor Zeiten, als am Hofe gar", "genre": "verse", "period": "N.A.", "pub_year": 1771, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vor Zeiten, als am Hofe gar", "tokens": ["Vor", "Zei\u00b7ten", ",", "als", "am", "Ho\u00b7fe", "gar"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "KOUS", "APPRART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein eignes Amt f\u00fcr Narren war,", "tokens": ["Ein", "eig\u00b7nes", "Amt", "f\u00fcr", "Nar\u00b7ren", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Statt da\u00df sie doch in unsern Tagen", "tokens": ["Statt", "da\u00df", "sie", "doch", "in", "un\u00b7sern", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KOUS", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dabey noch andre W\u00fcrden tragen,", "tokens": ["Da\u00b7bey", "noch", "and\u00b7re", "W\u00fcr\u00b7den", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Kam eines F\u00fcrsten lustger Rath", "tokens": ["Kam", "ei\u00b7nes", "F\u00fcrs\u00b7ten", "lust\u00b7ger", "Rath"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dem edlen Kanzler aus Versehen", "tokens": ["Dem", "ed\u00b7len", "Kanz\u00b7ler", "aus", "Ver\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Auf seine rechte Hand zu stehen.", "tokens": ["Auf", "sei\u00b7ne", "rech\u00b7te", "Hand", "zu", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Hilf Zevs, wie sch\u00e4umte der Magnat!", "tokens": ["Hilf", "Zevs", ",", "wie", "sch\u00e4um\u00b7te", "der", "Mag\u00b7nat", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PWAV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "So sch\u00e4umt ein Aurochs im Gefechte.", "tokens": ["So", "sch\u00e4umt", "ein", "Au\u00b7rochs", "im", "Ge\u00b7fech\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Fort, rief er, Schlingel, packe dich!", "tokens": ["Fort", ",", "rief", "er", ",", "Schlin\u00b7gel", ",", "pa\u00b7cke", "dich", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "$,", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ich lasse keinem Narrn die Rechte.", "tokens": ["Ich", "las\u00b7se", "kei\u00b7nem", "Narrn", "die", "Rech\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "O! sagte Niklas, aber ich ...", "tokens": ["O", "!", "sag\u00b7te", "Nik\u00b7las", ",", "a\u00b7ber", "ich", "..."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "NE", "$,", "KON", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und sprang mit einem losen Winke", "tokens": ["Und", "sprang", "mit", "ei\u00b7nem", "lo\u00b7sen", "Win\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Dem Staatsminister auf die Linke.", "tokens": ["Dem", "Staats\u00b7mi\u00b7nis\u00b7ter", "auf", "die", "Lin\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}