{"textgrid.poem.45355": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Ludwig Napoleon (1852)", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unbesonnenheit statt Mut,", "tokens": ["Un\u00b7be\u00b7son\u00b7nen\u00b7heit", "statt", "Mut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und Unversch\u00e4mtheit statt Verstand,", "tokens": ["Und", "Un\u00b7ver\u00b7sch\u00e4m\u00b7theit", "statt", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Setzt sich der Adler auf deinen Hut,", "tokens": ["Setzt", "sich", "der", "Ad\u00b7ler", "auf", "dei\u00b7nen", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Doch folgt er darum deiner Hand?", "tokens": ["Doch", "folgt", "er", "da\u00b7rum", "dei\u00b7ner", "Hand", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PAV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}