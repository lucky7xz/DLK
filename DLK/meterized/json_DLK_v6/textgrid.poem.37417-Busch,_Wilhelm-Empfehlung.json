{"textgrid.poem.37417": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Empfehlung", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist nerv\u00f6s. Drum lies doch mal", "tokens": ["Du", "bist", "ner\u00b7v\u00f6s", ".", "Drum", "lies", "doch", "mal"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NE", "$.", "PAV", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Buch, das man dir anempfahl.", "tokens": ["Das", "Buch", ",", "das", "man", "dir", "an\u00b7emp\u00b7fahl", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es ist beinah, wie eine Reise", "tokens": ["Es", "ist", "bei\u00b7nah", ",", "wie", "ei\u00b7ne", "Rei\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im alten, wohlbekannten Gleise.", "tokens": ["Im", "al\u00b7ten", ",", "wohl\u00b7be\u00b7kann\u00b7ten", "Glei\u00b7se", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Weg ist grad und flach das Land,", "tokens": ["Der", "Weg", "ist", "grad", "und", "flach", "das", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Rechts, links und unten nichts wie Sand.", "tokens": ["Rechts", ",", "links", "und", "un\u00b7ten", "nichts", "wie", "Sand", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "KON", "ADV", "PIS", "KOKOM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Kein R\u00e4derl\u00e4rm verbittert dich,", "tokens": ["Kein", "R\u00e4\u00b7der\u00b7l\u00e4rm", "ver\u00b7bit\u00b7tert", "dich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Kein harter Sto\u00df ersch\u00fcttert dich,", "tokens": ["Kein", "har\u00b7ter", "Sto\u00df", "er\u00b7sch\u00fct\u00b7tert", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und bald umf\u00e4ngt dich sanft und k\u00fchl", "tokens": ["Und", "bald", "um\u00b7f\u00e4ngt", "dich", "sanft", "und", "k\u00fchl"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ein Kaumvorhandenseinsgef\u00fchl.", "tokens": ["Ein", "Kaum\u00b7vor\u00b7han\u00b7den\u00b7seins\u00b7ge\u00b7f\u00fchl", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Du bist behaglich eingenickt.", "tokens": ["Du", "bist", "be\u00b7hag\u00b7lich", "ein\u00b7ge\u00b7nickt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Dann, wenn du angenehm erquickt,", "tokens": ["Dann", ",", "wenn", "du", "an\u00b7ge\u00b7nehm", "er\u00b7quickt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Kehrst du beim \u00bbstillen Wirte\u00ab ein.", "tokens": ["Kehrst", "du", "beim", "\u00bb", "stil\u00b7len", "Wir\u00b7te", "\u00ab", "ein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "$(", "ADJA", "NN", "$(", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Da gibt es weder Bier noch Wein.", "tokens": ["Da", "gibt", "es", "we\u00b7der", "Bier", "noch", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Du schl\u00fcrfst ein wenig \u00c4pfelmost,", "tokens": ["Du", "schl\u00fcrfst", "ein", "we\u00b7nig", "\u00c4p\u00b7fel\u00b7most", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "I\u00dft eine leichte L\u00f6ffelkost", "tokens": ["I\u00dft", "ei\u00b7ne", "leich\u00b7te", "L\u00f6f\u00b7fel\u00b7kost"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Mit wenig Fett und vieler Gr\u00fctze,", "tokens": ["Mit", "we\u00b7nig", "Fett", "und", "vie\u00b7ler", "Gr\u00fct\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Gehst fr\u00fch zu Bett in spitzer M\u00fctze", "tokens": ["Gehst", "fr\u00fch", "zu", "Bett", "in", "spit\u00b7zer", "M\u00fct\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Und trinkst zuletzt ein Gl\u00e4schen Wasser.", "tokens": ["Und", "trinkst", "zu\u00b7letzt", "ein", "Gl\u00e4s\u00b7chen", "Was\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Schlaf wohl, und segne den Verfasser!", "tokens": ["Schlaf", "wohl", ",", "und", "seg\u00b7ne", "den", "Ver\u00b7fas\u00b7ser", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}