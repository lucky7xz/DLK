{"textgrid.poem.39239": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Nacht liegt w\u00fcst auf der Meeresh\u00f6h',", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Nacht liegt w\u00fcst auf der Meeresh\u00f6h',", "tokens": ["Die", "Nacht", "liegt", "w\u00fcst", "auf", "der", "Mee\u00b7res\u00b7h\u00f6h'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Sturm pfeift grimm und grell,", "tokens": ["Der", "Sturm", "pfeift", "grimm", "und", "grell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du Nordsturm auf der Nordlandsee,", "tokens": ["Du", "Nord\u00b7sturm", "auf", "der", "Nord\u00b7land\u00b7see", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sei mit gegr\u00fc\u00dft, Gesell'!", "tokens": ["Sei", "mit", "ge\u00b7gr\u00fc\u00dft", ",", "Ge\u00b7sell'", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "APPR", "VVPP", "$,", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Eine Geisternacht, eine Schauerstund',", "tokens": ["Ei\u00b7ne", "Geis\u00b7ter\u00b7nacht", ",", "ei\u00b7ne", "Schau\u00b7er\u00b7stund'", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Eine Nacht f\u00fcr Nix' und Elf'.", "tokens": ["Ei\u00b7ne", "Nacht", "f\u00fcr", "Nix'", "und", "El\u00b7f'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "KON", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Das Fahrzeug st\u00f6hnt wie todeswund,", "tokens": ["Das", "Fahr\u00b7zeug", "st\u00f6hnt", "wie", "to\u00b7des\u00b7wund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Steuermann \u00e4chzt: \u00bbGott helf'!\u00ab", "tokens": ["Der", "Steu\u00b7er\u00b7mann", "\u00e4chzt", ":", "\u00bb", "Gott", "hel\u00b7f'", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich lehne mich \u00fcber das Taffarell,", "tokens": ["Ich", "leh\u00b7ne", "mich", "\u00fc\u00b7ber", "das", "Taf\u00b7fa\u00b7rell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die Flut umspritzt mein Haupt:", "tokens": ["Die", "Flut", "um\u00b7spritzt", "mein", "Haupt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbnun sage mir, mein Schiffsgesell',", "tokens": ["\u00bb", "nun", "sa\u00b7ge", "mir", ",", "mein", "Schiffs\u00b7ge\u00b7sell'", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was kommt so wild geschnaubt?\u00ab", "tokens": ["Was", "kommt", "so", "wild", "ge\u00b7schnaubt", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ADJD", "VVPP", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u00bbein Riesenschiff, wie Bergeslast,", "tokens": ["\u00bb", "ein", "Rie\u00b7sen\u00b7schiff", ",", "wie", "Ber\u00b7ges\u00b7last", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "PWAV", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Flut durchbraust es dumpf,", "tokens": ["Die", "Flut", "durch\u00b7braust", "es", "dumpf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Segel schwarz und schwarz der Mast", "tokens": ["Die", "Se\u00b7gel", "schwarz", "und", "schwarz", "der", "Mast"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und schwarz ist Spier' und Rumpf.\u00ab", "tokens": ["Und", "schwarz", "ist", "Spier'", "und", "Rumpf", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "VAFIN", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u00bbwie rennt es vor dem Sturme schmuck,", "tokens": ["\u00bb", "wie", "rennt", "es", "vor", "dem", "Stur\u00b7me", "schmuck", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VVFIN", "PPER", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So schwarz und gro\u00df und schwer,", "tokens": ["So", "schwarz", "und", "gro\u00df", "und", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit ungeheurem Segeldruck,", "tokens": ["Mit", "un\u00b7ge\u00b7heu\u00b7rem", "Se\u00b7gel\u00b7druck", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So schie\u00dft es \u00fcbers Meer!\u00ab", "tokens": ["So", "schie\u00dft", "es", "\u00fc\u00b7bers", "Meer", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Dem Steuermann bebt die Hand am Griff,", "tokens": ["Dem", "Steu\u00b7er\u00b7mann", "bebt", "die", "Hand", "am", "Griff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es schlottert sein Gebein:", "tokens": ["Es", "schlot\u00b7tert", "sein", "Ge\u00b7bein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbdas ist ein Wikings-Geisterschiff,", "tokens": ["\u00bb", "das", "ist", "ein", "Wi\u00b7kings\u00b7Geis\u00b7ter\u00b7schiff", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gott mag uns gn\u00e4dig sein!\u00ab", "tokens": ["Gott", "mag", "uns", "gn\u00e4\u00b7dig", "sein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADJD", "VAINF", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "\u2013 \u00bbWenn der Nordwind kommt aus kaltem Pol,", "tokens": ["\u2013", "\u00bb", "Wenn", "der", "Nord\u00b7wind", "kommt", "aus", "kal\u00b7tem", "Pol", ","], "token_info": ["punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "KOUS", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Ihn treibt's nach S\u00fcdens Luft,", "tokens": ["Ihn", "treibt's", "nach", "S\u00fc\u00b7dens", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da dringt sein scharfer Atem wohl", "tokens": ["Da", "dringt", "sein", "schar\u00b7fer", "A\u00b7tem", "wohl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In manche H\u00fcnengruft.", "tokens": ["In", "man\u00b7che", "H\u00fc\u00b7nen\u00b7gruft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wenn der Nordwind sch\u00fcttelt den Distelstrauch", "tokens": ["Wenn", "der", "Nord\u00b7wind", "sch\u00fct\u00b7telt", "den", "Dis\u00b7tel\u00b7strauch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Und k\u00fchlt der Toten Brust,", "tokens": ["Und", "k\u00fchlt", "der", "To\u00b7ten", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da weckt sein wohlbekannter Hauch", "tokens": ["Da", "weckt", "sein", "wohl\u00b7be\u00b7kann\u00b7ter", "Hauch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die alte, gewaltige Lust.\u00ab", "tokens": ["Die", "al\u00b7te", ",", "ge\u00b7wal\u00b7ti\u00b7ge", "Lust", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.9": {"line.1": {"text": "\u00bbdie wilde Lust nach der wilden See,", "tokens": ["\u00bb", "die", "wil\u00b7de", "Lust", "nach", "der", "wil\u00b7den", "See", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nach Wikingsfahrt und Streit,", "tokens": ["Nach", "Wi\u00b7kings\u00b7fahrt", "und", "Streit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Nach Wikingslust und Wikingsweh", "tokens": ["Nach", "Wi\u00b7kings\u00b7lust", "und", "Wi\u00b7kings\u00b7weh"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und S\u00fcdlands Herrlichkeit.\u00ab", "tokens": ["Und", "S\u00fcd\u00b7lands", "Herr\u00b7lich\u00b7keit", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["KON", "NN", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "\u00bbum Mitternacht am Meeresstrand,", "tokens": ["\u00bb", "um", "Mit\u00b7ter\u00b7nacht", "am", "Mee\u00b7res\u00b7strand", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da schreiten viel Helden stark,", "tokens": ["Da", "schrei\u00b7ten", "viel", "Hel\u00b7den", "stark", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Aus Schweden und aus Gotenland,", "tokens": ["Aus", "Schwe\u00b7den", "und", "aus", "Go\u00b7ten\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus Norweg und D\u00e4nemark.\u00ab", "tokens": ["Aus", "Nor\u00b7weg", "und", "D\u00e4\u00b7ne\u00b7mark", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NE", "KON", "NE", "$.", "$("], "meter": "-++-+-+", "measure": "unknown.measure.tetra"}}, "stanza.11": {"line.1": {"text": "\u00bbund wo sie's versenkt mit eig'ner Hand,", "tokens": ["\u00bb", "und", "wo", "sie's", "ver\u00b7senkt", "mit", "eig'\u00b7ner", "Hand", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PWAV", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Tief zwischen Bucht und Riff,", "tokens": ["Tief", "zwi\u00b7schen", "Bucht", "und", "Riff", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Da ziehn sie aus dem Meeressand", "tokens": ["Da", "ziehn", "sie", "aus", "dem", "Mee\u00b7res\u00b7sand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr schwarz besegelt' Schiff.\u00ab", "tokens": ["Ihr", "schwarz", "be\u00b7se\u00b7gelt'", "Schiff", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "\u00bbwenn der Schiffer betend kappt den Mast,", "tokens": ["\u00bb", "wenn", "der", "Schif\u00b7fer", "be\u00b7tend", "kappt", "den", "Mast", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "ART", "NN", "VVPP", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Den der Nordsturm krachend bog,", "tokens": ["Den", "der", "Nord\u00b7sturm", "kra\u00b7chend", "bog", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dann fahren mit voller Segellast", "tokens": ["Dann", "fah\u00b7ren", "mit", "vol\u00b7ler", "Se\u00b7gel\u00b7last"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Geister durchs Gewog.\u00ab \u2013", "tokens": ["Die", "Geis\u00b7ter", "durchs", "Ge\u00b7wog", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Vor\u00fcber kam es wild und gro\u00df,", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "kam", "es", "wild", "und", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Schiffer war darin,", "tokens": ["Kein", "Schif\u00b7fer", "war", "da\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir lehnten am Steuer regungslos,", "tokens": ["Wir", "lehn\u00b7ten", "am", "Steu\u00b7er", "re\u00b7gungs\u00b7los", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Es schwand und war dahin.", "tokens": ["Es", "schwand", "und", "war", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VAFIN", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Dahin, dahin, der Fr\u00fchwind pfiff,", "tokens": ["Da\u00b7hin", ",", "da\u00b7hin", ",", "der", "Fr\u00fch\u00b7wind", "pfiff", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PAV", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Herz ist wanderm\u00fcd',", "tokens": ["Mein", "Herz", "ist", "wan\u00b7der\u00b7m\u00fcd'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Herz, es wird zum Wikingschiff", "tokens": ["Mein", "Herz", ",", "es", "wird", "zum", "Wi\u00b7king\u00b7schiff"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und segelt frisc