{"textgrid.poem.55589": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "1L: Warum gabst du uns die tiefen Blicke,", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Warum gabst du uns die tiefen Blicke,", "tokens": ["Wa\u00b7rum", "gabst", "du", "uns", "die", "tie\u00b7fen", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Unsre Zukunft ahndungsvoll zu schaun,", "tokens": ["Uns\u00b7re", "Zu\u00b7kunft", "ahn\u00b7dungs\u00b7voll", "zu", "schaun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Unsrer Liebe, unserm Erdengl\u00fccke", "tokens": ["Uns\u00b7rer", "Lie\u00b7be", ",", "un\u00b7serm", "Er\u00b7den\u00b7gl\u00fc\u00b7cke"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "W\u00e4hnend selig nimmer hinzutraun?", "tokens": ["W\u00e4h\u00b7nend", "se\u00b7lig", "nim\u00b7mer", "hin\u00b7zu\u00b7traun", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Warum gabst uns, Schicksal, die Gef\u00fchle,", "tokens": ["Wa\u00b7rum", "gabst", "uns", ",", "Schick\u00b7sal", ",", "die", "Ge\u00b7f\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Uns einander in das Herz zu sehn,", "tokens": ["Uns", "ein\u00b7an\u00b7der", "in", "das", "Herz", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Um durch all die seltenen Gew\u00fchle", "tokens": ["Um", "durch", "all", "die", "sel\u00b7te\u00b7nen", "Ge\u00b7w\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "APPR", "PIAT", "ART", "ADJA", "NN"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Unser wahr Verh\u00e4ltnis auszusp\u00e4hn?", "tokens": ["Un\u00b7ser", "wahr", "Ver\u00b7h\u00e4lt\u00b7nis", "aus\u00b7zu\u00b7sp\u00e4hn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "NN", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Ach, so viele tausend Menschen kennen,", "tokens": ["Ach", ",", "so", "vie\u00b7le", "tau\u00b7send", "Men\u00b7schen", "ken\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "PIAT", "CARD", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dumpf sich treibend, kaum ihr eigen Herz,", "tokens": ["Dumpf", "sich", "trei\u00b7bend", ",", "kaum", "ihr", "ei\u00b7gen", "Herz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ADJD", "$,", "ADV", "PPER", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Schweben zwecklos hin und her und rennen", "tokens": ["Schwe\u00b7ben", "zweck\u00b7los", "hin", "und", "her", "und", "ren\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "PTKVZ", "KON", "PTKVZ", "KON", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Hoffungslos in unversehnem Schmerz;", "tokens": ["Hof\u00b7fungs\u00b7los", "in", "un\u00b7ver\u00b7seh\u00b7nem", "Schmerz", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Jauchzen wieder, wenn der schnellen Freuden", "tokens": ["Jauch\u00b7zen", "wie\u00b7der", ",", "wenn", "der", "schnel\u00b7len", "Freu\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADV", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Unerwart'te Morgenr\u00f6te tagt.", "tokens": ["Un\u00b7er\u00b7wart'\u00b7te", "Mor\u00b7gen\u00b7r\u00f6\u00b7te", "tagt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Nur uns armen liebevollen beiden", "tokens": ["Nur", "uns", "ar\u00b7men", "lie\u00b7be\u00b7vol\u00b7len", "bei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "ADJA", "VVFIN", "PIAT"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Ist das wechselseit'ge Gl\u00fcck versagt,", "tokens": ["Ist", "das", "wech\u00b7sel\u00b7seit'\u00b7ge", "Gl\u00fcck", "ver\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Uns zu lieben, ohn uns zu verstehen,", "tokens": ["Uns", "zu", "lie\u00b7ben", ",", "ohn", "uns", "zu", "ver\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "KOUI", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "In dem andern sehn, was er nie war,", "tokens": ["In", "dem", "an\u00b7dern", "sehn", ",", "was", "er", "nie", "war", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "VVINF", "$,", "PWS", "PPER", "ADV", "VAFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Immer frisch auf Traumgl\u00fcck auszugehen", "tokens": ["Im\u00b7mer", "frisch", "auf", "Traum\u00b7gl\u00fcck", "aus\u00b7zu\u00b7ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Und zu schwanken auch in Traumgefahr.", "tokens": ["Und", "zu", "schwan\u00b7ken", "auch", "in", "Traum\u00b7ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKZU", "VVINF", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Gl\u00fccklich, den ein leerer Traum besch\u00e4ftigt!", "tokens": ["Gl\u00fcck\u00b7lich", ",", "den", "ein", "lee\u00b7rer", "Traum", "be\u00b7sch\u00e4f\u00b7tigt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PRELS", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Gl\u00fccklich, dem die Ahndung eitel w\u00e4r!", "tokens": ["Gl\u00fcck\u00b7lich", ",", "dem", "die", "Ahn\u00b7dung", "ei\u00b7tel", "w\u00e4r", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PRELS", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Jede Gegenwart und jeder Blick bekr\u00e4ftigt", "tokens": ["Je\u00b7de", "Ge\u00b7gen\u00b7wart", "und", "je\u00b7der", "Blick", "be\u00b7kr\u00e4f\u00b7tigt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "VVPP"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Traum und Ahndung leider uns noch mehr.", "tokens": ["Traum", "und", "Ahn\u00b7dung", "lei\u00b7der", "uns", "noch", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "PPER", "ADV", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Sag, was will das Schicksal uns bereiten?", "tokens": ["Sag", ",", "was", "will", "das", "Schick\u00b7sal", "uns", "be\u00b7rei\u00b7ten", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VMFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Sag, wie band es uns so rein genau?", "tokens": ["Sag", ",", "wie", "band", "es", "uns", "so", "rein", "ge\u00b7nau", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VVFIN", "PPER", "PPER", "ADV", "ADJD", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Ach, du warst in abgelebten Zeiten", "tokens": ["Ach", ",", "du", "warst", "in", "ab\u00b7ge\u00b7leb\u00b7ten", "Zei\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Meine Schwester oder meine Frau.", "tokens": ["Mei\u00b7ne", "Schwes\u00b7ter", "o\u00b7der", "mei\u00b7ne", "Frau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Kanntest jeden Zug in meinem Wesen,", "tokens": ["Kann\u00b7test", "je\u00b7den", "Zug", "in", "mei\u00b7nem", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sp\u00e4htest, wie die reinste Nerve klingt,", "tokens": ["Sp\u00e4h\u00b7test", ",", "wie", "die", "reins\u00b7te", "Ner\u00b7ve", "klingt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Konntest mich mit ", "tokens": ["Konn\u00b7test", "mich", "mit"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PRF", "APPR"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Den so schwer ein sterblich Aug durchdringt;", "tokens": ["Den", "so", "schwer", "ein", "sterb\u00b7lich", "Aug", "durch\u00b7dringt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "ART", "ADJD", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Tropftest M\u00e4\u00dfigung dem hei\u00dfen Blute,", "tokens": ["Tropf\u00b7test", "M\u00e4\u00b7\u00dfi\u00b7gung", "dem", "hei\u00b7\u00dfen", "Blu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Richtetest den wilden irren Lauf,", "tokens": ["Rich\u00b7te\u00b7test", "den", "wil\u00b7den", "ir\u00b7ren", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Und in deinen Engelsarmen ruhte", "tokens": ["Und", "in", "dei\u00b7nen", "En\u00b7gel\u00b7sar\u00b7men", "ruh\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Die zerst\u00f6rte Brust sich wieder auf;", "tokens": ["Die", "zer\u00b7st\u00f6r\u00b7te", "Brust", "sich", "wie\u00b7der", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Hieltest zauberleicht ihn angebunden", "tokens": ["Hiel\u00b7test", "zau\u00b7berl\u00b7eicht", "ihn", "an\u00b7ge\u00b7bun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Und vergaukeltest ihm manchen Tag.", "tokens": ["Und", "ver\u00b7gau\u00b7kel\u00b7test", "ihm", "man\u00b7chen", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Welche Seligkeit glich jenen Wonnestunden,", "tokens": ["Wel\u00b7che", "Se\u00b7lig\u00b7keit", "glich", "je\u00b7nen", "Won\u00b7nes\u00b7tun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "VVFIN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.12": {"text": "Da er dankbar dir zu F\u00fc\u00dfen lag,", "tokens": ["Da", "er", "dank\u00b7bar", "dir", "zu", "F\u00fc\u00b7\u00dfen", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.13": {"text": "F\u00fchlt' sein Herz an deinem Herzen schwellen,", "tokens": ["F\u00fchlt'", "sein", "Herz", "an", "dei\u00b7nem", "Her\u00b7zen", "schwel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "F\u00fchlte sich in deinem Auge gut,", "tokens": ["F\u00fchl\u00b7te", "sich", "in", "dei\u00b7nem", "Au\u00b7ge", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.15": {"text": "Alle seine Sinnen sich erhellen", "tokens": ["Al\u00b7le", "sei\u00b7ne", "Sin\u00b7nen", "sich", "er\u00b7hel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN", "PRF", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Und beruhigen sein brausend Blut!", "tokens": ["Und", "be\u00b7ru\u00b7hi\u00b7gen", "sein", "brau\u00b7send", "Blut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJD", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Und von allem dem schwebt ein Erinnern", "tokens": ["Und", "von", "al\u00b7lem", "dem", "schwebt", "ein", "E\u00b7rin\u00b7nern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIS", "ART", "VVFIN", "ART", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Nur noch um das ungewisse Herz,", "tokens": ["Nur", "noch", "um", "das", "un\u00b7ge\u00b7wis\u00b7se", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "F\u00fchlt die alte Wahrheit ewig gleich im Innern,", "tokens": ["F\u00fchlt", "die", "al\u00b7te", "Wahr\u00b7heit", "e\u00b7wig", "gleich", "im", "In\u00b7nern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADJD", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Und der neue Zustand wird ihm Schmerz.", "tokens": ["Und", "der", "neu\u00b7e", "Zu\u00b7stand", "wird", "ihm", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "PPER", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Und wir scheinen uns nur halb beseelet,", "tokens": ["Und", "wir", "schei\u00b7nen", "uns", "nur", "halb", "be\u00b7see\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "D\u00e4mmernd ist um uns der hellste Tag.", "tokens": ["D\u00e4m\u00b7mernd", "ist", "um", "uns", "der", "hells\u00b7te", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Gl\u00fccklich, da\u00df das Schicksal, das uns qu\u00e4let,", "tokens": ["Gl\u00fcck\u00b7lich", ",", "da\u00df", "das", "Schick\u00b7sal", ",", "das", "uns", "qu\u00e4\u00b7let", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Uns doch nicht ver\u00e4ndern mag!", "tokens": ["Uns", "doch", "nicht", "ver\u00b7\u00e4n\u00b7dern", "mag", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}