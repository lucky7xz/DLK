{"textgrid.poem.52327": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "46. Wat einer hett, dat hett'e", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.85", "sv:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbso lang', as ick noch Ogen heww taum Seihn,", "tokens": ["\u00bb", "so", "lang'", ",", "as", "ick", "noch", "O\u00b7gen", "heww", "taum", "Seihn", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So lang' lat ick mi noch kein N\u00e4sen dreihn,", "tokens": ["So", "lang'", "lat", "ick", "mi", "noch", "kein", "N\u00e4\u00b7sen", "dreihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "NE", "ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So geiht dat noch nich los\u00ab, seggt Bur P\u00e4sel,", "tokens": ["So", "geiht", "dat", "noch", "nich", "los", "\u00ab", ",", "seggt", "Bur", "P\u00e4\u00b7sel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADV", "PTKNEG", "PTKVZ", "$(", "$,", "VVFIN", "NE", "NE", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "\u00bbso'n Schapskopp meint, ick b\u00fcn en Esel;", "tokens": ["\u00bb", "so'n", "Schaps\u00b7kopp", "meint", ",", "ick", "b\u00fcn", "en", "E\u00b7sel", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "NN", "VVFIN", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Ick sall em Geld up Zinsen leihn,", "tokens": ["Ick", "sall", "em", "Geld", "up", "Zin\u00b7sen", "leihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Un up'n Harwst will hei mi't wedder gewen.", "tokens": ["Un", "up'n", "Harwst", "will", "hei", "mi't", "wed\u00b7der", "ge\u00b7wen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "KON", "VVPP", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Ja! Wer en Nar doch wir, em dat tau gl\u00f6wen.", "tokens": ["Ja", "!", "Wer", "en", "Nar", "doch", "wir", ",", "em", "dat", "tau", "gl\u00f6\u00b7wen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PWS", "FM", "FM", "KON", "PPER", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Oll P\u00e4seln f\u00fchrt man so nich an.", "tokens": ["Oll", "P\u00e4\u00b7seln", "f\u00fchrt", "man", "so", "nich", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "PIS", "ADV", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ne, Vadder Vo\u00df, wenn ick di raden kann,", "tokens": ["Ne", ",", "Vad\u00b7der", "Vo\u00df", ",", "wenn", "ick", "di", "ra\u00b7den", "kann", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "NE", "$,", "KOUS", "PPER", "NE", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Tau so'ne Wippken un Maf\u00e4uken,", "tokens": ["Tau", "so'\u00b7ne", "Wipp\u00b7ken", "un", "Ma\u00b7f\u00e4u\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "FM", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.11": {"text": "Dor m\u00f6st di einen d\u00fcmmern s\u00e4uken,", "tokens": ["Dor", "m\u00f6st", "di", "ei\u00b7nen", "d\u00fcm\u00b7mern", "s\u00e4u\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "NE", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Ick lat mi keine N\u00e4sen dreihn!", "tokens": ["Ick", "lat", "mi", "kei\u00b7ne", "N\u00e4\u00b7sen", "dreihn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Wat einer ", "tokens": ["Wat", "ei\u00b7ner"], "token_info": ["word", "word"], "pos": ["NN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "Wenn einer klauk is, denn bef\u00f6tt'e", "tokens": ["Wenn", "ei\u00b7ner", "klauk", "is", ",", "denn", "be\u00b7f\u00f6tt'e"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PIS", "VVFIN", "FM", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Sick mit so'n Saken nich in desen slichten Tiden,", "tokens": ["Sick", "mit", "so'n", "Sa\u00b7ken", "nich", "in", "de\u00b7sen", "slich\u00b7ten", "Ti\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "PTKNEG", "APPR", "PRELAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Min Spruch, de is: Wat einer ", "tokens": ["Min", "Spruch", ",", "de", "is", ":", "Wat", "ei\u00b7ner"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "FM", "FM", "$.", "NN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dit wiren P\u00e4seln sin Gedanken,", "tokens": ["Dit", "wi\u00b7ren", "P\u00e4\u00b7seln", "sin", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "De hei so halwlud v\u00f6r sick spreckt,", "tokens": ["De", "hei", "so", "halw\u00b7lud", "v\u00f6r", "sick", "spreckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "As hei den Fautstig gung entlanken,", "tokens": ["As", "hei", "den", "Fauts\u00b7tig", "gung", "ent\u00b7lan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "NE", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "De an de Wisch sick r\u00fcmmer treckt.", "tokens": ["De", "an", "de", "Wisch", "sick", "r\u00fcm\u00b7mer", "treckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dat was tau Fr\u00fchjohrstid: de Strom, de hadd sick d\u00e4mmt,", "tokens": ["Dat", "was", "tau", "Fr\u00fch\u00b7johr\u00b7stid", ":", "de", "Strom", ",", "de", "hadd", "sick", "d\u00e4mmt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$.", "NE", "NN", "$,", "FM", "FM", "FM", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "De ganze Grund was \u00e4werswemmt;", "tokens": ["De", "gan\u00b7ze", "Grund", "was", "\u00e4\u00b7wers\u00b7wemmt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "PWS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dat Water, dat kamm up en Sto\u00df,", "tokens": ["Dat", "Wa\u00b7ter", ",", "dat", "kamm", "up", "en", "Sto\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ret Weg un Steg un Br\u00fcggen los,", "tokens": ["Ret", "Weg", "un", "Steg", "un", "Br\u00fcg\u00b7gen", "los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "FM", "FM", "FM", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Hadd Sch\u00fcnen, H\u00fcser runner smeten", "tokens": ["Hadd", "Sch\u00fc\u00b7nen", ",", "H\u00fc\u00b7ser", "run\u00b7ner", "sme\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Un Veih un Minschen mit sick reten.", "tokens": ["Un", "Veih", "un", "Min\u00b7schen", "mit", "sick", "re\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "NN", "APPR", "NN", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.11": {"text": "\u00bbne! Wat so'n Volk doch d\u00e4mlich is!", "tokens": ["\u00bb", "ne", "!", "Wat", "so'n", "Volk", "doch", "d\u00e4m\u00b7lich", "is", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "$.", "NN", "PIAT", "NN", "ADV", "ADJD", "FM", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Sick an dat Water antaubugen!", "tokens": ["Sick", "an", "dat", "Wa\u00b7ter", "an\u00b7tau\u00b7bu\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Je, ick! Ick s\u00fcll dat Water trugen?", "tokens": ["Je", ",", "ick", "!", "Ick", "s\u00fcll", "dat", "Wa\u00b7ter", "tru\u00b7gen", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "$.", "NE", "NE", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Ne!\u00ab seggt oll P\u00e4sel, \u00bbdat's gewi\u00df:", "tokens": ["Ne", "!", "\u00ab", "seggt", "oll", "P\u00e4\u00b7sel", ",", "\u00bb", "dat's", "ge\u00b7wi\u00df", ":"], "token_info": ["word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "$(", "VVFIN", "ADJD", "NN", "$,", "$(", "NE", "ADV", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.15": {"text": "Kein viruntwintig Pird, de t\u00f6gen", "tokens": ["Kein", "vir\u00b7unt\u00b7win\u00b7tig", "Pird", ",", "de", "t\u00f6\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "ADJD", "NN", "$,", "NE", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Mi an dat Water ran; ick bliw h\u00fcbsch up'n Dr\u00f6gen.\u00ab", "tokens": ["Mi", "an", "dat", "Wa\u00b7ter", "ran", ";", "ick", "bliw", "h\u00fcbsch", "up'n", "Dr\u00f6\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "APPR", "ART", "NN", "PTKVZ", "$.", "PPER", "VVFIN", "ADJD", "ADJA", "NN", "$.", "$("], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.17": {"text": "Un as hei noch so redt un deiht", "tokens": ["Un", "as", "hei", "noch", "so", "redt", "un", "deiht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "ADV", "ADV", "VVFIN", "FM", "FM"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.18": {"text": "Un \u00e4wer't Water r\u00e4wer s\u00fcht", "tokens": ["Un", "\u00e4\u00b7wer't", "Wa\u00b7ter", "r\u00e4\u00b7wer", "s\u00fcht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "NN", "ADJD", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.19": {"text": "Nah eine olle Ellerwrit,", "tokens": ["Nah", "ei\u00b7ne", "ol\u00b7le", "El\u00b7ler\u00b7writ", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "De midden in dat Water steiht,", "tokens": ["De", "mid\u00b7den", "in", "dat", "Wa\u00b7ter", "steiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Dunn is em't so, as ded sick dor wat r\u00f6gen.", "tokens": ["Dunn", "is", "em't", "so", ",", "as", "ded", "sick", "dor", "wat", "r\u00f6\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "VVFIN", "ADV", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.22": {"text": "\u00bbwo, Dunnerwetter! Kik einmal!", "tokens": ["\u00bb", "wo", ",", "Dun\u00b7ner\u00b7wet\u00b7ter", "!", "Kik", "ein\u00b7mal", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PWAV", "$,", "NN", "$.", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Dat is en Has'! De D\u00fcwel hal!", "tokens": ["Dat", "is", "en", "Has'", "!", "De", "D\u00fc\u00b7wel", "hal", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$.", "NE", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Wo's't m\u00e4glich! Ne! \u2013 Ne! s\u00fch dat Dirt!", "tokens": ["Wo'\u00b7s't", "m\u00e4g\u00b7lich", "!", "Ne", "!", "\u2013", "Ne", "!", "s\u00fch", "dat", "Dirt", "!"], "token_info": ["word", "word", "punct", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$.", "ART", "$.", "$(", "NE", "$.", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.25": {"text": "Dat Ding, dat is en Wagst\u00fcck wirt.\u00ab", "tokens": ["Dat", "Ding", ",", "dat", "is", "en", "Wag\u00b7st\u00fcck", "wirt", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "ART", "FM", "FM", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "En Kahn was ok denn bald tau Hand,", "tokens": ["En", "Kahn", "was", "ok", "denn", "bald", "tau", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "KON", "ADV", "CARD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Un hei st\u00f6tt lustig af von't Land", "tokens": ["Un", "hei", "st\u00f6tt", "lus\u00b7tig", "af", "von't", "Land"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "VVFIN", "ADJD", "NE", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Un lett dat R\u00e4uder d\u00fcchtig trecken.", "tokens": ["Un", "lett", "dat", "R\u00e4u\u00b7der", "d\u00fcch\u00b7tig", "tre\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.29": {"text": "\u00bbt\u00e4uw, du sallst her! Du sallst mi smecken!", "tokens": ["\u00bb", "t\u00e4uw", ",", "du", "sallst", "her", "!", "Du", "sallst", "mi", "sme\u00b7cken", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "$,", "PPER", "VVFIN", "PTKVZ", "$.", "PPER", "VVFIN", "NE", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Di hal ick mi! Kumm, H\u00e4schen, kumm!", "tokens": ["Di", "hal", "ick", "mi", "!", "Kumm", ",", "H\u00e4\u00b7schen", ",", "kumm", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$.", "NE", "$,", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Sei holl'n mi all v\u00f6r gruglich dumm!", "tokens": ["Sei", "holl'n", "mi", "all", "v\u00f6r", "grug\u00b7lich", "dumm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "NE", "PIAT", "NN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Je, ick b\u00fcn klauk; pa\u00dft ji man acht!", "tokens": ["Je", ",", "ick", "b\u00fcn", "klauk", ";", "pa\u00dft", "ji", "man", "acht", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "FM.la", "FM.la", "FM.la", "$.", "VVFIN", "PPER", "PIS", "CARD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Wer lacht tauletzt, am besten