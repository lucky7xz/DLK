{"textgrid.poem.60369": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "An die Biologen", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Wahrheit hat die Kunde", "tokens": ["Die", "Wahr\u00b7heit", "hat", "die", "Kun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vom tiefen Lebensgrunde", "tokens": ["Vom", "tie\u00b7fen", "Le\u00b7bens\u00b7grun\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als winzgen Zettel", "tokens": ["Als", "winz\u00b7gen", "Zet\u00b7tel"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "In eine Nu\u00df getan", "tokens": ["In", "ei\u00b7ne", "Nu\u00df", "ge\u00b7tan"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und warf den Bettel", "tokens": ["Und", "warf", "den", "Bet\u00b7tel"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "In den Ozean.", "tokens": ["In", "den", "O\u00b7ze\u00b7an", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Das Meer ist gro\u00df, die Nu\u00df ist klein;", "tokens": ["Das", "Meer", "ist", "gro\u00df", ",", "die", "Nu\u00df", "ist", "klein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Hat wohl am kleinen Wunderschrein", "tokens": ["Hat", "wohl", "am", "klei\u00b7nen", "Wun\u00b7der\u00b7schrein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Schon ein Pilot vorbeigeflucht?", "tokens": ["Schon", "ein", "Pi\u00b7lot", "vor\u00b7bei\u00b7ge\u00b7flucht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Sucht! sucht! \u2013", "tokens": ["Sucht", "!", "sucht", "!", "\u2013"], "token_info": ["word", "punct", "word", "punct", "punct"], "pos": ["VVFIN", "$.", "VVFIN", "$.", "$("], "meter": "--", "measure": "unknown.measure.zero"}, "line.11": {"text": "Die Wahrheit schrieb die Kunde", "tokens": ["Die", "Wahr\u00b7heit", "schrieb", "die", "Kun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Vom tiefen Lebensgrunde", "tokens": ["Vom", "tie\u00b7fen", "Le\u00b7bens\u00b7grun\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Wohl einem Voglern auf den Kopf,", "tokens": ["Wohl", "ei\u00b7nem", "Vog\u00b7lern", "auf", "den", "Kopf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Untern Schopf,", "tokens": ["Un\u00b7tern", "Schopf", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.15": {"text": "Auf des Hirnes glatte Schale;", "tokens": ["Auf", "des", "Hir\u00b7nes", "glat\u00b7te", "Scha\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Das V\u00f6glein flog in alle Welt,", "tokens": ["Das", "V\u00f6\u00b7glein", "flog", "in", "al\u00b7le", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Ihm ward durch Berg' und Tale", "tokens": ["Ihm", "ward", "durch", "Ber\u00b7g'", "und", "Ta\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "KON", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.18": {"text": "Bis jetzt vergeblich nachgestellt.", "tokens": ["Bis", "jetzt", "ver\u00b7geb\u00b7lich", "nach\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Nur zugeforscht! wer wei\u00df denn auch,", "tokens": ["Nur", "zu\u00b7ge\u00b7forscht", "!", "wer", "wei\u00df", "denn", "auch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$.", "PWS", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Ob nicht der Vogel euren Strauch", "tokens": ["Ob", "nicht", "der", "Vo\u00b7gel", "eu\u00b7ren", "Strauch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ART", "NE", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Zu seinem Sitze auserkiest", "tokens": ["Zu", "sei\u00b7nem", "Sit\u00b7ze", "aus\u00b7er\u00b7kiest"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Und, frohgelaunt, bei Fr\u00fchlingswettern", "tokens": ["Und", ",", "froh\u00b7ge\u00b7launt", ",", "bei", "Fr\u00fch\u00b7lings\u00b7wet\u00b7tern"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "$,", "ADJD", "$,", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Von seinen schopfgeborgnen Lettern", "tokens": ["Von", "sei\u00b7nen", "schopf\u00b7ge\u00b7borg\u00b7nen", "Let\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Euch singend was herunterliest!", "tokens": ["Euch", "sin\u00b7gend", "was", "her\u00b7un\u00b7ter\u00b7liest", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PWS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Ist auch das V\u00f6glein auf der Flucht,", "tokens": ["Ist", "auch", "das", "V\u00f6\u00b7glein", "auf", "der", "Flucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Sucht! sucht!", "tokens": ["Sucht", "!", "sucht", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$.", "VVFIN", "$."], "meter": "--", "measure": "unknown.measure.zero"}}}}}