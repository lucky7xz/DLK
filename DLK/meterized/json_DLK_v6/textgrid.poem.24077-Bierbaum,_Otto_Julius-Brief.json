{"textgrid.poem.24077": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Brief", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mir war die Liebe lange nur ein Spiel;", "tokens": ["Mir", "war", "die", "Lie\u00b7be", "lan\u00b7ge", "nur", "ein", "Spiel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Leicht setzt ich wenig ein und holte viel,", "tokens": ["Leicht", "setzt", "ich", "we\u00b7nig", "ein", "und", "hol\u00b7te", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "PTKVZ", "KON", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und lustig warf den goldenen Gewinn", "tokens": ["Und", "lus\u00b7tig", "warf", "den", "gol\u00b7de\u00b7nen", "Ge\u00b7winn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich gerne bald in andre Sch\u00fcrzen hin.", "tokens": ["Ich", "ger\u00b7ne", "bald", "in", "and\u00b7re", "Sch\u00fcr\u00b7zen", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Oh ja, das Herz, es war wohl auch dabei,", "tokens": ["Oh", "ja", ",", "das", "Herz", ",", "es", "war", "wohl", "auch", "da\u00b7bei", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$,", "ART", "NN", "$,", "PPER", "VAFIN", "ADV", "ADV", "PAV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Leis klang es mit wie ferne Melodei", "tokens": ["Leis", "klang", "es", "mit", "wie", "fer\u00b7ne", "Me\u00b7lo\u00b7dei"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "APPR", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dem lauten Sang der tanzbewegten Lust,", "tokens": ["Dem", "lau\u00b7ten", "Sang", "der", "tanz\u00b7be\u00b7weg\u00b7ten", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch Stille war im Innersten der Brust.", "tokens": ["Doch", "Stil\u00b7le", "war", "im", "In\u00b7ners\u00b7ten", "der", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Was da, von Friedensrosen mild umbl\u00fcht,", "tokens": ["Was", "da", ",", "von", "Frie\u00b7dens\u00b7ro\u00b7sen", "mild", "um\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "$,", "APPR", "NE", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Dem einen Herzen hei\u00df entgegengl\u00fcht,", "tokens": ["Dem", "ei\u00b7nen", "Her\u00b7zen", "hei\u00df", "ent\u00b7ge\u00b7gen\u00b7gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Du hasts zuerst geweckt; \u2013 nun ist es weh,", "tokens": ["Du", "hasts", "zu\u00b7erst", "ge\u00b7weckt", ";", "\u2013", "nun", "ist", "es", "weh", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$.", "$(", "ADV", "VAFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Das leichte Herz, ein wildbewegter See", "tokens": ["Das", "leich\u00b7te", "Herz", ",", "ein", "wild\u00b7be\u00b7weg\u00b7ter", "See"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Voll Unget\u00fcmen, die die Qual gebar,", "tokens": ["Voll", "Un\u00b7ge\u00b7t\u00fc\u00b7men", ",", "die", "die", "Qual", "ge\u00b7bar", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "PRELS", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Die doch nur Liebe, Liebe, Liebe war.", "tokens": ["Die", "doch", "nur", "Lie\u00b7be", ",", "Lie\u00b7be", ",", "Lie\u00b7be", "war", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "NN", "$,", "NN", "$,", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich wei\u00df, du lachst, wenn du von Qualen liest,", "tokens": ["Ich", "wei\u00df", ",", "du", "lachst", ",", "wenn", "du", "von", "Qua\u00b7len", "liest", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "KOUS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In deinem Herzen eine Blume sprie\u00dft,", "tokens": ["In", "dei\u00b7nem", "Her\u00b7zen", "ei\u00b7ne", "Blu\u00b7me", "sprie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die leicht im Winde ihre Bl\u00fcte tr\u00e4gt,", "tokens": ["Die", "leicht", "im", "Win\u00b7de", "ih\u00b7re", "Bl\u00fc\u00b7te", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPRART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die nichts nach Qualenunget\u00fcmen fr\u00e4gt;", "tokens": ["Die", "nichts", "nach", "Qua\u00b7le\u00b7nun\u00b7ge\u00b7t\u00fc\u00b7men", "fr\u00e4gt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Im eigenen Dufte wiegt sie her und hin \u2013:", "tokens": ["Im", "ei\u00b7ge\u00b7nen", "Duf\u00b7te", "wiegt", "sie", "her", "und", "hin", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "KON", "ADV", "$(", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Die Blume ist dein gl\u00fccklich-leichter Sinn.", "tokens": ["Die", "Blu\u00b7me", "ist", "dein", "gl\u00fc\u00b7ck\u00b7lich\u00b7leich\u00b7ter", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Sie soll dir nie im Herzensfrost vergehn,", "tokens": ["Sie", "soll", "dir", "nie", "im", "Her\u00b7zens\u00b7frost", "ver\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Aus jedem Leide soll sie auferstehn", "tokens": ["Aus", "je\u00b7dem", "Lei\u00b7de", "soll", "sie", "auf\u00b7er\u00b7stehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wie Maitaghelle, da der Winter schwand", "tokens": ["Wie", "Mai\u00b7tag\u00b7hel\u00b7le", ",", "da", "der", "Win\u00b7ter", "schwand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "$,", "KOUS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Dem Sonnensiege in das Nebelland ...", "tokens": ["Dem", "Son\u00b7nen\u00b7sie\u00b7ge", "in", "das", "Ne\u00b7bel\u00b7land", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Was mir die Liebe und ihr Leid beschied?", "tokens": ["Was", "mir", "die", "Lie\u00b7be", "und", "ihr", "Leid", "be\u00b7schied", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich f\u00fchl es schon; es keimt ein neues Lied.", "tokens": ["Ich", "f\u00fchl", "es", "schon", ";", "es", "keimt", "ein", "neu\u00b7es", "Lied", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das wird von dir ein gl\u00fchend Singen sein,", "tokens": ["Das", "wird", "von", "dir", "ein", "gl\u00fc\u00b7hend", "Sin\u00b7gen", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PPER", "ART", "ADJD", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das wird aus Qualenwust mein Herz befrein.", "tokens": ["Das", "wird", "aus", "Qua\u00b7len\u00b7wust", "mein", "Herz", "be\u00b7fr\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wie Thr\u00e4nensturz schwillt hei\u00df sein starker Flu\u00df,", "tokens": ["Wie", "Thr\u00e4\u00b7nen\u00b7sturz", "schwillt", "hei\u00df", "sein", "star\u00b7ker", "Flu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "ADJD", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und aus dem Herzen kommts in einem Gu\u00df,", "tokens": ["Und", "aus", "dem", "Her\u00b7zen", "kommts", "in", "ei\u00b7nem", "Gu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich halte nichts, ich halte nichts zur\u00fcck,", "tokens": ["Ich", "hal\u00b7te", "nichts", ",", "ich", "hal\u00b7te", "nichts", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$,", "PPER", "VVFIN", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Im Lied verstr\u00f6me ich mein ganzes Gl\u00fcck.", "tokens": ["Im", "Lied", "ver\u00b7str\u00f6\u00b7me", "ich", "mein", "gan\u00b7zes", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ob du es f\u00fchlst, was ich dir hier gesteh?", "tokens": ["Ob", "du", "es", "f\u00fchlst", ",", "was", "ich", "dir", "hier", "ge\u00b7steh", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "PWS", "PPER", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das f\u00fchlst du wohl, es ist ein tiefes Weh", "tokens": ["Das", "f\u00fchlst", "du", "wohl", ",", "es", "ist", "ein", "tie\u00b7fes", "Weh"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und eine Gnade doch; es raubt und giebt ...", "tokens": ["Und", "ei\u00b7ne", "Gna\u00b7de", "doch", ";", "es", "raubt", "und", "giebt", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "$.", "PPER", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Oh, M\u00e4dchen du, wie hab ich dich geliebt!", "tokens": ["Oh", ",", "M\u00e4d\u00b7chen", "du", ",", "wie", "hab", "ich", "dich", "ge\u00b7liebt", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "PPER", "$,", "PWAV", "VAFIN", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}