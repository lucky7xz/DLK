{"dta.poem.6492": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Ton.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Viel Oerter finden sich, die nie zur\u00fccke lencken", "tokens": ["Viel", "O\u00b7er\u00b7ter", "fin\u00b7den", "sich", ",", "die", "nie", "zu\u00b7r\u00fc\u00b7cke", "len\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "PRF", "$,", "PRELS", "ADV", "VVFIN", "VVINF"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die T\u00f6ne, die wir in sie sencken:", "tokens": ["Die", "T\u00f6\u00b7ne", ",", "die", "wir", "in", "sie", "sen\u00b7cken", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und andere, worinn also die T\u00f6ne fallen;", "tokens": ["Und", "an\u00b7de\u00b7re", ",", "wo\u00b7rinn", "al\u00b7so", "die", "T\u00f6\u00b7ne", "fal\u00b7len", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PWAV", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df selbe, wiederholt, zum \u00f6fftern r\u00fcckwerts prallen.", "tokens": ["Da\u00df", "sel\u00b7be", ",", "wie\u00b7der\u00b7holt", ",", "zum", "\u00f6ff\u00b7tern", "r\u00fcck\u00b7werts", "pral\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "$,", "VVPP", "$,", "APPRART", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Viel H\u00f6len giebts, worinn die T\u00f6ne gleichsam sterben,", "tokens": ["Viel", "H\u00f6\u00b7len", "giebts", ",", "wo\u00b7rinn", "die", "T\u00f6\u00b7ne", "gleich\u00b7sam", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,", "PWAV", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und, als verschluckt, ohn Gegenklang verderben.", "tokens": ["Und", ",", "als", "ver\u00b7schluckt", ",", "ohn", "Ge\u00b7gen\u00b7klang", "ver\u00b7der\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "VVPP", "$,", "KOUI", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Noch giebt es andre H\u00f6len,", "tokens": ["Noch", "giebt", "es", "and\u00b7re", "H\u00f6\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die, was man ihnen sagt, gedoppelt uns erzehlen.", "tokens": ["Die", ",", "was", "man", "ih\u00b7nen", "sagt", ",", "ge\u00b7dop\u00b7pelt", "uns", "er\u00b7zeh\u00b7len", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELS", "PIS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Auch Grotten, Felsen, Stein\u2019, in die der Ton sich senckt,", "tokens": ["Auch", "Grot\u00b7ten", ",", "Fel\u00b7sen", ",", "Stein'", ",", "in", "die", "der", "Ton", "sich", "senckt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "NN", "$,", "NN", "$,", "APPR", "PRELS", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Sich recht zusammen presst und r\u00fcckwerts lenckt,", "tokens": ["Sich", "recht", "zu\u00b7sam\u00b7men", "presst", "und", "r\u00fcck\u00b7werts", "lenckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ADV", "VVFIN", "KON", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Ja eben, wie man ihn formirt,", "tokens": ["Ja", "e\u00b7ben", ",", "wie", "man", "ihn", "for\u00b7mirt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "$,", "PWAV", "PIS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Uns, \u00f6ffters wiederholt, wird wieder zugef\u00fchrt.", "tokens": ["Uns", ",", "\u00f6ff\u00b7ters", "wie\u00b7der\u00b7holt", ",", "wird", "wie\u00b7der", "zu\u00b7ge\u00b7f\u00fchrt", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADV", "VVPP", "$,", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}