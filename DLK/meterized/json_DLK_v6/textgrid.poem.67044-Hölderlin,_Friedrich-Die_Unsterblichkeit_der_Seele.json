{"textgrid.poem.67044": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Unsterblichkeit der Seele", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da steh ich auf dem H\u00fcgel, und schau umher,", "tokens": ["Da", "steh", "ich", "auf", "dem", "H\u00fc\u00b7gel", ",", "und", "schau", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "KON", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wie alles auflebt, alles empor sich dehnt,", "tokens": ["Wie", "al\u00b7les", "auf\u00b7lebt", ",", "al\u00b7les", "em\u00b7por", "sich", "dehnt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "$,", "PIS", "PTKVZ", "PRF", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und Hain und Flur, und Tal, und H\u00fcgel", "tokens": ["Und", "Hain", "und", "Flur", ",", "und", "Tal", ",", "und", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "$,", "KON", "NN", "$,", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Jauchzet im herrlichen Morgenstrahle.", "tokens": ["Jauch\u00b7zet", "im", "herr\u00b7li\u00b7chen", "Mor\u00b7gen\u00b7strah\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "O diese Nacht \u2013 da bebtet ihr, Sch\u00f6pfungen!", "tokens": ["O", "die\u00b7se", "Nacht", "\u2013", "da", "beb\u00b7tet", "ihr", ",", "Sch\u00f6p\u00b7fun\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "PDAT", "NN", "$(", "ADV", "VVFIN", "PPER", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da weckten nahe Donner die Schlummernde,", "tokens": ["Da", "weck\u00b7ten", "na\u00b7he", "Don\u00b7ner", "die", "Schlum\u00b7mern\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da schreckten im Gefilde grause", "tokens": ["Da", "schreck\u00b7ten", "im", "Ge\u00b7fil\u00b7de", "grau\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zackigte Blitze die stille Schatten.", "tokens": ["Za\u00b7ckig\u00b7te", "Blit\u00b7ze", "die", "stil\u00b7le", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Jetzt jauchzt die Erde, feiert im Perlenschmuck", "tokens": ["Jetzt", "jauchzt", "die", "Er\u00b7de", ",", "fei\u00b7ert", "im", "Per\u00b7len\u00b7schmuck"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den Sieg des Tages \u00fcber das Graun der Nacht \u2013", "tokens": ["Den", "Sieg", "des", "Ta\u00b7ges", "\u00fc\u00b7ber", "das", "Graun", "der", "Nacht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Doch freut sich meine Seele sch\u00f6ner;", "tokens": ["Doch", "freut", "sich", "mei\u00b7ne", "See\u00b7le", "sch\u00f6\u00b7ner", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Denn sie besiegt der Vernichtung Grauen.", "tokens": ["Denn", "sie", "be\u00b7siegt", "der", "Ver\u00b7nich\u00b7tung", "Grau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Denn \u2013 o ihr Himmel! Adams Geschlechte sinds,", "tokens": ["Denn", "\u2013", "o", "ihr", "Him\u00b7mel", "!", "A\u00b7dams", "Ge\u00b7schlech\u00b7te", "sinds", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$(", "FM", "PPOSAT", "NN", "$.", "NE", "NN", "VAFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die diese Erd im niedrigen Scho\u00dfe tr\u00e4gt \u2013", "tokens": ["Die", "die\u00b7se", "Erd", "im", "nied\u00b7ri\u00b7gen", "Scho\u00b7\u00dfe", "tr\u00e4gt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "O betet an, Geschlechte Adams!", "tokens": ["O", "be\u00b7tet", "an", ",", "Ge\u00b7schlech\u00b7te", "A\u00b7dams", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKVZ", "$,", "NN", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Jauchzet mit Engeln, Geschlechte Adams!", "tokens": ["Jauch\u00b7zet", "mit", "En\u00b7geln", ",", "Ge\u00b7schlech\u00b7te", "A\u00b7dams", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "NN", "NE", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "O ihr seid sch\u00f6n, ihr herrliche Sch\u00f6pfungen!", "tokens": ["O", "ihr", "seid", "sch\u00f6n", ",", "ihr", "herr\u00b7li\u00b7che", "Sch\u00f6p\u00b7fun\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VAFIN", "ADJD", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Geschm\u00fcckt mit Perlen blitzet das Blumenfeld;", "tokens": ["Ge\u00b7schm\u00fcckt", "mit", "Per\u00b7len", "blit\u00b7zet", "das", "Blu\u00b7men\u00b7feld", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Doch sch\u00f6ner ist des Menschen Seele,", "tokens": ["Doch", "sch\u00f6\u00b7ner", "ist", "des", "Men\u00b7schen", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn sie von euch sich zu Gott erhebet.", "tokens": ["Wenn", "sie", "von", "euch", "sich", "zu", "Gott", "er\u00b7he\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "O, dich zu denken, die du aus Gottes Hand", "tokens": ["O", ",", "dich", "zu", "den\u00b7ken", ",", "die", "du", "aus", "Got\u00b7tes", "Hand"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PPER", "PTKZU", "VVINF", "$,", "PRELS", "PPER", "APPR", "NN", "NN"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Erhaben \u00fcber tausend Gesch\u00f6pfe gingst,", "tokens": ["Er\u00b7ha\u00b7ben", "\u00fc\u00b7ber", "tau\u00b7send", "Ge\u00b7sch\u00f6p\u00b7fe", "gingst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "In deiner Klarheit dich zu denken,", "tokens": ["In", "dei\u00b7ner", "Klar\u00b7heit", "dich", "zu", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn du zu Gott dich erhebst, o Seele!", "tokens": ["Wenn", "du", "zu", "Gott", "dich", "er\u00b7hebst", ",", "o", "See\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "PPER", "VVFIN", "$,", "FM", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Ha! diese Eiche \u2013 strecket die stolze nicht", "tokens": ["Ha", "!", "die\u00b7se", "Ei\u00b7che", "\u2013", "stre\u00b7cket", "die", "stol\u00b7ze", "nicht"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PDAT", "NN", "$(", "VVFIN", "ART", "ADJA", "PTKNEG"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Ihr Haupt empor, als st\u00fcnde sie ewig so?", "tokens": ["Ihr", "Haupt", "em\u00b7por", ",", "als", "st\u00fcn\u00b7de", "sie", "e\u00b7wig", "so", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$,", "KOUS", "ADJA", "PPER", "ADJD", "ADV", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und drohte nicht Jehovas Donner,", "tokens": ["Und", "droh\u00b7te", "nicht", "Je\u00b7ho\u00b7vas", "Don\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Niederzuschmettern die stolze Eiche?", "tokens": ["Nie\u00b7der\u00b7zu\u00b7schmet\u00b7tern", "die", "stol\u00b7ze", "Ei\u00b7che", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Ha! diese Felsen \u2013 blicken die stolze nicht", "tokens": ["Ha", "!", "die\u00b7se", "Fel\u00b7sen", "\u2013", "bli\u00b7cken", "die", "stol\u00b7ze", "nicht"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PDAT", "NN", "$(", "VVFIN", "ART", "ADJA", "PTKNEG"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Hinab ins Tal, als blieben sie ewig so?", "tokens": ["Hin\u00b7ab", "ins", "Tal", ",", "als", "blie\u00b7ben", "sie", "e\u00b7wig", "so", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "KOUS", "VVFIN", "PPER", "ADJD", "ADV", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Jahrhunderte \u2013 und an der Stelle", "tokens": ["Jahr\u00b7hun\u00b7der\u00b7te", "\u2013", "und", "an", "der", "Stel\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$(", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Malmet der Wandrer zu Staub das Sandkorn.", "tokens": ["Mal\u00b7met", "der", "Wand\u00b7rer", "zu", "Staub", "das", "Sand\u00b7korn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPR", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "Und meine Seele \u2013 wo ist dein Stachel, Tod?", "tokens": ["Und", "mei\u00b7ne", "See\u00b7le", "\u2013", "wo", "ist", "dein", "Sta\u00b7chel", ",", "Tod", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$(", "PWAV", "VAFIN", "PPOSAT", "NN", "$,", "NN", "$."], "meter": "-+-+-++-+-+", "measure": "unknown.measure.hexa"}, "line.2": {"text": "O beugt euch, Felsen! neiget euch ehrfurchtsvoll,", "tokens": ["O", "beugt", "euch", ",", "Fel\u00b7sen", "!", "nei\u00b7get", "euch", "ehr\u00b7furchts\u00b7voll", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "NN", "$.", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ihr stolze Eichen! \u2013 h\u00f6rts und beugt euch!", "tokens": ["Ihr", "stol\u00b7ze", "Ei\u00b7chen", "!", "\u2013", "h\u00f6rts", "und", "beugt", "euch", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "$(", "ADV", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ewig ist, ewig des Menschen Seele.", "tokens": ["E\u00b7wig", "ist", ",", "e\u00b7wig", "des", "Men\u00b7schen", "See\u00b7le", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$,", "ADJD", "ART", "NN", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.10": {"line.1": {"text": "Mit grausem Zischen brauset der Sturm daher,", "tokens": ["Mit", "grau\u00b7sem", "Zi\u00b7schen", "brau\u00b7set", "der", "Sturm", "da\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "PAV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ich komme, spricht er, und das Geh\u00f6lze kracht", "tokens": ["Ich", "kom\u00b7me", ",", "spricht", "er", ",", "und", "das", "Ge\u00b7h\u00f6l\u00b7ze", "kracht"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "PPER", "$,", "KON", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und T\u00fcrme wanken, St\u00e4dte sinken,", "tokens": ["Und", "T\u00fcr\u00b7me", "wan\u00b7ken", ",", "St\u00e4d\u00b7te", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$,", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "L\u00e4nder zerschmettern, wenn ich ergrimme.", "tokens": ["L\u00e4n\u00b7der", "zer\u00b7schmet\u00b7tern", ",", "wenn", "ich", "er\u00b7grim\u00b7me", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.11": {"line.1": {"text": "Doch \u2013 wandelt nicht in Schweigen der Winde Dr\u00e4un?", "tokens": ["Doch", "\u2013", "wan\u00b7delt", "nicht", "in", "Schwei\u00b7gen", "der", "Win\u00b7de", "Dr\u00e4un", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "VVFIN", "PTKNEG", "APPR", "NN", "ART", "NN", "NE", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Macht nicht ein Tag die brausende atemlos?", "tokens": ["Macht", "nicht", "ein", "Tag", "die", "brau\u00b7sen\u00b7de", "a\u00b7tem\u00b7los", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Ein Tag, ein Tag, an dem ein andrer", "tokens": ["Ein", "Tag", ",", "ein", "Tag", ",", "an", "dem", "ein", "an\u00b7drer"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "APPR", "PRELS", "ART", "ADJA"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sturm der Verwesten Gebeine sammelt.", "tokens": ["Sturm", "der", "Ver\u00b7wes\u00b7ten", "Ge\u00b7bei\u00b7ne", "sam\u00b7melt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.12": {"line.1": {"text": "Zum Himmel sch\u00e4umt und woget der Ozean", "tokens": ["Zum", "Him\u00b7mel", "sch\u00e4umt", "und", "wo\u00b7get", "der", "O\u00b7ze\u00b7an"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In seinem Grimm, der Sonnen und Monde Heer", "tokens": ["In", "sei\u00b7nem", "Grimm", ",", "der", "Son\u00b7nen", "und", "Mon\u00b7de", "Heer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NE", "$,", "ART", "NN", "KON", "NE", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Herab aus ihren H\u00f6hn, die stolze,", "tokens": ["Her\u00b7ab", "aus", "ih\u00b7ren", "H\u00f6hn", ",", "die", "stol\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Niederzurei\u00dfen in seine Tiefen.", "tokens": ["Nie\u00b7der\u00b7zu\u00b7rei\u00b7\u00dfen", "in", "sei\u00b7ne", "Tie\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.13": {"line.1": {"text": "Was bist du, Erde? hadert der Ozean,", "tokens": ["Was", "bist", "du", ",", "Er\u00b7de", "?", "ha\u00b7dert", "der", "O\u00b7ze\u00b7an", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "NN", "$.", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Was bist du? streck ich nicht, wie die Fittige", "tokens": ["Was", "bist", "du", "?", "streck", "ich", "nicht", ",", "wie", "die", "Fit\u00b7ti\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "$.", "VVFIN", "PPER", "PTKNEG", "$,", "PWAV", "ART", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Aufs Reh der Adler, meine Arme", "tokens": ["Aufs", "Reh", "der", "Ad\u00b7ler", ",", "mei\u00b7ne", "Ar\u00b7me"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u00dcber die Schw\u00e4chliche aus? \u2013 Was bist du,", "tokens": ["\u00dc\u00b7ber", "die", "Schw\u00e4ch\u00b7li\u00b7che", "aus", "?", "\u2013", "Was", "bist", "du", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$.", "$(", "PWS", "VAFIN", "PPER", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.14": {"line.1": {"text": "Wenn nicht zur Sonne segnend mein Hauch sich hebt,", "tokens": ["Wenn", "nicht", "zur", "Son\u00b7ne", "seg\u00b7nend", "mein", "Hauch", "sich", "hebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "APPRART", "NN", "VVPP", "PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Zu tr\u00e4nken dich mit Regen und Morgentau?", "tokens": ["Zu", "tr\u00e4n\u00b7ken", "dich", "mit", "Re\u00b7gen", "und", "Mor\u00b7gen\u00b7tau", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PRF", "APPR", "NN", "KON", "NE", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und wann er sich erhebt, zu nahn in", "tokens": ["Und", "wann", "er", "sich", "er\u00b7hebt", ",", "zu", "nahn", "in"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "PRF", "VVFIN", "$,", "APPR", "ADJA", "APPR"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mitternachtswolken, zu nahn mit Donnern,", "tokens": ["Mit\u00b7ter\u00b7nachts\u00b7wol\u00b7ken", ",", "zu", "nahn", "mit", "Don\u00b7nern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ADJA", "APPR", "NN", "$,"], "meter": "+-+---+-+-", "measure": "unknown.measure.tetra"}}, "stanza.15": {"line.1": {"text": "Ha! bebst du nicht, Gebrechliche? bebst du nicht? \u2013", "tokens": ["Ha", "!", "bebst", "du", "nicht", ",", "Ge\u00b7brech\u00b7li\u00b7che", "?", "bebst", "du", "nicht", "?", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "PTKNEG", "$,", "NN", "$.", "VVFIN", "PPER", "PTKNEG", "$.", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und doch! vor jenem Tage verkriechet sich", "tokens": ["Und", "doch", "!", "vor", "je\u00b7nem", "Ta\u00b7ge", "ver\u00b7krie\u00b7chet", "sich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$.", "APPR", "PDAT", "NN", "VVFIN", "PRF"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Das Meer, und seiner Wogen keine", "tokens": ["Das", "Meer", ",", "und", "sei\u00b7ner", "Wo\u00b7gen", "kei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "PPOSAT", "NN", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "T\u00f6nt in die Jubel der Auferstehung.", "tokens": ["T\u00f6nt", "in", "die", "Ju\u00b7bel", "der", "Auf\u00b7er\u00b7ste\u00b7hung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.16": {"line.1": {"text": "Wie herrlich, Sonne! wandelst du nicht daher!", "tokens": ["Wie", "herr\u00b7lich", ",", "Son\u00b7ne", "!", "wan\u00b7delst", "du", "nicht", "da\u00b7her", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "NN", "$.", "VVFIN", "PPER", "PTKNEG", "PAV", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dein Kommen und dein Scheiden ist Widerschein", "tokens": ["Dein", "Kom\u00b7men", "und", "dein", "Schei\u00b7den", "ist", "Wi\u00b7der\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "VAFIN", "NN"], "meter": "-+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Vom Thron des Ewigen; wie g\u00f6ttlich", "tokens": ["Vom", "Thron", "des", "E\u00b7wi\u00b7gen", ";", "wie", "g\u00f6tt\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJA", "$.", "PWAV", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Blickst du herab auf die Menschenkinder.", "tokens": ["Blickst", "du", "her\u00b7ab", "auf", "die", "Men\u00b7schen\u00b7kin\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.17": {"line.1": {"text": "Der Wilde gafft mit zitternden Wimpern dich,", "tokens": ["Der", "Wil\u00b7de", "gafft", "mit", "zit\u00b7tern\u00b7den", "Wim\u00b7pern", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "O Heldin, an, von heiligen Ahndungen", "tokens": ["O", "Hel\u00b7din", ",", "an", ",", "von", "hei\u00b7li\u00b7gen", "Ahn\u00b7dun\u00b7gen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "PTKVZ", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durchbebt, verh\u00fcllt er schnell sein Haupt und", "tokens": ["Durch\u00b7bebt", ",", "ver\u00b7h\u00fcllt", "er", "schnell", "sein", "Haupt", "und"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "KON"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nennet dich Gott, und erbaut dir Tempel.", "tokens": ["Nen\u00b7net", "dich", "Gott", ",", "und", "er\u00b7baut", "dir", "Tem\u00b7pel", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "KON", "VVFIN", "PPER", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.18": {"line.1": {"text": "Und doch, o Sonne! endet dereinst dein Lauf,", "tokens": ["Und", "doch", ",", "o", "Son\u00b7ne", "!", "en\u00b7det", "de\u00b7reinst", "dein", "Lauf", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "FM", "NN", "$.", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Verlischt an jenem Tage dein hehres Licht.", "tokens": ["Ver\u00b7lischt", "an", "je\u00b7nem", "Ta\u00b7ge", "dein", "heh\u00b7res", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDAT", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Doch wirbelt sie an jenem Tage", "tokens": ["Doch", "wir\u00b7belt", "sie", "an", "je\u00b7nem", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Rauchend die Himmel hindurch, und schmettert.", "tokens": ["Rau\u00b7chend", "die", "Him\u00b7mel", "hin\u00b7durch", ",", "und", "schmet\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "PAV", "$,", "KON", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.19": {"line.1": {"text": "O du Entz\u00fccken meiner Unsterblichkeit!", "tokens": ["O", "du", "Ent\u00b7z\u00fc\u00b7cken", "mei\u00b7ner", "U\u00b7nsterb\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "O kehre du Entz\u00fccken! du st\u00e4rkest mich!", "tokens": ["O", "keh\u00b7re", "du", "Ent\u00b7z\u00fc\u00b7cken", "!", "du", "st\u00e4r\u00b7kest", "mich", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "NN", "$.", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da\u00df ich nicht sinke, in dem Graun der", "tokens": ["Da\u00df", "ich", "nicht", "sin\u00b7ke", ",", "in", "dem", "Graun", "der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "$,", "APPR", "ART", "NN", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gro\u00dfen Vernichtungen nicht versinke.", "tokens": ["Gro\u00b7\u00dfen", "Ver\u00b7nich\u00b7tun\u00b7gen", "nicht", "ver\u00b7sin\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.20": {"line.1": {"text": "Wenn all dies anhebt \u2013 f\u00fchle dich ganz, o Mensch!", "tokens": ["Wenn", "all", "dies", "an\u00b7hebt", "\u2013", "f\u00fch\u00b7le", "dich", "ganz", ",", "o", "Mensch", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "PDS", "VVFIN", "$(", "VVFIN", "PPER", "ADV", "$,", "FM", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da wirst du jauchzen: Wo ist dein Stachel, Tod?", "tokens": ["Da", "wirst", "du", "jauch\u00b7zen", ":", "Wo", "ist", "dein", "Sta\u00b7chel", ",", "Tod", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVINF", "$.", "PWAV", "VAFIN", "PPOSAT", "NN", "$,", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dann ewig ist sie \u2013 t\u00f6nt es nach, ihr", "tokens": ["Dann", "e\u00b7wig", "ist", "sie", "\u2013", "t\u00f6nt", "es", "nach", ",", "ihr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "$(", "VVFIN", "PPER", "PTKVZ", "$,", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Harfen des Himmels, des Menschen Seele.", "tokens": ["Har\u00b7fen", "des", "Him\u00b7mels", ",", "des", "Men\u00b7schen", "See\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.21": {"line.1": {"text": "O Seele! jetzt schon bist du so wundervoll!", "tokens": ["O", "See\u00b7le", "!", "jetzt", "schon", "bist", "du", "so", "wun\u00b7der\u00b7voll", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ADV", "ADV", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wer denkt dich aus? da\u00df, wann du zu Gott dich nahst,", "tokens": ["Wer", "denkt", "dich", "aus", "?", "da\u00df", ",", "wann", "du", "zu", "Gott", "dich", "nahst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PTKVZ", "$.", "KOUS", "$,", "PWAV", "PPER", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Erhabne, mir im Auge blinket", "tokens": ["Er\u00b7hab\u00b7ne", ",", "mir", "im", "Au\u00b7ge", "blin\u00b7ket"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Deine Erhabenheit \u2013 da\u00df du, Seele!", "tokens": ["Dei\u00b7ne", "Er\u00b7ha\u00b7ben\u00b7heit", "\u2013", "da\u00df", "du", ",", "See\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "KOUS", "PPER", "$,", "NN", "$."], "meter": "+--+-++-+-", "measure": "iambic.penta.invert"}}, "stanza.22": {"line.1": {"text": "Wann auf die Flur das irdische Auge blickt,", "tokens": ["Wann", "auf", "die", "Flur", "das", "ir\u00b7di\u00b7sche", "Au\u00b7ge", "blickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "So s\u00fc\u00df, so himmlisch dann dich in mir erhebst \u2013", "tokens": ["So", "s\u00fc\u00df", ",", "so", "himm\u00b7lisch", "dann", "dich", "in", "mir", "er\u00b7hebst", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "ADV", "PPER", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wer sah, was Geist an K\u00f6rper bindt, wer", "tokens": ["Wer", "sah", ",", "was", "Geist", "an", "K\u00f6r\u00b7per", "bindt", ",", "wer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWS", "VVFIN", "$,", "PWS", "NN", "APPR", "NN", "VVFIN", "$,", "PWS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Lauschte die Sprache der Seele mit den", "tokens": ["Lauschte", "die", "Spra\u00b7che", "der", "See\u00b7le", "mit", "den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "APPR", "ART"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.23": {"line.1": {"text": "Verwesungen? \u2013 O Seele, schon jetzt bist du", "tokens": ["Ver\u00b7we\u00b7sun\u00b7gen", "?", "\u2013", "O", "See\u00b7le", ",", "schon", "jetzt", "bist", "du"], "token_info": ["word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "$(", "NE", "NN", "$,", "ADV", "ADV", "VAFIN", "PPER"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "So gro\u00df, so himmlisch, wann du von Erdentand", "tokens": ["So", "gro\u00df", ",", "so", "himm\u00b7lisch", ",", "wann", "du", "von", "Er\u00b7den\u00b7tand"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "PWAV", "PPER", "APPR", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und Menschendruck entlediget in", "tokens": ["Und", "Men\u00b7schen\u00b7druck", "ent\u00b7le\u00b7di\u00b7get", "in"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "APPR"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Gro\u00dfen Momenten zu deinem Urstoff", "tokens": ["Gro\u00b7\u00dfen", "Mo\u00b7men\u00b7ten", "zu", "dei\u00b7nem", "Ur\u00b7stoff"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-----+-+-", "measure": "dactylic.init"}}, "stanza.24": {"line.1": {"text": "Empor dich schwingst. Wie Schimmer Eloas Haupt", "tokens": ["Em\u00b7por", "dich", "schwingst", ".", "Wie", "Schim\u00b7mer", "E\u00b7loas", "Haupt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "VVFIN", "$.", "PWAV", "NN", "NE", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Umschwebt der Umkreis deiner Gedank