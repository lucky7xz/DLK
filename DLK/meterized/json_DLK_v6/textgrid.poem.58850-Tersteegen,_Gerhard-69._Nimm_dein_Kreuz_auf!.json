{"textgrid.poem.58850": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "69. Nimm dein Kreuz auf!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was dir zuwider wird getan", "tokens": ["Was", "dir", "zu\u00b7wi\u00b7der", "wird", "ge\u00b7tan"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADJD", "VAFIN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und was du sonst f\u00fcr Last zu tragen,", "tokens": ["Und", "was", "du", "sonst", "f\u00fcr", "Last", "zu", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nimm als ein Kreuz des Herren an", "tokens": ["Nimm", "als", "ein", "Kreuz", "des", "Her\u00b7ren", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "ART", "NN", "ART", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In stiller Sanftmut ohne Klagen!", "tokens": ["In", "stil\u00b7ler", "Sanft\u00b7mut", "oh\u00b7ne", "Kla\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}