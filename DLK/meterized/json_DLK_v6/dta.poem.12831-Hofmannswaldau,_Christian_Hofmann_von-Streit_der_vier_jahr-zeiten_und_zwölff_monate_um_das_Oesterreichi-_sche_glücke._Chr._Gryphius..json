{"dta.poem.12831": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Streit der vier jahr-zeiten und  \n zw\u00f6lff monate um das Oesterreichi-  \n sche gl\u00fccke.  \n Chr. Gryphius.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.71", "af:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es wird noch wol die sonne kalt,", "tokens": ["Es", "wird", "noch", "wol", "die", "son\u00b7ne", "kalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der himmel krumm und ungestalt,", "tokens": ["Der", "him\u00b7mel", "krumm", "und", "un\u00b7ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der wermuth s\u00fc\u00dfer wein,", "tokens": ["Der", "wer\u00b7muth", "s\u00fc\u00b7\u00dfer", "wein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die schatten lichter seyn.", "tokens": ["Die", "schat\u00b7ten", "lich\u00b7ter", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es wird sich alles so verdrehen,", "tokens": ["Es", "wird", "sich", "al\u00b7les", "so", "ver\u00b7dre\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "PIS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df wir zuletzt noch auf den k\u00f6pffen stehen.", "tokens": ["Da\u00df", "wir", "zu\u00b7letzt", "noch", "auf", "den", "k\u00f6pf\u00b7fen", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}