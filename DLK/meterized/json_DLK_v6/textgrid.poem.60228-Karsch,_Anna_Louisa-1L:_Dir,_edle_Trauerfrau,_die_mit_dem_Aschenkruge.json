{"textgrid.poem.60228": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dir, edle Trauerfrau, die mit dem Aschenkruge", "genre": "verse", "period": "N.A.", "pub_year": 1756, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dir, edle Trauerfrau, die mit dem Aschenkruge", "tokens": ["Dir", ",", "ed\u00b7le", "Trau\u00b7er\u00b7frau", ",", "die", "mit", "dem", "A\u00b7schen\u00b7kru\u00b7ge"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr turtelt\u00e4ubisch Herz verm\u00e4hlt,", "tokens": ["Ihr", "tur\u00b7tel\u00b7t\u00e4u\u00b7bisch", "Herz", "ver\u00b7m\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sich nach Sievers Sternenfluge", "tokens": ["Und", "sich", "nach", "Sie\u00b7vers", "Ster\u00b7nen\u00b7flu\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PRF", "APPR", "NE", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nie einen Busenfreund erw\u00e4hlt;", "tokens": ["Nie", "ei\u00b7nen", "Bu\u00b7sen\u00b7freund", "er\u00b7w\u00e4hlt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dir, Freundinn meines Herzens und Gesanges,", "tokens": ["Dir", ",", "Freun\u00b7dinn", "mei\u00b7nes", "Her\u00b7zens", "und", "Ge\u00b7san\u00b7ges", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Deinen Lippen oft ein L\u00e4cheln abgewann,", "tokens": ["Der", "Dei\u00b7nen", "Lip\u00b7pen", "oft", "ein", "L\u00e4\u00b7cheln", "ab\u00b7ge\u00b7wann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mit einer Art des s\u00fc\u00dfen Zwanges,", "tokens": ["Mit", "ei\u00b7ner", "Art", "des", "s\u00fc\u00b7\u00dfen", "Zwan\u00b7ges", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn Deine Blicke finster sahn,", "tokens": ["Wenn", "Dei\u00b7ne", "Bli\u00b7cke", "fins\u00b7ter", "sahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Voll Schwermuth deiner bangen Seele;", "tokens": ["Voll", "Schwer\u00b7muth", "dei\u00b7ner", "ban\u00b7gen", "See\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Dir soll das Schicksal der Cybele,", "tokens": ["Dir", "soll", "das", "Schick\u00b7sal", "der", "Cy\u00b7be\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+--", "measure": "dactylic.tri"}, "line.11": {"text": "Ihr Klagen und des Amors Hohn,", "tokens": ["Ihr", "Kla\u00b7gen", "und", "des", "A\u00b7mors", "Hohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "ART", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Den Mund zu lauten Lachen zwingen;", "tokens": ["Den", "Mund", "zu", "lau\u00b7ten", "La\u00b7chen", "zwin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Eh' noch des Liedes Warnungston", "tokens": ["Eh'", "noch", "des", "Lie\u00b7des", "War\u00b7nungs\u00b7ton"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "In andrer Wittwen Ohr wird dringen:", "tokens": ["In", "an\u00b7drer", "Witt\u00b7wen", "Ohr", "wird", "drin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Dich warnt die Muse nicht; der schlaue Cypripor", "tokens": ["Dich", "warnt", "die", "Mu\u00b7se", "nicht", ";", "der", "schlau\u00b7e", "Cyp\u00b7ri\u00b7por"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Erfuhr es schon in Jahren deiner Jugend,", "tokens": ["Er\u00b7fuhr", "es", "schon", "in", "Jah\u00b7ren", "dei\u00b7ner", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Da\u00df er an Dir die Macht verlor,", "tokens": ["Da\u00df", "er", "an", "Dir", "die", "Macht", "ver\u00b7lor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Und f\u00fcr der Thorheit Pfeil bewahrt Dich Deine Tugend.", "tokens": ["Und", "f\u00fcr", "der", "Thor\u00b7heit", "Pfeil", "be\u00b7wahrt", "Dich", "Dei\u00b7ne", "Tu\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Dir leg' ich den Gesang nur vor,", "tokens": ["Dir", "leg'", "ich", "den", "Ge\u00b7sang", "nur", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Nebst wenig andern kleinen Liedern,", "tokens": ["Nebst", "we\u00b7nig", "an\u00b7dern", "klei\u00b7nen", "Lie\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Um Deine Freundschaft, Deine warme Z\u00e4rtlichkeit", "tokens": ["Um", "Dei\u00b7ne", "Freund\u00b7schaft", ",", "Dei\u00b7ne", "war\u00b7me", "Z\u00e4rt\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Durch einen Funken zu erwiedern", "tokens": ["Durch", "ei\u00b7nen", "Fun\u00b7ken", "zu", "er\u00b7wie\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Mit \u00f6ffentlicher Dankbarkeit.", "tokens": ["Mit", "\u00f6f\u00b7fent\u00b7li\u00b7cher", "Dank\u00b7bar\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}