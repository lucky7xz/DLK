{"textgrid.poem.41683": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf der Menschheit Haupt hat im Hohne", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf der Menschheit Haupt hat im Hohne", "tokens": ["Auf", "der", "Menschheit", "Haupt", "hat", "im", "Hoh\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "VAFIN", "APPRART", "NN"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Sich Amor gesetzt,", "tokens": ["Sich", "A\u00b7mor", "ge\u00b7setzt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "NE", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und der Freche, der auf seinem Throne", "tokens": ["Und", "der", "Fre\u00b7che", ",", "der", "auf", "sei\u00b7nem", "Thro\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sich lachend ergetzt,", "tokens": ["Sich", "la\u00b7chend", "er\u00b7getzt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "L\u00e4\u00dft schillernde Kugeln steigen", "tokens": ["L\u00e4\u00dft", "schil\u00b7lern\u00b7de", "Ku\u00b7geln", "stei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJA", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Hinauf in die Luft,", "tokens": ["Hin\u00b7auf", "in", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Zu erreichen der Welten Reigen", "tokens": ["Zu", "er\u00b7rei\u00b7chen", "der", "Wel\u00b7ten", "Rei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "ART", "NN", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Im blauenden Duft,", "tokens": ["Im", "blau\u00b7en\u00b7den", "Duft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Der Lichtball schwebt, sich beschwingend,", "tokens": ["Der", "Licht\u00b7ball", "schwebt", ",", "sich", "be\u00b7schwin\u00b7gend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRF", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "In endlosen Raum,", "tokens": ["In", "end\u00b7lo\u00b7sen", "Raum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Birst und haucht seine Seele verklingend,", "tokens": ["Birst", "und", "haucht", "sei\u00b7ne", "See\u00b7le", "ver\u00b7klin\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "VVFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Wie goldenen Traum,", "tokens": ["Wie", "gol\u00b7de\u00b7nen", "Traum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Nun h\u00f6r bei den schwebenden Blasen", "tokens": ["Nun", "h\u00f6r", "bei", "den", "schwe\u00b7ben\u00b7den", "Bla\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Den Sch\u00e4del ich flehn:", "tokens": ["Den", "Sch\u00e4\u00b7del", "ich", "flehn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Dieses Spieles grausames Rasen,", "tokens": ["Die\u00b7ses", "Spie\u00b7les", "grau\u00b7sa\u00b7mes", "Ra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Wie lang soll es gehn?", "tokens": ["Wie", "lang", "soll", "es", "gehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Das, was dein Mund, dein verruchter,", "tokens": ["Das", ",", "was", "dein", "Mund", ",", "dein", "ver\u00b7ruch\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Im Spiele vertut,", "tokens": ["Im", "Spie\u00b7le", "ver\u00b7tut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Mein Hirn ist's, M\u00f6rder, verfluchter!", "tokens": ["Mein", "Hirn", "ist's", ",", "M\u00f6r\u00b7der", ",", "ver\u00b7fluch\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "$,", "NN", "$,", "ADJA", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mein Fleisch und mein Blut!", "tokens": ["Mein", "Fleisch", "und", "mein", "Blut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}