{"dta.poem.20678": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Loblied  \n bei                  \n  dem f\u00fcnf und sechzigsten Jahresschlu\u00df.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich mit Lob und Dank mein Auge r\u00fcckw\u00e4rts hin                 ", "tokens": ["Wenn", "ich", "mit", "Lob", "und", "Dank", "mein", "Au\u00b7ge", "r\u00fcck\u00b7w\u00e4rts", "hin"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "KON", "APPR", "PPOSAT", "NN", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bis auf den ersten Schritt des Erdenwandels lenke,", "tokens": ["Bis", "auf", "den", "ers\u00b7ten", "Schritt", "des", "Er\u00b7den\u00b7wan\u00b7dels", "len\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dann staun\u2019 ich, da\u00df ich bin,", "tokens": ["Dann", "staun'", "ich", ",", "da\u00df", "ich", "bin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df ich noch weiter fort zu wandeln hoff und denke.", "tokens": ["Da\u00df", "ich", "noch", "wei\u00b7ter", "fort", "zu", "wan\u00b7deln", "hoff", "und", "den\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "PTKVZ", "PTKZU", "VVINF", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Tr\u00fcbsal ward mir viel gewogen von dem Herrn,", "tokens": ["Der", "Tr\u00fcb\u00b7sal", "ward", "mir", "viel", "ge\u00b7wo\u00b7gen", "von", "dem", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und seine Weisheit wirft noch Kummer in die Wage,", "tokens": ["Und", "sei\u00b7ne", "Weis\u00b7heit", "wirft", "noch", "Kum\u00b7mer", "in", "die", "Wa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und dennoch z\u00e4hl\u2019 ich gern", "tokens": ["Und", "den\u00b7noch", "z\u00e4hl'", "ich", "gern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Bei grau gewordnem Haupt des m\u00fcden Alters Tage.", "tokens": ["Bei", "grau", "ge\u00b7word\u00b7nem", "Haupt", "des", "m\u00fc\u00b7den", "Al\u00b7ters", "Ta\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Denn Freundschaftss\u00fc\u00dfigkeit legt seine Vaterhuld", "tokens": ["Denn", "Freund\u00b7schafts\u00b7s\u00fc\u00b7\u00dfig\u00b7keit", "legt", "sei\u00b7ne", "Va\u00b7ter\u00b7huld"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit jedem Tage mir vollwichtig in die Schaale,", "tokens": ["Mit", "je\u00b7dem", "Ta\u00b7ge", "mir", "voll\u00b7wich\u00b7tig", "in", "die", "Schaa\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und giebt mir viel Geduld,", "tokens": ["Und", "giebt", "mir", "viel", "Ge\u00b7duld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und so wird mir die Welt zu keinem Distelthale.", "tokens": ["Und", "so", "wird", "mir", "die", "Welt", "zu", "kei\u00b7nem", "Dis\u00b7tel\u00b7tha\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ART", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-++--+-+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Ein jeder Seelengru\u00df von fernher oder nah,", "tokens": ["Ein", "je\u00b7der", "See\u00b7len\u00b7gru\u00df", "von", "fern\u00b7her", "o\u00b7der", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "APPR", "ADJA", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist eine Rose mir, die Laufbahn zu versch\u00f6nern,", "tokens": ["Ist", "ei\u00b7ne", "Ro\u00b7se", "mir", ",", "die", "Lauf\u00b7bahn", "zu", "ver\u00b7sch\u00f6\u00b7nern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bis ich, Halleluja!", "tokens": ["Bis", "ich", ",", "Hal\u00b7le\u00b7lu\u00b7ja", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ITJ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Dort oben sing im Chor mit Gottes Jubelt\u00f6nern.", "tokens": ["Dort", "o\u00b7ben", "sing", "im", "Chor", "mit", "Got\u00b7tes", "Ju\u00b7bel\u00b7t\u00f6\u00b7nern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "APPRART", "NN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}