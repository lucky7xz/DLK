{"textgrid.poem.53953": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Gl\u00fcck im Ungl\u00fcck", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin kein Mann, nach dem man in den Kissen schluchzt", "tokens": ["Ich", "bin", "kein", "Mann", ",", "nach", "dem", "man", "in", "den", "Kis\u00b7sen", "schluchzt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,", "APPR", "ART", "PIS", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ich wei\u00df es wohl;", "tokens": ["ich", "wei\u00df", "es", "wohl", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "da n\u00fctzt kein \u00d6dipus-Komplex, kein Fluchts\u2013", "tokens": ["da", "n\u00fctzt", "kein", "\u00d6\u00b7di\u00b7pus\u00b7Kom\u00b7plex", ",", "kein", "Fluchts", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "versuch in das Symbol.", "tokens": ["ver\u00b7such", "in", "das", "Sym\u00b7bol", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Seit Jahren sagen alle Frauen,", "tokens": ["Seit", "Jah\u00b7ren", "sa\u00b7gen", "al\u00b7le", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "wenn sie mir in die Augen schauen,", "tokens": ["wenn", "sie", "mir", "in", "die", "Au\u00b7gen", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "sie sagen, seit ich majorenn:", "tokens": ["sie", "sa\u00b7gen", ",", "seit", "ich", "ma\u00b7jo\u00b7renn", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "\u00bbsch\u00f6n bist du nicht \u2013 klug bist du nicht \u2013", "tokens": ["\u00bb", "sch\u00f6n", "bist", "du", "nicht", "\u2013", "klug", "bist", "du", "nicht", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VAFIN", "PPER", "PTKNEG", "$(", "ADJD", "VAFIN", "PPER", "PTKNEG", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "reich bist du nicht \u2013 lieb bist du nicht \u2013", "tokens": ["reich", "bist", "du", "nicht", "\u2013", "lieb", "bist", "du", "nicht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "PTKNEG", "$(", "ADJD", "VAFIN", "PPER", "PTKNEG", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "was bist du denn?\u00ab", "tokens": ["was", "bist", "du", "denn", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Das kr\u00e4nkte auf die Dauer jeden Mann", "tokens": ["Das", "kr\u00e4nk\u00b7te", "auf", "die", "Dau\u00b7er", "je\u00b7den", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "des Okzidents,", "tokens": ["des", "Ok\u00b7zi\u00b7dents", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Was folgt daraus? Ich zieh schon lange an", "tokens": ["Was", "folgt", "da\u00b7raus", "?", "Ich", "zieh", "schon", "lan\u00b7ge", "an"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PAV", "$.", "PPER", "VVFIN", "ADV", "ADV", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "der Konsequenz.", "tokens": ["der", "Kon\u00b7se\u00b7quenz", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Man mu\u00df nur sehn, mit wem ichs treibe:", "tokens": ["Man", "mu\u00df", "nur", "sehn", ",", "mit", "wem", "ichs", "trei\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADV", "VVINF", "$,", "APPR", "PWS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "an Geist vermiekert, Fett am Leibe \u2013", "tokens": ["an", "Geist", "ver\u00b7mie\u00b7kert", ",", "Fett", "am", "Lei\u00b7be", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "ich frage mich verd\u00fcstert, wenn . . . :", "tokens": ["ich", "fra\u00b7ge", "mich", "ver\u00b7d\u00fcs\u00b7tert", ",", "wenn", ".", ".", ".", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVPP", "$,", "KOUS", "$.", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "\u00bbsch\u00f6n sind sie nicht \u2013 reich sind sie nicht \u2013", "tokens": ["\u00bb", "sch\u00f6n", "sind", "sie", "nicht", "\u2013", "reich", "sind", "sie", "nicht", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VAFIN", "PPER", "PTKNEG", "$(", "ADJD", "VAFIN", "PPER", "PTKNEG", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "klug sind sie nicht \u2013 lieb sind sie nicht \u2013", "tokens": ["klug", "sind", "sie", "nicht", "\u2013", "lieb", "sind", "sie", "nicht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "PTKNEG", "$(", "ADJD", "VAFIN", "PPER", "PTKNEG", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.10": {"text": "was sind sie denn?\u00ab", "tokens": ["was", "sind", "sie", "denn", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Man hat mich d\u00fcnn wie Makkaroni,", "tokens": ["Man", "hat", "mich", "d\u00fcnn", "wie", "Mak\u00b7ka\u00b7ro\u00b7ni", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADJD", "KOKOM", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "den man in Mailand zieht.", "tokens": ["den", "man", "in", "Mai\u00b7land", "zieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Ich bin ja schlie\u00dflich kein Adoni \u2013", "tokens": ["Ich", "bin", "ja", "schlie\u00df\u00b7lich", "kein", "A\u00b7do\u00b7ni", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PIAT", "NE", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "wie heb ich meinen Sexualkredit?", "tokens": ["wie", "heb", "ich", "mei\u00b7nen", "Se\u00b7xu\u00b7al\u00b7kre\u00b7dit", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Ich seh mir die an, wo uns so regieren.", "tokens": ["Ich", "seh", "mir", "die", "an", ",", "wo", "uns", "so", "re\u00b7gie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "PTKVZ", "$,", "PWAV", "PPER", "ADV", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.16": {"text": "Da darf man wohl die Frage formulieren,", "tokens": ["Da", "darf", "man", "wohl", "die", "Fra\u00b7ge", "for\u00b7mu\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "betrachtet man die Gentlemen:", "tokens": ["be\u00b7trach\u00b7tet", "man", "die", "Gent\u00b7le\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "\u00bbsch\u00f6n sind sie nicht \u2013 klug sind sie nicht \u2013", "tokens": ["\u00bb", "sch\u00f6n", "sind", "sie", "nicht", "\u2013", "klug", "sind", "sie", "nicht", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VAFIN", "PPER", "PTKNEG", "$(", "ADJD", "VAFIN", "PPER", "PTKNEG", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "lieb sind sie nicht \u2013 'tellijent sind sie nicht \u2013", "tokens": ["lieb", "sind", "sie", "nicht", "\u2013", "'tel\u00b7li\u00b7jent", "sind", "sie", "nicht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "PTKNEG", "$(", "ADJD", "VAFIN", "PPER", "PTKNEG", "$("], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.20": {"text": "was sind sie denn \u2013", "tokens": ["was", "sind", "sie", "denn", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.21": {"text": "Ja, was sind sie denn \u2013?\u00ab", "tokens": ["Ja", ",", "was", "sind", "sie", "denn", "\u2013", "?", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PTKANT", "$,", "PWS", "VAFIN", "PPER", "ADV", "$(", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Schlau. Im Skatverein. Und immer vorhanden.", "tokens": ["Schlau", ".", "Im", "Skat\u00b7ver\u00b7ein", ".", "Und", "im\u00b7mer", "vor\u00b7han\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "APPRART", "NN", "$.", "KON", "ADV", "ADJD", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Das bef\u00e4higt zur F\u00fchrung in deutschen Landen.", "tokens": ["Das", "be\u00b7f\u00e4\u00b7higt", "zur", "F\u00fch\u00b7rung", "in", "deut\u00b7schen", "Lan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}}}}}