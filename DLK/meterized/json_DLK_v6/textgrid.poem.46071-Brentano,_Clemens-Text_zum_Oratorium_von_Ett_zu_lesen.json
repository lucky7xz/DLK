{"textgrid.poem.46071": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Text zum Oratorium von Ett zu lesen", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie haben allerlei gesungen,", "tokens": ["Sie", "ha\u00b7ben", "al\u00b7ler\u00b7lei", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und alles war ein einzig Lied,", "tokens": ["Und", "al\u00b7les", "war", "ein", "ein\u00b7zig", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vom Zauberknoten s\u00fc\u00df verschlungen", "tokens": ["Vom", "Zau\u00b7ber\u00b7kno\u00b7ten", "s\u00fc\u00df", "ver\u00b7schlun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aus Huld und Reiz von Glied zu Glied.", "tokens": ["Aus", "Huld", "und", "Reiz", "von", "Glied", "zu", "Glied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Von allem hab' ich nichts geh\u00f6ret,", "tokens": ["Von", "al\u00b7lem", "hab'", "ich", "nichts", "ge\u00b7h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VAFIN", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als deines Kinderherzens Schlag,", "tokens": ["Als", "dei\u00b7nes", "Kin\u00b7der\u00b7her\u00b7zens", "Schlag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An dem von T\u00f6nen ungest\u00f6ret", "tokens": ["An", "dem", "von", "T\u00f6\u00b7nen", "un\u00b7ge\u00b7st\u00f6\u00b7ret"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "APPR", "NN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "S\u00fc\u00df tr\u00e4umend meine Seele lag.", "tokens": ["S\u00fc\u00df", "tr\u00e4u\u00b7mend", "mei\u00b7ne", "See\u00b7le", "lag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich h\u00f6rte nur von Mirten s\u00e4uselnd,", "tokens": ["Ich", "h\u00f6r\u00b7te", "nur", "von", "Mir\u00b7ten", "s\u00e4u\u00b7selnd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von Lilien, die mir zugenickt,", "tokens": ["Von", "Li\u00b7li\u00b7en", ",", "die", "mir", "zu\u00b7ge\u00b7nickt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+----+-+", "measure": "dactylic.init"}, "line.3": {"text": "Von W\u00f6lkchen um den Mond hinkr\u00e4uselnd,", "tokens": ["Von", "W\u00f6lk\u00b7chen", "um", "den", "Mond", "hin\u00b7kr\u00e4u\u00b7selnd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von Sternen, die mich angeblickt.", "tokens": ["Von", "Ster\u00b7nen", ",", "die", "mich", "an\u00b7ge\u00b7blickt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich h\u00f6rte nur: s\u00fc\u00df ist die Linde,", "tokens": ["Ich", "h\u00f6r\u00b7te", "nur", ":", "s\u00fc\u00df", "ist", "die", "Lin\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$.", "ADJD", "VAFIN", "ART", "NE", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Schlank ist das Reh, blank ist der Fisch,", "tokens": ["Schlank", "ist", "das", "Reh", ",", "blank", "ist", "der", "Fisch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Seelchen gaukelt in dem Kinde,", "tokens": ["Das", "Seel\u00b7chen", "gau\u00b7kelt", "in", "dem", "Kin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Nympfchen in dem Waldquell frisch.", "tokens": ["Ein", "Nympf\u00b7chen", "in", "dem", "Wald\u00b7quell", "frisch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Was s\u00fc\u00df sich in den T\u00f6nen wieget", "tokens": ["Was", "s\u00fc\u00df", "sich", "in", "den", "T\u00f6\u00b7nen", "wie\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PRF", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was sehnet, seufzet, ringt und schwingt,", "tokens": ["Was", "seh\u00b7net", ",", "seuf\u00b7zet", ",", "ringt", "und", "schwingt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist all s\u00fc\u00df Lindi, die sich schmieget,", "tokens": ["Ist", "all", "s\u00fc\u00df", "Lin\u00b7di", ",", "die", "sich", "schmie\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ADJD", "NE", "$,", "PRELS", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn sie der Augenblick umschlingt.", "tokens": ["Wenn", "sie", "der", "Au\u00b7gen\u00b7blick", "um\u00b7schlingt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Es weben all die Wundert\u00f6ne", "tokens": ["Es", "we\u00b7ben", "all", "die", "Wun\u00b7der\u00b7t\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nur einen einzigen Akkord,", "tokens": ["Nur", "ei\u00b7nen", "ein\u00b7zi\u00b7gen", "Ak\u00b7kord", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "S\u00fc\u00df ist s\u00fc\u00df Lieb, sie ist das sch\u00f6ne,", "tokens": ["S\u00fc\u00df", "ist", "s\u00fc\u00df", "Lieb", ",", "sie", "ist", "das", "sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "NN", "$,", "PPER", "VAFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das linde, liebe, wahre Wort.", "tokens": ["Das", "lin\u00b7de", ",", "lie\u00b7be", ",", "wah\u00b7re", "Wort", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "In ihr wird jeder Mangel Zierde", "tokens": ["In", "ihr", "wird", "je\u00b7der", "Man\u00b7gel", "Zier\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VAFIN", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und jede Armut \u00dcberflu\u00df,", "tokens": ["Und", "je\u00b7de", "Ar\u00b7mut", "\u00dc\u00b7berf\u00b7lu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Kinderseelchen der Begierde", "tokens": ["Ein", "Kin\u00b7der\u00b7seel\u00b7chen", "der", "Be\u00b7gier\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schwebt leis in ihres Odems Ku\u00df.", "tokens": ["Schwebt", "leis", "in", "ih\u00b7res", "O\u00b7dems", "Ku\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Wie lieblich war es heut zu schauen", "tokens": ["Wie", "lieb\u00b7lich", "war", "es", "heut", "zu", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das reine feine Wunderbild,", "tokens": ["Das", "rei\u00b7ne", "fei\u00b7ne", "Wun\u00b7der\u00b7bild", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So schwebt die Elfe durch die Auen", "tokens": ["So", "schwebt", "die", "El\u00b7fe", "durch", "die", "Au\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und tr\u00e4gt ein Rosenblatt als Schild.", "tokens": ["Und", "tr\u00e4gt", "ein", "Ro\u00b7sen\u00b7blatt", "als", "Schild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "KOUS", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Wer hat so s\u00fc\u00df sie ausger\u00fcstet", "tokens": ["Wer", "hat", "so", "s\u00fc\u00df", "sie", "aus\u00b7ge\u00b7r\u00fcs\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ADV", "ADJD", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Ambra, Perl und Elfenbein,", "tokens": ["Wie", "A\u00b7mbra", ",", "Perl", "und", "El\u00b7fen\u00b7bein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "$,", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer hat ihr Herz so fein gebr\u00fcstet", "tokens": ["Wer", "hat", "ihr", "Herz", "so", "fein", "ge\u00b7br\u00fcs\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Wiegenbett der Engelein.", "tokens": ["Ein", "Wie\u00b7gen\u00b7bett", "der", "En\u00b7ge\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Wer schwang so rein das schlanke H\u00fcftchen,", "tokens": ["Wer", "schwang", "so", "rein", "das", "schlan\u00b7ke", "H\u00fcft\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wer zog die Anmut bis zum Fu\u00df,", "tokens": ["Wer", "zog", "die", "An\u00b7mut", "bis", "zum", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer tr\u00e4gt sie wie auf Fr\u00fchlingsl\u00fcftchen", "tokens": ["Wer", "tr\u00e4gt", "sie", "wie", "auf", "Fr\u00fch\u00b7lings\u00b7l\u00fcft\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "KOKOM", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Sehnsucht tr\u00e4gt der Liebe Gru\u00df?", "tokens": ["Die", "Sehn\u00b7sucht", "tr\u00e4gt", "der", "Lie\u00b7be", "Gru\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Wer wieget ihr das kluge K\u00f6pfchen", "tokens": ["Wer", "wie\u00b7get", "ihr", "das", "klu\u00b7ge", "K\u00f6pf\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gleich Blumen an der Quelle Saum,", "tokens": ["Gleich", "Blu\u00b7men", "an", "der", "Quel\u00b7le", "Saum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer flocht ihr in die schwarzen Z\u00f6pfchen", "tokens": ["Wer", "flocht", "ihr", "in", "die", "schwar\u00b7zen", "Z\u00f6pf\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der leichten linden Kinder Traum?", "tokens": ["Der", "leich\u00b7ten", "lin\u00b7den", "Kin\u00b7der", "Traum", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Wer hat dies holde Kind geschm\u00fccket,", "tokens": ["Wer", "hat", "dies", "hol\u00b7de", "Kind", "ge\u00b7schm\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PDS", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wer hat zu ihm sich hingeb\u00fcckt,", "tokens": ["Wer", "hat", "zu", "ihm", "sich", "hin\u00b7ge\u00b7b\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer hat es an sein Herz gedr\u00fccket?", "tokens": ["Wer", "hat", "es", "an", "sein", "Herz", "ge\u00b7dr\u00fc\u00b7cket", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der s\u00fc\u00dfe Gott, der mich entz\u00fcckt.", "tokens": ["Der", "s\u00fc\u00b7\u00dfe", "Gott", ",", "der", "mich", "ent\u00b7z\u00fcckt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}