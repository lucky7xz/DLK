{"textgrid.poem.25938": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Geh' ich an dem Bach entlang", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Bach, der rinnt wie helles Glas,", "tokens": ["Der", "Bach", ",", "der", "rinnt", "wie", "hel\u00b7les", "Glas", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als habe meines M\u00e4dchens Hand", "tokens": ["Als", "ha\u00b7be", "mei\u00b7nes", "M\u00e4d\u00b7chens", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Himmel, der im Herz ihr sa\u00df,", "tokens": ["Den", "Him\u00b7mel", ",", "der", "im", "Herz", "ihr", "sa\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ins Wiesengras hell ausgegossen,", "tokens": ["Ins", "Wie\u00b7sen\u00b7gras", "hell", "aus\u00b7ge\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und heimliche Gedanken schossen", "tokens": ["Und", "heim\u00b7li\u00b7che", "Ge\u00b7dan\u00b7ken", "schos\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Als Fischlein fort mit schlanken Flossen.", "tokens": ["Als", "Fisc\u00b7hlein", "fort", "mit", "schlan\u00b7ken", "Flos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PTKVZ", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Drum, geh' ich an dem Bach entlang,", "tokens": ["Drum", ",", "geh'", "ich", "an", "dem", "Bach", "ent\u00b7lang", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Blut stets Liebeslieder sang.", "tokens": ["Mein", "Blut", "stets", "Lie\u00b7bes\u00b7lie\u00b7der", "sang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es denkt bei jedem Schritt ans Minnen", "tokens": ["Es", "denkt", "bei", "je\u00b7dem", "Schritt", "ans", "Min\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und will wie's B\u00e4chlein mir entrinnen.", "tokens": ["Und", "will", "wie's", "B\u00e4ch\u00b7lein", "mir", "ent\u00b7rin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}