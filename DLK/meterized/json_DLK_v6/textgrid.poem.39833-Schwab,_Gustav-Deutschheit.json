{"textgrid.poem.39833": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Deutschheit", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie t\u00f6nen alle laut in mir zusammen", "tokens": ["Sie", "t\u00f6\u00b7nen", "al\u00b7le", "laut", "in", "mir", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "ADJD", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die reinen Hymnen vaterl\u00e4nd'scher Dichter;", "tokens": ["Die", "rei\u00b7nen", "Hym\u00b7nen", "va\u00b7ter\u00b7l\u00e4n\u00b7d'\u00b7scher", "Dich\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "In meinem deutschen Herzen wird es lichter:", "tokens": ["In", "mei\u00b7nem", "deut\u00b7schen", "Her\u00b7zen", "wird", "es", "lich\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nicht sch\u00e4m' ich mich, von solchem Volk zu stammen.", "tokens": ["Nicht", "sch\u00e4m'", "ich", "mich", ",", "von", "sol\u00b7chem", "Volk", "zu", "stam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "PRF", "$,", "APPR", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ob auch erloschen seines Mutes Flammen,", "tokens": ["Ob", "auch", "er\u00b7lo\u00b7schen", "sei\u00b7nes", "Mu\u00b7tes", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch immer aus geweihten S\u00e4ngen spricht er;", "tokens": ["Doch", "im\u00b7mer", "aus", "ge\u00b7weih\u00b7ten", "S\u00e4n\u00b7gen", "spricht", "er", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es h\u00e4lt der Kraft Ermunterer und Richter,", "tokens": ["Es", "h\u00e4lt", "der", "Kraft", "Er\u00b7mun\u00b7te\u00b7rer", "und", "Rich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Dichtung Geist, die Seelen noch beisammen.", "tokens": ["Der", "Dich\u00b7tung", "Geist", ",", "die", "See\u00b7len", "noch", "bei\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So schallet \u00fcber die gef\u00e4llten Eichen", "tokens": ["So", "schal\u00b7let", "\u00fc\u00b7ber", "die", "ge\u00b7f\u00e4ll\u00b7ten", "Ei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und \u00fcber des gest\u00fcrzten Haines Tr\u00fcmmer", "tokens": ["Und", "\u00fc\u00b7ber", "des", "ge\u00b7st\u00fcrz\u00b7ten", "Hai\u00b7nes", "Tr\u00fcm\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der V\u00f6gel lieblicher Gesang noch immer.", "tokens": ["Der", "V\u00f6\u00b7gel", "lieb\u00b7li\u00b7cher", "Ge\u00b7sang", "noch", "im\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sie singen ihre heil'gen Grabeslieder", "tokens": ["Sie", "sin\u00b7gen", "ih\u00b7re", "heil'\u00b7gen", "Gra\u00b7bes\u00b7lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf die gefallnen Riesenst\u00e4mme nieder,", "tokens": ["Auf", "die", "ge\u00b7fall\u00b7nen", "Rie\u00b7sen\u00b7st\u00e4m\u00b7me", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Wiegensang den neu aufbl\u00fchnden Zweigen.", "tokens": ["Und", "Wie\u00b7gen\u00b7sang", "den", "neu", "auf\u00b7bl\u00fchn\u00b7den", "Zwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}