{"dta.poem.5781": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Eigenschafften der  \n Materie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es werden ganze Str\u00f6m', aus unsichtbaren D\u00fcfften,", "tokens": ["Es", "wer\u00b7den", "gan\u00b7ze", "Str\u00f6m'", ",", "aus", "un\u00b7sicht\u00b7ba\u00b7ren", "D\u00fcff\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die mit beschaumten Stolz in weiten Ufern rennen.", "tokens": ["Die", "mit", "be\u00b7schaum\u00b7ten", "Stolz", "in", "wei\u00b7ten", "U\u00b7fern", "ren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Marmor wird formirt in seiner Br\u00fcche Kl\u00fcfften,", "tokens": ["Der", "Mar\u00b7mor", "wird", "for\u00b7mirt", "in", "sei\u00b7ner", "Br\u00fc\u00b7che", "Kl\u00fcff\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Aus Theilgen, die kein Auge finden k\u00f6nnen.", "tokens": ["Aus", "Theil\u00b7gen", ",", "die", "kein", "Au\u00b7ge", "fin\u00b7den", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PIAT", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Das schwereste Metall, das Silber und das Gold,", "tokens": ["Das", "schwe\u00b7res\u00b7te", "Me\u00b7tall", ",", "das", "Sil\u00b7ber", "und", "das", "Gold", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dem alle Geitzige mit solcher Sehnsucht hold,", "tokens": ["Dem", "al\u00b7le", "Geit\u00b7zi\u00b7ge", "mit", "sol\u00b7cher", "Sehn\u00b7sucht", "hold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "APPR", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Besteht aus C\u00f6rpern, die so klein,", "tokens": ["Be\u00b7steht", "aus", "C\u00f6r\u00b7pern", ",", "die", "so", "klein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "$,", "PRELS", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df sie nicht sicht-nicht f\u00fchlbar seyn.", "tokens": ["Da\u00df", "sie", "nicht", "sicht\u00b7nicht", "f\u00fchl\u00b7bar", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJD", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}