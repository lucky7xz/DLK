{"textgrid.poem.40683": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was im Blau die Lerche singt", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was im Blau die Lerche singt", "tokens": ["Was", "im", "Blau", "die", "Ler\u00b7che", "singt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPRART", "NN", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Das verfl\u00fcchtigt und verklingt;", "tokens": ["Das", "ver\u00b7fl\u00fcch\u00b7tigt", "und", "ver\u00b7klingt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVPP", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn sie h\u00fcpft im weichen Sand", "tokens": ["Wenn", "sie", "h\u00fcpft", "im", "wei\u00b7chen", "Sand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat die F\u00e4hrte mehr Bestand.", "tokens": ["Hat", "die", "F\u00e4hr\u00b7te", "mehr", "Be\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "H\u00e4ltst du drum die Spur der Pfoten", "tokens": ["H\u00e4ltst", "du", "drum", "die", "Spur", "der", "Pfo\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PAV", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Etwa gar f\u00fcr Himmelsnoten?", "tokens": ["Et\u00b7wa", "gar", "f\u00fcr", "Him\u00b7mels\u00b7no\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}