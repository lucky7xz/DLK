{"dta.poem.4218": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vergn\u00fcgliche  \n  Betrachtungen \u00fcber B\u00e4ume  \n im Fr\u00fchling.  \n An den  \n  ber\u00fchmten   S. T.   Herrn Hof-Raht  \n Drollinger.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von der erst j\u00fcngst begr\u00fcnten Erden, im Fr\u00fchling gleich-", "tokens": ["Von", "der", "erst", "j\u00fcngst", "be\u00b7gr\u00fcn\u00b7ten", "Er\u00b7den", ",", "im", "Fr\u00fch\u00b7ling", "gleich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "ADV", "ADJA", "NN", "$,", "APPRART", "NN", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "sam angelacht,", "tokens": ["sam", "an\u00b7ge\u00b7lacht", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Betrachtet\u2019 ich, vor andern, neulich, der sch\u00f6nen Erde", "tokens": ["Be\u00b7trach\u00b7tet'", "ich", ",", "vor", "an\u00b7dern", ",", "neu\u00b7lich", ",", "der", "sch\u00f6\u00b7nen", "Er\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "APPR", "PIS", "$,", "ADV", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "sch\u00f6nste Pracht,", "tokens": ["sch\u00f6ns\u00b7te", "Pracht", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Den Schmuck und Wuchs erhabner B\u00e4ume: Da ich denn,", "tokens": ["Den", "Schmuck", "und", "Wuchs", "er\u00b7hab\u00b7ner", "B\u00e4u\u00b7me", ":", "Da", "ich", "denn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ADJA", "NN", "$.", "KOUS", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "mit Vergn\u00fcgen, fand,", "tokens": ["mit", "Ver\u00b7gn\u00fc\u00b7gen", ",", "fand", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Da\u00df ihre zierliche Figur, zumahl der Wipfel R\u00fcnd\u2019, ent-", "tokens": ["Da\u00df", "ih\u00b7re", "zier\u00b7li\u00b7che", "Fi\u00b7gur", ",", "zu\u00b7mahl", "der", "Wip\u00b7fel", "R\u00fcnd'", ",", "ent"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "$,", "KOUS", "ART", "NN", "NN", "$,", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "stand", "tokens": ["stand"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.9": {"text": "Aus den noch nicht gesteiften Zweigen, die, von der dichten", "tokens": ["Aus", "den", "noch", "nicht", "ge\u00b7steif\u00b7ten", "Zwei\u00b7gen", ",", "die", ",", "von", "der", "dich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "PTKNEG", "ADJA", "NN", "$,", "PRELS", "$,", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Bl\u00e4tter Last,", "tokens": ["Bl\u00e4t\u00b7ter", "Last", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "Mit sanftem Druck herabgezogen,", "tokens": ["Mit", "sanf\u00b7tem", "Druck", "her\u00b7ab\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Sich Wolken-f\u00f6rmig abwerts bogen,", "tokens": ["Sich", "Wol\u00b7ken\u00b7f\u00f6r\u00b7mig", "ab\u00b7werts", "bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "NE", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Und, da sie den geraden Stamm zugleich an allen Enden", "tokens": ["Und", ",", "da", "sie", "den", "ge\u00b7ra\u00b7den", "Stamm", "zu\u00b7gleich", "an", "al\u00b7len", "En\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "PPER", "ART", "ADJA", "NN", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.14": {"text": "zierten,", "tokens": ["zier\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "Von innen zierliche Gew\u00f6lber, im halben Cirkel-Schlag,", "tokens": ["Von", "in\u00b7nen", "zier\u00b7li\u00b7che", "Ge\u00b7w\u00f6l\u00b7ber", ",", "im", "hal\u00b7ben", "Cir\u00b7kel\u00b7Schlag", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.16": {"text": "formierten.", "tokens": ["for\u00b7mier\u00b7ten", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Allein! es gehet ja kein Wind. Wie kommt es, da\u00df der", "tokens": ["Al\u00b7lein", "!", "es", "ge\u00b7het", "ja", "kein", "Wind", ".", "Wie", "kommt", "es", ",", "da\u00df", "der"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "$.", "PPER", "VVFIN", "ADV", "PIAT", "NN", "$.", "PWAV", "VVFIN", "PPER", "$,", "KOUS", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Baum sich regt?", "tokens": ["Baum", "sich", "regt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PRF", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Seht, wie so mancher schwanker Ast sich hier ersch\u00fcttert, dort", "tokens": ["Seht", ",", "wie", "so", "man\u00b7cher", "schwan\u00b7ker", "Ast", "sich", "hier", "er\u00b7sch\u00fct\u00b7tert", ",", "dort"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "$,", "PWAV", "ADV", "PIAT", "ADJA", "NN", "PRF", "ADV", "VVPP", "$,", "ADV"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "bewegt!", "tokens": ["be\u00b7wegt", "!"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.3": {"line.1": {"text": "Hier beugt sich einer, wenn sich dort ein andrer Zweig hin-", "tokens": ["Hier", "beugt", "sich", "ei\u00b7ner", ",", "wenn", "sich", "dort", "ein", "an\u00b7drer", "Zweig", "hin"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "$,", "KOUS", "PRF", "ADV", "ART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "gegen hebt.", "tokens": ["ge\u00b7gen", "hebt", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Wie geht die\u00df zu? Ist denn der Baum Bewegung f\u00e4hig, und", "tokens": ["Wie", "geht", "die\u00df", "zu", "?", "Ist", "denn", "der", "Baum", "Be\u00b7we\u00b7gung", "f\u00e4\u00b7hig", ",", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "VVFIN", "PDS", "PTKVZ", "$.", "VAFIN", "ADV", "ART", "NN", "NN", "ADJD", "$,", "KON"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "belebt?", "tokens": ["be\u00b7lebt", "?"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Ach nein. Der Klang, ein zwitschernd Gurgeln, von T\u00f6nen", "tokens": ["Ach", "nein", ".", "Der", "Klang", ",", "ein", "zwit\u00b7schernd", "Gur\u00b7geln", ",", "von", "T\u00f6\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PTKANT", "$.", "ART", "NN", "$,", "ART", "ADJD", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "ein gepre\u00dft Gedr\u00e4nge,", "tokens": ["ein", "ge\u00b7pre\u00dft", "Ge\u00b7dr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "VVPP", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Entdeckt die Quelle der Bewegung, verr\u00e4ht die Ursach:", "tokens": ["Ent\u00b7deckt", "die", "Quel\u00b7le", "der", "Be\u00b7we\u00b7gung", ",", "ver\u00b7r\u00e4ht", "die", "Ur\u00b7sach", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Eine Menge", "tokens": ["Ei\u00b7ne", "Men\u00b7ge"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Verliebter V\u00f6gel f\u00fcllt den Baum. Springt einer hier von", "tokens": ["Ver\u00b7lieb\u00b7ter", "V\u00f6\u00b7gel", "f\u00fcllt", "den", "Baum", ".", "Springt", "ei\u00b7ner", "hier", "von"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "$.", "VVFIN", "ART", "ADV", "APPR"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.10": {"text": "seinem Zweige,", "tokens": ["sei\u00b7nem", "Zwei\u00b7ge", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "So hebt der Zweig, den er entlastet, sich in die H\u00f6h. Ein", "tokens": ["So", "hebt", "der", "Zweig", ",", "den", "er", "ent\u00b7las\u00b7tet", ",", "sich", "in", "die", "H\u00f6h", ".", "Ein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,", "PRF", "APPR", "ART", "NN", "$.", "ART"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "andrer biegt", "tokens": ["an\u00b7drer", "biegt"], "token_info": ["word", "word"], "pos": ["PIS", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "Sich pl\u00f6tzlich abwerts, wenn auf ihn der kleine muntre Vogel", "tokens": ["Sich", "pl\u00f6tz\u00b7lich", "ab\u00b7werts", ",", "wenn", "auf", "ihn", "der", "klei\u00b7ne", "mun\u00b7tre", "Vo\u00b7gel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PRF", "ADJD", "PTKVZ", "$,", "KOUS", "APPR", "PPER", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.14": {"text": "fliegt.", "tokens": ["fliegt", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.15": {"text": "Ja, wenn auf einigen zuweilen, voll s\u00fcssen Feuers, manches", "tokens": ["Ja", ",", "wenn", "auf", "ei\u00b7ni\u00b7gen", "zu\u00b7wei\u00b7len", ",", "voll", "s\u00fcs\u00b7sen", "Feu\u00b7ers", ",", "man\u00b7ches"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PTKANT", "$,", "KOUS", "APPR", "PIAT", "ADV", "$,", "ADJD", "ADJA", "NN", "$,", "PIS"], "meter": "-+--+--+--+-+-+-", "measure": "amphibrach.tetra.plus"}, "line.16": {"text": "Paar", "tokens": ["Paar"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.17": {"text": "Der holden Liebe Triebe folgt; wird man ein sch\u00fctterndes", "tokens": ["Der", "hol\u00b7den", "Lie\u00b7be", "Trie\u00b7be", "folgt", ";", "wird", "man", "ein", "sch\u00fct\u00b7tern\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "$.", "VAFIN", "PIS", "ART", "ADJA"], "meter": "-+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.18": {"text": "Bewegen,", "tokens": ["Be\u00b7we\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.19": {"text": "Auch wenn verschiedne scherzend k\u00e4mpfen, ein auch nicht", "tokens": ["Auch", "wenn", "ver\u00b7schied\u00b7ne", "scher\u00b7zend", "k\u00e4mp\u00b7fen", ",", "ein", "auch", "nicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "KOUS", "ADJA", "ADJD", "VVINF", "$,", "ART", "ADV", "PTKNEG"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "minder heftigs Regen,", "tokens": ["min\u00b7der", "hef\u00b7tigs", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.21": {"text": "So, da\u00df die zarten Bl\u00e4tter zittern, fast selbst dadurch bewegt,", "tokens": ["So", ",", "da\u00df", "die", "zar\u00b7ten", "Bl\u00e4t\u00b7ter", "zit\u00b7tern", ",", "fast", "selbst", "da\u00b7durch", "be\u00b7wegt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$,", "ADV", "ADV", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.22": {"text": "gewahr.", "tokens": ["ge\u00b7wahr", "."], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.4": {"line.1": {"text": "An aller Pracht der zierlichen und holden B\u00e4ume, die so", "tokens": ["An", "al\u00b7ler", "Pracht", "der", "zier\u00b7li\u00b7chen", "und", "hol\u00b7den", "B\u00e4u\u00b7me", ",", "die", "so"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PIAT", "NN", "ART", "ADJA", "KON", "ADJA", "NN", "$,", "PRELS", "ADV"], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.2": {"text": "sch\u00f6n,", "tokens": ["sch\u00f6n", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Verwandt ich ernstlich Blick und Denken. Wie ich sie lange", "tokens": ["Ver\u00b7wandt", "ich", "ernst\u00b7lich", "Blick", "und", "Den\u00b7ken", ".", "Wie", "ich", "sie", "lan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "NN", "KON", "NN", "$.", "PWAV", "PPER", "PPER", "ADV"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "nun gesehn,", "tokens": ["nun", "ge\u00b7sehn", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Und ihren Wuchs und Schmuck bewundert; empfand ich", "tokens": ["Und", "ih\u00b7ren", "Wuchs", "und", "Schmuck", "be\u00b7wun\u00b7dert", ";", "emp\u00b7fand", "ich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "KON", "NN", "VVPP", "$.", "VVFIN", "PPER"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "einen regen Trieb,", "tokens": ["ei\u00b7nen", "re\u00b7gen", "Trieb", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Die Sch\u00f6nheit andern auch zu zeigen. Da ich denn folgends", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "an\u00b7dern", "auch", "zu", "zei\u00b7gen", ".", "Da", "ich", "denn", "fol\u00b7gends"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "ADV", "PTKZU", "VVINF", "$.", "KOUS", "PPER", "ADV", "PIS"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "niederschrieb:", "tokens": ["nie\u00b7der\u00b7schrieb", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Der B\u00e4ume Form, ihr sch\u00f6nes Gr\u00fcn, ihr holdes", "tokens": ["Der", "B\u00e4u\u00b7me", "Form", ",", "ihr", "sch\u00f6\u00b7nes", "Gr\u00fcn", ",", "ihr", "hol\u00b7des"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schirm-Dach f\u00fcr die Hitze,", "tokens": ["Schir\u00b7mDach", "f\u00fcr", "die", "Hit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Mit s\u00fcsser K\u00fchlung angef\u00fcllt, der angenehmen Schatten", "tokens": ["Mit", "s\u00fcs\u00b7ser", "K\u00fch\u00b7