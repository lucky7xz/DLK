{"textgrid.poem.63626": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "[ich wei\u00df, ein Wahn ist's und zum Wahnsinn bringt's]", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df, ein Wahn ist's und zum Wahnsinn bringt's,", "tokens": ["Ich", "wei\u00df", ",", "ein", "Wahn", "ist's", "und", "zum", "Wahn\u00b7sinn", "bringt's", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "NE", "KON", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ihm nachzuh\u00e4ngen. Dennoch, jeden Tag,", "tokens": ["Ihm", "nach\u00b7zu\u00b7h\u00e4n\u00b7gen", ".", "Den\u00b7noch", ",", "je\u00b7den", "Tag", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVIZU", "$.", "ADV", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sobald versank der Sonnenball und noch", "tokens": ["So\u00b7bald", "ver\u00b7sank", "der", "Son\u00b7nen\u00b7ball", "und", "noch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Trost des Sternenschimmers nicht erbl\u00fcht,", "tokens": ["Der", "Trost", "des", "Ster\u00b7nen\u00b7schim\u00b7mers", "nicht", "er\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nur bleiern bleiches Zwielicht auf dem pl\u00f6tzlich", "tokens": ["Nur", "blei\u00b7ern", "blei\u00b7ches", "Zwie\u00b7licht", "auf", "dem", "pl\u00f6tz\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "ADJA", "NN", "APPR", "ART", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Entseelten Angesicht der Erde ruht,", "tokens": ["Ent\u00b7seel\u00b7ten", "An\u00b7ge\u00b7sicht", "der", "Er\u00b7de", "ruht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Tritt vor mich hin dasselbe Graungespenst.", "tokens": ["Tritt", "vor", "mich", "hin", "das\u00b7sel\u00b7be", "Graun\u00b7ge\u00b7spenst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ADV", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Mir ist, mein Knabe sei in weiter Ferne", "tokens": ["Mir", "ist", ",", "mein", "Kna\u00b7be", "sei", "in", "wei\u00b7ter", "Fer\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "PPOSAT", "NN", "VAFIN", "APPR", "ADV", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Verirrt und finde nicht nach Haus. Ich seh' ihn", "tokens": ["Ver\u00b7irrt", "und", "fin\u00b7de", "nicht", "nach", "Haus", ".", "Ich", "seh'", "ihn"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "KON", "VVFIN", "PTKNEG", "APPR", "NN", "$.", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Durch graue Gassen einer fremden Stadt", "tokens": ["Durch", "grau\u00b7e", "Gas\u00b7sen", "ei\u00b7ner", "frem\u00b7den", "Stadt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Hineilen, seine kleinen F\u00fc\u00dfe wanken,", "tokens": ["Hi\u00b7nei\u00b7len", ",", "sei\u00b7ne", "klei\u00b7nen", "F\u00fc\u00b7\u00dfe", "wan\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Von k\u00fchlem Tau und kaltem Schwei\u00dfe klebt", "tokens": ["Von", "k\u00fch\u00b7lem", "Tau", "und", "kal\u00b7tem", "Schwei\u00b7\u00dfe", "klebt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Sein braunes Haar, die Augen suchen irr", "tokens": ["Sein", "brau\u00b7nes", "Haar", ",", "die", "Au\u00b7gen", "su\u00b7chen", "irr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Umher, ob sie das Haus nicht wiederfinden,", "tokens": ["Um\u00b7her", ",", "ob", "sie", "das", "Haus", "nicht", "wie\u00b7der\u00b7fin\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Wohin er soll, wo ihm das Bettchen steht,", "tokens": ["Wo\u00b7hin", "er", "soll", ",", "wo", "ihm", "das", "Bett\u00b7chen", "steht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VMFIN", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Die Mutter t\u00f6dlich sich um ihn zerbangt", "tokens": ["Die", "Mut\u00b7ter", "t\u00f6d\u00b7lich", "sich", "um", "ihn", "zer\u00b7bangt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "PRF", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Und trostlos sie der Vater tr\u00f6sten will.", "tokens": ["Und", "trost\u00b7los", "sie", "der", "Va\u00b7ter", "tr\u00f6s\u00b7ten", "will", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Und fremde Leute, hastig teilnahmlos,", "tokens": ["Und", "frem\u00b7de", "Leu\u00b7te", ",", "has\u00b7tig", "teil\u00b7nahm\u00b7los", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Gehn ihm vorbei \u2013 er ruft sie an \u2013 er fleht:", "tokens": ["Gehn", "ihm", "vor\u00b7bei", "\u2013", "er", "ruft", "sie", "an", "\u2013", "er", "fleht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKVZ", "$(", "PPER", "VVFIN", "PPER", "PTKVZ", "$(", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Bringt mich nach Hause! \u2013 Keiner h\u00f6rt auf ihn;", "tokens": ["Bringt", "mich", "nach", "Hau\u00b7se", "!", "\u2013", "Kei\u00b7ner", "h\u00f6rt", "auf", "ihn", ";"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "$.", "$(", "PIS", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Nicht eine Pforte tut sich ladend auf,", "tokens": ["Nicht", "ei\u00b7ne", "Pfor\u00b7te", "tut", "sich", "la\u00b7dend", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "VVFIN", "PRF", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Nicht eine Hand zieht ihn ins Wohnliche.", "tokens": ["Nicht", "ei\u00b7ne", "Hand", "zieht", "ihn", "ins", "Wohn\u00b7li\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.23": {"text": "Und so von T\u00fcr zu T\u00fcre, hingejagt", "tokens": ["Und", "so", "von", "T\u00fcr", "zu", "T\u00fc\u00b7re", ",", "hin\u00b7ge\u00b7jagt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADV", "APPR", "NN", "APPR", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Von Hunger, Angst und Sterbensm\u00fcdigkeit,", "tokens": ["Von", "Hun\u00b7ger", ",", "Angst", "und", "Ster\u00b7bens\u00b7m\u00fc\u00b7dig\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Sucht er und sucht \u2013 und keine Zuflucht winkt,", "tokens": ["Sucht", "er", "und", "sucht", "\u2013", "und", "kei\u00b7ne", "Zu\u00b7flucht", "winkt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "$(", "KON", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Und dichter, k\u00fchler, schauriger umdunkelt", "tokens": ["Und", "dich\u00b7ter", ",", "k\u00fch\u00b7ler", ",", "schau\u00b7ri\u00b7ger", "um\u00b7dun\u00b7kelt"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "ADJD", "$,", "ADJD", "$,", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Die Nacht sein banges Leben \u2013 schwer und schwerer", "tokens": ["Die", "Nacht", "sein", "ban\u00b7ges", "Le\u00b7ben", "\u2013", "schwer", "und", "schwe\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$(", "ADJD", "KON", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Den Atem ringt er aus beklemmter Brust \u2013", "tokens": ["Den", "A\u00b7tem", "ringt", "er", "aus", "be\u00b7klemm\u00b7ter", "Brust", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Und jetzt \u2013 die Kraft versiegt \u2013 mit leisem Ach", "tokens": ["Und", "jetzt", "\u2013", "die", "Kraft", "ver\u00b7siegt", "\u2013", "mit", "lei\u00b7sem", "Ach"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "$(", "ART", "NN", "VVPP", "$(", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Hinsinkt er auf den kalten Stein.", "tokens": ["Hin\u00b7sinkt", "er", "auf", "den", "kal\u00b7ten", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da sendet", "tokens": ["Da", "sen\u00b7det"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "+--", "measure": "dactylic.init"}, "line.2": {"text": "Ein g\u00fct'ger D\u00e4mon, der das Herz mir nicht", "tokens": ["Ein", "g\u00fct'\u00b7ger", "D\u00e4\u00b7mon", ",", "der", "das", "Herz", "mir", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "PPER", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Will springen lassen im lebend'gen Leibe,", "tokens": ["Will", "sprin\u00b7gen", "las\u00b7sen", "im", "le\u00b7ben\u00b7d'\u00b7gen", "Lei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "VVINF", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ihm Helfer in der h\u00f6chsten Not. Ich seh'", "tokens": ["Ihm", "Hel\u00b7fer", "in", "der", "h\u00f6chs\u00b7ten", "Not", ".", "Ich", "seh'"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "NN", "APPR", "ART", "ADJA", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Zwei andre Kinder um die Ecke biegen,", "tokens": ["Zwei", "and\u00b7re", "Kin\u00b7der", "um", "die", "E\u00b7cke", "bie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Stillgleitend wie mit Fl\u00fcgeln. An der Hand", "tokens": ["Still\u00b7glei\u00b7tend", "wie", "mit", "Fl\u00fc\u00b7geln", ".", "An", "der", "Hand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "APPR", "NN", "$.", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "F\u00fchrt ein halbw\u00fcchs'ger Knab' ein zierlich M\u00e4gdlein,", "tokens": ["F\u00fchrt", "ein", "halb\u00b7w\u00fcchs'\u00b7ger", "Knab'", "ein", "zier\u00b7lich", "M\u00e4gd\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Das kaum erst trippeln lernte. Stolz und ernst", "tokens": ["Das", "kaum", "erst", "trip\u00b7peln", "lern\u00b7te", ".", "Stolz", "und", "ernst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "ADV", "ADV", "VVINF", "VVFIN", "$.", "NN", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Gl\u00fcht unter blasser Stirn das Knabenauge", "tokens": ["Gl\u00fcht", "un\u00b7ter", "blas\u00b7ser", "Stirn", "das", "Kna\u00b7ben\u00b7au\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und rastet pl\u00f6tzlich auf dem Hingesunknen.", "tokens": ["Und", "ras\u00b7tet", "pl\u00f6tz\u00b7lich", "auf", "dem", "Hin\u00b7ge\u00b7sun\u00b7knen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Das M\u00e4gdlein aber stutzt und zeigt auf ihn,", "tokens": ["Das", "M\u00e4gd\u00b7lein", "a\u00b7ber", "stutzt", "und", "zeigt", "auf", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "KON", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und jetzt, mit holdem, unh\u00f6rbarem Lachen", "tokens": ["Und", "jetzt", ",", "mit", "hol\u00b7dem", ",", "un\u00b7h\u00f6r\u00b7ba\u00b7rem", "La\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "$,", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "L\u00e4uft's auf ihn zu und tupft ihn auf den Kopf,", "tokens": ["L\u00e4uft's", "auf", "ihn", "zu", "und", "tupft", "ihn", "auf", "den", "Kopf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "PTKVZ", "KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Und wie er aufsieht, streichelt sie ihm sanft", "tokens": ["Und", "wie", "er", "auf\u00b7sieht", ",", "strei\u00b7chelt", "sie", "ihm", "sanft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PPER", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Das taubetriefte Haar. Doch ihr Gef\u00e4hrte", "tokens": ["Das", "tau\u00b7be\u00b7trief\u00b7te", "Haar", ".", "Doch", "ihr", "Ge\u00b7f\u00e4hr\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Fa\u00dft br\u00fcderlich den Kleinen unterm Arm", "tokens": ["Fa\u00dft", "br\u00fc\u00b7der\u00b7lich", "den", "Klei\u00b7nen", "un\u00b7term", "Arm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Und richtet ihn empor. Da sehn die Drei", "tokens": ["Und", "rich\u00b7tet", "ihn", "em\u00b7por", ".", "Da", "sehn", "die", "Drei"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$.", "ADV", "VVFIN", "ART", "CARD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Sich an mit Kinderneugier, rasch vertraut,", "tokens": ["Sich", "an", "mit", "Kin\u00b7der\u00b7neu\u00b7gier", ",", "rasch", "ver\u00b7traut", ","], "token