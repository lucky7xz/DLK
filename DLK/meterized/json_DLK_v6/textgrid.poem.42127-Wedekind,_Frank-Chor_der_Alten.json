{"textgrid.poem.42127": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Chor der Alten", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Majest\u00e4tisch und mit Schweigen", "tokens": ["Ma\u00b7jes\u00b7t\u00e4\u00b7tisch", "und", "mit", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "KON", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Treten leise wir hervor,", "tokens": ["Tre\u00b7ten", "lei\u00b7se", "wir", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rufend, aufgestellt im Reigen:", "tokens": ["Ru\u00b7fend", ",", "auf\u00b7ge\u00b7stellt", "im", "Rei\u00b7gen", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Galathea, sieh dich vor!", "tokens": ["Ga\u00b7la\u00b7thea", ",", "sieh", "dich", "vor", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVIMP", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "H\u00f6r uns alte Greise an,", "tokens": ["H\u00f6r", "uns", "al\u00b7te", "Grei\u00b7se", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Die wir in der Zukunft lesen,", "tokens": ["Die", "wir", "in", "der", "Zu\u00b7kunft", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Was schon \u00f6fter dagewesen", "tokens": ["Was", "schon", "\u00f6f\u00b7ter", "da\u00b7ge\u00b7we\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "ADV", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und auch dir passieren kann.", "tokens": ["Und", "auch", "dir", "pas\u00b7sie\u00b7ren", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Siehst du jenen bleichen Knaben", "tokens": ["Siehst", "du", "je\u00b7nen", "blei\u00b7chen", "Kna\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Hinter seinen Schafen traben?", "tokens": ["Hin\u00b7ter", "sei\u00b7nen", "Scha\u00b7fen", "tra\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Galathea, siehst du nicht,", "tokens": ["Ga\u00b7la\u00b7thea", ",", "siehst", "du", "nicht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Da\u00df er mit sich selber spricht?", "tokens": ["Da\u00df", "er", "mit", "sich", "sel\u00b7ber", "spricht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PRF", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Mit der Zunge, wie vor Hitze,", "tokens": ["Mit", "der", "Zun\u00b7ge", ",", "wie", "vor", "Hit\u00b7ze", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Leckt er sich die Nasenspitze.", "tokens": ["Leckt", "er", "sich", "die", "Na\u00b7sen\u00b7spit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Felix nennt der Knabe sich;", "tokens": ["Fe\u00b7lix", "nennt", "der", "Kna\u00b7be", "sich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Galathea, h\u00fcte dich!", "tokens": ["Ga\u00b7la\u00b7thea", ",", "h\u00fc\u00b7te", "dich", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Sieh, er schmiedet seine Pl\u00e4ne,", "tokens": ["Sieh", ",", "er", "schmie\u00b7det", "sei\u00b7ne", "Pl\u00e4\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Kommt dann in dem Kleid des Schafes,", "tokens": ["Kommt", "dann", "in", "dem", "Kleid", "des", "Scha\u00b7fes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.19": {"text": "St\u00f6rt die Ruhe deines Schlafes,", "tokens": ["St\u00f6rt", "die", "Ru\u00b7he", "dei\u00b7nes", "Schla\u00b7fes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Pl\u00f6tzlich weist er dir die Z\u00e4hne", "tokens": ["Pl\u00f6tz\u00b7lich", "weist", "er", "dir", "die", "Z\u00e4h\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Und bevor du ihm entflohn,", "tokens": ["Und", "be\u00b7vor", "du", "ihm", "ent\u00b7flohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Bei\u00dft er dir die Kehle schon.", "tokens": ["Bei\u00dft", "er", "dir", "die", "Keh\u00b7le", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Drauf packt er dich bei den H\u00e4nden,", "tokens": ["Drauf", "packt", "er", "dich", "bei", "den", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.24": {"text": "Um sein Mordwerk zu vollenden;", "tokens": ["Um", "sein", "Mord\u00b7werk", "zu", "voll\u00b7en\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Deine Glieder strampeln noch,", "tokens": ["Dei\u00b7ne", "Glie\u00b7der", "stram\u00b7peln", "noch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.26": {"text": "Aber er bekommt dich doch.", "tokens": ["A\u00b7ber", "er", "be\u00b7kommt", "dich", "doch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Pl\u00f6tzlich f\u00fchlst du aus den Knien", "tokens": ["Pl\u00f6tz\u00b7lich", "f\u00fchlst", "du", "aus", "den", "Kni\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Alle Kraft von hinnen ziehen,", "tokens": ["Al\u00b7le", "Kraft", "von", "hin\u00b7nen", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Deine Muskeln werden schwach,", "tokens": ["Dei\u00b7ne", "Mus\u00b7keln", "wer\u00b7den", "schwach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.30": {"text": "Du beschr\u00e4nkst dich auf ein Ach.", "tokens": ["Du", "be\u00b7schr\u00e4nkst", "dich", "auf", "ein", "Ach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Er indes wird immer toller,", "tokens": ["Er", "in\u00b7des", "wird", "im\u00b7mer", "tol\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADV", "ADJA", "$,"], "meter": "---+--+-", "measure": "iambic.di.relaxed"}, "line.32": {"text": "Seine Miene sorgenvoller;", "tokens": ["Sei\u00b7ne", "Mie\u00b7ne", "sor\u00b7gen\u00b7vol\u00b7ler", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Dabei br\u00fcllt er wie ein Leu,", "tokens": ["Da\u00b7bei", "br\u00fcllt", "er", "wie", "ein", "Leu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "KOKOM", "ART", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.34": {"text": "Weil ihm das Gef\u00fchl noch neu.", "tokens": ["Weil", "ihm", "das", "Ge\u00b7f\u00fchl", "noch", "neu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "Dich jedoch packt erst ein Schlucken,", "tokens": ["Dich", "je\u00b7doch", "packt", "erst", "ein", "Schlu\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.36": {"text": "Dann ein Zittern, dann ein Zucken,", "tokens": ["Dann", "ein", "Zit\u00b7tern", ",", "dann", "ein", "Zu\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Und dann wird dir so gewaltig,", "tokens": ["Und", "dann", "wird", "dir", "so", "ge\u00b7wal\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Wie du's nie an dir erprobt.", "tokens": ["Wie", "du's", "nie", "an", "dir", "er\u00b7probt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.39": {"text": "Und du k\u00fc\u00dft ihn mannigfaltig,", "tokens": ["Und", "du", "k\u00fc\u00dft", "ihn", "man\u00b7nig\u00b7fal\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Da\u00df er's nur nicht lassen wolle,", "tokens": ["Da\u00df", "er's", "nur", "nicht", "las\u00b7sen", "wol\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Bis sich der erwartungsvolle", "tokens": ["Bis", "sich", "der", "er\u00b7war\u00b7tungs\u00b7vol\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PRF", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Jubel in dir ausgetobt. \u2013", "tokens": ["Ju\u00b7bel", "in", "dir", "aus\u00b7ge\u00b7tobt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "PPER", "VVPP", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.43": {"text": "Das ist so in gro\u00dfen Z\u00fcgen", "tokens": ["Das", "ist", "so", "in", "gro\u00b7\u00dfen", "Z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.44": {"text": "Das gef\u00e4hrliche Vergn\u00fcgen,", "tokens": ["Das", "ge\u00b7f\u00e4hr\u00b7li\u00b7che", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.45": {"text": "Dran der bleiche Knabe denkt,", "tokens": ["Dran", "der", "blei\u00b7che", "Kna\u00b7be", "denkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.46": {"text": "Wenn er seine Schafe tr\u00e4nkt.", "tokens": ["Wenn", "er", "sei\u00b7ne", "Scha\u00b7fe", "tr\u00e4nkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.47": {"text": "Du kannst freilich nicht begreifen,", "tokens": ["Du", "kannst", "frei\u00b7lich", "nicht", "be\u00b7grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "Welche Pl\u00e4ne in ihm reifen,", "tokens": ["Wel\u00b7che", "Pl\u00e4\u00b7ne", "in", "ihm", "rei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.49": {"text": "Denn noch bist du nicht gerissen", "tokens": ["Denn", "noch", "bist", "du", "nicht", "ge\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "PTKNEG", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.50": {"text": "Aus dem Traume deiner Kindheit,", "tokens": ["Aus", "dem", "Trau\u00b7me", "dei\u00b7ner", "Kind\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.51": {"text": "Aus der Ruhe deiner Blindheit", "tokens": ["Aus", "der", "Ru\u00b7he", "dei\u00b7ner", "Blind\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.52": {"text": "Durch ein unheilvolles Wissen.", "tokens": ["Durch", "ein", "un\u00b7heil\u00b7vol\u00b7les", "Wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.53": {"text": "Doch er wird die Hei\u00dfbegehrte", "tokens": ["Doch", "er", "wird", "die", "Hei\u00df\u00b7be\u00b7gehr\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.54": {"text": "Lehren, was das Sch\u00e4tzenswerte", "tokens": ["Leh\u00b7ren", ",", "was", "das", "Sch\u00e4t\u00b7zens\u00b7wer\u00b7te"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.55": {"text": "Hier auf Erden und wozu", "tokens": ["Hier", "auf", "Er\u00b7den", "und", "wo\u00b7zu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "KON", "PWAV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.56": {"text": "Er nicht auch so dumm wie du.", "tokens": ["Er", "nicht", "auch", "so", "dumm", "wie", "du", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADV", "ADV", "ADJD", "KOKOM", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}