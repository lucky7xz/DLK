{"textgrid.poem.61218": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Kriegslied", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und wenn uns nichts mehr \u00fcbrig blieb,", "tokens": ["Und", "wenn", "uns", "nichts", "mehr", "\u00fcb\u00b7rig", "blieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PIS", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So blieb uns doch ein Schwert,", "tokens": ["So", "blieb", "uns", "doch", "ein", "Schwert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das zorngemut mit scharfem Hieb", "tokens": ["Das", "zorn\u00b7ge\u00b7mut", "mit", "schar\u00b7fem", "Hieb"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Trutz des Fremdlings wehrt;", "tokens": ["Dem", "Trutz", "des", "Fremd\u00b7lings", "wehrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So blieb die Schlacht als letzt Gericht", "tokens": ["So", "blieb", "die", "Schlacht", "als", "letzt", "Ge\u00b7richt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "KOKOM", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auf Leben und auf Tod;", "tokens": ["Auf", "Le\u00b7ben", "und", "auf", "Tod", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und wenn die Not nicht Eisen bricht,", "tokens": ["Und", "wenn", "die", "Not", "nicht", "Ei\u00b7sen", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PTKNEG", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das Eisen bricht die Not.", "tokens": ["Das", "Ei\u00b7sen", "bricht", "die", "Not", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wohlauf, du kleine Schar, wohlauf,", "tokens": ["Wohl\u00b7auf", ",", "du", "klei\u00b7ne", "Schar", ",", "wohl\u00b7auf", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "PPER", "ADJA", "NN", "$,", "PWAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vertrau' auf Gott den Herrn!", "tokens": ["Ver\u00b7trau'", "auf", "Gott", "den", "Herrn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es geht ein Stern am Himmel auf,", "tokens": ["Es", "geht", "ein", "Stern", "am", "Him\u00b7mel", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das ist der Freiheit Stern.", "tokens": ["Das", "ist", "der", "Frei\u00b7heit", "Stern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Als wie ein Fr\u00fchlingssturm erbraust", "tokens": ["Als", "wie", "ein", "Fr\u00fch\u00b7lings\u00b7sturm", "er\u00b7braust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOKOM", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der V\u00f6lker Aufgebot;", "tokens": ["Der", "V\u00f6l\u00b7ker", "Auf\u00b7ge\u00b7bot", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da f\u00e4hrt ans Eisen jede Faust,", "tokens": ["Da", "f\u00e4hrt", "ans", "Ei\u00b7sen", "je\u00b7de", "Faust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das Eisen bricht die Not.", "tokens": ["Das", "Ei\u00b7sen", "bricht", "die", "Not", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und ob der fremden S\u00f6ldner Schar", "tokens": ["Und", "ob", "der", "frem\u00b7den", "S\u00f6ld\u00b7ner", "Schar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie D\u00fcnensand sich mehrt:", "tokens": ["Wie", "D\u00fc\u00b7nen\u00b7sand", "sich", "mehrt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Getrost, je gr\u00f6\u00dfer die Gefahr,", "tokens": ["Ge\u00b7trost", ",", "je", "gr\u00f6\u00b7\u00dfer", "die", "Ge\u00b7fahr", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Je h\u00f6her Herz und Schwert!", "tokens": ["Je", "h\u00f6\u00b7her", "Herz", "und", "Schwert", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und ob aus seiner H\u00f6llenburg", "tokens": ["Und", "ob", "aus", "sei\u00b7ner", "H\u00f6l\u00b7len\u00b7burg"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Teufel selber droht:", "tokens": ["Der", "Teu\u00b7fel", "sel\u00b7ber", "droht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ein k\u00fchner Mut geht mittendurch,", "tokens": ["Ein", "k\u00fch\u00b7ner", "Mut", "geht", "mit\u00b7ten\u00b7durch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das Eisen bricht die Not.", "tokens": ["Das", "Ei\u00b7sen", "bricht", "die", "Not", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Schon hallt des Feinds Trompetenruf,", "tokens": ["Schon", "hallt", "des", "Feinds", "Trom\u00b7pe\u00b7ten\u00b7ruf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kanonen brummen drein.", "tokens": ["Ka\u00b7no\u00b7nen", "brum\u00b7men", "drein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wohlauf, wohlauf mit raschem Huf", "tokens": ["Wohl\u00b7auf", ",", "wohl\u00b7auf", "mit", "ra\u00b7schem", "Huf"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In seine Lanzenreihn!", "tokens": ["In", "sei\u00b7ne", "Lan\u00b7zen\u00b7reihn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es klingt der Stahl, es steigt der Brand,", "tokens": ["Es", "klingt", "der", "Stahl", ",", "es", "steigt", "der", "Brand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Bronnen springen rot -", "tokens": ["Die", "Bron\u00b7nen", "sprin\u00b7gen", "rot"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "So gr\u00fc\u00df dich Gott, mein deutsches Land!", "tokens": ["So", "gr\u00fc\u00df", "dich", "Gott", ",", "mein", "deut\u00b7sches", "Land", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das Eisen bricht die Not.", "tokens": ["Das", "Ei\u00b7sen", "bricht", "die", "Not", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}