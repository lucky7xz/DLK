 ["APPR", "ADJA", "NN", "VVFIN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Die der Sturm nebst Steur und Seegeln zu der Wolken H\u00f6he schwingt,", "tokens": ["Die", "der", "Sturm", "nebst", "Steur", "und", "See\u00b7geln", "zu", "der", "Wol\u00b7ken", "H\u00f6\u00b7he", "schwingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "NN", "KON", "NN", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Bis sie schnell der schwarze Rachen des ergrimmten Meers verschlingt.", "tokens": ["Bis", "sie", "schnell", "der", "schwar\u00b7ze", "Ra\u00b7chen", "des", "er\u00b7grimm\u00b7ten", "Meers", "ver\u00b7schlingt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.14": {"line.1": {"text": "Sagt, wer donnert in den Wolken? Sagt, wer brauset in den St\u00fcrmen?", "tokens": ["Sagt", ",", "wer", "don\u00b7nert", "in", "den", "Wol\u00b7ken", "?", "Sagt", ",", "wer", "brau\u00b7set", "in", "den", "St\u00fcr\u00b7men", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "VVFIN", "APPR", "ART", "NN", "$.", "VVFIN", "$,", "PWS", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Zweifler sprich! wer schwingt die Fluthen, die sich wie Gebirge th\u00fcrmen?", "tokens": ["Zweif\u00b7ler", "sprich", "!", "wer", "schwingt", "die", "Flut\u00b7hen", ",", "die", "sich", "wie", "Ge\u00b7bir\u00b7ge", "th\u00fcr\u00b7men", "?"], "