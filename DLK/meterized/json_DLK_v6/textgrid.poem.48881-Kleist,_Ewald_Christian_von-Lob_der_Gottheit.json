{"textgrid.poem.48881": {"metadata": {"author": {"name": "Kleist, Ewald Christian von", "birth": "N.A.", "death": "N.A."}, "title": "Lob der Gottheit", "genre": "verse", "period": "N.A.", "pub_year": 1737, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tausend Sternenheere loben meines Sch\u00f6pfers Macht und St\u00e4rke,", "tokens": ["Tau\u00b7send", "Ster\u00b7nen\u00b7hee\u00b7re", "lo\u00b7ben", "mei\u00b7nes", "Sch\u00f6p\u00b7fers", "Macht", "und", "St\u00e4r\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPOSAT", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Aller Welten Himmelskreise preisen seiner Weisheit Werke,", "tokens": ["Al\u00b7ler", "Wel\u00b7ten", "Him\u00b7mels\u00b7krei\u00b7se", "prei\u00b7sen", "sei\u00b7ner", "Weis\u00b7heit", "Wer\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Meere, Berge, W\u00e4lder, Kl\u00fcfte, die sein Wink hervorgebracht,", "tokens": ["Mee\u00b7re", ",", "Ber\u00b7ge", ",", "W\u00e4l\u00b7der", ",", "Kl\u00fcf\u00b7te", ",", "die", "sein", "Wink", "her\u00b7vor\u00b7ge\u00b7bracht", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Sind Posaunen seiner Liebe, sind Posaunen seiner Macht.", "tokens": ["Sind", "Po\u00b7sau\u00b7nen", "sei\u00b7ner", "Lie\u00b7be", ",", "sind", "Po\u00b7sau\u00b7nen", "sei\u00b7ner", "Macht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPOSAT", "NN", "$,", "VAFIN", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Soll ich denn allein verstummen? Soll ich ihm kein Loblied bringen?", "tokens": ["Soll", "ich", "denn", "al\u00b7lein", "ver\u00b7stum\u00b7men", "?", "Soll", "ich", "ihm", "kein", "Lob\u00b7lied", "brin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "VVINF", "$.", "VMFIN", "PPER", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Nein, ich will des Geistes Fl\u00fcgel auch zu seinem Throne schwingen;", "tokens": ["Nein", ",", "ich", "will", "des", "Geis\u00b7tes", "Fl\u00fc\u00b7gel", "auch", "zu", "sei\u00b7nem", "Thro\u00b7ne", "schwin\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VMFIN", "ART", "NN", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Und wenn meine Zunge stammelt, o! so sollen nur allein", "tokens": ["Und", "wenn", "mei\u00b7ne", "Zun\u00b7ge", "stam\u00b7melt", ",", "o", "!", "so", "sol\u00b7len", "nur", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "VVFIN", "$,", "FM", "$.", "ADV", "VMFIN", "ADV", "ADV"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Dieser Augen milde B\u00e4che Zeugen meiner Ehrfurcht seyn.", "tokens": ["Die\u00b7ser", "Au\u00b7gen", "mil\u00b7de", "B\u00e4\u00b7che", "Zeu\u00b7gen", "mei\u00b7ner", "Ehr\u00b7furcht", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJA", "NN", "NN", "PPOSAT", "NN", "VAINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.3": {"line.1": {"text": "Ja, sie stammelt; sieh, o Sch\u00f6pfer, meines Herzens Altar rauchen!", "tokens": ["Ja", ",", "sie", "stam\u00b7melt", ";", "sieh", ",", "o", "Sch\u00f6p\u00b7fer", ",", "mei\u00b7nes", "Her\u00b7zens", "Al\u00b7tar", "rau\u00b7chen", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "$.", "VVFIN", "$,", "FM", "NN", "$,", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "K\u00f6nnt ich gleich den bl\u00f6den Pinsel in der Sonne Flammen tauchen,", "tokens": ["K\u00f6nnt", "ich", "gleich", "den", "bl\u00f6\u00b7den", "Pin\u00b7sel", "in", "der", "Son\u00b7ne", "Flam\u00b7men", "tau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "ADJA", "NN", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "O! so w\u00fcrd von deinem Wesen doch durch ihn kein Strich gemacht;", "tokens": ["O", "!", "so", "w\u00fcrd", "von", "dei\u00b7nem", "We\u00b7sen", "doch", "durch", "ihn", "kein", "Strich", "ge\u00b7macht", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VAFIN", "APPR", "PPOSAT", "NN", "ADV", "APPR", "PPER", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Dir wird selbst von reinen Geistern nur ein schwaches Lob gebracht.", "tokens": ["Dir", "wird", "selbst", "von", "rei\u00b7nen", "Geis\u00b7tern", "nur", "ein", "schwa\u00b7ches", "Lob", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ADJA", "NN", "ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.4": {"line.1": {"text": "Wer hei\u00dft tausend tausend Sonnen, pr\u00e4chtig, majest\u00e4tisch gl\u00e4nzen?", "tokens": ["Wer", "hei\u00dft", "tau\u00b7send", "tau\u00b7send", "Son\u00b7nen", ",", "pr\u00e4ch\u00b7tig", ",", "ma\u00b7jes\u00b7t\u00e4\u00b7tisch", "gl\u00e4n\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "CARD", "CARD", "NN", "$,", "ADJD", "$,", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Wer bestimmt dem Wunderlauf unz\u00e4hlbarer Erden Gr\u00e4nzen?", "tokens": ["Wer", "be\u00b7stimmt", "dem", "Wun\u00b7der\u00b7lauf", "un\u00b7z\u00e4hl\u00b7ba\u00b7rer", "Er\u00b7den", "Gr\u00e4n\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "ADJA", "NN", "NN", "$."], "meter": "+-+-+---+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Wer verbindet sie zusammen? Wer belebet jeden Kreis?", "tokens": ["Wer", "ver\u00b7bin\u00b7det", "sie", "zu\u00b7sam\u00b7men", "?", "Wer", "be\u00b7le\u00b7bet", "je\u00b7den", "Kreis", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PTKVZ", "$.", "PWS", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Deines Mundes sanfter Athem, HERR! dein m\u00e4chtigstes Gehei\u00df.", "tokens": ["Dei\u00b7nes", "Mun\u00b7des", "sanf\u00b7ter", "A\u00b7them", ",", "HeRR", "!", "dein", "m\u00e4ch\u00b7tigs\u00b7tes", "Ge\u00b7hei\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "NN", "$,", "NN", "$.", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.5": {"line.1": {"text": "Alles ist durch dich. Die Schaaren ungeheurer Sph\u00e4ren liefen", "tokens": ["Al\u00b7les", "ist", "durch", "dich", ".", "Die", "Schaa\u00b7ren", "un\u00b7ge\u00b7heu\u00b7rer", "Sph\u00e4\u00b7ren", "lie\u00b7fen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "APPR", "PPER", "$.", "ART", "NN", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Auf den Ton von deinen Lippen, durch die ewig leere Tiefen.", "tokens": ["Auf", "den", "Ton", "von", "dei\u00b7nen", "Lip\u00b7pen", ",", "durch", "die", "e\u00b7wig", "lee\u00b7re", "Tie\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "$,", "APPR", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Fische, V\u00f6gel, zahme Thiere, Wild das Feld und Hain durchstrich,", "tokens": ["Fi\u00b7sche", ",", "V\u00f6\u00b7gel", ",", "zah\u00b7me", "Thie\u00b7re", ",", "Wild", "das", "Feld", "und", "Hain", "durch\u00b7strich", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADJA", "NN", "$,", "NE", "ART", "NN", "KON", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Und vern\u00fcnftige Gesch\u00f6pfe scherzten drauf, und freuten sich.", "tokens": ["Und", "ver\u00b7n\u00fcnf\u00b7ti\u00b7ge", "Ge\u00b7sch\u00f6p\u00b7fe", "scherz\u00b7ten", "drauf", ",", "und", "freu\u00b7ten", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "PRF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.6": {"line.1": {"text": "Du giebst den entz\u00fcckten Blicken zwischen kr\u00e4uterreichen Auen", "tokens": ["Du", "giebst", "den", "ent\u00b7z\u00fcck\u00b7ten", "Bli\u00b7cken", "zwi\u00b7schen", "kr\u00e4u\u00b7ter\u00b7rei\u00b7chen", "Au\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "W\u00e4lder, die sich in den Wolken fast verlieren, anzuschauen.", "tokens": ["W\u00e4l\u00b7der", ",", "die", "sich", "in", "den", "Wol\u00b7ken", "fast", "ver\u00b7lie\u00b7ren", ",", "an\u00b7zu\u00b7schau\u00b7en", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PRF", "APPR", "ART", "NN", "ADV", "VVINF", "$,", "VVIZU", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Du machst, da\u00df darinn aus Felsen w\u00fctend sich ein Na\u00df ergie\u00dft,", "tokens": ["Du", "machst", ",", "da\u00df", "da\u00b7rinn", "aus", "Fel\u00b7sen", "w\u00fc\u00b7tend", "sich", "ein", "Na\u00df", "er\u00b7gie\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PAV", "APPR", "NN", "ADJD", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Das sich endlich blitzend schl\u00e4ngelt, und in Muscheln rieselnd flie\u00dft.", "tokens": ["Das", "sich", "end\u00b7lich", "blit\u00b7zend", "schl\u00e4n\u00b7gelt", ",", "und", "in", "Mu\u00b7scheln", "rie\u00b7selnd", "flie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "ADV", "ADJD", "VVFIN", "$,", "KON", "APPR", "NN", "VVPP", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.7": {"line.1": {"text": "Du r\u00fchrst, durch unz\u00e4hlige Gegenst\u00e4nde alle Sinnen,", "tokens": ["Du", "r\u00fchrst", ",", "durch", "un\u00b7z\u00e4h\u00b7li\u00b7ge", "Ge\u00b7gen\u00b7st\u00e4n\u00b7de", "al\u00b7le", "Sin\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ADJA", "NN", "PIAT", "NN", "$,"], "meter": "-+-+---+-+-+-+-", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Du l\u00e4\u00dft die Gesundheit bl\u00fchen, und aus tausend Quellen rinnen,", "tokens": ["Du", "l\u00e4\u00dft", "die", "Ge\u00b7sund\u00b7heit", "bl\u00fc\u00b7hen", ",", "und", "aus", "tau\u00b7send", "Quel\u00b7len", "rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN", "$,", "KON", "APPR", "CARD", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Tr\u00e4nkest mit der Milch des Regens, und mit Thau die d\u00fcrre Flur,", "tokens": ["Tr\u00e4n\u00b7kest", "mit", "der", "Milch", "des", "Re\u00b7gens", ",", "und", "mit", "Thau", "die", "d\u00fcr\u00b7re", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$,", "KON", "APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "K\u00fchlst die Luft durch sanfte Winde, und erfrischest die Natur.", "tokens": ["K\u00fchlst", "die", "Luft", "durch", "sanf\u00b7te", "Win\u00b7de", ",", "und", "er\u00b7fri\u00b7schest", "die", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ADJA", "NN", "$,", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.8": {"line.1": {"text": "Durch dich schm\u00fcckt die Hand des Fr\u00fchlings mit Tapeten unsre Gr\u00e4nzen,", "tokens": ["Durch", "dich", "schm\u00fcckt", "die", "Hand", "des", "Fr\u00fch\u00b7lings", "mit", "Ta\u00b7pe\u00b7ten", "uns\u00b7re", "Gr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Durch dich mu\u00df das Gold der \u00c4hren, und der Trauben Purpur gl\u00e4nzen,", "tokens": ["Durch", "dich", "mu\u00df", "das", "Gold", "der", "\u00c4h\u00b7ren", ",", "und", "der", "Trau\u00b7ben", "Pur\u00b7pur", "gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "ART", "NN", "ART", "NN", "$,", "KON", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Du erf\u00fcllst die Welt mit Freude, wenn die K\u00e4lte sie besiegt,", "tokens": ["Du", "er\u00b7f\u00fcllst", "die", "Welt", "mit", "Freu\u00b7de", ",", "wenn", "die", "K\u00e4l\u00b7te", "sie", "be\u00b7siegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$,", "KOUS", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Wenn sie eingeh\u00fcllt in Flocken, wie in zarten Windeln liegt.", "tokens": ["Wenn", "sie", "ein\u00b7ge\u00b7h\u00fcllt", "in", "Flo\u00b7cken", ",", "wie", "in", "zar\u00b7ten", "Win\u00b7deln", "liegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "NN", "$,", "PWAV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.9": {"line.1": {"text": "Durch dich kann des Menschen Seele in der Sternen Kreise dringen,", "tokens": ["Durch", "dich", "kann", "des", "Men\u00b7schen", "See\u00b7le", "in", "der", "Ster\u00b7nen", "Krei\u00b7se", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "ART", "NN", "NN", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Durch dich weis sie das Vergangne, hat Begriffe von den Dingen,", "tokens": ["Durch", "dich", "weis", "sie", "das", "Ver\u00b7gang\u00b7ne", ",", "hat", "Be\u00b7grif\u00b7fe", "von", "den", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKVZ", "PPER", "ART", "NN", "$,", "VAFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Scheidt der Sachen \u00c4hnlichkeiten von den Sachen selber ab,", "tokens": ["Scheidt", "der", "Sa\u00b7chen", "\u00c4hn\u00b7lich\u00b7kei\u00b7ten", "von", "den", "Sa\u00b7chen", "sel\u0