{"dta.poem.10957": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Einzige  \n Quelle des Vergn\u00fcgens.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Welt ist voller Lust und Anmuth, f\u00fcr ein vergn\u00fcgt und\nund fr\u00f6lichs Herz;", "tokens": ["Die", "Welt", "ist", "vol\u00b7ler", "Lust", "und", "An\u00b7muth", ",", "f\u00fcr", "ein", "ver\u00b7gn\u00fcgt", "und", "und", "fr\u00f6\u00b7lichs", "Herz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJA", "NN", "KON", "NN", "$,", "APPR", "ART", "VVPP", "KON", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+--+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "F\u00fcr einen unzufriednen Sinn ist sie voll Unmuth, Last und", "tokens": ["F\u00fcr", "ei\u00b7nen", "un\u00b7zu\u00b7fried\u00b7nen", "Sinn", "ist", "sie", "voll", "Un\u00b7muth", ",", "Last", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VAFIN", "PPER", "ADJD", "NN", "$,", "NN", "KON"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Die\u00df ist unwidersprechlich wahr. Es sind auf Erden alle Sachen", "tokens": ["Die\u00df", "ist", "un\u00b7wi\u00b7der\u00b7sprech\u00b7lich", "wahr", ".", "Es", "sind", "auf", "Er\u00b7den", "al\u00b7le", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADJD", "PTKVZ", "$.", "PPER", "VAFIN", "APPR", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.4": {"text": "Durchaus nicht, was sie sind; sie sind das, wozu wir sie selber", "tokens": ["Durc\u00b7haus", "nicht", ",", "was", "sie", "sind", ";", "sie", "sind", "das", ",", "wo\u00b7zu", "wir", "sie", "sel\u00b7ber"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PTKNEG", "$,", "PRELS", "PPER", "VAFIN", "$.", "PPER", "VAFIN", "PDS", "$,", "PWAV", "PPER", "PPER", "ADV"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.5": {"text": "Darum ja wohl nichts n\u00f6thigers, als da\u00df wir uns mit Ernst", "tokens": ["Da\u00b7rum", "ja", "wohl", "nichts", "n\u00f6\u00b7thi\u00b7gers", ",", "als", "da\u00df", "wir", "uns", "mit", "Ernst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "ADV", "PIS", "ADJA", "$,", "KOKOM", "KOUS", "PPER", "PRF", "APPR", "NE"], "meter": "-+-+-+-+-++--+", "measure": "iambic.septa.chol"}, "line.6": {"text": "Den Geist von schwarzer Schwermuth ab-und seine Kraft", "tokens": ["Den", "Geist", "von", "schwar\u00b7zer", "Schwer\u00b7muth", "ab\u00b7\u00b7und", "sei\u00b7ne", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So viel uns immer m\u00f6glich ist. Ach! la\u00dft es doch nicht aus", "tokens": ["So", "viel", "uns", "im\u00b7mer", "m\u00f6g\u00b7lich", "ist", ".", "Ach", "!", "la\u00dft", "es", "doch", "nicht", "aus"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "ADV", "ADJD", "VAFIN", "$.", "ITJ", "$.", "VVIMP", "PPER", "ADV", "PTKNEG", "PTKVZ"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "Weil die\u00df allein, was s\u00fc\u00df ist, s\u00fcsser, das Bittre minder bitter macht.", "tokens": ["Weil", "die\u00df", "al\u00b7lein", ",", "was", "s\u00fc\u00df", "ist", ",", "s\u00fcs\u00b7ser", ",", "das", "Bitt\u00b7re", "min\u00b7der", "bit\u00b7ter", "macht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ADV", "$,", "PRELS", "ADJD", "VAFIN", "$,", "ADJD", "$,", "ART", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.9": {"text": "Es hat ein jedes Ding zwo Seiten. Nachdem wir jedes", "tokens": ["Es", "hat", "ein", "je\u00b7des", "Ding", "zwo", "Sei\u00b7ten", ".", "Nach\u00b7dem", "wir", "je\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "PIAT", "NN", "CARD", "NN", "$.", "KOUS", "PPER", "PIAT"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Ist es verdrie\u00dflich und vergn\u00fcglich, ist es ver\u00e4chtlich oder sch\u00f6n.", "tokens": ["Ist", "es", "ver\u00b7drie\u00df\u00b7lich", "und", "ver\u00b7gn\u00fcg\u00b7lich", ",", "ist", "es", "ver\u00b7\u00e4cht\u00b7lich", "o\u00b7der", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "KON", "ADJD", "$,", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+--+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.11": {"text": "Der leichtste Weg zu diesem Gl\u00fcck, ohn den kein Gl\u00fcck sonst,", "tokens": ["Der", "leichts\u00b7te", "Weg", "zu", "die\u00b7sem", "Gl\u00fcck", ",", "ohn", "den", "kein", "Gl\u00fcck", "sonst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PDAT", "NN", "$,", "KOUI", "ART", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+--++-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Ist, mit Bedachtsamkeit zu achten auf alles, was wir Guts", "tokens": ["Ist", ",", "mit", "Be\u00b7dacht\u00b7sam\u00b7keit", "zu", "ach\u00b7ten", "auf", "al\u00b7les", ",", "was", "wir", "Guts"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "$,", "APPR", "NN", "PTKZU", "VVINF", "APPR", "PIS", "$,", "PRELS", "PPER", "NN"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.13": {"text": "Ist, uns von Blindheit der Gewohnheit im Guten, eifrig zu", "tokens": ["Ist", ",", "uns", "von", "Blind\u00b7heit", "der", "Ge\u00b7wohn\u00b7heit", "im", "Gu\u00b7ten", ",", "eif\u00b7rig", "zu"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "$,", "PPER", "APPR", "NN", "ART", "NN", "APPRART", "NN", "$,", "ADJD", "PTKZU"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.14": {"text": "Ist, besser, als bisher geschehn, die Sinnen zu gebrauchen lernen,", "tokens": ["Ist", ",", "bes\u00b7ser", ",", "als", "bis\u00b7her", "ge\u00b7schehn", ",", "die", "Sin\u00b7nen", "zu", "ge\u00b7brau\u00b7chen", "ler\u00b7nen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "ADJD", "$,", "KOUS", "ADV", "VVPP", "$,", "ART", "NN", "PTKZU", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.15": {"text": "Und nimmer ohne Denken, Sehn, Empfinden, Riechen, H\u00f6ren,", "tokens": ["Und", "nim\u00b7mer", "oh\u00b7ne", "Den\u00b7ken", ",", "Sehn", ",", "Emp\u00b7fin\u00b7den", ",", "Rie\u00b7chen", ",", "H\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "$,", "VVFIN", "$,", "NN", "$,", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.16": {"text": "Weil unser Gl\u00fcck und Gottes Ehr, allein durch Denken, zu ent-", "tokens": ["Weil", "un\u00b7ser", "Gl\u00fcck", "und", "Got\u00b7tes", "Ehr", ",", "al\u00b7lein", "durch", "Den\u00b7ken", ",", "zu", "ent"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "KON", "NN", "NN", "$,", "ADV", "APPR", "NN", "$,", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.17": {"text": "Bed\u00e4chten wir, wie viel wir haben, und wie so wenig wir", "tokens": ["Be\u00b7d\u00e4ch\u00b7ten", "wir", ",", "wie", "viel", "wir", "ha\u00b7ben", ",", "und", "wie", "so", "we\u00b7nig", "wir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "PIS", "PPER", "VAFIN", "$,", "KON", "PWAV", "ADV", "PIS", "PPER"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.18": {"text": "Unm\u00f6glich k\u00f6nnten wir so oft, so viel zu klagen, uns erk\u00fchnen.", "tokens": ["Un\u00b7m\u00f6g\u00b7lich", "k\u00f6nn\u00b7ten", "wir", "so", "oft", ",", "so", "viel", "zu", "kla\u00b7gen", ",", "uns", "er\u00b7k\u00fch\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "$,", "ADV", "PIS", "PTKZU", "VVINF", "$,", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.19": {"text": "So aber kehren wir es um, und lenken unsre Kraft der Seelen", "tokens": ["So", "a\u00b7ber", "keh\u00b7ren", "wir", "es", "um", ",", "und", "len\u00b7ken", "uns\u00b7re", "Kraft", "der", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.20": {"text": "Von allem, was wir haben, ab, und blo\u00df auf Dinge, die uns fehlen.", "tokens": ["Von", "al\u00b7lem", ",", "was", "wir", "ha\u00b7ben", ",", "ab", ",", "und", "blo\u00df", "auf", "Din\u00b7ge", ",", "die", "uns", "feh\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "PPER", "VAFIN", "$,", "PTKVZ", "$,", "KON", "ADV", "APPR", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.21": {"text": "So lange wir also verfahren: Kann es unm\u00f6glich anders seyn,", "tokens": ["So", "lan\u00b7ge", "wir", "al\u00b7so", "ver\u00b7fah\u00b7ren", ":", "Kann", "es", "un\u00b7m\u00f6g\u00b7lich", "an\u00b7ders", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "VVINF", "$.", "VMFIN", "PPER", "ADJD", "ADV", "VAINF", "$,"], "meter": "-+---+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.22": {"text": "(und, tr\u00e4ff auch alles, was wir wollen, nach unserm eignen", "tokens": ["(", "und", ",", "tr\u00e4ff", "auch", "al\u00b7les", ",", "was", "wir", "wol\u00b7len", ",", "nach", "un\u00b7serm", "eig\u00b7nen"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "KON", "$,", "VVFIN", "ADV", "PIS", "$,", "PRELS", "PPER", "VMFIN", "$,", "APPR", "PPOSAT", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "Unm\u00f6glich kann man sich vergn\u00fcgen. Denn, eben dadurch,", "tokens": ["Un\u00b7m\u00f6g\u00b7lich", "kann", "man", "sich", "ver\u00b7gn\u00fc\u00b7gen", ".", "Denn", ",", "e\u00b7ben", "da\u00b7durch", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PIS", "PRF", "VVINF", "$.", "KON", "$,", "ADV", "PAV", "$,"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "Und denn nicht mehr daran gedenken, verschwindet eben das", "tokens": ["Und", "denn", "nicht", "mehr", "da\u00b7ran", "ge\u00b7den\u00b7ken", ",", "ver\u00b7schwin\u00b7det", "e\u00b7ben", "das"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "PTKNEG", "ADV", "PAV", "VVINF", "$,", "VVFIN", "ADV", "ART"], "meter": "-+-+-+-+--+-+--", "measure": "iambic.hexa.relaxed"}, "line.25": {"text": "Dadurch wird meistens nun die Welt so voller Gram und vol-", "tokens": ["Da\u00b7durch", "wird", "meis\u00b7tens", "nun", "die", "Welt", "so", "vol\u00b7ler", "Gram", "und", "vol"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "ADV", "ADV", "ART", "NN", "ADV", "ADJA", "NN", "KON", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.26": {"text": "Und nicht so sehr durch ihr selbst\u00e4ndig\u2019 u. eigene Beschaffenheit.", "tokens": ["Und", "nicht", "so", "sehr", "durch", "ihr", "selb\u00b7st\u00e4n\u00b7dig'", "u.", "ei\u00b7ge\u00b7ne", "Be\u00b7schaf\u00b7fen\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "abbreviation", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "ADV", "APPR", "PPOSAT", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.27": {"text": "Wofern nicht jemand die Gesundheit, und etwan Speis und", "tokens": ["Wo\u00b7fern", "nicht", "je\u00b7mand", "die", "Ge\u00b7sund\u00b7heit", ",", "und", "et\u00b7wan", "Speis", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "PIS", "ART", "NN", "$,", "KON", "ADV", "NN", "KON"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "So thut er wirklich Unrecht, sich den Ungl\u00fcckselgen zu zu z\u00e4hlen.", "tokens": ["So", "thut", "er", "wirk\u00b7lich", "Un\u00b7recht", ",", "sich", "den", "Un\u00b7gl\u00fcck\u00b7sel\u00b7gen", "zu", "zu", "z\u00e4h\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "NN", "$,", "PRF", "ART", "NN", "PTKZU", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.29": {"text": "Wenn er nur in sich gehen will, u. seiner Plagen Quell ergr\u00fcnden:", "tokens": ["Wenn", "er", "nur", "in", "sich", "ge\u00b7hen", "will", ",", "u.", "sei\u00b7ner", "Pla\u00b7gen", "Quell", "er\u00b7gr\u00fcn\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "abbreviation", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PRF", "VVINF", "VMFIN", "$,", "APPR", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.30": {"text": "Wird er sie meist in seinem Herzen, und selten ausser sich befinden.", "tokens": ["Wird", "er", "sie", "meist", "in", "sei\u00b7nem", "Her\u00b7zen", ",", "und", "sel\u00b7ten", "aus\u00b7ser", "sich", "be\u00b7fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,", "KON", "ADJD", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.31": {"text": "Erkennest du nicht \u00fcberzeuglich, wenn du nur selbst willt in", "tokens": ["Er\u00b7ken\u00b7nest", "du", "nicht", "\u00fc\u00b7berz\u00b7eug\u00b7lich", ",", "wenn", "du", "nur", "selbst", "willt", "in"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADJD", "$,", "KOUS", "PPER", "ADV", "ADV", "VMFIN", "APPR"], "meter": "-+--+--+--+-+-+", "measure": "amphibrach.tetra.plus"}, "line.32": {"text": "Da\u00df mehrentheils blo\u00df in der Meynung die Dinge dieser Welt", "tokens": ["Da\u00df", "meh\u00b7ren\u00b7theils", "blo\u00df", "in", "der", "Mey\u00b7nung", "die", "Din\u00b7ge", "die\u00b7ser", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "APPR", "ART", "NN", "ART", "NN", "PDAT", "NN"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.33": {"text": "Willt du nun wirklich gl\u00fccklich seyn, und suchest es nicht blo\u00df", "tokens": ["Willt", "du", "nun", "wirk\u00b7lich", "gl\u00fcck\u00b7lich", "seyn", ",", "und", "su\u00b7chest", "es", "nicht", "blo\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "ADJD", "VAINF", "$,", "KON", "VVFIN", "PPER", "PTKNEG", "ADV"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.34": {"text": "So fange bey dir selber an; lern anders, als bishero, meynen.", "tokens": ["So", "fan\u00b7ge", "bey", "dir", "sel\u00b7ber", "an", ";", "lern", "an\u00b7ders", ",", "als", "bis\u00b7he\u00b7ro", ",", "mey\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ADV", "PT