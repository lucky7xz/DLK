{"textgrid.poem.61088": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Eros, der Schenk", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich w\u00e4hle mir den Liebesgott zum Schenken,", "tokens": ["Ich", "w\u00e4h\u00b7le", "mir", "den", "Lie\u00b7bes\u00b7gott", "zum", "Schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er f\u00fcllt den Becher mir aus Zauberkr\u00fcgen", "tokens": ["Er", "f\u00fcllt", "den", "Be\u00b7cher", "mir", "aus", "Zau\u00b7ber\u00b7kr\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wei\u00df das Herz in seliges Gen\u00fcgen,", "tokens": ["Und", "wei\u00df", "das", "Herz", "in", "se\u00b7li\u00b7ges", "Ge\u00b7n\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Sinn in s\u00fc\u00dfen Taumel zu versenken.", "tokens": ["Den", "Sinn", "in", "s\u00fc\u00b7\u00dfen", "Tau\u00b7mel", "zu", "ver\u00b7sen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Auch lehrt er mich, zu holdem Angedenken", "tokens": ["Auch", "lehrt", "er", "mich", ",", "zu", "hol\u00b7dem", "An\u00b7ge\u00b7den\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Wein zu schl\u00fcrfen in bed\u00e4cht'gen Z\u00fcgen,", "tokens": ["Den", "Wein", "zu", "schl\u00fcr\u00b7fen", "in", "be\u00b7d\u00e4cht'\u00b7gen", "Z\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu zartem Gru\u00dfe Reim in Reim zu f\u00fcgen", "tokens": ["Zu", "zar\u00b7tem", "Gru\u00b7\u00dfe", "Reim", "in", "Reim", "zu", "f\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN", "APPR", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und sanft der Musen wei\u00dfes Ro\u00df zu lenken.", "tokens": ["Und", "sanft", "der", "Mu\u00b7sen", "wei\u00b7\u00dfes", "Ro\u00df", "zu", "len\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und wenn des Abends Schatten sich verbreiten,", "tokens": ["Und", "wenn", "des", "A\u00b7bends", "Schat\u00b7ten", "sich", "ver\u00b7brei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und m\u00fcd ich ruhe von des Tags Genusse,", "tokens": ["Und", "m\u00fcd", "ich", "ru\u00b7he", "von", "des", "Tags", "Ge\u00b7nus\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erregt er sacht der Zither goldne Saiten.", "tokens": ["Er\u00b7regt", "er", "sacht", "der", "Zi\u00b7ther", "gold\u00b7ne", "Sai\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Da mu\u00df im Schlaf gleich Wimpeln auf dem Flusse", "tokens": ["Da", "mu\u00df", "im", "Schlaf", "gleich", "Wim\u00b7peln", "auf", "dem", "Flus\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "APPRART", "NN", "ADV", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Manch holdes Traumbild mir vor\u00fcbergleiten,", "tokens": ["Manch", "hol\u00b7des", "Traum\u00b7bild", "mir", "vor\u00b7\u00fc\u00b7berg\u00b7lei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bis mich der Morgen weckt mit ros'gem Kusse.", "tokens": ["Bis", "mich", "der", "Mor\u00b7gen", "weckt", "mit", "ros'\u00b7gem", "Kus\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}