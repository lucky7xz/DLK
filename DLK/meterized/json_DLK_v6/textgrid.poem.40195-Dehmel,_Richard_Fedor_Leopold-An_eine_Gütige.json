{"textgrid.poem.40195": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "An eine G\u00fctige", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es mag mir oft nicht im Gesichte stehen,", "tokens": ["Es", "mag", "mir", "oft", "nicht", "im", "Ge\u00b7sich\u00b7te", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "wie tief du dich ins Herz mir eingeschrieben;", "tokens": ["wie", "tief", "du", "dich", "ins", "Herz", "mir", "ein\u00b7ge\u00b7schrie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "PRF", "APPRART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "ach, oft schon hat es mich zum Wort getrieben,", "tokens": ["ach", ",", "oft", "schon", "hat", "es", "mich", "zum", "Wort", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["XY", "$,", "ADV", "ADV", "VAFIN", "PPER", "PRF", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "um dennoch wortlos meines Wegs zu gehen.", "tokens": ["um", "den\u00b7noch", "wort\u00b7los", "mei\u00b7nes", "Wegs", "zu", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ADJD", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So ist \u2013 wie ", "tokens": ["So", "ist", "\u2013", "wie"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADV", "VAFIN", "$(", "KOKOM"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "ein Ungeseh'nes zwischen uns geblieben:", "tokens": ["ein", "Un\u00b7ge\u00b7seh'\u00b7nes", "zwi\u00b7schen", "uns", "ge\u00b7blie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "das alte R\u00e4tsel, da\u00df sich Menschen lieben,", "tokens": ["das", "al\u00b7te", "R\u00e4t\u00b7sel", ",", "da\u00df", "sich", "Men\u00b7schen", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "PRF", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "die dennoch starr im Eignen Kreis sich drehen.", "tokens": ["die", "den\u00b7noch", "starr", "im", "Eig\u00b7nen", "Kreis", "sich", "dre\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "APPRART", "ADJA", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie fruchtlos schon des Kindes Spiel sich m\u00fchte,", "tokens": ["Wie", "frucht\u00b7los", "schon", "des", "Kin\u00b7des", "Spiel", "sich", "m\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "ART", "NN", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "da\u00df ganz Ein Kreis sich auf den andern lege!", "tokens": ["da\u00df", "ganz", "Ein", "Kreis", "sich", "auf", "den", "an\u00b7dern", "le\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PRF", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-++", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Bald lag er und durchschnitt die Bahn des andern,", "tokens": ["Bald", "lag", "er", "und", "durch\u00b7schnitt", "die", "Bahn", "des", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "ART", "NN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und bald verschlang er gar des andern Wege.", "tokens": ["und", "bald", "ver\u00b7schlang", "er", "gar", "des", "an\u00b7dern", "We\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "In Einem Kreis nur ist gemeinsam Wandern:", "tokens": ["In", "Ei\u00b7nem", "Kreis", "nur", "ist", "ge\u00b7mein\u00b7sam", "Wan\u00b7dern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VAFIN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "dem ", "tokens": ["dem"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}}}}}