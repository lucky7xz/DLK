{"dta.poem.8150": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Elisabeth.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Den Andern ich nehmen sollt';", "tokens": ["Den", "An\u00b7dern", "ich", "neh\u00b7men", "sollt'", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Was ich zuvor besessen,", "tokens": ["Was", "ich", "zu\u00b7vor", "be\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mein Herz sollt' es vergessen;", "tokens": ["Mein", "Herz", "sollt'", "es", "ver\u00b7ges\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das hat es nicht gewollt.", "tokens": ["Das", "hat", "es", "nicht", "ge\u00b7wollt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PTKNEG", "VMPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Meine Mutter klag' ich an,", "tokens": ["Mei\u00b7ne", "Mut\u00b7ter", "klag'", "ich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie hat nicht wohlgethan;", "tokens": ["Sie", "hat", "nicht", "wohl\u00b7ge\u00b7than", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was sonst in Ehren st\u00fcnde,", "tokens": ["Was", "sonst", "in", "Eh\u00b7ren", "st\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nun ist es worden S\u00fcnde.", "tokens": ["Nun", "ist", "es", "wor\u00b7den", "S\u00fcn\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VAPP", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Was fang' ich an!", "tokens": ["Was", "fang'", "ich", "an", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "F\u00fcr all' mein Stolz und Freud'", "tokens": ["F\u00fcr", "all'", "mein", "Stolz", "und", "Freud'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Gewonnen hab' ich Leid.", "tokens": ["Ge\u00b7won\u00b7nen", "hab'", "ich", "Leid", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ach, w\u00e4r' das nicht geschehen,", "tokens": ["Ach", ",", "w\u00e4r'", "das", "nicht", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VAFIN", "PDS", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ach, k\u00f6nnt' ich betteln gehen", "tokens": ["Ach", ",", "k\u00f6nnt'", "ich", "bet\u00b7teln", "ge\u00b7hen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VMFIN", "PPER", "VVINF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ueber die braune Haid'!", "tokens": ["Ue\u00b7ber", "die", "brau\u00b7ne", "Haid'", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}