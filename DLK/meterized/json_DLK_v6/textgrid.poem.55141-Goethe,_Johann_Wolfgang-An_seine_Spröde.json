{"textgrid.poem.55141": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "An seine Spr\u00f6de", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Siehst du die Pomeranze?", "tokens": ["Siehst", "du", "die", "Po\u00b7me\u00b7ran\u00b7ze", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Noch h\u00e4ngt sie an dem Baume;", "tokens": ["Noch", "h\u00e4ngt", "sie", "an", "dem", "Bau\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Schon ist der M\u00e4rz verflossen,", "tokens": ["Schon", "ist", "der", "M\u00e4rz", "ver\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und neue Bl\u00fcten kommen.", "tokens": ["Und", "neu\u00b7e", "Bl\u00fc\u00b7ten", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich trete zu dem Baume", "tokens": ["Ich", "tre\u00b7te", "zu", "dem", "Bau\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und sage: \u00bbPomeranze,", "tokens": ["Und", "sa\u00b7ge", ":", "\u00bb", "Po\u00b7me\u00b7ran\u00b7ze", ","], "token_info": ["word", "word", "punct", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Du reife Pomeranze,", "tokens": ["Du", "rei\u00b7fe", "Po\u00b7me\u00b7ran\u00b7ze", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Du s\u00fc\u00dfe Pomeranze,", "tokens": ["Du", "s\u00fc\u00b7\u00dfe", "Po\u00b7me\u00b7ran\u00b7ze", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Ich sch\u00fcttle, f\u00fchl, ich sch\u00fcttle,", "tokens": ["Ich", "sch\u00fctt\u00b7le", ",", "f\u00fchl", ",", "ich", "sch\u00fctt\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "O fall in meinen Scho\u00df!\u00ab", "tokens": ["O", "fall", "in", "mei\u00b7nen", "Scho\u00df", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NN", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}