{"textgrid.poem.50278": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Eh, ", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eh, ", "tokens": ["Eh", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "So fodert sie, um mich zu pr\u00fcfen, ", "tokens": ["So", "fo\u00b7dert", "sie", ",", "um", "mich", "zu", "pr\u00fc\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUI", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch merckt die ", "tokens": ["Doch", "merckt", "die"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Denn ehe sie kennt meinen Sinn,", "tokens": ["Denn", "e\u00b7he", "sie", "kennt", "mei\u00b7nen", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "So l\u00e4ufft sie, dass sie nicht mehr wird geliebt, Gefahr;", "tokens": ["So", "l\u00e4ufft", "sie", ",", "dass", "sie", "nicht", "mehr", "wird", "ge\u00b7liebt", ",", "Ge\u00b7fahr", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "VAFIN", "VVPP", "$,", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die zeigt mir nicht mehr, ", "tokens": ["Die", "zeigt", "mir", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKNEG", "ADV", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}