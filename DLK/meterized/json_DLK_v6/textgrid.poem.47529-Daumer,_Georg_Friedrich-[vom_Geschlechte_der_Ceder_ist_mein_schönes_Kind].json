{"textgrid.poem.47529": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[vom Geschlechte der Ceder ist mein sch\u00f6nes Kind]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vom Geschlechte der Ceder ist mein sch\u00f6nes Kind;", "tokens": ["Vom", "Ge\u00b7schlech\u00b7te", "der", "Ce\u00b7der", "ist", "mein", "sch\u00f6\u00b7nes", "Kind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Macht mir einen Sarg aus Cederholz!", "tokens": ["Macht", "mir", "ei\u00b7nen", "Sarg", "aus", "Ce\u00b7der\u00b7holz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "So begraben, o wie werd' ich wunderlind", "tokens": ["So", "be\u00b7gra\u00b7ben", ",", "o", "wie", "werd'", "ich", "wun\u00b7der\u00b7lind"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "$,", "FM", "KOKOM", "VAFIN", "PPER", "VAFIN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Rasten, o wie freudig und wie stolz!", "tokens": ["Ras\u00b7ten", ",", "o", "wie", "freu\u00b7dig", "und", "wie", "stolz", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "KOKOM", "ADJD", "KON", "PWAV", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}