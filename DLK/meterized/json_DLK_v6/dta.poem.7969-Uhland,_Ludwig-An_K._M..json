{"dta.poem.7969": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "An K. M.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Wann die Natur will kn\u00fcpfen und erbauen,", "tokens": ["Wann", "die", "Na\u00b7tur", "will", "kn\u00fcp\u00b7fen", "und", "er\u00b7bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VMFIN", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dann liebt in stillen Tiefen sie zu walten;", "tokens": ["Dann", "liebt", "in", "stil\u00b7len", "Tie\u00b7fen", "sie", "zu", "wal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Geweihten einzig ist verg\u00f6nnt, zu schauen,", "tokens": ["Ge\u00b7weih\u00b7ten", "ein\u00b7zig", "ist", "ver\u00b7g\u00f6nnt", ",", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJD", "VAFIN", "VVPP", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie ihre Hand den Fr\u00fchling mag gestalten,", "tokens": ["Wie", "ih\u00b7re", "Hand", "den", "Fr\u00fch\u00b7ling", "mag", "ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ART", "NN", "VMFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wie sie erzieht zu Eintracht und Vertrauen", "tokens": ["Wie", "sie", "er\u00b7zieht", "zu", "Ein\u00b7tracht", "und", "Ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Kinder fr\u00fch in dunkeln Aufenthalten.", "tokens": ["Die", "Kin\u00b7der", "fr\u00fch", "in", "dun\u00b7keln", "Auf\u00b7ent\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Nur wenn sie will zerst\u00f6ren und ersch\u00fcttern,", "tokens": ["Nur", "wenn", "sie", "will", "zer\u00b7st\u00f6\u00b7ren", "und", "er\u00b7sch\u00fct\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "VMFIN", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Erbraust sie in Orkanen und Gewittern.", "tokens": ["Er\u00b7braust", "sie", "in", "Or\u00b7ka\u00b7nen", "und", "Ge\u00b7wit\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So \u00fcbet auch die Liebe tief und leise", "tokens": ["So", "\u00fc\u00b7bet", "auch", "die", "Lie\u00b7be", "tief", "und", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Reich der Geister ihre Wundermacht;", "tokens": ["Im", "Reich", "der", "Geis\u00b7ter", "ih\u00b7re", "Wun\u00b7der\u00b7macht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie zieht unsichtbar ihre Zauberkreise", "tokens": ["Sie", "zieht", "un\u00b7sicht\u00b7bar", "ih\u00b7re", "Zau\u00b7ber\u00b7krei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Am goldnen Abend, in der Sternennacht;", "tokens": ["Am", "gold\u00b7nen", "A\u00b7bend", ",", "in", "der", "Ster\u00b7nen\u00b7nacht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sie weckt durch feierlicher Lieder Weise", "tokens": ["Sie", "weckt", "durch", "fei\u00b7er\u00b7li\u00b7cher", "Lie\u00b7der", "Wei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Verwandte Ch\u00f6re in der Geister Schacht;", "tokens": ["Ver\u00b7wand\u00b7te", "Ch\u00f6\u00b7re", "in", "der", "Geis\u00b7ter", "Schacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sie wei\u00df durch stiller Augen Strahl die Seelen", "tokens": ["Sie", "wei\u00df", "durch", "stil\u00b7ler", "Au\u00b7gen", "Strahl", "die", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Zu kn\u00fcpfen und auf ewig zu verm\u00e4hlen.", "tokens": ["Zu", "kn\u00fcp\u00b7fen", "und", "auf", "e\u00b7wig", "zu", "ver\u00b7m\u00e4h\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KON", "APPR", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dort in des Stromes wild emp\u00f6rte Wogen", "tokens": ["Dort", "in", "des", "Stro\u00b7mes", "wild", "em\u00b7p\u00f6r\u00b7te", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Warf sich ein J\u00fcngling, voll von raschen Gluten,", "tokens": ["Warf", "sich", "ein", "J\u00fcng\u00b7ling", ",", "voll", "von", "ra\u00b7schen", "Glu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ART", "NN", "$,", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch jene Wallung, die ihn fortgezogen,", "tokens": ["Doch", "je\u00b7ne", "Wal\u00b7lung", ",", "die", "ihn", "fort\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie mu\u00dft\u2019 ihn wieder an das Ufer fluten.", "tokens": ["Sie", "mu\u00dft'", "ihn", "wie\u00b7der", "an", "das", "U\u00b7fer", "flu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich aber sah es, wie des Himmels Bogen,", "tokens": ["Ich", "a\u00b7ber", "sah", "es", ",", "wie", "des", "Him\u00b7mels", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Erde Glanz im stillen Teiche ruhten:", "tokens": ["Der", "Er\u00b7de", "Glanz", "im", "stil\u00b7len", "Tei\u00b7che", "ruh\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Da sank ich hin, von sanfter Wonne trunken,", "tokens": ["Da", "sank", "ich", "hin", ",", "von", "sanf\u00b7ter", "Won\u00b7ne", "trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "APPR", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ich sank und bin auf ewig nun versunken.", "tokens": ["Ich", "sank", "und", "bin", "auf", "e\u00b7wig", "nun", "ver\u00b7sun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VAFIN", "APPR", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}