{"textgrid.poem.33911": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "N\u00e4chtlicher Gru\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In dieser dunklen Stunde", "tokens": ["In", "die\u00b7ser", "dunk\u00b7len", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der rings ergo\u00dfnen Nacht", "tokens": ["Der", "rings", "er\u00b7go\u00df\u00b7nen", "Nacht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hab' ich bei Euch die Runde", "tokens": ["Hab'", "ich", "bei", "Euch", "die", "Run\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu Gru\u00df und Ku\u00df gemacht.", "tokens": ["Zu", "Gru\u00df", "und", "Ku\u00df", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In eines Jeden Hause", "tokens": ["In", "ei\u00b7nes", "Je\u00b7den", "Hau\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sprach ich getreulich vor,", "tokens": ["Sprach", "ich", "ge\u00b7treu\u00b7lich", "vor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Bis in des Letzten Klause", "tokens": ["Bis", "in", "des", "Letz\u00b7ten", "Klau\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mein Geist sich ganz verlor.", "tokens": ["Mein", "Geist", "sich", "ganz", "ver\u00b7lor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nun seid ihr l\u00e4ngst versunken", "tokens": ["Nun", "seid", "ihr", "l\u00e4ngst", "ver\u00b7sun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Schlaf und tiefen Traum,", "tokens": ["In", "Schlaf", "und", "tie\u00b7fen", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und schwingt Euch ahnungstrunken", "tokens": ["Und", "schwingt", "Euch", "ah\u00b7nungs\u00b7trun\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hoch \u00fcber Zeit und Raum.", "tokens": ["Hoch", "\u00fc\u00b7ber", "Zeit", "und", "Raum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Leicht glaubt Ihr zu erstreben,", "tokens": ["Leicht", "glaubt", "Ihr", "zu", "er\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Was nie die Erde bot,", "tokens": ["Was", "nie", "die", "Er\u00b7de", "bot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und habt so doppelt Leben.", "tokens": ["Und", "habt", "so", "dop\u00b7pelt", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "F\u00fcr einen halben Tod.", "tokens": ["F\u00fcr", "ei\u00b7nen", "hal\u00b7ben", "Tod", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich aber habe leise", "tokens": ["Ich", "a\u00b7ber", "ha\u00b7be", "lei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Pforte mich genaht,", "tokens": ["Der", "Pfor\u00b7te", "mich", "ge\u00b7naht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die in die ew'gen Kreise", "tokens": ["Die", "in", "die", "ew'\u00b7gen", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Euch aufgethan den Pfad,", "tokens": ["Euch", "auf\u00b7ge\u00b7than", "den", "Pfad", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und all' die stumme Trauer,", "tokens": ["Und", "all'", "die", "stum\u00b7me", "Trau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die mir das Herz noch schwellt,", "tokens": ["Die", "mir", "das", "Herz", "noch", "schwellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Umschwebt als letzter Schauer", "tokens": ["Um\u00b7schwebt", "als", "letz\u00b7ter", "Schau\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Euch kalt aus dieser Welt.", "tokens": ["Euch", "kalt", "aus", "die\u00b7ser", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}