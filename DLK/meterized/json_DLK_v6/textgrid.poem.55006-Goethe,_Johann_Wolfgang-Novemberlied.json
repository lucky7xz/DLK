{"textgrid.poem.55006": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Novemberlied", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dem Sch\u00fctzen, doch dem alten nicht,", "tokens": ["Dem", "Sch\u00fct\u00b7zen", ",", "doch", "dem", "al\u00b7ten", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ART", "ADJA", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu dem die Sonne flieht,", "tokens": ["Zu", "dem", "die", "Son\u00b7ne", "flieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der uns ihr fernes Angesicht", "tokens": ["Der", "uns", "ihr", "fer\u00b7nes", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Wolken \u00fcberzieht,", "tokens": ["Mit", "Wol\u00b7ken", "\u00fc\u00b7berz\u00b7ieht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dem Knaben sei dies Lied geweiht,", "tokens": ["Dem", "Kna\u00b7ben", "sei", "dies", "Lied", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PDS", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der zwischen Rosen spielt,", "tokens": ["Der", "zwi\u00b7schen", "Ro\u00b7sen", "spielt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Uns h\u00f6ret und zur rechten Zeit", "tokens": ["Uns", "h\u00f6\u00b7ret", "und", "zur", "rech\u00b7ten", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach sch\u00f6nen Herzen zielt.", "tokens": ["Nach", "sch\u00f6\u00b7nen", "Her\u00b7zen", "zielt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Durch ihn hat uns des Winters Nacht,", "tokens": ["Durch", "ihn", "hat", "uns", "des", "Win\u00b7ters", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "So h\u00e4\u00dflich sonst und rauh,", "tokens": ["So", "h\u00e4\u00df\u00b7lich", "sonst", "und", "rauh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gar manchen werten Freund gebracht", "tokens": ["Gar", "man\u00b7chen", "wer\u00b7ten", "Freund", "ge\u00b7bracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und manche liebe Frau.", "tokens": ["Und", "man\u00b7che", "lie\u00b7be", "Frau", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Von nun an soll sein sch\u00f6nes Bild", "tokens": ["Von", "nun", "an", "soll", "sein", "sch\u00f6\u00b7nes", "Bild"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "APZR", "VMFIN", "PPOSAT", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Am Sternenhimmel stehn,", "tokens": ["Am", "Ster\u00b7nen\u00b7him\u00b7mel", "stehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und er soll ewig, hold und mild,", "tokens": ["Und", "er", "soll", "e\u00b7wig", ",", "hold", "und", "mild", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Uns auf- und untergehn.", "tokens": ["Uns", "auf", "und", "un\u00b7ter\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "TRUNC", "KON", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}