{"textgrid.poem.67394": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Das ertr\u00e4umte Paradies", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von Vater Adam's Lebensart,", "tokens": ["Von", "Va\u00b7ter", "A\u00b7dam's", "Le\u00b7ben\u00b7sart", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was ist uns \u00fcberblieben?", "tokens": ["Was", "ist", "uns", "\u00fc\u00b7berb\u00b7lie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie uns die Bibel offenbart,", "tokens": ["Wie", "uns", "die", "Bi\u00b7bel", "of\u00b7fen\u00b7bart", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hat ", "tokens": ["Hat"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Nur was dem Vater Kr\u00e4fte gab,", "tokens": ["Nur", "was", "dem", "Va\u00b7ter", "Kr\u00e4f\u00b7te", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ward bald den S\u00f6hnen Plage;", "tokens": ["Ward", "bald", "den", "S\u00f6h\u00b7nen", "Pla\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Fritz Adam spannt' am Morgen ab,", "tokens": ["Fritz", "A\u00b7dam", "spannt'", "am", "Mor\u00b7gen", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Franz Adam am Mittage.", "tokens": ["Franz", "A\u00b7dam", "am", "Mit\u00b7ta\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPRART", "NN", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Sie wanderten ins Paradeis", "tokens": ["Sie", "wan\u00b7der\u00b7ten", "ins", "Pa\u00b7ra\u00b7deis"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zur\u00fcck mit Weib und Kindern;", "tokens": ["Zu\u00b7r\u00fcck", "mit", "Weib", "und", "Kin\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da wandelt' auf Krystall und Eis", "tokens": ["Da", "wan\u00b7delt'", "auf", "Krys\u00b7tall", "und", "Eis"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Cherub, dies zu hindern.", "tokens": ["Ein", "Che\u00b7rub", ",", "dies", "zu", "hin\u00b7dern", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "PDS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sogleich umflo\u00df sie Nebelwahn", "tokens": ["Sog\u00b7leich", "um\u00b7flo\u00df", "sie", "Ne\u00b7bel\u00b7wahn"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vom neuen Weisheitsbaume;", "tokens": ["Vom", "neu\u00b7en", "Weis\u00b7heits\u00b7bau\u00b7me", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "In herrlichen Entw\u00fcrfen sahn", "tokens": ["In", "herr\u00b7li\u00b7chen", "Ent\u00b7w\u00fcr\u00b7fen", "sahn"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Eden sie \u2013 im Traume.", "tokens": ["Ein", "E\u00b7den", "sie", "\u2013", "im", "Trau\u00b7me", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "$(", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und naschten da nach Herzenslust", "tokens": ["Und", "naschten", "da", "nach", "Her\u00b7zens\u00b7lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "(was kann den Hunger stillen?)", "tokens": ["(", "was", "kann", "den", "Hun\u00b7ger", "stil\u00b7len", "?", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "VMFIN", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und kleideten sich schambewu\u00dft", "tokens": ["Und", "klei\u00b7de\u00b7ten", "sich", "scham\u00b7be\u00b7wu\u00dft"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Feigenbl\u00e4tterh\u00fcllen.", "tokens": ["In", "Fei\u00b7gen\u00b7bl\u00e4t\u00b7ter\u00b7h\u00fcl\u00b7len", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ihr Paradies gerieth zum Staat", "tokens": ["Ihr", "Pa\u00b7ra\u00b7dies", "ge\u00b7rieth", "zum", "Staat"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von viel Kategorieen;", "tokens": ["Von", "viel", "Ka\u00b7te\u00b7go\u00b7ri\u00b7een", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Die Distel sollte zum Salat,", "tokens": ["Die", "Dis\u00b7tel", "soll\u00b7te", "zum", "Sa\u00b7lat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Schleh zur Rose bl\u00fchen.", "tokens": ["Der", "Schleh", "zur", "Ro\u00b7se", "bl\u00fc\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Auch fanden sie der K\u00fcnste viel,", "tokens": ["Auch", "fan\u00b7den", "sie", "der", "K\u00fcns\u00b7te", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vor andern Schwert und Eisen,", "tokens": ["Vor", "an\u00b7dern", "Schwert", "und", "Ei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und priesen es mit Saitenspiel;", "tokens": ["Und", "prie\u00b7sen", "es", "mit", "Sai\u00b7ten\u00b7spiel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es war gar hoch zu preisen.", "tokens": ["Es", "war", "gar", "hoch", "zu", "prei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und mordeten freundbr\u00fcderlich", "tokens": ["Und", "mor\u00b7de\u00b7ten", "freund\u00b7br\u00fc\u00b7der\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.6": {"text": "Manch ehrlich frommen Abel", "tokens": ["Manch", "ehr\u00b7lich", "from\u00b7men", "A\u00b7bel"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und baueten gro\u00dfmeisterlich", "tokens": ["Und", "bau\u00b7e\u00b7ten", "gro\u00df\u00b7meis\u00b7ter\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.8": {"text": "Zum Himmel manches Babel.", "tokens": ["Zum", "Him\u00b7mel", "man\u00b7ches", "Ba\u00b7bel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ihr Adamskinder, auf! hinweg", "tokens": ["Ihr", "A\u00b7dams\u00b7kin\u00b7der", ",", "auf", "!", "hin\u00b7weg"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "$,", "PTKVZ", "$.", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vom langgeb\u00fc\u00dften Truge!", "tokens": ["Vom", "lang\u00b7ge\u00b7b\u00fc\u00df\u00b7ten", "Tru\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zum Gl\u00fccke f\u00fchrt ein enger Steg;", "tokens": ["Zum", "Gl\u00fc\u00b7cke", "f\u00fchrt", "ein", "en\u00b7ger", "Steg", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Frisch wieder hin zum Pfluge!", "tokens": ["Frisch", "wie\u00b7der", "hin", "zum", "Pflu\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Aus Eurer Mutter Lebensschoo\u00df", "tokens": ["Aus", "Eu\u00b7rer", "Mut\u00b7ter", "Le\u00b7bens\u00b7schoo\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kommt neue Kraft Euch wieder;", "tokens": ["Kommt", "neu\u00b7e", "Kraft", "Euch", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "PPER", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Nur Th\u00e4tigkeit ist unser Loos,", "tokens": ["Nur", "Th\u00e4\u00b7tig\u00b7keit", "ist", "un\u00b7ser", "Loos", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nur th\u00e4tig sind wir Br\u00fcder.", "tokens": ["Nur", "th\u00e4\u00b7tig", "sind", "wir", "Br\u00fc\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}