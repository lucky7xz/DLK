{"textgrid.poem.50474": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "62. Geschickligkeit \u00fcber Weissheit und Wissenschafft zu Hofe", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du must nicht ", "tokens": ["Du", "must", "nicht"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Wo du mit Sicherheit und unter gutem Schein,", "tokens": ["Wo", "du", "mit", "Si\u00b7cher\u00b7heit", "und", "un\u00b7ter", "gu\u00b7tem", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "KON", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Und die ", "tokens": ["Und", "die"], "token_info": ["word", "word"], "pos": ["KON", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Wer jedem hat gelernt ", "tokens": ["Wer", "je\u00b7dem", "hat", "ge\u00b7lernt"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "PIS", "VAFIN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der richtet hier gewiss mehr aus mit seinem Fleiss,", "tokens": ["Der", "rich\u00b7tet", "hier", "ge\u00b7wiss", "mehr", "aus", "mit", "sei\u00b7nem", "Fleiss", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "ADV", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Als der, ", "tokens": ["Als", "der", ","], "token_info": ["word", "word", "punct"], "pos": ["KOUS", "ART", "$,"], "meter": "+-", "measure": "trochaic.single"}}}}}