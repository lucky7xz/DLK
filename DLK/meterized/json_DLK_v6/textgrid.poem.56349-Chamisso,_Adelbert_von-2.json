{"textgrid.poem.56349": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "2", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbihn wird der Zorn des Himmels doch zertr\u00fcmmern.", "tokens": ["\u00bb", "ihn", "wird", "der", "Zorn", "des", "Him\u00b7mels", "doch", "zer\u00b7tr\u00fcm\u00b7mern", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gott hei\u00dft ", "tokens": ["Gott", "hei\u00dft"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Und l\u00e4\u00dft die Saat der S\u00fcnde nicht verk\u00fcmmern.\u00ab", "tokens": ["Und", "l\u00e4\u00dft", "die", "Saat", "der", "S\u00fcn\u00b7de", "nicht", "ver\u00b7k\u00fcm\u00b7mern", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "PTKNEG", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So klang es zu Jakutsk beim Sternenlichte", "tokens": ["So", "klang", "es", "zu", "Ja\u00b7kutsk", "beim", "Ster\u00b7nen\u00b7lich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NE", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "In kalter Nacht. Ein r\u00fcst'ger J\u00e4ger sang,", "tokens": ["In", "kal\u00b7ter", "Nacht", ".", "Ein", "r\u00fcst'\u00b7ger", "J\u00e4\u00b7ger", "sang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Gar seltnen Reiz verleihend dem Gedichte.", "tokens": ["Gar", "selt\u00b7nen", "Reiz", "ver\u00b7lei\u00b7hend", "dem", "Ge\u00b7dich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVPP", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ein fremdes Ohr belauschte den Gesang,", "tokens": ["Ein", "frem\u00b7des", "Ohr", "be\u00b7lauschte", "den", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Ein Mann, der j\u00fcngst, der Wissenschaft zu fr\u00f6nen,", "tokens": ["Ein", "Mann", ",", "der", "j\u00fcngst", ",", "der", "Wis\u00b7sen\u00b7schaft", "zu", "fr\u00f6\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Bis hieher in das Reich des Winters drang:", "tokens": ["Bis", "hie\u00b7her", "in", "das", "Reich", "des", "Win\u00b7ters", "drang", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PAV", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "\u00bbwer bist du, der die Nacht belebt mit T\u00f6nen?\u00ab \u2013", "tokens": ["\u00bb", "wer", "bist", "du", ",", "der", "die", "Nacht", "be\u00b7lebt", "mit", "T\u00f6\u00b7nen", "?", "\u00ab", "\u2013"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "PWS", "VAFIN", "PPER", "$,", "PRELS", "ART", "NN", "VVPP", "APPR", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "\u00bbwer du, der du mich fragst? das Lied ist mein,", "tokens": ["\u00bb", "wer", "du", ",", "der", "du", "mich", "fragst", "?", "das", "Lied", "ist", "mein", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "PPER", "$,", "PRELS", "PPER", "PRF", "VVFIN", "$.", "ART", "NN", "VAFIN", "PPOSAT", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Du wirst es nicht zu singen mich entw\u00f6hnen.\u00ab \u2013", "tokens": ["Du", "wirst", "es", "nicht", "zu", "sin\u00b7gen", "mich", "ent\u00b7w\u00f6h\u00b7nen", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "PTKZU", "VVFIN", "PPER", "VVINF", "$.", "$(", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "\u00bbgefraget hat ein Fremder dich allein,", "tokens": ["\u00bb", "ge\u00b7fra\u00b7get", "hat", "ein", "Frem\u00b7der", "dich", "al\u00b7lein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVPP", "VAFIN", "ART", "NN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Weil ihn des Liedes m\u00e4cht'ger Klang erfreute;", "tokens": ["Weil", "ihn", "des", "Lie\u00b7des", "m\u00e4cht'\u00b7ger", "Klang", "er\u00b7freu\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Es lag ihm fern, unfreundlich dir zu sein.\u00ab \u2013", "tokens": ["Es", "lag", "ihm", "fern", ",", "un\u00b7freund\u00b7lich", "dir", "zu", "sein", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,", "ADJD", "PPER", "PTKZU", "VAINF", "$.", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "\u00bbsei mir gegr\u00fc\u00dft, und nicht zum Argen deute", "tokens": ["\u00bb", "sei", "mir", "ge\u00b7gr\u00fc\u00dft", ",", "und", "nicht", "zum", "Ar\u00b7gen", "deu\u00b7te"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "VAFIN", "PPER", "VVPP", "$,", "KON", "PTKNEG", "APPRART", "NN", "VVFIN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.17": {"text": "Der ungeme\u00dfnen Rede fl\u00fccht'ge Hast,", "tokens": ["Der", "un\u00b7ge\u00b7me\u00df\u00b7nen", "Re\u00b7de", "fl\u00fccht'\u00b7ge", "Hast", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Dieweil mir stolz zu sein geziemet heute.", "tokens": ["Die\u00b7weil", "mir", "stolz", "zu", "sein", "ge\u00b7zie\u00b7met", "heu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PTKZU", "VAINF", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Komm in mein Haus, sei des Verbannten Gast;", "tokens": ["Komm", "in", "mein", "Haus", ",", "sei", "des", "Ver\u00b7bann\u00b7ten", "Gast", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.20": {"text": "Ich werde dir berichten sonder S\u00e4umen,", "tokens": ["Ich", "wer\u00b7de", "dir", "be\u00b7rich\u00b7ten", "son\u00b7der", "S\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVINF", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Was du zu wissen Lust bezeuget hast.", "tokens": ["Was", "du", "zu", "wis\u00b7sen", "Lust", "be\u00b7zeu\u00b7get", "hast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKZU", "VVINF", "NN", "VVFIN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Ich bin in dieses meines Grabes R\u00e4umen", "tokens": ["Ich", "bin", "in", "die\u00b7ses", "mei\u00b7nes", "Gra\u00b7bes", "R\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PDAT", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Ein freier Mann, und bin die Nachtigall,", "tokens": ["Ein", "frei\u00b7er", "Mann", ",", "und", "bin", "die", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Die hier alln\u00e4chtlich singt von ihren Tr\u00e4umen.", "tokens": ["Die", "hier", "all\u00b7n\u00e4cht\u00b7lich", "singt", "von", "ih\u00b7ren", "Tr\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Mir bleibt der freien Stimme voller Schall,", "tokens": ["Mir", "bleibt", "der", "frei\u00b7en", "Stim\u00b7me", "vol\u00b7ler", "Schall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Die volle Brust des ungebrochnen Mutes,", "tokens": ["Die", "vol\u00b7le", "Brust", "des", "un\u00b7ge\u00b7broch\u00b7nen", "Mu\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Und der ich bin, der bin ich \u00fcberall.", "tokens": ["Und", "der", "ich", "bin", ",", "der", "bin", "ich", "\u00fc\u00b7be\u00b7rall", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "VAFIN", "$,", "PRELS", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Die Erde lehrt mich und der Himmel tut es,", "tokens": ["Die", "Er\u00b7de", "lehrt", "mich", "und", "der", "Him\u00b7mel", "tut", "es", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "KON", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Die Sterne, welche kreisend zu mir sagen:", "tokens": ["Die", "Ster\u00b7ne", ",", "wel\u00b7che", "krei\u00b7send", "zu", "mir", "sa\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Es treibt uns unabl\u00e4ssig, nimmer ruht es.", "tokens": ["Es", "treibt", "uns", "un\u00b7ab\u00b7l\u00e4s\u00b7sig", ",", "nim\u00b7mer", "ruht", "es", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Sieh scheitelrecht dort \u00fcber dir den Wagen,", "tokens": ["Sieh", "schei\u00b7tel\u00b7recht", "dort", "\u00fc\u00b7ber", "dir", "den", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Noch lenkt er aufw\u00e4rts, strebet noch hinan,", "tokens": ["Noch", "lenkt", "er", "auf\u00b7w\u00e4rts", ",", "stre\u00b7bet", "noch", "hi\u00b7nan", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Um zu der Tiefe jenseits umzuschlagen.", "tokens": ["Um", "zu", "der", "Tie\u00b7fe", "jen\u00b7seits", "um\u00b7zu\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "ART", "NN", "ADV", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Ich bin zur Tiefe kommen meiner Bahn,", "tokens": ["Ich", "bin", "zur", "Tie\u00b7fe", "kom\u00b7men", "mei\u00b7ner", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Ich oder andre m\u00fcssen wieder steigen,", "tokens": ["Ich", "o\u00b7der", "and\u00b7re", "m\u00fcs\u00b7sen", "wie\u00b7der", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "PIS", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Und was ich tr\u00e4umte, war kein leerer Wahn.", "tokens": ["Und", "was", "ich", "tr\u00e4um\u00b7te", ",", "war", "kein", "lee\u00b7rer", "Wahn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "VAFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Das wird am Tag der V\u00f6lker bald sich zeigen,", "tokens": ["Das", "wird", "am", "Tag", "der", "V\u00f6l\u00b7ker", "bald", "sich", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPRART", "NN", "ART", "NN", "ADV", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Denn h\u00e4lt die Waage schwankend sich noch gleich,", "tokens": ["Denn", "h\u00e4lt", "die", "Waa\u00b7ge", "schwan\u00b7kend", "sich", "noch", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "PRF", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "So mu\u00df die volle Schale doch sich neigen.", "tokens": ["So", "mu\u00df", "die", "vol\u00b7le", "Scha\u00b7le", "doch", "sich", "nei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "ADV", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Gew\u00fcrfelt hab ich um ein Kaiserreich;", "tokens": ["Ge\u00b7w\u00fcr\u00b7felt", "hab", "ich", "um", "ein", "Kai\u00b7ser\u00b7reich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "Noch einmal ist der k\u00fchne Wurf mi\u00dflungen, \u2013", "tokens": ["Noch", "ein\u00b7mal", "ist", "der", "k\u00fch\u00b7ne", "Wurf", "mi\u00df\u00b7lun\u00b7gen", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "Er bot die Brust enbl\u00f6\u00dft dem Todesstreich!", "tokens": ["Er", "bot", "die", "Brust", "en\u00b7bl\u00f6\u00dft", "dem", "To\u00b7des\u00b7streich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "Ich bin ", "tokens": ["Ich", "bin"], "token_info": ["word", "word"], "pos": ["PPER", "VAFIN"], "meter": "-+", "measure": "iambic.single"}, "line.44": {"text": "Relejeffs Mitverschworenen genannt,", "tokens": ["Re\u00b7le\u00b7jeffs", "Mit\u00b7ver\u00b7schwo\u00b7re\u00b7nen", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVPP", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.45": {"text": "Dem er sein hohes Schwanenlied gesungen;", "tokens": ["Dem", "er", "sein", "ho\u00b7hes", "Schwa\u00b7nen\u00b7lied", "ge\u00b7sun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.46": {"text": "Das Lied von Woinarowski, wo entbrannt", "tokens": ["Das", "Lied", "von", "Woi\u00b7na\u00b7rows\u00b7ki", ",", "wo", "ent\u00b7brannt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "APPR", "NE", "$,", "PWAV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.47": {"text": "F\u00fcr Freiheit er sein Heiligstes gegeben,", "tokens": ["F\u00fcr", "Frei\u00b7heit", "er", "sein", "Hei\u00b7ligs\u00b7tes", "ge\u00b7ge\u00b7ben", ","], "token_info"