{"textgrid.poem.26858": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Es steht ein gro\u00df ger\u00e4umig Haus ...", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es steht ein gro\u00df ger\u00e4umig Haus", "tokens": ["Es", "steht", "ein", "gro\u00df", "ge\u00b7r\u00e4u\u00b7mig", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf unsichtbaren S\u00e4ulen,", "tokens": ["Auf", "un\u00b7sicht\u00b7ba\u00b7ren", "S\u00e4u\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Es mi\u00dfts und gehts kein Wandrer aus,", "tokens": ["Es", "mi\u00dfts", "und", "gehts", "kein", "Wand\u00b7rer", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und keiner darf drin weilen.", "tokens": ["Und", "kei\u00b7ner", "darf", "drin", "wei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nach einem unbegriffnen Plan", "tokens": ["Nach", "ei\u00b7nem", "un\u00b7be\u00b7griff\u00b7nen", "Plan"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ist es mit Kunst gezimmert,", "tokens": ["Ist", "es", "mit", "Kunst", "ge\u00b7zim\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "Es steckt sich selbst die Lampe an,", "tokens": ["Es", "steckt", "sich", "selbst", "die", "Lam\u00b7pe", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die es mit Pracht durchschimmert.", "tokens": ["Die", "es", "mit", "Pracht", "durch\u00b7schim\u00b7mert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Es hat ein Dach, kristallenrein,", "tokens": ["Es", "hat", "ein", "Dach", ",", "kris\u00b7tal\u00b7len\u00b7rein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Von einem einzgen Edelstein,", "tokens": ["Von", "ei\u00b7nem", "einz\u00b7gen", "E\u00b7del\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Doch noch kein Auge schaute", "tokens": ["Doch", "noch", "kein", "Au\u00b7ge", "schau\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Den Meister, der es baute.", "tokens": ["Den", "Meis\u00b7ter", ",", "der", "es", "bau\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}