{"textgrid.poem.41585": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am Sonntage nach Weihnachten", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An Jahren reif und an Geschicke", "tokens": ["An", "Jah\u00b7ren", "reif", "und", "an", "Ge\u00b7schi\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJD", "KON", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Blieb ich ein Kind vor Gottes Augen,", "tokens": ["Blieb", "ich", "ein", "Kind", "vor", "Got\u00b7tes", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein schlimmes Kind, voll schwacher T\u00fccke,", "tokens": ["Ein", "schlim\u00b7mes", "Kind", ",", "voll", "schwa\u00b7cher", "T\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die selber mir zu schaden taugen.", "tokens": ["Die", "sel\u00b7ber", "mir", "zu", "scha\u00b7den", "tau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nicht hat Erfahrung mich bereichert;", "tokens": ["Nicht", "hat", "Er\u00b7fah\u00b7rung", "mich", "be\u00b7rei\u00b7chert", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VAFIN", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "W\u00fcst ist mein Kopf, der Busen leer;", "tokens": ["W\u00fcst", "ist", "mein", "Kopf", ",", "der", "Bu\u00b7sen", "leer", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$,", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ach! keine Frucht hab' ich gespeichert", "tokens": ["Ach", "!", "kei\u00b7ne", "Frucht", "hab'", "ich", "ge\u00b7spei\u00b7chert"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PIAT", "NN", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und schau auch keine Saaten mehr!", "tokens": ["Und", "schau", "auch", "kei\u00b7ne", "Saa\u00b7ten", "mehr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ging so die teure Zeit verloren,", "tokens": ["Ging", "so", "die", "teu\u00b7re", "Zeit", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die \u00fcber Hoffen zugegeben", "tokens": ["Die", "\u00fc\u00b7ber", "Hof\u00b7fen", "zu\u00b7ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dem Wesen, was noch kaum geboren", "tokens": ["Dem", "We\u00b7sen", ",", "was", "noch", "kaum", "ge\u00b7bo\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schon schmerzlich k\u00e4mpfte um sein Leben!", "tokens": ["Schon", "schmerz\u00b7lich", "k\u00e4mpf\u00b7te", "um", "sein", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich, die den Tod seit Jahren f\u00fchle", "tokens": ["Ich", ",", "die", "den", "Tod", "seit", "Jah\u00b7ren", "f\u00fch\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sich langsam nagend bis ans Herz,", "tokens": ["Sich", "lang\u00b7sam", "na\u00b7gend", "bis", "ans", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVPP", "KON", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Weh mir! ich treibe Kinderspiele,", "tokens": ["Weh", "mir", "!", "ich", "trei\u00b7be", "Kin\u00b7der\u00b7spie\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Als sei der Sarg ein Mummenscherz.", "tokens": ["Als", "sei", "der", "Sarg", "ein", "Mum\u00b7men\u00b7scherz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "In siechen Kindes Haupte d\u00e4mmert", "tokens": ["In", "sie\u00b7chen", "Kin\u00b7des", "Haup\u00b7te", "d\u00e4m\u00b7mert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das unverstandne Mi\u00dfbehagen;", "tokens": ["Das", "un\u00b7ver\u00b7stand\u00b7ne", "Mi\u00df\u00b7be\u00b7ha\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So wenn der Grabwurm lauter h\u00e4mmert", "tokens": ["So", "wenn", "der", "Grab\u00b7wurm", "lau\u00b7ter", "h\u00e4m\u00b7mert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fchl' b\u00e4nger ich die Pulse schlagen.", "tokens": ["F\u00fchl'", "b\u00e4n\u00b7ger", "ich", "die", "Pul\u00b7se", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dann bricht hervor das matte St\u00f6hnen,", "tokens": ["Dann", "bricht", "her\u00b7vor", "das", "mat\u00b7te", "St\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der kranke, schmerzged\u00e4mpfte Schrei;", "tokens": ["Der", "kran\u00b7ke", ",", "schmerz\u00b7ge\u00b7d\u00e4mpf\u00b7te", "Schrei", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich lange mit des Wurmes Dehnen", "tokens": ["Ich", "lan\u00b7ge", "mit", "des", "Wur\u00b7mes", "Deh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sehns\u00fcchtig nach der Arzenei.", "tokens": ["Sehn\u00b7s\u00fcch\u00b7tig", "nach", "der", "Ar\u00b7ze\u00b7nei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch wenn ein frischer Hauch die welke", "tokens": ["Doch", "wenn", "ein", "fri\u00b7scher", "Hauch", "die", "wel\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Todsieche Nessel hat ber\u00fchret:", "tokens": ["Tod\u00b7sie\u00b7che", "Nes\u00b7sel", "hat", "be\u00b7r\u00fch\u00b7ret", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "VVPP", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Dann h\u00e4lt sie sich wie Ros' und Nelke", "tokens": ["Dann", "h\u00e4lt", "sie", "sich", "wie", "Ros'", "und", "Nel\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "KOKOM", "NE", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und meint sich k\u00f6niglich gezieret.", "tokens": ["Und", "meint", "sich", "k\u00f6\u00b7nig\u00b7lich", "ge\u00b7zie\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "O Leichtsinn, Leichtsinn sondergleichen", "tokens": ["O", "Leicht\u00b7sinn", ",", "Leicht\u00b7sinn", "son\u00b7derg\u00b7lei\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$,", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Als ob kein Seufzer ihn gest\u00f6rt!", "tokens": ["Als", "ob", "kein", "Seuf\u00b7zer", "ihn", "ge\u00b7st\u00f6rt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PIAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und doch mu\u00df ich vor Gram erbleichen,", "tokens": ["Und", "doch", "mu\u00df", "ich", "vor", "Gram", "er\u00b7blei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Durch meine Seele ging ein Schwert.", "tokens": ["Durch", "mei\u00b7ne", "See\u00b7le", "ging", "ein", "Schwert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wer mu\u00dft' so vieles Leid erfahren", "tokens": ["Wer", "mu\u00dft'", "so", "vie\u00b7les", "Leid", "er\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "ADV", "PIS", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "An K\u00f6rpernot und Seelenleiden", "tokens": ["An", "K\u00f6r\u00b7per\u00b7not", "und", "See\u00b7len\u00b7lei\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und dennoch in so langen Jahren", "tokens": ["Und", "den\u00b7noch", "in", "so", "lan\u00b7gen", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sich von der Welt nicht mochte scheiden?", "tokens": ["Sich", "von", "der", "Welt", "nicht", "moch\u00b7te", "schei\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ob er als Frevler sich dem Rade,", "tokens": ["Ob", "er", "als", "Frev\u00b7ler", "sich", "dem", "Ra\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOUS", "NN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Als Tor geselle sich dem Spott:", "tokens": ["Als", "Tor", "ge\u00b7sel\u00b7le", "sich", "dem", "Spott", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "O sei barmherzig ew'ge Gnade,", "tokens": ["O", "sei", "barm\u00b7her\u00b7zig", "ew'\u00b7ge", "Gna\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Richt' ihn als Toren milder Gott!", "tokens": ["Richt'", "ihn", "als", "To\u00b7ren", "mil\u00b7der", "Gott", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOUS", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Du hast sein siedend Hirn gebildet,", "tokens": ["Du", "hast", "sein", "sie\u00b7dend", "Hirn", "ge\u00b7bil\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "CARD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Nerven rastlos flatternd Spielen", "tokens": ["Der", "Ner\u00b7ven", "rast\u00b7los", "flat\u00b7ternd", "Spie\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VVPP", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht von gesundem Blut geschildet;", "tokens": ["Nicht", "von", "ge\u00b7sun\u00b7dem", "Blut", "ge\u00b7schil\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wei\u00dft seine dumpfe Angst zu f\u00fchlen,", "tokens": ["Wei\u00dft", "sei\u00b7ne", "dump\u00b7fe", "Angst", "zu", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn er sich windet unter Schlingen,", "tokens": ["Wenn", "er", "sich", "win\u00b7det", "un\u00b7ter", "Schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zu m\u00e4chtig ihm, und doch verha\u00dft,", "tokens": ["Zu", "m\u00e4ch\u00b7tig", "ihm", ",", "und", "doch", "ver\u00b7ha\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "PPER", "$,", "KON", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Er gern ein Opfer m\u00f6chte bringen,", "tokens": ["Er", "gern", "ein", "Op\u00b7fer", "m\u00f6ch\u00b7te", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn es nur seine Hand erfa\u00dft.", "tokens": ["Wenn", "es", "nur", "sei\u00b7ne", "Hand", "er\u00b7fa\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Was S\u00fcnde war, du wirst es richten,", "tokens": ["Was", "S\u00fcn\u00b7de", "war", ",", "du", "wirst", "es", "rich\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "$,", "PPER", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und meine Strafe mu\u00df ich tragen;", "tokens": ["Und", "mei\u00b7ne", "Stra\u00b7fe", "mu\u00df", "ich", "tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und was Verwirrung wirst du schlichten", "tokens": ["Und", "was", "Ver\u00b7wir\u00b7rung", "wirst", "du", "schlich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "NN", "VAFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weit gn\u00e4d'ger, als ich d\u00fcrfte sagen.", "tokens": ["Weit", "gn\u00e4d'\u00b7ger", ",", "als", "ich", "d\u00fcrf\u00b7te", "sa\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "$,", "KOUS", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn klar das Haupt, die F\u00e4den l\u00f6ser,", "tokens": ["Wenn", "klar", "das", "Haupt", ",", "die", "F\u00e4\u00b7den", "l\u00f6\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Was dann mein Teil, ich wei\u00df es nicht;", "tokens": ["Was", "dann", "mein", "Teil", ",", "ich", "wei\u00df", "es", "nicht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Jetzt kann ich stammeln nur: Erl\u00f6ser!", "tokens": ["Jetzt", "kann", "ich", "stam\u00b7meln", "nur", ":", "Er\u00b7l\u00f6\u00b7ser", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "ADV", "$.", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ich gebe mich in dein Gericht!", "tokens": ["Ich", "ge\u00b7be", "mich", "in", "dein", "Ge\u00b7richt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}