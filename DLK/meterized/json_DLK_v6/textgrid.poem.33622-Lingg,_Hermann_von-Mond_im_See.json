{"textgrid.poem.33622": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Mond im See", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcber H\u00f6hn, die dunkel liegen,", "tokens": ["\u00dc\u00b7ber", "H\u00f6hn", ",", "die", "dun\u00b7kel", "lie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Leuchtend in sein Geisterreich", "tokens": ["Leuch\u00b7tend", "in", "sein", "Geis\u00b7ter\u00b7reich"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kommt der Mond heraufgestiegen,", "tokens": ["Kommt", "der", "Mond", "her\u00b7auf\u00b7ge\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einer Feuerlilie gleich.", "tokens": ["Ei\u00b7ner", "Feu\u00b7er\u00b7li\u00b7lie", "gleich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "H\u00f6her schwebend, immer blasser", "tokens": ["H\u00f6\u00b7her", "schwe\u00b7bend", ",", "im\u00b7mer", "blas\u00b7ser"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "ADJD", "$,", "ADV", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird sein Licht im Nachtazur,", "tokens": ["Wird", "sein", "Licht", "im", "Nach\u00b7ta\u00b7zur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Aber unter ihm die Wasser", "tokens": ["A\u00b7ber", "un\u00b7ter", "ihm", "die", "Was\u00b7ser"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Strahlen umso sch\u00f6ner nur.", "tokens": ["Strah\u00b7len", "um\u00b7so", "sch\u00f6\u00b7ner", "nur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sanft aus ewigem Gefilde", "tokens": ["Sanft", "aus", "e\u00b7wi\u00b7gem", "Ge\u00b7fil\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blickt sein Glanz, wie ein Gem\u00fct,", "tokens": ["Blickt", "sein", "Glanz", ",", "wie", "ein", "Ge\u00b7m\u00fct", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das sich selbst bezwang und milde", "tokens": ["Das", "sich", "selbst", "be\u00b7zwang", "und", "mil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PRF", "ADV", "ADJD", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nun in reinster Regung gl\u00fcht.", "tokens": ["Nun", "in", "reins\u00b7ter", "Re\u00b7gung", "gl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Du verh\u00fcllst dich \u2013\u2013 und ein Schatten", "tokens": ["Du", "ver\u00b7h\u00fcllst", "dich", "\u2013", "\u2013", "und", "ein", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$(", "$(", "KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dunkelt um die Wellen weit,", "tokens": ["Dun\u00b7kelt", "um", "die", "Wel\u00b7len", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die durch dich geleuchtet hatten,", "tokens": ["Die", "durch", "dich", "ge\u00b7leuch\u00b7tet", "hat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stolzer Stern der Einsamkeit.", "tokens": ["Stol\u00b7zer", "Stern", "der", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}