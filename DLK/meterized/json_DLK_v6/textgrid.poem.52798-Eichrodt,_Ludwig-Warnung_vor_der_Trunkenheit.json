{"textgrid.poem.52798": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Warnung vor der Trunkenheit", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer in dem goldnen Saft der Reben", "tokens": ["Wer", "in", "dem", "gold\u00b7nen", "Saft", "der", "Re\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit frechem Muthe sich betrinkt,", "tokens": ["Mit", "fre\u00b7chem", "Mu\u00b7the", "sich", "be\u00b7trinkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der ist nicht werth, als Mensch zu leben,", "tokens": ["Der", "ist", "nicht", "werth", ",", "als", "Mensch", "zu", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKNEG", "ADJD", "$,", "KOUS", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn er als Thier zu Boden sinkt.", "tokens": ["Wenn", "er", "als", "Thier", "zu", "Bo\u00b7den", "sinkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOUS", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihm ist kein Mann von Ehre hold,", "tokens": ["Ihm", "ist", "kein", "Mann", "von", "Eh\u00b7re", "hold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Verachtung dir, o Trunkenbold!", "tokens": ["Ver\u00b7ach\u00b7tung", "dir", ",", "o", "Trun\u00b7ken\u00b7bold", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So manchen Gatten, treu und bieder,", "tokens": ["So", "man\u00b7chen", "Gat\u00b7ten", ",", "treu", "und", "bie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hat schon die Trunkenheit beth\u00f6rt,", "tokens": ["Hat", "schon", "die", "Trun\u00b7ken\u00b7heit", "be\u00b7th\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gottlob, da\u00df uns ein solcher wieder", "tokens": ["Gott\u00b7lob", ",", "da\u00df", "uns", "ein", "sol\u00b7cher", "wie\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "ART", "PIAT", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zur N\u00fcchternheit zur\u00fcckgekehrt,", "tokens": ["Zur", "N\u00fcch\u00b7tern\u00b7heit", "zu\u00b7r\u00fcck\u00b7ge\u00b7kehrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Kein And'rer war so l\u00fcderlich,", "tokens": ["Kein", "An\u00b7d'\u00b7rer", "war", "so", "l\u00fc\u00b7der\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Doch seit Jakobi macht' er sich.", "tokens": ["Doch", "seit", "Ja\u00b7ko\u00b7bi", "macht'", "er", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Gott l\u00e4\u00dft den Hausstand oft zerr\u00fctten,", "tokens": ["Gott", "l\u00e4\u00dft", "den", "Haus\u00b7stand", "oft", "zer\u00b7r\u00fct\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Um mit dem Kreuz zu winken uns,", "tokens": ["Um", "mit", "dem", "Kreuz", "zu", "win\u00b7ken", "uns", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "ART", "NN", "PTKZU", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch lie\u00df er sich durch Reu erbitten", "tokens": ["Doch", "lie\u00df", "er", "sich", "durch", "Reu", "er\u00b7bit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PRF", "APPR", "NE", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erst eben k\u00fcrzlich bei Karl Kunz.", "tokens": ["Erst", "e\u00b7ben", "k\u00fcrz\u00b7lich", "bei", "Karl", "Kunz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "APPR", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es war die h\u00f6chste Zeit, das Amt", "tokens": ["Es", "war", "die", "h\u00f6chs\u00b7te", "Zeit", ",", "das", "Amt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bedrohte Kunzen schon mit Gant.", "tokens": ["Be\u00b7droh\u00b7te", "Kun\u00b7zen", "schon", "mit", "Gant", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Drum preiset ihn, der die Gesch\u00f6pfe", "tokens": ["Drum", "prei\u00b7set", "ihn", ",", "der", "die", "Ge\u00b7sch\u00f6p\u00b7fe"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht gerne ganz versinken l\u00e4\u00dft,", "tokens": ["Nicht", "ger\u00b7ne", "ganz", "ver\u00b7sin\u00b7ken", "l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sichtbarlich sind des Herrn Fu\u00dfst\u00e4pfe,", "tokens": ["Sicht\u00b7bar\u00b7lich", "sind", "des", "Herrn", "Fu\u00df\u00b7st\u00e4p\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-++--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "So tretet drein, und haltet fest,", "tokens": ["So", "tre\u00b7tet", "drein", ",", "und", "hal\u00b7tet", "fest", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ja, haltet fest an seinem Pfad, \u2013", "tokens": ["Ja", ",", "hal\u00b7tet", "fest", "an", "sei\u00b7nem", "Pfad", ",", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Christ geht n\u00fcchtern und gerad.", "tokens": ["Der", "Christ", "geht", "n\u00fcch\u00b7tern", "und", "ge\u00b7rad", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}