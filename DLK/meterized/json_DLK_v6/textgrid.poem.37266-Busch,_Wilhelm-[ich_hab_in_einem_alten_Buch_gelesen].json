{"textgrid.poem.37266": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[ich hab in einem alten Buch gelesen]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hab in einem alten Buch gelesen", "tokens": ["Ich", "hab", "in", "ei\u00b7nem", "al\u00b7ten", "Buch", "ge\u00b7le\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von einem J\u00fcngling, welcher schlimm gewesen.", "tokens": ["Von", "ei\u00b7nem", "J\u00fcng\u00b7ling", ",", "wel\u00b7cher", "schlimm", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADJD", "VAPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er streut sein Hab und Gut in alle Winde.", "tokens": ["Er", "streut", "sein", "Hab", "und", "Gut", "in", "al\u00b7le", "Win\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "ADJD", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von Lust zu L\u00fcsten und von S\u00fcnd zu S\u00fcnde,", "tokens": ["Von", "Lust", "zu", "L\u00fcs\u00b7ten", "und", "von", "S\u00fcnd", "zu", "S\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "KON", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "In tollem Drang, in schrankenlosem Streben", "tokens": ["In", "tol\u00b7lem", "Drang", ",", "in", "schran\u00b7ken\u00b7lo\u00b7sem", "Stre\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Spornt er sein Ro\u00df hinein ins wilde Leben,", "tokens": ["Spornt", "er", "sein", "Ro\u00df", "hin\u00b7ein", "ins", "wil\u00b7de", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Bis ihn ein j\u00e4her Sturz vom Felsenrand", "tokens": ["Bis", "ihn", "ein", "j\u00e4\u00b7her", "Sturz", "vom", "Fel\u00b7sen\u00b7rand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Dahingestreckt in Sand und Sonnenbrand,", "tokens": ["Da\u00b7hin\u00b7ge\u00b7streckt", "in", "Sand", "und", "Son\u00b7nen\u00b7brand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Da\u00df Str\u00f6me Bluts aus seinem Munde dringen", "tokens": ["Da\u00df", "Str\u00f6\u00b7me", "Bluts", "aus", "sei\u00b7nem", "Mun\u00b7de", "drin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "NN", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und jede Hoffnung fast erloschen ist.", "tokens": ["Und", "je\u00b7de", "Hoff\u00b7nung", "fast", "er\u00b7lo\u00b7schen", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Ich aber hoffe \u2013 sagt hier der Chronist \u2013", "tokens": ["Ich", "a\u00b7ber", "hof\u00b7fe", "\u2013", "sagt", "hier", "der", "Chro\u00b7nist", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$(", "VVFIN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Die Gnade leiht dem J\u00fcngling ihre Schwingen.", "tokens": ["Die", "Gna\u00b7de", "leiht", "dem", "J\u00fcng\u00b7ling", "ih\u00b7re", "Schwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Im selben Buche hab ich auch gelesen", "tokens": ["Im", "sel\u00b7ben", "Bu\u00b7che", "hab", "ich", "auch", "ge\u00b7le\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von einem Manne, der honett gewesen.", "tokens": ["Von", "ei\u00b7nem", "Man\u00b7ne", ",", "der", "ho\u00b7nett", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADJD", "VAPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es war ein Mann, den die Gemeinde ehrte,", "tokens": ["Es", "war", "ein", "Mann", ",", "den", "die", "Ge\u00b7mein\u00b7de", "ehr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der so von sechs bis acht sein Sch\u00f6ppchen leerte,", "tokens": ["Der", "so", "von", "sechs", "bis", "acht", "sein", "Sch\u00f6ppc\u00b7hen", "leer\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "CARD", "APPR", "CARD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der aus Prinzip nie einem etwas borgte,", "tokens": ["Der", "aus", "Prin\u00b7zip", "nie", "ei\u00b7nem", "et\u00b7was", "borg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "ADV", "ART", "PIS", "VVFIN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Der emsig nur f\u00fcr Frau und Kinder sorgte;", "tokens": ["Der", "em\u00b7sig", "nur", "f\u00fcr", "Frau", "und", "Kin\u00b7der", "sorg\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Dazu ein proprer Mann, der nie geflucht,", "tokens": ["Da\u00b7zu", "ein", "prop\u00b7rer", "Mann", ",", "der", "nie", "ge\u00b7flucht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der seine Kirche musterhaft besucht.", "tokens": ["Der", "sei\u00b7ne", "Kir\u00b7che", "mus\u00b7ter\u00b7haft", "be\u00b7sucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Kurzum, er hielt sein R\u00f6\u00dflein stramm im Z\u00fcgel,", "tokens": ["Kur\u00b7zum", ",", "er", "hielt", "sein", "R\u00f6\u00df\u00b7lein", "stramm", "im", "Z\u00fc\u00b7gel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und war, wie man so sagt, ein guter Christ.", "tokens": ["Und", "war", ",", "wie", "man", "so", "sagt", ",", "ein", "gu\u00b7ter", "Christ", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "PWAV", "PIS", "ADV", "VVFIN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Ich f\u00fcrchte nur \u2013 bemerkt hier der Chronist \u2013", "tokens": ["Ich", "f\u00fcrch\u00b7te", "nur", "\u2013", "be\u00b7merkt", "hier", "der", "Chro\u00b7nist", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$(", "VVFIN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Dem Biedermanne wachsen keine Fl\u00fcgel.", "tokens": ["Dem", "Bie\u00b7der\u00b7man\u00b7ne", "wach\u00b7sen", "kei\u00b7ne", "Fl\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}