{"textgrid.poem.43986": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du hast mich klug genug probiert", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du hast mich klug genug probiert", "tokens": ["Du", "hast", "mich", "klug", "ge\u00b7nug", "pro\u00b7biert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und kennst, mein Kind, mein z\u00e4rtlich Lieben;", "tokens": ["Und", "kennst", ",", "mein", "Kind", ",", "mein", "z\u00e4rt\u00b7lich", "Lie\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "ADJD", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So scharf du mich herum gef\u00fchrt,", "tokens": ["So", "scharf", "du", "mich", "he\u00b7rum", "ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APZR", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So fest ist Wuntsch und Treu verblieben,", "tokens": ["So", "fest", "ist", "Wunt\u00b7sch", "und", "Treu", "ver\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Da nichts als Phillis in der Welt", "tokens": ["Da", "nichts", "als", "Phil\u00b7lis", "in", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "KOKOM", "NE", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mir noch die Sterbenslust verg\u00e4llt.", "tokens": ["Mir", "noch", "die", "Ster\u00b7bens\u00b7lust", "ver\u00b7g\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Aus dieser s\u00fc\u00dfen Redligkeit", "tokens": ["Aus", "die\u00b7ser", "s\u00fc\u00b7\u00dfen", "Red\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Entspringt nunmehr mein traurig Wesen;", "tokens": ["Ent\u00b7springt", "nun\u00b7mehr", "mein", "trau\u00b7rig", "We\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du f\u00fchlst ja wohl mein zitternd Leid", "tokens": ["Du", "f\u00fchlst", "ja", "wohl", "mein", "zit\u00b7ternd", "Leid"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und kanst es aus der Stirne lesen.", "tokens": ["Und", "kanst", "es", "aus", "der", "Stir\u00b7ne", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Was macht es? Dein verwegner Schritt,", "tokens": ["Was", "macht", "es", "?", "Dein", "ver\u00b7weg\u00b7ner", "Schritt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der hurtig an das Ufer tritt.", "tokens": ["Der", "hur\u00b7tig", "an", "das", "U\u00b7fer", "tritt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dein Abschied lockt dich auf das Meer;", "tokens": ["Dein", "Ab\u00b7schied", "lockt", "dich", "auf", "das", "Meer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich d\u00f6rfte dich bald th\u00f6richt nennen.", "tokens": ["Ich", "d\u00f6rf\u00b7te", "dich", "bald", "th\u00f6\u00b7richt", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-++--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Wo nimmstu das Vertrauen her?", "tokens": ["Wo", "nimms\u00b7tu", "das", "Ver\u00b7trau\u00b7en", "her", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du must das Wa\u00dfer noch nicht kennen;", "tokens": ["Du", "must", "das", "Wa\u00b7\u00dfer", "noch", "nicht", "ken\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ach, hat man dir noch nicht erzehlt,", "tokens": ["Ach", ",", "hat", "man", "dir", "noch", "nicht", "er\u00b7zehlt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VAFIN", "PIS", "PPER", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was Hero vor ein Grab gewehlt?", "tokens": ["Was", "He\u00b7ro", "vor", "ein", "Grab", "ge\u00b7wehlt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Trennung thut mir freylich weh,", "tokens": ["Die", "Tren\u00b7nung", "thut", "mir", "frey\u00b7lich", "weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch f\u00fcrcht ich mehr um deinetwegen.", "tokens": ["Doch", "f\u00fcrcht", "ich", "mehr", "um", "dei\u00b7net\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Was wird dir nicht die wilde See", "tokens": ["Was", "wird", "dir", "nicht", "die", "wil\u00b7de", "See"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor Eckel, Schmerz und Angst erregen,", "tokens": ["Vor", "E\u00b7ckel", ",", "Schmerz", "und", "Angst", "er\u00b7re\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn Wetter, Sturm und Bliz und Nacht", "tokens": ["Wenn", "Wet\u00b7ter", ",", "Sturm", "und", "Bliz", "und", "Nacht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "$,", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Compa\u00df und Mast zu Schanden macht!", "tokens": ["Com\u00b7pa\u00df", "und", "Mast", "zu", "Schan\u00b7den", "macht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Geh in dich, allerliebster Schaz,", "tokens": ["Geh", "in", "dich", ",", "al\u00b7ler\u00b7liebs\u00b7ter", "Schaz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und untersuche dein Gewi\u00dfen;", "tokens": ["Und", "un\u00b7ter\u00b7su\u00b7che", "dein", "Ge\u00b7wi\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hier ist der Rache Richterplaz,", "tokens": ["Hier", "ist", "der", "Ra\u00b7che", "Rich\u00b7ter\u00b7plaz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hier mu\u00df der kleinste Meineid b\u00fc\u00dfen.", "tokens": ["Hier", "mu\u00df", "der", "kleins\u00b7te", "Mei\u00b7neid", "b\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wer weis, wie oft auch meine Treu", "tokens": ["Wer", "weis", ",", "wie", "oft", "auch", "mei\u00b7ne", "Treu"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "PTKVZ", "$,", "PWAV", "ADV", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von dir bisher beleidigt sey!", "tokens": ["Von", "dir", "bis\u00b7her", "be\u00b7lei\u00b7digt", "sey", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ist aber ja kein Halten mehr,", "tokens": ["Ist", "a\u00b7ber", "ja", "kein", "Hal\u00b7ten", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So seegle mit geneigten Winden!", "tokens": ["So", "seeg\u00b7le", "mit", "ge\u00b7neig\u00b7ten", "Win\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Himmel giebt auch mir Geh\u00f6r,", "tokens": ["Der", "Him\u00b7mel", "giebt", "auch", "mir", "Ge\u00b7h\u00f6r", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du wirst den Hafen gl\u00fccklich finden;", "tokens": ["Du", "wirst", "den", "Ha\u00b7fen", "gl\u00fcck\u00b7lich", "fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Doch, Engel, denck auch stets an den,", "tokens": ["Doch", ",", "En\u00b7gel", ",", "denck", "auch", "stets", "an", "den", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NE", "$,", "PRELS", "ADV", "ADV", "APPR", "ART", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Den Stern und Ufer warthen sehn.", "tokens": ["Den", "Stern", "und", "U\u00b7fer", "wart\u00b7hen", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}