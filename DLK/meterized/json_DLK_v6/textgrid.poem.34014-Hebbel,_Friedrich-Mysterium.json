{"textgrid.poem.34014": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Mysterium", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O, k\u00f6nnte ich den Faden doch gewinnen,", "tokens": ["O", ",", "k\u00f6nn\u00b7te", "ich", "den", "Fa\u00b7den", "doch", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VMFIN", "PPER", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der, mich mit Gott und der Natur verkn\u00fcpfend,", "tokens": ["Der", ",", "mich", "mit", "Gott", "und", "der", "Na\u00b7tur", "ver\u00b7kn\u00fcp\u00b7fend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRF", "APPR", "NN", "KON", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und, abgewickelt, das Geheimste l\u00fcpfend,", "tokens": ["Und", ",", "ab\u00b7ge\u00b7wi\u00b7ckelt", ",", "das", "Ge\u00b7heims\u00b7te", "l\u00fcp\u00b7fend", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVPP", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verborgen sitzt im Geist und in den Sinnen!", "tokens": ["Ver\u00b7bor\u00b7gen", "sitzt", "im", "Geist", "und", "in", "den", "Sin\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "KON", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie wollte ich ihn muthig r\u00fcckw\u00e4rts spinnen,", "tokens": ["Wie", "woll\u00b7te", "ich", "ihn", "mut\u00b7hig", "r\u00fcck\u00b7w\u00e4rts", "spin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PPER", "ADJD", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bis er mir, endlich von der Spindel h\u00fcpfend,", "tokens": ["Bis", "er", "mir", ",", "end\u00b7lich", "von", "der", "Spin\u00b7del", "h\u00fcp\u00b7fend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "$,", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und in den Mittelpunct hin\u00fcber schl\u00fcpfend,", "tokens": ["Und", "in", "den", "Mit\u00b7tel\u00b7punct", "hin\u00b7\u00fc\u00b7ber", "schl\u00fcp\u00b7fend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gezeigt, wie All und Ich in Eins zerrinnen.", "tokens": ["Ge\u00b7zeigt", ",", "wie", "All", "und", "Ich", "in", "Eins", "zer\u00b7rin\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWAV", "NN", "KON", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nur f\u00fcrchte ich, da\u00df, wie ich selbst Gedanken,", "tokens": ["Nur", "f\u00fcrch\u00b7te", "ich", ",", "da\u00df", ",", "wie", "ich", "selbst", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "$,", "PWAV", "PPER", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die gleich Kometen blitzten, schon erstickte,", "tokens": ["Die", "gleich", "Ko\u00b7me\u00b7ten", "blitz\u00b7ten", ",", "schon", "er\u00b7stick\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "VVFIN", "$,", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Eh' ich verging in ihrem gl\u00fch'nden Lichte,", "tokens": ["Eh'", "ich", "ver\u00b7ging", "in", "ih\u00b7rem", "gl\u00fch'n\u00b7den", "Lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So auch das All ein Ich, das, seiner Schranken", "tokens": ["So", "auch", "das", "All", "ein", "Ich", ",", "das", ",", "sei\u00b7ner", "Schran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "ART", "PPER", "$,", "PDS", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vergessen, an das Welten-R\u00e4thsel tickte,", "tokens": ["Ver\u00b7ges\u00b7sen", ",", "an", "das", "Wel\u00b7ten\u00b7R\u00e4th\u00b7sel", "tick\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aus Nothwehr, eh' es tiefer dringt, vernichte.", "tokens": ["Aus", "Noth\u00b7wehr", ",", "eh'", "es", "tie\u00b7fer", "dringt", ",", "ver\u00b7nich\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}