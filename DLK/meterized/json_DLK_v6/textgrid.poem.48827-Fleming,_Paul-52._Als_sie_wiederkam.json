{"textgrid.poem.48827": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "52. Als sie wiederkam", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Luft hat ausgeweint, der Himmel l\u00e4\u00dft den Flor", "tokens": ["Die", "Luft", "hat", "aus\u00b7ge\u00b7weint", ",", "der", "Him\u00b7mel", "l\u00e4\u00dft", "den", "Flor"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "der schwarzen Wolken ab, der Sturm, der ist vor\u00fcber,", "tokens": ["der", "schwar\u00b7zen", "Wol\u00b7ken", "ab", ",", "der", "Sturm", ",", "der", "ist", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,", "ART", "NN", "$,", "PRELS", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "der West bef\u00e4llt den Wald mit einem sanften Fieber,", "tokens": ["der", "West", "be\u00b7f\u00e4llt", "den", "Wald", "mit", "ei\u00b7nem", "sanf\u00b7ten", "Fie\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "die hohe Sonne hebt ihr sch\u00f6nes H\u00e4upt empor", "tokens": ["die", "ho\u00b7he", "Son\u00b7ne", "hebt", "ihr", "sch\u00f6\u00b7nes", "H\u00e4upt", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "und f\u00fchret mit sich auf der Blumen ganzen Chor.", "tokens": ["und", "f\u00fch\u00b7ret", "mit", "sich", "auf", "der", "Blu\u00b7men", "gan\u00b7zen", "Chor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PRF", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Lust ist lustiger, die Liebe selbst scheint lieber,", "tokens": ["Die", "Lust", "ist", "lus\u00b7ti\u00b7ger", ",", "die", "Lie\u00b7be", "selbst", "scheint", "lie\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "ADV", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und stellt sich itzund Nichts, als die Betr\u00fcbn\u00fc\u00df tr\u00fcber,", "tokens": ["und", "stellt", "sich", "it\u00b7zund", "Nichts", ",", "als", "die", "Be\u00b7tr\u00fcb\u00b7n\u00fc\u00df", "tr\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "PIS", "$,", "KOUS", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "die froh bei Trauren wird und tut sich nicht hervor.", "tokens": ["die", "froh", "bei", "Trau\u00b7ren", "wird", "und", "tut", "sich", "nicht", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "NN", "VAFIN", "KON", "VVFIN", "PRF", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich auch, wie krank ich bin nun in den f\u00fcnften Tag,", "tokens": ["Ich", "auch", ",", "wie", "krank", "ich", "bin", "nun", "in", "den", "f\u00fcnf\u00b7ten", "Tag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "empfind' ein stilles Heil durch alle meine Glieder,", "tokens": ["emp\u00b7find'", "ein", "stil\u00b7les", "Heil", "durch", "al\u00b7le", "mei\u00b7ne", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "so da\u00df ich, der ich vor in Ohnmacht ganz darnieder,", "tokens": ["so", "da\u00df", "ich", ",", "der", "ich", "vor", "in", "Ohn\u00b7macht", "ganz", "dar\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "$,", "PRELS", "PPER", "APPR", "APPR", "NN", "ADV", "PAV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "bei hei\u00dfer Herzensangst in kaltem Schwei\u00dfe lag,", "tokens": ["bei", "hei\u00b7\u00dfer", "Her\u00b7zen\u00b7sangst", "in", "kal\u00b7tem", "Schwei\u00b7\u00dfe", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "nun wieder Kost und Lust und frisch und froh sein mag.", "tokens": ["nun", "wie\u00b7der", "Kost", "und", "Lust", "und", "frisch", "und", "froh", "sein", "mag."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "ADV", "NN", "KON", "NN", "KON", "ADJD", "KON", "ADJD", "VAINF", "VMFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Woher kans anders sein, als da\u00df mein Trost kommt wieder?", "tokens": ["Wo\u00b7her", "kans", "an\u00b7ders", "sein", ",", "als", "da\u00df", "mein", "Trost", "kommt", "wie\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ADV", "VAINF", "$,", "KOKOM", "KOUS", "PPOSAT", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}