{"textgrid.poem.33987": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An des Kaisers von Oesterreich Majest\u00e4t", "genre": "verse", "period": "N.A.", "pub_year": 1853, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "War auch der M\u00f6rder, welcher, tief verblendet,", "tokens": ["War", "auch", "der", "M\u00f6r\u00b7der", ",", "wel\u00b7cher", ",", "tief", "ver\u00b7blen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$,", "PRELS", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den meuchlerischen Stahl auf Dich gez\u00fcckt,", "tokens": ["Den", "meuch\u00b7le\u00b7ri\u00b7schen", "Stahl", "auf", "Dich", "ge\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Bote, den die H\u00f6lle selbst gesendet,", "tokens": ["Ein", "Bo\u00b7te", ",", "den", "die", "H\u00f6l\u00b7le", "selbst", "ge\u00b7sen\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nachdem sie ihn im Innersten ber\u00fcckt,", "tokens": ["Nach\u00b7dem", "sie", "ihn", "im", "In\u00b7ners\u00b7ten", "be\u00b7r\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "So hat es doch der Himmel so gewendet,", "tokens": ["So", "hat", "es", "doch", "der", "Him\u00b7mel", "so", "ge\u00b7wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df jetzt ihn die Apostelkrone schm\u00fcckt,", "tokens": ["Da\u00df", "jetzt", "ihn", "die", "A\u00b7pos\u00b7tel\u00b7kro\u00b7ne", "schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Denn Kunde hat der Herr durch ihn gegeben:", "tokens": ["Denn", "Kun\u00b7de", "hat", "der", "Herr", "durch", "ihn", "ge\u00b7ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ART", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Gefeit ist, weil geweiht, des Kaisers Leben!", "tokens": ["Ge\u00b7feit", "ist", ",", "weil", "ge\u00b7weiht", ",", "des", "Kai\u00b7sers", "Le\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "KOUS", "VVPP", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nun darfst Du doppelt auf Dich selbst vertrauen,", "tokens": ["Nun", "darfst", "Du", "dop\u00b7pelt", "auf", "Dich", "selbst", "ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und doppelt hoffen darf auf Dich die Welt,", "tokens": ["Und", "dop\u00b7pelt", "hof\u00b7fen", "darf", "auf", "Dich", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVINF", "VMFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Dichter aber blickt mit heil'gem Grauen", "tokens": ["Der", "Dich\u00b7ter", "a\u00b7ber", "blickt", "mit", "heil'\u00b7gem", "Grau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In Deine Zukunft, die sich ihm erhellt,", "tokens": ["In", "Dei\u00b7ne", "Zu\u00b7kunft", ",", "die", "sich", "ihm", "er\u00b7hellt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "PRF", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Du wirst, er glaubt's den Thron auf's Neue bauen,", "tokens": ["Du", "wirst", ",", "er", "glaubt's", "den", "Thron", "auf's", "Neu\u00b7e", "bau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PPER", "VVFIN", "ART", "NN", "APPRART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Den Karl der Gro\u00dfe einst so hoch gestellt,", "tokens": ["Den", "Karl", "der", "Gro\u00b7\u00dfe", "einst", "so", "hoch", "ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ART", "ADJA", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Denn soll's noch einmal auf der Erde tagen,", "tokens": ["Denn", "soll's", "noch", "ein\u00b7mal", "auf", "der", "Er\u00b7de", "ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "So mu\u00df das Herz Europas wieder schlagen!", "tokens": ["So", "mu\u00df", "das", "Herz", "Eu\u00b7ro\u00b7pas", "wie\u00b7der", "schla\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "NE", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So schmiede denn mit einer eh'rnen Klammer", "tokens": ["So", "schmie\u00b7de", "denn", "mit", "ei\u00b7ner", "eh'r\u00b7nen", "Klam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das eig'ne fest an's alte Deutsche Reich;", "tokens": ["Das", "eig'\u00b7ne", "fest", "an's", "al\u00b7te", "Deut\u00b7sche", "Reich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJD", "APPRART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dann endest Du den allgemeinen Jammer", "tokens": ["Dann", "en\u00b7dest", "Du", "den", "all\u00b7ge\u00b7mei\u00b7nen", "Jam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und den des edlen Deutschen Volks zugleich:", "tokens": ["Und", "den", "des", "ed\u00b7len", "Deut\u00b7schen", "Volks", "zu\u00b7gleich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "ADJA", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wo dr\u00e4ngt sich auch durch eine Herzensklammer", "tokens": ["Wo", "dr\u00e4ngt", "sich", "auch", "durch", "ei\u00b7ne", "Her\u00b7zens\u00b7klam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PRF", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Das Blut und l\u00e4\u00dft die and're leer und bleich?", "tokens": ["Das", "Blut", "und", "l\u00e4\u00dft", "die", "an\u00b7d'\u00b7re", "leer", "und", "bleich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVFIN", "ART", "ADJA", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+----+-+", "measure": "unknown.measure.tetra"}, "line.7": {"text": "Durch alle beide mu\u00df es wechselnd fluten,", "tokens": ["Durch", "al\u00b7le", "bei\u00b7de", "mu\u00df", "es", "wech\u00b7selnd", "flu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PIS", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dann weckt es die verborg'nen Lebensgluten!", "tokens": ["Dann", "weckt", "es", "die", "ver\u00b7bor\u00b7g'\u00b7nen", "Le\u00b7bens\u00b7glu\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Und liegt das alte Reich auch tief darnieder,", "tokens": ["Und", "liegt", "das", "al\u00b7te", "Reich", "auch", "tief", "dar\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ADV", "ADJD", "PAV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Wink von Dir, und es erhebt sich schon,", "tokens": ["Ein", "Wink", "von", "Dir", ",", "und", "es", "er\u00b7hebt", "sich", "schon", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "$,", "KON", "PPER", "VVFIN", "PRF", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es starb ja nicht an seiner eig'nen Hyder,", "tokens": ["Es", "starb", "ja", "nicht", "an", "sei\u00b7ner", "eig'\u00b7nen", "Hy\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es ward zermalmt durch einen G\u00f6ttersohn,", "tokens": ["Es", "ward", "zer\u00b7malmt", "durch", "ei\u00b7nen", "G\u00f6t\u00b7ter\u00b7sohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "In C\u00e4sar kehrte Alexander wieder", "tokens": ["In", "C\u00e4\u00b7sar", "kehr\u00b7te", "A\u00b7lex\u00b7an\u00b7der", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "VVFIN", "NE", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und alle Beide in Napoleon,", "tokens": ["Und", "al\u00b7le", "Bei\u00b7de", "in", "Na\u00b7po\u00b7le\u00b7on", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PIS", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und sehen wir den Erdball selber schwanken,", "tokens": ["Und", "se\u00b7hen", "wir", "den", "Erd\u00b7ball", "sel\u00b7ber", "schwan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "So darf auch ohne Schmach die Eiche wanken!", "tokens": ["So", "darf", "auch", "oh\u00b7ne", "Schmach", "die", "Ei\u00b7che", "wan\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "APPR", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Es glich dem d\u00fcstern Helden jener Sage,", "tokens": ["Es", "glich", "dem", "d\u00fcs\u00b7tern", "Hel\u00b7den", "je\u00b7ner", "Sa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der seine Feinde nicht blo\u00df \u00fcberwand,", "tokens": ["Der", "sei\u00b7ne", "Fein\u00b7de", "nicht", "blo\u00df", "\u00fc\u00b7ber\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nein, der sich auch zu seiner eig'nen Klage", "tokens": ["Nein", ",", "der", "sich", "auch", "zu", "sei\u00b7ner", "eig'\u00b7nen", "Kla\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PRELS", "PRF", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nach jedem Siege doppelt st\u00e4rker fand,", "tokens": ["Nach", "je\u00b7dem", "Sie\u00b7ge", "dop\u00b7pelt", "st\u00e4r\u00b7ker", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So da\u00df er an dem Abend seiner Tage", "tokens": ["So", "da\u00df", "er", "an", "dem", "A\u00b7bend", "sei\u00b7ner", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Kraft der Welt in sich zusammen band,", "tokens": ["Die", "Kraft", "der", "Welt", "in", "sich", "zu\u00b7sam\u00b7men", "band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und, da ihm doch beschieden war, zu enden,", "tokens": ["Und", ",", "da", "ihm", "doch", "be\u00b7schie\u00b7den", "war", ",", "zu", "en\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "ADV", "VVPP", "VAFIN", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Den Tod erlitt von aller G\u00f6tter H\u00e4nden!", "tokens": ["Den", "Tod", "er\u00b7litt", "von", "al\u00b7ler", "G\u00f6t\u00b7ter", "H\u00e4n\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Drum ist, was ihm erlag, nur halb erlegen,", "tokens": ["Drum", "ist", ",", "was", "ihm", "er\u00b7lag", ",", "nur", "halb", "er\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "$,", "PWS", "PPER", "VVFIN", "$,", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es sank bet\u00e4ubt, doch war es nicht erschlafft,", "tokens": ["Es", "sank", "be\u00b7t\u00e4ubt", ",", "doch", "war", "es", "nicht", "er\u00b7schlafft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVPP", "$,", "ADV", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Scheintod selbst, er ward vielleicht zum Segen,", "tokens": ["Der", "Schein\u00b7tod", "selbst", ",", "er", "ward", "viel\u00b7leicht", "zum", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PPER", "VAFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sogar ein Traum entz\u00fcndet oft die Kraft,", "tokens": ["So\u00b7gar", "ein", "Traum", "ent\u00b7z\u00fcn\u00b7det", "oft", "die", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Auch seh'n wir manchen Zwerg sich wieder regen,", "tokens": ["Auch", "seh'n", "wir", "man\u00b7chen", "Zwerg", "sich", "wie\u00b7der", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "PRF", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der keck und trotzig sich empor gerafft:", "tokens": ["Der", "keck", "und", "trot\u00b7zig", "sich", "em\u00b7por", "ge\u00b7rafft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "PRF", "PTKVZ", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Was schl\u00e4ft denn noch der erste aller Recken?", "tokens": ["Was", "schl\u00e4ft", "denn", "noch", "der", "ers\u00b7te", "al\u00b7ler", "Re\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ADV", "ART", "ADJA", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ber\u00fchr' ihn, Herr, ein Habsburg kann ihn wecken!", "tokens": ["Be\u00b7r\u00fchr'", "ihn", ",", "Herr", ",", "ein", "Habs\u00b7burg", "kann", "ihn", "we\u00b7cken", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "NN", "$,", "ART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}