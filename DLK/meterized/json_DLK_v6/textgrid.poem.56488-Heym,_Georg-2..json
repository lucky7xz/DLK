{"textgrid.poem.56488": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hoch wo das Dunkel seine Schatten t\u00fcrmt", "tokens": ["Hoch", "wo", "das", "Dun\u00b7kel", "sei\u00b7ne", "Schat\u00b7ten", "t\u00fcrmt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "PWAV", "ART", "NN", "PPOSAT", "NN", "VVFIN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Durch Ewigkeiten fern vom Grund der Qual,", "tokens": ["Durch", "E\u00b7wig\u00b7kei\u00b7ten", "fern", "vom", "Grund", "der", "Qual", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hoch oben, wo im Dom der Regen st\u00fcrmt,", "tokens": ["Hoch", "o\u00b7ben", ",", "wo", "im", "Dom", "der", "Re\u00b7gen", "st\u00fcrmt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "$,", "PWAV", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Erscheint des Gottes Haupt, wie Morgen fahl.", "tokens": ["Er\u00b7scheint", "des", "Got\u00b7tes", "Haupt", ",", "wie", "Mor\u00b7gen", "fahl", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,", "PWAV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die weiten Kirchen f\u00fcllt der Sph\u00e4ren Traum", "tokens": ["Die", "wei\u00b7ten", "Kir\u00b7chen", "f\u00fcllt", "der", "Sph\u00e4\u00b7ren", "Traum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Voll Schweigen, das wie leise Harfen klingt,", "tokens": ["Voll", "Schwei\u00b7gen", ",", "das", "wie", "lei\u00b7se", "Har\u00b7fen", "klingt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "PRELS", "KOKOM", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da, wie der Mond vom gro\u00dfen Himmelsraum,", "tokens": ["Da", ",", "wie", "der", "Mond", "vom", "gro\u00b7\u00dfen", "Him\u00b7mels\u00b7raum", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Des Gottes wei\u00dfes Haupt heruntersinkt.", "tokens": ["Des", "Got\u00b7tes", "wei\u00b7\u00dfes", "Haupt", "her\u00b7un\u00b7ter\u00b7sinkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Tretet heran. Sein Mund ist s\u00fc\u00df wie Frucht,", "tokens": ["Tre\u00b7tet", "he\u00b7ran", ".", "Sein", "Mund", "ist", "s\u00fc\u00df", "wie", "Frucht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$.", "PPOSAT", "NN", "VAFIN", "ADJD", "KOKOM", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Sein Blut ist, wie der Wein, langsam und schwer.", "tokens": ["Sein", "Blut", "ist", ",", "wie", "der", "Wein", ",", "lang\u00b7sam", "und", "schwer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "$,", "PWAV", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auf seiner Lippen dunkelroter Bucht", "tokens": ["Auf", "sei\u00b7ner", "Lip\u00b7pen", "dun\u00b7kel\u00b7ro\u00b7ter", "Bucht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wiegt blaue Glut von fernem Sommermeer.", "tokens": ["Wiegt", "blau\u00b7e", "Glut", "von", "fer\u00b7nem", "Som\u00b7mer\u00b7meer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Tretet heran. Wie Flaum von Faltern zart,", "tokens": ["Tre\u00b7tet", "he\u00b7ran", ".", "Wie", "Flaum", "von", "Fal\u00b7tern", "zart", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$.", "PWAV", "NN", "APPR", "NN", "ADJD", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Wie eines jungen Sternes goldne Nacht,", "tokens": ["Wie", "ei\u00b7nes", "jun\u00b7gen", "Ster\u00b7nes", "gold\u00b7ne", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zittert sein Mund, in seinem goldnen Bart,", "tokens": ["Zit\u00b7tert", "sein", "Mund", ",", "in", "sei\u00b7nem", "gold\u00b7nen", "Bart", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Wie Chrysolith in einem tiefen Schacht.", "tokens": ["Wie", "Chry\u00b7so\u00b7lith", "in", "ei\u00b7nem", "tie\u00b7fen", "Schacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Tretet heran. Wie einer Schlange Haut", "tokens": ["Tre\u00b7tet", "he\u00b7ran", ".", "Wie", "ei\u00b7ner", "Schlan\u00b7ge", "Haut"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "$.", "PWAV", "ART", "NN", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "So k\u00fchl ist er, weich wie ein Purpurkleid,", "tokens": ["So", "k\u00fchl", "ist", "er", ",", "weich", "wie", "ein", "Pur\u00b7pur\u00b7kleid", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "$,", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Wie Abendrot so sanft, das \u00fcbergraut", "tokens": ["Wie", "A\u00b7ben\u00b7drot", "so", "sanft", ",", "das", "\u00fc\u00b7berg\u00b7raut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "NN", "ADV", "ADJD", "$,", "PRELS", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Brennender Liebe wildes Herzeleid.", "tokens": ["Bren\u00b7nen\u00b7der", "Lie\u00b7be", "wil\u00b7des", "Her\u00b7ze\u00b7leid", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.6": {"line.1": {"text": "Der Gram gefallner Engel ruht, ein Traum,", "tokens": ["Der", "Gram", "ge\u00b7fall\u00b7ner", "En\u00b7gel", "ruht", ",", "ein", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf seiner Stirn, der Qualen wei\u00dfem Thron,", "tokens": ["Auf", "sei\u00b7ner", "Stirn", ",", "der", "Qua\u00b7len", "wei\u00b7\u00dfem", "Thron", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie Schl\u00e4fer traurig, denen floh zum Saum", "tokens": ["Wie", "Schl\u00e4\u00b7fer", "trau\u00b7rig", ",", "de\u00b7nen", "floh", "zum", "Saum"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "ADJD", "$,", "PRELS", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Des blassen Morgens ihre Vision.", "tokens": ["Des", "blas\u00b7sen", "Mor\u00b7gens", "ih\u00b7re", "Vi\u00b7si\u00b7on", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Tiefer als tausend leere Himmel tief", "tokens": ["Tie\u00b7fer", "als", "tau\u00b7send", "lee\u00b7re", "Him\u00b7mel", "tief"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "CARD", "ADJA", "NN", "ADJD"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Ist seine Schwermut, wie die H\u00f6lle sch\u00f6n,", "tokens": ["Ist", "sei\u00b7ne", "Schwer\u00b7mut", ",", "wie", "die", "H\u00f6l\u00b7le", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "PWAV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo in den roten Abgrund sich verlief", "tokens": ["Wo", "in", "den", "ro\u00b7ten", "Ab\u00b7grund", "sich", "ver\u00b7lief"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein bleicher Sonnenstrahl aus Mittagsh\u00f6hn.", "tokens": ["Ein", "blei\u00b7cher", "Son\u00b7nen\u00b7strahl", "aus", "Mit\u00b7tags\u00b7h\u00f6hn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Sein Leid ist wie ein Leuchter in der Nacht,", "tokens": ["Sein", "Leid", "ist", "wie", "ein", "Leuch\u00b7ter", "in", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "KOKOM", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Schauet die Flamme, die sein Haupt umloht,", "tokens": ["Schau\u00b7et", "die", "Flam\u00b7me", ",", "die", "sein", "Haupt", "um\u00b7loht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Und doppelh\u00f6rnig in der d\u00fcstren Pracht", "tokens": ["Und", "dop\u00b7pel\u00b7h\u00f6r\u00b7nig", "in", "der", "d\u00fcst\u00b7ren", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Aus seinem Lockenwald ins Dunkel droht.", "tokens": ["Aus", "sei\u00b7nem", "Lo\u00b7cken\u00b7wald", "ins", "Dun\u00b7kel", "droht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Sein Leid ist wie ein Teppich, drauf die Schrift", "tokens": ["Sein", "Leid", "ist", "wie", "ein", "Tep\u00b7pich", ",", "drauf", "die", "Schrift"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "KOKOM", "ART", "NN", "$,", "PAV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Kabbalisten brennt durch Dunkelheit,", "tokens": ["Der", "Kab\u00b7ba\u00b7lis\u00b7ten", "brennt", "durch", "Dun\u00b7kel\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Eiland, dem \u2039vorbei\u203a ein Segler schifft,", "tokens": ["Ein", "Ei\u00b7land", ",", "dem", "\u2039", "vor\u00b7bei", "\u203a", "ein", "Seg\u00b7ler", "schifft", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "$(", "PTKVZ", "$(", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wenn in den Bergen fern das Einhorn schreit.", "tokens": ["Wenn", "in", "den", "Ber\u00b7gen", "fern", "das", "Ein\u00b7horn", "schreit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Sein Leib tr\u00e4gt eines Schattenwaldes Duft,", "tokens": ["Sein", "Leib", "tr\u00e4gt", "ei\u00b7nes", "Schat\u00b7ten\u00b7wal\u00b7des", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo gro\u00dfer S\u00fcmpfe Trauerv\u00f6gel ziehn,", "tokens": ["Wo", "gro\u00b7\u00dfer", "S\u00fcmp\u00b7fe", "Trau\u00b7er\u00b7v\u00f6\u00b7gel", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein K\u00f6nig, der durch seiner Ahnen Gruft", "tokens": ["Ein", "K\u00f6\u00b7nig", ",", "der", "durch", "sei\u00b7ner", "Ah\u00b7nen", "Gruft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nachdenklich geht in wei\u00dfem Hermelin.", "tokens": ["Nach\u00b7denk\u00b7lich", "geht", "in", "wei\u00b7\u00dfem", "Her\u00b7me\u00b7lin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Tretet heran, entflammt von seinem Gram.", "tokens": ["Tre\u00b7tet", "he\u00b7ran", ",", "ent\u00b7flammt", "von", "sei\u00b7nem", "Gram", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Trinkt seinen Atem, der so k\u00fchl wie Eis,", "tokens": ["Trinkt", "sei\u00b7nen", "A\u00b7tem", ",", "der", "so", "k\u00fchl", "wie", "Eis", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "PRELS", "ADV", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der \u00fcber tausend Paradiese kam,", "tokens": ["Der", "\u00fc\u00b7ber", "tau\u00b7send", "Pa\u00b7ra\u00b7die\u00b7se", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Voll Duft, der jeden Kummer wei\u00df.", "tokens": ["Voll", "Duft", ",", "der", "je\u00b7den", "Kum\u00b7mer", "wei\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Er l\u00e4chelt, seht. Und eurer Seele Bild", "tokens": ["Er", "l\u00e4\u00b7chelt", ",", "seht", ".", "Und", "eu\u00b7rer", "See\u00b7le", "Bild"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$.", "KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wird wie ein Weiher, der im Schilfe schweigt,", "tokens": ["Wird", "wie", "ein", "Wei\u00b7her", ",", "der", "im", "Schil\u00b7fe", "schweigt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo leis des Hirtengottes Fl\u00f6te schwillt,", "tokens": ["Wo", "leis", "des", "Hir\u00b7ten\u00b7got\u00b7tes", "Fl\u00f6\u00b7te", "schwillt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der durch die Lorbeerschlucht heruntersteigt.", "tokens": ["Der", "durch", "die", "Lor\u00b7beer\u00b7schlucht", "her\u00b7un\u00b7ter\u00b7steigt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "V