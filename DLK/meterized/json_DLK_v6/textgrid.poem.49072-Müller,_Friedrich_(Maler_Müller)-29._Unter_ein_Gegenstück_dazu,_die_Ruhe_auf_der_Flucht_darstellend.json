{"textgrid.poem.49072": {"metadata": {"author": {"name": "M\u00fcller, Friedrich (Maler M\u00fcller)", "birth": "N.A.", "death": "N.A."}, "title": "29. Unter ein Gegenst\u00fcck dazu, die Ruhe auf der Flucht darstellend", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und wenn die D\u00e4mm'rung jetzt auf rauhem Pfade", "tokens": ["Und", "wenn", "die", "D\u00e4m\u00b7m'\u00b7rung", "jetzt", "auf", "rau\u00b7hem", "Pfa\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Unschuld \u00fcberf\u00e4llt, l\u00e4\u00dft friedlich sie", "tokens": ["Die", "Un\u00b7schuld", "\u00fc\u00b7berf\u00b7\u00e4llt", ",", "l\u00e4\u00dft", "fried\u00b7lich", "sie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "VVFIN", "ADJD", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bei erstem Schirm sich nieder;", "tokens": ["Bei", "ers\u00b7tem", "Schirm", "sich", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Denn inn're Furcht, die schn\u00f6der That entsprie\u00dft,", "tokens": ["Denn", "inn'\u00b7re", "Furcht", ",", "die", "schn\u00f6\u00b7der", "That", "ent\u00b7sprie\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Beschwert nicht ihre Brust, sie schlummert", "tokens": ["Be\u00b7schwert", "nicht", "ih\u00b7re", "Brust", ",", "sie", "schlum\u00b7mert"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PTKNEG", "PPOSAT", "NN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In s\u00fc\u00dfer Ruhe bald und \u00fcber ihr", "tokens": ["In", "s\u00fc\u00b7\u00dfer", "Ru\u00b7he", "bald", "und", "\u00fc\u00b7ber", "ihr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADV", "KON", "APPR", "PPOSAT"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Er\u00f6ffnet sich der Himmel.", "tokens": ["Er\u00b7\u00f6ff\u00b7net", "sich", "der", "Him\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}