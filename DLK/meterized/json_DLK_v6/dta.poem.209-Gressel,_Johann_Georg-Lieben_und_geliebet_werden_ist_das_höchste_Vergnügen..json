{"dta.poem.209": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Lieben und geliebet werden ist das h\u00f6chste  \n Vergn\u00fcgen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Was ist Vergn\u00fcglichers im gantzen Rund der Erden/", "tokens": ["Was", "ist", "Ver\u00b7gn\u00fcg\u00b7li\u00b7chers", "im", "gant\u00b7zen", "Rund", "der", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "NN", "APPRART", "ADJA", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als Lieben/ und zugleich mit Ernst geliebet werden", "tokens": ["Als", "Lie\u00b7ben", "/", "und", "zu\u00b7gleich", "mit", "Ernst", "ge\u00b7lie\u00b7bet", "wer\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "$(", "KON", "ADV", "APPR", "NE", "VVPP", "VAINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was ist annehmlichers als ein ", "tokens": ["Was", "ist", "an\u00b7nehm\u00b7li\u00b7chers", "als", "ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PIS", "KOKOM", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den reine Liebe schenckt aus innerm Hertzen-Flu\u00df/", "tokens": ["Den", "rei\u00b7ne", "Lie\u00b7be", "schenckt", "aus", "in\u00b7nerm", "Hert\u00b7zen\u00b7Flu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was ist erquickender als sch\u00f6ne Brust Granaten/", "tokens": ["Was", "ist", "er\u00b7qui\u00b7cken\u00b7der", "als", "sch\u00f6\u00b7ne", "Brust", "Gra\u00b7na\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADJD", "KOKOM", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Worinnen Milch und Blut zur K\u00fchlung hingerahten.", "tokens": ["Wo\u00b7rin\u00b7nen", "Milch", "und", "Blut", "zur", "K\u00fch\u00b7lung", "hin\u00b7ge\u00b7rah\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Was ist bezauberndes als die gew\u00f6lbte Schoo\u00df?", "tokens": ["Was", "ist", "be\u00b7zau\u00b7bern\u00b7des", "als", "die", "ge\u00b7w\u00f6lb\u00b7te", "Schoo\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PIS", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die uns entz\u00fccket macht der satten Sinnen lo\u00df.", "tokens": ["Die", "uns", "ent\u00b7z\u00fc\u00b7cket", "macht", "der", "sat\u00b7ten", "Sin\u00b7nen", "lo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Was ist verzuckerter als feuriges Umhalsen?", "tokens": ["Was", "ist", "ver\u00b7zu\u00b7cker\u00b7ter", "als", "feu\u00b7ri\u00b7ges", "Um\u00b7hal\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADJD", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.10": {"text": "Das Honig-Kuchen macht aus bittern Wermuths-Salsen.", "tokens": ["Das", "Ho\u00b7nig\u00b7Ku\u00b7chen", "macht", "aus", "bit\u00b7tern", "Wer\u00b7muths\u00b7Sal\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Was ist anmuhtiger als ein ", "tokens": ["Was", "ist", "an\u00b7muh\u00b7ti\u00b7ger", "als", "ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ADJD", "KOKOM", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Von zarten Helffenbein zur N\u00e4chte Zeit-Vertreib?", "tokens": ["Von", "zar\u00b7ten", "Helf\u00b7fen\u00b7bein", "zur", "N\u00e4ch\u00b7te", "Zeit\u00b7Ver\u00b7treib", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Was ist gew\u00fcnscheters als Leib an Leiber leimen/", "tokens": ["Was", "ist", "ge\u00b7w\u00fcn\u00b7sche\u00b7ters", "als", "Leib", "an", "Lei\u00b7ber", "lei\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "KOUS", "NN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und feuchten Perlen-Thau in Liebes-Muscheln sch\u00e4umen?", "tokens": ["Und", "feuch\u00b7ten", "Per\u00b7len\u00b7\u00b7T\u00b7hau", "in", "Lie\u00b7bes\u00b7Mu\u00b7scheln", "sch\u00e4u\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Was ist entz\u00fcckender als in der Muschel ruhn/", "tokens": ["Was", "ist", "ent\u00b7z\u00fc\u00b7cken\u00b7der", "als", "in", "der", "Mu\u00b7schel", "ruhn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADJD", "KOKOM", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wo Lust und Kitzelung der Wollust-Thor auf thun?", "tokens": ["Wo", "Lust", "und", "Kit\u00b7ze\u00b7lung", "der", "Wol\u00b7lust\u00b7Thor", "auf", "thun", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "ART", "NN", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Was ist begierlicher als da den Eintritt nehmen?", "tokens": ["Was", "ist", "be\u00b7gier\u00b7li\u00b7cher", "als", "da", "den", "Ein\u00b7tritt", "neh\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADJD", "KOKOM", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Wo ", "tokens": ["Wo"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}, "line.19": {"text": "Nichts ist Vergn\u00fcglichers/ nichts das mehr Wollust schafft/", "tokens": ["Nichts", "ist", "Ver\u00b7gn\u00fcg\u00b7li\u00b7chers", "/", "nichts", "das", "mehr", "Wol\u00b7lust", "schafft", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "NN", "$(", "PIS", "ART", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Als wenn nur gleicher Will in beyder Hertzen hafft.", "tokens": ["Als", "wenn", "nur", "glei\u00b7cher", "Will", "in", "bey\u00b7der", "Hert\u00b7zen", "hafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ADV", "ADJA", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Nichts ist/ das mehr erquickt/ da\u00df mehr die Geister blendet/", "tokens": ["Nichts", "ist", "/", "das", "mehr", "er\u00b7quickt", "/", "da\u00df", "mehr", "die", "Geis\u00b7ter", "blen\u00b7det", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "$(", "PDS", "ADV", "VVPP", "$(", "KOUS", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Als wenn man seine Brunst im Schoo\u00df zur K\u00fchlung sendet/", "tokens": ["Als", "wenn", "man", "sei\u00b7ne", "Brunst", "im", "Schoo\u00df", "zur", "K\u00fch\u00b7lung", "sen\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PIS", "PPOSAT", "NN", "APPRART", "NN", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Nichts ist verzuckerter/ nichts kommt gew\u00fcnschter an/", "tokens": ["Nichts", "ist", "ver\u00b7zu\u00b7cker\u00b7ter", "/", "nichts", "kommt", "ge\u00b7w\u00fcnschter", "an", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADJD", "$(", "PIS", "VVFIN", "ADJD", "PTKVZ", "$("], "meter": "-+--+-++--+", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "Als wenn man in der Lieb sich recht erg\u00f6tzen kan.", "tokens": ["Als", "wenn", "man", "in", "der", "Lieb", "sich", "recht", "er\u00b7g\u00f6t\u00b7zen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PIS", "APPR", "ART", "NN", "PRF", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}