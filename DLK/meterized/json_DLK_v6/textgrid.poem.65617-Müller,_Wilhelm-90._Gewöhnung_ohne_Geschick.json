{"textgrid.poem.65617": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "90. Gew\u00f6hnung ohne Geschick", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auch die links gew\u00f6hnten Knaben", "tokens": ["Auch", "die", "links", "ge\u00b7w\u00f6hn\u00b7ten", "Kna\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eine gute Rechte haben.", "tokens": ["Ei\u00b7ne", "gu\u00b7te", "Rech\u00b7te", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00fc\u00dfte Jeder, was er kann,", "tokens": ["W\u00fc\u00df\u00b7te", "Je\u00b7der", ",", "was", "er", "kann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "PWS", "PPER", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dummkopf fing' es kl\u00fcger an.", "tokens": ["Dumm\u00b7kopf", "fing'", "es", "kl\u00fc\u00b7ger", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}