: "trochaic.tetra"}, "line.4": {"text": "Fehlten B\u00e4rte den Balbiren", "tokens": ["Fehl\u00b7ten", "B\u00e4r\u00b7te", "den", "Bal\u00b7bi\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "W\u00fcrden sie viel Brod verlieren/", "tokens": ["W\u00fcr\u00b7den", "sie", "viel", "Brod", "ver\u00b7lie\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lieber tragt den B\u00e4rten Gunst.", "tokens": ["Lie\u00b7ber", "tragt", "den", "B\u00e4r\u00b7ten", "Gunst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}