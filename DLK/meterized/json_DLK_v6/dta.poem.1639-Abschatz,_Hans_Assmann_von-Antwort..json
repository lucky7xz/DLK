{"dta.poem.1639": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Antwort.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Spotte wer da will den Bart: ", "tokens": ["Spot\u00b7te", "wer", "da", "will", "den", "Bart", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWS", "ADV", "VMFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Knaben bleiben unvollkommen/", "tokens": ["Kna\u00b7ben", "blei\u00b7ben", "un\u00b7voll\u00b7kom\u00b7men", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bi\u00df der Bart hat zugenommen:", "tokens": ["Bi\u00df", "der", "Bart", "hat", "zu\u00b7ge\u00b7nom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kahle Jugend mu\u00df sich sch\u00e4men/", "tokens": ["Kah\u00b7le", "Ju\u00b7gend", "mu\u00df", "sich", "sch\u00e4\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "PRF", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Glattes Mauls ein Weib zu nehmen/", "tokens": ["Glat\u00b7tes", "Mauls", "ein", "Weib", "zu", "neh\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bi\u00df sich Bart und Witz gepaart.", "tokens": ["Bi\u00df", "sich", "Bart