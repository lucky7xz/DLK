{"dta.poem.3159": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Erhebe dich, mein Herz, mit Wogenschlag, und gleiche", "tokens": ["Er\u00b7he\u00b7be", "dich", ",", "mein", "Herz", ",", "mit", "Wo\u00b7gen\u00b7schlag", ",", "und", "glei\u00b7che"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "APPR", "NN", "$,", "KON", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem Meere, das bei sich nicht leidet eine Leiche.", "tokens": ["Dem", "Mee\u00b7re", ",", "das", "bei", "sich", "nicht", "lei\u00b7det", "ei\u00b7ne", "Lei\u00b7che", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PRF", "PTKNEG", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Es wirft die Leichen aus; so du mit heil'gem Braus", "tokens": ["Es", "wirft", "die", "Lei\u00b7chen", "aus", ";", "so", "du", "mit", "heil'\u00b7gem", "Braus"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$.", "ADV", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erhebe dich und wirf fort allen Todesgraus.", "tokens": ["Er\u00b7he\u00b7be", "dich", "und", "wirf", "fort", "al\u00b7len", "To\u00b7des\u00b7graus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVIMP", "PTKVZ", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wie F\u00f6bos Eiland, wo kein Todter ward begraben,", "tokens": ["Wie", "F\u00f6\u00b7bos", "Ei\u00b7land", ",", "wo", "kein", "Tod\u00b7ter", "ward", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "NN", "$,", "PWAV", "PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So soll in dir der Tod auch keine St\u00e4tte haben.", "tokens": ["So", "soll", "in", "dir", "der", "Tod", "auch", "kei\u00b7ne", "St\u00e4t\u00b7te", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "APPR", "PPER", "ART", "NN", "ADV", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und war's ein theuerstes, was todt ist das ist ab;", "tokens": ["Und", "wa\u00b7r's", "ein", "theu\u00b7ers\u00b7tes", ",", "was", "todt", "ist", "das", "ist", "ab", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "$,", "PRELS", "ADJD", "VAFIN", "PDS", "VAFIN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Im Himmel fliegt der Geist, der Moder liegt im Grab.", "tokens": ["Im", "Him\u00b7mel", "fliegt", "der", "Geist", ",", "der", "Mo\u00b7der", "liegt", "im", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Du sei die Grube nicht, worin Verwesung liegt;", "tokens": ["Du", "sei", "die", "Gru\u00b7be", "nicht", ",", "wo\u00b7rin", "Ver\u00b7we\u00b7sung", "liegt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PTKNEG", "$,", "PWAV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sei du der Himmel, drin der reine Geist sich wiegt.", "tokens": ["Sei", "du", "der", "Him\u00b7mel", ",", "drin", "der", "rei\u00b7ne", "Geist", "sich", "wiegt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "ADV", "ART", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}