{"textgrid.poem.50477": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer gegen diese jetzt die ", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer gegen diese jetzt die ", "tokens": ["Wer", "ge\u00b7gen", "die\u00b7se", "jetzt", "die"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "PDAT", "ADV", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der findet, wo ihm nur die M\u00fche nicht missf\u00e4llt,", "tokens": ["Der", "fin\u00b7det", ",", "wo", "ihm", "nur", "die", "M\u00fc\u00b7he", "nicht", "miss\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PWAV", "PPER", "ADV", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dass fast kein Verse nicht, den ich zuvor geschrieben,", "tokens": ["Dass", "fast", "kein", "Ver\u00b7se", "nicht", ",", "den", "ich", "zu\u00b7vor", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIAT", "NN", "PTKNEG", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ist was er ", "tokens": ["Ist", "was", "er"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PWS", "PPER"], "meter": "+--", "measure": "dactylic.init"}, "line.5": {"text": "O h\u00e4tt' ich,", "tokens": ["O", "h\u00e4tt'", "ich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "$,"], "meter": "+--", "measure": "dactylic.init"}, "line.6": {"text": "So mercklich den ", "tokens": ["So", "merck\u00b7lich", "den"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJD", "ART"], "meter": "-+--", "measure": "dactylic.init"}, "line.7": {"text": "Dass wie die ", "tokens": ["Dass", "wie", "die"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "KOKOM", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Man auch die ", "tokens": ["Man", "auch", "die"], "token_info": ["word", "word", "word"], "pos": ["PIS", "ADV", "ART"], "meter": "+--", "measure": "dactylic.init"}}}}}