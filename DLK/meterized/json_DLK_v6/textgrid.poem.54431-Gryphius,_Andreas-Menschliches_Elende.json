{"textgrid.poem.54431": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Menschliches Elende", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was sind wir Menschen doch! ein Wonhau\u00df gri ier Schmertz\u1ebd?", "tokens": ["Was", "sind", "wir", "Men\u00b7schen", "doch", "!", "ein", "Won\u00b7hau\u00df", "gri", "ier", "Schmertz\u1ebd", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "NN", "ADV", "$.", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Baal des falschen Gl\u00fccks/ ein Irrliecht dieser zeit/", "tokens": ["Ein", "Baal", "des", "fal\u00b7schen", "Gl\u00fccks", "/", "ein", "I\u00b7rrliecht", "die\u00b7ser", "zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$(", "ART", "NN", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein Schawplatz aller Angst/ vnnd Widerwertigkeit/", "tokens": ["Ein", "Schaw\u00b7platz", "al\u00b7ler", "Angst", "/", "vnnd", "Wi\u00b7der\u00b7wer\u00b7tig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$(", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein bald verschmeltzter Schnee/ vnd abgebrante Kertzen/", "tokens": ["Ein", "bald", "ver\u00b7schmeltz\u00b7ter", "Schnee", "/", "vnd", "ab\u00b7ge\u00b7bran\u00b7te", "Kert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$(", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Di\u00df Leben fleucht darvon wie ein Geschw\u00e4tz vnd Schertzen.", "tokens": ["Di\u00df", "Le\u00b7ben", "fleucht", "dar\u00b7von", "wie", "ein", "Ge\u00b7schw\u00e4tz", "vnd", "Schert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VVFIN", "PAV", "KOKOM", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die vor vns abgelegt des schwachen Leibes kleid/", "tokens": ["Die", "vor", "vns", "ab\u00b7ge\u00b7legt", "des", "schwa\u00b7chen", "Lei\u00b7bes", "kleid", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVPP", "ART", "ADJA", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Vnd in das Todten Buch der grossen Sterbligkeit", "tokens": ["Vnd", "in", "das", "Tod\u00b7ten", "Buch", "der", "gros\u00b7sen", "Ster\u00b7blig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "L\u00e4ngst eingeschrieben sind; sind vns au\u00df Sinn' vnd Hertzen:", "tokens": ["L\u00e4ngst", "ein\u00b7ge\u00b7schrie\u00b7ben", "sind", ";", "sind", "vns", "au\u00df", "Sinn'", "vnd", "Hert\u00b7zen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "$.", "VAFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Gleich wie ein eitel Traum leicht au\u00df der acht hinf\u00e4lt/", "tokens": ["Gleich", "wie", "ein", "ei\u00b7tel", "Traum", "leicht", "au\u00df", "der", "acht", "hin\u00b7f\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "ADJD", "NN", "ADJD", "APPR", "ART", "CARD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Vnd wie ein Strom verfleust/ den keine Macht auffhelt;", "tokens": ["Vnd", "wie", "ein", "Strom", "ver\u00b7fleust", "/", "den", "kei\u00b7ne", "Macht", "auf\u00b7fhelt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "$(", "ART", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So mu\u00df auch vnser Nahm/ Lob/ Ehr vnd Ruhm verschwinden.", "tokens": ["So", "mu\u00df", "auch", "vn\u00b7ser", "Nahm", "/", "Lob", "/", "Ehr", "vnd", "Ruhm", "ver\u00b7schwin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "PPOSAT", "NN", "$(", "NN", "$(", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Was jtzund Athem holt; f\u00e4lt vnversehns dahin;", "tokens": ["Was", "jt\u00b7zund", "A\u00b7them", "holt", ";", "f\u00e4lt", "vn\u00b7ver\u00b7sehns", "da\u00b7hin", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "NN", "VVFIN", "$.", "VVFIN", "PPER", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Was nach vns kompt/ wird auch der Todt ins Grab hinzihn/", "tokens": ["Was", "nach", "vns", "kompt", "/", "wird", "auch", "der", "Todt", "ins", "Grab", "hin\u00b7zihn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "VVFIN", "$(", "VAFIN", "ADV", "ART", "NN", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So werden wir verjagt gleich wie ein Rauch von Winden.", "tokens": ["So", "wer\u00b7den", "wir", "ver\u00b7jagt", "gleich", "wie", "ein", "Rauch", "von", "Win\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "ADV", "KOKOM", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}