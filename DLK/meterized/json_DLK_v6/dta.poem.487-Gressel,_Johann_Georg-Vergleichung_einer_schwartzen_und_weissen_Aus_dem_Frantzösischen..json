{"dta.poem.487": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Vergleichung einer schwartzen und weissen  \n Aus dem Frantz\u00f6sischen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Es brennet Amors Feur am meisten bey den Mohren/", "tokens": ["Es", "bren\u00b7net", "A\u00b7mors", "Feur", "am", "meis\u00b7ten", "bey", "den", "Moh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "NN", "APPRART", "PIS", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie ihr erhitzter Schoo\u00df davon mit Nachdruck zeugt/", "tokens": ["Wie", "ihr", "er\u00b7hitz\u00b7ter", "Schoo\u00df", "da\u00b7von", "mit", "Nach\u00b7druck", "zeugt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "PAV", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Beym Weissen aber hats die meiste Krafft verlohren/", "tokens": ["Beym", "Weis\u00b7sen", "a\u00b7ber", "hats", "die", "meis\u00b7te", "Krafft", "ver\u00b7loh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil diese kalte Art vor seiner Gluth entweicht.", "tokens": ["Weil", "die\u00b7se", "kal\u00b7te", "Art", "vor", "sei\u00b7ner", "Gluth", "ent\u00b7weicht", "."], "token_info": ["word", "word", "word", "word", "word"