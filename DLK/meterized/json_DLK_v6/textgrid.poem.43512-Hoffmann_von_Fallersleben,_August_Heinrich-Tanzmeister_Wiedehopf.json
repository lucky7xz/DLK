{"textgrid.poem.43512": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Tanzmeister Wiedehopf", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Kuckuck nicket mit dem Kopf", "tokens": ["Der", "Ku\u00b7ckuck", "ni\u00b7cket", "mit", "dem", "Kopf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und spricht: Gevatter Wiedehopf,", "tokens": ["Und", "spricht", ":", "Ge\u00b7vat\u00b7ter", "Wie\u00b7de\u00b7hopf", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Willst du der beste T\u00e4nzer sein", "tokens": ["Willst", "du", "der", "bes\u00b7te", "T\u00e4n\u00b7zer", "sein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor allen V\u00f6geln gro\u00df und klein,", "tokens": ["Vor", "al\u00b7len", "V\u00f6\u00b7geln", "gro\u00df", "und", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zeig deine K\u00fcnste denn im Nu!", "tokens": ["Zeig", "dei\u00b7ne", "K\u00fcns\u00b7te", "denn", "im", "Nu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "KON", "APPRART", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich musicire dir dazu:", "tokens": ["Ich", "mu\u00b7si\u00b7ci\u00b7re", "dir", "da\u00b7zu", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Kuckuck, Kuckuck! hopp, hopp!", "tokens": ["Ku\u00b7ckuck", ",", "Ku\u00b7ckuck", "!", "hopp", ",", "hopp", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$.", "ADJD", "$,", "ITJ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Da hub Gevatter Wiedehopf", "tokens": ["Da", "hub", "Ge\u00b7vat\u00b7ter", "Wie\u00b7de\u00b7hopf"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gar stolz empor den bunten Schopf,", "tokens": ["Gar", "stolz", "em\u00b7por", "den", "bun\u00b7ten", "Schopf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und hopste lustig, hopp, hopp, hopp!", "tokens": ["Und", "hops\u00b7te", "lus\u00b7tig", ",", "hopp", ",", "hopp", ",", "hopp", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$,", "ITJ", "$,", "ITJ", "$,", "ITJ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und freute sich gar sehr darob.", "tokens": ["Und", "freu\u00b7te", "sich", "gar", "sehr", "da\u00b7rob", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "ADV", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wer gerne tanzt, ist gleich bereit,", "tokens": ["Wer", "ger\u00b7ne", "tanzt", ",", "ist", "gleich", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$,", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und wenn auch nur ein Kuckuck schreit:", "tokens": ["Und", "wenn", "auch", "nur", "ein", "Ku\u00b7ckuck", "schreit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Kuckuck, Kuckuck! hopp, hopp!", "tokens": ["Ku\u00b7ckuck", ",", "Ku\u00b7ckuck", "!", "hopp", ",", "hopp", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$.", "ADJD", "$,", "ITJ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}