{"textgrid.poem.64328": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "49. Bei Homers Bilde", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du guter, alter, blinder Mann,", "tokens": ["Du", "gu\u00b7ter", ",", "al\u00b7ter", ",", "blin\u00b7der", "Mann", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie ist mein Herz dir zugethan!", "tokens": ["Wie", "ist", "mein", "Herz", "dir", "zu\u00b7ge\u00b7than", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nimm dieses Herzens hei\u00dfen Dank", "tokens": ["Nimm", "die\u00b7ses", "Her\u00b7zens", "hei\u00b7\u00dfen", "Dank"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PDAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr deinen g\u00f6ttlichen Gesang.", "tokens": ["F\u00fcr", "dei\u00b7nen", "g\u00f6tt\u00b7li\u00b7chen", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O h\u00e4tt' ich deiner Lieder Macht!", "tokens": ["O", "h\u00e4tt'", "ich", "dei\u00b7ner", "Lie\u00b7der", "Macht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich rief' dir durch der Gr\u00e4ber Nacht,", "tokens": ["Ich", "rie\u00b7f'", "dir", "durch", "der", "Gr\u00e4\u00b7ber", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Du k\u00e4mst, in Morgenrot geh\u00fcllt,", "tokens": ["Du", "k\u00e4mst", ",", "in", "Mor\u00b7gen\u00b7rot", "ge\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So hehr und freundlich, wie dein Bild;", "tokens": ["So", "hehr", "und", "freund\u00b7lich", ",", "wie", "dein", "Bild", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,", "PWAV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und reichtest mir die Strahlenhand;", "tokens": ["Und", "reich\u00b7test", "mir", "die", "Strah\u00b7len\u00b7hand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich aber k\u00fc\u00dfte dein Gewand;", "tokens": ["Ich", "a\u00b7ber", "k\u00fc\u00df\u00b7te", "dein", "Ge\u00b7wand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch bald ermannte mich dein Gru\u00df", "tokens": ["Doch", "bald", "er\u00b7mann\u00b7te", "mich", "dein", "Gru\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu Handschlag und zu Lippenku\u00df.", "tokens": ["Zu", "Hand\u00b7schlag", "und", "zu", "Lip\u00b7pen\u00b7ku\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Auch spr\u00e4ch' ich: was ich hab', ist dein!", "tokens": ["Auch", "spr\u00e4ch'", "ich", ":", "was", "ich", "hab'", ",", "ist", "dein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "PWS", "PPER", "VAFIN", "$,", "VAFIN", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Trink, alter Halbgott, diesen Wein!", "tokens": ["Trink", ",", "al\u00b7ter", "Halb\u00b7gott", ",", "die\u00b7sen", "Wein", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er r\u00f6tet sich im Morgenland,", "tokens": ["Er", "r\u00f6\u00b7tet", "sich", "im", "Mor\u00b7gen\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Am allerfernsten Mohrenstrand.", "tokens": ["Am", "al\u00b7ler\u00b7ferns\u00b7ten", "Moh\u00b7ren\u00b7strand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Nun tr\u00e4nkst du des Olympos Lust", "tokens": ["Nun", "tr\u00e4nkst", "du", "des", "O\u00b7lym\u00b7pos", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit langen Z\u00fcgen in die Brust.", "tokens": ["Mit", "lan\u00b7gen", "Z\u00fc\u00b7gen", "in", "die", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich l\u00e4s' auf deinem Angesicht:", "tokens": ["Ich", "l\u00e4s'", "auf", "dei\u00b7nem", "An\u00b7ge\u00b7sicht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den neuen Nektar kannt' ich nicht!", "tokens": ["Den", "neu\u00b7en", "Nek\u00b7tar", "kannt'", "ich", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}