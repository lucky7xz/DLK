{"textgrid.poem.45719": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[der Syllogismus w\u00e4re ein rechter Schatz]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Syllogismus w\u00e4re ein rechter Schatz,", "tokens": ["Der", "Syl\u00b7lo\u00b7gis\u00b7mus", "w\u00e4\u00b7re", "ein", "rech\u00b7ter", "Schatz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "H\u00e4tte man nur immer einen ersten Satz,", "tokens": ["H\u00e4t\u00b7te", "man", "nur", "im\u00b7mer", "ei\u00b7nen", "ers\u00b7ten", "Satz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Doch nimmt man einen falschen oder ungewissen,", "tokens": ["Doch", "nimmt", "man", "ei\u00b7nen", "fal\u00b7schen", "o\u00b7der", "un\u00b7ge\u00b7wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ART", "ADJA", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "W\u00e4chst der Irrtum im richtigen Schlie\u00dfen.", "tokens": ["W\u00e4chst", "der", "Irr\u00b7tum", "im", "rich\u00b7ti\u00b7gen", "Schlie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}}}}