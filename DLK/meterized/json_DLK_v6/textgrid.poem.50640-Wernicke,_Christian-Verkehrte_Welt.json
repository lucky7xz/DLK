{"textgrid.poem.50640": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Verkehrte Welt", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es kehrten ", "tokens": ["Es", "kehr\u00b7ten"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Und legten beide m\u00fcd'", "tokens": ["Und", "leg\u00b7ten", "bei\u00b7de", "m\u00fcd'"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIS", "VMFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von ihren Seiten ab. Sie schliefen, bis der Schein", "tokens": ["Von", "ih\u00b7ren", "Sei\u00b7ten", "ab", ".", "Sie", "schlie\u00b7fen", ",", "bis", "der", "Schein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$.", "PPER", "VVFIN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Sonn' im S\u00fcden war, so da\u00df aus gro\u00dfer Eile", "tokens": ["Der", "Sonn'", "im", "S\u00fc\u00b7den", "war", ",", "so", "da\u00df", "aus", "gro\u00b7\u00dfer", "Ei\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "VAFIN", "$,", "ADV", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Beim Abzug keines nicht sein recht Gewehr bekam.", "tokens": ["Beim", "Ab\u00b7zug", "kei\u00b7nes", "nicht", "sein", "recht", "Ge\u00b7wehr", "be\u00b7kam", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIS", "PTKNEG", "PPOSAT", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie nun bei ", "tokens": ["Wie", "nun", "bei"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADV", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Und jedes ", "tokens": ["Und", "je\u00b7des"], "token_info": ["word", "word"], "pos": ["KON", "PIAT"], "meter": "-+-", "measure": "amphibrach.single"}}}}}