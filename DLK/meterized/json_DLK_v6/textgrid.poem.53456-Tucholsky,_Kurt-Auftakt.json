{"textgrid.poem.53456": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Auftakt", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Thalia st\u00fcrzt sich in die Winterrobe", "tokens": ["Tha\u00b7lia", "st\u00fcrzt", "sich", "in", "die", "Win\u00b7ter\u00b7ro\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "und macht sich bis zum R\u00fcckenwirbel blo\u00df . . .", "tokens": ["und", "macht", "sich", "bis", "zum", "R\u00fc\u00b7cken\u00b7wir\u00b7bel", "blo\u00df", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "APPRART", "NN", "ADV", "$.", "$.", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ab wirft sie ihren Schmoddergown \u2013 ick jloobe,", "tokens": ["Ab", "wirft", "sie", "ih\u00b7ren", "Schmod\u00b7der\u00b7gown", "\u2013", "ick", "jloo\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "PPOSAT", "NN", "$(", "PPER", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "jetzt geht es los.", "tokens": ["jetzt", "geht", "es", "los", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Das Winterfieber packt die kleinsten Schmieren,", "tokens": ["Das", "Win\u00b7ter\u00b7fie\u00b7ber", "packt", "die", "kleins\u00b7ten", "Schmie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "der Mime schw\u00e4rzt den alten Schappohklapp,", "tokens": ["der", "Mi\u00b7me", "schw\u00e4rzt", "den", "al\u00b7ten", "Schap\u00b7poh\u00b7klapp", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "der Direkt\u00f6hr l\u00e4\u00dft das Theater renovieren", "tokens": ["der", "Di\u00b7rek\u00b7t\u00f6hr", "l\u00e4\u00dft", "das", "The\u00b7a\u00b7ter", "re\u00b7no\u00b7vie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "und staubt die Hypotheken ab.", "tokens": ["und", "staubt", "die", "Hy\u00b7po\u00b7the\u00b7ken", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Spielplan steigt: man wird Modernes geben,", "tokens": ["Der", "Spiel\u00b7plan", "steigt", ":", "man", "wird", "Mo\u00b7der\u00b7nes", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PIS", "VAFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bongs Klassiker, Band eins bis hundertzehn,", "tokens": ["Bongs", "Klas\u00b7si\u00b7ker", ",", "Band", "eins", "bis", "hun\u00b7dert\u00b7zehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "CARD", "APPR", "CARD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "und Ibsen, Shakespeare und Herrn Sch\u00f6nherrleben \u2013", "tokens": ["und", "Ib\u00b7sen", ",", "Sha\u00b7ke\u00b7spe\u00b7a\u00b7re", "und", "Herrn", "Sch\u00f6n\u00b7herr\u00b7le\u00b7ben", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "NE", "KON", "NN", "NN", "$("], "meter": "-+-+-+-+-++-+-", "measure": "unknown.measure.septa"}, "line.4": {"text": "ihr werdets sehn!", "tokens": ["ihr", "wer\u00b7dets", "sehn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Man ist erregt bis in die tiefsten Tiefen \u2013", "tokens": ["Man", "ist", "er\u00b7regt", "bis", "in", "die", "tiefs\u00b7ten", "Tie\u00b7fen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "VVPP", "KON", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "selbst nachts brennt Licht im Direktionsb\u00fcro.", "tokens": ["selbst", "nachts", "brennt", "Licht", "im", "Di\u00b7rek\u00b7ti\u00b7ons\u00b7b\u00fc\u00b7ro", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Schon h\u00f6rt man unsern Holzbock interwiefen . . .", "tokens": ["Schon", "h\u00f6rt", "man", "un\u00b7sern", "Holz\u00b7bock", "in\u00b7ter\u00b7wie\u00b7fen", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPOSAT", "NN", "VVINF", "$.", "$.", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Rideau!", "tokens": ["Ri\u00b7de\u00b7au", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Rideau!", "tokens": ["Ri\u00b7de\u00b7au", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}