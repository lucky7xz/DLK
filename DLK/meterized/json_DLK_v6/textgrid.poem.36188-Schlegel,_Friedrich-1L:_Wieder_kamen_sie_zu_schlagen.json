{"textgrid.poem.36188": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wieder kamen sie zu schlagen", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wieder kamen sie zu schlagen", "tokens": ["Wie\u00b7der", "ka\u00b7men", "sie", "zu", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bei der hohen Tala Burgtor,", "tokens": ["Bei", "der", "ho\u00b7hen", "Ta\u00b7la", "Burg\u00b7tor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dort wo an Sanktonas Mauern", "tokens": ["Dort", "wo", "an", "Sank\u00b7to\u00b7nas", "Mau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PWAV", "APPR", "NE", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Die Caranta schl\u00e4ngelt kunstlos.", "tokens": ["Die", "Ca\u00b7ran\u00b7ta", "schl\u00e4n\u00b7gelt", "kunst\u00b7los", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "PTKVZ", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Wo den seinen frommen Kriegern", "tokens": ["Wo", "den", "sei\u00b7nen", "from\u00b7men", "Krie\u00b7gern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wieder gleiches Wunder Gott schuf,", "tokens": ["Wie\u00b7der", "glei\u00b7ches", "Wun\u00b7der", "Gott", "schuf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Welcher Lanze n\u00e4chtlich gr\u00fcnet,", "tokens": ["Wel\u00b7cher", "Lan\u00b7ze", "n\u00e4cht\u00b7lich", "gr\u00fc\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Solche soll'n im Himmels Lustort", "tokens": ["Sol\u00b7che", "soll'n", "im", "Him\u00b7mels", "Lus\u00b7tort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPRART", "NN", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Morgen heil'ge Sterne schauen,", "tokens": ["Mor\u00b7gen", "heil'\u00b7ge", "Ster\u00b7ne", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Rein gebadet in dem Blutstrom.", "tokens": ["Rein", "ge\u00b7ba\u00b7det", "in", "dem", "Bluts\u00b7trom", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Froh des heil'gen M\u00e4rtertumes", "tokens": ["Froh", "des", "heil'\u00b7gen", "M\u00e4r\u00b7ter\u00b7tu\u00b7mes"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "St\u00fcrzten in den Tod sie mutvoll,", "tokens": ["St\u00fcrz\u00b7ten", "in", "den", "Tod", "sie", "mut\u00b7voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PPER", "ADJD", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.13": {"text": "Doch unz\u00e4hlige der Heiden", "tokens": ["Doch", "un\u00b7z\u00e4h\u00b7li\u00b7ge", "der", "Hei\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "F\u00e4rbten noch zuvor den Grund rot.", "tokens": ["F\u00e4rb\u00b7ten", "noch", "zu\u00b7vor", "den", "Grund", "rot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Agolante n\u00e4chtlich fliehet,", "tokens": ["A\u00b7go\u00b7lan\u00b7te", "n\u00e4cht\u00b7lich", "flie\u00b7het", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Da von Karl ihn trennt der Flu\u00df noch;", "tokens": ["Da", "von", "Karl", "ihn", "trennt", "der", "Flu\u00df", "noch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "PPER", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.17": {"text": "Doch kaum gl\u00fcht des Morgens Purpur,", "tokens": ["Doch", "kaum", "gl\u00fcht", "des", "Mor\u00b7gens", "Pur\u00b7pur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Als schon Karol seiner Spur folgt.", "tokens": ["Als", "schon", "Ka\u00b7rol", "sei\u00b7ner", "Spur", "folgt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NE", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Bugiens K\u00f6nig und Algarbens", "tokens": ["Bu\u00b7giens", "K\u00f6\u00b7nig", "und", "Al\u00b7gar\u00b7bens"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NN", "KON", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Zittern vor dem Helden mutlos,", "tokens": ["Zit\u00b7tern", "vor", "dem", "Hel\u00b7den", "mut\u00b7los", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.21": {"text": "Und nach mancher herben Wunde", "tokens": ["Und", "nach", "man\u00b7cher", "her\u00b7ben", "Wun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "F\u00e4rbt sein Schwert ihr grimmes Blut rot.", "tokens": ["F\u00e4rbt", "sein", "Schwert", "ihr", "grim\u00b7mes", "Blut", "rot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PPOSAT", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Da der Christen Heer nun rastet,", "tokens": ["Da", "der", "Chris\u00b7ten", "Heer", "nun", "ras\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Nach dem wilden Streit die Ruh' folgt,", "tokens": ["Nach", "dem", "wil\u00b7den", "Streit", "die", "Ruh'", "folgt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Da geschah ein seltsam Zeichen", "tokens": ["Da", "ge\u00b7schah", "ein", "selt\u00b7sam", "Zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Warnend, wie der S\u00fcnde Trug lohnt.", "tokens": ["War\u00b7nend", ",", "wie", "der", "S\u00fcn\u00b7de", "Trug", "lohnt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Romarich, ein kranker Krieger,", "tokens": ["Ro\u00b7ma\u00b7rich", ",", "ein", "kran\u00b7ker", "Krie\u00b7ger", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Da der Tod ihm nahet wutvoll,", "tokens": ["Da", "der", "Tod", "ihm", "na\u00b7het", "wut\u00b7voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "ADJD", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.29": {"text": "Lie\u00df dem teuersten Gesellen,", "tokens": ["Lie\u00df", "dem", "teu\u00b7ers\u00b7ten", "Ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "---+--+-", "measure": "iambic.di.relaxed"}, "line.30": {"text": "Ob vor Gott er w\u00fcrde schuldlos,", "tokens": ["Ob", "vor", "Gott", "er", "w\u00fcr\u00b7de", "schuld\u00b7los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "PPER", "VAFIN", "ADJD", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.31": {"text": "Noch sein Ro\u00df, des Wert den Armen", "tokens": ["Noch", "sein", "Ro\u00df", ",", "des", "Wert", "den", "Ar\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "$,", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Er soll geben lieb und huldvoll.", "tokens": ["Er", "soll", "ge\u00b7ben", "lieb", "und", "huld\u00b7voll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "ADJD", "KON", "ADJD", "$."], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.33": {"text": "Jener aber treulos denkend,", "tokens": ["Je\u00b7ner", "a\u00b7ber", "treu\u00b7los", "den\u00b7kend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Mit der wilden S\u00fcnd' im Bund schon,", "tokens": ["Mit", "der", "wil\u00b7den", "S\u00fcnd'", "im", "Bund", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPRART", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Die er l\u00f6ste, hundert Gulden,", "tokens": ["Die", "er", "l\u00f6s\u00b7te", ",", "hun\u00b7dert", "Gul\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Schnell verschwendet er sie nutzlos,", "tokens": ["Schnell", "ver\u00b7schwen\u00b7det", "er", "sie", "nutz\u00b7los", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "ADJD", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.37": {"text": "Lebt im frechen Sinn so f\u00fcrder,", "tokens": ["Lebt", "im", "fre\u00b7chen", "Sinn", "so", "f\u00fcr\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Denkt nicht seiner Worte trugvoll.", "tokens": ["Denkt", "nicht", "sei\u00b7ner", "Wor\u00b7te", "trug\u00b7voll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Da nun drei\u00dfig Tage waren,", "tokens": ["Da", "nun", "drei\u00b7\u00dfig", "Ta\u00b7ge", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "CARD", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Da\u00df am Freund er ward so schuldvoll,", "tokens": ["Da\u00df", "am", "Freund", "er", "ward", "so", "schuld\u00b7voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "--+-+---", "measure": "anapaest.init"}, "line.41": {"text": "Da erschien der Geist des Freundes,", "tokens": ["Da", "er\u00b7schien", "der", "Geist", "des", "Freun\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Furchtbar schauend, bleich und blutlos,", "tokens": ["Furcht\u00b7bar", "schau\u00b7end", ",", "bleich", "und", "blut\u00b7los", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.43": {"text": "Sprechend: Wisse, da\u00df all' meine", "tokens": ["Spre\u00b7chend", ":", "Wis\u00b7se", ",", "da\u00df", "all'", "mei\u00b7ne"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "$.", "NN", "$,", "KOUS", "PIS", "PPOSAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "S\u00fcnden sind getilgt und spurlos.", "tokens": ["S\u00fcn\u00b7den", "sind", "ge\u00b7tilgt", "und", "spur\u00b7los", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "KON", "ADJD", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.45": {"text": "In der H\u00f6lle Tal wirst k\u00fcnftig", "tokens": ["In", "der", "H\u00f6l\u00b7le", "Tal", "wirst", "k\u00fcnf\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "VAFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Du statt meiner jammern wutvoll;", "tokens": ["Du", "statt", "mei\u00b7ner", "jam\u00b7mern", "wut\u00b7voll", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "----+--+", "measure": "iambic.di.chol"}, "line.47": {"text": "Also lautet jenes Richters", "tokens": ["Al\u00b7so", "lau\u00b7tet", "je\u00b7nes", "Rich\u00b7ters"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "Ewig streng gerechtes Spruchwort. \u2013", "tokens": ["E\u00b7wig", "streng", "ge\u00b7rech\u00b7tes", "Spruch\u00b7wort", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.49": {"text": "So verschwand der Tote wieder,", "tokens": ["So", "ver\u00b7schwand", "der", "To\u00b7te", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.50": {"text": "Jener starret sinn- und mutlos.", "tokens": ["Je\u00b7ner", "star\u00b7ret", "sinn", "und", "mut\u00b7los", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "VVFIN", "TRUNC", "KON", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.51": {"text": "Fr\u00fch am andern Tage Morgens", "tokens": ["Fr\u00fch", "am", "an\u00b7dern", "Ta\u00b7ge", "Mor\u00b7gens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.52": {"text": "Tut er's den Gesellen kund noch.", "tokens": ["Tut", "er's", "den", "Ge\u00b7sel\u00b7len", "kund", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ART", "NN", "PTKVZ", "ADV", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.53": {"text": "Als er eben frech nun redet,", "tokens": ["Als", "er", "e\u00b7ben", "frech", "nun", "re\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.54": {"text": "Da erhebt sich in der Luft hoch", "tokens": ["Da", "er\u00b7hebt", "sich", "in", "der", "Luft", "hoch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "ART", "NN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.55": {"text": "Br\u00fcllen, wie von L\u00f6wen, K\u00e4lbern,", "tokens": ["Br\u00fcl\u00b7len", ",", "wie", "von", "L\u00f6\u00b7wen", ",", "K\u00e4l\u00b7bern", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "PWAV", "APPR", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.56": {"text": "Wie die W\u00f6lfe heulen wutvoll.", "tokens": ["Wie", "die", "W\u00f6l\u00b7fe", "heu\u00b7len", "wut\u00b7voll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "ADJD", "$."], "meter": "--+-+---", "measure": "anapaest.init"}, "line.57": {"text": "Luftig fahren durcheinander", "tokens": ["Luf\u00b7tig", "fah\u00b7ren", "durch\u00b7ein\u00b7an\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["NE", "VVFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.58": {"text": "Ungeheu'r in wilder Unform,", "tokens": ["Un\u00b7ge\u00b7heu'r", "in", "wil\u00b7der", "Un\u00b7form", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.59": {"text": "Blut'ge Flammen zucken strahlend", "tokens": ["Blut'\u00b7ge", "Flam\u00b7men", "zu\u00b7cken", "strah\u00b7lend"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.60": {"text": "Aus der dunklen Wolke Glutscho\u00df.", "tokens": ["Aus", "der", "dunk\u00b7len", "Wol\u00b7ke", "Glut\u00b7scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.61": {"text": "Noch lebendig ward von Teufeln", "tokens": ["Noch", "le\u00b7ben\u00b7dig", "ward", "von", "Teu\u00b7feln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.62": {"text": "Weggef\u00fchrt er durch die Luft so,", "tokens": ["Weg\u00b7ge\u00b7f\u00fchrt", "er", "durch", "die", "Luft", "so", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.63": {"text": "Aus der Mitte der Genossen,", "tokens": ["Aus", "der", "Mit\u00b7te", "der", "Ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.64": {"text": "Mit Geheul und wildem Fluchwort.", "tokens": ["Mit", "Ge\u00b7heul", "und", "wil\u00b7dem", "Fluch\u00b7wort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.65": {"text": "Da das Heer nun weiter wandelt", "tokens": ["Da", "das", "Heer", "nun", "wei\u00b7ter", "wan\u00b7delt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.66": {"text": "Wohl zw\u00f6lf Tage rast- und ruhlos,", "tokens": ["Wohl", "zw\u00f6lf", "Ta\u00b7ge", "rast", "und", "ruh\u00b7los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "TRUNC", "KON", "ADJD", "$,"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.67": {"text": "Durch die W\u00fcsten, durch die Berge,", "tokens": ["Durch", "die", "W\u00fcs\u00b7ten", ",", "durch", "die", "Ber\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.68": {"text": "Findet man die Leiche wundvoll", "tokens": ["Fin\u00b7det", "man", "die", "Lei\u00b7che", "wund\u00b7voll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ART", "NN", "ADJD"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.69": {"text": "An der j\u00e4hen Felsenspitze,", "tokens": ["An", "der", "j\u00e4\u00b7hen", "Fel\u00b7sen\u00b7spit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.70": {"text": "Findet da die Spur von Blut noch,", "tokens": ["Fin\u00b7det", "da", "die", "Spur", "von", "Blut", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "NN", "ADV", "$,"], "meter": "+-+-