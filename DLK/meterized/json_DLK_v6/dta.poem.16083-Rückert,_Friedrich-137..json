{"dta.poem.16083": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "137.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du ruhest weichgepf\u00fchlt am Ufer strombesp\u00fclt,", "tokens": ["Du", "ru\u00b7hest", "weich\u00b7ge\u00b7pf\u00fchlt", "am", "U\u00b7fer", "strom\u00b7be\u00b7sp\u00fclt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dich schl\u00e4fert ein die Flut, die leis dich unterw\u00fchlt.", "tokens": ["Dich", "schl\u00e4\u00b7fert", "ein", "die", "Flut", ",", "die", "leis", "dich", "un\u00b7ter\u00b7w\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ART", "NN", "$,", "PRELS", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dich schaukelt Sommerluft, umgaukelt Bl\u00fctenduft,", "tokens": ["Dich", "schau\u00b7kelt", "Som\u00b7mer\u00b7luft", ",", "um\u00b7gau\u00b7kelt", "Bl\u00fc\u00b7ten\u00b7duft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und losgerissen tr\u00e4gt dein Bette dich zur Gruft.", "tokens": ["Und", "los\u00b7ge\u00b7ris\u00b7sen", "tr\u00e4gt", "dein", "Bet\u00b7te", "dich", "zur", "Gruft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIZU", "VVFIN", "PPOSAT", "NN", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sollt' ich erwecken dich, um zu erschrecken dich?", "tokens": ["Sollt'", "ich", "er\u00b7we\u00b7cken", "dich", ",", "um", "zu", "er\u00b7schre\u00b7cken", "dich", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "PPER", "$,", "KOUI", "PTKZU", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schwimm hin, und sanft im Traum die Flut soll decken dich.", "tokens": ["Schwimm", "hin", ",", "und", "sanft", "im", "Traum", "die", "Flut", "soll", "de\u00b7cken", "dich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "KON", "ADJD", "APPRART", "NN", "ART", "NN", "VMFIN", "VVINF", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}