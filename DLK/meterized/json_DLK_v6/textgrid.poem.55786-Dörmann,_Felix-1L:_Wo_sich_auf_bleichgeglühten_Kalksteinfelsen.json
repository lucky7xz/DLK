{"textgrid.poem.55786": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wo sich auf bleichgegl\u00fchten Kalksteinfelsen", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo sich auf bleichgegl\u00fchten Kalksteinfelsen", "tokens": ["Wo", "sich", "auf", "bleich\u00b7ge\u00b7gl\u00fch\u00b7ten", "Kalk\u00b7stein\u00b7fel\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "APPR", "ADJA", "NN"], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Der Rosenhain erhebt, dort ruhtest du ...", "tokens": ["Der", "Ro\u00b7sen\u00b7hain", "er\u00b7hebt", ",", "dort", "ruh\u00b7test", "du", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Luft ist schw\u00fcl, dem fernen Horizont", "tokens": ["Die", "Luft", "ist", "schw\u00fcl", ",", "dem", "fer\u00b7nen", "Ho\u00b7ri\u00b7zont"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Entsteigt ein nachtgef\u00e4rbtes Sturmgew\u00f6lk,", "tokens": ["Ent\u00b7steigt", "ein", "nacht\u00b7ge\u00b7f\u00e4rb\u00b7tes", "Sturm\u00b7ge\u00b7w\u00f6lk", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das noch ein letztes, blasses Roth ums\u00e4umt.", "tokens": ["Das", "noch", "ein", "letz\u00b7tes", ",", "blas\u00b7ses", "Roth", "um\u00b7s\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "ADJA", "$,", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "In schw\u00e4rzlich-gr\u00fcnen, weiten Wellen schl\u00e4gt", "tokens": ["In", "schw\u00e4rz\u00b7lich\u00b7gr\u00fc\u00b7nen", ",", "wei\u00b7ten", "Wel\u00b7len", "schl\u00e4gt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Schwerathmend schon empor zum Strandgeklipp", "tokens": ["Schwe\u00b7rath\u00b7mend", "schon", "em\u00b7por", "zum", "Strand\u00b7ge\u00b7klipp"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "PTKVZ", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Das wilde Meer und jauchzt dem Sturm entgegen,", "tokens": ["Das", "wil\u00b7de", "Meer", "und", "jauchzt", "dem", "Sturm", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Der es durchsch\u00fcttert bis zum letzten Grund.", "tokens": ["Der", "es", "durch\u00b7sch\u00fct\u00b7tert", "bis", "zum", "letz\u00b7ten", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Zu Neste fl\u00fcchtet scheu das Seegev\u00f6gel,", "tokens": ["Zu", "Nes\u00b7te", "fl\u00fcch\u00b7tet", "scheu", "das", "See\u00b7ge\u00b7v\u00f6\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Und heimw\u00e4rts flieht mit hochgebauschtem Segel", "tokens": ["Und", "heim\u00b7w\u00e4rts", "flieht", "mit", "hoch\u00b7ge\u00b7bauschtem", "Se\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Der Fischerbarken angstgepeitschte Schaar ...", "tokens": ["Der", "Fi\u00b7scher\u00b7bar\u00b7ken", "angst\u00b7ge\u00b7peitschte", "Schaar", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.13": {"text": "Du aber ruhst in marmornem Gest\u00fchl", "tokens": ["Du", "a\u00b7ber", "ruhst", "in", "mar\u00b7mor\u00b7nem", "Ge\u00b7st\u00fchl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.14": {"text": "Und saugst in gierig langen Athemz\u00fcgen", "tokens": ["Und", "saugst", "in", "gie\u00b7rig", "lan\u00b7gen", "A\u00b7them\u00b7z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Der goldig-blassen Rosen schweren Hauch,", "tokens": ["Der", "gol\u00b7dig\u00b7blas\u00b7sen", "Ro\u00b7sen", "schwe\u00b7ren", "Hauch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Und l\u00e4sst den Sturm in liebeshei\u00dfem Spiel", "tokens": ["Und", "l\u00e4sst", "den", "Sturm", "in", "lie\u00b7bes\u00b7hei\u00b7\u00dfem", "Spiel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Von Brust und Schultern Dir den Schleier l\u00f6sen", "tokens": ["Von", "Brust", "und", "Schul\u00b7tern", "Dir", "den", "Schlei\u00b7er", "l\u00f6\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Und starrst hinaus auf gischtgekr\u00f6nte Wasser", "tokens": ["Und", "starrst", "hin\u00b7aus", "auf", "gischt\u00b7ge\u00b7kr\u00f6n\u00b7te", "Was\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Und tr\u00e4umst vom Sturm, der Deine Seele liebt", "tokens": ["Und", "tr\u00e4umst", "vom", "Sturm", ",", "der", "Dei\u00b7ne", "See\u00b7le", "liebt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Und sie durchsch\u00fcttert bis zum letzten Grund.", "tokens": ["Und", "sie", "durch\u00b7sch\u00fct\u00b7tert", "bis", "zum", "letz\u00b7ten", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}