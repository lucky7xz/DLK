{"dta.poem.7710": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "82.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dem, was ich f\u00fcrchte, wag' ich Namen nicht zu geben,", "tokens": ["Dem", ",", "was", "ich", "f\u00fcrch\u00b7te", ",", "wag'", "ich", "Na\u00b7men", "nicht", "zu", "ge\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "NN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus Furcht, da\u00df erst dadurch es treten m\u00f6cht' ins Leben.", "tokens": ["Aus", "Furcht", ",", "da\u00df", "erst", "da\u00b7durch", "es", "tre\u00b7ten", "m\u00f6cht'", "ins", "Le\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUS", "ADV", "PAV", "PPER", "VVINF", "VMFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich wage, was ich hoff', auch nicht bestimmt zu nennen,", "tokens": ["Ich", "wa\u00b7ge", ",", "was", "ich", "hoff'", ",", "auch", "nicht", "be\u00b7stimmt", "zu", "nen\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,", "ADV", "PTKNEG", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus Furcht, es m\u00f6cht' ein Hauch das luft'ge Bildnis trennen.", "tokens": ["Aus", "Furcht", ",", "es", "m\u00f6cht'", "ein", "Hauch", "das", "luft'\u00b7ge", "Bild\u00b7nis", "tren\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PPER", "VMFIN", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Es sei, so was ich hoff', als was ich f\u00fcrchte, still", "tokens": ["Es", "sei", ",", "so", "was", "ich", "hoff'", ",", "als", "was", "ich", "f\u00fcrch\u00b7te", ",", "still"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "$,", "ADV", "PWS", "PPER", "VVFIN", "$,", "KOUS", "PRELS", "PPER", "VVFIN", "$,", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dahin gestellt, wie Gott es senden, wenden will.", "tokens": ["Da\u00b7hin", "ge\u00b7stellt", ",", "wie", "Gott", "es", "sen\u00b7den", ",", "wen\u00b7den", "will", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVPP", "$,", "PWAV", "NN", "PPER", "VVINF", "$,", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}