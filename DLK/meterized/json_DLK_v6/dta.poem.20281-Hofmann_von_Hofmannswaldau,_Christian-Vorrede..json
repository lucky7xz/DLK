{"dta.poem.20281": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Vorrede.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der helden geist ist stahl/ ihr hertz aus diamant/", "tokens": ["Der", "hel\u00b7den", "geist", "ist", "stahl", "/", "ihr", "hertz", "aus", "di\u00b7a\u00b7mant", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAFIN", "ADV", "$(", "PPOSAT", "NN", "APPR", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn es mit m\u00e4nnern k\u00e4mpfft; Alleine wachs bey frauen.", "tokens": ["Wenn", "es", "mit", "m\u00e4n\u00b7nern", "k\u00e4mpfft", ";", "Al\u00b7lei\u00b7ne", "wachs", "bey", "frau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "VVFIN", "$.", "ADV", "PIS", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn adler lieben zwar nur adler/ pfaue pfauen;", "tokens": ["Denn", "ad\u00b7ler", "lie\u00b7ben", "zwar", "nur", "ad\u00b7ler", "/", "pfau\u00b7e", "pfau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADV", "ADV", "NN", "$(", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch Alexandern zwingt der geilen Thais brand.", "tokens": ["Doch", "A\u00b7lex\u00b7an\u00b7dern", "zwingt", "der", "gei\u00b7len", "Thais", "brand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die spindel Omphalens entweyht Alcidens hand.", "tokens": ["Die", "spin\u00b7del", "Om\u00b7pha\u00b7lens", "ent\u00b7weyht", "Al\u00b7ci\u00b7dens", "hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Achilles/ wenn er liebt/ kriegt f\u00fcr den krieg ein grauen.", "tokens": ["A\u00b7chil\u00b7les", "/", "wenn", "er", "liebt", "/", "kriegt", "f\u00fcr", "den", "krieg", "ein", "grau\u00b7en", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "KOUS", "PPER", "VVFIN", "$(", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Anton stirbt als ein weib in einer Mohrin klauen/", "tokens": ["An\u00b7ton", "stirbt", "als", "ein", "weib", "in", "ei\u00b7ner", "Moh\u00b7rin", "klau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KOKOM", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ja auch der g\u00f6tter lieb\u2019 ist wahnwitz anverwandt.", "tokens": ["Ja", "auch", "der", "g\u00f6t\u00b7ter", "lieb'", "ist", "wahn\u00b7witz", "an\u00b7ver\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ART", "ADJA", "NN", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "F\u00fcrst Herrmann aber liebt mit grosser tapfferkeit.", "tokens": ["F\u00fcrst", "Herr\u00b7mann", "a\u00b7ber", "liebt", "mit", "gros\u00b7ser", "tapf\u00b7fer\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Denn er verm\u00e4hlet ihm Minerven mit Thusnelden/", "tokens": ["Denn", "er", "ver\u00b7m\u00e4h\u00b7let", "ihm", "Mi\u00b7ner\u00b7ven", "mit", "Thus\u00b7nel\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "NN", "APPR", "NN", "$("], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Sie ihr den Hercules mit Deutschlands grossen helden.", "tokens": ["Sie", "ihr", "den", "Her\u00b7cu\u00b7les", "mit", "Deutschlands", "gros\u00b7sen", "hel\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "ART", "NN", "APPR", "NE", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Und zwischen beyden ist kaum einig unterscheid.", "tokens": ["Und", "zwi\u00b7schen", "bey\u00b7den", "ist", "kaum", "ei\u00b7nig", "un\u00b7ter\u00b7scheid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "VAFIN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Man wei\u00df nicht/ wer sey Mars/ wenn sie die waffen \u00fcben/", "tokens": ["Man", "wei\u00df", "nicht", "/", "wer", "sey", "Mars", "/", "wenn", "sie", "die", "waf\u00b7fen", "\u00fc\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKNEG", "$(", "PWS", "VAFIN", "NN", "$(", "KOUS", "PPER", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Nicht/ wer die liebe sey/ wenn sie einander lieben.", "tokens": ["Nicht", "/", "wer", "die", "lie\u00b7be", "sey", "/", "wenn", "sie", "ein\u00b7an\u00b7der", "lie\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$(", "PWS", "ART", "ADJA", "VAFIN", "$(", "KOUS", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}