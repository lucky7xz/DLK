{"textgrid.poem.44933": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mit Fleischregie betraut und Studienreferat,", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit Fleischregie betraut und Studienreferat,", "tokens": ["Mit", "Flei\u00b7schre\u00b7gie", "be\u00b7traut", "und", "Stu\u00b7di\u00b7en\u00b7re\u00b7fe\u00b7rat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "KON", "NN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Vermischt er oft die F\u00e4cher, ob nicht gerne,", "tokens": ["Ver\u00b7mischt", "er", "oft", "die", "F\u00e4\u00b7cher", ",", "ob", "nicht", "ger\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,", "KOUS", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und bracht im Vorschlag f\u00fcr die Bibliothek", "tokens": ["Und", "bracht", "im", "Vor\u00b7schlag", "f\u00fcr", "die", "Bib\u00b7lio\u00b7thek"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "J\u00fcngst aus Versehn drei Ochsen in die Terne.", "tokens": ["J\u00fcngst", "aus", "Ver\u00b7sehn", "drei", "Och\u00b7sen", "in", "die", "Ter\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "CARD", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}