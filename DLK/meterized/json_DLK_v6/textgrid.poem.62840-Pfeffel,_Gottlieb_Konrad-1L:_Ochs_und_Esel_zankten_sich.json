{"textgrid.poem.62840": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ochs und Esel zankten sich", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ochs und Esel zankten sich", "tokens": ["Ochs", "und", "E\u00b7sel", "zank\u00b7ten", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Beym Spatziergang um die Wette,", "tokens": ["Beym", "Spat\u00b7zier\u00b7gang", "um", "die", "Wet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer am meisten Weisheit h\u00e4tte:", "tokens": ["Wer", "am", "meis\u00b7ten", "Weis\u00b7heit", "h\u00e4t\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Keiner siegte, keiner wich.", "tokens": ["Kei\u00b7ner", "sieg\u00b7te", ",", "kei\u00b7ner", "wich", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PIS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Endlich kam man \u00fcberein,", "tokens": ["End\u00b7lich", "kam", "man", "\u00fc\u00b7be\u00b7re\u00b7in", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PTKVZ", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Da\u00df der L\u00f6we, wenn er wollte,", "tokens": ["Da\u00df", "der", "L\u00f6\u00b7we", ",", "wenn", "er", "woll\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NE", "$,", "KOUS", "PPER", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Diesen Streit entscheiden sollte;", "tokens": ["Die\u00b7sen", "Streit", "ent\u00b7schei\u00b7den", "soll\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und was konnte kl\u00fcger seyn?", "tokens": ["Und", "was", "konn\u00b7te", "kl\u00fc\u00b7ger", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VMFIN", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Beyde reden tief geb\u00fcckt", "tokens": ["Bey\u00b7de", "re\u00b7den", "tief", "ge\u00b7b\u00fcckt"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADJD", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Vor des Thierbeherrschers Throne,", "tokens": ["Vor", "des", "Thier\u00b7be\u00b7herr\u00b7schers", "Thro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der mit einem edlen Hohne", "tokens": ["Der", "mit", "ei\u00b7nem", "ed\u00b7len", "Hoh\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf das Paar herunter blickt.", "tokens": ["Auf", "das", "Paar", "her\u00b7un\u00b7ter", "blickt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APZR", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Endlich sprach die Majest\u00e4t", "tokens": ["End\u00b7lich", "sprach", "die", "Ma\u00b7jes\u00b7t\u00e4t"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu dem Esel und dem Farren:", "tokens": ["Zu", "dem", "E\u00b7sel", "und", "dem", "Far\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Ihr seyd alle beyde Narren.", "tokens": ["Ihr", "seyd", "al\u00b7le", "bey\u00b7de", "Nar\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jeder gafft ihn an und geht.", "tokens": ["Je\u00b7der", "gafft", "ihn", "an", "und", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}