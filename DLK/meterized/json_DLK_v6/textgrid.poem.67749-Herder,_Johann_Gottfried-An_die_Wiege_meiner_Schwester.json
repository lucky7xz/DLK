{"textgrid.poem.67749": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "An die Wiege meiner Schwester", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein erster Palast, Du der j\u00fcngsten Thr\u00e4nen", "tokens": ["Mein", "ers\u00b7ter", "Pa\u00b7last", ",", "Du", "der", "j\u00fcng\u00b7sten", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geerbter liebster Trost,", "tokens": ["Ge\u00b7erb\u00b7ter", "liebs\u00b7ter", "Trost", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo ich, wenn Sybars auch auf Rosen g\u00e4hnen,", "tokens": ["Wo", "ich", ",", "wenn", "Sy\u00b7bars", "auch", "auf", "Ro\u00b7sen", "g\u00e4h\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "KOUS", "NE", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sanft wie die Unschuld schlief und meine zweite Kost,", "tokens": ["Sanft", "wie", "die", "Un\u00b7schuld", "schlief", "und", "mei\u00b7ne", "zwei\u00b7te", "Kost", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "VVFIN", "KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Den Schlaf, wie Muttermilch einsaugen lernte,", "tokens": ["Den", "Schlaf", ",", "wie", "Mut\u00b7ter\u00b7milch", "ein\u00b7sau\u00b7gen", "lern\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "NN", "VVIZU", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo ich mein Jetzt gena\u00df,", "tokens": ["Wo", "ich", "mein", "Jetzt", "ge\u00b7na\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mich selbst im Traum empfand und das Entfernte", "tokens": ["Mich", "selbst", "im", "Traum", "emp\u00b7fand", "und", "das", "Ent\u00b7fern\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "APPRART", "NN", "VVFIN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So leicht als Tugend ihren Feind verga\u00df.", "tokens": ["So", "leicht", "als", "Tu\u00b7gend", "ih\u00b7ren", "Feind", "ver\u00b7ga\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Zu Dir entflieh' ich, weiser Thorheit m\u00fcde;", "tokens": ["Zu", "Dir", "ent\u00b7flieh'", "ich", ",", "wei\u00b7ser", "Thor\u00b7heit", "m\u00fc\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "$,", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn, Wiege, heut empfing", "tokens": ["Denn", ",", "Wie\u00b7ge", ",", "heut", "emp\u00b7fing"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "$,", "NN", "$,", "ADV", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dein Schoo\u00df einst Die (o jedem Heut sei Friede!),", "tokens": ["Dein", "Schoo\u00df", "einst", "Die", "(", "o", "je\u00b7dem", "Heut", "sei", "Frie\u00b7de", "!", ")", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ART", "$(", "FM", "PIAT", "NN", "VAFIN", "NN", "$.", "$(", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit der ich einst an ", "tokens": ["Mit", "der", "ich", "einst", "an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "ADV", "APPR"], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "An ", "tokens": ["An"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Von ", "tokens": ["Von"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "F\u00fcr die ich ewig brenne, der der Bogen", "tokens": ["F\u00fcr", "die", "ich", "e\u00b7wig", "bren\u00b7ne", ",", "der", "der", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "ADJD", "VVFIN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Opferrauchs sich w\u00f6lbt, f\u00fcr die nach Tempens Art", "tokens": ["Des", "Op\u00b7fer\u00b7rauchs", "sich", "w\u00f6lbt", ",", "f\u00fcr", "die", "nach", "Tem\u00b7pens", "Art"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PRF", "VVFIN", "$,", "APPR", "ART", "APPR", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ich heut, o Wiege, Dich mit Liedern kr\u00e4nze,", "tokens": ["Ich", "heut", ",", "o", "Wie\u00b7ge", ",", "Dich", "mit", "Lie\u00b7dern", "kr\u00e4n\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "FM", "NN", "$,", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df Deine Unschuldsruh,", "tokens": ["Da\u00df", "Dei\u00b7ne", "Un\u00b7schulds\u00b7ruh", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr Jugendschmuck, sie siebenfach umgl\u00e4nze,", "tokens": ["Ihr", "Ju\u00b7gend\u00b7schmuck", ",", "sie", "sie\u00b7ben\u00b7fach", "um\u00b7gl\u00e4n\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als Grazie verkl\u00e4r' und segne; da\u00df auch Du", "tokens": ["Als", "Gra\u00b7zie", "ver\u00b7kl\u00e4r'", "und", "seg\u00b7ne", ";", "da\u00df", "auch", "Du"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "NN", "VVFIN", "KON", "VVFIN", "$.", "KOUS", "ADV", "PPER"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Oft T\u00e4nze um Dich sehst, Dir Lieder schallen,", "tokens": ["Oft", "T\u00e4n\u00b7ze", "um", "Dich", "sehst", ",", "Dir", "Lie\u00b7der", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "PPER", "VVFIN", "$,", "PPER", "NN", "VVFIN", "$,"], "meter": "-+--++-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die meine Seele f\u00fchlt,", "tokens": ["Die", "mei\u00b7ne", "See\u00b7le", "f\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Jovah h\u00f6rt, den Eltern Wohlgefallen,", "tokens": ["Die", "Jo\u00b7vah", "h\u00f6rt", ",", "den", "El\u00b7tern", "Wohl\u00b7ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und sie, sie selbst, den Schwestern wiederspielt.", "tokens": ["Und", "sie", ",", "sie", "selbst", ",", "den", "Schwes\u00b7tern", "wie\u00b7der\u00b7spielt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPER", "ADV", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}