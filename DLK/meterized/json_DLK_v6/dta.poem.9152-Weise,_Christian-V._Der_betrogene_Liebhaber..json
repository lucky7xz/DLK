{"dta.poem.9152": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "V.  \n Der betrogene Liebhaber.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Kommt ihr leute kommt und schauet", "tokens": ["Kommt", "ihr", "leu\u00b7te", "kommt", "und", "schau\u00b7et"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "VVFIN", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mein betr\u00fcbt exempel an/", "tokens": ["Mein", "be\u00b7tr\u00fcbt", "ex\u00b7em\u00b7pel", "an", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVPP", "NN", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seht wie einer fallen kan", "tokens": ["Seht", "wie", "ei\u00b7ner", "fal\u00b7len", "kan"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "KOKOM", "ART", "VVINF", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Welcher auff die liebe bauet/", "tokens": ["Wel\u00b7cher", "auff", "die", "lie\u00b7be", "bau\u00b7et", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "APPR", "ART", "ADJA", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und sein leben auf das spiel", "tokens": ["Und", "sein", "le\u00b7ben", "auf", "das", "spiel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Eitler wollust gr\u00fcnden wil.", "tokens": ["Eit\u00b7ler", "wol\u00b7lust", "gr\u00fcn\u00b7den", "wil", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "2. W\u00e4r ich vor so klug gewesen", "tokens": ["W\u00e4r", "ich", "vor", "so", "klug", "ge\u00b7we\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "ADV", "ADJD", "VAPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als ich itzo worden bin/", "tokens": ["Als", "ich", "it\u00b7zo", "wor\u00b7den", "bin", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VAPP", "VAFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00e4tte mein betrogner sinn", "tokens": ["H\u00e4t\u00b7te", "mein", "be\u00b7trog\u00b7ner", "sinn"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wol was bessers ausserlesen/", "tokens": ["Wol", "was", "bes\u00b7sers", "aus\u00b7ser\u00b7le\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "ADV", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber ach ich armes kind", "tokens": ["A\u00b7ber", "ach", "ich", "ar\u00b7mes", "kind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "XY", "PPER", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "War an beyden augen blind!", "tokens": ["War", "an", "bey\u00b7den", "au\u00b7gen", "blind", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PIAT", "NN", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "3. Die beliebten purpur-wangen", "tokens": ["Die", "be\u00b7lieb\u00b7ten", "pur\u00b7pur\u00b7wan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nahmen mir das leben ein/", "tokens": ["Nah\u00b7men", "mir", "das", "le\u00b7ben", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PDS", "VVFIN", "ART", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und erregten durch den schein", "tokens": ["Und", "er\u00b7reg\u00b7ten", "durch", "den", "schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles hoffen und verlangen/", "tokens": ["Al\u00b7les", "hof\u00b7fen", "und", "ver\u00b7lan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVINF", "KON", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bi\u00df ich alles was ich fand", "tokens": ["Bi\u00df", "ich", "al\u00b7les", "was", "ich", "fand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PIS", "PWS", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Jhr und ihre gunst verband.", "tokens": ["Ihr", "und", "ih\u00b7re", "gunst", "ver\u00b7band", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "4. Was ich dichte/ was ich machte/", "tokens": ["Was", "ich", "dich\u00b7te", "/", "was", "ich", "mach\u00b7te", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$(", "PWS", "PPER", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "War auf sie allein gericht/", "tokens": ["War", "auf", "sie", "al\u00b7lein", "ge\u00b7richt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "ADV", "VVPP", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie/ mein engel/ war mein liecht/", "tokens": ["Sie", "/", "mein", "en\u00b7gel", "/", "war", "mein", "liecht", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "PPOSAT", "NN", "$(", "VAFIN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und wann ich an sie gedachte/", "tokens": ["Und", "wann", "ich", "an", "sie", "ge\u00b7dach\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Lieff das blut in einem nu", "tokens": ["Lieff", "das", "blut", "in", "ei\u00b7nem", "nu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "APPR", "ART", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dem verliebten hertzen zu.", "tokens": ["Dem", "ver\u00b7lieb\u00b7ten", "hert\u00b7zen", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "5. Wann ich gleich in einem tag", "tokens": ["Wann", "ich", "gleich", "in", "ei\u00b7nem", "tag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kaum ein halbes blickgen sah/", "tokens": ["Kaum", "ein", "hal\u00b7bes", "blick\u00b7gen", "sah", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gieng mir doch der strahl so nah/", "tokens": ["Gieng", "mir", "doch", "der", "strahl", "so", "nah", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "ADV", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df ich ausser aller klage/", "tokens": ["Da\u00df", "ich", "aus\u00b7ser", "al\u00b7ler", "kla\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PIAT", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Gleichsam als zu grossem danck", "tokens": ["Gleich\u00b7sam", "als", "zu", "gros\u00b7sem", "danck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Manch erfreutes lust-lied sang.", "tokens": ["Manch", "er\u00b7freu\u00b7tes", "lust\u00b7lied", "sang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "6. Aber nun bin ich verdorben/", "tokens": ["A\u00b7ber", "nun", "bin", "ich", "ver\u00b7dor\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seit mir die gelegenheit", "tokens": ["Seit", "mir", "die", "ge\u00b7le\u00b7gen\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "NN"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Zu dergleichen freundlichkeit", "tokens": ["Zu", "derg\u00b7lei\u00b7chen", "freund\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIS", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Unverhofft ist abgestorben/", "tokens": ["Un\u00b7ver\u00b7hofft", "ist", "ab\u00b7ge\u00b7stor\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und mein hertze keine statt", "tokens": ["Und", "mein", "hert\u00b7ze", "kei\u00b7ne", "statt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mehr in ihrem hertzen hat.", "tokens": ["Mehr", "in", "ih\u00b7rem", "hert\u00b7zen", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "7. Alle lust ist mir zu wider/", "tokens": ["Al\u00b7le", "lust", "ist", "mir", "zu", "wi\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "PTKZU", "PTKVZ", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was ich sehn und h\u00f6ren mu\u00df/", "tokens": ["Was", "ich", "sehn", "und", "h\u00f6\u00b7ren", "mu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVINF", "KON", "VVINF", "VMFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bringt mir lauter \u00fcberdru\u00df/", "tokens": ["Bringt", "mir", "lau\u00b7ter", "\u00fc\u00b7berd\u00b7ru\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die allersch\u00f6nsten lieder", "tokens": ["Und", "die", "al\u00b7ler\u00b7sch\u00f6ns\u00b7ten", "lie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Von der liebe kommen mir", "tokens": ["Von", "der", "lie\u00b7be", "kom\u00b7men", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Abgeschmackt und alber f\u00fcr.", "tokens": ["Ab\u00b7ge\u00b7schmackt", "und", "al\u00b7ber", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADV", "APPR", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "8. Wann ich ja bi\u00dfweilen schertze/", "tokens": ["Wann", "ich", "ja", "bi\u00df\u00b7wei\u00b7len", "schert\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und mein altes freuden-spiel", "tokens": ["Und", "mein", "al\u00b7tes", "freu\u00b7den\u00b7spiel"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wiederum verneuen will/", "tokens": ["Wie\u00b7de\u00b7rum", "ver\u00b7neu\u00b7en", "will", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach so rufft mein schwaches hertze", "tokens": ["Ach", "so", "rufft", "mein", "schwa\u00b7ches", "hert\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mitten in der s\u00fcssen ruh", "tokens": ["Mit\u00b7ten", "in", "der", "s\u00fcs\u00b7sen", "ruh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Meinem schmertzen wieder zu.", "tokens": ["Mei\u00b7nem", "schmert\u00b7zen", "wie\u00b7der", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Zwar ich sehe schon von weiten", "tokens": ["Zwar", "ich", "se\u00b7he", "schon", "von", "wei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "APPR", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df die zeit mich tr\u00f6sten kan/", "tokens": ["Da\u00df", "die", "zeit", "mich", "tr\u00f6s\u00b7ten", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVINF", "VMFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Doch wiewohl ist der daran/", "tokens": ["Doch", "wie\u00b7wohl", "ist", "der", "da\u00b7ran", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "PAV", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "Welcher solchen eitelkeiten", "tokens": ["Wel\u00b7cher", "sol\u00b7chen", "ei\u00b7tel\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["PWAT", "PIAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Seine seele nicht ergiebt/", "tokens": ["Sei\u00b7ne", "see\u00b7le", "nicht", "er\u00b7giebt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "PTKNEG", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Oder doch gel\u00fccklich liebt.", "tokens": ["O\u00b7der", "doch", "ge\u00b7l\u00fcck\u00b7lich", "liebt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}