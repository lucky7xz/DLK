{"textgrid.poem.52270": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Die Toten an die Lebenden", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Kugel mitten in der Brust, die Stirne breit gespalten,", "tokens": ["Die", "Ku\u00b7gel", "mit\u00b7ten", "in", "der", "Brust", ",", "die", "Stir\u00b7ne", "breit", "ge\u00b7spal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ART", "NN", "$,", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "So habt ihr uns auf blut'gem Brett hoch in die Luft gehalten!", "tokens": ["So", "habt", "ihr", "uns", "auf", "blut'\u00b7gem", "Brett", "hoch", "in", "die", "Luft", "ge\u00b7hal\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Hoch in die Luft mit wildem Schrei, da\u00df unsre Schmerzgeb\u00e4rde", "tokens": ["Hoch", "in", "die", "Luft", "mit", "wil\u00b7dem", "Schrei", ",", "da\u00df", "uns\u00b7re", "Schmerz\u00b7ge\u00b7b\u00e4r\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Dem, der zu t\u00f6ten uns befahl, ein Fluch auf ewig werde!", "tokens": ["Dem", ",", "der", "zu", "t\u00f6\u00b7ten", "uns", "be\u00b7fahl", ",", "ein", "Fluch", "auf", "e\u00b7wig", "wer\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PTKZU", "VVFIN", "PPER", "VVFIN", "$,", "ART", "NN", "APPR", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.5": {"text": "Da\u00df er sie sehe, Tag und Nacht, im Wachen und im Traume \u2013", "tokens": ["Da\u00df", "er", "sie", "se\u00b7he", ",", "Tag", "und", "Nacht", ",", "im", "Wa\u00b7chen", "und", "im", "Trau\u00b7me", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "NN", "KON", "NN", "$,", "APPRART", "NN", "KON", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Im \u00d6ffnen seines Bibelbuchs wie im Champagnerschaume!", "tokens": ["Im", "\u00d6ff\u00b7nen", "sei\u00b7nes", "Bi\u00b7bel\u00b7buchs", "wie", "im", "Cham\u00b7pag\u00b7ner\u00b7schau\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "KOKOM", "APPRART", "NN", "$."], "meter": "-+-+-+-+--++-+-", "measure": "iambic.septa.relaxed"}, "line.7": {"text": "Da\u00df wie ein Brandmal sie sich tief in seine Seele brenne:", "tokens": ["Da\u00df", "wie", "ein", "Brand\u00b7mal", "sie", "sich", "tief", "in", "sei\u00b7ne", "See\u00b7le", "bren\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "PPER", "PRF", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "Da\u00df nirgendwo und nimmermehr er vor ihr fliehen k\u00f6nne!", "tokens": ["Da\u00df", "nir\u00b7gend\u00b7wo", "und", "nim\u00b7mer\u00b7mehr", "er", "vor", "ihr", "flie\u00b7hen", "k\u00f6n\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "KON", "ADV", "PPER", "APPR", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.9": {"text": "Da\u00df jeder qualverzogne Mund, da\u00df jede rote Wunde", "tokens": ["Da\u00df", "je\u00b7der", "qual\u00b7ver\u00b7zog\u00b7ne", "Mund", ",", "da\u00df", "je\u00b7de", "ro\u00b7te", "Wun\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "ADJA", "NN", "$,", "KOUS", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.10": {"text": "Ihn schrecke noch, ihn \u00e4ngste noch in seiner letzten Stunde!", "tokens": ["Ihn", "schre\u00b7cke", "noch", ",", "ihn", "\u00e4ngs\u00b7te", "noch", "in", "sei\u00b7ner", "letz\u00b7ten", "Stun\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.11": {"text": "Da\u00df jedes Schluchzen um us her dem Sterbenden noch schalle,", "tokens": ["Da\u00df", "je\u00b7des", "Schluch\u00b7zen", "um", "us", "her", "dem", "Ster\u00b7ben\u00b7den", "noch", "schal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "APPR", "NE", "APZR", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.12": {"text": "Da\u00df jede tote Faust sich noch nach seinem Haupte balle \u2013", "tokens": ["Da\u00df", "je\u00b7de", "to\u00b7te", "Faust", "sich", "noch", "nach", "sei\u00b7nem", "Haup\u00b7te", "bal\u00b7le", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ADJA", "NN", "PRF", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.13": {"text": "M\u00f6g' er das Haupt nun auf ein Bett, wie andre Leute pflegen,", "tokens": ["M\u00f6g'", "er", "das", "Haupt", "nun", "auf", "ein", "Bett", ",", "wie", "and\u00b7re", "Leu\u00b7te", "pfle\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ADV", "APPR", "ART", "NN", "$,", "PWAV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.14": {"text": "M\u00f6g' er es auf ein Blutger\u00fcst zum letzten Atmen legen!", "tokens": ["M\u00f6g'", "er", "es", "auf", "ein", "Blut\u00b7ge\u00b7r\u00fcst", "zum", "letz\u00b7ten", "At\u00b7men", "le\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "APPR", "ART", "NN", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.2": {"line.1": {"text": "So war's! Die Kugel in der Brust, die Stirne breit gespalten,", "tokens": ["So", "wa\u00b7r's", "!", "Die", "Ku\u00b7gel", "in", "der", "Brust", ",", "die", "Stir\u00b7ne", "breit", "ge\u00b7spal\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$.", "ART", "NN", "APPR", "ART", "NN", "$,", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+--+-+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "So habt ihr uns auf schwankem Brett auf zum Altan gehalten!", "tokens": ["So", "habt", "ihr", "uns", "auf", "schwan\u00b7kem", "Brett", "auf", "zum", "Al\u00b7tan", "ge\u00b7hal\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "APPR", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "\u00bbherunter!\u00ab \u2013 und er kam gewankt \u2013 gewankt an unser Bette;", "tokens": ["\u00bb", "her\u00b7un\u00b7ter", "!", "\u00ab", "\u2013", "und", "er", "kam", "ge\u00b7wankt", "\u2013", "ge\u00b7wankt", "an", "un\u00b7ser", "Bet\u00b7te", ";"], "token_info": ["punct", "word", "punct", "punct", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$.", "$(", "$(", "KON", "PPER", "VVFIN", "VVPP", "$(", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "\u00bbhut ab!\u00ab \u2013 er zog \u2013 er neigte sich! (so sank zur Marionette,", "tokens": ["\u00bb", "hut", "ab", "!", "\u00ab", "\u2013", "er", "zog", "\u2013", "er", "neig\u00b7te", "sich", "!", "(", "so", "sank", "zur", "Ma\u00b7ri\u00b7o\u00b7net\u00b7te", ","], "token_info": ["punct", "word", "word", "punct", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "$.", "$(", "$(", "PPER", "VVFIN", "$(", "PPER", "VVFIN", "PRF", "$.", "$(", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.5": {"text": "Der erst ein Kom\u00f6diante war!) \u2013 bleich stand er und beklommen!", "tokens": ["Der", "erst", "ein", "Ko\u00b7m\u00f6\u00b7di\u00b7an\u00b7te", "war", "!", ")", "\u2013", "bleich", "stand", "er", "und", "be\u00b7klom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VAFIN", "$.", "$(", "$(", "ADJD", "VVFIN", "PPER", "KON", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Das Heer indes verlie\u00df die Stadt, die sterbend wir genommen!", "tokens": ["Das", "Heer", "in\u00b7des", "ver\u00b7lie\u00df", "die", "Stadt", ",", "die", "ster\u00b7bend", "wir", "ge\u00b7nom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.7": {"text": "Dann \u00bbJesus meine Zuversicht!\u00ab wie ihr's im Buch k\u00f6nnt lesen:", "tokens": ["Dann", "\u00bb", "Je\u00b7sus", "mei\u00b7ne", "Zu\u00b7ver\u00b7sicht", "!", "\u00ab", "wie", "ih\u00b7r's", "im", "Buch", "k\u00f6nnt", "le\u00b7sen", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "NE", "PPOSAT", "NN", "$.", "$(", "PWAV", "PIS", "APPRART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.8": {"text": "Ein \u00bbEisen meine Zuversicht!\u00ab w\u00e4r' pa\u00dflicher gewesen!", "tokens": ["Ein", "\u00bb", "Ei\u00b7sen", "mei\u00b7ne", "Zu\u00b7ver\u00b7sicht", "!", "\u00ab", "w\u00e4r'", "pa\u00df\u00b7li\u00b7cher", "ge\u00b7we\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$(", "NN", "PPOSAT", "NN", "$.", "$(", "VAFIN", "ADJD", "VAPP", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.3": {"line.1": {"text": "Das war den Morgen auf die Nacht, in der man uns erschlagen;", "tokens": ["Das", "war", "den", "Mor\u00b7gen", "auf", "die", "Nacht", ",", "in", "der", "man", "uns", "er\u00b7schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$,", "APPR", "PRELS", "PIS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "So habt ihr triumphierend und in unsre Gruft getragen!", "tokens": ["So", "habt", "ihr", "tri\u00b7um\u00b7phie\u00b7rend", "und", "in", "uns\u00b7re", "Gruft", "ge\u00b7tra\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Und wir \u2013 wohl war der Sch\u00e4del uns zerschossen und zerhauen,", "tokens": ["Und", "wir", "\u2013", "wohl", "war", "der", "Sch\u00e4\u00b7del", "uns", "zer\u00b7schos\u00b7sen", "und", "zer\u00b7hau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$(", "ADV", "VAFIN", "ART", "NN", "PPER", "VVPP", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Doch lag des Sieges froher Stolz auf unsern grimmen Brauen.", "tokens": ["Doch", "lag", "des", "Sie\u00b7ges", "fro\u00b7her", "Stolz", "auf", "un\u00b7sern", "grim\u00b7men", "Brau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.5": {"text": "Wir dachten: Hoch zwar ist der Preis, doch echt auch ist die Ware!", "tokens": ["Wir", "dach\u00b7ten", ":", "Hoch", "zwar", "ist", "der", "Preis", ",", "doch", "echt", "auch", "ist", "die", "Wa\u00b7re", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ADJD", "ADV", "VAFIN", "ART", "NN", "$,", "ADV", "ADJD", "ADV", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Und legten uns in Frieden drum zurecht auf unsrer Bahre.", "tokens": ["Und", "leg\u00b7ten", "uns", "in", "Frie\u00b7den", "drum", "zu\u00b7recht", "auf", "uns\u00b7rer", "Bah\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "PAV", "PTKVZ", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.4": {"line.1": {"text": "Weh euch, wir haben uns get\u00e4uscht! Vier Monden erst vergangen,", "tokens": ["Weh", "euch", ",", "wir", "ha\u00b7ben", "uns", "ge\u00b7t\u00e4uscht", "!", "Vier", "Mon\u00b7den", "erst", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PPER", "VAFIN", "PPER", "VVPP", "$.", "CARD", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Und alles feig durch euch verscherzt, was trotzig wir errangen!", "tokens": ["Und", "al\u00b7les", "feig", "durch", "euch", "ver\u00b7scherzt", ",", "was", "trot\u00b7zig", "wir", "er\u00b7ran\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADJD", "APPR", "PPER", "VVPP", "$,", "PRELS", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "O, alles, alles h\u00f6rten wir mit leisen Geisterohren!", "tokens": ["O", ",", "al\u00b7les", ",", "al\u00b7les", "h\u00f6r\u00b7ten", "wir", "mit", "lei\u00b7sen", "Geis\u00b7te\u00b7roh\u00b7ren", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PIS", "$,", "PIS", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Wie Wellen braust' an uns heran, was sich begab im Lande:", "tokens": ["Wie", "Wel\u00b7len", "braust'", "an", "uns", "he\u00b7ran", ",", "was", "sich", "be\u00b7gab", "im", "Lan\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$,", "PRELS", "PRF", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.5": {"text": "Der Aberwitz des D\u00e4nenkriegs, die letzte Polenschande;", "tokens": ["Der", "A\u00b7ber\u00b7witz", "des", "D\u00e4\u00b7nen\u00b7kriegs", ",", "die", "letz\u00b7te", "Po\u00b7len\u00b7schan\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Das r\u00fcde Toben der Vendee in stockigen Provinzen;", "tokens": ["Das", "r\u00fc\u00b7de", "To\u00b7ben", "der", "Ven\u00b7dee", "in", "sto\u00b7cki\u00b7gen", "Pro\u00b7vin\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+--++--+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Der Soldateska Wiederkehr, die Wiederkehr des Prinzen;", "tokens": ["Der", "Sol\u00b7da\u00b7tes\u00b7ka", "Wie\u00b7der\u00b7kehr", ",", "die", "Wie\u00b7der\u00b7kehr", "des", "Prin\u00b7zen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "Die Sc