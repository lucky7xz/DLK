{"textgrid.poem.38739": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der ist nicht einsam, der noch Schmerzen f\u00fchlet,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der ist nicht einsam, der noch Schmerzen f\u00fchlet,", "tokens": ["Der", "ist", "nicht", "ein\u00b7sam", ",", "der", "noch", "Schmer\u00b7zen", "f\u00fch\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKNEG", "ADJD", "$,", "PRELS", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verlassen von den Freunden und der Welt,", "tokens": ["Ver\u00b7las\u00b7sen", "von", "den", "Freun\u00b7den", "und", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn er die hei\u00dfe Angst in Trauer k\u00fchlet,", "tokens": ["Wenn", "er", "die", "hei\u00b7\u00dfe", "Angst", "in", "Trau\u00b7er", "k\u00fch\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und des Verlustes Bild im Herzen h\u00e4lt,", "tokens": ["Und", "des", "Ver\u00b7lus\u00b7tes", "Bild", "im", "Her\u00b7zen", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Vergangenheit noch kindlich um ihn spielet", "tokens": ["Ver\u00b7gan\u00b7gen\u00b7heit", "noch", "kind\u00b7lich", "um", "ihn", "spie\u00b7let"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ADJD", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und Zukunft ihren Spiegel vor ihn stellt:", "tokens": ["Und", "Zu\u00b7kunft", "ih\u00b7ren", "Spie\u00b7gel", "vor", "ihn", "stellt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dem sind die Schmerzen Freunde wie die Thr\u00e4nen,", "tokens": ["Dem", "sind", "die", "Schmer\u00b7zen", "Freun\u00b7de", "wie", "die", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und er genie\u00dft sich selbst im stillen Sehnen.", "tokens": ["Und", "er", "ge\u00b7nie\u00dft", "sich", "selbst", "im", "stil\u00b7len", "Seh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch wenn das Herz entfremdet f\u00fchlt die Lieben,", "tokens": ["Doch", "wenn", "das", "Herz", "ent\u00b7frem\u00b7det", "f\u00fchlt", "die", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVPP", "VVFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch Misverst\u00e4ndni\u00df von ihm abgewandt,", "tokens": ["Durch", "Mis\u00b7ver\u00b7st\u00e4nd\u00b7ni\u00df", "von", "ihm", "ab\u00b7ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dann mu\u00df der Mensch sich inniglich betr\u00fcben,", "tokens": ["Dann", "mu\u00df", "der", "Mensch", "sich", "in\u00b7nig\u00b7lich", "be\u00b7tr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dann wandert er aus seinem Vaterland,", "tokens": ["Dann", "wan\u00b7dert", "er", "aus", "sei\u00b7nem", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und keine St\u00e4tt' ist ihm, kein Heil geblieben;", "tokens": ["Und", "kei\u00b7ne", "St\u00e4tt'", "ist", "ihm", ",", "kein", "Heil", "ge\u00b7blie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "PPER", "$,", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Er ist von Tempel, Weib und Kind verbannt,", "tokens": ["Er", "ist", "von", "Tem\u00b7pel", ",", "Weib", "und", "Kind", "ver\u00b7bannt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "$,", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wohin er schaut, ist ihm die Welt getrennt,", "tokens": ["Wo\u00b7hin", "er", "schaut", ",", "ist", "ihm", "die", "Welt", "ge\u00b7trennt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und feindlich dr\u00e4ut ihm selbst das Element.", "tokens": ["Und", "feind\u00b7lich", "dr\u00e4ut", "ihm", "selbst", "das", "E\u00b7le\u00b7ment", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dann f\u00fchlt das Herz den Todesdruck der Schwere,", "tokens": ["Dann", "f\u00fchlt", "das", "Herz", "den", "To\u00b7des\u00b7druck", "der", "Schwe\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und um sich ausgestorben die Natur;", "tokens": ["Und", "um", "sich", "aus\u00b7ge\u00b7stor\u00b7ben", "die", "Na\u00b7tur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUI", "PRF", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Rings Einsamkeit, und dunkle w\u00fcste Leere", "tokens": ["Rings", "Ein\u00b7sam\u00b7keit", ",", "und", "dunk\u00b7le", "w\u00fcs\u00b7te", "Lee\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NN", "$,", "KON", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zieth sich durch Thal und Wald und gr\u00fcne Flur;", "tokens": ["Zieth", "sich", "durch", "Thal", "und", "Wald", "und", "gr\u00fc\u00b7ne", "Flur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "KON", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Freunde waren, stehn im Feindesheere,", "tokens": ["Die", "Freun\u00b7de", "wa\u00b7ren", ",", "stehn", "im", "Fein\u00b7des\u00b7hee\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der wilde Ha\u00df verfolget seine Spur,", "tokens": ["Der", "wil\u00b7de", "Ha\u00df", "ver\u00b7fol\u00b7get", "sei\u00b7ne", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die innre Liebe strebt empor zu flammen,", "tokens": ["Die", "inn\u00b7re", "Lie\u00b7be", "strebt", "em\u00b7por", "zu", "flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Doch dr\u00fcckt die schwarze Nacht das Licht zusammen.", "tokens": ["Doch", "dr\u00fcckt", "die", "schwar\u00b7ze", "Nacht", "das", "Licht", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dann bin ich fern im Tode fest verschlossen,", "tokens": ["Dann", "bin", "ich", "fern", "im", "To\u00b7de", "fest", "ver\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "APPRART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich h\u00f6re keinen Ton, der zu mir dringt,", "tokens": ["Ich", "h\u00f6\u00b7re", "kei\u00b7nen", "Ton", ",", "der", "zu", "mir", "dringt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und Freud' und Schmerz sind aus der Brust geflossen,", "tokens": ["Und", "Freud'", "und", "Schmerz", "sind", "aus", "der", "Brust", "ge\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die in sich selbst in tiefsten Aengsten ringt,", "tokens": ["Die", "in", "sich", "selbst", "in", "tiefs\u00b7ten", "A\u00b7engs\u00b7ten", "ringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PRF", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Auch kein Erinnern des, was sie genossen,", "tokens": ["Auch", "kein", "E\u00b7rin\u00b7nern", "des", ",", "was", "sie", "ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ART", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In ihrer tauben Leere wiederklingt,", "tokens": ["In", "ih\u00b7rer", "tau\u00b7ben", "Lee\u00b7re", "wie\u00b7der\u00b7klingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und h\u00f6hnend ruft der innre b\u00f6se Feind:", "tokens": ["Und", "h\u00f6h\u00b7nend", "ruft", "der", "inn\u00b7re", "b\u00f6\u00b7se", "Feind", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Gen\u00fcge dir, so wie du sonst gemeint!", "tokens": ["Ge\u00b7n\u00fc\u00b7ge", "dir", ",", "so", "wie", "du", "sonst", "ge\u00b7meint", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "ADV", "KOKOM", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ich bin gefangen, seufzt die arme Seele,", "tokens": ["Ich", "bin", "ge\u00b7fan\u00b7gen", ",", "seufzt", "die", "ar\u00b7me", "See\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bedarf wohl deren, welche mich verstehn;", "tokens": ["Be\u00b7darf", "wohl", "de\u00b7ren", ",", "wel\u00b7che", "mich", "ver\u00b7stehn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PDS", "$,", "PRELS", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch wenn ich mich so stumm verlassen qu\u00e4le,", "tokens": ["Doch", "wenn", "ich", "mich", "so", "stumm", "ver\u00b7las\u00b7sen", "qu\u00e4\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "ADV", "ADJD", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So mu\u00df ich in mir selbst zu Grunde gehn.", "tokens": ["So", "mu\u00df", "ich", "in", "mir", "selbst", "zu", "Grun\u00b7de", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "PPER", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Was frommt es, wenn ich dir den Wunsch verhehle?", "tokens": ["Was", "frommt", "es", ",", "wenn", "ich", "dir", "den", "Wunsch", "ver\u00b7heh\u00b7le", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ich mu\u00df mein Licht in andern Augen sehn;", "tokens": ["Ich", "mu\u00df", "mein", "Licht", "in", "an\u00b7dern", "Au\u00b7gen", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Mit jenen eins, bin ich von dir befreiet,", "tokens": ["Mit", "je\u00b7nen", "eins", ",", "bin", "ich", "von", "dir", "be\u00b7frei\u00b7et", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "PIS", "$,", "VAFIN", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mit mir allein, bin ich mir selbst entzweiet.", "tokens": ["Mit", "mir", "al\u00b7lein", ",", "bin", "ich", "mir", "selbst", "ent\u00b7zwei\u00b7et", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "$,", "VAFIN", "PPER", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Mit ihnen seh' ich die mir abw\u00e4rts neigen,", "tokens": ["Mit", "ih\u00b7nen", "seh'", "ich", "die", "mir", "ab\u00b7w\u00e4rts", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "ART", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die von der todten Welt sich schon geschieden,", "tokens": ["Die", "von", "der", "tod\u00b7ten", "Welt", "sich", "schon", "ge\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und die ich seelig f\u00fchlte stets mein eigen;", "tokens": ["Und", "die", "ich", "see\u00b7lig", "f\u00fchl\u00b7te", "stets", "mein", "ei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELS", "PPER", "ADJD", "VVFIN", "ADV", "PPOSAT", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von Wald und Flur und Thal bin ich vermieden,", "tokens": ["Von", "Wald", "und", "Flur", "und", "Thal", "bin", "ich", "ver\u00b7mie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Blumen wollen sich nicht freundlich zeigen,", "tokens": ["Die", "Blu\u00b7men", "wol\u00b7len", "sich", "nicht", "freund\u00b7lich", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Sterne g\u00f6nnen mir nicht mehr den Frieden,", "tokens": ["Die", "Ster\u00b7ne", "g\u00f6n\u00b7nen", "mir", "nicht", "mehr", "den", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "PPER", "PTKNEG", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Natur, die heil'ge, zieht sich weit zur\u00fccke,", "tokens": ["Na\u00b7tur", ",", "die", "heil'\u00b7ge", ",", "zieht", "sich", "weit", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "$,", "VVFIN", "PRF", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ich flehe wohl, sie sieht nicht meine Blicke.", "tokens": ["Ich", "fle\u00b7he", "wohl", ",", "sie", "sieht", "nicht", "mei\u00b7ne", "Bli\u00b7cke", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Das Unsichtbare, das ich in mir hegte,", "tokens": ["Das", "Un\u00b7sicht\u00b7ba\u00b7re", ",", "das", "ich", "in", "mir", "heg\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die alte Zeit, die Liebe zu dem Hohen,", "tokens": ["Die", "al\u00b7te", "Zeit", ",", "die", "Lie\u00b7be", "zu", "dem", "Ho\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Glaub' an Kunst, den ich so innig pflegte,", "tokens": ["Der", "Glaub'", "an", "Kunst", ",", "den", "ich", "so", "in\u00b7nig", "pfleg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word"