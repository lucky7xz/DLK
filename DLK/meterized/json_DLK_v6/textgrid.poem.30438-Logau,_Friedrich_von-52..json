, ",", "V\u00f6l\u00b7le", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADJD", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und was sonst erwarb sein Wille:", "tokens": ["Und", "was", "sonst", "er\u00b7warb", "sein", "Wil\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Diese, seine sch\u00f6ne Habe,", "tokens": ["Die\u00b7se", ",", "sei\u00b7ne", "sch\u00f6\u00b7ne", "Ha\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Nennet Veit de\u00df Herren Gabe,", "tokens": ["Nen\u00b7net", "Veit", "de\u00df", "Her\u00b7ren", "Ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Wil von solchem Gott bescheren", "tokens": ["Wil", "von", "sol\u00b7chem", "Gott", "be\u00b7sche\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "PIAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Sich mit Gott und Ehren n\u00e4hren.", "tokens": ["Sich", "mit", "Gott", "und", "Eh\u00b7ren", "n\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}