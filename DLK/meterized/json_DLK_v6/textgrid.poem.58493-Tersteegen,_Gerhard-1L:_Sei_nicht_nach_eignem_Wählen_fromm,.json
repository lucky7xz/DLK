{"textgrid.poem.58493": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sei nicht nach eignem W\u00e4hlen fromm,", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sei nicht nach eignem W\u00e4hlen fromm,", "tokens": ["Sei", "nicht", "nach", "eig\u00b7nem", "W\u00e4h\u00b7len", "fromm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "APPR", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lern doch, an Gott zum Eigentum", "tokens": ["Lern", "doch", ",", "an", "Gott", "zum", "Ei\u00b7gen\u00b7tum"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "APPR", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Herz und Willen wahrlich geben", "tokens": ["Dein", "Herz", "und", "Wil\u00b7len", "wahr\u00b7lich", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und seinem Wink gelassen leben!", "tokens": ["Und", "sei\u00b7nem", "Wink", "ge\u00b7las\u00b7sen", "le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}