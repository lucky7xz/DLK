{"textgrid.poem.33430": {"metadata": {"author": {"name": "Novalis", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fern in Osten wird es helle,", "tokens": ["Fern", "in", "Os\u00b7ten", "wird", "es", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VAFIN", "PPER", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Graue Zeiten werden jung;", "tokens": ["Grau\u00b7e", "Zei\u00b7ten", "wer\u00b7den", "jung", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus der lichten Farbenquelle", "tokens": ["Aus", "der", "lich\u00b7ten", "Far\u00b7ben\u00b7quel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einen langen tiefen Trunk!", "tokens": ["Ei\u00b7nen", "lan\u00b7gen", "tie\u00b7fen", "Trunk", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alter Sehnsucht heilige Gew\u00e4hrung,", "tokens": ["Al\u00b7ter", "Sehn\u00b7sucht", "hei\u00b7li\u00b7ge", "Ge\u00b7w\u00e4h\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "S\u00fc\u00dfe Lieb' in g\u00f6ttlicher Verkl\u00e4rung.", "tokens": ["S\u00fc\u00b7\u00dfe", "Lieb'", "in", "g\u00f6tt\u00b7li\u00b7cher", "Ver\u00b7kl\u00e4\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Endlich kommt zur Erde nieder", "tokens": ["End\u00b7lich", "kommt", "zur", "Er\u00b7de", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aller Himmel sel'ges Kind,", "tokens": ["Al\u00b7ler", "Him\u00b7mel", "sel'\u00b7ges", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schaffend im Gesang weht wieder", "tokens": ["Schaf\u00b7fend", "im", "Ge\u00b7sang", "weht", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "VVFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Um die Erde Lebenswind,", "tokens": ["Um", "die", "Er\u00b7de", "Le\u00b7bens\u00b7wind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weht zu neuen ewig lichten Flammen", "tokens": ["Weht", "zu", "neu\u00b7en", "e\u00b7wig", "lich\u00b7ten", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "L\u00e4ngst verstiebte Funken hier zusammen.", "tokens": ["L\u00e4ngst", "ver\u00b7stieb\u00b7te", "Fun\u00b7ken", "hier", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Ueberall entspringt aus Gr\u00fcften", "tokens": ["Ue\u00b7be\u00b7rall", "ent\u00b7springt", "aus", "Gr\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Neues Leben, neues Blut,", "tokens": ["Neu\u00b7es", "Le\u00b7ben", ",", "neu\u00b7es", "Blut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ew'gen Frieden uns zu stiften,", "tokens": ["Ew'\u00b7gen", "Frie\u00b7den", "uns", "zu", "stif\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Taucht er in die Lebensfluth;", "tokens": ["Taucht", "er", "in", "die", "Le\u00b7bens\u00b7fluth", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Steht mit vollen H\u00e4nden in der Mitte", "tokens": ["Steht", "mit", "vol\u00b7len", "H\u00e4n\u00b7den", "in", "der", "Mit\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Liebevoll gew\u00e4rtig jeder Bitte.", "tokens": ["Lie\u00b7be\u00b7voll", "ge\u00b7w\u00e4r\u00b7tig", "je\u00b7der", "Bit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Lasse seine milden Blicke", "tokens": ["Las\u00b7se", "sei\u00b7ne", "mil\u00b7den", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tief in deine Seele gehn,", "tokens": ["Tief", "in", "dei\u00b7ne", "See\u00b7le", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und von seinem ewgen Gl\u00fccke", "tokens": ["Und", "von", "sei\u00b7nem", "ew\u00b7gen", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sollst du dich ergriffen sehn.", "tokens": ["Sollst", "du", "dich", "er\u00b7grif\u00b7fen", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "VVINF", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alle Herzen, Geister und die Sinnen", "tokens": ["Al\u00b7le", "Her\u00b7zen", ",", "Geis\u00b7ter", "und", "die", "Sin\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Werden einen neuen Tanz beginnen.", "tokens": ["Wer\u00b7den", "ei\u00b7nen", "neu\u00b7en", "Tanz", "be\u00b7gin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Greife dreist nach seinen H\u00e4nden,", "tokens": ["Grei\u00b7fe", "dreist", "nach", "sei\u00b7nen", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Pr\u00e4ge dir sein Antlitz ein,", "tokens": ["Pr\u00e4\u00b7ge", "dir", "sein", "Ant\u00b7litz", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mu\u00dft dich immer nach ihm wenden,", "tokens": ["Mu\u00dft", "dich", "im\u00b7mer", "nach", "ihm", "wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bl\u00fcthe nach dem Sonnenschein;", "tokens": ["Bl\u00fc\u00b7the", "nach", "dem", "Son\u00b7nen\u00b7schein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wirst du nur das ganze Herz ihm zeigen,", "tokens": ["Wirst", "du", "nur", "das", "gan\u00b7ze", "Herz", "ihm", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Bleibt er wie ein treues Weib dir eigen.", "tokens": ["Bleibt", "er", "wie", "ein", "treu\u00b7es", "Weib", "dir", "ei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "ART", "ADJA", "NN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Unser ist sie nun geworden,", "tokens": ["Un\u00b7ser", "ist", "sie", "nun", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gottheit, die uns oft erschreckt,", "tokens": ["Got\u00b7theit", ",", "die", "uns", "oft", "er\u00b7schreckt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat im S\u00fcden und im Norden", "tokens": ["Hat", "im", "S\u00fc\u00b7den", "und", "im", "Nor\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPRART", "NN", "KON", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Himmelskeime rasch geweckt,", "tokens": ["Him\u00b7mel\u00b7skei\u00b7me", "rasch", "ge\u00b7weckt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und so la\u00dft im vollen Gottesgarten", "tokens": ["Und", "so", "la\u00dft", "im", "vol\u00b7len", "Got\u00b7tes\u00b7gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Treu uns jede Knosp' und Bl\u00fcthe warten.", "tokens": ["Treu", "uns", "je\u00b7de", "Knosp'", "und", "Bl\u00fc\u00b7the", "war\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PIAT", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}