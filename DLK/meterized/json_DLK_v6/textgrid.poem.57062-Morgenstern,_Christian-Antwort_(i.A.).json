{"textgrid.poem.57062": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Antwort (i.A.)", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbsehr geehrter Herr! Gestatten", "tokens": ["\u00bb", "sehr", "ge\u00b7ehr\u00b7ter", "Herr", "!", "Ge\u00b7stat\u00b7ten"], "token_info": ["punct", "word", "word", "word", "punct", "word"], "pos": ["$(", "ADV", "ADJA", "NN", "$.", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie der Gattin meines Gatten", "tokens": ["Sie", "der", "Gat\u00b7tin", "mei\u00b7nes", "Gat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "seine Antwort mitzuteilen.", "tokens": ["sei\u00b7ne", "Ant\u00b7wort", "mit\u00b7zu\u00b7tei\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Er begl\u00fcckw\u00fcnscht sich zu solchen", "tokens": ["Er", "be\u00b7gl\u00fcck\u00b7w\u00fcnscht", "sich", "zu", "sol\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PIAT"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "\u00c4u\u00dferungen, die gleich Dolchen", "tokens": ["\u00c4u\u00b7\u00dfe\u00b7run\u00b7gen", ",", "die", "gleich", "Dol\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "ADV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "seiner Werke Brust durchw\u00fchlen.", "tokens": ["sei\u00b7ner", "Wer\u00b7ke", "Brust", "durch\u00b7w\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch er ist zurzeit verhindert.", "tokens": ["Doch", "er", "ist", "zur\u00b7zeit", "ver\u00b7hin\u00b7dert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "N\u00e4mlich (was den Vorwurf mindert)", "tokens": ["N\u00e4m\u00b7lich", "(", "was", "den", "Vor\u00b7wurf", "min\u00b7dert", ")"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "PWS", "ART", "NN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "durch Verfolgung jenes Falles \u2013", "tokens": ["durch", "Ver\u00b7fol\u00b7gung", "je\u00b7nes", "Fal\u00b7les", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PDAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "statt nach r\u00fcckw\u00e4rts, wie Sie streben,", "tokens": ["statt", "nach", "r\u00fcck\u00b7w\u00e4rts", ",", "wie", "Sie", "stre\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "vorw\u00e4rts: in das neue Leben", "tokens": ["vor\u00b7w\u00e4rts", ":", "in", "das", "neu\u00b7e", "Le\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$.", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "unsrer trefflichen Schalalster!", "tokens": ["uns\u00b7rer", "treff\u00b7li\u00b7chen", "Scha\u00b7lals\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--++-", "measure": "trochaic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "(ach, mein Herr, ich w\u00fcnsch es keinem.)", "tokens": ["(", "ach", ",", "mein", "Herr", ",", "ich", "w\u00fcnsch", "es", "kei\u00b7nem", ".", ")"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ITJ", "$,", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER", "PIS", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Folgender \u203aEntwurf zu einem", "tokens": ["Fol\u00b7gen\u00b7der", "\u203a", "Ent\u00b7wurf", "zu", "ei\u00b7nem"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$(", "NN", "APPR", "ART"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "b\u00fcrgerlichen Trauerspiele\u2039", "tokens": ["b\u00fcr\u00b7ger\u00b7li\u00b7chen", "Trau\u00b7er\u00b7spie\u00b7le", "\u2039"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "gibt dem Ganzen eine Wende,", "tokens": ["gibt", "dem", "Gan\u00b7zen", "ei\u00b7ne", "Wen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "die uns, wie Sie (und wohl viele)", "tokens": ["die", "uns", ",", "wie", "Sie", "(", "und", "wohl", "vie\u00b7le", ")"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "PWAV", "PPER", "$(", "KON", "ADV", "PIAT", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "nicht ganz ungleichm\u00fctig f\u00fchlen", "tokens": ["nicht", "ganz", "un\u00b7gleich\u00b7m\u00fc\u00b7tig", "f\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "werden, lehrt, wie doch noch alles", "tokens": ["wer\u00b7den", ",", "lehrt", ",", "wie", "doch", "noch", "al\u00b7les"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VAINF", "$,", "VVFIN", "$,", "PWAV", "ADV", "ADV", "PIS"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "recht in Blindheit lebt. Derweilen,", "tokens": ["recht", "in", "Blind\u00b7heit", "lebt", ".", "Der\u00b7wei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "$.", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "und mit Dank und Gr\u00fc\u00dfen (falls der", "tokens": ["und", "mit", "Dank", "und", "Gr\u00fc\u00b7\u00dfen", "(", "falls", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "NN", "$(", "KOUS", "ART"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Anteil an der Fisch-Allmende", "tokens": ["An\u00b7teil", "an", "der", "FischAll\u00b7men\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.2": {"text": "wirklich echt in Ihren Zeilen!)", "tokens": ["wirk\u00b7lich", "echt", "in", "Ih\u00b7ren", "Zei\u00b7len", "!", ")"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "ADJD", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihre X. \u2013 Ich bin zu Ende.\u00ab", "tokens": ["Ih\u00b7re", "X.", "\u2013", "Ich", "bin", "zu", "En\u00b7de", ".", "\u00ab"], "token_info": ["word", "abbreviation", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NE", "$(", "PPER", "VAFIN", "APPR", "NN", "$.", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}}}}