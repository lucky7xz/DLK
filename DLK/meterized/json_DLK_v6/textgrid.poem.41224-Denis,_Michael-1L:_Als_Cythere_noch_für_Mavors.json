{"textgrid.poem.41224": {"metadata": {"author": {"name": "Denis, Michael", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als Cythere noch f\u00fcr Mavors", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als Cythere noch f\u00fcr Mavors", "tokens": ["Als", "Cy\u00b7the\u00b7re", "noch", "f\u00fcr", "Ma\u00b7vors"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "ADV", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Keine Seitentriebe f\u00fchlte,", "tokens": ["Kei\u00b7ne", "Sei\u00b7ten\u00b7trie\u00b7be", "f\u00fchl\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Als noch Mavors auf Cytheren", "tokens": ["Als", "noch", "Ma\u00b7vors", "auf", "Cy\u00b7the\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Keinen Augenausfall wagte,", "tokens": ["Kei\u00b7nen", "Au\u00b7gen\u00b7aus\u00b7fall", "wag\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hatte Venus ihren Dichter,", "tokens": ["Hat\u00b7te", "Ve\u00b7nus", "ih\u00b7ren", "Dich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hatte seinen Dichter Mavors;", "tokens": ["Hat\u00b7te", "sei\u00b7nen", "Dich\u00b7ter", "Ma\u00b7vors", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NE", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Sie den muntern Tejergreisen,", "tokens": ["Sie", "den", "mun\u00b7tern", "Te\u00b7jer\u00b7grei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Er den Mutherwecker Spartens;", "tokens": ["Er", "den", "Mu\u00b7ther\u00b7we\u00b7cker", "Spar\u00b7tens", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Aber als der lahme Gatte", "tokens": ["A\u00b7ber", "als", "der", "lah\u00b7me", "Gat\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Seine Netze fein zu ketten,", "tokens": ["Sei\u00b7ne", "Net\u00b7ze", "fein", "zu", "ket\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Und die schl\u00fcpfrige Cythere", "tokens": ["Und", "die", "schl\u00fcpf\u00b7ri\u00b7ge", "Cy\u00b7the\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Mit dem schl\u00fcpfrigen Gradivus", "tokens": ["Mit", "dem", "schl\u00fcpf\u00b7ri\u00b7gen", "Gra\u00b7di\u00b7vus"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.13": {"text": "Gleich den Aalen zu ber\u00fccken", "tokens": ["Gleich", "den", "Aa\u00b7len", "zu", "be\u00b7r\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Sich des Argus Augen w\u00fcnschte,", "tokens": ["Sich", "des", "Ar\u00b7gus", "Au\u00b7gen", "w\u00fcnschte", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Damal in der sch\u00f6nsten Stunde", "tokens": ["Da\u00b7mal", "in", "der", "sch\u00f6ns\u00b7ten", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.16": {"text": "Sprachen Mavors und Cythere:", "tokens": ["Spra\u00b7chen", "Ma\u00b7vors", "und", "Cy\u00b7the\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "KON", "NN", "$."], "meter": "+--+-+--", "measure": "iambic.tri.invert"}, "line.17": {"text": "\u00bbeinen Wunsch und Ein Vergn\u00fcgen", "tokens": ["\u00bb", "ei\u00b7nen", "Wunsch", "und", "Ein", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Haben wir, Ein Herz und Lager,", "tokens": ["Ha\u00b7ben", "wir", ",", "Ein", "Herz", "und", "La\u00b7ger", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "ART", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Sollten wir nicht alle Beyde", "tokens": ["Soll\u00b7ten", "wir", "nicht", "al\u00b7le", "Bey\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PTKNEG", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Auch nur Einen Dichter haben?\u00ab", "tokens": ["Auch", "nur", "Ei\u00b7nen", "Dich\u00b7ter", "ha\u00b7ben", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VAFIN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Und die losen Amoretten,", "tokens": ["Und", "die", "lo\u00b7sen", "A\u00b7mo\u00b7ret\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Die der Venus Wangen k\u00fchlten,", "tokens": ["Die", "der", "Ve\u00b7nus", "Wan\u00b7gen", "k\u00fchl\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Die mit Mavors Waffen spielten,", "tokens": ["Die", "mit", "Ma\u00b7vors", "Waf\u00b7fen", "spiel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Schlugen ihre Seidenfl\u00fcgel,", "tokens": ["Schlu\u00b7gen", "ih\u00b7re", "Sei\u00b7den\u00b7fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Wiederholten: \u00bbEinen Dichter!", "tokens": ["Wie\u00b7der\u00b7hol\u00b7ten", ":", "\u00bb", "Ei\u00b7nen", "Dich\u00b7ter", "!"], "token_info": ["word", "punct", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Einer flog, und holte Rosen", "tokens": ["Ei\u00b7ner", "flog", ",", "und", "hol\u00b7te", "Ro\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "KON", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Von dem besten Grunde Paphos,", "tokens": ["Von", "dem", "bes\u00b7ten", "Grun\u00b7de", "Pa\u00b7phos", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NE", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.28": {"text": "Diesen Dichter zu bekr\u00e4nzen.", "tokens": ["Die\u00b7sen", "Dich\u00b7ter", "zu", "be\u00b7kr\u00e4n\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Einer flog auf Pindus H\u00f6hen,", "tokens": ["Ei\u00b7ner", "flog", "auf", "Pin\u00b7dus", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Von Kalliopen die Tuba", "tokens": ["Von", "Kal\u00b7li\u00b7o\u00b7pen", "die", "Tu\u00b7ba"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.31": {"text": "Diesem Dichter zu erbitten.", "tokens": ["Die\u00b7sem", "Dich\u00b7ter", "zu", "er\u00b7bit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Und ich horchte still, und wollte", "tokens": ["Und", "ich", "horch\u00b7te", "still", ",", "und", "woll\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ", "$,", "KON", "VMFIN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.33": {"text": "Dieses Dichters der Cythere,", "tokens": ["Die\u00b7ses", "Dich\u00b7ters", "der", "Cy\u00b7the\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Dieses Dichters des Gradivus", "tokens": ["Die\u00b7ses", "Dich\u00b7ters", "des", "Gra\u00b7di\u00b7vus"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "NN", "ART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.35": {"text": "Ehrenvollen Namen h\u00f6ren.", "tokens": ["Eh\u00b7ren\u00b7vol\u00b7len", "Na\u00b7men", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Und das Chor der Amoretten,", "tokens": ["Und", "das", "Chor", "der", "A\u00b7mo\u00b7ret\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Die der Venus Wangen k\u00fchlten,", "tokens": ["Die", "der", "Ve\u00b7nus", "Wan\u00b7gen", "k\u00fchl\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Die mit Mavors Waffen spielten,", "tokens": ["Die", "mit", "Ma\u00b7vors", "Waf\u00b7fen", "spiel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Schlugen ihre Seidenfl\u00fcgel,", "tokens": ["Schlu\u00b7gen", "ih\u00b7re", "Sei\u00b7den\u00b7fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Klatschten mit der Hand, und riefen:", "tokens": ["Klatschten", "mit", "der", "Hand", ",", "und", "rie\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "KON", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.41": {"text": "Singe von Cytherens Siegen,", "tokens": ["Sin\u00b7ge", "von", "Cy\u00b7the\u00b7rens", "Sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.42": {"text": "Singe von Gradivus Siegen,", "tokens": ["Sin\u00b7ge", "von", "Gra\u00b7di\u00b7vus", "Sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.43": {"text": "Gleim \u2013 Anakreon \u2013 Tyrt\u00e4us!\u00ab", "tokens": ["Gleim", "\u2013", "A\u00b7nak\u00b7re\u00b7on", "\u2013", "Tyr\u00b7t\u00e4us", "!", "\u00ab"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["ADV", "$(", "NE", "$(", "NE", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}