{"textgrid.poem.62017": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[v\u00f6glein auf den gr\u00fcnen Zweigen]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "V\u00f6glein auf den gr\u00fcnen Zweigen,", "tokens": ["V\u00f6\u00b7glein", "auf", "den", "gr\u00fc\u00b7nen", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die sich auf- und abw\u00e4rts neigen,", "tokens": ["Die", "sich", "auf", "und", "ab\u00b7w\u00e4rts", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "TRUNC", "KON", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Freude hebet eure Brust,", "tokens": ["Freu\u00b7de", "he\u00b7bet", "eu\u00b7re", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Klopfen h\u00f6r' ich sie vor Lust!", "tokens": ["Klop\u00b7fen", "h\u00f6r'", "ich", "sie", "vor", "Lust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Fr\u00fchlingswonne, Schwalben, Lerchen,", "tokens": ["Fr\u00fch\u00b7lings\u00b7won\u00b7ne", ",", "Schwal\u00b7ben", ",", "Ler\u00b7chen", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Laut Geklapper unter St\u00f6rchen,", "tokens": ["Laut", "Ge\u00b7klap\u00b7per", "un\u00b7ter", "St\u00f6r\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wiedersehen, Reiselust,", "tokens": ["Wie\u00b7der\u00b7se\u00b7hen", ",", "Rei\u00b7se\u00b7lust", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hohe Freude in der Brust.", "tokens": ["Ho\u00b7he", "Freu\u00b7de", "in", "der", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "G\u00f6nnet mir Die Freudenfeier,", "tokens": ["G\u00f6n\u00b7net", "mir", "Die", "Freu\u00b7den\u00b7fei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine Seele atmet freier,", "tokens": ["Mei\u00b7ne", "See\u00b7le", "at\u00b7met", "frei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Herr im Himmel, habe Dank", "tokens": ["Herr", "im", "Him\u00b7mel", ",", "ha\u00b7be", "Dank"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPRART", "NN", "$,", "VAFIN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcr den innern Festgesang!", "tokens": ["F\u00fcr", "den", "in\u00b7nern", "Fest\u00b7ge\u00b7sang", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}