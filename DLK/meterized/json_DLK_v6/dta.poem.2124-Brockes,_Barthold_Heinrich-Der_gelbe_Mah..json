{"dta.poem.2124": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der gelbe Mah.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie ich j\u00fcngst deinen Glanz und Schimmer, gelber", "tokens": ["Wie", "ich", "j\u00fcngst", "dei\u00b7nen", "Glanz", "und", "Schim\u00b7mer", ",", "gel\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "PPER", "ADV", "PPOSAT", "NN", "KON", "NN", "$,", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mah,", "tokens": ["Mah", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Der in Siberien entsprossen, wieder sah;", "tokens": ["Der", "in", "Si\u00b7be\u00b7ri\u00b7en", "ent\u00b7spros\u00b7sen", ",", "wie\u00b7der", "sah", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "VVPP", "$,", "ADV", "VVFIN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Ergetzt ich mich aufs neu an deinem Schmuck und Schein,", "tokens": ["Er\u00b7getzt", "ich", "mich", "aufs", "neu", "an", "dei\u00b7nem", "Schmuck", "und", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPRART", "ADJD", "APPR", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und fiel dabey mir die Betrachtung ein:", "tokens": ["Und", "fiel", "da\u00b7bey", "mir", "die", "Be\u00b7trach\u00b7tung", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da du, in so entfernten Landen,", "tokens": ["Da", "du", ",", "in", "so", "ent\u00b7fern\u00b7ten", "Lan\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.7": {"text": "Und abgelegnem Himmelsstrich", "tokens": ["Und", "ab\u00b7ge\u00b7leg\u00b7nem", "Him\u00b7mels\u00b7strich"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Erzeugt, gewachsen und entstanden,", "tokens": ["Er\u00b7zeugt", ",", "ge\u00b7wach\u00b7sen", "und", "ent\u00b7stan\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Beweisest du uns sichtbarlich,", "tokens": ["Be\u00b7wei\u00b7sest", "du", "uns", "sicht\u00b7bar\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Da\u00df, auch in so entlegnen Fluren,", "tokens": ["Da\u00df", ",", "auch", "in", "so", "ent\u00b7leg\u00b7nen", "Flu\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "ADV", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "So sch\u00f6n geschm\u00fcckte Kreaturen,", "tokens": ["So", "sch\u00f6n", "ge\u00b7schm\u00fcck\u00b7te", "Kre\u00b7a\u00b7tu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Wodurch wir Gott auch dort entdecken.", "tokens": ["Wo\u00b7durch", "wir", "Gott", "auch", "dort", "ent\u00b7de\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Uns zeiget deine Zier und Pracht,", "tokens": ["Uns", "zei\u00b7get", "dei\u00b7ne", "Zier", "und", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Wie seine Weisheit, Lieb und Macht", "tokens": ["Wie", "sei\u00b7ne", "Weis\u00b7heit", ",", "Lieb", "und", "Macht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Sich \u00fcber aller Erden Enden in gleicher Majest\u00e4t er-", "tokens": ["Sich", "\u00fc\u00b7ber", "al\u00b7ler", "Er\u00b7den", "En\u00b7den", "in", "glei\u00b7cher", "Ma\u00b7jes\u00b7t\u00e4t", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "PIAT", "NN", "NN", "APPR", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.16": {"text": "strecken.", "tokens": ["stre\u00b7cken", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}