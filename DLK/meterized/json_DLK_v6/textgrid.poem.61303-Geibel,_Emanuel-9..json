{"textgrid.poem.61303": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es t\u00fcrmt sich Not und Jammer unermessen", "tokens": ["Es", "t\u00fcrmt", "sich", "Not", "und", "Jam\u00b7mer", "un\u00b7er\u00b7mes\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "NN", "KON", "NN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom Eispol bis zum Nil in weiter Runde,", "tokens": ["Vom", "Eis\u00b7pol", "bis", "zum", "Nil", "in", "wei\u00b7ter", "Run\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "APPRART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zwist, Aufruhr, Seuchen wandeln hin im Bunde,", "tokens": ["Zwist", ",", "Auf\u00b7ruhr", ",", "Seu\u00b7chen", "wan\u00b7deln", "hin", "im", "Bun\u00b7de", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von Land zu Land der St\u00e4dte Mark zu fressen.", "tokens": ["Von", "Land", "zu", "Land", "der", "St\u00e4d\u00b7te", "Mark", "zu", "fres\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Reb' ist schwarz, will sie der Winzer pressen;", "tokens": ["Die", "Reb'", "ist", "schwarz", ",", "will", "sie", "der", "Win\u00b7zer", "pres\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "VMFIN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zermalmt vom Hagel liegt die Frucht am Grunde;", "tokens": ["Zer\u00b7malmt", "vom", "Ha\u00b7gel", "liegt", "die", "Frucht", "am", "Grun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Luft trieft Feuer, und mit gier'gem Schlunde", "tokens": ["Die", "Luft", "trieft", "Feu\u00b7er", ",", "und", "mit", "gier'\u00b7gem", "Schlun\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NN", "$,", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verschlingt die Woge, was die Glut vergessen.", "tokens": ["Ver\u00b7schlingt", "die", "Wo\u00b7ge", ",", "was", "die", "Glut", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So war es stets, wenn abendlich und bange", "tokens": ["So", "war", "es", "stets", ",", "wenn", "a\u00b7bend\u00b7lich", "und", "ban\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "KOUS", "ADJD", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die kalten Schatten auf den Erdkreis fielen", "tokens": ["Die", "kal\u00b7ten", "Schat\u00b7ten", "auf", "den", "Erd\u00b7kreis", "fie\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von einer Weltzeit Sonnenuntergange.", "tokens": ["Von", "ei\u00b7ner", "Welt\u00b7zeit", "Son\u00b7nen\u00b7un\u00b7ter\u00b7gan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch nicht an Zeichen, die aufs Ende zielen,", "tokens": ["Doch", "nicht", "an", "Zei\u00b7chen", ",", "die", "aufs", "En\u00b7de", "zie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Glaubt dies Geschlecht und schreit im irren Drange", "tokens": ["Glaubt", "dies", "Ge\u00b7schlecht", "und", "schreit", "im", "ir\u00b7ren", "Dran\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PDS", "NN", "KON", "ADJD", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Am offnen Grabe nur nach Brot und Spielen.", "tokens": ["Am", "off\u00b7nen", "Gra\u00b7be", "nur", "nach", "Brot", "und", "Spie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}