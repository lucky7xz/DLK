{"textgrid.poem.25514": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Morgen flieg' ich auf das Land,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Morgen flieg' ich auf das Land,", "tokens": ["Mor\u00b7gen", "flieg'", "ich", "auf", "das", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Komm, mein frommer Amarant,", "tokens": ["Komm", ",", "mein", "from\u00b7mer", "A\u00b7ma\u00b7rant", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df uns Hirten werden!", "tokens": ["La\u00df", "uns", "Hir\u00b7ten", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "VAINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Komm! vergi\u00df am Wasserfall'", "tokens": ["Komm", "!", "ver\u00b7gi\u00df", "am", "Was\u00b7ser\u00b7fall'"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$.", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ruhm und Akten, Spiel und Ball,", "tokens": ["Ruhm", "und", "Ak\u00b7ten", ",", "Spiel", "und", "Ball", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Diesen Tand der Erden.", "tokens": ["Die\u00b7sen", "Tand", "der", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Was den Hirten Rosen streut:", "tokens": ["Was", "den", "Hir\u00b7ten", "Ro\u00b7sen", "streut", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Unschuld und Zufriedenheit,", "tokens": ["Un\u00b7schuld", "und", "Zu\u00b7frie\u00b7den\u00b7heit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Haben wir ja Beide!", "tokens": ["Ha\u00b7ben", "wir", "ja", "Bei\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PIS", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Und den Hirten gleich zu seyn:", "tokens": ["Und", "den", "Hir\u00b7ten", "gleich", "zu", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Welcher K\u00f6nigskrone Schein", "tokens": ["Wel\u00b7cher", "K\u00f6\u00b7nigs\u00b7kro\u00b7ne", "Schein"], "token_info": ["word", "word", "word"], "pos": ["PWAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Strahlt so viele Freude?", "tokens": ["Strahlt", "so", "vie\u00b7le", "Freu\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.13": {"text": "O so la\u00df den kurzen May", "tokens": ["O", "so", "la\u00df", "den", "kur\u00b7zen", "May"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VVIMP", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Dieses Lebens uns getreu", "tokens": ["Die\u00b7ses", "Le\u00b7bens", "uns", "ge\u00b7treu"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "NN", "PPER", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Mit einander schmecken!", "tokens": ["Mit", "ein\u00b7an\u00b7der", "schme\u00b7cken", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PRF", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.16": {"text": "Wenn der Sommer uns erreicht,", "tokens": ["Wenn", "der", "Som\u00b7mer", "uns", "er\u00b7reicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Hinkt die Lust, im Winter schleicht", "tokens": ["Hinkt", "die", "Lust", ",", "im", "Win\u00b7ter", "schleicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Sie den Gang der Schnecken.", "tokens": ["Sie", "den", "Gang", "der", "Schne\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.19": {"text": "Und, o Mann! wie ungewi\u00df,", "tokens": ["Und", ",", "o", "Mann", "!", "wie", "un\u00b7ge\u00b7wi\u00df", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "FM", "NN", "$.", "PWAV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "Ob nicht Todesfinsterni\u00df", "tokens": ["Ob", "nicht", "To\u00b7des\u00b7fins\u00b7ter\u00b7ni\u00df"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PTKNEG", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Unser Aug' umziehet,", "tokens": ["Un\u00b7ser", "Aug'", "um\u00b7zie\u00b7het", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.22": {"text": "Eh' es von der ganzen Zahl", "tokens": ["Eh'", "es", "von", "der", "gan\u00b7zen", "Zahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Sommerfreuden, nur einmal", "tokens": ["Som\u00b7mer\u00b7freu\u00b7den", ",", "nur", "ein\u00b7mal"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Eine wirklich siehet?", "tokens": ["Ei\u00b7ne", "wirk\u00b7lich", "sie\u00b7het", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.25": {"text": "Komm denn, k\u00fc\u00df' als Hirtin mich!", "tokens": ["Komm", "denn", ",", "k\u00fc\u00df'", "als", "Hir\u00b7tin", "mich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "VVFIN", "KOUS", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.26": {"text": "Aber ach! ich bitte dich,", "tokens": ["A\u00b7ber", "ach", "!", "ich", "bit\u00b7te", "dich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "PPER", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Schone deiner Pferde;", "tokens": ["Scho\u00b7ne", "dei\u00b7ner", "Pfer\u00b7de", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.28": {"text": "Denn ich mag nicht, da\u00df ein Thier", "tokens": ["Denn", "ich", "mag", "nicht", ",", "da\u00df", "ein", "Thier"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "PTKNEG", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Blo\u00df aus Leidenschaft zu mir", "tokens": ["Blo\u00df", "aus", "Lei\u00b7den\u00b7schaft", "zu", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.30": {"text": "Abgemartert werde.", "tokens": ["Ab\u00b7ge\u00b7mar\u00b7tert", "wer\u00b7de", "."], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}