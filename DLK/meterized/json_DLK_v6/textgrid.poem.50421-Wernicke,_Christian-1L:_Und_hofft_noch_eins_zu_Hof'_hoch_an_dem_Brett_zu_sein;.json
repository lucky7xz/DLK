{"textgrid.poem.50421": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und hofft noch eins zu Hof' hoch an dem Brett zu sein;", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und hofft noch eins zu Hof' hoch an dem Brett zu sein;", "tokens": ["Und", "hofft", "noch", "eins", "zu", "Hof'", "hoch", "an", "dem", "Brett", "zu", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PIS", "APPR", "NN", "ADJD", "APPR", "ART", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn pfleget er gleich insgemein", "tokens": ["Denn", "pfle\u00b7get", "er", "gleich", "ins\u00b7ge\u00b7mein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Die unerkante Zeit ", "tokens": ["Die", "un\u00b7er\u00b7kan\u00b7te", "Zeit"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So nimmt man dennoch leichtlich ab,", "tokens": ["So", "nimmt", "man", "den\u00b7noch", "leicht\u00b7lich", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dass eben ", "tokens": ["Dass", "e\u00b7ben"], "token_info": ["word", "word"], "pos": ["KOUS", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Er weiss woll, ", "tokens": ["Er", "weiss", "woll", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,"], "meter": "-+-", "measure": "amphibrach.single"}}}}}