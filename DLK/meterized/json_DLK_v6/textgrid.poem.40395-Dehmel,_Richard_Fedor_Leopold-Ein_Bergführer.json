{"textgrid.poem.40395": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Ein Bergf\u00fchrer", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kann ein Bergsteiger werden,", "tokens": ["Ich", "kann", "ein", "Berg\u00b7stei\u00b7ger", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VAINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "der die andern alle f\u00fchrt.", "tokens": ["der", "die", "an\u00b7dern", "al\u00b7le", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "PIS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Pfade, wo nie ein Schritt erklang:", "tokens": ["Pfa\u00b7de", ",", "wo", "nie", "ein", "Schritt", "er\u00b7klang", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "wer hat sie aufgesp\u00fcrt?", "tokens": ["wer", "hat", "sie", "auf\u00b7ge\u00b7sp\u00fcrt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das tat meine Herzenslust!", "tokens": ["Das", "tat", "mei\u00b7ne", "Her\u00b7zens\u00b7lust", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Die treibt mich hin zu den Gipfeln,", "tokens": ["Die", "treibt", "mich", "hin", "zu", "den", "Gip\u00b7feln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "\u00fcber Schnee, durch Wetterschlag,", "tokens": ["\u00fc\u00b7ber", "Schnee", ",", "durch", "Wet\u00b7ter\u00b7schlag", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "am Sturzbach hin, am Gletscherrand,", "tokens": ["am", "Sturz\u00b7bach", "hin", ",", "am", "Glet\u00b7scher\u00b7rand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "hinauf! Nun klettert nach,", "tokens": ["hin\u00b7auf", "!", "Nun", "klet\u00b7tert", "nach", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$.", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "ihr andern Wageh\u00e4lse!", "tokens": ["ihr", "an\u00b7dern", "Wa\u00b7ge\u00b7h\u00e4l\u00b7se", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mir nach mit gl\u00fchendem Herzen,", "tokens": ["Mir", "nach", "mit", "gl\u00fc\u00b7hen\u00b7dem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "hinauf ins freie Eis!", "tokens": ["hin\u00b7auf", "ins", "frei\u00b7e", "Eis", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer st\u00fcrzt, den schm\u00fcckt im Paradies", "tokens": ["Wer", "st\u00fcrzt", ",", "den", "schm\u00fcckt", "im", "Pa\u00b7ra\u00b7dies"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$,", "ART", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "die Blume Edelwei\u00df!", "tokens": ["die", "Blu\u00b7me", "E\u00b7del\u00b7wei\u00df", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Kommt! jauchzend gr\u00fc\u00df ich euch.", "tokens": ["Kommt", "!", "jauch\u00b7zend", "gr\u00fc\u00df", "ich", "euch", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "VVPP", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Aber am liebsten steh ich", "tokens": ["A\u00b7ber", "am", "liebs\u00b7ten", "steh", "ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PTKA", "ADJD", "VVFIN", "PPER"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "hoch oben ganz allein,", "tokens": ["hoch", "o\u00b7ben", "ganz", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "mitten im stillen Himmelskreis,", "tokens": ["mit\u00b7ten", "im", "stil\u00b7len", "Him\u00b7mels\u00b7kreis", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "und h\u00f6re die Adler schrein:", "tokens": ["und", "h\u00f6\u00b7re", "die", "Ad\u00b7ler", "schrein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "gr\u00fc\u00df Gott, du kleiner Held!", "tokens": ["gr\u00fc\u00df", "Gott", ",", "du", "klei\u00b7ner", "Held", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}