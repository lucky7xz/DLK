{"dta.poem.20019": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Mayenklage .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ziehe nicht so spr\u00f6d' und schnelle,", "tokens": ["Zie\u00b7he", "nicht", "so", "spr\u00f6d'", "und", "schnel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "S\u00fc\u00dfer May, an mir vor\u00fcber!", "tokens": ["S\u00fc\u00b7\u00dfer", "May", ",", "an", "mir", "vor\u00b7\u00fc\u00b7ber", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Einen Strahl nur deiner Helle!", "tokens": ["Ei\u00b7nen", "Strahl", "nur", "dei\u00b7ner", "Hel\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur ein einzig Bl\u00fcmlein, Lieber!", "tokens": ["Nur", "ein", "ein\u00b7zig", "Bl\u00fcm\u00b7lein", ",", "Lie\u00b7ber", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ART", "ADJD", "NN", "$,", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Quellen rauschen, V\u00f6gel singen,", "tokens": ["Quel\u00b7len", "rau\u00b7schen", ",", "V\u00f6\u00b7gel", "sin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Volle Bl\u00fcthenb\u00e4ume wehen,", "tokens": ["Vol\u00b7le", "Bl\u00fc\u00b7then\u00b7b\u00e4u\u00b7me", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch an all' den s\u00fc\u00dfen Dingen", "tokens": ["Doch", "an", "all'", "den", "s\u00fc\u00b7\u00dfen", "Din\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIS", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mu\u00df ich kalt vor\u00fcber gehen.", "tokens": ["Mu\u00df", "ich", "kalt", "vor\u00b7\u00fc\u00b7ber", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Waldesnacht, wo V\u00f6gel schliefen,", "tokens": ["Wal\u00b7des\u00b7nacht", ",", "wo", "V\u00f6\u00b7gel", "schlie\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist erhellt von Blumen, Quellen,", "tokens": ["Ist", "er\u00b7hellt", "von", "Blu\u00b7men", ",", "Quel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPR", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach! des Busens bange Tiefen", "tokens": ["Ach", "!", "des", "Bu\u00b7sens", "ban\u00b7ge", "Tie\u00b7fen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "ART", "NN", "ADV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kann kein Mayenstrahl erhellen!", "tokens": ["Kann", "kein", "Ma\u00b7yen\u00b7strahl", "er\u00b7hel\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "La\u00df die Stern' an Himmelszinnen,", "tokens": ["La\u00df", "die", "Stern'", "an", "Him\u00b7mels\u00b7zin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bl\u00fcthen auf der Erde gl\u00e4nzen, \u2014", "tokens": ["Bl\u00fc\u00b7then", "auf", "der", "Er\u00b7de", "gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Todtes Herz! im H\u00fcgel innen", "tokens": ["Tod\u00b7tes", "Herz", "!", "im", "H\u00fc\u00b7gel", "in\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "APPRART", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Liegst du unter welken Kr\u00e4nzen!", "tokens": ["Liegst", "du", "un\u00b7ter", "wel\u00b7ken", "Kr\u00e4n\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PWAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}