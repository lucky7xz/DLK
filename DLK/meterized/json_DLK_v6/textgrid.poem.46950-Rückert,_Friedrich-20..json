{"textgrid.poem.46950": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "20.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O welche M\u00e4nner steigen im Vereine,", "tokens": ["O", "wel\u00b7che", "M\u00e4n\u00b7ner", "stei\u00b7gen", "im", "Ver\u00b7ei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAT", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie Heldengeister aus der Gr\u00fcfte Wasen,", "tokens": ["Wie", "Hel\u00b7den\u00b7geis\u00b7ter", "aus", "der", "Gr\u00fcf\u00b7te", "Wa\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mein Vaterland, empor aus dir und rasen", "tokens": ["Mein", "Va\u00b7ter\u00b7land", ",", "em\u00b7por", "aus", "dir", "und", "ra\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PTKVZ", "APPR", "PPER", "KON", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Sturm um ihre Freiheit und um deine.", "tokens": ["Im", "Sturm", "um", "ih\u00b7re", "Frei\u00b7heit", "und", "um", "dei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPOSAT", "NN", "KON", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Zwei aber find's vor allen, die ich meine,", "tokens": ["Zwei", "a\u00b7ber", "fin\u00b7d's", "vor", "al\u00b7len", ",", "die", "ich", "mei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "ADV", "VVFIN", "APPR", "PIAT", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Zwei sind's, von denen in des Volkes Nasen", "tokens": ["Zwei", "sin\u00b7d's", ",", "von", "de\u00b7nen", "in", "des", "Vol\u00b7kes", "Na\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["CARD", "VAFIN", "$,", "APPR", "PRELS", "APPR", "ART", "NN", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Zumeist solch edler Sturm ward angeblasen,", "tokens": ["Zu\u00b7meist", "solch", "ed\u00b7ler", "Sturm", "ward", "an\u00b7ge\u00b7bla\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Von einem ", "tokens": ["Von", "ei\u00b7nem"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Auf diesem Felsstein, diesem Harten Berge,", "tokens": ["Auf", "die\u00b7sem", "Felss\u00b7tein", ",", "die\u00b7sem", "Har\u00b7ten", "Ber\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Soll sein das neue Vaterland gegr\u00fcndet,", "tokens": ["Soll", "sein", "das", "neu\u00b7e", "Va\u00b7ter\u00b7land", "ge\u00b7gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Drauf gro\u00df gleich Riesen sollen stehn selbst Zwerge.", "tokens": ["Drauf", "gro\u00df", "gleich", "Rie\u00b7sen", "sol\u00b7len", "stehn", "selbst", "Zwer\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJD", "ADV", "NN", "VMFIN", "VVINF", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Und hoch dar\u00fcber soll von Lust entz\u00fcndet", "tokens": ["Und", "hoch", "da\u00b7r\u00fc\u00b7ber", "soll", "von", "Lust", "ent\u00b7z\u00fcn\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "PAV", "VMFIN", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Aufschweben mein Gesang als Himmelslerche,", "tokens": ["Auf\u00b7schwe\u00b7ben", "mein", "Ge\u00b7sang", "als", "Him\u00b7mels\u00b7ler\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Die \u00fcberm Berg den roten Morgen k\u00fcndet.", "tokens": ["Die", "\u00fc\u00b7berm", "Berg", "den", "ro\u00b7ten", "Mor\u00b7gen", "k\u00fcn\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}