{"textgrid.poem.47810": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Weihnachtabend", "genre": "verse", "period": "N.A.", "pub_year": 1852, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die fremde Stadt durchschritt ich sorgenvoll,", "tokens": ["Die", "frem\u00b7de", "Stadt", "durch\u00b7schritt", "ich", "sor\u00b7gen\u00b7voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Kinder denkend, die ich lie\u00df zu Haus.", "tokens": ["Der", "Kin\u00b7der", "den\u00b7kend", ",", "die", "ich", "lie\u00df", "zu", "Haus", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PRELS", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Weihnachten war's; durch alle Gassen scholl", "tokens": ["Weih\u00b7nach\u00b7ten", "wa\u00b7r's", ";", "durch", "al\u00b7le", "Gas\u00b7sen", "scholl"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "$.", "APPR", "PIAT", "NN", "ADJD"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Der Kinderjubel und des Markts Gebraus.", "tokens": ["Der", "Kin\u00b7der\u00b7ju\u00b7bel", "und", "des", "Markts", "Ge\u00b7braus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und wie der Menschenstrom mich fortgesp\u00fclt,", "tokens": ["Und", "wie", "der", "Men\u00b7schen\u00b7strom", "mich", "fort\u00b7ge\u00b7sp\u00fclt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Drang mir ein heiser Stimmlein in das Ohr:", "tokens": ["Drang", "mir", "ein", "hei\u00b7ser", "Stimm\u00b7lein", "in", "das", "Ohr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbkauft, lieber Herr!\u00ab Ein magres H\u00e4ndchen hielt", "tokens": ["\u00bb", "kauft", ",", "lie\u00b7ber", "Herr", "!", "\u00ab", "Ein", "mag\u00b7res", "H\u00e4nd\u00b7chen", "hielt"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "$,", "ADV", "NN", "$.", "$(", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Feilbietend mir ein \u00e4rmlich Spielzeug vor.", "tokens": ["Feil\u00b7bie\u00b7tend", "mir", "ein", "\u00e4rm\u00b7lich", "Spiel\u00b7zeug", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich schrak empor, und beim Laternenschein", "tokens": ["Ich", "schrak", "em\u00b7por", ",", "und", "beim", "La\u00b7ter\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "KON", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sah ich ein bleiches Kinderangesicht;", "tokens": ["Sah", "ich", "ein", "blei\u00b7ches", "Kin\u00b7de\u00b7ran\u00b7ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wes Alters und Geschlechts es mochte sein,", "tokens": ["Wes", "Al\u00b7ters", "und", "Ge\u00b7schlechts", "es", "moch\u00b7te", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "PPER", "VVFIN", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Erkannt ich im Vor\u00fcbertreiben nicht.", "tokens": ["Er\u00b7kannt", "ich", "im", "Vor\u00b7\u00fc\u00b7bert\u00b7rei\u00b7ben", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nur von dem Treppenstein, darauf es sa\u00df,", "tokens": ["Nur", "von", "dem", "Trep\u00b7pen\u00b7stein", ",", "da\u00b7rauf", "es", "sa\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "PAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Noch immer h\u00f6rt ich, m\u00fchsam, wie es schien:", "tokens": ["Noch", "im\u00b7mer", "h\u00f6rt", "ich", ",", "m\u00fch\u00b7sam", ",", "wie", "es", "schien", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "$,", "ADJD", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbkauft, lieber Herr!\u00ab den Ruf ohn Unterla\u00df;", "tokens": ["\u00bb", "kauft", ",", "lie\u00b7ber", "Herr", "!", "\u00ab", "den", "Ruf", "ohn", "Un\u00b7ter\u00b7la\u00df", ";"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "ADV", "NN", "$.", "$(", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch hat wohl keiner ihm Geh\u00f6r verliehn.", "tokens": ["Doch", "hat", "wohl", "kei\u00b7ner", "ihm", "Ge\u00b7h\u00f6r", "ver\u00b7liehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PIS", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und ich? \u2013 War's Ungeschick, war es die Scham,", "tokens": ["Und", "ich", "?", "\u2013", "Wa\u00b7r's", "Un\u00b7ge\u00b7schick", ",", "war", "es", "die", "Scham", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$.", "$(", "NE", "NN", "$,", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Am Weg zu handeln mit dem Bettelkind?", "tokens": ["Am", "Weg", "zu", "han\u00b7deln", "mit", "dem", "Bet\u00b7tel\u00b7kind", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKZU", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Eh meine Hand zu meiner B\u00f6rse kam,", "tokens": ["Eh", "mei\u00b7ne", "Hand", "zu", "mei\u00b7ner", "B\u00f6r\u00b7se", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Verscholl das Stimmlein hinter mir im Wind.", "tokens": ["Ver\u00b7scholl", "das", "Stimm\u00b7lein", "hin\u00b7ter", "mir", "im", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "APPR", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Doch als ich endlich war mit mir allein,", "tokens": ["Doch", "als", "ich", "end\u00b7lich", "war", "mit", "mir", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VAFIN", "APPR", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erfa\u00dfte mich die Angst im Herzen so,", "tokens": ["Er\u00b7fa\u00df\u00b7te", "mich", "die", "Angst", "im", "Her\u00b7zen", "so", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als s\u00e4\u00df mein eigen Kind auf jenem Stein", "tokens": ["Als", "s\u00e4\u00df", "mein", "ei\u00b7gen", "Kind", "auf", "je\u00b7nem", "Stein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und schrie nach Brot, indessen ich entfloh.", "tokens": ["Und", "schrie", "nach", "Brot", ",", "in\u00b7des\u00b7sen", "ich", "ent\u00b7floh", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}