{"textgrid.poem.25007": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "In der Krankheit", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht kann ich schaun den lieblichen April,", "tokens": ["Nicht", "kann", "ich", "schaun", "den", "lieb\u00b7li\u00b7chen", "Ap\u00b7ril", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VMFIN", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie reinre Luft sich um die Erde breitet", "tokens": ["Wie", "rein\u00b7re", "Luft", "sich", "um", "die", "Er\u00b7de", "brei\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "PRF", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und \u00fcbers Antlitz schon ein Traum ihr gleitet", "tokens": ["Und", "\u00fc\u00b7bers", "Ant\u00b7litz", "schon", "ein", "Traum", "ihr", "glei\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ADV", "ART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Fr\u00fchlings, der erwachen will.", "tokens": ["Des", "Fr\u00fch\u00b7lings", ",", "der", "er\u00b7wa\u00b7chen", "will", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der du dich leuchtend ob der Erde wiegst", "tokens": ["Der", "du", "dich", "leuch\u00b7tend", "ob", "der", "Er\u00b7de", "wiegst"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PRF", "VVPP", "KOUS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und hoch und h\u00f6her dort den Himmel r\u00f6test,", "tokens": ["Und", "hoch", "und", "h\u00f6\u00b7her", "dort", "den", "Him\u00b7mel", "r\u00f6\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O Morgen, da\u00df du mir die Schwingen b\u00f6test", "tokens": ["O", "Mor\u00b7gen", ",", "da\u00df", "du", "mir", "die", "Schwin\u00b7gen", "b\u00f6\u00b7test"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "KOUS", "PPER", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und mich empor vom Lager tr\u00fcgst!", "tokens": ["Und", "mich", "em\u00b7por", "vom", "La\u00b7ger", "tr\u00fcgst", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKVZ", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dann h\u00f6rt' ich, wie mit lautem Wogenschlag", "tokens": ["Dann", "h\u00f6rt'", "ich", ",", "wie", "mit", "lau\u00b7tem", "Wo\u00b7gen\u00b7schlag"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das Meer an allen Ufern rauscht' und riefe", "tokens": ["Das", "Meer", "an", "al\u00b7len", "U\u00b7fern", "rauscht'", "und", "rie\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und aus den Buchten, aus des Abgrunds Tiefe", "tokens": ["Und", "aus", "den", "Buch\u00b7ten", ",", "aus", "des", "Ab\u00b7grunds", "Tie\u00b7fe"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Entgegenjubelte dem Tag.", "tokens": ["Ent\u00b7ge\u00b7gen\u00b7ju\u00b7bel\u00b7te", "dem", "Tag", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Einmal noch s\u00e4h' ich \u00fcber Th\u00e4lergr\u00fcn", "tokens": ["Ein\u00b7mal", "noch", "s\u00e4h'", "ich", "\u00fc\u00b7ber", "Th\u00e4\u00b7ler\u00b7gr\u00fcn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Berge H\u00e4upter rosig sich verkl\u00e4ren", "tokens": ["Der", "Ber\u00b7ge", "H\u00e4up\u00b7ter", "ro\u00b7sig", "sich", "ver\u00b7kl\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "ADJD", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und hochauf von der Gletscher Eisalt\u00e4ren", "tokens": ["Und", "hoch\u00b7auf", "von", "der", "Glet\u00b7scher", "Ei\u00b7sal\u00b7t\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Morgenopferfeuer gl\u00fchn.", "tokens": ["Die", "Mor\u00b7ge\u00b7no\u00b7pfer\u00b7feu\u00b7er", "gl\u00fchn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Vergebens! Tausendfach, indes das Licht", "tokens": ["Ver\u00b7ge\u00b7bens", "!", "Tau\u00b7send\u00b7fach", ",", "in\u00b7des", "das", "Licht"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$.", "NN", "$,", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du tr\u00e4gst von Weltgestad' zu Weltgestade,", "tokens": ["Du", "tr\u00e4gst", "von", "Welt\u00b7ge\u00b7stad'", "zu", "Welt\u00b7ge\u00b7sta\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sinkt ja das Leben hin auf deinem Pfade \u2013", "tokens": ["Sinkt", "ja", "das", "Le\u00b7ben", "hin", "auf", "dei\u00b7nem", "Pfa\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was macht's, ob meins zusammenbricht?", "tokens": ["Was", "macht's", ",", "ob", "meins", "zu\u00b7sam\u00b7men\u00b7bricht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Oft noch, wie deine hohe Bahn du ziehst,", "tokens": ["Oft", "noch", ",", "wie", "dei\u00b7ne", "ho\u00b7he", "Bahn", "du", "ziehst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PWAV", "PPOSAT", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wirst du die L\u00e4nder und die Meere wecken,", "tokens": ["Wirst", "du", "die", "L\u00e4n\u00b7der", "und", "die", "Mee\u00b7re", "we\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "KON", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch mich nicht, wenn mich dunkle Schollen decken", "tokens": ["Doch", "mich", "nicht", ",", "wenn", "mich", "dunk\u00b7le", "Schol\u00b7len", "de\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "PTKNEG", "$,", "KOUS", "PPER", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und \u00fcber mir der Rasen sprie\u00dft.", "tokens": ["Und", "\u00fc\u00b7ber", "mir", "der", "Ra\u00b7sen", "sprie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}