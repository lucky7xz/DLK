{"textgrid.poem.66455": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: In der Fr\u00fch, in der Fr\u00fch, wenn die Sonn' erwacht,", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In der Fr\u00fch, in der Fr\u00fch, wenn die Sonn' erwacht,", "tokens": ["In", "der", "Fr\u00fch", ",", "in", "der", "Fr\u00fch", ",", "wenn", "die", "Sonn'", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,", "KOUS", "ART", "NN", "VVPP", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Von dem Fenster bieg' ich die Reben,", "tokens": ["Von", "dem", "Fens\u00b7ter", "bieg'", "ich", "die", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Hab getr\u00e4umet von dir wohl die ganze Nacht,", "tokens": ["Hab", "ge\u00b7tr\u00e4u\u00b7met", "von", "dir", "wohl", "die", "gan\u00b7ze", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Gr\u00fc\u00df dich Gott, gr\u00fc\u00df dich Gott, du mein Leben!", "tokens": ["Gr\u00fc\u00df", "dich", "Gott", ",", "gr\u00fc\u00df", "dich", "Gott", ",", "du", "mein", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "$,", "VVFIN", "PPER", "NN", "$,", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+--++-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Ueber Tag, \u00fcber Tag, was ich schaff' und thu',", "tokens": ["Ue\u00b7ber", "Tag", ",", "\u00fc\u00b7ber", "Tag", ",", "was", "ich", "schaff'", "und", "thu'", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$,", "PWS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Blick' ich hundertmal um die Reben,", "tokens": ["Blick'", "ich", "hun\u00b7dert\u00b7mal", "um", "die", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Denn mein Sinnen ist dein, und mein Denken bist du,", "tokens": ["Denn", "mein", "Sin\u00b7nen", "ist", "dein", ",", "und", "mein", "Den\u00b7ken", "bist", "du", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPOSAT", "$,", "KON", "PPOSAT", "NN", "VAFIN", "PPER", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Gr\u00fc\u00df dich Gott, gr\u00fc\u00df dich Gott, du mein Leben!", "tokens": ["Gr\u00fc\u00df", "dich", "Gott", ",", "gr\u00fc\u00df", "dich", "Gott", ",", "du", "mein", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "$,", "VVFIN", "PPER", "NN", "$,", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+--++-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Und zu Nacht, und zu Nacht, bei der Sterne Schein,", "tokens": ["Und", "zu", "Nacht", ",", "und", "zu", "Nacht", ",", "bei", "der", "Ster\u00b7ne", "Schein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "$,", "KON", "APPR", "NN", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Schl\u00e4gt der Wind mir an's Fenster die Reben,", "tokens": ["Schl\u00e4gt", "der", "Wind", "mir", "an's", "Fens\u00b7ter", "die", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Wach' ich auf, denk' ich dein, \u00fcber's Jahr bist du mein,", "tokens": ["Wach'", "ich", "auf", ",", "denk'", "ich", "dein", ",", "\u00fc\u00b7ber's", "Jahr", "bist", "du", "mein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "$,", "VVFIN", "PPER", "PPOSAT", "$,", "APPRART", "NN", "VAFIN", "PPER", "PPOSAT", "$,"], "meter": "+--+-+---+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Gr\u00fc\u00df dich Gott, gr\u00fc\u00df dich Gott, du mein Leben!", "tokens": ["Gr\u00fc\u00df", "dich", "Gott", ",", "gr\u00fc\u00df", "dich", "Gott", ",", "du", "mein", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "$,", "VVFIN", "PPER", "NN", "$,", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+--++-+-", "measure": "trochaic.penta.relaxed"}}}}}