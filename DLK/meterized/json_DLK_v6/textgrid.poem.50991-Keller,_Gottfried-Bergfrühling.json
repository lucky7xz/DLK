{"textgrid.poem.50991": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Bergfr\u00fchling", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Lenz ist da, die Lauine f\u00e4llt,", "tokens": ["Der", "Lenz", "ist", "da", ",", "die", "Lau\u00b7i\u00b7ne", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Sie rollt mit Tosen und Sausen ins Tal;", "tokens": ["Sie", "rollt", "mit", "To\u00b7sen", "und", "Sau\u00b7sen", "ins", "Tal", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich hab mein H\u00fcttlein daneben gestellt", "tokens": ["Ich", "hab", "mein", "H\u00fctt\u00b7lein", "da\u00b7ne\u00b7ben", "ge\u00b7stellt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "PAV", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Auf gr\u00fcnende Matten am sonnigen Strahl.", "tokens": ["Auf", "gr\u00fc\u00b7nen\u00b7de", "Mat\u00b7ten", "am", "son\u00b7ni\u00b7gen", "Strahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Und ob auch die Laue mein H\u00fcttchen trifft", "tokens": ["Und", "ob", "auch", "die", "Lau\u00b7e", "mein", "H\u00fctt\u00b7chen", "trifft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "ART", "NN", "PPOSAT", "NN", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und nieder es f\u00fchrt im donnernden Lauf \u2013", "tokens": ["Und", "nie\u00b7der", "es", "f\u00fchrt", "im", "don\u00b7nern\u00b7den", "Lauf", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sobald wieder trocken die Alpentrift,", "tokens": ["So\u00b7bald", "wie\u00b7der", "tro\u00b7cken", "die", "Al\u00b7pen\u00b7trift", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Bau ich mir singend ein neues auf.", "tokens": ["Bau", "ich", "mir", "sin\u00b7gend", "ein", "neu\u00b7es", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "ADJD", "ART", "ADJA", "PTKVZ", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Doch wenn in meines Landes Bann", "tokens": ["Doch", "wenn", "in", "mei\u00b7nes", "Lan\u00b7des", "Bann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Knechtschaft verheerende L\u00f6win f\u00e4llt,", "tokens": ["Der", "Knecht\u00b7schaft", "ver\u00b7hee\u00b7ren\u00b7de", "L\u00f6\u00b7win", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Dann z\u00fcnd ich selber die Heimstatt an", "tokens": ["Dann", "z\u00fcnd", "ich", "sel\u00b7ber", "die", "Heims\u00b7tatt", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "APPR"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und ziehe hinaus in die weite Welt!", "tokens": ["Und", "zie\u00b7he", "hin\u00b7aus", "in", "die", "wei\u00b7te", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-----+-+", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Hinaus in die Welt, in das finstere Reich,", "tokens": ["Hin\u00b7aus", "in", "die", "Welt", ",", "in", "das", "fins\u00b7te\u00b7re", "Reich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Zu dienen im Dunkel dem fremden Mann,", "tokens": ["Zu", "die\u00b7nen", "im", "Dun\u00b7kel", "dem", "frem\u00b7den", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Ein armer Gesell, der die Sterne bleich", "tokens": ["Ein", "ar\u00b7mer", "Ge\u00b7sell", ",", "der", "die", "Ster\u00b7ne", "bleich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "ADJD"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Der Heimat nimmer vergessen kann!", "tokens": ["Der", "Hei\u00b7mat", "nim\u00b7mer", "ver\u00b7ges\u00b7sen", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}