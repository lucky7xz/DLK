{"dta.poem.10684": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Antwort de\u00df Abendtsterns.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sonsten Hesperus, hab jetzundt mich gewendet/", "tokens": ["Ich", "sons\u00b7ten", "Hes\u00b7pe\u00b7rus", ",", "hab", "je\u00b7tzundt", "mich", "ge\u00b7wen\u00b7det", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "$,", "VAFIN", "ADV", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd werde ", "tokens": ["Vnd", "wer\u00b7de"], "token_info": ["word", "word"], "pos": ["KON", "VAFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Sey nit best\u00fcrtzt/ da\u00df ich den alten Lauff verkehr/", "tokens": ["Sey", "nit", "be\u00b7st\u00fcrtzt", "/", "da\u00df", "ich", "den", "al\u00b7ten", "Lauff", "ver\u00b7kehr", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "VVPP", "$(", "KOUS", "PPER", "ART", "ADJA", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil deine Sonne kompt/ so geh ich f\u00fcr jhr her.", "tokens": ["Weil", "dei\u00b7ne", "Son\u00b7ne", "kompt", "/", "so", "geh", "ich", "f\u00fcr", "jhr", "her", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$(", "ADV", "VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}