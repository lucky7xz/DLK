{"dta.poem.491": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Auf ein Frauen-Zimmer/ die ein Bereiter  \n ein wenig scharff geliebet/ und der er die Proben  \n davon hinterlassen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Freund/ du hast allhier das Reiten wollen fassen/", "tokens": ["Mein", "Freund", "/", "du", "hast", "all\u00b7hier", "das", "Rei\u00b7ten", "wol\u00b7len", "fas\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PPER", "VAFIN", "ADV", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jm Reiten hast du dich auch ziemlich angelassen:", "tokens": ["Jm", "Rei\u00b7ten", "hast", "du", "dich", "auch", "ziem\u00b7lich", "an\u00b7ge\u00b7las\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "PRF", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Jungfer ", "tokens": ["Die", "Jung\u00b7fer"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Da\u00df du ein muntres Pferd kanst nach der Kunst besteigen.", "tokens": ["Da\u00df", "du", "ein", "mun\u00b7tres", "Pferd", "kanst", "nach", "der", "Kunst", "be\u00b7stei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-