{"textgrid.poem.54363": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr lebt mein Herr, vergn\u00fcgt und frey,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.85", "da:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr lebt mein Herr, vergn\u00fcgt und frey,", "tokens": ["Ihr", "lebt", "mein", "Herr", ",", "ver\u00b7gn\u00fcgt", "und", "frey", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "VVPP", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und seyd Hans ohne Sorg dabey,", "tokens": ["Und", "seyd", "Hans", "oh\u00b7ne", "Sorg", "da\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NE", "APPR", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich aber fange Grillen.", "tokens": ["Ich", "a\u00b7ber", "fan\u00b7ge", "Gril\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihr gehet mit mir um zu hart", "tokens": ["Ihr", "ge\u00b7het", "mit", "mir", "um", "zu", "hart"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "APPR", "PTKA", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Um neun Uhr hab ich euch erwart,", "tokens": ["Um", "neun", "Uhr", "hab", "ich", "euch", "er\u00b7wart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VAFIN", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Sehnsucht mir zu stillen.", "tokens": ["Die", "Sehn\u00b7sucht", "mir", "zu", "stil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich risse mich um acht Uhr schon", "tokens": ["Ich", "ris\u00b7se", "mich", "um", "acht", "Uhr", "schon"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "CARD", "NN", "ADV"], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Aus meinem weichen Federthron", "tokens": ["Aus", "mei\u00b7nem", "wei\u00b7chen", "Fe\u00b7der\u00b7thron"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Um euch sch\u00f6n zu empfangen.", "tokens": ["Um", "euch", "sch\u00f6n", "zu", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Allein umsonst; statt eurer kam", "tokens": ["Al\u00b7lein", "um\u00b7sonst", ";", "statt", "eu\u00b7rer", "kam"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "$.", "APPR", "PPOSAT", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Diener, der kroch trefflich lahm,", "tokens": ["Ein", "Die\u00b7ner", ",", "der", "kroch", "treff\u00b7lich", "lahm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit einem Brief gegangen.", "tokens": ["Mit", "ei\u00b7nem", "Brief", "ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ey dacht ich, Schade vor den Brief,", "tokens": ["Ey", "dacht", "ich", ",", "Scha\u00b7de", "vor", "den", "Brief", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den mir der Bothe noch so tief", "tokens": ["Den", "mir", "der", "Bo\u00b7the", "noch", "so", "tief"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit einem Scharfu\u00df bringet:", "tokens": ["Mit", "ei\u00b7nem", "Schar\u00b7fu\u00df", "brin\u00b7get", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn nicht der Herr sich selbst einstellt,", "tokens": ["Wenn", "nicht", "der", "Herr", "sich", "selbst", "ein\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und sein Versprechen redlich h\u00e4lt,", "tokens": ["Und", "sein", "Ver\u00b7spre\u00b7chen", "red\u00b7lich", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das nun ganz anders klinget.", "tokens": ["Das", "nun", "ganz", "an\u00b7ders", "klin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Plagt euch der Henker ganz und gar?", "tokens": ["Plagt", "euch", "der", "Hen\u00b7ker", "ganz", "und", "gar", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "KON", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Freund, die Wittwen werden rar,", "tokens": ["Mein", "Freund", ",", "die", "Witt\u00b7wen", "wer\u00b7den", "rar", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie ich und meines gleichen.", "tokens": ["Wie", "ich", "und", "mei\u00b7nes", "glei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "KON", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein solches frommes Tugendbild", "tokens": ["Ein", "sol\u00b7ches", "from\u00b7mes", "Tu\u00b7gend\u00b7bild"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "F\u00fchrt keinen Schalk in ihrem Schild;", "tokens": ["F\u00fchrt", "kei\u00b7nen", "Schalk", "in", "ih\u00b7rem", "Schild", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Darf keiner Jungfer weichen.", "tokens": ["Darf", "kei\u00b7ner", "Jung\u00b7fer", "wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Zwar klingt das eigne Lob nicht sch\u00f6n,", "tokens": ["Zwar", "klingt", "das", "eig\u00b7ne", "Lob", "nicht", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das will ich euch wohl zugestehn;", "tokens": ["Das", "will", "ich", "euch", "wohl", "zu\u00b7ge\u00b7stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch nehmet dies zu Herzen:", "tokens": ["Doch", "neh\u00b7met", "dies", "zu", "Her\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDS", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein falsches Wort uns gleich bet\u00e4ubt,", "tokens": ["Ein", "fal\u00b7sches", "Wort", "uns", "gleich", "be\u00b7t\u00e4ubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wenn es in der W\u00e4sche bleibt,", "tokens": ["Und", "wenn", "es", "in", "der", "W\u00e4\u00b7sche", "bleibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So mu\u00df es freylich schmerzen.", "tokens": ["So", "mu\u00df", "es", "frey\u00b7lich", "schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Das Wort zu brechen, hei\u00dft sonst viel,", "tokens": ["Das", "Wort", "zu", "bre\u00b7chen", ",", "hei\u00dft", "sonst", "viel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Euch aber nur ein Kinderspiel,", "tokens": ["Euch", "a\u00b7ber", "nur", "ein", "Kin\u00b7der\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer sollte dies wohl denken!", "tokens": ["Wer", "soll\u00b7te", "dies", "wohl", "den\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PDS", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kam euch denn dies nicht in den Sinn,", "tokens": ["Kam", "euch", "denn", "dies", "nicht", "in", "den", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "PDS", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Da\u00df ich hier fremd und einsam bin?", "tokens": ["Da\u00df", "ich", "hier", "fremd", "und", "ein\u00b7sam", "bin", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "KON", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Man mu\u00df nicht G\u00e4ste kr\u00e4nken.", "tokens": ["Man", "mu\u00df", "nicht", "G\u00e4s\u00b7te", "kr\u00e4n\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PTKNEG", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ich komm den weiten Weg herauf,", "tokens": ["Ich", "komm", "den", "wei\u00b7ten", "Weg", "her\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und dennoch achtet ihr nicht drauf,", "tokens": ["Und", "den\u00b7noch", "ach\u00b7tet", "ihr", "nicht", "drauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das hei\u00dft ein feines Wesen.", "tokens": ["Das", "hei\u00dft", "ein", "fei\u00b7nes", "We\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nun reis ich auf den \u2013 \u2013 -", "tokens": ["Nun", "reis", "ich", "auf", "den", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "$(", "$(", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Und wenn die Wiederkehr wird seyn,", "tokens": ["Und", "wenn", "die", "Wie\u00b7der\u00b7kehr", "wird", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VAFIN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Will ich den Text euch lesen.", "tokens": ["Will", "ich", "den", "Text", "euch", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Doch f\u00fcrchtet euch nicht allzusehr,", "tokens": ["Doch", "f\u00fcrch\u00b7tet", "euch", "nicht", "all\u00b7zu\u00b7sehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als wenn ich gar zu grimmig w\u00e4r.", "tokens": ["Als", "wenn", "ich", "gar", "zu", "grim\u00b7mig", "w\u00e4r", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADV", "PTKA", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Lauge soll zwar beissen;", "tokens": ["Die", "Lau\u00b7ge", "soll", "zwar", "beis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch d\u00fcrft ihr, weil ich in der Still", "tokens": ["Doch", "d\u00fcrft", "ihr", ",", "weil", "ich", "in", "der", "Still"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "$,", "KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den Kopf zum Lohn euch waschen will,", "tokens": ["Den", "Kopf", "zum", "Lohn", "euch", "wa\u00b7schen", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Krause nicht zerreissen.", "tokens": ["Die", "Krau\u00b7se", "nicht", "zer\u00b7reis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}