{"dta.poem.6139": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Schwere und der Leichte,  \n von der Fluht und Ebbe  \n des Meeres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Weise kan uns nun nicht mehr verborgen bleiben,", "tokens": ["Die", "Wei\u00b7se", "kan", "uns", "nun", "nicht", "mehr", "ver\u00b7bor\u00b7gen", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "PTKNEG", "ADV", "VVPP", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df nemlich Obere die Untern C\u00f6rper treiben.", "tokens": ["Da\u00df", "nem\u00b7lich", "O\u00b7be\u00b7re", "die", "Un\u00b7tern", "C\u00f6r\u00b7per", "trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJA", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bald ist\u2019s ein sanffter, bald ein starcker Druck allein,", "tokens": ["Bald", "ist's", "ein", "sanff\u00b7ter", ",", "bald", "ein", "star\u00b7cker", "Druck", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJD", "$,", "ADV", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durch dessen wechselndes Gedr\u00e4nge", "tokens": ["Durch", "des\u00b7sen", "wech\u00b7seln\u00b7des", "Ge\u00b7dr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Jm Elementen, solche Menge", "tokens": ["Jm", "E\u00b7le\u00b7men\u00b7ten", ",", "sol\u00b7che", "Men\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$,", "PIAT", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Von Aendrungen gew\u00fcrcket seyn.", "tokens": ["Von", "A\u00b7en\u00b7drun\u00b7gen", "ge\u00b7w\u00fcr\u00b7cket", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVPP", "VAINF", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}