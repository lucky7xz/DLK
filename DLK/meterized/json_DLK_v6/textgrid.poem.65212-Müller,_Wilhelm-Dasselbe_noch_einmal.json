{"textgrid.poem.65212": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Dasselbe noch einmal", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Abendnebel sinken", "tokens": ["Die", "A\u00b7bend\u00b7ne\u00b7bel", "sin\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.2": {"text": "Hernieder kalt und schwer,", "tokens": ["Her\u00b7nie\u00b7der", "kalt", "und", "schwer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Todesengel schweben", "tokens": ["Und", "To\u00b7de\u00b7sen\u00b7gel", "schwe\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In ihrem Dampf umher.", "tokens": ["In", "ih\u00b7rem", "Dampf", "um\u00b7her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Geh\u00fcllt in meinen Mantel,", "tokens": ["Ge\u00b7h\u00fcllt", "in", "mei\u00b7nen", "Man\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Spie\u00df an's Herz gedr\u00fcckt,", "tokens": ["Den", "Spie\u00df", "an's", "Herz", "ge\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schau' ich empor zum Berge", "tokens": ["Schau'", "ich", "em\u00b7por", "zum", "Ber\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PTKVZ", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und tr\u00e4ume mich begl\u00fcckt.", "tokens": ["Und", "tr\u00e4u\u00b7me", "mich", "be\u00b7gl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er steigt so gr\u00fcn und helle", "tokens": ["Er", "steigt", "so", "gr\u00fcn", "und", "hel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "KON", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hervor aus grauem Duft,", "tokens": ["Her\u00b7vor", "aus", "grau\u00b7em", "Duft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie eine Zauberinsel", "tokens": ["Wie", "ei\u00b7ne", "Zau\u00b7be\u00b7rin\u00b7sel"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "In wogenblauer Luft.", "tokens": ["In", "wo\u00b7gen\u00b7blau\u00b7er", "Luft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der letzte Strahl der Sonne", "tokens": ["Der", "letz\u00b7te", "Strahl", "der", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ruht sich auf ihm so gern,", "tokens": ["Ruht", "sich", "auf", "ihm", "so", "gern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPER", "ADV", "ADV", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Mit seinem ersten Schimmer", "tokens": ["Mit", "sei\u00b7nem", "ers\u00b7ten", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gr\u00fc\u00dft ihn der Abendstern.", "tokens": ["Gr\u00fc\u00dft", "ihn", "der", "A\u00b7bends\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Er tr\u00e4gt ein kleines H\u00fcttchen,", "tokens": ["Er", "tr\u00e4gt", "ein", "klei\u00b7nes", "H\u00fctt\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich seh's von unten kaum,", "tokens": ["Ich", "seh's", "von", "un\u00b7ten", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und vor der H\u00fcttenth\u00fcre", "tokens": ["Und", "vor", "der", "H\u00fct\u00b7tent\u00b7h\u00fc\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bl\u00fcht ein Zitronenbaum.", "tokens": ["Bl\u00fcht", "ein", "Zit\u00b7ro\u00b7nen\u00b7baum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-++-+", "measure": "unknown.measure.tetra"}}, "stanza.6": {"line.1": {"text": "Darunter sitzt mein M\u00e4dchen,", "tokens": ["Da\u00b7run\u00b7ter", "sitzt", "mein", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Spindel in der Hand,", "tokens": ["Die", "Spin\u00b7del", "in", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und spinnt und sinnt und schauet", "tokens": ["Und", "spinnt", "und", "sinnt", "und", "schau\u00b7et"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Herab in's ebne Land.", "tokens": ["Her\u00b7ab", "in's", "eb\u00b7ne", "Land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Es lodert helles Feuer", "tokens": ["Es", "lo\u00b7dert", "hel\u00b7les", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hier unten in der Nacht,", "tokens": ["Hier", "un\u00b7ten", "in", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das ihr die St\u00e4tte weise,", "tokens": ["Das", "ihr", "die", "St\u00e4t\u00b7te", "wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo ihr Geliebter wacht.", "tokens": ["Wo", "ihr", "Ge\u00b7lieb\u00b7ter", "wacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Mein gellend Hifthorn richt' ich", "tokens": ["Mein", "gel\u00b7lend", "Hif\u00b7thorn", "richt'", "ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "VVPP", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hoch in die Luft empor,", "tokens": ["Hoch", "in", "die", "Luft", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Die Wiederhalle tragen", "tokens": ["Die", "Wie\u00b7der\u00b7hal\u00b7le", "tra\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Klang zu ihrem Ohr.", "tokens": ["Den", "Klang", "zu", "ih\u00b7rem", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Und ist das Horn verklungen", "tokens": ["Und", "ist", "das", "Horn", "ver\u00b7klun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und glimmt das Feuer aus,", "tokens": ["Und", "glimmt", "das", "Feu\u00b7er", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Geliebte, geh' und pfl\u00fccke", "tokens": ["Ge\u00b7lieb\u00b7te", ",", "geh'", "und", "pfl\u00fc\u00b7cke"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir einen Blumenstrau\u00df.", "tokens": ["Mir", "ei\u00b7nen", "Blu\u00b7men\u00b7strau\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Und wirf ihn von der H\u00f6he", "tokens": ["Und", "wirf", "ihn", "von", "der", "H\u00f6\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit einem Gru\u00df herab,", "tokens": ["Mit", "ei\u00b7nem", "Gru\u00df", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann tragen schnelle Winde", "tokens": ["Dann", "tra\u00b7gen", "schnel\u00b7le", "Win\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihn auf mein frisches Grab.", "tokens": ["Ihn", "auf", "mein", "fri\u00b7sches", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}