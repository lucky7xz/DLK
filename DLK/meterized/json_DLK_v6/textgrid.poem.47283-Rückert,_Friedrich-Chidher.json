{"textgrid.poem.47283": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Chidher", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Chidher, der ewig junge, sprach:", "tokens": ["Chid\u00b7her", ",", "der", "e\u00b7wig", "jun\u00b7ge", ",", "sprach", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADJD", "ADJA", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbich fuhr an einer Stadt vorbei,", "tokens": ["\u00bb", "ich", "fuhr", "an", "ei\u00b7ner", "Stadt", "vor\u00b7bei", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Mann im Garten Fr\u00fcchte brach;", "tokens": ["Ein", "Mann", "im", "Gar\u00b7ten", "Fr\u00fcch\u00b7te", "brach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich fragte, seit wann die Stadt hier sei?\u00ab", "tokens": ["Ich", "frag\u00b7te", ",", "seit", "wann", "die", "Stadt", "hier", "sei", "?", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "PWAV", "ART", "NN", "ADV", "VAFIN", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Er sprach und pfl\u00fcckte die Fr\u00fcchte fort:", "tokens": ["Er", "sprach", "und", "pfl\u00fcck\u00b7te", "die", "Fr\u00fcch\u00b7te", "fort", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "\u00bbdie Stadt steht ewig an diesem Ort", "tokens": ["\u00bb", "die", "Stadt", "steht", "e\u00b7wig", "an", "die\u00b7sem", "Ort"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "NN", "VVFIN", "ADJD", "APPR", "PDAT", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und wird so stehen ewig fort.\u00ab", "tokens": ["Und", "wird", "so", "ste\u00b7hen", "e\u00b7wig", "fort", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VAFIN", "ADV", "VVFIN", "ADJD", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und aber nach f\u00fcnfhundert Jahren", "tokens": ["Und", "a\u00b7ber", "nach", "f\u00fcnf\u00b7hun\u00b7dert", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "CARD", "NN"], "meter": "-+-+++-+-", "measure": "unknown.measure.penta"}, "line.9": {"text": "Kam ich desselbigen Wegs gefahren.", "tokens": ["Kam", "ich", "des\u00b7sel\u00b7bi\u00b7gen", "Wegs", "ge\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PAV", "NN", "VVPP", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Da fand ich keine Spur der Stadt;", "tokens": ["Da", "fand", "ich", "kei\u00b7ne", "Spur", "der", "Stadt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein einsamer Sch\u00e4fer blies die Schalmei,", "tokens": ["Ein", "ein\u00b7sa\u00b7mer", "Sch\u00e4\u00b7fer", "blies", "die", "Schal\u00b7mei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Die Herde weidete Laub und Blatt;", "tokens": ["Die", "Her\u00b7de", "wei\u00b7de\u00b7te", "Laub", "und", "Blatt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ich fragte: \u00bbWie lang' ist die Stadt vorbei?\u00ab", "tokens": ["Ich", "frag\u00b7te", ":", "\u00bb", "Wie", "lang'", "ist", "die", "Stadt", "vor\u00b7bei", "?", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "PWAV", "ADV", "VAFIN", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Er sprach und blies auf dem Rohre fort:", "tokens": ["Er", "sprach", "und", "blies", "auf", "dem", "Roh\u00b7re", "fort", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "\u00bbdas eine w\u00e4chst, wenn das andre dorrt;", "tokens": ["\u00bb", "das", "ei\u00b7ne", "w\u00e4chst", ",", "wenn", "das", "and\u00b7re", "dorrt", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "PIS", "VVFIN", "$,", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Das ist mein ewiger Weideort.\u00ab", "tokens": ["Das", "ist", "mein", "e\u00b7wi\u00b7ger", "Wei\u00b7de\u00b7ort", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und aber nach f\u00fcnfhundert Jahren", "tokens": ["Und", "a\u00b7ber", "nach", "f\u00fcnf\u00b7hun\u00b7dert", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "CARD", "NN"], "meter": "-+-+++-+-", "measure": "unknown.measure.penta"}, "line.9": {"text": "Kam ich desselbigen Wegs gefahren.", "tokens": ["Kam", "ich", "des\u00b7sel\u00b7bi\u00b7gen", "Wegs", "ge\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PAV", "NN", "VVPP", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Da fand ich ein Meer, das Wellen schlug,", "tokens": ["Da", "fand", "ich", "ein", "Meer", ",", "das", "Wel\u00b7len", "schlug", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein Schiffer warf die Netze frei;", "tokens": ["Ein", "Schif\u00b7fer", "warf", "die", "Net\u00b7ze", "frei", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und als er ruhte vom schweren Zug,", "tokens": ["Und", "als", "er", "ruh\u00b7te", "vom", "schwe\u00b7ren", "Zug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Fragt' ich, seit wann das Meer hier sei?", "tokens": ["Fragt'", "ich", ",", "seit", "wann", "das", "Meer", "hier", "sei", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "PWAV", "ART", "NN", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er sprach und lachte meinem Wort:", "tokens": ["Er", "sprach", "und", "lach\u00b7te", "mei\u00b7nem", "Wort", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u00bbso lang' als sch\u00e4umen die Wellen dort,", "tokens": ["\u00bb", "so", "lang'", "als", "sch\u00e4u\u00b7men", "die", "Wel\u00b7len", "dort", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "KOKOM", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Fischt man und fischt man in diesem Port.\u00ab", "tokens": ["Fischt", "man", "und", "fischt", "man", "in", "die\u00b7sem", "Port", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PIS", "KON", "VVFIN", "PIS", "APPR", "PDAT", "NN", "$.", "$("], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.8": {"text": "Und aber nach f\u00fcnfhundert Jahren", "tokens": ["Und", "a\u00b7ber", "nach", "f\u00fcnf\u00b7hun\u00b7dert", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "CARD", "NN"], "meter": "-+-+++-+-", "measure": "unknown.measure.penta"}, "line.9": {"text": "Kam ich desselbigen Wegs gefahren.", "tokens": ["Kam", "ich", "des\u00b7sel\u00b7bi\u00b7gen", "Wegs", "ge\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PAV", "NN", "VVPP", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Da fand ich einen waldigen Raum", "tokens": ["Da", "fand", "ich", "ei\u00b7nen", "wal\u00b7di\u00b7gen", "Raum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und einen Mann in der Siedelei,", "tokens": ["Und", "ei\u00b7nen", "Mann", "in", "der", "Sie\u00b7de\u00b7lei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Er f\u00e4llte mit der Axt den Baum;", "tokens": ["Er", "f\u00e4ll\u00b7te", "mit", "der", "Axt", "den", "Baum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich fragte, wie alt der Wald hier sei?", "tokens": ["Ich", "frag\u00b7te", ",", "wie", "alt", "der", "Wald", "hier", "sei", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ADJD", "ART", "NN", "ADV", "VAFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Er sprach: \u00bbDer Wald ist ein ewiger Hort;", "tokens": ["Er", "sprach", ":", "\u00bb", "Der", "Wald", "ist", "ein", "e\u00b7wi\u00b7ger", "Hort", ";"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Schon ewig wohn' ich an diesem Ort,", "tokens": ["Schon", "e\u00b7wig", "wohn'", "ich", "an", "die\u00b7sem", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und ewig wachsen die B\u00e4um' hier fort.\u00ab", "tokens": ["Und", "e\u00b7wig", "wach\u00b7sen", "die", "B\u00e4um'", "hier", "fort", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und aber nach f\u00fcnfhundert Jahren", "tokens": ["Und", "a\u00b7ber", "nach", "f\u00fcnf\u00b7hun\u00b7dert", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "CARD", "NN"], "meter": "-+-+++-+-", "measure": "unknown.measure.penta"}, "line.9": {"text": "Kam ich desselbigen Wegs gefahren.", "tokens": ["Kam", "ich", "des\u00b7sel\u00b7bi\u00b7gen", "Wegs", "ge\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PAV", "NN", "VVPP", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Da fand ich eine Stadt, und laut", "tokens": ["Da", "fand", "ich", "ei\u00b7ne", "Stadt", ",", "und", "laut"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erschallte der Markt vom Volksgeschrei.", "tokens": ["Er\u00b7schall\u00b7te", "der", "Markt", "vom", "Volks\u00b7ge\u00b7schrei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich fragte: \u00bbSeit wann ist die Stadt erbaut?", "tokens": ["Ich", "frag\u00b7te", ":", "\u00bb", "Seit", "wann", "ist", "die", "Stadt", "er\u00b7baut", "?"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "APPR", "PWAV", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wohin ist Wald und Meer und Schalmei?\u00ab", "tokens": ["Wo\u00b7hin", "ist", "Wald", "und", "Meer", "und", "Schal\u00b7mei", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VAFIN", "NN", "KON", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie schrien und h\u00f6rten nicht mein Wort:", "tokens": ["Sie", "schri\u00b7en", "und", "h\u00f6r\u00b7ten", "nicht", "mein", "Wort", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "\u00bbso ging es ewig an diesem Ort", "tokens": ["\u00bb", "so", "ging", "es", "e\u00b7wig", "an", "die\u00b7sem", "Ort"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ADJD", "APPR", "PDAT", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und wird so gehen ewig fort.\u00ab", "tokens": ["Und", "wird", "so", "ge\u00b7hen", "e\u00b7wig", "fort", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VAFIN", "ADV", "VVFIN", "ADJD", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und aber nach f\u00fcnfhundert Jahren", "tokens": ["Und", "a\u00b7ber", "nach", "f\u00fcnf\u00b7hun\u00b7dert", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "CARD", "NN"], "meter": "-+-+++-+-", "measure": "unknown.measure.penta"}, "line.9": {"text": "Will ich desselbigen Weges fahren.",