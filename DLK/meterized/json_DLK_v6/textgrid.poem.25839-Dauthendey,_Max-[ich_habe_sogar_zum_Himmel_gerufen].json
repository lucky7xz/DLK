{"textgrid.poem.25839": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "[ich habe sogar zum Himmel gerufen]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe sogar zum Himmel gerufen,", "tokens": ["Ich", "ha\u00b7be", "so\u00b7gar", "zum", "Him\u00b7mel", "ge\u00b7ru\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er lie\u00df einen Regenbogen prangen,", "tokens": ["Er", "lie\u00df", "ei\u00b7nen", "Re\u00b7gen\u00b7bo\u00b7gen", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich wollte dich doch lachend und weinend", "tokens": ["Ich", "woll\u00b7te", "dich", "doch", "la\u00b7chend", "und", "wei\u00b7nend"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADJD", "KON", "VVPP"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit allen Himmelsfarben empfangen.", "tokens": ["Mit", "al\u00b7len", "Him\u00b7mels\u00b7far\u00b7ben", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ich stand auf den Klippen", "tokens": ["Ich", "stand", "auf", "den", "Klip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Und schaute und schaute,", "tokens": ["Und", "schau\u00b7te", "und", "schau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Das Auge hungrig,", "tokens": ["Das", "Au\u00b7ge", "hung\u00b7rig", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Das Herz gl\u00fcckschwer,", "tokens": ["Das", "Herz", "gl\u00fcck\u00b7schwer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Ich sandte die M\u00f6ven,", "tokens": ["Ich", "sand\u00b7te", "die", "M\u00f6\u00b7ven", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Nach dir zu sp\u00e4hen,", "tokens": ["Nach", "dir", "zu", "sp\u00e4\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Hinaus auf das leere, herzleere Meer,", "tokens": ["Hin\u00b7aus", "auf", "das", "lee\u00b7re", ",", "herz\u00b7lee\u00b7re", "Meer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Der Himmel welkte,", "tokens": ["Der", "Him\u00b7mel", "welk\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Die Nacht hob sich ernst,", "tokens": ["Die", "Nacht", "hob", "sich", "ernst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.14": {"text": "Ernster und k\u00e4lter als alle N\u00e4chte;", "tokens": ["Erns\u00b7ter", "und", "k\u00e4l\u00b7ter", "als", "al\u00b7le", "N\u00e4ch\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "KOKOM", "PIAT", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.15": {"text": "Sacht stieg ich nieder.", "tokens": ["Sacht", "stieg", "ich", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.16": {"text": "Ging sacht zum Haus,", "tokens": ["Ging", "sacht", "zum", "Haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.17": {"text": "Ernster und k\u00e4lter noch als diese Nacht.", "tokens": ["Erns\u00b7ter", "und", "k\u00e4l\u00b7ter", "noch", "als", "die\u00b7se", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "ADV", "KOUS", "PDAT", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}}}}