{"textgrid.poem.34195": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Zu hoher Preis", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ob ich den Wirkungskreis mir w\u00fcnsche? K\u00f6nnt ihr noch fragen,", "tokens": ["Ob", "ich", "den", "Wir\u00b7kungs\u00b7kreis", "mir", "w\u00fcn\u00b7sche", "?", "K\u00f6nnt", "ihr", "noch", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PPER", "VVFIN", "$.", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+--+--+-+--+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wenn es im Spott nicht geschieht? Stellt nur den Preis nicht zu hoch.", "tokens": ["Wenn", "es", "im", "Spott", "nicht", "ge\u00b7schieht", "?", "Stellt", "nur", "den", "Preis", "nicht", "zu", "hoch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PTKNEG", "VVFIN", "$.", "VVFIN", "ADV", "ART", "NN", "PTKNEG", "PTKA", "ADJD", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Eh' ich das Leben mit dem erkaufe, was ihm den Werth giebt,", "tokens": ["Eh'", "ich", "das", "Le\u00b7ben", "mit", "dem", "er\u00b7kau\u00b7fe", ",", "was", "ihm", "den", "Werth", "giebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "ART", "VVFIN", "$,", "PWS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Lass' ich's fahren, und das wird ja fast immer verlangt!", "tokens": ["Lass'", "ich's", "fah\u00b7ren", ",", "und", "das", "wird", "ja", "fast", "im\u00b7mer", "ver\u00b7langt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIS", "VVINF", "$,", "KON", "PDS", "VAFIN", "ADV", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}}}}}