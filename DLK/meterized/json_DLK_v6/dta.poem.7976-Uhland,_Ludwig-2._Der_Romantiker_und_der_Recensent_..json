{"dta.poem.7976": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "2.  Der Romantiker und der Recensent .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "La\u00df Er doch sein n\u00e4chtlich Johlen,", "tokens": ["La\u00df", "Er", "doch", "sein", "n\u00e4cht\u00b7lich", "Joh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Poetaster Helikanus!", "tokens": ["Poe\u00b7tas\u00b7ter", "He\u00b7li\u00b7ka\u00b7nus", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was Er singt, ist nur gestohlen", "tokens": ["Was", "Er", "singt", ",", "ist", "nur", "ge\u00b7stoh\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VAFIN", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus dem Kaiser Oktavianus,", "tokens": ["Aus", "dem", "Kai\u00b7ser", "Ok\u00b7ta\u00b7vi\u00b7a\u00b7nus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Der bei mir nicht sehr empfohlen,", "tokens": ["Der", "bei", "mir", "nicht", "sehr", "emp\u00b7foh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Den ich der gelehrten Welt", "tokens": ["Den", "ich", "der", "ge\u00b7lehr\u00b7ten", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Von den Alpen bis zum Belt", "tokens": ["Von", "den", "Al\u00b7pen", "bis", "zum", "Belt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "APPRART", "NN"], "meter": "---+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Preisgab als ein Werk der Rotte,", "tokens": ["Preis\u00b7gab", "als", "ein", "Werk", "der", "Rot\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Die den Unsinn hub zum Gotte,", "tokens": ["Die", "den", "Un\u00b7sinn", "hub", "zum", "Got\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Die den Sinn gefangen h&#228;lt.", "tokens": ["Die", "den", "Sinn", "ge\u00b7fan\u00b7gen", "h", "&#228;", "lt."], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "abbreviation"], "pos": ["ART", "ART", "NN", "ADJD", "VVFIN", "$(", "XY"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}