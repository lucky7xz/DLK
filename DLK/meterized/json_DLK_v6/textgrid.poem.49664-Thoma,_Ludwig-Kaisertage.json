{"textgrid.poem.49664": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Kaisertage", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Venedig, Fahnen, Blumenpracht", "tokens": ["Ve\u00b7ne\u00b7dig", ",", "Fah\u00b7nen", ",", "Blu\u00b7men\u00b7pracht"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NE", "$,", "NN", "$,", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Und Kr\u00e4nze von dem Lorbeerboom,", "tokens": ["Und", "Kr\u00e4n\u00b7ze", "von", "dem", "Lor\u00b7beer\u00b7boom", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es br\u00fcllt und johlt und zischt und kracht", "tokens": ["Es", "br\u00fcllt", "und", "johlt", "und", "zischt", "und", "kracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Rings um den alten Markusdom.", "tokens": ["Rings", "um", "den", "al\u00b7ten", "Mar\u00b7kus\u00b7dom", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Acht Tage keine Stunde Ruh',", "tokens": ["Acht", "Ta\u00b7ge", "kei\u00b7ne", "Stun\u00b7de", "Ruh'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kanonenschl\u00e4ge, Redeflu\u00df,", "tokens": ["Ka\u00b7no\u00b7nen\u00b7schl\u00e4\u00b7ge", ",", "Re\u00b7de\u00b7flu\u00df", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann kam Messina, kam Korfu,", "tokens": ["Dann", "kam", "Mes\u00b7si\u00b7na", ",", "kam", "Kor\u00b7fu", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "$,", "VVFIN", "NE", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Die Glocke brummt, es kracht der Schu\u00df.", "tokens": ["Die", "Glo\u00b7cke", "brummt", ",", "es", "kracht", "der", "Schu\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dann geht die Fahrt nach Pola hin,", "tokens": ["Dann", "geht", "die", "Fahrt", "nach", "Po\u00b7la", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hier schmettern Salven ohne Zahl,", "tokens": ["Hier", "schmet\u00b7tern", "Sal\u00b7ven", "oh\u00b7ne", "Zahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du lieber Himmel, erst in Wien!", "tokens": ["Du", "lie\u00b7ber", "Him\u00b7mel", ",", "erst", "in", "Wi\u00b7en", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "NN", "$,", "ADV", "APPR", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zwei Tage nichts als Fest und Mahl!", "tokens": ["Zwei", "Ta\u00b7ge", "nichts", "als", "Fest", "und", "Mahl", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "PIS", "KOKOM", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wahrhaftig, h\u00f6chstes Pflichtgef\u00fchl", "tokens": ["Wahr\u00b7haf\u00b7tig", ",", "h\u00f6chs\u00b7tes", "Pflicht\u00b7ge\u00b7f\u00fchl"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADJD", "$,", "ADJA", "NN"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Bringt einzig unsern Kaiser durch", "tokens": ["Bringt", "ein\u00b7zig", "un\u00b7sern", "Kai\u00b7ser", "durch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sechs Wochen langes Festgew\u00fchl.", "tokens": ["Sechs", "Wo\u00b7chen", "lan\u00b7ges", "Fest\u00b7ge\u00b7w\u00fchl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nun weiht er die Hohk\u00f6nigsburg.", "tokens": ["Nun", "weiht", "er", "die", "Hoh\u00b7k\u00f6\u00b7nigs\u00b7burg", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}}}}}