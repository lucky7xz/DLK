{"textgrid.poem.60750": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "1L: O sch\u00f6ne Zeit, in der der Mensch die Menschen lieben kann!", "genre": "verse", "period": "N.A.", "pub_year": 1830, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O sch\u00f6ne Zeit, in der der Mensch die Menschen lieben kann!", "tokens": ["O", "sch\u00f6\u00b7ne", "Zeit", ",", "in", "der", "der", "Mensch", "die", "Men\u00b7schen", "lie\u00b7ben", "kann", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "APPR", "PRELS", "ART", "NN", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Auf meinem Herzen liegt ein Fluch, auf meinem Geist ein Bann.", "tokens": ["Auf", "mei\u00b7nem", "Her\u00b7zen", "liegt", "ein", "Fluch", ",", "auf", "mei\u00b7nem", "Geist", "ein", "Bann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,", "APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.2": {"line.1": {"text": "Erst litt ich manche hei\u00dfe Qual, nun find ich Lieb und Gl\u00fcck;", "tokens": ["Erst", "litt", "ich", "man\u00b7che", "hei\u00b7\u00dfe", "Qual", ",", "nun", "find", "ich", "Lieb", "und", "Gl\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "ADJA", "NN", "$,", "ADV", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Doch solch ein sch\u00f6nes Hochgef\u00fchl, ich geb es nicht zur\u00fcck!", "tokens": ["Doch", "solch", "ein", "sch\u00f6\u00b7nes", "Hoch\u00b7ge\u00b7f\u00fchl", ",", "ich", "geb", "es", "nicht", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.3": {"line.1": {"text": "Voll Ruhe, doch wie freudenlos durchschweif ich West und Ost:", "tokens": ["Voll", "Ru\u00b7he", ",", "doch", "wie", "freu\u00b7den\u00b7los", "durch\u00b7schweif", "ich", "West", "und", "Ost", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "ADV", "KOKOM", "ADJD", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Auf namenlose Gluten folgt ein namenloser Frost.", "tokens": ["Auf", "na\u00b7men\u00b7lo\u00b7se", "Glu\u00b7ten", "folgt", "ein", "na\u00b7men\u00b7lo\u00b7ser", "Frost", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.4": {"line.1": {"text": "Und dr\u00fcckt ein Mensch mir liebevoll und leise nur die Hand,", "tokens": ["Und", "dr\u00fcckt", "ein", "Mensch", "mir", "lie\u00b7be\u00b7voll", "und", "lei\u00b7se", "nur", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "ADJD", "KON", "ADJD", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Empfind ich Gleich geheimen Schmerz und tiefen Widerstand.", "tokens": ["Emp\u00b7find", "ich", "Gleich", "ge\u00b7hei\u00b7men", "Schmerz", "und", "tie\u00b7fen", "Wi\u00b7der\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.5": {"line.1": {"text": "Was stellt sich mir mit solchem Glanz dein holdes Wesen dar,", "tokens": ["Was", "stellt", "sich", "mir", "mit", "sol\u00b7chem", "Glanz", "dein", "hol\u00b7des", "We\u00b7sen", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "PPER", "APPR", "PIAT", "NN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Als w\u00e4r ich noch so warm, so voll, wie meine Jugend war.", "tokens": ["Als", "w\u00e4r", "ich", "noch", "so", "warm", ",", "so", "voll", ",", "wie", "mei\u00b7ne", "Ju\u00b7gend", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "PWAV", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}