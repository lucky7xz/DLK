{"dta.poem.10596": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Echo oder Widerschall.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Di\u00df Ort mit B\u00e4umen gantz vmbgeben/", "tokens": ["Di\u00df", "Ort", "mit", "B\u00e4u\u00b7men", "gantz", "vmb\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "APPR", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da nichts als Furcht vnd Schatten schweben/", "tokens": ["Da", "nichts", "als", "Furcht", "vnd", "Schat\u00b7ten", "schwe\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "KOKOM", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da Trawrigkeit sich hin verf\u00fcgt/", "tokens": ["Da", "Traw\u00b7rig\u00b7keit", "sich", "hin", "ver\u00b7f\u00fcgt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PRF", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da alles w\u00fcst vnd \u00f6de ligt/", "tokens": ["Da", "al\u00b7les", "w\u00fcst", "vnd", "\u00f6\u00b7de", "ligt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "KON", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da auch die Sonne nicht hinreichet/", "tokens": ["Da", "auch", "die", "Son\u00b7ne", "nicht", "hin\u00b7rei\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da gifftig Vngeziefer schleichet/", "tokens": ["Da", "giff\u00b7tig", "Vn\u00b7ge\u00b7zie\u00b7fer", "schlei\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da gar kein Wasser sich ergeu\u00dft/", "tokens": ["Da", "gar", "kein", "Was\u00b7ser", "sich", "er\u00b7geu\u00dft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIAT", "NN", "PRF", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Als da\u00df au\u00df meinen Augen fleust/", "tokens": ["Als", "da\u00df", "au\u00df", "mei\u00b7nen", "Au\u00b7gen", "fleust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Da gar kein Liecht nicht wirdt erkennet/", "tokens": ["Da", "gar", "kein", "Liecht", "nicht", "wirdt", "er\u00b7ken\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIAT", "NN", "PTKNEG", "VAFIN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Als das au\u00df meinem Hertzen brennet/", "tokens": ["Als", "das", "au\u00df", "mei\u00b7nem", "Hert\u00b7zen", "bren\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Beduncket mich bequem zusein/", "tokens": ["Be\u00b7dun\u00b7cket", "mich", "be\u00b7quem", "zu\u00b7sein", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Da ich mich klag ab meiner Pein/", "tokens": ["Da", "ich", "mich", "klag", "ab", "mei\u00b7ner", "Pein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Ab meiner Pein/ ab meinem Leiden/", "tokens": ["Ab", "mei\u00b7ner", "Pein", "/", "ab", "mei\u00b7nem", "Lei\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$(", "APPR", "PPOSAT", "NN", "$("], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Da\u00df mich jetzundt wirdt von mir scheyden/", "tokens": ["Da\u00df", "mich", "je\u00b7tzundt", "wirdt", "von", "mir", "schey\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "APPR", "PPER", "VVINF", "$("], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "Doch eh der lang gew\u00fcnschte Todt", "tokens": ["Doch", "eh", "der", "lang", "ge\u00b7w\u00fcnschte", "Todt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.16": {"text": "Mit frewden abhilfft meiner Noth/", "tokens": ["Mit", "frew\u00b7den", "ab\u00b7hilfft", "mei\u00b7ner", "Noth", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Will ich von meiner Liebe klagen/", "tokens": ["Will", "ich", "von", "mei\u00b7ner", "Lie\u00b7be", "kla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Vnd/ ob schon gantz vergeblich/ fragen/", "tokens": ["Vnd", "/", "ob", "schon", "gantz", "ver\u00b7geb\u00b7lich", "/", "fra\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "$(", "KOUS", "ADV", "ADV", "ADJD", "$(", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Ist dann niemandt der tr\u00f6ste mich/", "tokens": ["Ist", "dann", "nie\u00b7mandt", "der", "tr\u00f6s\u00b7te", "mich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIS", "ART", "ADJA", "PPER", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.20": {"text": "Weil ich so trawre jnniglich? Ich.", "tokens": ["Weil", "ich", "so", "traw\u00b7re", "jn\u00b7nig\u00b7lich", "?", "Ich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "PPER", "$.", "PPER", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.21": {"text": "O Echo/ wirdt ohn dich alleine", "tokens": ["O", "E\u00b7cho", "/", "wirdt", "ohn", "dich", "al\u00b7lei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$(", "VAFIN", "APPR", "PPER", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Hinfort mich nimmer tr\u00f6sten keine? Eine.", "tokens": ["Hin\u00b7fort", "mich", "nim\u00b7mer", "tr\u00f6s\u00b7ten", "kei\u00b7ne", "?", "Ei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "VVFIN", "PIAT", "$.", "ART", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Wie soll sie l\u00f6schen meinen Brandt/", "tokens": ["Wie", "soll", "sie", "l\u00f6\u00b7schen", "mei\u00b7nen", "Brandt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Ist sie mir doch noch vnbekandt? Bekandt.", "tokens": ["Ist", "sie", "mir", "doch", "noch", "vn\u00b7be\u00b7kandt", "?", "Be\u00b7kandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "ADV", "ADJD", "$.", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Die die ich kenn wills nicht verstehen/", "tokens": ["Die", "die", "ich", "kenn", "wills", "nicht", "ver\u00b7ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRELS", "PPER", "VVFIN", "VMFIN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Lest mich in Leydt ohn abla\u00df gehen. La\u00df gehen.", "tokens": ["Lest", "mich", "in", "Leydt", "ohn", "ab\u00b7la\u00df", "ge\u00b7hen", ".", "La\u00df", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "APPR", "NN", "VVINF", "$.", "VVIMP", "VVINF", "$."], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.27": {"text": "La\u00df ich es gehn vnd komm in freudt/", "tokens": ["La\u00df", "ich", "es", "gehn", "vnd", "komm", "in", "freudt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "VVINF", "KON", "VVFIN", "APPR", "NE", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Wem soll ichs dancken mit der Zeit? Der Zeit.", "tokens": ["Wem", "soll", "ichs", "dan\u00b7cken", "mit", "der", "Zeit", "?", "Der", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PIS", "VVINF", "APPR", "ART", "NN", "$.", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "So ist nun noth da\u00df ich verscharre", "tokens": ["So", "ist", "nun", "noth", "da\u00df", "ich", "ver\u00b7schar\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "NN", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Das Fewer/ vnd der Stundt erharre? Harre/", "tokens": ["Das", "Fe\u00b7wer", "/", "vnd", "der", "Stundt", "er\u00b7har\u00b7re", "?", "Har\u00b7re", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$(", "KON", "ART", "NN", "VVFIN", "$.", "NE", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Wenn ich zu lange harren solt/", "tokens": ["Wenn", "ich", "zu", "lan\u00b7ge", "har\u00b7ren", "solt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKA", "ADV", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Was h\u00fclffe meiner Vngedult? Gedult.", "tokens": ["Was", "h\u00fclf\u00b7fe", "mei\u00b7ner", "Vn\u00b7ge\u00b7dult", "?", "Ge\u00b7dult", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "$.", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Vieleichte m\u00f6cht ich sterben eh/", "tokens": ["Vie\u00b7leich\u00b7te", "m\u00f6cht", "ich", "ster\u00b7ben", "eh", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VVFIN", "KOUS", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Weil ich in h\u00f6chstem Elend geh. Entgehe.", "tokens": ["Weil", "ich", "in", "h\u00f6chs\u00b7tem", "E\u00b7lend", "geh.", "Ent\u00b7ge\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "abbreviation", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.35": {"text": "So folg ich deinem Rathe schlecht/", "tokens": ["So", "folg", "ich", "dei\u00b7nem", "Ra\u00b7the", "schlecht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Hoff alles werde gut vnd recht. Recht.", "tokens": ["Hoff", "al\u00b7les", "wer\u00b7de", "gut", "vnd", "recht", ".", "Recht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PIS", "VAFIN", "ADJD", "KON", "ADJD", "$.", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.37": {"text": "Nun bin ich vieler Noth entbunden", "tokens": ["Nun", "bin", "ich", "vie\u00b7ler", "Noth", "ent\u00b7bun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Vnd habe guten Trost empfunden/", "tokens": ["Vnd", "ha\u00b7be", "gu\u00b7ten", "Trost", "emp\u00b7fun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "Du vnbewohnte Trawrigkeit/", "tokens": ["Du", "vn\u00b7be\u00b7wohn\u00b7te", "Traw\u00b7rig\u00b7keit", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Ihr Hecken voll von meinem Leidt.", "tokens": ["Ihr", "He\u00b7cken", "voll", "von", "mei\u00b7nem", "Leidt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Ihr grausam H\u00f6len vnd jhr W\u00fcsten/", "tokens": ["Ihr", "grau\u00b7sam", "H\u00f6\u00b7len", "vnd", "jhr", "W\u00fcs\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "KON", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "Da Eulen/ Natern/ Schlangen nisten/", "tokens": ["Da", "Eu\u00b7len", "/", "Na\u00b7tern", "/", "Schlan\u00b7gen", "nis\u00b7ten", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NE", "$(", "NN", "$(", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Du wuster Orth gehab dich wohl/", "tokens": ["Du", "wus\u00b7ter", "Orth", "ge\u00b7hab", "dich", "wohl", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Ich bin f\u00fcr trawren frewde voll.", "tokens": ["Ich", "bin", "f\u00fcr", "traw\u00b7ren", "frew\u00b7de", "voll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "F\u00fcr Finsterni\u00df/ such ich die Sonnen/", "tokens": ["F\u00fcr", "Fins\u00b7ter\u00b7ni\u00df", "/", "such", "ich", "die", "Son\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "VVIMP", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.46": {"text": "F\u00fcr Thr\u00e4nen/ einen k\u00fclen Bronnen/", "tokens": ["F\u00fcr", "Thr\u00e4\u00b7nen", "/", "ei\u00b7nen", "k\u00fc\u00b7len", "Bron\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.47": {"text": "Die so Vertr\u00f6stung mir gethan/", "tokens": ["Die", "so", "Ver\u00b7tr\u00f6s\u00b7tung", "mir", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Gewi\u00dflich nicht betriegen kan.", "tokens": ["Ge\u00b7wi\u00df\u00b7lich", "nicht", "be\u00b7trie\u00b7gen", "kan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}