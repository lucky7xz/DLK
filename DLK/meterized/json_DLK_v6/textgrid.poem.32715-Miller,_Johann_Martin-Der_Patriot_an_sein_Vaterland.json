{"textgrid.poem.32715": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "Der Patriot an sein Vaterland", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "S\u00fc\u00df ist der Name Vaterland,", "tokens": ["S\u00fc\u00df", "ist", "der", "Na\u00b7me", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo Einigkeit mit festem Band", "tokens": ["Wo", "Ei\u00b7nig\u00b7keit", "mit", "fes\u00b7tem", "Band"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die B\u00fcrgerherzen kettet;", "tokens": ["Die", "B\u00fcr\u00b7ger\u00b7her\u00b7zen", "ket\u00b7tet", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo jeder gern durch eignes Blut", "tokens": ["Wo", "je\u00b7der", "gern", "durch", "eig\u00b7nes", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Des Nebenb\u00fcrgers Hab und Gut", "tokens": ["Des", "Ne\u00b7ben\u00b7b\u00fcr\u00b7gers", "Hab", "und", "Gut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von R\u00e4uberh\u00e4nden rettet.", "tokens": ["Von", "R\u00e4u\u00b7ber\u00b7h\u00e4n\u00b7den", "ret\u00b7tet", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wo auf dem Thron Gerechtigkeit", "tokens": ["Wo", "auf", "dem", "Thron", "Ge\u00b7rech\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Armen, der um H\u00fclfe schreit,", "tokens": ["Den", "Ar\u00b7men", ",", "der", "um", "H\u00fcl\u00b7fe", "schreit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vor Unterdr\u00fcckung sch\u00fctzet;", "tokens": ["Vor", "Un\u00b7ter\u00b7dr\u00fc\u00b7ckung", "sch\u00fct\u00b7zet", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und, von der Unschuld Wehr umschanzt,", "tokens": ["Und", ",", "von", "der", "Un\u00b7schuld", "Wehr", "um\u00b7schanzt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In Lauben, die er selbst gepflanzt,", "tokens": ["In", "Lau\u00b7ben", ",", "die", "er", "selbst", "ge\u00b7pflanzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der greise B\u00fcrger sitzet.", "tokens": ["Der", "grei\u00b7se", "B\u00fcr\u00b7ger", "sit\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wo alle Priester Christen sind,", "tokens": ["Wo", "al\u00b7le", "Pries\u00b7ter", "Chris\u00b7ten", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und kein Verf\u00fchrer unser Kind", "tokens": ["Und", "kein", "Ver\u00b7f\u00fch\u00b7rer", "un\u00b7ser", "Kind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu niedern L\u00fcsten reizet;", "tokens": ["Zu", "nie\u00b7dern", "L\u00fcs\u00b7ten", "rei\u00b7zet", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo nur allein nach Sittsamkeit", "tokens": ["Wo", "nur", "al\u00b7lein", "nach", "Sitt\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und Unschuld und Bescheidenheit", "tokens": ["Und", "Un\u00b7schuld", "und", "Be\u00b7schei\u00b7den\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das zarte M\u00e4dchen geizet.", "tokens": ["Das", "zar\u00b7te", "M\u00e4d\u00b7chen", "gei\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wo man das graue Alter ehrt,", "tokens": ["Wo", "man", "das", "grau\u00b7e", "Al\u00b7ter", "ehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf M\u00e4nnerrat und Warnung h\u00f6rt,", "tokens": ["Auf", "M\u00e4n\u00b7ner\u00b7rat", "und", "War\u00b7nung", "h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und offenherzig handelt;", "tokens": ["Und", "of\u00b7fen\u00b7her\u00b7zig", "han\u00b7delt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf guten, strengen Sitten h\u00e4lt,", "tokens": ["Auf", "gu\u00b7ten", ",", "stren\u00b7gen", "Sit\u00b7ten", "h\u00e4lt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und nicht die Einfalt erster Welt", "tokens": ["Und", "nicht", "die", "Ein\u00b7falt", "ers\u00b7ter", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In neuern Prunk verwandelt.", "tokens": ["In", "neu\u00b7ern", "Prunk", "ver\u00b7wan\u00b7delt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "O welch ein Bild! O Vaterland!", "tokens": ["O", "welch", "ein", "Bild", "!", "O", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PIAT", "ART", "NN", "$.", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich seh', das Auge weggewandt,", "tokens": ["Ich", "seh'", ",", "das", "Au\u00b7ge", "weg\u00b7ge\u00b7wandt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf ewig dich nicht wieder;", "tokens": ["Auf", "e\u00b7wig", "dich", "nicht", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und flieh', und bete noch f\u00fcr dich.", "tokens": ["Und", "flieh'", ",", "und", "be\u00b7te", "noch", "f\u00fcr", "dich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KON", "VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O, senkten meine W\u00fcnsche sich", "tokens": ["O", ",", "senk\u00b7ten", "mei\u00b7ne", "W\u00fcn\u00b7sche", "sich"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "PPOSAT", "NN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Doch bald auf dich hernieder!", "tokens": ["Doch", "bald", "auf", "dich", "her\u00b7nie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}