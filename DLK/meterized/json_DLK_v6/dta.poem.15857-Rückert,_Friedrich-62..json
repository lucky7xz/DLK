{"dta.poem.15857": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "62.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Halt' aufrecht, lieber Sohn, den Wuchs und deinen Geist,", "tokens": ["Halt'", "auf\u00b7recht", ",", "lie\u00b7ber", "Sohn", ",", "den", "Wuchs", "und", "dei\u00b7nen", "Geist", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "ADV", "NN", "$,", "ART", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df du von gradem Sinn und graden Gliedern seist!", "tokens": ["Da\u00df", "du", "von", "gra\u00b7dem", "Sinn", "und", "gra\u00b7den", "Glie\u00b7dern", "seist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "KON", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die falsche Demut senkt, die T\u00fccke senkt ihr Haupt;", "tokens": ["Die", "fal\u00b7sche", "De\u00b7mut", "senkt", ",", "die", "T\u00fc\u00b7cke", "senkt", "ihr", "Haupt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem freien Muth hat Gott empor zu schaun erlaubt.", "tokens": ["Dem", "frei\u00b7en", "Muth", "hat", "Gott", "em\u00b7por", "zu", "schaun", "er\u00b7laubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "NN", "PTKVZ", "PTKZU", "VVINF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Bedenke, wessen Sohn du bist, richt' auf im Adel", "tokens": ["Be\u00b7den\u00b7ke", ",", "wes\u00b7sen", "Sohn", "du", "bist", ",", "richt'", "auf", "im", "A\u00b7del"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "ADJA", "NN", "PPER", "VAFIN", "$,", "VVFIN", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Selbstgef\u00fchles dich, und f\u00fcrchte keinen Tadel.", "tokens": ["Des", "Selbst\u00b7ge\u00b7f\u00fch\u00b7les", "dich", ",", "und", "f\u00fcrch\u00b7te", "kei\u00b7nen", "Ta\u00b7del", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "$,", "KON", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Den Tadel hast du nur zu f\u00fcrchten, wenn du weichst", "tokens": ["Den", "Ta\u00b7del", "hast", "du", "nur", "zu", "f\u00fcrch\u00b7ten", ",", "wenn", "du", "weichst"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem Vater einst am Werth, dem du am Bilde gleichst.", "tokens": ["Dem", "Va\u00b7ter", "einst", "am", "Werth", ",", "dem", "du", "am", "Bil\u00b7de", "gleichst", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}