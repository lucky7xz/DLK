{"textgrid.poem.51222": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie gl\u00e4nzt der helle Mond so kalt und fern,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie gl\u00e4nzt der helle Mond so kalt und fern,", "tokens": ["Wie", "gl\u00e4nzt", "der", "hel\u00b7le", "Mond", "so", "kalt", "und", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Doch ferner schimmert meiner Sch\u00f6nheit Stern!", "tokens": ["Doch", "fer\u00b7ner", "schim\u00b7mert", "mei\u00b7ner", "Sch\u00f6n\u00b7heit", "Stern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wohl rauschet weit von mir des Meeres Strand,", "tokens": ["Wohl", "rau\u00b7schet", "weit", "von", "mir", "des", "Mee\u00b7res", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "APPR", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Doch weiter hin liegt meiner Jugend Land!", "tokens": ["Doch", "wei\u00b7ter", "hin", "liegt", "mei\u00b7ner", "Ju\u00b7gend", "Land", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ohn Rad und Deichsel gibt's ein W\u00e4gelein,", "tokens": ["Ohn", "Rad", "und", "Deich\u00b7sel", "gibt's", "ein", "W\u00e4\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Drin fahr ich bald zum Paradies hinein.", "tokens": ["Drin", "fahr", "ich", "bald", "zum", "Pa\u00b7ra\u00b7dies", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dort sitzt die Mutter Gottes auf dem Thron,", "tokens": ["Dort", "sitzt", "die", "Mut\u00b7ter", "Got\u00b7tes", "auf", "dem", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf ihren Knien schl\u00e4ft ihr sel'ger Sohn.", "tokens": ["Auf", "ih\u00b7ren", "Kni\u00b7en", "schl\u00e4ft", "ihr", "sel'\u00b7ger", "Sohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Dort sitzt Gott Vater, der den Heil'gen Geist", "tokens": ["Dort", "sitzt", "Gott", "Va\u00b7ter", ",", "der", "den", "Heil'\u00b7gen", "Geist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "NN", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Aus seiner Hand mit Himmelsk\u00f6rnern speist.", "tokens": ["Aus", "sei\u00b7ner", "Hand", "mit", "Him\u00b7mels\u00b7k\u00f6r\u00b7nern", "speist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "In einem Silberschleier sitz ich dann", "tokens": ["In", "ei\u00b7nem", "Sil\u00b7ber\u00b7schlei\u00b7er", "sitz", "ich", "dann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVIMP", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und schaue meine wei\u00dfen Finger an.", "tokens": ["Und", "schau\u00b7e", "mei\u00b7ne", "wei\u00b7\u00dfen", "Fin\u00b7ger", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Sankt Petrus aber g\u00f6nnt sich keine Ruh,", "tokens": ["Sankt", "Pet\u00b7rus", "a\u00b7ber", "g\u00f6nnt", "sich", "kei\u00b7ne", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "FM", "ADV", "VVFIN", "PRF", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hockt vor der T\u00fcr und flickt die alten Schuh'.", "tokens": ["Hockt", "vor", "der", "T\u00fcr", "und", "flickt", "die", "al\u00b7ten", "Schuh'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}