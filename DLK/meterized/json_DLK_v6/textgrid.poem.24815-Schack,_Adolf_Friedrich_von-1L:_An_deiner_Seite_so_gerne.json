{"textgrid.poem.24815": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: An deiner Seite so gerne", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An deiner Seite so gerne", "tokens": ["An", "dei\u00b7ner", "Sei\u00b7te", "so", "ger\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ADV"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Durchtr\u00e4um' ich die Fr\u00fchlingsnacht;", "tokens": ["Durch\u00b7tr\u00e4um'", "ich", "die", "Fr\u00fch\u00b7lings\u00b7nacht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Treu halten die heiligen Sterne", "tokens": ["Treu", "hal\u00b7ten", "die", "hei\u00b7li\u00b7gen", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Vor deinem Fenster die Wacht,", "tokens": ["Vor", "dei\u00b7nem", "Fens\u00b7ter", "die", "Wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Indes wir in Armen uns hangen,", "tokens": ["In\u00b7des", "wir", "in", "Ar\u00b7men", "uns", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "PPER", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "In Seele die Seele versinkt", "tokens": ["In", "See\u00b7le", "die", "See\u00b7le", "ver\u00b7sinkt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN", "VVFIN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Und Mund von Mund in langen", "tokens": ["Und", "Mund", "von", "Mund", "in", "lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "NN", "APPR", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Z\u00fcgen den Atem trinkt.", "tokens": ["Z\u00fc\u00b7gen", "den", "A\u00b7tem", "trinkt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Aus Wipfeln, drin V\u00f6gel br\u00fcten,", "tokens": ["Aus", "Wip\u00b7feln", ",", "drin", "V\u00f6\u00b7gel", "br\u00fc\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADV", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wirft sanft der duftende Mai", "tokens": ["Wirft", "sanft", "der", "duf\u00b7ten\u00b7de", "Mai"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Seine Knospen und Bl\u00fcten", "tokens": ["Sei\u00b7ne", "Knos\u00b7pen", "und", "Bl\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Herab auf uns selige zwei,", "tokens": ["Her\u00b7ab", "auf", "uns", "se\u00b7li\u00b7ge", "zwei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "ADJA", "CARD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Und durch die Fensterbogen", "tokens": ["Und", "durch", "die", "Fens\u00b7ter\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nachtwandelnd weht der Wind", "tokens": ["Nacht\u00b7wan\u00b7delnd", "weht", "der", "Wind"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "ART", "NN"], "meter": "++-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Deine Locken in Wogen", "tokens": ["Dei\u00b7ne", "Lo\u00b7cken", "in", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Ueber mein Haupt gelind.", "tokens": ["Ue\u00b7ber", "mein", "Haupt", "ge\u00b7lind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Wir zittern, wir erblassen", "tokens": ["Wir", "zit\u00b7tern", ",", "wir", "er\u00b7blas\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vor Liebe, und jedem quillt", "tokens": ["Vor", "Lie\u00b7be", ",", "und", "je\u00b7dem", "quillt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "KON", "PIS", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Im wonnethr\u00e4nennassen", "tokens": ["Im", "won\u00b7net\u00b7hr\u00e4\u00b7nen\u00b7nas\u00b7sen"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auge des andern Bild.", "tokens": ["Au\u00b7ge", "des", "an\u00b7dern", "Bild", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Ach! steigt schon im Osten der rote", "tokens": ["Ach", "!", "steigt", "schon", "im", "Os\u00b7ten", "der", "ro\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "VVFIN", "ADV", "APPRART", "NN", "ART", "ADJA"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Schimmer des Morgens empor?", "tokens": ["Schim\u00b7mer", "des", "Mor\u00b7gens", "em\u00b7por", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADV", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "Nein, durch den Himmel lohte", "tokens": ["Nein", ",", "durch", "den", "Him\u00b7mel", "loh\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ein n\u00e4chtliches Meteor.", "tokens": ["Ein", "n\u00e4cht\u00b7li\u00b7ches", "Me\u00b7te\u00b7or", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Tausend Geheimnisse m\u00fcssen", "tokens": ["Tau\u00b7send", "Ge\u00b7heim\u00b7nis\u00b7se", "m\u00fcs\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["CARD", "NN", "VMFIN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Wir noch einander vertraun,", "tokens": ["Wir", "noch", "ein\u00b7an\u00b7der", "ver\u00b7traun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und tausend K\u00fcsse noch k\u00fcssen,", "tokens": ["Und", "tau\u00b7send", "K\u00fcs\u00b7se", "noch", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Eh' der Morgen beginnt zu graun.", "tokens": ["Eh'", "der", "Mor\u00b7gen", "be\u00b7ginnt", "zu", "graun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Was scheuchst du mit deinem Gesange,", "tokens": ["Was", "scheuchst", "du", "mit", "dei\u00b7nem", "Ge\u00b7san\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "O Schwalbe, so fr\u00fche die Nacht?", "tokens": ["O", "Schwal\u00b7be", ",", "so", "fr\u00fc\u00b7he", "die", "Nacht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADV", "ADJA", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Schweig, schweig! Und haltet noch lange,", "tokens": ["Schweig", ",", "schweig", "!", "Und", "hal\u00b7tet", "noch", "lan\u00b7ge", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$.", "KON", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Ihr heiligen Sterne, die Wacht!", "tokens": ["Ihr", "hei\u00b7li\u00b7gen", "Ster\u00b7ne", ",", "die", "Wacht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}