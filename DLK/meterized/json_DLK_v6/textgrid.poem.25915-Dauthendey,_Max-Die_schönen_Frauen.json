{"textgrid.poem.25915": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Die sch\u00f6nen Frauen", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie sind so sch\u00f6n, die sch\u00f6nen Frauen,", "tokens": ["Sie", "sind", "so", "sch\u00f6n", ",", "die", "sch\u00f6\u00b7nen", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn die Augen zitternd schauen", "tokens": ["Wenn", "die", "Au\u00b7gen", "zit\u00b7ternd", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Sehnsucht gleitende Schl\u00f6sser bauen.", "tokens": ["Und", "der", "Sehn\u00b7sucht", "glei\u00b7ten\u00b7de", "Schl\u00f6s\u00b7ser", "bau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Doch nie sind sie sch\u00f6ner, die sch\u00f6nen Frauen,", "tokens": ["Doch", "nie", "sind", "sie", "sch\u00f6\u00b7ner", ",", "die", "sch\u00f6\u00b7nen", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADJD", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Als wenn die Augen sich schlie\u00dfen m\u00fcssen,", "tokens": ["Als", "wenn", "die", "Au\u00b7gen", "sich", "schlie\u00b7\u00dfen", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "NN", "PRF", "VVINF", "VMINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und die dunkeln Wangen zeigen:", "tokens": ["Und", "die", "dun\u00b7keln", "Wan\u00b7gen", "zei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seht, mich hielt der Geliebte in K\u00fcssen,", "tokens": ["Seht", ",", "mich", "hielt", "der", "Ge\u00b7lieb\u00b7te", "in", "K\u00fcs\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Und sein Blut ist mein eigen.", "tokens": ["Und", "sein", "Blut", "ist", "mein", "ei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}}}}