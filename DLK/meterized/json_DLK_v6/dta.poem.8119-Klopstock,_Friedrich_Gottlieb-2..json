{"dta.poem.8119": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1773", "urn": "urn:nbn:de:kobv:b4-200905199504", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Seldst\u00e4ndiger! Hochheiliger! Allseliger! tief wirft, Gott!", "tokens": ["Seld\u00b7st\u00e4n\u00b7di\u00b7ger", "!", "Hoch\u00b7hei\u00b7li\u00b7ger", "!", "All\u00b7se\u00b7li\u00b7ger", "!", "tief", "wirft", ",", "Gott", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$.", "NN", "$.", "NN", "$.", "ADJD", "VVFIN", "$,", "NN", "$."], "meter": "-+--++--+---+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Von dem Thron fern, wo erh\u00f6ht Du der Gestirn\u2019 Heer schufst,", "tokens": ["Von", "dem", "Thron", "fern", ",", "wo", "er\u00b7h\u00f6ht", "Du", "der", "Ge\u00b7stirn'", "Heer", "schufst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,", "PWAV", "VVFIN", "PPER", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+++-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Sich ein Staub dankend hin, und erstaunt \u00fcber sein Heil,", "tokens": ["Sich", "ein", "Staub", "dan\u00b7kend", "hin", ",", "und", "er\u00b7staunt", "\u00fc\u00b7ber", "sein", "Heil", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "ADJD", "PTKVZ", "$,", "KON", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "--+--+--++--+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Da\u00df ihn Gott h\u00f6rt in des Gebeinthals Nacht.", "tokens": ["Da\u00df", "ihn", "Gott", "h\u00f6rt", "in", "des", "Ge\u00b7beint\u00b7hals", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}