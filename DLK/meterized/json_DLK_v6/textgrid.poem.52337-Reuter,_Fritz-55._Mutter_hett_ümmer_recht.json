{"textgrid.poem.52337": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "55. Mutter hett \u00fcmmer recht", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "As Anno achtunvirtig jedermann", "tokens": ["As", "An\u00b7no", "acht\u00b7un\u00b7vir\u00b7tig", "je\u00b7der\u00b7mann"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NN", "ADJD", "PIS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So'n beten ut en H\u00fcschen kamen was,", "tokens": ["So'n", "be\u00b7ten", "ut", "en", "H\u00fc\u00b7schen", "ka\u00b7men", "was", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "FM", "FM", "NN", "VVFIN", "PIS", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Dunn was ok unsen Eddelmann", "tokens": ["Dunn", "was", "ok", "un\u00b7sen", "Ed\u00b7del\u00b7mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Di de Geschicht nich recht tau Pa\u00df:", "tokens": ["Di", "de", "Ge\u00b7schicht", "nich", "recht", "tau", "Pa\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "PTKNEG", "ADV", "CARD", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Hei smet sick \u00e4wer Kopp verlangst", "tokens": ["Hei", "smet", "sick", "\u00e4\u00b7wer", "Kopp", "ver\u00b7langst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "FM", "NN", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Ganz in de Poletik herin", "tokens": ["Ganz", "in", "de", "Po\u00b7le\u00b7tik", "he\u00b7rin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NE", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Un kreg dat glik mit so 'ne Angst,", "tokens": ["Un", "kreg", "dat", "glik", "mit", "so", "'ne", "Angst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "NN", "APPR", "ADV", "ART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "As wenn em dat an't Lewen g\u00fcng,", "tokens": ["As", "wenn", "em", "dat", "an't", "Le\u00b7wen", "g\u00fcng", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.9": {"text": "As wenn wi em all k\u00f6ppen wullen;", "tokens": ["As", "wenn", "wi", "em", "all", "k\u00f6p\u00b7pen", "wul\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "XY", "XY", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "Un dorbi satt hei stiw vull Schullen.", "tokens": ["Un", "dor\u00b7bi", "satt", "hei", "stiw", "vull", "Schul\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "NN", "$."], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.11": {"text": "Na, as hei gl\u00f6wen ded, de Sak, de g\u00fcng verdwas,", "tokens": ["Na", ",", "as", "hei", "gl\u00f6\u00b7wen", "ded", ",", "de", "Sak", ",", "de", "g\u00fcng", "verd\u00b7was", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "FM", "FM", "FM", "FM", "$,", "NE", "NE", "$,", "FM", "FM", "FM", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.12": {"text": "Un as em 'n beten huddlig was,", "tokens": ["Un", "as", "em", "'n", "be\u00b7ten", "hudd\u00b7lig", "was", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "VVFIN", "ADJD", "PWS", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Dunn schickt hei uns en grotes Schriwen:", "tokens": ["Dunn", "schickt", "hei", "uns", "en", "gro\u00b7tes", "Schri\u00b7wen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "FM", "FM", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Wi s\u00fcll'n sin trugen Buren bliwen,", "tokens": ["Wi", "s\u00fcll'n", "sin", "tru\u00b7gen", "Bu\u00b7ren", "bli\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.15": {"text": "Mit em in eine Karw rin hau'n;", "tokens": ["Mit", "em", "in", "ei\u00b7ne", "Karw", "rin", "hau'n", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "APPR", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Hei wull uns ok wer weit wat dauhn;", "tokens": ["Hei", "wull", "uns", "ok", "wer", "weit", "wat", "dauhn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "ADV", "PWS", "ADJD", "ADJD", "VVINF", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.17": {"text": "Un s\u00fcll'n uns nich uphitzen laten,", "tokens": ["Un", "s\u00fcll'n", "uns", "nich", "up\u00b7hit\u00b7zen", "la\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKNEG", "VVINF", "VVFIN", "$,"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.18": {"text": "V\u00f6r allen nich von Juden und Avkaten,", "tokens": ["V\u00f6r", "al\u00b7len", "nich", "von", "Ju\u00b7den", "und", "Av\u00b7ka\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "PTKNEG", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "De hadden all so vel verf\u00fchrt,", "tokens": ["De", "had\u00b7den", "all", "so", "vel", "ver\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PIAT", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Sei wiren nich dat Pulver wirt.", "tokens": ["Sei", "wi\u00b7ren", "nich", "dat", "Pul\u00b7ver", "wirt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "VAFIN", "PTKNEG", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "De Breiw was an min'n Nahwer Br\u00fcmmer,", "tokens": ["De", "Breiw", "was", "an", "min'n", "Nah\u00b7wer", "Br\u00fcm\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PRELS", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "So heit dormals de Schult mit Namen.", "tokens": ["So", "heit", "dor\u00b7mals", "de", "Schult", "mit", "Na\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "NE", "NN", "APPR", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.23": {"text": "De Schult schickt nu den Kn\u00fcppel r\u00fcmmer", "tokens": ["De", "Schult", "schickt", "nu", "den", "Kn\u00fcp\u00b7pel", "r\u00fcm\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "VVFIN", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Un lett dat D\u00f6rp tausamen kamen.", "tokens": ["Un", "lett", "dat", "D\u00f6rp", "tau\u00b7sa\u00b7men", "ka\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "NN", "VVINF", "VVFIN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.25": {"text": "Na, ick g\u00fcng hen. Tausam was all de Klunker", "tokens": ["Na", ",", "ick", "g\u00fcng", "hen", ".", "Tau\u00b7sam", "was", "all", "de", "Klun\u00b7ker"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "ADJD", "VVINF", "$.", "NN", "PRELS", "PIAT", "NE", "NE"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.26": {"text": "Von all de Bur'n. Doch Gott erbarm!", "tokens": ["Von", "all", "de", "Bur'", "n.", "Doch", "Gott", "er\u00b7barm", "!"], "token_info": ["word", "word", "word", "word", "abbreviation", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NE", "NE", "NE", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Wat hadden s' f\u00f6r en D\u00fcwelslarm!", "tokens": ["Wat", "had\u00b7den", "s'", "f\u00f6r", "en", "D\u00fc\u00b7wels\u00b7larm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.28": {"text": "\u00bbwat will de Kirl, wat will de Junker?", "tokens": ["\u00bb", "wat", "will", "de", "Kirl", ",", "wat", "will", "de", "Jun\u00b7ker", "?"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VMFIN", "NE", "NE", "$,", "PWS", "VMFIN", "NE", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Wat? Hei will lang' uns noch befehlen?", "tokens": ["Wat", "?", "Hei", "will", "lang'", "uns", "noch", "be\u00b7feh\u00b7len", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "VMFIN", "ADV", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Dat St\u00fcck, dat sall nich l\u00e4nger spelen!", "tokens": ["Dat", "St\u00fcck", ",", "dat", "sall", "nich", "l\u00e4n\u00b7ger", "spe\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "De Ritterschaft, de m\u00f6t dat liden!", "tokens": ["De", "Rit\u00b7ter\u00b7schaft", ",", "de", "m\u00f6t", "dat", "li\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NE", "NE", "ART", "NN", "$."], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.32": {"text": "Wi hewwen nu ganz ann're Tiden.", "tokens": ["Wi", "hew\u00b7wen", "nu", "ganz", "ann'\u00b7re", "Ti\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Wat? Hei will uns noch kaschelieren", "tokens": ["Wat", "?", "Hei", "will", "uns", "noch", "ka\u00b7sche\u00b7lie\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "NN", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Un is doch s\u00fclwst so'n groten Snurrer?", "tokens": ["Un", "is", "doch", "s\u00fclwst", "so'n", "gro\u00b7ten", "Snur\u00b7rer", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "ADV", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "F\u00f6r all sin sch\u00f6nen glatten W\u00fcrden", "tokens": ["F\u00f6r", "all", "sin", "sch\u00f6\u00b7nen", "glat\u00b7ten", "W\u00fcr\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "PIAT", "NN", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Geww ick nich desen Pipenpurrer!\u00ab", "tokens": ["Geww", "ick", "nich", "de\u00b7sen", "Pi\u00b7pen\u00b7pur\u00b7rer", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.37": {"text": "Un so gung't \u00fcmmer d\u00fcller furt,", "tokens": ["Un", "so", "gung't", "\u00fcm\u00b7mer", "d\u00fcl\u00b7ler", "furt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJD", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.38": {"text": "Kein Minsch verstunn sin eigen Wurd.", "tokens": ["Kein", "Minsch", "ver\u00b7stunn", "sin", "ei\u00b7gen", "Wurd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "VAFIN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Na, as sei nu so r\u00fcmmer streden", "tokens": ["Na", ",", "as", "sei", "nu", "so", "r\u00fcm\u00b7mer", "stre\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "NE", "VAFIN", "ADV", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Un up den Eddelmann so r\u00fcmmer reden", "tokens": ["Un", "up", "den", "Ed\u00b7del\u00b7mann", "so", "r\u00fcm\u00b7mer", "re\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "ART", "NN", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Un doch nich kemen tau en richt'gen Zweck,", "tokens": ["Un", "doch", "nich", "ke\u00b7men", "tau", "en", "richt'\u00b7gen", "Zweck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PTKNEG", "VVFIN", "FM", "FM", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.42": {"text": "Dunn krop uns' Schultenmutter ut de Eck", "tokens": ["Dunn", "krop", "un\u00b7s'", "Schul\u00b7ten\u00b7mut\u00b7ter", "ut", "de", "Eck"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.43": {"text": "Bi'n warmen Aben rut un s\u00e4d:", "tokens": ["Bi'n", "war\u00b7men", "A\u00b7ben", "rut", "un", "s\u00e4d", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "FM", "FM", "FM", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.44": {"text": "\u00bbnu holl't dat Mul, wenn ick hir red!", "tokens": ["\u00bb", "nu", "holl't", "dat", "Mul", ",", "wenn", "ick", "hir", "red", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Dat is nu so, so as dat is.", "tokens": ["Dat", "is", "nu", "so", ",", "so", "as", "dat", "is", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "ADV", "ADV", "$,", "ADV", "FM", "FM", "FM", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Dat ein is \u00e4wer ganz gewi\u00df:", "tokens": ["Dat", "ein", "is", "\u00e4\u00b7wer", "ganz", "ge\u00b7wi\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "FM", "ADJD", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Hei seggt, ji s\u00e4len jug von Juden un Avkaten", "tokens": ["Hei", "seggt", ",", "ji", "s\u00e4\u00b7len", "jug", "von", "Ju\u00b7den", "un", "Av\u00b7ka\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Nich in de Fingern krigen laten,", "tokens": ["Nich", "in", "de", "Fin\u00b7gern", "kri\u00b7gen", "la\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NE", "NN", "VVINF", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.49": {"text": "Un dat m\u00f6t ick verst\u00e4nnig heiten,", "tokens": ["Un", "dat", "m\u00f6t", "ick", "ver\u00b7st\u00e4n\u00b7nig", "hei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.50": {"text": "Un dorin hett hei seker recht:", "tokens": ["Un", "do\u00b7rin", "hett", "hei", "se\u00b7ker", "recht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.51": {"text": "Dat m\u00f6t hei s\u00fclwst am besten weiten,", "tokens": ["Dat", "m\u00f6t", "hei", "s\u00fclwst", "am", "bes\u00b7ten", "wei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "PTKA", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.52": {"text": "De heww'n em in sin Schullen br\u00f6cht.\u00ab", "tokens": ["De", "hew\u00b7w'n", "em", "in", "sin", "Schul\u00b7len", "br\u00f6cht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}