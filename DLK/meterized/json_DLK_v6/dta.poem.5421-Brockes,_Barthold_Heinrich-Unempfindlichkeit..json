{"dta.poem.5421": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Unempfindlichkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da wir von GOtt, in dieser Welt, unzehlich Gutes", "tokens": ["Da", "wir", "von", "Gott", ",", "in", "die\u00b7ser", "Welt", ",", "un\u00b7zeh\u00b7lich", "Gu\u00b7tes"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "$,", "APPR", "PDAT", "NN", "$,", "ADJD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u00fcberkommen,", "tokens": ["\u00fc\u00b7ber\u00b7kom\u00b7men", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Und, zum Besitz so vieler G\u00fcter, der k\u00fcnstlichen f\u00fcnf Sinnen", "tokens": ["Und", ",", "zum", "Be\u00b7sitz", "so", "vie\u00b7ler", "G\u00fc\u00b7ter", ",", "der", "k\u00fcnst\u00b7li\u00b7chen", "f\u00fcnf", "Sin\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "APPRART", "NN", "ADV", "PIAT", "NN", "$,", "ART", "ADJA", "CARD", "NN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Th\u00fcren;", "tokens": ["Th\u00fc\u00b7ren", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Wie k\u00f6mmt es denn, da\u00df wir dadurch nicht tausendfache", "tokens": ["Wie", "k\u00f6mmt", "es", "denn", ",", "da\u00df", "wir", "da\u00b7durch", "nicht", "tau\u00b7send\u00b7fa\u00b7che"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "PAV", "PTKNEG", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Lust versp\u00fchren?", "tokens": ["Lust", "ver\u00b7sp\u00fch\u00b7ren", "?"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Hat etwann unser Feind, der Teufel, uns die Empfindlich-", "tokens": ["Hat", "et\u00b7wann", "un\u00b7ser", "Feind", ",", "der", "Teu\u00b7fel", ",", "uns", "die", "Emp\u00b7find\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "$,", "ART", "NN", "$,", "PPER", "ART", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "keit genommen?", "tokens": ["keit", "ge\u00b7nom\u00b7men", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}