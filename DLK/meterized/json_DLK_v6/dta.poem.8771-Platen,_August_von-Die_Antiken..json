{"dta.poem.8771": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Die Antiken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht am dumpfigen Ort in Gew\u00f6lben zu wohnen ge-", "tokens": ["Nicht", "am", "dump\u00b7fi\u00b7gen", "Ort", "in", "Ge\u00b7w\u00f6l\u00b7ben", "zu", "woh\u00b7nen", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPRART", "ADJA", "NN", "APPR", "NN", "PTKZU", "VVINF", "TRUNC"], "meter": "--+--+--+--+-+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Denkt doch, was wir und wo wir gewesen, und schenket", "tokens": ["Denkt", "doch", ",", "was", "wir", "und", "wo", "wir", "ge\u00b7we\u00b7sen", ",", "und", "schen\u00b7ket"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "PRELS", "PPER", "KON", "PWAV", "PPER", "VAPP", "$,", "KON", "VVFIN"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Dies uralte Gef\u00e4\u00df war einst der egyptischen G\u00e4rten", "tokens": ["Dies", "ur\u00b7al\u00b7te", "Ge\u00b7f\u00e4\u00df", "war", "einst", "der", "e\u00b7gypt\u00b7i\u00b7schen", "G\u00e4r\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADJA", "NN", "VAFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+---+-+-+-+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Zier, und Cleopatra selbst lie\u00df f\u00fcllen mit Myrtenge-", "tokens": ["Zier", ",", "und", "Cleo\u00b7pat\u00b7ra", "selbst", "lie\u00df", "f\u00fcl\u00b7len", "mit", "Myr\u00b7ten\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KON", "NE", "ADV", "VVFIN", "VVFIN", "APPR", "TRUNC"], "meter": "+-+--+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Dieser geschnittene Stein, ein doppelgeschichteter Onyx,", "tokens": ["Die\u00b7ser", "ge\u00b7schnit\u00b7te\u00b7ne", "Stein", ",", "ein", "dop\u00b7pel\u00b7ge\u00b7schich\u00b7te\u00b7ter", "O\u00b7nyx", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+--+-+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "Zierte des jungen Antinous Hand, als k\u00f6stlichen Ring-", "tokens": ["Zier\u00b7te", "des", "jun\u00b7gen", "An\u00b7ti\u00b7nous", "Hand", ",", "als", "k\u00f6st\u00b7li\u00b7chen", "Ring"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "NN", "$,", "KOUS", "ADJA", "NN"], "meter": "+--+-+--+-+--+", "measure": "iambic.hexa.invert"}, "line.7": {"text": "Trug ihn der sch\u00f6ne, doch ach! zu fr\u00fche verg\u00f6tterte", "tokens": ["Trug", "ihn", "der", "sch\u00f6\u00b7ne", ",", "doch", "ach", "!", "zu", "fr\u00fc\u00b7he", "ver\u00b7g\u00f6t\u00b7ter\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "ADJA", "$,", "ADV", "ADV", "$.", "APPR", "ADJA", "NN"], "meter": "+--+--+-+--+-+", "measure": "dactylic.di.plus"}, "line.8": {"text": "Ich, als Hermes, stand in der Halle des C\u00e4sar Augustus,", "tokens": ["Ich", ",", "als", "Her\u00b7mes", ",", "stand", "in", "der", "Hal\u00b7le", "des", "C\u00e4\u00b7sar", "Au\u00b7gus\u00b7tus", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KOUS", "NE", "$,", "VVFIN", "APPR", "ART", "NN", "ART", "NE", "NE", "$,"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.9": {"text": "Wo mich ein Lorbeergew\u00e4chs mit s\u00fcdlichem Duft an-", "tokens": ["Wo", "mich", "ein", "Lor\u00b7beer\u00b7ge\u00b7w\u00e4chs", "mit", "s\u00fcd\u00b7li\u00b7chem", "Duft", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "TRUNC"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Und nun habt ihr uns hier aneinandergeh\u00e4uft und", "tokens": ["Und", "nun", "habt", "ihr", "uns", "hier", "an\u00b7ein\u00b7an\u00b7der\u00b7ge\u00b7h\u00e4uft", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "PPER", "ADV", "VVFIN", "KON"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.11": {"text": "Eines das andre verdr\u00e4ngend, und dies durch jenes", "tokens": ["Ei\u00b7nes", "das", "and\u00b7re", "ver\u00b7dr\u00e4n\u00b7gend", ",", "und", "dies", "durch", "je\u00b7nes"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "ART", "ADJA", "VVPP", "$,", "KON", "PDS", "APPR", "PDAT"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.12": {"text": "Keins am schicklichen Ort, in belebendem Schimmer der", "tokens": ["Keins", "am", "schick\u00b7li\u00b7chen", "Ort", ",", "in", "be\u00b7le\u00b7ben\u00b7dem", "Schim\u00b7mer", "der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "ART"], "meter": "+-+--+---+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.13": {"text": "Selbst das gelehrte Gesicht des begaffenden Kenners er-", "tokens": ["Selbst", "das", "ge\u00b7lehr\u00b7te", "Ge\u00b7sicht", "des", "be\u00b7ga\u00b7ffen\u00b7den", "Ken\u00b7ners", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "TRUNC"], "meter": "+--+--+---+-+--", "measure": "dactylic.tri.plus"}, "line.14": {"text": "Liegen geschichtet wir hier, gleich traurigen Knochen im", "tokens": ["Lie\u00b7gen", "ge\u00b7schich\u00b7tet", "wir", "hier", ",", "gleich", "trau\u00b7ri\u00b7gen", "Kno\u00b7chen", "im"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "PPER", "ADV", "$,", "ADV", "ADJA", "NN", "APPRART"], "meter": "+--+-+--+--+-+", "measure": "iambic.hexa.invert"}, "line.15": {"text": "Und in empf\u00e4nglicher Brust aufregen wir schmerzliche", "tokens": ["Und", "in", "emp\u00b7f\u00e4ng\u00b7li\u00b7cher", "Brust", "auf\u00b7re\u00b7gen", "wir", "schmerz\u00b7li\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "PPER", "VVFIN"], "meter": "-+-+--+-+--+--", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Nach den Tagen, in denen wir fast wie Lebendige prangten.", "tokens": ["Nach", "den", "Ta\u00b7gen", ",", "in", "de\u00b7nen", "wir", "fast", "wie", "Le\u00b7ben\u00b7di\u00b7ge", "prang\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "ADV", "KOKOM", "NN", "VVFIN", "$."], "meter": "--+--+--+-+-+-+-", "measure": "anapaest.tri.plus"}, "line.17": {"text": "Zieht nicht Rosen auch ihr, frischbl\u00fchende Flechte zu", "tokens": ["Zieht", "nicht", "Ro\u00b7sen", "auch", "ihr", ",", "frischbl\u00fc\u00b7hen\u00b7de", "Flech\u00b7te", "zu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "NN", "ADV", "PPER", "$,", "ADJA", "NN", "PTKZU"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.18": {"text": "Um den etrurischen Krug und die Scheitel der B\u00fcste", "tokens": ["Um", "den", "e\u00b7tru\u00b7ri\u00b7schen", "Krug", "und", "die", "Schei\u00b7tel", "der", "B\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "ADJA", "NN", "KON", "ART", "NN", "ART", "NN"], "meter": "---+--+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Habt nicht Tempel auch ihr, und schattige Gartenarkaden,", "tokens": ["Habt", "nicht", "Tem\u00b7pel", "auch", "ihr", ",", "und", "schat\u00b7ti\u00b7ge", "Gar\u00b7ten\u00b7ar\u00b7ka\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "NN", "ADV", "PPER", "$,", "KON", "ADJA", "NN", "$,"], "meter": "--+-++-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "Da\u00df ihr uns dorthin pflanzt, in die N\u00e4he des ewigen", "tokens": ["Da\u00df", "ihr", "uns", "dor\u00b7thin", "pflanzt", ",", "in", "die", "N\u00e4\u00b7he", "des", "e\u00b7wi\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVFIN", "$,", "APPR", "ART", "NN", "ART", "ADJA"], "meter": "--+--+--+--+--", "measure": "anapaest.tetra.plus"}, "line.21": {"text": "Jedem Beschauer zur Lust, uns selbst zur s\u00fc\u00dfen Ge-", "tokens": ["Je\u00b7dem", "Be\u00b7schau\u00b7er", "zur", "Lust", ",", "uns", "selbst", "zur", "s\u00fc\u00b7\u00dfen", "Ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPRART", "NN", "$,", "PPER", "ADV", "APPRART", "ADJA", "NN"], "meter": "+--+--+-+-+--", "measure": "dactylic.di.plus"}}}}}