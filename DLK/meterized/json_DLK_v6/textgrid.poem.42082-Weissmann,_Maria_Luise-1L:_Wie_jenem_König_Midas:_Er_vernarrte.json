{"textgrid.poem.42082": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie jenem K\u00f6nig Midas: Er vernarrte", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie jenem K\u00f6nig Midas: Er vernarrte", "tokens": ["Wie", "je\u00b7nem", "K\u00f6\u00b7nig", "Mi\u00b7das", ":", "Er", "ver\u00b7narr\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PDAT", "NN", "NE", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zuletzt dem Schein von Goldenem sich so,", "tokens": ["Zu\u00b7letzt", "dem", "Schein", "von", "Gol\u00b7de\u00b7nem", "sich", "so", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "NE", "PRF", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df was er griff zu gelbem Glanz erstarrte,", "tokens": ["Da\u00df", "was", "er", "griff", "zu", "gel\u00b7bem", "Glanz", "er\u00b7starr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWS", "PPER", "VVFIN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Speise und Trank; die Erde, drauf er floh", "tokens": ["Spei\u00b7se", "und", "Trank", ";", "die", "Er\u00b7de", ",", "drauf", "er", "floh"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$.", "ART", "NN", "$,", "PAV", "PPER", "VVFIN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Vor seinem Fluch, gl\u00e4nzte ihm unterm Schritt", "tokens": ["Vor", "sei\u00b7nem", "Fluch", ",", "gl\u00e4nz\u00b7te", "ihm", "un\u00b7term", "Schritt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Vergoldet auf. So wandelt sich in Glut", "tokens": ["Ver\u00b7gol\u00b7det", "auf", ".", "So", "wan\u00b7delt", "sich", "in", "Glut"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "$.", "ADV", "VVFIN", "PRF", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was mich ber\u00fchrt. So unentrinnbar tritt", "tokens": ["Was", "mich", "be\u00b7r\u00fchrt", ".", "So", "un\u00b7en\u00b7trinn\u00b7bar", "tritt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "PPER", "VVPP", "$.", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Geliebtes pl\u00f6tzlich aus der fremden Hut,", "tokens": ["Ge\u00b7lieb\u00b7tes", "pl\u00f6tz\u00b7lich", "aus", "der", "frem\u00b7den", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So unausweichlich nahst mir Du aus allen", "tokens": ["So", "un\u00b7aus\u00b7weich\u00b7lich", "nahst", "mir", "Du", "aus", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "PPER", "APPR", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Weiten des Himmels, das verh\u00e4ngte Ziel,", "tokens": ["Wei\u00b7ten", "des", "Him\u00b7mels", ",", "das", "ver\u00b7h\u00e4ng\u00b7te", "Ziel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Dem jeder Weg schon zu Beginn verfallen.", "tokens": ["Dem", "je\u00b7der", "Weg", "schon", "zu", "Be\u00b7ginn", "ver\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und auf dem abgewandten, fliehnden, leer", "tokens": ["Und", "auf", "dem", "ab\u00b7ge\u00b7wand\u00b7ten", ",", "fliehn\u00b7den", ",", "leer"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "$,", "VVFIN", "$,", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Geglaubten, sieh, in einem neuen Spiel", "tokens": ["Ge\u00b7glaub\u00b7ten", ",", "sieh", ",", "in", "ei\u00b7nem", "neu\u00b7en", "Spiel"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dir zu entgehen, fand ich Dich noch mehr.", "tokens": ["Dir", "zu", "ent\u00b7ge\u00b7hen", ",", "fand", "ich", "Dich", "noch", "mehr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "VVFIN", "PPER", "PRF", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}