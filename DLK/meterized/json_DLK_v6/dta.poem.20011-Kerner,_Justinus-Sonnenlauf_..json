{"dta.poem.20011": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Sonnenlauf .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Weh, o weh der b\u00f6sen Sonne! stellt mit liebelosem Stral", "tokens": ["Weh", ",", "o", "weh", "der", "b\u00f6\u00b7sen", "Son\u00b7ne", "!", "stellt", "mit", "lie\u00b7be\u00b7lo\u00b7sem", "Stral"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "FM", "ADV", "ART", "ADJA", "NN", "$.", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Zwischen mich und Sie, die Ferne, hohe Berg' und tiefe", "tokens": ["Zwi\u00b7schen", "mich", "und", "Sie", ",", "die", "Fer\u00b7ne", ",", "ho\u00b7he", "Ber\u00b7g'", "und", "tie\u00b7fe"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "KON", "PPER", "$,", "ART", "NN", "$,", "ADJA", "NN", "KON", "ADJA"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Thal',", "tokens": ["Thal'", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Bringet D\u00f6rfer, bringet St\u00e4dte, ziehet Fl\u00fcsse, leitet Seen,", "tokens": ["Brin\u00b7get", "D\u00f6r\u00b7fer", ",", "brin\u00b7get", "St\u00e4d\u00b7te", ",", "zie\u00b7het", "Fl\u00fcs\u00b7se", ",", "lei\u00b7tet", "Seen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "VVFIN", "NN", "$,", "VVFIN", "NN", "$,", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "L\u00e4\u00dft ein wild Gew\u00fchl von Menschen zwischen Ihr und mir", "tokens": ["L\u00e4\u00dft", "ein", "wild", "Ge\u00b7w\u00fchl", "von", "Men\u00b7schen", "zwi\u00b7schen", "Ihr", "und", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJD", "NN", "APPR", "NN", "APPR", "PPER", "KON", "PPER"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.6": {"text": "erstehn.", "tokens": ["er\u00b7stehn", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Und je n\u00e4her dann die Sonne leuchtend an dem Himmel", "tokens": ["Und", "je", "n\u00e4\u00b7her", "dann", "die", "Son\u00b7ne", "leuch\u00b7tend", "an", "dem", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "ADV", "ART", "NN", "VVPP", "APPR", "ART", "NN"], "meter": "--+-+-+-+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "zieht,", "tokens": ["zieht", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.9": {"text": "Weh! je ferner Sie, die Ferne, \u00fcber Berg' und Thale", "tokens": ["Weh", "!", "je", "fer\u00b7ner", "Sie", ",", "die", "Fer\u00b7ne", ",", "\u00fc\u00b7ber", "Ber\u00b7g'", "und", "Tha\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "ADV", "ADV", "PPER", "$,", "ART", "NN", "$,", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "flieht.", "tokens": ["flieht", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.11": {"text": "Aber wann die Sonne fliehet, mit sich ziehend Berg und", "tokens": ["A\u00b7ber", "wann", "die", "Son\u00b7ne", "flie\u00b7het", ",", "mit", "sich", "zie\u00b7hend", "Berg", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "$,", "APPR", "PRF", "VVPP", "NN", "KON"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.12": {"text": "Thal,", "tokens": ["Thal", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.13": {"text": "Mit sich ziehend Fl\u00fcss' und St\u00e4dte, und die Menschen all-", "tokens": ["Mit", "sich", "zie\u00b7hend", "Fl\u00fcss'", "und", "St\u00e4d\u00b7te", ",", "und", "die", "Men\u00b7schen", "all"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PRF", "VVPP", "NN", "KON", "NN", "$,", "KON", "ART", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.14": {"text": "zumal:", "tokens": ["zu\u00b7mal", ":"], "token_info": ["word", "punct"], "pos": ["KOUS", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "Kehret schon die Ferne wieder; leis vom Abendstern bewacht,", "tokens": ["Keh\u00b7ret", "schon", "die", "Fer\u00b7ne", "wie\u00b7der", ";", "leis", "vom", "A\u00b7bends\u00b7tern", "be\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADV", "$.", "ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+---+", "measure": "unknown.measure.septa"}, "line.16": {"text": "Schifft sie in dem Kahn des Mondes durch das stille Meer", "tokens": ["Schifft", "sie", "in", "dem", "Kahn", "des", "Mon\u00b7des", "durch", "das", "stil\u00b7le", "Meer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.17": {"text": "der Nacht.", "tokens": ["der", "Nacht", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+", "measure": "iambic.single"}}}}}