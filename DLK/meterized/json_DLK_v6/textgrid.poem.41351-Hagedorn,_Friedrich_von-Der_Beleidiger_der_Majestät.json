{"textgrid.poem.41351": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der Beleidiger der Majest\u00e4t", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein K\u00f6nig, dem an Macht und Weisheit keiner glich,", "tokens": ["Ein", "K\u00f6\u00b7nig", ",", "dem", "an", "Macht", "und", "Weis\u00b7heit", "kei\u00b7ner", "glich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "KON", "NN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erwies sich jederzeit im Herrschen v\u00e4terlich.", "tokens": ["Er\u00b7wies", "sich", "je\u00b7der\u00b7zeit", "im", "Herr\u00b7schen", "v\u00e4\u00b7ter\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sein Liebling, dessen Gl\u00fcck, so lang' er treulich diente,", "tokens": ["Sein", "Lieb\u00b7ling", ",", "des\u00b7sen", "Gl\u00fcck", ",", "so", "lang'", "er", "treu\u00b7lich", "dien\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELAT", "NN", "$,", "ADV", "ADV", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So, wie ein starker Baum an frischen Quellen, gr\u00fcnte,", "tokens": ["So", ",", "wie", "ein", "star\u00b7ker", "Baum", "an", "fri\u00b7schen", "Quel\u00b7len", ",", "gr\u00fcn\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Verscherzte seine Huld durch schn\u00f6den Hochverrath,", "tokens": ["Ver\u00b7scherz\u00b7te", "sei\u00b7ne", "Huld", "durch", "schn\u00f6\u00b7den", "Hoch\u00b7ver\u00b7rath", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ward Seiner Feinde Freund, verwirrte Seinen Staat,", "tokens": ["Ward", "Sei\u00b7ner", "Fein\u00b7de", "Freund", ",", "ver\u00b7wirr\u00b7te", "Sei\u00b7nen", "Staat", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und durfte durch Gewalt Gesetz und Recht vernichten,", "tokens": ["Und", "durf\u00b7te", "durch", "Ge\u00b7walt", "Ge\u00b7setz", "und", "Recht", "ver\u00b7nich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "NN", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mit Blut sich H\u00e4user baun, und um Geschenke richten.", "tokens": ["Mit", "Blut", "sich", "H\u00e4u\u00b7ser", "baun", ",", "und", "um", "Ge\u00b7schen\u00b7ke", "rich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "NN", "VVINF", "$,", "KON", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der g\u00fctige Monarch ermahnt' ihn mit Geduld,", "tokens": ["Der", "g\u00fc\u00b7ti\u00b7ge", "Mon\u00b7arch", "er\u00b7mahnt'", "ihn", "mit", "Ge\u00b7duld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und sprach: Undankbarer! verehre meine Huld,", "tokens": ["Und", "sprach", ":", "Un\u00b7dank\u00b7ba\u00b7rer", "!", "ver\u00b7eh\u00b7re", "mei\u00b7ne", "Huld", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "NN", "$.", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Huld, die deinen Stand mit reichem Segen schm\u00fccket,", "tokens": ["Die", "Huld", ",", "die", "dei\u00b7nen", "Stand", "mit", "rei\u00b7chem", "Se\u00b7gen", "schm\u00fc\u00b7cket", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So kr\u00e4ftig dich besch\u00fctzt, so unverdient begl\u00fccket;", "tokens": ["So", "kr\u00e4f\u00b7tig", "dich", "be\u00b7sch\u00fctzt", ",", "so", "un\u00b7ver\u00b7di\u00b7ent", "be\u00b7gl\u00fc\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VVPP", "$,", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Du sollst, der h\u00f6chsten Schmach und Strafe zu entgehn,", "tokens": ["Du", "sollst", ",", "der", "h\u00f6chs\u00b7ten", "Schmach", "und", "Stra\u00b7fe", "zu", "ent\u00b7gehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "ART", "ADJA", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was du ver\u00fcbet hast, mir insgeheim gestehn.", "tokens": ["Was", "du", "ver\u00b7\u00fc\u00b7bet", "hast", ",", "mir", "ins\u00b7ge\u00b7heim", "ge\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "VAFIN", "$,", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Erkenne deine Schuld, so wird sie dir vergeben:", "tokens": ["Er\u00b7ken\u00b7ne", "dei\u00b7ne", "Schuld", ",", "so", "wird", "sie", "dir", "ver\u00b7ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "ADV", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Das Leben schenk' ich dir, nur weihe mir dein Leben.", "tokens": ["Das", "Le\u00b7ben", "schenk'", "ich", "dir", ",", "nur", "wei\u00b7he", "mir", "dein", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPER", "$,", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Den Frevler, dessen Herz ein Herz voll T\u00fccke war,", "tokens": ["Den", "Frev\u00b7ler", ",", "des\u00b7sen", "Herz", "ein", "Herz", "voll", "T\u00fc\u00b7cke", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELAT", "NN", "ART", "NN", "ADJD", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erweicht' und schreckte nichts. Er lachte der Gefahr.", "tokens": ["Er\u00b7weicht'", "und", "schreck\u00b7te", "nichts", ".", "Er", "lach\u00b7te", "der", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PIS", "$.", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Drauf ward er, ein Gef\u00fchl der Reue zu erlangen,", "tokens": ["Drauf", "ward", "er", ",", "ein", "Ge\u00b7f\u00fchl", "der", "Reu\u00b7e", "zu", "er\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "$,", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Recht \u00fcber einer Kluft an Faden aufgehangen:", "tokens": ["Recht", "\u00fc\u00b7ber", "ei\u00b7ner", "Kluft", "an", "Fa\u00b7den", "auf\u00b7ge\u00b7han\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die schnitt man nach und nach, und immer einzeln, ab,", "tokens": ["Die", "schnitt", "man", "nach", "und", "nach", ",", "und", "im\u00b7mer", "ein\u00b7zeln", ",", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "APPR", "KON", "APPR", "$,", "KON", "ADV", "ADJD", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da ihm des Richters Gunst stets neue Fristen gab.", "tokens": ["Da", "ihm", "des", "Rich\u00b7ters", "Gunst", "stets", "neu\u00b7e", "Fris\u00b7ten", "gab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Man hoffte, doch umsonst, er w\u00fcrde sich noch fassen,", "tokens": ["Man", "hoff\u00b7te", ",", "doch", "um\u00b7sonst", ",", "er", "w\u00fcr\u00b7de", "sich", "noch", "fas\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "ADV", "ADV", "$,", "PPER", "VAFIN", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Selbst sein Erretter sein, und nicht sein Leben hassen.", "tokens": ["Selbst", "sein", "Er\u00b7ret\u00b7ter", "sein", ",", "und", "nicht", "sein", "Le\u00b7ben", "has\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAINF", "$,", "KON", "PTKNEG", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Er sah, und sah auch nicht die Gr\u00f6\u00dfe seiner Noth,", "tokens": ["Er", "sah", ",", "und", "sah", "auch", "nicht", "die", "Gr\u00f6\u00b7\u00dfe", "sei\u00b7ner", "Noth", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "ADV", "PTKNEG", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Folge blinder Wahl, den st\u00fcndlich n\u00e4hern Tod.", "tokens": ["Die", "Fol\u00b7ge", "blin\u00b7der", "Wahl", ",", "den", "st\u00fcnd\u00b7lich", "n\u00e4\u00b7hern", "Tod", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "PRELS", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kein Schrecken, keine Reu' erweckte sein Gewissen.", "tokens": ["Kein", "Schre\u00b7cken", ",", "kei\u00b7ne", "Reu'", "er\u00b7weck\u00b7te", "sein", "Ge\u00b7wis\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Thor verblich verstockt, bis alle Faden rissen,", "tokens": ["Der", "Thor", "ver\u00b7blich", "ver\u00b7stockt", ",", "bis", "al\u00b7le", "Fa\u00b7den", "ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$,", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und der Unselige fand seiner Bosheit Ziel,", "tokens": ["Und", "der", "Un\u00b7se\u00b7li\u00b7ge", "fand", "sei\u00b7ner", "Bos\u00b7heit", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPOSS", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Als er, beim letzten Schnitt, in Kluft und Abgrund fiel.", "tokens": ["Als", "er", ",", "beim", "letz\u00b7ten", "Schnitt", ",", "in", "Kluft", "und", "Ab\u00b7grund", "fiel", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPRART", "ADJA", "NN", "$,", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Der Herr, der Heilige, der Richter unsrer V\u00e4ter,", "tokens": ["Der", "Herr", ",", "der", "Hei\u00b7li\u00b7ge", ",", "der", "Rich\u00b7ter", "uns\u00b7rer", "V\u00e4\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist der Monarch voll Huld; der Mensch der Misseth\u00e4ter;", "tokens": ["Ist", "der", "Mon\u00b7arch", "voll", "Huld", ";", "der", "Mensch", "der", "Mis\u00b7set\u00b7h\u00e4\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "NN", "$.", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein Faden jedes Jahr, das Er zur Bu\u00dfe g\u00f6nnt;", "tokens": ["Ein", "Fa\u00b7den", "je\u00b7des", "Jahr", ",", "das", "Er", "zur", "Bu\u00b7\u00dfe", "g\u00f6nnt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Kluft der ew'ge Pful, der jeden Frevler brennt,", "tokens": ["Die", "Kluft", "der", "ew'\u00b7ge", "Pful", ",", "der", "je\u00b7den", "Frev\u00b7ler", "brennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der wider eignes Heil mit frecher Unart streitet,", "tokens": ["Der", "wi\u00b7der", "eig\u00b7nes", "Heil", "mit", "fre\u00b7cher", "Un\u00b7art", "strei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und den nicht Huld noch Ernst den Weg des Lebens leitet.", "tokens": ["Und", "den", "nicht", "Huld", "noch", "Ernst", "den", "Weg", "des", "Le\u00b7bens", "lei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PTKNEG", "NN", "ADV", "NE", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}