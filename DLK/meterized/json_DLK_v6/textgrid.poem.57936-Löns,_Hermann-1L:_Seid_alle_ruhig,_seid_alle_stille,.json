{"textgrid.poem.57936": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Seid alle ruhig, seid alle stille,", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.71", "et:0.28"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seid alle ruhig, seid alle stille,", "tokens": ["Seid", "al\u00b7le", "ru\u00b7hig", ",", "seid", "al\u00b7le", "stil\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "PIS", "ADJD", "$,", "VAFIN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Opposition hat keinen Zweck,", "tokens": ["Op\u00b7po\u00b7si\u00b7ti\u00b7on", "hat", "kei\u00b7nen", "Zweck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Der Magistrat, der stimmt ja doch blo\u00df", "tokens": ["Der", "Ma\u00b7gist\u00b7rat", ",", "der", "stimmt", "ja", "doch", "blo\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "ADV", "ADV", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Ratsch \u00fcber unsere K\u00f6pfe weg.", "tokens": ["Ratsch", "\u00fc\u00b7ber", "un\u00b7se\u00b7re", "K\u00f6p\u00b7fe", "weg", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Was hilft uns denn die eigne Meinung,", "tokens": ["Was", "hilft", "uns", "denn", "die", "eig\u00b7ne", "Mei\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wird sie von oben ignoriert;", "tokens": ["Wird", "sie", "von", "o\u00b7ben", "ig\u00b7no\u00b7riert", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Stimmt man uns \u00fcber, sind wir im Buddel,", "tokens": ["Stimmt", "man", "uns", "\u00fc\u00b7ber", ",", "sind", "wir", "im", "Bud\u00b7del", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PRF", "APPR", "$,", "VAFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Und obendreine noch blamiert.", "tokens": ["Und", "o\u00b7ben\u00b7drei\u00b7ne", "noch", "bla\u00b7miert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Was hilft ein Differenzbeschl\u00fc\u00dfchen,", "tokens": ["Was", "hilft", "ein", "Dif\u00b7fe\u00b7renz\u00b7be\u00b7schl\u00fc\u00df\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Magistrat tut, was er will;", "tokens": ["Der", "Ma\u00b7gist\u00b7rat", "tut", ",", "was", "er", "will", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PWS", "PPER", "VMFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Benickkoppt alles, was er vorschl\u00e4gt,", "tokens": ["Be\u00b7nick\u00b7koppt", "al\u00b7les", ",", "was", "er", "vor\u00b7schl\u00e4gt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und haltet euch nur sonst recht still.", "tokens": ["Und", "hal\u00b7tet", "euch", "nur", "sonst", "recht", "still", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Im Magistrat hat man die Bildung,", "tokens": ["Im", "Ma\u00b7gist\u00b7rat", "hat", "man", "die", "Bil\u00b7dung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PIS", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Dagegen an kann man nicht gut,", "tokens": ["Da\u00b7ge\u00b7gen", "an", "kann", "man", "nicht", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APZR", "VMFIN", "PIS", "PTKNEG", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Riskiert mal einer eine Lippe,", "tokens": ["Ris\u00b7kiert", "mal", "ei\u00b7ner", "ei\u00b7ne", "Lip\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bums, hat er gleich eins auf dem Hut.", "tokens": ["Bums", ",", "hat", "er", "gleich", "eins", "auf", "dem", "Hut", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VAFIN", "PPER", "ADV", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Drum schweiget stille und haltet die R\u00e4nder,", "tokens": ["Drum", "schwei\u00b7get", "stil\u00b7le", "und", "hal\u00b7tet", "die", "R\u00e4n\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADJA", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Bis da\u00df es wieder kommt zur Wahl,", "tokens": ["Bis", "da\u00df", "es", "wie\u00b7der", "kommt", "zur", "Wahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als Kandidat macht man nat\u00fcrlich", "tokens": ["Als", "Kan\u00b7di\u00b7dat", "macht", "man", "na\u00b7t\u00fcr\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "VVFIN", "PIS", "ADV"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Ein ganz klein wenig in Skandal.", "tokens": ["Ein", "ganz", "klein", "we\u00b7nig", "in", "Skan\u00b7dal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "ADV", "APPR", "NN", "$."], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}}, "stanza.6": {"line.1": {"text": "Das kostet nichts und macht Vergn\u00fcgen,", "tokens": ["Das", "kos\u00b7tet", "nichts", "und", "macht", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "KON", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch weiter hat das keinen Wert;", "tokens": ["Doch", "wei\u00b7ter", "hat", "das", "kei\u00b7nen", "Wert", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sitzt man erst wieder auf dem Rathaus,", "tokens": ["Sitzt", "man", "erst", "wie\u00b7der", "auf", "dem", "Rat\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dann hei\u00dft's doch wieder: \u00bbRechtsum kehrt!\u00ab", "tokens": ["Dann", "hei\u00dft's", "doch", "wie\u00b7der", ":", "\u00bb", "Recht\u00b7sum", "kehrt", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "$.", "$(", "ADV", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}