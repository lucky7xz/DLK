s": ["ADV", "VVFIN", "PIS", "ADV", "ADV", "PPER", "VVINF"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und die Wege deines Lichtes gehen.", "tokens": ["Und", "die", "We\u00b7ge", "dei\u00b7nes", "Lich\u00b7tes", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.24": {"line.1": {"text": "Vater, gib mir Kraft, wenn Pflicht mich fodert,", "tokens": ["Va\u00b7ter", ",", "gib", "mir", "Kraft", ",", "wenn", "Pflicht", "mich", "fo\u00b7dert", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPER", "NN", "$,", "KOUS", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Kraft, so gro\u00df wie Feuer in mir lodert,", "tokens": ["Kraft", ",", "so", "gro\u00df", "wie", "Feu\u00b7er", "in", "mir", "lo\u00b7dert", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADJD", "KOKOM", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Da\u00df ich ohne Furcht die Wahrheit sage,", "tokens": ["Da\u00df", "ich", "oh\u00b7ne", "Furcht", "die", "Wahr\u00b7heit", "sa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und f\u00fcr deine Wahrheit alles wage.", "tokens": ["Und", "f\u00fcr", "dei\u00b7ne", "Wahr\u00b7heit", "al\u00b7les", "wa\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.25": {"line.1": {"text": "Wenig hab' ich noch in meinem Leben", "tokens": ["We\u00b7nig", "hab'", "ich", "noch", "in", "mei\u00b7nem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "F\u00fcr die gute Sache hingegeben,", "tokens": ["F\u00fcr", "die", "gu\u00b7te", "Sa\u00b7che", "hin\u00b7ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Bin vielleicht an meinem Wanderstabe", "tokens": ["Bin", "viel\u00b7leicht", "an", "mei\u00b7nem", "Wan\u00b7der\u00b7sta\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Nur an Bart ein Mann, an Geist ein Knabe.", "tokens": ["Nur", "an", "Bart", "ein", "Mann", ",", "an", "Geist", "ein", "Kna\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ART", "NN", "$,", "APPR", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.26": {"line.1": {"text": "Durst nach Thaten brennt in meiner Seele,", "tokens": ["Durst", "nach", "Tha\u00b7ten", "brennt", "in", "mei\u00b7ner", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Thaten, die mein guter Engel z\u00e4hle:", "tokens": ["Tha\u00b7ten", ",", "die", "mein", "gu\u00b7ter", "En\u00b7gel", "z\u00e4h\u00b7le", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Werd' ein Held im Blut der Menschheit Ruthe;", "tokens": ["Werd'", "ein", "Held", "im", "Blut", "der", "Menschheit", "Ru\u00b7the", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPRART", "NN", "ART", "NN", "NE", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Wahre Gr\u00f6\u00dfe ist nur wahres Gute.", "tokens": ["Wah\u00b7re", "Gr\u00f6\u00b7\u00dfe", "ist", "nur", "wah\u00b7res", "Gu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.27": {"line.1": {"text": "Vater, hilf die Stunden mir gewinnen,", "tokens": ["Va\u00b7ter", ",", "hilf", "die", "Stun\u00b7den", "mir", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Bis der Urne letzte Tropfen rinnen;", "tokens": ["Bis", "der", "Ur\u00b7ne", "letz\u00b7te", "Trop\u00b7fen", "rin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Da\u00df ich dann in meines Lebens Buche", "tokens": ["Da\u00df", "ich", "dann", "in", "mei\u00b7nes", "Le\u00b7bens", "Bu\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Nicht vergebens meine Werke suche.", "tokens": ["Nicht", "ver\u00b7ge\u00b7bens", "mei\u00b7ne", "Wer\u00b7ke", "su\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}