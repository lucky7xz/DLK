{"textgrid.poem.52991": {"metadata": {"author": {"name": "Seume, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Drey\u00dfig Mahl ist mir das Jahr entronnen;", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Drey\u00dfig Mahl ist mir das Jahr entronnen;", "tokens": ["Drey\u00b7\u00dfig", "Mahl", "ist", "mir", "das", "Jahr", "ent\u00b7ron\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und was hab' ich aus dem Flug gewonnen?", "tokens": ["Und", "was", "hab'", "ich", "aus", "dem", "Flug", "ge\u00b7won\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wie ein Kahn durch St\u00fcrme, Fluth und Wogen,", "tokens": ["Wie", "ein", "Kahn", "durch", "St\u00fcr\u00b7me", ",", "Fluth", "und", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sind sie adlerschnell dahin geflogen.", "tokens": ["Sind", "sie", "ad\u00b7ler\u00b7schnell", "da\u00b7hin", "ge\u00b7flo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "PAV", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Aus dem Hinterhalt hat, wenn ich lachte", "tokens": ["Aus", "dem", "Hin\u00b7ter\u00b7halt", "hat", ",", "wenn", "ich", "lach\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VAFIN", "$,", "KOUS", "PPER", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und nur Frohgenu\u00df des Lebens dachte,", "tokens": ["Und", "nur", "Froh\u00b7ge\u00b7nu\u00df", "des", "Le\u00b7bens", "dach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Oft der Tod mir in den Mayentagen", "tokens": ["Oft", "der", "Tod", "mir", "in", "den", "Ma\u00b7yen\u00b7ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Zu der gro\u00dfen Reise L\u00e4rm geschlagen.", "tokens": ["Zu", "der", "gro\u00b7\u00dfen", "Rei\u00b7se", "L\u00e4rm", "ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Von des Meeres tiefem Felsengrunde,", "tokens": ["Von", "des", "Mee\u00b7res", "tie\u00b7fem", "Fel\u00b7sen\u00b7grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Aus der Kriegsmaschine Feuerschlunde", "tokens": ["Aus", "der", "Kriegs\u00b7ma\u00b7schi\u00b7ne", "Feu\u00b7er\u00b7schlun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "G\u00e4hnte von der Parze schwarzen Wegen", "tokens": ["G\u00e4hn\u00b7te", "von", "der", "Par\u00b7ze", "schwar\u00b7zen", "We\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Mir Verderben oft und grell entgegen.", "tokens": ["Mir", "Ver\u00b7der\u00b7ben", "oft", "und", "grell", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "KON", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Und ich sah durch die gebrochnen Glieder,", "tokens": ["Und", "ich", "sah", "durch", "die", "ge\u00b7broch\u00b7nen", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Hingestreckt vom W\u00fcrger, meine Br\u00fcder", "tokens": ["Hin\u00b7ge\u00b7streckt", "vom", "W\u00fcr\u00b7ger", ",", "mei\u00b7ne", "Br\u00fc\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "In der Sterbestunde letzten Z\u00fcgen", "tokens": ["In", "der", "Ster\u00b7be\u00b7stun\u00b7de", "letz\u00b7ten", "Z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Blutig r\u00f6chelnd, bethend, fluchend liegen.", "tokens": ["Blu\u00b7tig", "r\u00f6\u00b7chelnd", ",", "be\u00b7thend", ",", "flu\u00b7chend", "lie\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "VVPP", "$,", "VVPP", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Auf der alten und der neuen Erde,", "tokens": ["Auf", "der", "al\u00b7ten", "und", "der", "neu\u00b7en", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Von dem F\u00fcrstensaal zum Bettlerherde,", "tokens": ["Von", "dem", "F\u00fcrs\u00b7ten\u00b7saal", "zum", "Bett\u00b7ler\u00b7her\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "H\u00f6rt' ich Menschen \u00fcber Menschenplagen", "tokens": ["H\u00f6rt'", "ich", "Men\u00b7schen", "\u00fc\u00b7ber", "Men\u00b7schen\u00b7pla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Mit des Jammers hei\u00dfen Thr\u00e4nen klagen.", "tokens": ["Mit", "des", "Jam\u00b7mers", "hei\u00b7\u00dfen", "Thr\u00e4\u00b7nen", "kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Auf der Wollust seidnem Dunenlager", "tokens": ["Auf", "der", "Wol\u00b7lust", "seid\u00b7nem", "Du\u00b7nen\u00b7la\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sa\u00df der Kummer abgeh\u00e4rmt und hager;", "tokens": ["Sa\u00df", "der", "Kum\u00b7mer", "ab\u00b7ge\u00b7h\u00e4rmt", "und", "ha\u00b7ger", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVPP", "KON", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Unterm Strohdach auf der Binsenmatte", "tokens": ["Un\u00b7term", "Stroh\u00b7dach", "auf", "der", "Bin\u00b7sen\u00b7mat\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Weinte stummen Schmerz des Elends Gatte.", "tokens": ["Wein\u00b7te", "stum\u00b7men", "Schmerz", "des", "E\u00b7lends", "Gat\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Himmel, schlagen deiner Strafen Flammen", "tokens": ["Him\u00b7mel", ",", "schla\u00b7gen", "dei\u00b7ner", "Stra\u00b7fen", "Flam\u00b7men"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Alle, alle \u00fcber uns zusammen?", "tokens": ["Al\u00b7le", ",", "al\u00b7le", "\u00fc\u00b7ber", "uns", "zu\u00b7sam\u00b7men", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PIS", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Hier und hier ist aller Marter Quelle:", "tokens": ["Hier", "und", "hier", "ist", "al\u00b7ler", "Mar\u00b7ter", "Quel\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "VAFIN", "PIAT", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Braucht der Fr\u00f6mmler denn noch Eine H\u00f6lle?", "tokens": ["Braucht", "der", "Fr\u00f6mm\u00b7ler", "denn", "noch", "Ei\u00b7ne", "H\u00f6l\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Leidenschaften w\u00fchlen an den St\u00fctzen,", "tokens": ["Lei\u00b7den\u00b7schaf\u00b7ten", "w\u00fch\u00b7len", "an", "den", "St\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die den armen Stamm des Lebens sch\u00fctzen:", "tokens": ["Die", "den", "ar\u00b7men", "Stamm", "des", "Le\u00b7bens", "sch\u00fct\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und sie w\u00fchlen oft in einer Stunde", "tokens": ["Und", "sie", "w\u00fch\u00b7len", "oft", "in", "ei\u00b7ner", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ganzer langer Jahre Werk zu Grunde.", "tokens": ["Gan\u00b7zer", "lan\u00b7ger", "Jah\u00b7re", "Werk", "zu", "Grun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Und die himmlische Natur zu r\u00e4chen,", "tokens": ["Und", "die", "himm\u00b7li\u00b7sche", "Na\u00b7tur", "zu", "r\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Kocht ihr Busen herrliche Verbrechen,", "tokens": ["Kocht", "ihr", "Bu\u00b7sen", "herr\u00b7li\u00b7che", "Ver\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Die in Fluch verwandeln Gottes Segen,", "tokens": ["Die", "in", "Fluch", "ver\u00b7wan\u00b7deln", "Got\u00b7tes", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVINF", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und durch Elend Keim zu Elend legen.", "tokens": ["Und", "durch", "E\u00b7lend", "Keim", "zu", "E\u00b7lend", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "NN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Bosheit gie\u00dfet zu dem Thr\u00e4nenmahle", "tokens": ["Bos\u00b7heit", "gie\u00b7\u00dfet", "zu", "dem", "Thr\u00e4\u00b7nen\u00b7mah\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schleichend Gift noch in die Wermuthschale;", "tokens": ["Schlei\u00b7chend", "Gift", "noch", "in", "die", "Wer\u00b7muth\u00b7scha\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und die Thorheit, ihre Schwester, biethet", "tokens": ["Und", "die", "Thor\u00b7heit", ",", "ih\u00b7re", "Schwes\u00b7ter", ",", "bie\u00b7thet"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "ART", "NN", "$,", "PPOSAT", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Fertig ihr die H\u00e4nde, wenn sie w\u00fcthet.", "tokens": ["Fer\u00b7tig", "ihr", "die", "H\u00e4n\u00b7de", ",", "wenn", "sie", "w\u00fct\u00b7het", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.11": {"line.1": {"text": "Aus dem alten orthodoxen Mantel", "tokens": ["Aus", "dem", "al\u00b7ten", "or\u00b7tho\u00b7do\u00b7xen", "Man\u00b7tel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sticht des Unsinns giftige Tarantel;", "tokens": ["Sticht", "des", "Un\u00b7sinns", "gif\u00b7ti\u00b7ge", "Ta\u00b7ran\u00b7tel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Aus der Irrphilosophie Gewimmel", "tokens": ["Aus", "der", "Irr\u00b7phi\u00b7lo\u00b7so\u00b7phie", "Ge\u00b7wim\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Fliegen Zweifel \u00fcber Gott und Himmel.", "tokens": ["Flie\u00b7gen", "Zwei\u00b7fel", "\u00fc\u00b7ber", "Gott", "und", "Him\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.12": {"line.1": {"text": "G\u00f6tterliebe sinkt zu feilen L\u00fcsten,", "tokens": ["G\u00f6t\u00b7ter\u00b7lie\u00b7be", "sinkt", "zu", "fei\u00b7len", "L\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Unser sch\u00f6nes Eden zu verw\