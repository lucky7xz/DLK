{"textgrid.poem.55728": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn die Gluten des Weines", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn die Gluten des Weines", "tokens": ["Wenn", "die", "Glu\u00b7ten", "des", "Wei\u00b7nes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Dein Antlitz r\u00f6ten,", "tokens": ["Dein", "Ant\u00b7litz", "r\u00f6\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Bleiche Madonna,", "tokens": ["Blei\u00b7che", "Ma\u00b7don\u00b7na", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Dann flieh' meinen Augen.", "tokens": ["Dann", "flieh'", "mei\u00b7nen", "Au\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Ich bete Dich an", "tokens": ["Ich", "be\u00b7te", "Dich", "an"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Und liebe Dich grenzenlos, \u2013", "tokens": ["Und", "lie\u00b7be", "Dich", "gren\u00b7zen\u00b7los", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$,", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Aber nur mit todesblassen Wangen;", "tokens": ["A\u00b7ber", "nur", "mit", "to\u00b7des\u00b7blas\u00b7sen", "Wan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Doch zeigst Du Dich mir", "tokens": ["Doch", "zeigst", "Du", "Dich", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPER", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Durchflutet vom Feuer", "tokens": ["Durch\u00b7flu\u00b7tet", "vom", "Feu\u00b7er"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Des spanischen Weines,", "tokens": ["Des", "spa\u00b7ni\u00b7schen", "Wei\u00b7nes", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Sanft ergl\u00fchend,", "tokens": ["Sanft", "er\u00b7gl\u00fc\u00b7hend", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.12": {"text": "So fa\u00dft mich die Lust,", "tokens": ["So", "fa\u00dft", "mich", "die", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.13": {"text": "Die schreiende Lust,", "tokens": ["Die", "schrei\u00b7en\u00b7de", "Lust", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.14": {"text": "Dich zu k\u00fcssen, zu k\u00fcssen, zu k\u00fcssen,", "tokens": ["Dich", "zu", "k\u00fcs\u00b7sen", ",", "zu", "k\u00fcs\u00b7sen", ",", "zu", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.15": {"text": "Und im Ku\u00df zu erdrosseln; \u2013", "tokens": ["Und", "im", "Ku\u00df", "zu", "er\u00b7dros\u00b7seln", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "APPRART", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.16": {"text": "Denn Du mu\u00dft bleich sein,", "tokens": ["Denn", "Du", "mu\u00dft", "bleich", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.17": {"text": "Totenbleich.", "tokens": ["To\u00b7ten\u00b7bleich", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}