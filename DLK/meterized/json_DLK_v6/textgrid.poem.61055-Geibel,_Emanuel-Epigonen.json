{"textgrid.poem.61055": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Epigonen", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kam in einen gr\u00fcnen Hain,", "tokens": ["Ich", "kam", "in", "ei\u00b7nen", "gr\u00fc\u00b7nen", "Hain", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Viel Eichen standen in der Runde,", "tokens": ["Viel", "Ei\u00b7chen", "stan\u00b7den", "in", "der", "Run\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Durch die gew\u00f6lbte Laubrotunde", "tokens": ["Durch", "die", "ge\u00b7w\u00f6lb\u00b7te", "Lau\u00b7bro\u00b7tun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Flo\u00df goldner Sonnenglanz herein;", "tokens": ["Flo\u00df", "gold\u00b7ner", "Son\u00b7nen\u00b7glanz", "her\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da streckt' ich mich ins Gras zur Ruh'", "tokens": ["Da", "streckt'", "ich", "mich", "ins", "Gras", "zur", "Ruh'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPRART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und sah dem Spiel der Bl\u00e4tter zu.", "tokens": ["Und", "sah", "dem", "Spiel", "der", "Bl\u00e4t\u00b7ter", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nach f\u00fcnfzig Jahren kam ich wieder,", "tokens": ["Nach", "f\u00fcnf\u00b7zig", "Jah\u00b7ren", "kam", "ich", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch mocht' ich andres da erschaun:", "tokens": ["Doch", "mocht'", "ich", "and\u00b7res", "da", "er\u00b7schaun", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die sch\u00f6nen Wipfel lagen nieder,", "tokens": ["Die", "sch\u00f6\u00b7nen", "Wip\u00b7fel", "la\u00b7gen", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die St\u00e4mme waren ausgehaun;", "tokens": ["Die", "St\u00e4m\u00b7me", "wa\u00b7ren", "aus\u00b7ge\u00b7haun", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Statt dessen bl\u00fchten in der Rund'", "tokens": ["Statt", "des\u00b7sen", "bl\u00fch\u00b7ten", "in", "der", "Rund'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PDS", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Viel tausend Bl\u00fcmlein, klein, doch bunt.", "tokens": ["Viel", "tau\u00b7send", "Bl\u00fcm\u00b7lein", ",", "klein", ",", "doch", "bunt", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "$,", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und weil die Eichen nun verschwunden,", "tokens": ["Und", "weil", "die", "Ei\u00b7chen", "nun", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Br\u00fcsten sich stolz die Bl\u00fcmelein", "tokens": ["Br\u00fcs\u00b7ten", "sich", "stolz", "die", "Bl\u00fc\u00b7me\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ADJD", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und meinen gar in manchen Stunden,", "tokens": ["Und", "mei\u00b7nen", "gar", "in", "man\u00b7chen", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie m\u00f6chten selbst wohl Eichen sein.", "tokens": ["Sie", "m\u00f6ch\u00b7ten", "selbst", "wohl", "Ei\u00b7chen", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}