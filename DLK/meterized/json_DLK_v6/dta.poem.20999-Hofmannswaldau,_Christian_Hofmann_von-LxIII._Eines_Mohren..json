{"dta.poem.20999": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "LxIII.  \n Eines Mohren.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Kein ", "tokens": ["Kein"], "token_info": ["word"], "pos": ["PIAT"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Und lachen da\u00df ich schwartz und nackend bin gewe-", "tokens": ["Und", "la\u00b7chen", "da\u00df", "ich", "schwartz", "und", "na\u00b7ckend", "bin", "ge\u00b7we"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KOUS", "PPER", "ADJD", "KON", "VVPP", "VAFIN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "sen.", "tokens": ["sen."], "token_info": ["abbreviation"], "pos": ["XY"]}, "line.4": {"text": "Ich trug das Mutter kleid/ dich kleidet Bock und", "tokens": ["Ich", "trug", "das", "Mut\u00b7ter", "kleid", "/", "dich", "klei\u00b7det", "Bock", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN", "$(", "PPER", "VVFIN", "NE", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "K\u00fch/", "tokens": ["K\u00fch", "/"], "token_info": ["word", "punct"], "pos": ["NE", "$("], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Du bist mehr Vieh als ich/ ich war mehr Mensch", "tokens": ["Du", "bist", "mehr", "Vieh", "als", "ich", "/", "ich", "war", "mehr", "Mensch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "KOUS", "PPER", "$(", "PPER", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "als du.", "tokens": ["als", "du", "."], "token_info": ["word", "word", "punct"], "pos": ["KOUS", "PPER", "$."], "meter": "-+", "measure": "iambic.single"}}}}}