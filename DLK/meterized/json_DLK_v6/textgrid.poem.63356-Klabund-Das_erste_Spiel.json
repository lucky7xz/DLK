{"textgrid.poem.63356": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Das erste Spiel", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir liegen in der Welt. Das erste Spiel", "tokens": ["Wir", "lie\u00b7gen", "in", "der", "Welt", ".", "Das", "ers\u00b7te", "Spiel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Treibt wohl die Mutter mit den Br\u00fcsten leis.", "tokens": ["Treibt", "wohl", "die", "Mut\u00b7ter", "mit", "den", "Br\u00fcs\u00b7ten", "leis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dann tritt die Amme in den krausen Kreis,", "tokens": ["Dann", "tritt", "die", "Am\u00b7me", "in", "den", "krau\u00b7sen", "Kreis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sie wei\u00df sehr wenig und sie lehrt uns viel.", "tokens": ["Sie", "wei\u00df", "sehr", "we\u00b7nig", "und", "sie", "lehrt", "uns", "viel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "KON", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Bleisoldat schie\u00dft nun nach seinem Ziel.", "tokens": ["Der", "Blei\u00b7sol\u00b7dat", "schie\u00dft", "nun", "nach", "sei\u00b7nem", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Beim Murmelschieben winkt manch sch\u00f6ner Preis.", "tokens": ["Beim", "Mur\u00b7mel\u00b7schie\u00b7ben", "winkt", "manch", "sch\u00f6\u00b7ner", "Preis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit Reifen rennen freut den Buben.", "tokens": ["Mit", "Rei\u00b7fen", "ren\u00b7nen", "freut", "den", "Bu\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sei's f\u00fcr sich, sei's mit dem z\u00e4rtlichen Gespiel,", "tokens": ["Sei's", "f\u00fcr", "sich", ",", "sei's", "mit", "dem", "z\u00e4rt\u00b7li\u00b7chen", "Ge\u00b7spiel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PRF", "$,", "VAFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Dem M\u00e4dchen, dem die erste Andacht gilt.", "tokens": ["Dem", "M\u00e4d\u00b7chen", ",", "dem", "die", "ers\u00b7te", "An\u00b7dacht", "gilt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bald spielt sie mit dem Knaben ganz allein.", "tokens": ["Bald", "spielt", "sie", "mit", "dem", "Kna\u00b7ben", "ganz", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie schtreichelt ihn. Sie schmollt. Sie lacht. Sie schilt.", "tokens": ["Sie", "schtrei\u00b7chelt", "ihn", ".", "Sie", "schmollt", ".", "Sie", "lacht", ".", "Sie", "schilt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Er flieht zu W\u00fcrfel, Dirnenscherz und Wein.", "tokens": ["Er", "flieht", "zu", "W\u00fcr\u00b7fel", ",", "Dir\u00b7nen\u00b7scherz", "und", "Wein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sie wendet schaudernd sich von seinem Bild", "tokens": ["Sie", "wen\u00b7det", "schau\u00b7dernd", "sich", "von", "sei\u00b7nem", "Bild"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und st\u00f6\u00dft unwissend ihn in Nacht hinein.", "tokens": ["Und", "st\u00f6\u00dft", "un\u00b7wis\u00b7send", "ihn", "in", "Nacht", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}