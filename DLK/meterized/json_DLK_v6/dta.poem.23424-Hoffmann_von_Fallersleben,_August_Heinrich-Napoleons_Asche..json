{"dta.poem.23424": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Napoleons Asche.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr lieben Leute, seid doch willig", "tokens": ["Ihr", "lie\u00b7ben", "Leu\u00b7te", ",", "seid", "doch", "wil\u00b7lig"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VAFIN", "ADV", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und bringt ein gr\u00f6\u00dfres Opfer dar!", "tokens": ["Und", "bringt", "ein", "gr\u00f6\u00df\u00b7res", "Op\u00b7fer", "dar", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr hattet ihn doch nie so billig,", "tokens": ["Ihr", "hat\u00b7tet", "ihn", "doch", "nie", "so", "bil\u00b7lig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als er noch euer Kaiser war.", "tokens": ["Als", "er", "noch", "eu\u00b7er", "Kai\u00b7ser", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Was sind zwei Millionen Franken?", "tokens": ["Was", "sind", "zwei", "Mil\u00b7lion\u00b7en", "Fran\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "CARD", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wer lebt sei dankbar allezeit:", "tokens": ["Wer", "lebt", "sei", "dank\u00b7bar", "al\u00b7le\u00b7zeit", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "VAFIN", "ADJD", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem Todten habet ihr zu danken,", "tokens": ["Dem", "Tod\u00b7ten", "ha\u00b7bet", "ihr", "zu", "dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ihr noch all' am Leben seid.", "tokens": ["Da\u00df", "ihr", "noch", "all'", "am", "Le\u00b7ben", "seid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIS", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}