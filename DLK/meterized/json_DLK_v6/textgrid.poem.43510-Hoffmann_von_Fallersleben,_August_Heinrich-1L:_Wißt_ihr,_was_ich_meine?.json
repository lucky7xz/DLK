{"textgrid.poem.43510": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wi\u00dft ihr, was ich meine?", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wi\u00dft ihr, was ich meine?", "tokens": ["Wi\u00dft", "ihr", ",", "was", "ich", "mei\u00b7ne", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Singen wollen wir!", "tokens": ["Sin\u00b7gen", "wol\u00b7len", "wir", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Kuckuck ruft und Kibitz schreit,", "tokens": ["Ku\u00b7ckuck", "ruft", "und", "Ki\u00b7bitz", "schreit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Fr\u00fchlingszeit bringt Fr\u00f6hlichkeit:", "tokens": ["Fr\u00fch\u00b7lings\u00b7zeit", "bringt", "Fr\u00f6h\u00b7lich\u00b7keit", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Kommt und singt mit mir!", "tokens": ["Kommt", "und", "singt", "mit", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Wi\u00dft ihr, was ich meine?", "tokens": ["Wi\u00dft", "ihr", ",", "was", "ich", "mei\u00b7ne", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Tanzen wollen wir!", "tokens": ["Tan\u00b7zen", "wol\u00b7len", "wir", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wo die bunten Blumen bl\u00fch'n,", "tokens": ["Wo", "die", "bun\u00b7ten", "Blu\u00b7men", "bl\u00fch'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Tanzt das Lamm im frischen Gr\u00fcn:", "tokens": ["Tanzt", "das", "Lamm", "im", "fri\u00b7schen", "Gr\u00fcn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Kommt und tanzt mit mir!", "tokens": ["Kommt", "und", "tanzt", "mit", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Wi\u00dft ihr, was ich meine?", "tokens": ["Wi\u00dft", "ihr", ",", "was", "ich", "mei\u00b7ne", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Froh sein wollen wir!", "tokens": ["Froh", "sein", "wol\u00b7len", "wir", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAINF", "VMFIN", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Denn der Fr\u00fchling ja gebeut,", "tokens": ["Denn", "der", "Fr\u00fch\u00b7ling", "ja", "ge\u00b7beut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df sich alle Welt erfreut:", "tokens": ["Da\u00df", "sich", "al\u00b7le", "Welt", "er\u00b7freut", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Kommt, freut euch mit mir!", "tokens": ["Kommt", ",", "freut", "euch", "mit", "mir", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}