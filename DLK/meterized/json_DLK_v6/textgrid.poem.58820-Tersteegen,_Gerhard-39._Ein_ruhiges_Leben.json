{"textgrid.poem.58820": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "39. Ein ruhiges Leben", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kein Ort noch Stand noch Kreatur", "tokens": ["Kein", "Ort", "noch", "Stand", "noch", "Kre\u00b7a\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "ADV", "NN", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kann deiner Seele Ruhe geben;", "tokens": ["Kann", "dei\u00b7ner", "See\u00b7le", "Ru\u00b7he", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Brich deine Lust und Willen nur,", "tokens": ["Brich", "dei\u00b7ne", "Lust", "und", "Wil\u00b7len", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So hast du bald ein ruhig's Leben!", "tokens": ["So", "hast", "du", "bald", "ein", "ru\u00b7hig's", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}