{"dta.poem.19546": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Zwei Segel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Zwei Segel erhellend", "tokens": ["Zwei", "Se\u00b7gel", "er\u00b7hel\u00b7lend"], "token_info": ["word", "word", "word"], "pos": ["CARD", "NN", "VVPP"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Die tiefblaue Bucht!", "tokens": ["Die", "tief\u00b7blau\u00b7e", "Bucht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-++-+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Zwei Segel sich schwellend", "tokens": ["Zwei", "Se\u00b7gel", "sich", "schwel\u00b7lend"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "NN", "PRF", "ADJD"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Zu ruhiger Flucht!", "tokens": ["Zu", "ru\u00b7hi\u00b7ger", "Flucht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Wie eins in den Winden", "tokens": ["Wie", "eins", "in", "den", "Win\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Sich w\u00f6lbt und bewegt,", "tokens": ["Sich", "w\u00f6lbt", "und", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Wird auch das Empfinden", "tokens": ["Wird", "auch", "das", "Emp\u00b7fin\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Des andern erregt.", "tokens": ["Des", "an\u00b7dern", "er\u00b7regt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Begehrt eins zu hasten,", "tokens": ["Be\u00b7gehrt", "eins", "zu", "has\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Das andre geht schnell,", "tokens": ["Das", "and\u00b7re", "geht", "schnell", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Verlangt eins zu rasten,", "tokens": ["Ver\u00b7langt", "eins", "zu", "ras\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ruht auch sein Gesell.", "tokens": ["Ruht", "auch", "sein", "Ge\u00b7sell", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}