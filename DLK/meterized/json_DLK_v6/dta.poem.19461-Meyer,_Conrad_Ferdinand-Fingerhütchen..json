", "punct"], "pos": ["APPR", "NN", "$,", "KOUI", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Dieses hast du brav gemacht,", "tokens": ["Die\u00b7ses", "hast", "du", "brav", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Lernet es, ihr S\u00e4nger!", "tokens": ["Ler\u00b7net", "es", ",", "ihr", "S\u00e4n\u00b7ger", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Wie du es zu Stand gebracht,", "tokens": ["Wie", "du", "es", "zu", "Stand", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "H\u00fcbscher ist's und l\u00e4nger!", "tokens": ["H\u00fcb\u00b7scher", "ist's", "und", "l\u00e4n\u00b7ger", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "KON", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Zeig dich einmal, sch\u00f6ner Mann!", "tokens": ["Zeig", "dich", "ein\u00b7mal", ",", "sch\u00f6\u00b7ner", "Mann", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df dich einmal sehen!", "tokens": ["La\u00df", "dich", "ein\u00b7mal", "se\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Vorn zuerst und hinten dann!", "tokens": ["Vorn", "zu\u00b7erst", "und", "hin\u00b