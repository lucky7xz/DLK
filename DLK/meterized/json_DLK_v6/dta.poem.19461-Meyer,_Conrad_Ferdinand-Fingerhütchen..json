{"dta.poem.19461": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Fingerh\u00fctchen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Liebe Kinder, wisst ihr, wo", "tokens": ["Lie\u00b7be", "Kin\u00b7der", ",", "wisst", "ihr", ",", "wo"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "$,", "PWAV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Fingerhut zu Hause?", "tokens": ["Fin\u00b7ger\u00b7hut", "zu", "Hau\u00b7se", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Tief im Thal von Acherloo", "tokens": ["Tief", "im", "Thal", "von", "A\u00b7cher\u00b7loo"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "APPR", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat er Herd und Klause;", "tokens": ["Hat", "er", "Herd", "und", "Klau\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Aber schon in jungen Tagen", "tokens": ["A\u00b7ber", "schon", "in", "jun\u00b7gen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mu\u00df er einen H\u00f6cker tragen,", "tokens": ["Mu\u00df", "er", "ei\u00b7nen", "H\u00f6\u00b7cker", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Geht er, wunderlicher nie", "tokens": ["Geht", "er", ",", "wun\u00b7der\u00b7li\u00b7cher", "nie"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wallte man auf Erden!", "tokens": ["Wall\u00b7te", "man", "auf", "Er\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Sitzt er, staunen Kinn und Knie,", "tokens": ["Sitzt", "er", ",", "stau\u00b7nen", "Kinn", "und", "Knie", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Da\u00df sie Nachbarn werden.", "tokens": ["Da\u00df", "sie", "Nach\u00b7barn", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VAINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "K\u00f6rbe flicht aus Binsen er,", "tokens": ["K\u00f6r\u00b7be", "flicht", "aus", "Bin\u00b7sen", "er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Fr\u00fch und sp\u00e4t sich regend,", "tokens": ["Fr\u00fch", "und", "sp\u00e4t", "sich", "re\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "PRF", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Tr\u00e4gt sie zum Verkauf umher", "tokens": ["Tr\u00e4gt", "sie", "zum", "Ver\u00b7kauf", "um\u00b7her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In der ganzen Gegend,", "tokens": ["In", "der", "gan\u00b7zen", "Ge\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Und er g\u00e4be sich zufrieden,", "tokens": ["Und", "er", "g\u00e4\u00b7be", "sich", "zu\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "W\u00e4r' er nicht im Volk gemieden;", "tokens": ["W\u00e4r'", "er", "nicht", "im", "Volk", "ge\u00b7mie\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn man zischelt mancherlei:", "tokens": ["Denn", "man", "zi\u00b7schelt", "man\u00b7cher\u00b7lei", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PIS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df ein Hexenmeister,", "tokens": ["Da\u00df", "ein", "He\u00b7xen\u00b7meis\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Da\u00df er kr\u00e4uterkundig sei", "tokens": ["Da\u00df", "er", "kr\u00e4u\u00b7ter\u00b7kun\u00b7dig", "sei"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Und im Bund der Geister.", "tokens": ["Und", "im", "Bund", "der", "Geis\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Solches ist die Wahrheit nicht,", "tokens": ["Sol\u00b7ches", "ist", "die", "Wahr\u00b7heit", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ART", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist ein leeres Meinen,", "tokens": ["Ist", "ein", "lee\u00b7res", "Mei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Doch das Volk im D\u00e4mmerlicht", "tokens": ["Doch", "das", "Volk", "im", "D\u00e4m\u00b7mer\u00b7licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schaudert vor dem Kleinen.", "tokens": ["Schau\u00b7dert", "vor", "dem", "Klei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "So die Jungen wie die Alten", "tokens": ["So", "die", "Jun\u00b7gen", "wie", "die", "Al\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "KOKOM", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Weichen aus dem Ungestalten \u2014", "tokens": ["Wei\u00b7chen", "aus", "dem", "Un\u00b7ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch vor\u00fcber wohlgemut", "tokens": ["Doch", "vor\u00b7\u00fc\u00b7ber", "wohl\u00b7ge\u00b7mut"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Auf des Schusters R\u00e4ppchen", "tokens": ["Auf", "des", "Schus\u00b7ters", "R\u00e4pp\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Trabt er. Blauer Fingerhut", "tokens": ["Trabt", "er", ".", "Blau\u00b7er", "Fin\u00b7ger\u00b7hut"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$.", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Nickt von seinem K\u00e4ppchen.", "tokens": ["Nickt", "von", "sei\u00b7nem", "K\u00e4pp\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Einmal geht er heim bei Nacht", "tokens": ["Ein\u00b7mal", "geht", "er", "heim", "bei", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach des Tages Lasten,", "tokens": ["Nach", "des", "Ta\u00b7ges", "Las\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Hat den halben Weg gemacht,", "tokens": ["Hat", "den", "hal\u00b7ben", "Weg", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Darf ein bischen rasten,", "tokens": ["Darf", "ein", "bi\u00b7schen", "ras\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADV", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Setzt sich und den Korb daneben,", "tokens": ["Setzt", "sich", "und", "den", "Korb", "da\u00b7ne\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "KON", "ART", "NN", "PAV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schimmernd hebt der Mond sich eben:", "tokens": ["Schim\u00b7mernd", "hebt", "der", "Mond", "sich", "e\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PRF", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Fingerhut ist gar nicht bang,", "tokens": ["Fin\u00b7ger\u00b7hut", "ist", "gar", "nicht", "bang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ihm ist gar nicht schaurig,", "tokens": ["Ihm", "ist", "gar", "nicht", "schau\u00b7rig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Nur da\u00df noch der Weg so lang,", "tokens": ["Nur", "da\u00df", "noch", "der", "Weg", "so", "lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ADV", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Macht den Kleinen traurig.", "tokens": ["Macht", "den", "Klei\u00b7nen", "trau\u00b7rig", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Etwas h\u00f6rt er klingen fein \u2014", "tokens": ["Et\u00b7was", "h\u00f6rt", "er", "klin\u00b7gen", "fein"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht mit rechten Dingen,", "tokens": ["Nicht", "mit", "rech\u00b7ten", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Mitten aus dem gr\u00fcnen Rain", "tokens": ["Mit\u00b7ten", "aus", "dem", "gr\u00fc\u00b7nen", "Rain"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ein melodisch Singen:", "tokens": ["Ein", "me\u00b7lo\u00b7disch", "Sin\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "\u201esilberf\u00e4hre, gleitest leise\u201c \u2014", "tokens": ["\u201e", "sil\u00b7ber\u00b7f\u00e4h\u00b7re", ",", "glei\u00b7test", "lei\u00b7se", "\u201c"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "$,", "ADV", "ADJD", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schon verstummt die kurze Weise.", "tokens": ["Schon", "ver\u00b7stummt", "die", "kur\u00b7ze", "Wei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Fingerh\u00fctchen sp\u00e4het scharf", "tokens": ["Fin\u00b7ger\u00b7h\u00fct\u00b7chen", "sp\u00e4\u00b7het", "scharf"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVFIN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und kann nichts entdecken,", "tokens": ["Und", "kann", "nichts", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Aber was er h\u00f6ren darf,", "tokens": ["A\u00b7ber", "was", "er", "h\u00f6\u00b7ren", "darf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Ist nicht zum Erschrecken.", "tokens": ["Ist", "nicht", "zum", "Er\u00b7schre\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Wieder hebt das Liedchen an", "tokens": ["Wie\u00b7der", "hebt", "das", "Lied\u00b7chen", "an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Unter Busch und Hecken,", "tokens": ["Un\u00b7ter", "Busch", "und", "He\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Doch es bleibt der Reimgespan", "tokens": ["Doch", "es", "bleibt", "der", "Reim\u00b7ge\u00b7span"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Stets im H\u00fcgel stecken.", "tokens": ["Stets", "im", "H\u00fc\u00b7gel", "ste\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "\u201esilberf\u00e4hre, gleitest leise\u201c \u2014", "tokens": ["\u201e", "sil\u00b7ber\u00b7f\u00e4h\u00b7re", ",", "glei\u00b7test", "lei\u00b7se", "\u201c"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "$,", "ADV", "ADJD", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wiederum verstummt die Weise.", "tokens": ["Wie\u00b7de\u00b7rum", "ver\u00b7stummt", "die", "Wei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Lieblich ist, doch einerlei", "tokens": ["Lieb\u00b7lich", "ist", ",", "doch", "ei\u00b7ner\u00b7lei"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJD", "VAFIN", "$,", "ADV", "PIS"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Der Gesang der Elfen,", "tokens": ["Der", "Ge\u00b7sang", "der", "El\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Fingerh\u00fctchen f\u00e4llt es bei,", "tokens": ["Fin\u00b7ger\u00b7h\u00fct\u00b7chen", "f\u00e4llt", "es", "bei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Ihnen einzuhelfen.", "tokens": ["Ih\u00b7nen", "ein\u00b7zu\u00b7hel\u00b7fen", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVIZU", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Fingerh\u00fctchen lauert still", "tokens": ["Fin\u00b7ger\u00b7h\u00fct\u00b7chen", "lau\u00b7ert", "still"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVFIN", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf der T\u00f6ne Leiter,", "tokens": ["Auf", "der", "T\u00f6\u00b7ne", "Lei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wie das Liedchen enden will", "tokens": ["Wie", "das", "Lied\u00b7chen", "en\u00b7den", "will"], "token_