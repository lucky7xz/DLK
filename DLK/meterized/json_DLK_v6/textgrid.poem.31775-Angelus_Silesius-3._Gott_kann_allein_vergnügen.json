{"textgrid.poem.31775": {"metadata": {"author": {"name": "Angelus Silesius", "birth": "N.A.", "death": "N.A."}, "title": "3. Gott kann allein vergn\u00fcgen", "genre": "verse", "period": "N.A.", "pub_year": 1650, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weg, weg, ihr Seraphim, ihr k\u00f6nnt mich nicht erquicken,", "tokens": ["Weg", ",", "weg", ",", "ihr", "Se\u00b7ra\u00b7phim", ",", "ihr", "k\u00f6nnt", "mich", "nicht", "er\u00b7qui\u00b7cken", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weg, weg, ihr Heiligen und was an euch tut blicken.", "tokens": ["Weg", ",", "weg", ",", "ihr", "Hei\u00b7li\u00b7gen", "und", "was", "an", "euch", "tut", "bli\u00b7cken", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PTKVZ", "$,", "PPOSAT", "NN", "KON", "PWS", "APPR", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ich will nun eurer nicht, ich werfe mich allein", "tokens": ["Ich", "will", "nun", "eu\u00b7rer", "nicht", ",", "ich", "wer\u00b7fe", "mich", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "PPOSAT", "PTKNEG", "$,", "PPER", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ins ungeschaffne Meer der blo\u00dfen Gottheit ein. ", "tokens": ["Ins", "un\u00b7ge\u00b7schaff\u00b7ne", "Meer", "der", "blo\u00b7\u00dfen", "Got\u00b7theit", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}