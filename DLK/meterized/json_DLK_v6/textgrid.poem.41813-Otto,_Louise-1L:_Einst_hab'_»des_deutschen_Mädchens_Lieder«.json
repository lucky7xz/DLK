{"textgrid.poem.41813": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Einst hab' \u00bbdes deutschen M\u00e4dchens Lieder\u00ab", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einst hab' \u00bbdes deutschen M\u00e4dchens Lieder\u00ab", "tokens": ["Einst", "hab'", "\u00bb", "des", "deut\u00b7schen", "M\u00e4d\u00b7chens", "Lie\u00b7der", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$(", "ART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich Euch als ersten Gru\u00df geweiht,", "tokens": ["Ich", "Euch", "als", "ers\u00b7ten", "Gru\u00df", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "KOUS", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie hallten wohl bei vielen wieder", "tokens": ["Sie", "hall\u00b7ten", "wohl", "bei", "vie\u00b7len", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PIS", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In einer sturmbewegten Zeit.", "tokens": ["In", "ei\u00b7ner", "sturm\u00b7be\u00b7weg\u00b7ten", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "In einer Zeit, drinn ohne Wanken", "tokens": ["In", "ei\u00b7ner", "Zeit", ",", "drinn", "oh\u00b7ne", "Wan\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich zu Euch stand, und sonder Scheu,", "tokens": ["Ich", "zu", "Euch", "stand", ",", "und", "son\u00b7der", "Scheu", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "VVFIN", "$,", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df so in Thaten als Gedanken", "tokens": ["Da\u00df", "so", "in", "Tha\u00b7ten", "als", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "NN", "KOUS", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auch ich bew\u00e4hrt die alte Treu. \u2013", "tokens": ["Auch", "ich", "be\u00b7w\u00e4hrt", "die", "al\u00b7te", "Treu", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nun Jahr um Jahr ist hingeflossen", "tokens": ["Nun", "Jahr", "um", "Jahr", "ist", "hin\u00b7ge\u00b7flos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "APPR", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Seit jener ersten Lieder Klang,", "tokens": ["Seit", "je\u00b7ner", "ers\u00b7ten", "Lie\u00b7der", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den, wie der B\u00e4ume frisches Sprossen,", "tokens": ["Den", ",", "wie", "der", "B\u00e4u\u00b7me", "fri\u00b7sches", "Spros\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Geweckt des Lenzes Freiheitsdrang.", "tokens": ["Ge\u00b7weckt", "des", "Len\u00b7zes", "Frei\u00b7heits\u00b7drang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und wieder m\u00f6cht ich nun Euch bieten,", "tokens": ["Und", "wie\u00b7der", "m\u00f6cht", "ich", "nun", "Euch", "bie\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was in der Zeiten rascher Flucht", "tokens": ["Was", "in", "der", "Zei\u00b7ten", "ra\u00b7scher", "Flucht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Gott des Sanges mir beschieden,", "tokens": ["Der", "Gott", "des", "San\u00b7ges", "mir", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn ich bei ihm nach Trost gesucht.", "tokens": ["Wenn", "ich", "bei", "ihm", "nach", "Trost", "ge\u00b7sucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Nach Trost in schweren Pr\u00fcfungstagen,", "tokens": ["Nach", "Trost", "in", "schwe\u00b7ren", "Pr\u00fc\u00b7fungs\u00b7ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn alles wankte um und um", "tokens": ["Wenn", "al\u00b7les", "wank\u00b7te", "um", "und", "um"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "VVFIN", "PTKVZ", "KON", "KOUI"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und mich besch\u00fctzte vor Verzagen", "tokens": ["Und", "mich", "be\u00b7sch\u00fctz\u00b7te", "vor", "Ver\u00b7za\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Allein der Dichtung Heiligtum.", "tokens": ["Al\u00b7lein", "der", "Dich\u00b7tung", "Hei\u00b7lig\u00b7tum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Da sang ich bald die eignen Schmerzen", "tokens": ["Da", "sang", "ich", "bald", "die", "eig\u00b7nen", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und bald des Vaterlandes Not,", "tokens": ["Und", "bald", "des", "Va\u00b7ter\u00b7lan\u00b7des", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Entz\u00fcndete noch Hoffnungskerzen,", "tokens": ["Ent\u00b7z\u00fcn\u00b7de\u00b7te", "noch", "Hoff\u00b7nungs\u00b7ker\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo Nacht und Elend nur gedroht.", "tokens": ["Wo", "Nacht", "und", "E\u00b7lend", "nur", "ge\u00b7droht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Und ich betrat mit heil'gem Ahnen", "tokens": ["Und", "ich", "be\u00b7trat", "mit", "heil'\u00b7gem", "Ah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Tempel der Natur und Kunst,", "tokens": ["Die", "Tem\u00b7pel", "der", "Na\u00b7tur", "und", "Kunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die an das Ewige uns mahnen,", "tokens": ["Die", "an", "das", "E\u00b7wi\u00b7ge", "uns", "mah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Geoffenbart durch Himmelsgunst.", "tokens": ["Ge\u00b7of\u00b7fen\u00b7bart", "durch", "Him\u00b7mels\u00b7gunst", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und gr\u00fc\u00dft' ich froh die deutschen Lande.", "tokens": ["Und", "gr\u00fc\u00dft'", "ich", "froh", "die", "deut\u00b7schen", "Lan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und floh ich zur Vergangenheit \u2013", "tokens": ["Und", "floh", "ich", "zur", "Ver\u00b7gan\u00b7gen\u00b7heit", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mir ward ein Lied zum Unterpfande", "tokens": ["Mir", "ward", "ein", "Lied", "zum", "Un\u00b7ter\u00b7pfan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr deutscher Dichtung Ewigkeit.", "tokens": ["F\u00fcr", "deut\u00b7scher", "Dich\u00b7tung", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Und dieser Lieder bunte Spende \u2013", "tokens": ["Und", "die\u00b7ser", "Lie\u00b7der", "bun\u00b7te", "Spen\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Gleichgesinnten, nah und fern \u2013", "tokens": ["Ihr", "Gleich\u00b7ge\u00b7sinn\u00b7ten", ",", "nah", "und", "fern", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich leg' sie nun in Eure H\u00e4nde", "tokens": ["Ich", "leg'", "sie", "nun", "in", "Eu\u00b7re", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit froher Zuversicht und gern.", "tokens": ["Mit", "fro\u00b7her", "Zu\u00b7ver\u00b7sicht", "und", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Mit Stolz, mich Euch verwandt zu finden,", "tokens": ["Mit", "Stolz", ",", "mich", "Euch", "ver\u00b7wandt", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PPER", "PPER", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Ihr im Dienst der Menschheit ringt,", "tokens": ["Die", "Ihr", "im", "Dienst", "der", "Menschheit", "ringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "La\u00dft diesen Liederkranz mich winden,", "tokens": ["La\u00dft", "die\u00b7sen", "Lie\u00b7der\u00b7kranz", "mich", "win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PDAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der meine Gr\u00fc\u00dfe zu Euch bringt.", "tokens": ["Der", "mei\u00b7ne", "Gr\u00fc\u00b7\u00dfe", "zu", "Euch", "bringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}