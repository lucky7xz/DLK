{"dta.poem.11420": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Abri\u00df einer b\u00f6sen zeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der priester zancket, geitzt und saufft,", "tokens": ["Der", "pries\u00b7ter", "zan\u00b7cket", ",", "geitzt", "und", "saufft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und treibet nichts als faul geschw\u00e4tze:", "tokens": ["Und", "trei\u00b7bet", "nichts", "als", "faul", "ge\u00b7schw\u00e4t\u00b7ze", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "KOKOM", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die bo\u00dfheit achtet kein gesetze,", "tokens": ["Die", "bo\u00df\u00b7heit", "ach\u00b7tet", "kein", "ge\u00b7set\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weil man das recht ums geld erkaufft.", "tokens": ["Weil", "man", "das", "recht", "ums", "geld", "er\u00b7kaufft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "ADJD", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die \u00e4rtzte pflegen wohl zu morden,", "tokens": ["Die", "\u00e4rtz\u00b7te", "pfle\u00b7gen", "wohl", "zu", "mor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Drum ist bi\u00dfhero h\u00f6ll und tod", "tokens": ["Drum", "ist", "bi\u00df\u00b7he\u00b7ro", "h\u00f6ll", "und", "tod"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "ADV", "ADJD", "KON", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Viel reicher, als der himmel worden:", "tokens": ["Viel", "rei\u00b7cher", ",", "als", "der", "him\u00b7mel", "wor\u00b7den", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "ART", "NN", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ach! wer behertzigt diese noth!", "tokens": ["Ach", "!", "wer", "be\u00b7hert\u00b7zigt", "die\u00b7se", "noth", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWS", "VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}