{"textgrid.poem.48814": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "39. Auf ein Armband", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwar da\u00df dein teurer Wert an Perlen, Gold und Seide,", "tokens": ["Zwar", "da\u00df", "dein", "teu\u00b7rer", "Wert", "an", "Per\u00b7len", ",", "Gold", "und", "Sei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPOSAT", "ADJA", "NN", "APPR", "NN", "$,", "NN", "KON", "NE", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "bei mir, o liebes Band, in hohen W\u00fcrden ist,", "tokens": ["bei", "mir", ",", "o", "lie\u00b7bes", "Band", ",", "in", "ho\u00b7hen", "W\u00fcr\u00b7den", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "FM", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "das k\u00f6mmet darvon her, da\u00df du dich so bem\u00fchst", "tokens": ["das", "k\u00f6m\u00b7met", "dar\u00b7von", "her", ",", "da\u00df", "du", "dich", "so", "be\u00b7m\u00fchst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PAV", "PTKVZ", "$,", "KOUS", "PPER", "PRF", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "mir durch die Treflichkeit zu schaffen h\u00f6chste Freude.", "tokens": ["mir", "durch", "die", "Tref\u00b7lich\u00b7keit", "zu", "schaf\u00b7fen", "h\u00f6chs\u00b7te", "Freu\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "PTKZU", "VVINF", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch, mein' ich, \u00fcbertrifft (nicht nachgeredt zum Neide)", "tokens": ["Doch", ",", "mein'", "ich", ",", "\u00fc\u00b7bert\u00b7rifft", "(", "nicht", "nach\u00b7ge\u00b7redt", "zum", "Nei\u00b7de", ")"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PPER", "$,", "VVFIN", "$(", "PTKNEG", "ADJD", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "den Wert die edle Kunst, von der du gleich als bl\u00fchst,", "tokens": ["den", "Wert", "die", "ed\u00b7le", "Kunst", ",", "von", "der", "du", "gleich", "als", "bl\u00fchst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,", "APPR", "PRELS", "PPER", "ADV", "KOKOM", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "wenn du mit solcher Zier in mein Gesichte siehst,", "tokens": ["wenn", "du", "mit", "sol\u00b7cher", "Zier", "in", "mein", "Ge\u00b7sich\u00b7te", "siehst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "so bin ich, den du h\u00e4ltst, ganz frei von allem Leide.", "tokens": ["so", "bin", "ich", ",", "den", "du", "h\u00e4ltst", ",", "ganz", "frei", "von", "al\u00b7lem", "Lei\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "PRELS", "PPER", "VVFIN", "$,", "ADV", "ADJD", "APPR", "PIS", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Iedoch wie hoch der Wert, wie gro\u00df auch ist die Kunst,", "tokens": ["Ie\u00b7doch", "wie", "hoch", "der", "Wert", ",", "wie", "gro\u00df", "auch", "ist", "die", "Kunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ADJD", "ART", "NN", "$,", "PWAV", "ADJD", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "so halt' ich h\u00f6her weit, weit gr\u00f6\u00dfer diese Gunst,", "tokens": ["so", "halt'", "ich", "h\u00f6\u00b7her", "weit", ",", "weit", "gr\u00f6\u00b7\u00dfer", "die\u00b7se", "Gunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADJD", "$,", "ADJD", "ADJD", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "da\u00df hier stehn unterst\u00fcckt der Treusten teure Haare.", "tokens": ["da\u00df", "hier", "stehn", "un\u00b7ter\u00b7st\u00fcckt", "der", "Treus\u00b7ten", "teu\u00b7re", "Haa\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VVINF", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Weg Perlen, Seide, Gold und was von Fernen k\u00f6mt,", "tokens": ["Weg", "Per\u00b7len", ",", "Sei\u00b7de", ",", "Gold", "und", "was", "von", "Fer\u00b7nen", "k\u00f6mt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "NE", "$,", "NN", "KON", "PWS", "APPR", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "weg, was von sch\u00f6ner Kunst den edlen Namen nimt,", "tokens": ["weg", ",", "was", "von", "sch\u00f6\u00b7ner", "Kunst", "den", "ed\u00b7len", "Na\u00b7men", "nimt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "PRELS", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "hier ist und von Natur weit eine bessre Ware!", "tokens": ["hier", "ist", "und", "von", "Na\u00b7tur", "weit", "ei\u00b7ne", "bess\u00b7re", "Wa\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "KON", "APPR", "NN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}