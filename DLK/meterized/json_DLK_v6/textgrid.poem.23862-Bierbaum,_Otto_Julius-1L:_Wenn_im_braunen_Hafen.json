{"textgrid.poem.23862": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn im braunen Hafen", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn im braunen Hafen", "tokens": ["Wenn", "im", "brau\u00b7nen", "Ha\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Alle Schiffe schlafen,", "tokens": ["Al\u00b7le", "Schif\u00b7fe", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wach ich auf zu dir.", "tokens": ["Wach", "ich", "auf", "zu", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "APPR", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Stille in der Runde,", "tokens": ["Stil\u00b7le", "in", "der", "Run\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Heilig diese Stunde,", "tokens": ["Hei\u00b7lig", "die\u00b7se", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "PDAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Denn sie bringt dich, atemhaltend, mir.", "tokens": ["Denn", "sie", "bringt", "dich", ",", "a\u00b7tem\u00b7hal\u00b7tend", ",", "mir", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,", "VVPP", "$,", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Stehst in Mondenhelle", "tokens": ["Stehst", "in", "Mon\u00b7den\u00b7hel\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wartend an der Schwelle,", "tokens": ["War\u00b7tend", "an", "der", "Schwel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und ich f\u00fchle dich;", "tokens": ["Und", "ich", "f\u00fch\u00b7le", "dich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Komm', da\u00df ich dich halte,", "tokens": ["Komm'", ",", "da\u00df", "ich", "dich", "hal\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Deine Seele walte", "tokens": ["Dei\u00b7ne", "See\u00b7le", "wal\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Ueber meinen Tr\u00e4umen m\u00fctterlich.", "tokens": ["Ue\u00b7ber", "mei\u00b7nen", "Tr\u00e4u\u00b7men", "m\u00fct\u00b7ter\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}