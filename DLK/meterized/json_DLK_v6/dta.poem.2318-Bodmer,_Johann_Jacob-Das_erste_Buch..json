{"dta.poem.2318": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Das erste Buch.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1741", "urn": "urn:nbn:de:kobv:b4-200905198534", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So ward die Geisterwelt; verschiedne Macht und Ehre,", "tokens": ["So", "ward", "die", "Geis\u00b7ter\u00b7welt", ";", "ver\u00b7schied\u00b7ne", "Macht", "und", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$.", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Entschieden stuffenweis die unz\u00e4hlbaren Heere,", "tokens": ["Ent\u00b7schie\u00b7den", "stuf\u00b7fen\u00b7weis", "die", "un\u00b7z\u00e4hl\u00b7ba\u00b7ren", "Hee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die ungleich satt vom Glantz des mitgetheilten Lichts", "tokens": ["Die", "un\u00b7gleich", "satt", "vom", "Glantz", "des", "mit\u00b7ge\u00b7theil\u00b7ten", "Lichts"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJD", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In langer Ordnung stehn, von Gott zum \u00f6den Nichts.", "tokens": ["In", "lan\u00b7ger", "Ord\u00b7nung", "stehn", ",", "von", "Gott", "zum", "\u00f6\u00b7den", "Nichts", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,", "APPR", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"