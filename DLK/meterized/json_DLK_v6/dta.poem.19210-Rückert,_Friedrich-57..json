{"dta.poem.19210": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "57.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie mittheilt ein Magnet die eigne Eigenschaft", "tokens": ["Wie", "mit\u00b7theilt", "ein", "Mag\u00b7net", "die", "eig\u00b7ne", "Ei\u00b7gen\u00b7schaft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dem Eisen, ohne da\u00df er selbst verliert an Kraft,", "tokens": ["Dem", "Ei\u00b7sen", ",", "oh\u00b7ne", "da\u00df", "er", "selbst", "ver\u00b7liert", "an", "Kraft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUI", "KOUS", "PPER", "ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Weil, was er mittheilt, nicht ist seiner Kraft Bewegung,", "tokens": ["Weil", ",", "was", "er", "mit\u00b7theilt", ",", "nicht", "ist", "sei\u00b7ner", "Kraft", "Be\u00b7we\u00b7gung", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWS", "PPER", "VVPP", "$,", "PTKNEG", "VAFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vielmehr die Richtung nur und gleicher Kraft Anregung;", "tokens": ["Viel\u00b7mehr", "die", "Rich\u00b7tung", "nur", "und", "glei\u00b7cher", "Kraft", "An\u00b7re\u00b7gung", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "KON", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nicht, wie ein Feuchtes, wenn man drein ein Trocknes taucht,", "tokens": ["Nicht", ",", "wie", "ein", "Feuch\u00b7tes", ",", "wenn", "man", "drein", "ein", "Trock\u00b7nes", "taucht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "PWAV", "ART", "NN", "$,", "KOUS", "PIS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Theilchen Feuchte f\u00fchlt vom Trocknen aufgebraucht,", "tokens": ["Ein", "Theil\u00b7chen", "Feuch\u00b7te", "f\u00fchlt", "vom", "Trock\u00b7nen", "auf\u00b7ge\u00b7braucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und Warmes k\u00e4lter wird, das Kaltes machet w\u00e4rmer;", "tokens": ["Und", "War\u00b7mes", "k\u00e4l\u00b7ter", "wird", ",", "das", "Kal\u00b7tes", "ma\u00b7chet", "w\u00e4r\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "VAFIN", "$,", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wird ein Reicher, der dem Armen reicht, wol \u00e4rmer,", "tokens": ["So", "wird", "ein", "Rei\u00b7cher", ",", "der", "dem", "Ar\u00b7men", "reicht", ",", "wol", "\u00e4r\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Doch \u00e4rmer werden soll kein Geist, wenn angehaucht", "tokens": ["Doch", "\u00e4r\u00b7mer", "wer\u00b7den", "soll", "kein", "Geist", ",", "wenn", "an\u00b7ge\u00b7haucht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJD", "VAINF", "VMFIN", "PIAT", "NN", "$,", "KOUS", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von ihm ein andrer auch nun brennet oder raucht:", "tokens": ["Von", "ihm", "ein", "an\u00b7drer", "auch", "nun", "bren\u00b7net", "o\u00b7der", "raucht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "ADV", "ADV", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Klagst du, da\u00df etwas durch Mittheilung dir entgeht,", "tokens": ["Klagst", "du", ",", "da\u00df", "et\u00b7was", "durch", "Mit\u00b7thei\u00b7lung", "dir", "ent\u00b7geht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PIS", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O sch\u00e4me dich, du bist ein Schwamm, und kein Magnet.", "tokens": ["O", "sch\u00e4\u00b7me", "dich", ",", "du", "bist", "ein", "Schwamm", ",", "und", "kein", "Mag\u00b7net", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "PPER", "VAFIN", "ART", "NN", "$,", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}}}}