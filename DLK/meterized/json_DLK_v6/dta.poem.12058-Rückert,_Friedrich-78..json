{"dta.poem.12058": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "78.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gesittung strebt, das Thier dem Menschen auszuziehn,", "tokens": ["Ge\u00b7sit\u00b7tung", "strebt", ",", "das", "Thier", "dem", "Men\u00b7schen", "aus\u00b7zu\u00b7ziehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zur\u00fcck zur Menschheit die er auszog f\u00fchrt sie ihn.", "tokens": ["Zu\u00b7r\u00fcck", "zur", "Menschheit", "die", "er", "aus\u00b7zog", "f\u00fchrt", "sie", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "PPER", "VVFIN", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Zur\u00fcck zum Paradies f\u00fchrt sie den Nackten wieder,", "tokens": ["Zu\u00b7r\u00fcck", "zum", "Pa\u00b7ra\u00b7dies", "f\u00fchrt", "sie", "den", "Nack\u00b7ten", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie ehr des Thieres Fell er zog um seine Glieder.", "tokens": ["Wie", "ehr", "des", "Thie\u00b7res", "Fell", "er", "zog", "um", "sei\u00b7ne", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "NN", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Solang (wielang noch?) ist nicht ihr Beruf erf\u00fcllt,", "tokens": ["So\u00b7lang", "(", "wie\u00b7lang", "noch", "?", ")", "ist", "nicht", "ihr", "Be\u00b7ruf", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "VVFIN", "ADV", "$.", "$(", "VAFIN", "PTKNEG", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als sie statt auszuziehn die Thierheit nur verh\u00fcllt.", "tokens": ["Als", "sie", "statt", "aus\u00b7zu\u00b7ziehn", "die", "Thier\u00b7heit", "nur", "ver\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}