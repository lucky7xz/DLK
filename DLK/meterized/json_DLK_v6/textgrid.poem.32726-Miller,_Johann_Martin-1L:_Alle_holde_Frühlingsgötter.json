{"textgrid.poem.32726": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "1L: Alle holde Fr\u00fchlingsg\u00f6tter", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alle holde Fr\u00fchlingsg\u00f6tter", "tokens": ["Al\u00b7le", "hol\u00b7de", "Fr\u00fch\u00b7lings\u00b7g\u00f6t\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Segnen dich, o Rosenstrauch!", "tokens": ["Seg\u00b7nen", "dich", ",", "o", "Ro\u00b7sen\u00b7strauch", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "FM", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Treibe sch\u00f6ne, frische Bl\u00e4tter!", "tokens": ["Trei\u00b7be", "sch\u00f6\u00b7ne", ",", "fri\u00b7sche", "Bl\u00e4t\u00b7ter", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber trage Bl\u00fcmchen auch!", "tokens": ["A\u00b7ber", "tra\u00b7ge", "Bl\u00fcm\u00b7chen", "auch", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Da\u00df, wann mir in k\u00fcnft'gen Lenzen", "tokens": ["Da\u00df", ",", "wann", "mir", "in", "k\u00fcnft'\u00b7gen", "Len\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "PWAV", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine Daphne milder wird,", "tokens": ["Mei\u00b7ne", "Daph\u00b7ne", "mil\u00b7der", "wird", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie mit frischen Rosenkr\u00e4nzen", "tokens": ["Sie", "mit", "fri\u00b7schen", "Ro\u00b7sen\u00b7kr\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine blonde Locken ziert!", "tokens": ["Mei\u00b7ne", "blon\u00b7de", "Lo\u00b7cken", "ziert", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wann sie grausam bleibt, dann pfl\u00fccke", "tokens": ["Wann", "sie", "grau\u00b7sam", "bleibt", ",", "dann", "pfl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PPER", "ADJD", "VVFIN", "$,", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Damon deine Bl\u00fcmchen ab,", "tokens": ["Da\u00b7mon", "dei\u00b7ne", "Bl\u00fcm\u00b7chen", "ab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und beweine mich, und schm\u00fccke", "tokens": ["Und", "be\u00b7wei\u00b7ne", "mich", ",", "und", "schm\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit den Erstlingen mein Grab!", "tokens": ["Mit", "den", "Erst\u00b7lin\u00b7gen", "mein", "Grab", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}