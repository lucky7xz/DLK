{"textgrid.poem.66009": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sah zu j\u00fcngst einem Villenbau,", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sah zu j\u00fcngst einem Villenbau,", "tokens": ["Sah", "zu", "j\u00fcngst", "ei\u00b7nem", "Vil\u00b7len\u00b7bau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Stra\u00dfe wei\u00df ich nicht genau,", "tokens": ["Die", "Stra\u00b7\u00dfe", "wei\u00df", "ich", "nicht", "ge\u00b7nau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ging eine schmucke Dirn vorbei", "tokens": ["Ging", "ei\u00b7ne", "schmu\u00b7cke", "Dirn", "vor\u00b7bei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im blauen Waschkleid, die Arme frei,", "tokens": ["Im", "blau\u00b7en", "Waschkleid", ",", "die", "Ar\u00b7me", "frei", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Trug einen Korb, draus quoll hervor", "tokens": ["Trug", "ei\u00b7nen", "Korb", ",", "draus", "quoll", "her\u00b7vor"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "PAV", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der sch\u00f6nste zarte Rosenflor.", "tokens": ["Der", "sch\u00f6ns\u00b7te", "zar\u00b7te", "Ro\u00b7sen\u00b7flor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Den Rosen glichen ihre Wangen,", "tokens": ["Den", "Ro\u00b7sen", "gli\u00b7chen", "ih\u00b7re", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Lippen weckten Kussverlangen,", "tokens": ["Die", "Lip\u00b7pen", "weck\u00b7ten", "Kuss\u00b7ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und eh' sie wusst', wie ihr geschah,", "tokens": ["Und", "eh'", "sie", "wusst'", ",", "wie", "ihr", "ge\u00b7schah", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Sich rechts und links umworben sah.", "tokens": ["Sich", "rechts", "und", "links", "um\u00b7wor\u00b7ben", "sah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "KON", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Hatt' gleich an jeder Seit einen Schalk,", "tokens": ["Hatt'", "gleich", "an", "je\u00b7der", "Seit", "ei\u00b7nen", "Schalk", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Bestaubt mit Ziegelmehl und Kalk.", "tokens": ["Be\u00b7staubt", "mit", "Zie\u00b7gel\u00b7mehl", "und", "Kalk", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der griff ihr um die H\u00fcfte schlank,", "tokens": ["Der", "griff", "ihr", "um", "die", "H\u00fcf\u00b7te", "schlank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der langte nach den Rosen frank,", "tokens": ["Der", "lang\u00b7te", "nach", "den", "Ro\u00b7sen", "frank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und h\u00e4tt' mit grober Werkelfaust", "tokens": ["Und", "h\u00e4tt'", "mit", "gro\u00b7ber", "Wer\u00b7kel\u00b7faust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die zarte Fracht ihr fast zerzaust.", "tokens": ["Die", "zar\u00b7te", "Fracht", "ihr", "fast", "zer\u00b7zaust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "H\u00fclflos vor keckem \u00dcbermut,", "tokens": ["H\u00fcl\u00b7flos", "vor", "ke\u00b7ckem", "\u00dc\u00b7ber\u00b7mut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "In Sorge um der Herrin Gut.", "tokens": ["In", "Sor\u00b7ge", "um", "der", "Her\u00b7rin", "Gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die vollen Arme hoch erhoben,", "tokens": ["Die", "vol\u00b7len", "Ar\u00b7me", "hoch", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Korb zu retten vor den Groben,", "tokens": ["Den", "Korb", "zu", "ret\u00b7ten", "vor", "den", "Gro\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Musst so sie leiden voller Scham,", "tokens": ["Musst", "so", "sie", "lei\u00b7den", "vol\u00b7ler", "Scham", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dass ihr ein dritter ein K\u00fcsschen nahm.", "tokens": ["Dass", "ihr", "ein", "drit\u00b7ter", "ein", "K\u00fcs\u00b7schen", "nahm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Da fuhr ein alter Graukopf drein,", "tokens": ["Da", "fuhr", "ein", "al\u00b7ter", "Grau\u00b7kopf", "drein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nannt' selbst wohl solch ein M\u00e4dchen sein:", "tokens": ["Nannt'", "selbst", "wohl", "solch", "ein", "M\u00e4d\u00b7chen", "sein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PIAT", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bblat doch de Deern! Ji drivt't to dull!", "tokens": ["\u00bb", "lat", "doch", "de", "Deern", "!", "Ji", "drivt't", "to", "dull", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "FM.it", "FM.it", "FM.it", "FM.it", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "So'n Rosenpl\u00fccken m\u00f6gt ji wull?\u00ab", "tokens": ["So'n", "Ro\u00b7sen\u00b7pl\u00fc\u00b7cken", "m\u00f6gt", "ji", "wull", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NN", "VMFIN", "PPER", "VMFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}