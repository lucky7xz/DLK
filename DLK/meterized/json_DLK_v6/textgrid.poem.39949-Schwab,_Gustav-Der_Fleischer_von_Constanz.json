{"textgrid.poem.39949": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Der Fleischer von Constanz", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl wehrt sich die alte, die freie Stadt,", "tokens": ["Wohl", "wehrt", "sich", "die", "al\u00b7te", ",", "die", "frei\u00b7e", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Den herrlichen r\u00f6mischen Namen sie hat,", "tokens": ["Den", "herr\u00b7li\u00b7chen", "r\u00f6\u00b7mi\u00b7schen", "Na\u00b7men", "sie", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "PPER", "VAFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und r\u00f6mischen Mut,", "tokens": ["Und", "r\u00f6\u00b7mi\u00b7schen", "Mut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Und deutsches Blut,", "tokens": ["Und", "deut\u00b7sches", "Blut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Und Christenglauben,", "tokens": ["Und", "Chris\u00b7ten\u00b7glau\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Den soll ihr der spanische Henker nicht rauben!", "tokens": ["Den", "soll", "ihr", "der", "spa\u00b7ni\u00b7sche", "Hen\u00b7ker", "nicht", "rau\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "ART", "ADJA", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Drum k\u00e4mpfen die B\u00fcrger vom Thurm und am Thor,", "tokens": ["Drum", "k\u00e4mp\u00b7fen", "die", "B\u00fcr\u00b7ger", "vom", "Thurm", "und", "am", "Thor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "APPRART", "NN", "KON", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und dringen zur hallenden Br\u00fccke hervor,", "tokens": ["Und", "drin\u00b7gen", "zur", "hal\u00b7len\u00b7den", "Br\u00fc\u00b7cke", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Es h\u00f6rt es der Rhein,", "tokens": ["Es", "h\u00f6rt", "es", "der", "Rhein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NE", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Da rauschet er drein,", "tokens": ["Da", "rau\u00b7schet", "er", "drein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Es ruft die S\u00f6hne", "tokens": ["Es", "ruft", "die", "S\u00f6h\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Der See mit der tosenden Wellen Get\u00f6ne.", "tokens": ["Der", "See", "mit", "der", "to\u00b7sen\u00b7den", "Wel\u00b7len", "Ge\u00b7t\u00f6\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Wer streitet am k\u00fchnsten f\u00fcr Ehr' und f\u00fcr Heil?", "tokens": ["Wer", "strei\u00b7tet", "am", "k\u00fchns\u00b7ten", "f\u00fcr", "Ehr'", "und", "f\u00fcr", "Heil", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPRART", "ADJA", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Das ist der Fleischer mit hauendem Beil.", "tokens": ["Das", "ist", "der", "Flei\u00b7scher", "mit", "hau\u00b7en\u00b7dem", "Beil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sonst schl\u00e4gt er den Stier,", "tokens": ["Sonst", "schl\u00e4gt", "er", "den", "Stier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Das br\u00fcllende Thier,", "tokens": ["Das", "br\u00fcl\u00b7len\u00b7de", "Thier", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Heut mu\u00df er sie schlachten,", "tokens": ["Heut", "mu\u00df", "er", "sie", "schlach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Die ihm nach der Metzig, der blutigen, trachten.", "tokens": ["Die", "ihm", "nach", "der", "Met\u00b7zig", ",", "der", "blu\u00b7ti\u00b7gen", ",", "trach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NE", "$,", "ART", "ADJA", "$,", "VVFIN", "$."], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Er steht auf der Br\u00fccke zuvorderst im Schwarm,", "tokens": ["Er", "steht", "auf", "der", "Br\u00fc\u00b7cke", "zu\u00b7vor\u00b7derst", "im", "Schwarm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den Aermel gest\u00fclpet, mit nervigtem Arm,", "tokens": ["Den", "A\u00b7er\u00b7mel", "ge\u00b7st\u00fcl\u00b7pet", ",", "mit", "ner\u00b7vig\u00b7tem", "Arm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und jeder Streich", "tokens": ["Und", "je\u00b7der", "Streich"], "token_info": ["word", "word", "word"], "pos": ["KON", "PIAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Schl\u00e4gt einen bleich,", "tokens": ["Schl\u00e4gt", "ei\u00b7nen", "bleich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Da kommen die Andern:", "tokens": ["Da", "kom\u00b7men", "die", "An\u00b7dern", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Zur Schlachtbank l\u00e4\u00dft er sie sp\u00f6ttlich wandern.", "tokens": ["Zur", "Schlacht\u00b7bank", "l\u00e4\u00dft", "er", "sie", "sp\u00f6tt\u00b7lich", "wan\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "O weh, ihr Br\u00fcder! verlasset ihr ihn?", "tokens": ["O", "weh", ",", "ihr", "Br\u00fc\u00b7der", "!", "ver\u00b7las\u00b7set", "ihr", "ihn", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PPOSAT", "NN", "$.", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+--++-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Es doppelt der Spanischen Heer sich, sie flieh'n,", "tokens": ["Es", "dop\u00b7pelt", "der", "Spa\u00b7ni\u00b7schen", "Heer", "sich", ",", "sie", "flieh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ART", "ADJA", "NN", "PRF", "$,", "PPER", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Sie rufen ihn mit:", "tokens": ["Sie", "ru\u00b7fen", "ihn", "mit", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Doch keinen Schritt", "tokens": ["Doch", "kei\u00b7nen", "Schritt"], "token_info": ["word", "word", "word"], "pos": ["KON", "PIAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Weicht von der Stelle,", "tokens": ["Weicht", "von", "der", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Alle Feinde bek\u00e4mpfet der k\u00fchne Geselle.", "tokens": ["Al\u00b7le", "Fein\u00b7de", "be\u00b7k\u00e4mp\u00b7fet", "der", "k\u00fch\u00b7ne", "Ge\u00b7sel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}}, "stanza.6": {"line.1": {"text": "Vorn Einer und hinten da nahet ein Paar,", "tokens": ["Vorn", "Ei\u00b7ner", "und", "hin\u00b7ten", "da", "na\u00b7het", "ein", "Paar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "KON", "ADV", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die wildesten Knechte der st\u00fcrmenden Schar,", "tokens": ["Die", "wil\u00b7des\u00b7ten", "Knech\u00b7te", "der", "st\u00fcr\u00b7men\u00b7den", "Schar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Sie packen in Eil'", "tokens": ["Sie", "pa\u00b7cken", "in", "Eil'"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Des Fleischers Beil \u2013", "tokens": ["Des", "Flei\u00b7schers", "Beil", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Er ist verloren;", "tokens": ["Er", "ist", "ver\u00b7lo\u00b7ren", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Da denkt er: Soll ihnen nicht frommen, den Thoren!", "tokens": ["Da", "denkt", "er", ":", "Soll", "ih\u00b7nen", "nicht", "from\u00b7men", ",", "den", "Tho\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "VMFIN", "PPER", "PTKNEG", "ADJA", "$,", "ART", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.7": {"line.1": {"text": "Zween Arme ja hat er, die fassen die zwei:", "tokens": ["Zween", "Ar\u00b7me", "ja", "hat", "er", ",", "die", "fas\u00b7sen", "die", "zwei", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADV", "VAFIN", "PPER", "$,", "PRELS", "ADV", "ART", "CARD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und wollt ihr Ein Leben, so opfr' ich Euch drei!", "tokens": ["Und", "wollt", "ihr", "Ein", "Le\u00b7ben", ",", "so", "opf\u00b7r'", "ich", "Euch", "drei", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "PPER", "CARD", "$."], "meter": "-+-++--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Er h\u00e4lt sie umspannt,", "tokens": ["Er", "h\u00e4lt", "sie", "um\u00b7spannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Er dr\u00e4ngt sie zum Rand,", "tokens": ["Er", "dr\u00e4ngt", "sie", "zum", "Rand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Er sendet die Blicke", "tokens": ["Er", "sen\u00b7det", "die", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Hinab zu dem sch\u00e4umenden Rhein von der Br\u00fccke.", "tokens": ["Hin\u00b7ab", "zu", "dem", "sch\u00e4u\u00b7men\u00b7den", "Rhein", "von", "der", "Br\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NE", "APPR", "ART", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.8": {"line.1": {"text": "Und schnell an's Gel\u00e4nder, eh' Andere nahn,", "tokens": ["Und", "schnell", "an's", "Ge\u00b7l\u00e4n\u00b7der", ",", "eh'", "An\u00b7de\u00b7re", "nahn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "$,", "KOUS", "PIS", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Dr\u00fcckt er sie, die ringenden, kr\u00e4ftiglich an;", "tokens": ["Dr\u00fcckt", "er", "sie", ",", "die", "rin\u00b7gen\u00b7den", ",", "kr\u00e4f\u00b7tig\u00b7lich", "an", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "PRELS", "VVFIN", "$,", "ADJD", "PTKVZ", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Mit ihnen hinein", "tokens": ["Mit", "ih\u00b7nen", "hin\u00b7ein"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPER", "PTKVZ"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Kopf\u00fcber zum Rhein", "tokens": ["Kopf\u00b7\u00fc\u00b7ber", "zum", "Rhein"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NE"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Mit frohem Schwunge", "tokens": ["Mit", "fro\u00b7hem", "Schwun\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Sieht man ihn st\u00fcrzen im t\u00f6tlichen Sprunge.", "tokens": ["Sieht", "man", "ihn", "st\u00fcr\u00b7zen", "im", "t\u00f6t\u00b7li\u00b7chen", "Sprun\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.9": {"line.1": {"text": "Die klagenden Feinde verschlinget die Flut;", "tokens": ["Die", "kla\u00b7gen\u00b7den", "Fein\u00b7de", "ver\u00b7schlin\u00b7get", "die", "Flut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Lang wiegt sie, lang tr\u00e4gt sie den B\u00fcrger gut,", "tokens": ["Lang", "wiegt", "sie", ",", "lang", "tr\u00e4gt", "sie", "den", "B\u00fcr\u00b7ger", "gut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "ADJD", "VVFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.3": {"text": "Jetzt zeigt sie den Fu\u00df,", "tokens": ["Jetzt", "zeigt", "sie", "den", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Den Arm, wie zum Gru\u00df,", "tokens": ["Den", "Arm", ",", "wie", "zum", "Gru\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "APPRART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Die Schultern, die blanken,", "tokens": ["Die", "Schul\u00b7tern", ",", "die", "blan\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Das lockigte Haupt und den Nacken, den schlanken.", "tokens": ["Das", "lo\u00b7ckig\u00b7te", "Haupt", "und", "den", "Na\u00b7cken", ",", "den", "schlan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "KON", "ART", "NN", "$,", "ART", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.10": {"line.1": {"text": "Da sucht ihn das fremde Gescho\u00df, doch der Rhein,", "tokens": ["Da", "sucht", "ihn", "das", "frem\u00b7de", "Ge\u00b7scho\u00df", ",", "doch", "der", "Rhein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,", "ADV", "ART", "NE", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "H\u00fcllt fromm in den Mantel, den gr\u00fcnen, ihn ein.", "tokens": ["H\u00fcllt", "fromm", "in", "den", "Man\u00b7tel", ",", "den", "gr\u00fc\u00b7nen", ",", "ihn", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "$,", "ART", "ADJA", "$,", "PPER", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Er zieht ihn hinab", "tokens": ["Er", "zieht", "ihn", "hin\u00b7ab"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "In's festliche Grab,", "tokens": ["In's", "fest\u00b7li\u00b7che", "Grab", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Dort ruht er geborgen", "tokens": ["Dort", "ruht", "er", "ge\u00b7bor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVPP"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Vor feindlicher Schmach bis zum ewigen Morgen.", "tokens": ["Vor", "feind\u00b7li\u00b7cher", "Schmach", "bis", "zum", "e\u00b7wi\u00b7gen", "Mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.11": {"line.1": {"text": "Dort schl\u00e4ft ohne Traum er den s\u00fc\u00dfesten Schlaf,", "tokens": ["Dort", "schl\u00e4ft", "oh\u00b7ne", "Traum", "er", "den", "s\u00fc\u00b7\u00dfes\u00b7ten", "Schlaf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Er wei\u00df nicht das Loos, das die Heimat ihm traf.", "tokens": ["Er", "wei\u00df", "nicht", "das", "Loos", ",", "das", "die", "Hei\u00b7mat", "ihm", "traf", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Man tr\u00fcgt, man raubt", "tokens": ["Man", "tr\u00fcgt", ",", "man", "raubt"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "PIS", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Ob seinem Haupt", "tokens": ["Ob", "sei\u00b7nem", "Haupt"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Freiheit und Glauben;", "tokens": ["Frei\u00b7heit", "und", "Glau\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Die M\u00e4rtyrerkrone wird keiner ihm rauben.", "tokens": ["Die", "M\u00e4r\u00b7ty\u00b7rer\u00b7kro\u00b7ne", "wird", "kei\u00b7ner", "ihm", "rau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIS", "PPE