{"textgrid.poem.43326": {"metadata": {"author": {"name": "Gerstenberg, Heinrich Wilhelm von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich f\u00fchls, es k\u00e4mpfen in mir die schon verwandelten Glieder!", "genre": "verse", "period": "N.A.", "pub_year": 1780, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich f\u00fchls, es k\u00e4mpfen in mir die schon verwandelten Glieder!", "tokens": ["Ich", "f\u00fchls", ",", "es", "k\u00e4mp\u00b7fen", "in", "mir", "die", "schon", "ver\u00b7wan\u00b7del\u00b7ten", "Glie\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PPER", "VVFIN", "APPR", "PPER", "ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+---+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ich f\u00fchls, der Mensch h\u00f6rt auf in mir!", "tokens": ["Ich", "f\u00fchls", ",", "der", "Mensch", "h\u00f6rt", "auf", "in", "mir", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ART", "NN", "VVFIN", "APPR", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da schwimm ich durch Wolken dahin mit neuerschaffnem Gefieder,", "tokens": ["Da", "schwimm", "ich", "durch", "Wol\u00b7ken", "da\u00b7hin", "mit", "neu\u00b7er\u00b7schaff\u00b7nem", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "PAV", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Zu stolz vor niedrigerm Revier!", "tokens": ["Zu", "stolz", "vor", "nied\u00b7ri\u00b7germ", "Re\u00b7vier", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich eil im unbeflogne H\u00f6hen!", "tokens": ["Ich", "eil", "im", "un\u00b7be\u00b7flog\u00b7ne", "H\u00f6\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Kaum kann mich Hammons Adler sehen!", "tokens": ["Kaum", "kann", "mich", "Ham\u00b7mons", "Ad\u00b7ler", "se\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PRF", "NE", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Itzt werd ich, schnell wie der Ost, die Getulischen Syrten erfliegen,", "tokens": ["Itzt", "werd", "ich", ",", "schnell", "wie", "der", "Ost", ",", "die", "Ge\u00b7tu\u00b7li\u00b7schen", "Syr\u00b7ten", "er\u00b7flie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "ADJD", "KOKOM", "ART", "NN", "$,", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Und itzt des Bosphors engen Strand!", "tokens": ["Und", "itzt", "des", "Bosp\u00b7hors", "en\u00b7gen", "Strand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Seh itzt die scytischen W\u00fcsten erstarrt tief unter mir liegen,", "tokens": ["Seh", "itzt", "die", "scy\u00b7ti\u00b7schen", "W\u00fcs\u00b7ten", "er\u00b7starrt", "tief", "un\u00b7ter", "mir", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "VVFIN", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Und itzt der Marser d\u00fcrres Land!", "tokens": ["Und", "itzt", "der", "Mar\u00b7ser", "d\u00fcr\u00b7res", "Land", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mich sollen ferne Perser kennen!", "tokens": ["Mich", "sol\u00b7len", "fer\u00b7ne", "Per\u00b7ser", "ken\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mich Indier mit Ehrfurcht nennen.", "tokens": ["Mich", "In\u00b7dier", "mit", "Ehr\u00b7furcht", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "NN", "VVINF", "$."], "meter": "--+--+--", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Entweiht, entweihet mich nicht mit euren sch\u00e4ndenden Z\u00e4hren!", "tokens": ["Ent\u00b7weiht", ",", "ent\u00b7wei\u00b7het", "mich", "nicht", "mit", "eu\u00b7ren", "sch\u00e4n\u00b7den\u00b7den", "Z\u00e4h\u00b7ren", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Bez\u00e4hmt, bez\u00e4hmt das niedre Leid!", "tokens": ["Be\u00b7z\u00e4hmt", ",", "be\u00b7z\u00e4hmt", "das", "nied\u00b7re", "Leid", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was brauchts des festlichen Grams, und der zu irdischen Ehren,", "tokens": ["Was", "brauchts", "des", "fest\u00b7li\u00b7chen", "Grams", ",", "und", "der", "zu", "ir\u00b7di\u00b7schen", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "$,", "KON", "ART", "PTKZU", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und eures Grablieds Ewigkeit!", "tokens": ["Und", "eu\u00b7res", "Grab\u00b7li\u00b7eds", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Hoch \u00fcber Wolken hingetragen,", "tokens": ["Hoch", "\u00fc\u00b7ber", "Wol\u00b7ken", "hin\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Werd ich ein Spatz an Venus Wagen!", "tokens": ["Werd", "ich", "ein", "Spatz", "an", "Ve\u00b7nus", "Wa\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}