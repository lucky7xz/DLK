{"dta.poem.11844": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "21.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In jedem Augenblick, wo ich von meiner Seite", "tokens": ["In", "je\u00b7dem", "Au\u00b7gen\u00b7blick", ",", "wo", "ich", "von", "mei\u00b7ner", "Sei\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "PWAV", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Lie\u00df gehn ein liebes Kind in seines Gl\u00fccks Geleite,", "tokens": ["Lie\u00df", "gehn", "ein", "lie\u00b7bes", "Kind", "in", "sei\u00b7nes", "Gl\u00fccks", "Ge\u00b7lei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Bis zu dem Augenblick, wo ich es wieder finde;", "tokens": ["Bis", "zu", "dem", "Au\u00b7gen\u00b7blick", ",", "wo", "ich", "es", "wie\u00b7der", "fin\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "$,", "PWAV", "PPER", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie mancher Unfall gieng vor\u00fcber meinem Kinde!", "tokens": ["Wie", "man\u00b7cher", "Un\u00b7fall", "gieng", "vor\u00b7\u00fc\u00b7ber", "mei\u00b7nem", "Kin\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wie vielmal mir zur\u00fcck geschenkt hab' ichs erhalten", "tokens": ["Wie", "viel\u00b7mal", "mir", "zu\u00b7r\u00fcck", "ge\u00b7schenkt", "hab'", "ichs", "er\u00b7hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PPER", "PTKVZ", "VVPP", "VAFIN", "PIS", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von th\u00e4tig ihm zum Schutz gewesenen Gewalten!", "tokens": ["Von", "th\u00e4\u00b7tig", "ihm", "zum", "Schutz", "ge\u00b7we\u00b7se\u00b7nen", "Ge\u00b7wal\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "PPER", "APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nur vielfach lie\u00dfen sie das Schutzgeld auch in Qualen", "tokens": ["Nur", "viel\u00b7fach", "lie\u00b7\u00dfen", "sie", "das", "Schutz\u00b7geld", "auch", "in", "Qua\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "NN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mich mit vergeblichen Besorgnissen bezahlen.", "tokens": ["Mich", "mit", "ver\u00b7geb\u00b7li\u00b7chen", "Be\u00b7sorg\u00b7nis\u00b7sen", "be\u00b7zah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}