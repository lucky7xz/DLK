{"dta.poem.2836": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "37.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Erw\u00e4g' an jeder Frucht, was dient zu deinem Male?", "tokens": ["Er\u00b7w\u00e4g'", "an", "je\u00b7der", "Frucht", ",", "was", "dient", "zu", "dei\u00b7nem", "Ma\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "$,", "PWS", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von einer ists der Kern, von anderer die Schale.", "tokens": ["Von", "ei\u00b7ner", "ists", "der", "Kern", ",", "von", "an\u00b7de\u00b7rer", "die", "Scha\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "APPR", "PIS", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Verst\u00e4ndig ist, wer das genie\u00dfet, was ihm taugt,", "tokens": ["Ver\u00b7st\u00e4n\u00b7dig", "ist", ",", "wer", "das", "ge\u00b7nie\u00b7\u00dfet", ",", "was", "ihm", "taugt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$,", "PWS", "PDS", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den markigen Kern aufknackt, das saftige Fleisch ansaugt.", "tokens": ["Den", "mar\u00b7ki\u00b7gen", "Kern", "auf\u00b7knackt", ",", "das", "saf\u00b7ti\u00b7ge", "Fleisch", "an\u00b7saugt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Ein Thor, wer dieses Fleisch und jenes Mark wegschmei\u00dft,", "tokens": ["Ein", "Thor", ",", "wer", "die\u00b7ses", "Fleisch", "und", "je\u00b7nes", "Mark", "weg\u00b7schmei\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWS", "PDAT", "NN", "KON", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Daf\u00fcr hier harten Stein, dort herbe Rinde bei\u00dft.", "tokens": ["Da\u00b7f\u00fcr", "hier", "har\u00b7ten", "Stein", ",", "dort", "her\u00b7be", "Rin\u00b7de", "bei\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ADJA", "NN", "$,", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}