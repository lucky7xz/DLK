{"dta.poem.19560": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Ihr Heim.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Lang vor\u00fcber ging ich den Gehegen,", "tokens": ["Lang", "vor\u00b7\u00fc\u00b7ber", "ging", "ich", "den", "Ge\u00b7he\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Drin der Giebel deines Heimes ragt,", "tokens": ["Drin", "der", "Gie\u00b7bel", "dei\u00b7nes", "Hei\u00b7mes", "ragt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Dieser Pforte, diesen Schattenwegen!", "tokens": ["Die\u00b7ser", "Pfor\u00b7te", ",", "die\u00b7sen", "Schat\u00b7ten\u00b7we\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PDAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wer da wohne, hab' ich nicht gefragt.", "tokens": ["Wer", "da", "woh\u00b7ne", ",", "hab'", "ich", "nicht", "ge\u00b7fragt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$,", "VAFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Wer da wohne", "tokens": ["Wer", "da", "woh\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["PWS", "ADV", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Hinter einer dunkeln Lindenkrone,", "tokens": ["Hin\u00b7ter", "ei\u00b7ner", "dun\u00b7keln", "Lin\u00b7den\u00b7kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Hat das Herz mir nicht vorausgesagt.", "tokens": ["Hat", "das", "Herz", "mir", "nicht", "vor\u00b7aus\u00b7ge\u00b7sagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Pfade liefen durch die feuchte Wiese,", "tokens": ["Pfa\u00b7de", "lie\u00b7fen", "durch", "die", "feuch\u00b7te", "Wie\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Kleine Sohlen sah ich hier und dort", "tokens": ["Klei\u00b7ne", "Soh\u00b7len", "sah", "ich", "hier", "und", "dort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "ADV", "KON", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Eingezeichnet auf dem weichen Kiese,", "tokens": ["Ein\u00b7ge\u00b7zeich\u00b7net", "auf", "dem", "wei\u00b7chen", "Kie\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Aber meines Weges zog ich fort.", "tokens": ["A\u00b7ber", "mei\u00b7nes", "We\u00b7ges", "zog", "ich", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Ich begehrte", "tokens": ["Ich", "be\u00b7gehr\u00b7te"], "token_info": ["word", "word"], "pos": ["PPER", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Zu verfolgen nicht die fl\u00fccht'ge F\u00e4hrte,", "tokens": ["Zu", "ver\u00b7fol\u00b7gen", "nicht", "die", "fl\u00fccht'\u00b7ge", "F\u00e4hr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Zu betreten nicht den stummen Ort.", "tokens": ["Zu", "be\u00b7tre\u00b7ten", "nicht", "den", "stum\u00b7men", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Auch ein Rauschen h\u00f6rt' ich aus der Linde,", "tokens": ["Auch", "ein", "Rau\u00b7schen", "h\u00f6rt'", "ich", "aus", "der", "Lin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NE", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die der Hauch der Abendl\u00fcfte bog;", "tokens": ["Die", "der", "Hauch", "der", "A\u00b7ben\u00b7dl\u00fcf\u00b7te", "bog", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "\u201ekomme, Wandrer\u201c, rief es, \u201ekomm und finde!\u201c", "tokens": ["\u201e", "kom\u00b7me", ",", "Wand\u00b7rer", "\u201c", ",", "rief", "es", ",", "\u201e", "komm", "und", "fin\u00b7de", "!", "\u201c"], "token_info": ["punct", "word", "punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "$,", "NN", "$(", "$,", "VVFIN", "PPER", "$,", "$(", "VVFIN", "KON", "VVFIN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "W\u00e4hrend rascher ich des Weges zog.", "tokens": ["W\u00e4h\u00b7rend", "ra\u00b7scher", "ich", "des", "We\u00b7ges", "zog", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Ich vertraute", "tokens": ["Ich", "ver\u00b7trau\u00b7te"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Dem Versprechen nicht der Geisterlaute,", "tokens": ["Dem", "Ver\u00b7spre\u00b7chen", "nicht", "der", "Geis\u00b7ter\u00b7lau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Deren Wehn mir oft das Herz betrog.", "tokens": ["De\u00b7ren", "Wehn", "mir", "oft", "das", "Herz", "be\u00b7trog", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Und den Stern der Liebe sah ich eilen", "tokens": ["Und", "den", "Stern", "der", "Lie\u00b7be", "sah", "ich", "ei\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVFIN", "PPER", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dort zum dunkelscharfen Bergesrand,", "tokens": ["Dort", "zum", "dun\u00b7kel\u00b7schar\u00b7fen", "Ber\u00b7ges\u00b7rand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Auf dem schlanken Giebel blitzend weilen,", "tokens": ["Auf", "dem", "schlan\u00b7ken", "Gie\u00b7bel", "blit\u00b7zend", "wei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wie ein zitternd Feuer, eh' er schwand.", "tokens": ["Wie", "ein", "zit\u00b7ternd", "Feu\u00b7er", ",", "eh'", "er", "schwand", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJD", "NN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Im Entweichen", "tokens": ["Im", "Ent\u00b7wei\u00b7chen"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Gab der Freund am Himmel mir ein Zeichen,", "tokens": ["Gab", "der", "Freund", "am", "Him\u00b7mel", "mir", "ein", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Wann er \u00fcber meinem Gl\u00fccke stand.", "tokens": ["Wann", "er", "\u00fc\u00b7ber", "mei\u00b7nem", "Gl\u00fc\u00b7cke", "stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "L\u00e4ngst versunken glaubt' ich's in der Ferne,", "tokens": ["L\u00e4ngst", "ver\u00b7sun\u00b7ken", "glaubt'", "ich's", "in", "der", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VVFIN", "PIS", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Das so nahe mir verborgen lag!", "tokens": ["Das", "so", "na\u00b7he", "mir", "ver\u00b7bor\u00b7gen", "lag", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPR", "PPER", "VVPP", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wer versteht den stillen Wink der Sterne", "tokens": ["Wer", "ver\u00b7steht", "den", "stil\u00b7len", "Wink", "der", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Vor dem rechten, dem bestimmten Tag?", "tokens": ["Vor", "dem", "rech\u00b7ten", ",", "dem", "be\u00b7stimm\u00b7ten", "Tag", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Vor der Stunde,", "tokens": ["Vor", "der", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Die ihn zieht zu dem ersehnten Bunde,", "tokens": ["Die", "ihn", "zieht", "zu", "dem", "er\u00b7sehn\u00b7ten", "Bun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Den nicht Tod noch Leben trennen mag?", "tokens": ["Den", "nicht", "Tod", "noch", "Le\u00b7ben", "tren\u00b7nen", "mag", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "NN", "ADV", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Lang vor\u00fcber ging ich deiner Liebe", "tokens": ["Lang", "vor\u00b7\u00fc\u00b7ber", "ging", "ich", "dei\u00b7ner", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Durch den Staub des Lebens unbewu\u00dft,", "tokens": ["Durch", "den", "Staub", "des", "Le\u00b7bens", "un\u00b7be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Da\u00df zur Wonne mir die Klage bliebe", "tokens": ["Da\u00df", "zur", "Won\u00b7ne", "mir", "die", "Kla\u00b7ge", "blie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "PPER", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und ein leiser Schmerz in sel'ger Brust \u2014", "tokens": ["Und", "ein", "lei\u00b7ser", "Schmerz", "in", "sel'\u00b7ger", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Schmerz und Klage", "tokens": ["Schmerz", "und", "Kla\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Ueber ohne dich verdarbte Tage,", "tokens": ["Ue\u00b7ber", "oh\u00b7ne", "dich", "ver\u00b7darb\u00b7te", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PPER", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Die mit deinem Ku\u00df du stillen mu\u00dft.", "tokens": ["Die", "mit", "dei\u00b7nem", "Ku\u00df", "du", "stil\u00b7len", "mu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}