{"dta.poem.5364": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Abend-Andacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da nunmehr des Tages Gl\u00e4ntzen und der Sonnen", "tokens": ["Da", "nun\u00b7mehr", "des", "Ta\u00b7ges", "Gl\u00e4nt\u00b7zen", "und", "der", "Son\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "helle Pracht", "tokens": ["hel\u00b7le", "Pracht"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Unsern Augen sich entzogen; da die Schatten-reiche Nacht,", "tokens": ["Un\u00b7sern", "Au\u00b7gen", "sich", "ent\u00b7zo\u00b7gen", ";", "da", "die", "Schat\u00b7ten\u00b7rei\u00b7che", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "VVPP", "$.", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Mit den stillen Finsternissen, Himmel, Erd\u2019 und Meer", "tokens": ["Mit", "den", "stil\u00b7len", "Fins\u00b7ter\u00b7nis\u00b7sen", ",", "Him\u00b7mel", ",", "Erd'", "und", "Meer"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.5": {"text": "erf\u00fcllt,", "tokens": ["er\u00b7f\u00fcllt", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Und der irdischen Gesch\u00f6pfe Farb\u2019 und Bildung gantz", "tokens": ["Und", "der", "ir\u00b7di\u00b7schen", "Ge\u00b7sch\u00f6p\u00b7fe", "Fa\u00b7rb'", "und", "Bil\u00b7dung", "gantz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "NN", "KON", "NN", "ADV"], "meter": "--+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "verh\u00fcllt,", "tokens": ["ver\u00b7h\u00fcllt", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Unsern Leib zur Ruhe ruft, um, nach heut vollbrachten", "tokens": ["Un\u00b7sern", "Leib", "zur", "Ru\u00b7he", "ruft", ",", "um", ",", "nach", "heut", "voll\u00b7brach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$,", "KOUI", "$,", "APPR", "ADV", "ADJA"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.9": {"text": "Wercken,", "tokens": ["Wer\u00b7cken", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.10": {"text": "Jhn, durch einen sanften Schlaf, kr\u00e4ftig wiederum zu", "tokens": ["Jhn", ",", "durch", "ei\u00b7nen", "sanf\u00b7ten", "Schlaf", ",", "kr\u00e4f\u00b7tig", "wie\u00b7de\u00b7rum", "zu"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "APPR", "ART", "ADJA", "NN", "$,", "ADJD", "ADV", "APPR"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "st\u00e4rcken,", "tokens": ["st\u00e4r\u00b7cken", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.12": {"text": "Und ich, voll Beqvemlichkeit, auf so weichen Feder-Decken,", "tokens": ["Und", "ich", ",", "voll", "Be\u00b7qvem\u00b7lich\u00b7keit", ",", "auf", "so", "wei\u00b7chen", "Fe\u00b7der\u00b7De\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ADJD", "NN", "$,", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-++-+--+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.13": {"text": "Sonder Sorgen, Furcht und Gram, mich gesund vermag", "tokens": ["Son\u00b7der", "Sor\u00b7gen", ",", "Furcht", "und", "Gram", ",", "mich", "ge\u00b7sund", "ver\u00b7mag"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "$,", "NN", "KON", "NN", "$,", "PPER", "ADJD", "VVFIN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.14": {"text": "zu strecken:", "tokens": ["zu", "stre\u00b7cken", ":"], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.15": {"text": "Auf mein Geist! den grossen Sch\u00f6pfer der den Himmel und", "tokens": ["Auf", "mein", "Geist", "!", "den", "gros\u00b7sen", "Sch\u00f6p\u00b7fer", "der", "den", "Him\u00b7mel", "und"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$.", "ART", "ADJA", "NN", "ART", "ART", "NN", "KON"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.16": {"text": "die Welt,", "tokens": ["die", "Welt", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.17": {"text": "Bey so wunderbarem Wechsel, in so richt\u2019ger Ordnung", "tokens": ["Bey", "so", "wun\u00b7der\u00b7ba\u00b7rem", "Wech\u00b7sel", ",", "in", "so", "richt'\u00b7ger", "Ord\u00b7nung"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,", "APPR", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.18": {"text": "h\u00e4lt,", "tokens": ["h\u00e4lt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.19": {"text": "Der die stille Nacht zur Ruhe dir geg\u00f6nnet, zu besingen!", "tokens": ["Der", "die", "stil\u00b7le", "Nacht", "zur", "Ru\u00b7he", "dir", "ge\u00b7g\u00f6n\u00b7net", ",", "zu", "be\u00b7sin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "APPRART", "NN", "PPER", "VVPP", "$,", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.20": {"text": "Und ein schuldig Abend-Opfer seiner Lieb\u2019 und Macht zu", "tokens": ["Und", "ein", "schul\u00b7dig", "A\u00b7ben\u00b7dOp\u00b7fer", "sei\u00b7ner", "Lieb'", "und", "Macht", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJD", "NN", "PPOSAT", "NN", "KON", "NN", "APPR"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.21": {"text": "bringen!", "tokens": ["brin\u00b7gen", "!"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.22": {"text": "Dieses kann nun f\u00fcglicher, wie du selber wirst gestehn,", "tokens": ["Die\u00b7ses", "kann", "nun", "f\u00fcg\u00b7li\u00b7cher", ",", "wie", "du", "sel\u00b7ber", "wirst", "ge\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "PTKVZ", "$,", "PWAV", "PPER", "ADV", "VAFIN", "VVPP", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.23": {"text": "Wenn du redlich danckst, mein Geist, als durch Dancken,", "tokens": ["Wenn", "du", "red\u00b7lich", "danckst", ",", "mein", "Geist", ",", "als", "durch", "Dan\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "$,", "PPOSAT", "NN", "$,", "KOUS", "APPR", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.24": {"text": "nicht geschehn.", "tokens": ["nicht", "ge\u00b7schehn", "."], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.25": {"text": "Will man aber redlich Dancken; mu\u00df man, mit vergn\u00fcg-", "tokens": ["Will", "man", "a\u00b7ber", "red\u00b7lich", "Dan\u00b7cken", ";", "mu\u00df", "man", ",", "mit", "ver\u00b7gn\u00fcg"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "PIS", "ADV", "ADJD", "NN", "$.", "VMFIN", "PIS", "$,", "APPR", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.26": {"text": "tem Muth,", "tokens": ["tem", "Muth", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.27": {"text": "Sich mit rechtem Ernst bestreben, das von ", "tokens": ["Sich", "mit", "rech\u00b7tem", "Ernst", "be\u00b7stre\u00b7ben", ",", "das", "von"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PRF", "APPR", "ADJA", "NN", "VVPP", "$,", "PRELS", "APPR"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.28": {"text": "empfangne Gut,", "tokens": ["emp\u00b7fang\u00b7ne", "Gut", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}}}}}