{"dta.poem.8841": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Xx.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Doch alles ist vergebens, ihr Alle seyd es nicht!", "tokens": ["Doch", "al\u00b7les", "ist", "ver\u00b7ge\u00b7bens", ",", "ihr", "Al\u00b7le", "seyd", "es", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ADV", "$,", "PPER", "PIS", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ihr blauen Augen werdet nie meine Sterne seyn,", "tokens": ["Ihr", "blau\u00b7en", "Au\u00b7gen", "wer\u00b7det", "nie", "mei\u00b7ne", "Ster\u00b7ne", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ein schwarzes Auge wei\u00df ich, aus diesem saug' ich Licht.", "tokens": ["Ein", "schwar\u00b7zes", "Au\u00b7ge", "wei\u00df", "ich", ",", "aus", "die\u00b7sem", "saug'", "ich", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "$,", "APPR", "PDAT", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ein hartes Wort bef\u00fcrcht' ich von deinem spr\u00f6den Mund,", "tokens": ["Ein", "har\u00b7tes", "Wort", "be\u00b7f\u00fcrcht'", "ich", "von", "dei\u00b7nem", "spr\u00f6\u00b7den", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+----+-+-+", "measure": "unknown.measure.penta"}, "line.5": {"text": "D'rum la\u00df die Lippe schweigen, so lang das Auge spricht!", "tokens": ["D'\u00b7rum", "la\u00df", "die", "Lip\u00b7pe", "schwei\u00b7gen", ",", "so", "lang", "das", "Au\u00b7ge", "spricht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "VVINF", "$,", "ADV", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Die Sonne w\u00e4rmet Steine, wie sollte nicht dein Aug'", "tokens": ["Die", "Son\u00b7ne", "w\u00e4r\u00b7met", "Stei\u00b7ne", ",", "wie", "soll\u00b7te", "nicht", "dein", "Aug'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "NN", "$,", "PWAV", "VMFIN", "PTKNEG", "PPOSAT", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Ein Herz erw\u00e4rmen, dem es an W\u00e4rme nie gebricht?", "tokens": ["Ein", "Herz", "er\u00b7w\u00e4r\u00b7men", ",", "dem", "es", "an", "W\u00e4r\u00b7me", "nie", "ge\u00b7bricht", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "PRELS", "PPER", "APPR", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Doch rath' ich dir, vertraue dem Geiste nicht zu sehr,", "tokens": ["Doch", "ra\u00b7th'", "ich", "dir", ",", "ver\u00b7trau\u00b7e", "dem", "Geis\u00b7te", "nicht", "zu", "sehr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "$,", "VVFIN", "ART", "NN", "PTKNEG", "PTKA", "ADV", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.9": {"text": "Der, fl\u00fccht'ger als die Rose, nur fl\u00fccht'ge Bande flicht;", "tokens": ["Der", ",", "fl\u00fccht'\u00b7ger", "als", "die", "Ro\u00b7se", ",", "nur", "fl\u00fccht'\u00b7ge", "Ban\u00b7de", "flicht", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADJD", "KOKOM", "ART", "NN", "$,", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Der gern erproben m\u00f6chte die ganze Welt umher,", "tokens": ["Der", "gern", "er\u00b7pro\u00b7ben", "m\u00f6ch\u00b7te", "die", "gan\u00b7ze", "Welt", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVINF", "VMFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Dem nach so viel gel\u00fcstet, den ach! so viel besticht.", "tokens": ["Dem", "nach", "so", "viel", "ge\u00b7l\u00fcs\u00b7tet", ",", "den", "ach", "!", "so", "viel", "be\u00b7sticht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADV", "ADV", "VVPP", "$,", "ART", "XY", "$.", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Allein was sag' ich? Flehen um Liebe sollt' ich dich,", "tokens": ["Al\u00b7lein", "was", "sag'", "ich", "?", "Fle\u00b7hen", "um", "Lie\u00b7be", "sollt'", "ich", "dich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VVFIN", "PPER", "$.", "NN", "APPR", "NN", "VMFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Denn dich vor mir zu warnen, ist \u00fcber meine Pflicht!", "tokens": ["Denn", "dich", "vor", "mir", "zu", "war\u00b7nen", ",", "ist", "\u00fc\u00b7ber", "mei\u00b7ne", "Pflicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPER", "PTKZU", "VVINF", "$,", "VAFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Mein leichtes Wesen h\u00e4tte sich l\u00e4ngst, wie Spreu, zer-", "tokens": ["Mein", "leich\u00b7tes", "We\u00b7sen", "h\u00e4t\u00b7te", "sich", "l\u00e4ngst", ",", "wie", "Spreu", ",", "zer"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PRF", "ADV", "$,", "PWAV", "ADJD", "$,", "TRUNC"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Doch Schmerz um deine Liebe verleiht mir noch Gewicht.", "tokens": ["Doch", "Schmerz", "um", "dei\u00b7ne", "Lie\u00b7be", "ver\u00b7leiht", "mir", "noch", "Ge\u00b7wicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}