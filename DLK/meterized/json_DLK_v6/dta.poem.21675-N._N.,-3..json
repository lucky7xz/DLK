{"dta.poem.21675": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn die Sonne erkaltet,", "tokens": ["Wenn", "die", "Son\u00b7ne", "er\u00b7kal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Die Sterne veraltet", "tokens": ["Die", "Ster\u00b7ne", "ver\u00b7al\u00b7tet"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVPP"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und das Buch des Gerichtes sich entfaltet,", "tokens": ["Und", "das", "Buch", "des", "Ge\u00b7rich\u00b7tes", "sich", "ent\u00b7fal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Wenn \u00fcber Al-Sirats Flammenbr\u00fccke", "tokens": ["Wenn", "\u00fc\u00b7ber", "Al\u00b7Si\u00b7rats", "Flam\u00b7men\u00b7br\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NE", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ich siegreich z\u00f6ge \u2014 ich fragte schnelle:", "tokens": ["Ich", "sieg\u00b7reich", "z\u00f6\u00b7ge", "ich", "frag\u00b7te", "schnel\u00b7le", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "VVFIN", "$(", "PPER", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "\u201ezuleika, die makellose Gazelle,", "tokens": ["\u201e", "zu\u00b7lei\u00b7ka", ",", "die", "ma\u00b7kel\u00b7lo\u00b7se", "Ga\u00b7zel\u00b7le", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Wird sie schwelgen mit mir in ewigem Gl\u00fccke?\u201c", "tokens": ["Wird", "sie", "schwel\u00b7gen", "mit", "mir", "in", "e\u00b7wi\u00b7gem", "Gl\u00fc\u00b7cke", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "APPR", "PPER", "APPR", "ADJA", "NN", "$.", "$("], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "\u201enein!\u201c ist die Antwort, \u201edenn wie des Dschemschid", "tokens": ["\u201e", "nein", "!", "\u201c", "ist", "die", "Ant\u00b7wort", ",", "\u201e", "denn", "wie", "des", "Dschem\u00b7schid"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["$(", "PTKANT", "$.", "$(", "VAFIN", "ART", "NN", "$,", "$(", "KON", "PWAV", "ART", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.9": {"text": "Rubin die unsterbliche Seele gl\u00fcht \u2014", "tokens": ["Ru\u00b7bin", "die", "uns\u00b7terb\u00b7li\u00b7che", "See\u00b7le", "gl\u00fcht"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+---+--+-+", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "Doch nur in des Mannes Brust. Das Weib", "tokens": ["Doch", "nur", "in", "des", "Man\u00b7nes", "Brust", ".", "Das", "Weib"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "NN", "$.", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Endet, zerfiel sein sterblicher Leib.", "tokens": ["En\u00b7det", ",", "zer\u00b7fiel", "sein", "sterb\u00b7li\u00b7cher", "Leib", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.12": {"text": "Hier winkt dir die Houri unsterblich-sch\u00f6n.\u201c", "tokens": ["Hier", "winkt", "dir", "die", "Hou\u00b7ri", "un\u00b7sterb\u00b7lich\u00b7sch\u00f6n", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NE", "NE", "$.", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.13": {"text": "Lebt wohl denn f\u00fcr immer, ihr Himmelsh\u00f6hn!", "tokens": ["Lebt", "wohl", "denn", "f\u00fcr", "im\u00b7mer", ",", "ihr", "Him\u00b7mels\u00b7h\u00f6hn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "ADV", "$,", "PPOSAT", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.14": {"text": "Ich st\u00fcrze mich selbst in des Eblis H\u00f6lle,", "tokens": ["Ich", "st\u00fcr\u00b7ze", "mich", "selbst", "in", "des", "Eb\u00b7lis", "H\u00f6l\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ART", "NE", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.15": {"text": "Ins ewige Feuer und Lavager\u00f6lle.", "tokens": ["Ins", "e\u00b7wi\u00b7ge", "Feu\u00b7er", "und", "La\u00b7va\u00b7ge\u00b7r\u00f6l\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "KON", "NE", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.16": {"text": "Der irdischen Liebe bin ich geweiht", "tokens": ["Der", "ir\u00b7di\u00b7schen", "Lie\u00b7be", "bin", "ich", "ge\u00b7weiht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "VVPP"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Und der sterblichen schwachen Weiblichkeit!", "tokens": ["Und", "der", "sterb\u00b7li\u00b7chen", "schwa\u00b7chen", "Weib\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}}}}}