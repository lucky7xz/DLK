{"textgrid.poem.36166": {"metadata": {"author": {"name": "Kosegarten, Gotthard Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Getreues Deutschland, zittre nicht,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Getreues Deutschland, zittre nicht,", "tokens": ["Ge\u00b7treu\u00b7es", "Deutschland", ",", "zitt\u00b7re", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "VVFIN", "PTKNEG", "$,"], "meter": "-+----+", "measure": "dactylic.init"}, "line.2": {"text": "Mag dir der Feind gleich dr\u00e4uen!", "tokens": ["Mag", "dir", "der", "Feind", "gleich", "dr\u00e4u\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Schau unverzagt ins Angesicht", "tokens": ["Schau", "un\u00b7ver\u00b7zagt", "ins", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem wuthentflammten Leuen!", "tokens": ["Dem", "wut\u00b7hent\u00b7flamm\u00b7ten", "Leu\u00b7en", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nicht lenkt der Mensch den Krieg.", "tokens": ["Nicht", "lenkt", "der", "Mensch", "den", "Krieg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Gott giebt und nimmt den Sieg.", "tokens": ["Gott", "giebt", "und", "nimmt", "den", "Sieg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Doch den getrosten Muth,", "tokens": ["Doch", "den", "ge\u00b7tros\u00b7ten", "Muth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Den Muth der Wunder thut,", "tokens": ["Den", "Muth", "der", "Wun\u00b7der", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Giebt die gerechte Sache!", "tokens": ["Giebt", "die", "ge\u00b7rech\u00b7te", "Sa\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Gerecht ist deiner Krieger Kampf,", "tokens": ["Ge\u00b7recht", "ist", "dei\u00b7ner", "Krie\u00b7ger", "Kampf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gerecht dein Kampf, das glaube!", "tokens": ["Ge\u00b7recht", "dein", "Kampf", ",", "das", "glau\u00b7be", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "PDS", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nicht lockt uns eiteln Ruhmes Dampf,", "tokens": ["Nicht", "lockt", "uns", "ei\u00b7teln", "Ruh\u00b7mes", "Dampf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht schn\u00f6de Lust zum Raube.", "tokens": ["Nicht", "schn\u00f6\u00b7de", "Lust", "zum", "Rau\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es gilt f\u00fcr Seel' und Leib,", "tokens": ["Es", "gilt", "f\u00fcr", "Seel'", "und", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "F\u00fcr Gut, Ehr', Kind und Weib,", "tokens": ["F\u00fcr", "Gut", ",", "Ehr'", ",", "Kind", "und", "Weib", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "F\u00fcr Freiheit, Kirch' und Heerd,", "tokens": ["F\u00fcr", "Frei\u00b7heit", ",", "Kirch'", "und", "Heerd", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "F\u00fcr alles was uns werth,", "tokens": ["F\u00fcr", "al\u00b7les", "was", "uns", "werth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PWS", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Was k\u00f6stlich uns und heilig!", "tokens": ["Was", "k\u00f6st\u00b7lich", "uns", "und", "hei\u00b7lig", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "PPER", "KON", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "D'rum denke der vergang'nen Zeit,", "tokens": ["D'\u00b7rum", "den\u00b7ke", "der", "ver\u00b7gang'\u00b7nen", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Gedenk der gro\u00dfen Ahnen,", "tokens": ["Ge\u00b7denk", "der", "gro\u00b7\u00dfen", "Ah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und la\u00df der V\u00e4ter T\u00fcchtigkeit", "tokens": ["Und", "la\u00df", "der", "V\u00e4\u00b7ter", "T\u00fcch\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu gleichem Ernst dich mahnen!", "tokens": ["Zu", "glei\u00b7chem", "Ernst", "dich", "mah\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mild war ihr Sinn im Rath,", "tokens": ["Mild", "war", "ihr", "Sinn", "im", "Rath", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ein Wetterstrahl die That.", "tokens": ["Ein", "Wet\u00b7ter\u00b7strahl", "die", "That", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Es wagten Herr und Knecht", "tokens": ["Es", "wag\u00b7ten", "Herr", "und", "Knecht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "F\u00fcr Pflicht, Gesetz und Recht", "tokens": ["F\u00fcr", "Pflicht", ",", "Ge\u00b7setz", "und", "Recht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Getrost Gut, Blut und Leben.", "tokens": ["Ge\u00b7trost", "Gut", ",", "Blut", "und", "Le\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Denk, wie zu deinem Untergang", "tokens": ["Denk", ",", "wie", "zu", "dei\u00b7nem", "Un\u00b7ter\u00b7gang"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich Ost und West verbanden!", "tokens": ["Sich", "Ost", "und", "West", "ver\u00b7ban\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NE", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hei\u00df war der Kampf und hart der Drang;", "tokens": ["Hei\u00df", "war", "der", "Kampf", "und", "hart", "der", "Drang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "KON", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch ward der Feind zu Schanden.", "tokens": ["Doch", "ward", "der", "Feind", "zu", "Schan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Des schn\u00f6den ", "tokens": ["Des", "schn\u00f6\u00b7den"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Des furchtbar'n ", "tokens": ["Des", "furcht\u00b7ba\u00b7r'n"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Des stolzen ", "tokens": ["Des", "stol\u00b7zen"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Brach sich am Heldenmuth,", "tokens": ["Brach", "sich", "am", "Hel\u00b7den\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.9": {"text": "O Deutschland, deiner Braven!", "tokens": ["O", "Deutschland", ",", "dei\u00b7ner", "Bra\u00b7ven", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPOSAT", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "O Deutschland sey der V\u00e4ter werth,", "tokens": ["O", "Deutschland", "sey", "der", "V\u00e4\u00b7ter", "werth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und la\u00df an deinen W\u00e4llen,", "tokens": ["Und", "la\u00df", "an", "dei\u00b7nen", "W\u00e4l\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "An deiner Starken gutem Schwert", "tokens": ["An", "dei\u00b7ner", "Star\u00b7ken", "gu\u00b7tem", "Schwert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Feindes Grimm zerschellen!", "tokens": ["Des", "Fein\u00b7des", "Grimm", "zer\u00b7schel\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Trotz' seiner Macht und Wuth!", "tokens": ["Trotz'", "sei\u00b7ner", "Macht", "und", "Wuth", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Du stehst in Gottes Hut.", "tokens": ["Du", "stehst", "in", "Got\u00b7tes", "Hut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wer freudig wagt, obsiegt!", "tokens": ["Wer", "freu\u00b7dig", "wagt", ",", "ob\u00b7siegt", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "ADJD", "VVFIN", "$,", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Der Feige nur erliegt!", "tokens": ["Der", "Fei\u00b7ge", "nur", "er\u00b7liegt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Der Tapfre f\u00e4llt frohlockend!", "tokens": ["Der", "Tapf\u00b7re", "f\u00e4llt", "froh\u00b7lo\u00b7ckend", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Getreues Deutschland, zittre nicht,", "tokens": ["Ge\u00b7treu\u00b7es", "Deutschland", ",", "zitt\u00b7re", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "VVFIN", "PTKNEG", "$,"], "meter": "-+----+", "measure": "dactylic.init"}, "line.2": {"text": "Wie sehr der Leu auch schnaube!", "tokens": ["Wie", "sehr", "der", "Leu", "auch", "schnau\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gott sey dein Hort, das Recht dein Licht,", "tokens": ["Gott", "sey", "dein", "Hort", ",", "das", "Recht", "dein", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$,", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Schild und Schwert der Glaube", "tokens": ["Dein", "Schild", "und", "Schwert", "der", "Glau\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wohl auf, Herr Zebaoth!", "tokens": ["Wohl", "auf", ",", "Herr", "Ze\u00b7bao\u00b7th", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wohl her zu Streit und Tod!", "tokens": ["Wohl", "her", "zu", "Streit", "und", "Tod", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Beschirme Leut' und Land!", "tokens": ["Be\u00b7schir\u00b7me", "Leut'", "und", "Land", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Halt' uns in deiner Hand,", "tokens": ["Halt'", "uns", "in", "dei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Wir siegen oder fallen!", "tokens": ["Wir", "sie\u00b7gen", "o\u00b7der", "fal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}