{"textgrid.poem.33649": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sprecht, ihr mittern\u00e4cht'gen Sterne,", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sprecht, ihr mittern\u00e4cht'gen Sterne,", "tokens": ["Sprecht", ",", "ihr", "mit\u00b7ter\u00b7n\u00e4cht'\u00b7gen", "Ster\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Neigt ihr euch zum Untergang?", "tokens": ["Neigt", "ihr", "euch", "zum", "Un\u00b7ter\u00b7gang", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weht schon Morgenluft von ferne?", "tokens": ["Weht", "schon", "Mor\u00b7gen\u00b7luft", "von", "fer\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "APPR", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sinkt der Mond am Bergabhang?", "tokens": ["Sinkt", "der", "Mond", "am", "Ber\u00b7gab\u00b7hang", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "La\u00dft mich wachen, la\u00dft mich schauen,", "tokens": ["La\u00dft", "mich", "wa\u00b7chen", ",", "la\u00dft", "mich", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$,", "VVIMP", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie die Nacht in Tag vergeht,", "tokens": ["Wie", "die", "Nacht", "in", "Tag", "ver\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn im hellen \u00c4therblauen", "tokens": ["Wenn", "im", "hel\u00b7len", "\u00c4\u00b7ther\u00b7blau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur der Morgenstern noch steht.", "tokens": ["Nur", "der", "Mor\u00b7gens\u00b7tern", "noch", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "+-+---+", "measure": "unknown.measure.tri"}}, "stanza.3": {"line.1": {"text": "Augen, vor dem Tod erstarrend,", "tokens": ["Au\u00b7gen", ",", "vor", "dem", "Tod", "er\u00b7star\u00b7rend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hab' ich trauernd zugedr\u00fcckt,", "tokens": ["Hab'", "ich", "trau\u00b7ernd", "zu\u00b7ge\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Blumen, noch des Tages harrend,", "tokens": ["Blu\u00b7men", ",", "noch", "des", "Ta\u00b7ges", "har\u00b7rend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Oft mit Tr\u00e4nen abgepfl\u00fcckt.", "tokens": ["Oft", "mit", "Tr\u00e4\u00b7nen", "ab\u00b7ge\u00b7pfl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "St\u00fcrzen sah ich stolze B\u00e4ume,", "tokens": ["St\u00fcr\u00b7zen", "sah", "ich", "stol\u00b7ze", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sah viel Gl\u00fcck vom Sturm verwehn \u2013", "tokens": ["Sah", "viel", "Gl\u00fcck", "vom", "Sturm", "ver\u00b7wehn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "APPRART", "NN", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00dft mich einmal Nacht und Tr\u00e4ume", "tokens": ["La\u00dft", "mich", "ein\u00b7mal", "Nacht", "und", "Tr\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sehn in Licht und Tag vergehn!", "tokens": ["Sehn", "in", "Licht", "und", "Tag", "ver\u00b7gehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}