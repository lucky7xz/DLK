{"dta.poem.19953": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Morgengef\u00fchl .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Morgenr\u00f6the Schein", "tokens": ["Der", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", "Schein"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Den neuen Tag verk\u00fcndet,", "tokens": ["Den", "neu\u00b7en", "Tag", "ver\u00b7k\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es steht der junge Hain", "tokens": ["Es", "steht", "der", "jun\u00b7ge", "Hain"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Von Liebesglut entz\u00fcndet.", "tokens": ["Von", "Lie\u00b7bes\u00b7glut", "ent\u00b7z\u00fcn\u00b7det", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Sterne, Wanderns satt,", "tokens": ["Die", "Ster\u00b7ne", ",", "Wan\u00b7derns", "satt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Sind l\u00e4ngst hinabgestiegen,", "tokens": ["Sind", "l\u00e4ngst", "hin\u00b7ab\u00b7ge\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die V\u00f6gel an der Statt", "tokens": ["Die", "V\u00f6\u00b7gel", "an", "der", "Statt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Froh durch den Himmel fliegen.", "tokens": ["Froh", "durch", "den", "Him\u00b7mel", "flie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Du armes Herz voll Pein,", "tokens": ["Du", "ar\u00b7mes", "Herz", "voll", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wie bist du bang befangen;", "tokens": ["Wie", "bist", "du", "bang", "be\u00b7fan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es sizt ein V\u00f6gelein", "tokens": ["Es", "sizt", "ein", "V\u00f6\u00b7ge\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Krank hinter Eisenstangen.", "tokens": ["Krank", "hin\u00b7ter", "Ei\u00b7sen\u00b7stan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wol h\u00f6rt es den Gesang,", "tokens": ["Wol", "h\u00f6rt", "es", "den", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Den frohen Flug der andern,", "tokens": ["Den", "fro\u00b7hen", "Flug", "der", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da sizt es, matt und krank,", "tokens": ["Da", "sizt", "es", ",", "matt", "und", "krank", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Kann singen nicht, noch wandern.", "tokens": ["Kann", "sin\u00b7gen", "nicht", ",", "noch", "wan\u00b7dern", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "VVFIN", "PTKNEG", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und meinte doch im Traum,", "tokens": ["Und", "mein\u00b7te", "doch", "im", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Das Haupt versteckt im Fl\u00fcgel,", "tokens": ["Das", "Haupt", "ver\u00b7steckt", "im", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es s\u00e4ng' auf einem Baum,", "tokens": ["Es", "s\u00e4ng'", "auf", "ei\u00b7nem", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Fl\u00f6g' \u00fcber Thal und H\u00fcgel.", "tokens": ["Fl\u00f6g'", "\u00fc\u00b7ber", "Thal", "und", "H\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Erl\u00f6sch du Sonnenstrahl!", "tokens": ["Er\u00b7l\u00f6sch", "du", "Son\u00b7nen\u00b7strahl", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Nacht! komm empor gestiegen,", "tokens": ["Nacht", "!", "komm", "em\u00b7por", "ge\u00b7stie\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PTKVZ", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df \u00fcber Berg und Thal", "tokens": ["Da\u00df", "\u00fc\u00b7ber", "Berg", "und", "Thal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wir wieder fr\u00f6hlich fliegen!", "tokens": ["Wir", "wie\u00b7der", "fr\u00f6h\u00b7lich", "flie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}