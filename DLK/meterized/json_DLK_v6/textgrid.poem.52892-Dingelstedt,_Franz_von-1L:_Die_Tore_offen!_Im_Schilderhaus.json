{"textgrid.poem.52892": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Tore offen! Im Schilderhaus", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Tore offen! Im Schilderhaus", "tokens": ["Die", "To\u00b7re", "of\u00b7fen", "!", "Im", "Schil\u00b7der\u00b7haus"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$.", "APPRART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wird's gleich ein \u00bbWer da?\u00ab schreien;", "tokens": ["Wird's", "gleich", "ein", "\u00bb", "Wer", "da", "?", "\u00ab", "schrei\u00b7en", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "$(", "PWS", "ADV", "$.", "$(", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Schritt, ein Tritt \u2013 und ich bin hinaus,", "tokens": ["Ein", "Schritt", ",", "ein", "Tritt", "\u2013", "und", "ich", "bin", "hin\u00b7aus", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$(", "KON", "PPER", "VAFIN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In der Welt, im Weiten, im Freien!", "tokens": ["In", "der", "Welt", ",", "im", "Wei\u00b7ten", ",", "im", "Frei\u00b7en", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPRART", "NN", "$,", "APPRART", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Wer h\u00e4lt mich denn am \u00c4rmel fest,", "tokens": ["Wer", "h\u00e4lt", "mich", "denn", "am", "\u00c4r\u00b7mel", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was beizt mich im Auge wie Zwiebeln?", "tokens": ["Was", "beizt", "mich", "im", "Au\u00b7ge", "wie", "Zwie\u00b7beln", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPRART", "NN", "KOKOM", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Warum fesselt mich denn dieses alte Nest", "tokens": ["Wa\u00b7rum", "fes\u00b7selt", "mich", "denn", "die\u00b7ses", "al\u00b7te", "Nest"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Mit seinen T\u00fcrmen und Giebeln?", "tokens": ["Mit", "sei\u00b7nen", "T\u00fcr\u00b7men", "und", "Gie\u00b7beln", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Gew\u00f6hnung! O allm\u00e4chtiger Trieb,", "tokens": ["Ge\u00b7w\u00f6h\u00b7nung", "!", "O", "all\u00b7m\u00e4ch\u00b7ti\u00b7ger", "Trieb", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Wer mag sich deiner erwehren?", "tokens": ["Wer", "mag", "sich", "dei\u00b7ner", "er\u00b7weh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PRF", "PPOSAT", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dem Sklaven wird seine Kette lieb,", "tokens": ["Dem", "Skla\u00b7ven", "wird", "sei\u00b7ne", "Ket\u00b7te", "lieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Soll er sie zum ersten entbehren.", "tokens": ["Soll", "er", "sie", "zum", "ers\u00b7ten", "ent\u00b7beh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "APPRART", "ADJA", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Und Mariandel, die gute, ehrliche Haut!", "tokens": ["Und", "Ma\u00b7ri\u00b7an\u00b7del", ",", "die", "gu\u00b7te", ",", "ehr\u00b7li\u00b7che", "Haut", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wie wird sie's gr\u00e4men und schmerzen,", "tokens": ["Wie", "wird", "sie's", "gr\u00e4\u00b7men", "und", "schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PIS", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wenn sie morgen fr\u00fch aus dem Fenster schaut,", "tokens": ["Wenn", "sie", "mor\u00b7gen", "fr\u00fch", "aus", "dem", "Fens\u00b7ter", "schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mich erwartend mit treuem Herzen.", "tokens": ["Mich", "er\u00b7war\u00b7tend", "mit", "treu\u00b7em", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Es gilt ihr nicht um meine Person,", "tokens": ["Es", "gilt", "ihr", "nicht", "um", "mei\u00b7ne", "Per\u00b7son", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Daran ist wenig gelegen;", "tokens": ["Da\u00b7ran", "ist", "we\u00b7nig", "ge\u00b7le\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ihr ist's nur um das Geschw\u00e4tz und den Hohn,", "tokens": ["Ihr", "ist's", "nur", "um", "das", "Ge\u00b7schw\u00e4tz", "und", "den", "Hohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nur der anderen Leute wegen.", "tokens": ["Nur", "der", "an\u00b7de\u00b7ren", "Leu\u00b7te", "we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "APPR", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "So tr\u00f6ste dich Gott! Ich kann nicht zur\u00fcck,", "tokens": ["So", "tr\u00f6s\u00b7te", "dich", "Gott", "!", "Ich", "kann", "nicht", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$.", "PPER", "VMFIN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es mahnt die Stunde zu eilen;", "tokens": ["Es", "mahnt", "die", "Stun\u00b7de", "zu", "ei\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKZU", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Doch find ich drau\u00dfen ein ordentlich Gl\u00fcck,", "tokens": ["Doch", "find", "ich", "drau\u00b7\u00dfen", "ein", "or\u00b7dent\u00b7lich", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit der Alten m\u00fc\u00dft ich's teilen.", "tokens": ["Mit", "der", "Al\u00b7ten", "m\u00fc\u00dft", "ich's", "tei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Dort steigt der Mond im Osten auf,", "tokens": ["Dort", "steigt", "der", "Mond", "im", "Os\u00b7ten", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein willkommener Weggeleiter;", "tokens": ["Ein", "will\u00b7kom\u00b7me\u00b7ner", "Weg\u00b7ge\u00b7lei\u00b7ter", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Mit Silber bestreut er meinen Lauf,", "tokens": ["Mit", "Sil\u00b7ber", "be\u00b7streut", "er", "mei\u00b7nen", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie hell die Stra\u00dfe, wie heiter!", "tokens": ["Wie", "hell", "die", "Stra\u00b7\u00dfe", ",", "wie", "hei\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "$,", "PWAV", "ADJD", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Ein Posthorn klingt aus fernem Tal", "tokens": ["Ein", "Post\u00b7horn", "klingt", "aus", "fer\u00b7nem", "Tal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und verschwimmt im blauen \u00c4ther \u2013", "tokens": ["Und", "ver\u00b7schwimmt", "im", "blau\u00b7en", "\u00c4\u00b7ther", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Leb wohl, leb wohl viel' tausend Mal,", "tokens": ["Leb", "wohl", ",", "leb", "wohl", "viel'", "tau\u00b7send", "Mal", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "VVFIN", "ADV", "ADV", "CARD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du heilige Stadt meiner V\u00e4ter!", "tokens": ["Du", "hei\u00b7li\u00b7ge", "Stadt", "mei\u00b7ner", "V\u00e4\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.9": {"line.1": {"text": "Ich k\u00fcsse dein Tor im Mondenlicht,", "tokens": ["Ich", "k\u00fcs\u00b7se", "dein", "Tor", "im", "Mon\u00b7den\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Auf den Boden fall ich nieder;", "tokens": ["Auf", "den", "Bo\u00b7den", "fall", "ich", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dein Sohn entflieht \u2013 O frag ihn nicht:", "tokens": ["Dein", "Sohn", "ent\u00b7flieht", "\u2013", "O", "frag", "ihn", "nicht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$(", "NE", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie kommst du und wannen wieder?", "tokens": ["Wie", "kommst", "du", "und", "wan\u00b7nen", "wie\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "KON", "ADV", "ADV", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}