{"dta.poem.4322": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die schreckliche Gewalt des  \n Wassers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von einem schwehren Regen-Gu\u00df,", "tokens": ["Von", "ei\u00b7nem", "schweh\u00b7ren", "Re\u00b7gen\u00b7Gu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und von geschmolznem Schnee, geschwollen,", "tokens": ["Und", "von", "ge\u00b7schmolz\u00b7nem", "Schnee", ",", "ge\u00b7schwol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wuchs auf dem Berg\u2019 ein kleiner Flu\u00df,", "tokens": ["Wuchs", "auf", "dem", "Ber\u00b7g'", "ein", "klei\u00b7ner", "Flu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Fing an, vermehrt von tausend B\u00e4chen,", "tokens": ["Fing", "an", ",", "ver\u00b7mehrt", "von", "tau\u00b7send", "B\u00e4\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "ADJD", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mit seinem stets verst\u00e4rkten Schu\u00df,", "tokens": ["Mit", "sei\u00b7nem", "stets", "ver\u00b7st\u00e4rk\u00b7ten", "Schu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Durch allen Gegenstand zu brechen,", "tokens": ["Durch", "al\u00b7len", "Ge\u00b7gen\u00b7stand", "zu", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und alles mit sich fort zu rollen,", "tokens": ["Und", "al\u00b7les", "mit", "sich", "fort", "zu", "rol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "PRF", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ergo\u00df sich, mit erz\u00fcrntem Brausen,", "tokens": ["Er\u00b7go\u00df", "sich", ",", "mit", "er\u00b7z\u00fcrn\u00b7tem", "Brau\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und sprengte den verha\u00dften Damm.", "tokens": ["Und", "spreng\u00b7te", "den", "ver\u00b7ha\u00df\u00b7ten", "Damm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Von fern erschallt\u2019 ein fremdes Sausen,", "tokens": ["Von", "fern", "er\u00b7schallt'", "ein", "frem\u00b7des", "Sau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Das aber pl\u00f6tzlich n\u00e4her kam.", "tokens": ["Das", "a\u00b7ber", "pl\u00f6tz\u00b7lich", "n\u00e4\u00b7her", "kam", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJD", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Hier rei\u00dft sein Sturz von ihrem Ort", "tokens": ["Hier", "rei\u00dft", "sein", "Sturz", "von", "ih\u00b7rem", "Ort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Zernagte Steine mit sich fort,", "tokens": ["Zer\u00b7nag\u00b7te", "Stei\u00b7ne", "mit", "sich", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Da St\u00fccke von durchbrochnen Deichen,", "tokens": ["Da", "St\u00fc\u00b7cke", "von", "durch\u00b7broch\u00b7nen", "Dei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Dort schwehre St\u00e4mme rauher Eichen,", "tokens": ["Dort", "schweh\u00b7re", "St\u00e4m\u00b7me", "rau\u00b7her", "Ei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Zerborstne Felsen st\u00fcrzen nach.", "tokens": ["Zer\u00b7borst\u00b7ne", "Fel\u00b7sen", "st\u00fcr\u00b7zen", "nach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Von manchem Baum, der borst und brach,", "tokens": ["Von", "man\u00b7chem", "Baum", ",", "der", "borst", "und", "brach", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "ADV", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "H\u00f6rt\u2019 man ein f\u00fcrchterlich Gekrach.", "tokens": ["H\u00f6rt'", "man", "ein", "f\u00fcrch\u00b7ter\u00b7lich", "Ge\u00b7krach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Berge schrien, die W\u00e4lder heulten,", "tokens": ["Die", "Ber\u00b7ge", "schri\u00b7en", ",", "die", "W\u00e4l\u00b7der", "heul\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Wurzeln stiegen aus der Erden.", "tokens": ["Die", "Wur\u00b7zeln", "stie\u00b7gen", "aus", "der", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Hier welzten H\u00e4user, dorten Heerden,", "tokens": ["Hier", "welz\u00b7ten", "H\u00e4u\u00b7ser", ",", "dor\u00b7ten", "Heer\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So wie die Fluhten sie ereilten.", "tokens": ["So", "wie", "die", "Fluh\u00b7ten", "sie", "er\u00b7eil\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mit allgemeinem Klag-Geschrey", "tokens": ["Mit", "all\u00b7ge\u00b7mei\u00b7nem", "Klag\u00b7Ge\u00b7schrey"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sah man, von grossen Balken, Tr\u00fcmmer,", "tokens": ["Sah", "man", ",", "von", "gros\u00b7sen", "Bal\u00b7ken", ",", "Tr\u00fcm\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "APPR", "ADJA", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da schwummen D\u00e4cher, halbe Zimmer,", "tokens": ["Da", "schwum\u00b7men", "D\u00e4\u00b7cher", ",", "hal\u00b7be", "Zim\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie fuhren, wie ein Strahl, vorbey.", "tokens": ["Sie", "fuh\u00b7ren", ",", "wie", "ein", "Strahl", ",", "vor\u00b7bey", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ART", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hier schwung, mit wei\u00df besch\u00e4umtem Grimm,", "tokens": ["Hier", "schwung", ",", "mit", "wei\u00df", "be\u00b7sch\u00e4um\u00b7tem", "Grimm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "APPR", "VVFIN", "ADJA", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein strenger Strudel alles \u00fcm.", "tokens": ["Ein", "stren\u00b7ger", "Stru\u00b7del", "al\u00b7les", "\u00fcm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die reife Saat ward unterdruckt,", "tokens": ["Die", "rei\u00b7fe", "Saat", "ward", "un\u00b7ter\u00b7druckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Gem\u00e4hte Garben weggesp\u00fchlet,", "tokens": ["Ge\u00b7m\u00e4h\u00b7te", "Gar\u00b7ben", "weg\u00b7ge\u00b7sp\u00fch\u00b7let", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Der Boden selbst ward aufgew\u00fchlet,", "tokens": ["Der", "Bo\u00b7den", "selbst", "ward", "auf\u00b7ge\u00b7w\u00fch\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die h\u00f6chsten Ufer eingeschluckt.", "tokens": ["Die", "h\u00f6chs\u00b7ten", "U\u00b7fer", "ein\u00b7ge\u00b7schluckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Es f\u00fcllt ein dunkler Wasser-Schwall", "tokens": ["Es", "f\u00fcllt", "ein", "dunk\u00b7ler", "Was\u00b7ser\u00b7Schwall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Die hohlen Tiefen \u00fcberall,", "tokens": ["Die", "hoh\u00b7len", "Tie\u00b7fen", "\u00fc\u00b7be\u00b7rall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Auch war von den erhabnen H\u00f6hen", "tokens": ["Auch", "war", "von", "den", "er\u00b7hab\u00b7nen", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Nicht eins die Stelle mehr zu sehen.", "tokens": ["Nicht", "eins", "die", "Stel\u00b7le", "mehr", "zu", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIS", "ART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Feld, Aecker, Berg\u2019 und Th\u00e4ler deckte", "tokens": ["Feld", ",", "A\u00b7e\u00b7cker", ",", "Ber\u00b7g'", "und", "Th\u00e4\u00b7ler", "deck\u00b7te"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "VVFIN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.16": {"text": "Ein\u2019 Ebne, die das Auge schreckte.", "tokens": ["Ein'", "Eb\u00b7ne", ",", "die", "das", "Au\u00b7ge", "schreck\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich sahe die\u00df erstaunet an,", "tokens": ["Ich", "sa\u00b7he", "die\u00df", "er\u00b7stau\u00b7net", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und dacht\u2019: Ein blosser Regen kann", "tokens": ["Und", "dacht'", ":", "Ein", "blos\u00b7ser", "Re\u00b7gen", "kann"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "ART", "ADJA", "NN", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Erden Fl\u00e4che ganz verheeren,", "tokens": ["Der", "Er\u00b7den", "Fl\u00e4\u00b7che", "ganz", "ver\u00b7hee\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Menschen Werk so schnell verst\u00f6hren,", "tokens": ["Der", "Men\u00b7schen", "Werk", "so", "schnell", "ver\u00b7st\u00f6h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df alles sinket, st\u00fcrzet, bricht,", "tokens": ["Da\u00df", "al\u00b7les", "sin\u00b7ket", ",", "st\u00fcr\u00b7zet", ",", "bricht", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und alles unterst oben kehren!", "tokens": ["Und", "al\u00b7les", "un\u00b7terst", "o\u00b7ben", "keh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Mit welcher Ehrfurcht sollte nicht", "tokens": ["Mit", "wel\u00b7cher", "Ehr\u00b7furcht", "soll\u00b7te", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "NN", "VMFIN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Das unbegreifliche Verwalten,", "tokens": ["Das", "un\u00b7be\u00b7greif\u00b7li\u00b7che", "Ver\u00b7wal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Regieren, St\u00fctzen und Erhalten", "tokens": ["Re\u00b7gie\u00b7ren", ",", "St\u00fct\u00b7zen", "und", "Er\u00b7hal\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Des spr\u00f6den Kreises dieser Erden", "tokens": ["Des", "spr\u00f6\u00b7den", "Krei\u00b7ses", "die\u00b7ser", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bewundernd angebetet werden!", "tokens": ["Be\u00b7wun\u00b7dernd", "an\u00b7ge\u00b7be\u00b7tet", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ach, da\u00df wir doch den HErrn der Welt,", "tokens": ["Ach", ",", "da\u00df", "wir", "doch", "den", "Herrn", "der", "Welt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Herrscher aller Elementen,", "tokens": ["Den", "Herr\u00b7scher", "al\u00b7ler", "E\u00b7le\u00b7men\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der das, was Er gemacht, erh\u00e4lt,", "tokens": ["Der", "das", ",", "was", "Er", "ge\u00b7macht", ",", "er\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PDS", "$,", "PRELS", "PPER", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Voll Andacht, w\u00fcrdig preisen k\u00f6nnten!", "tokens": ["Voll", "An\u00b7dacht", ",", "w\u00fcr\u00b7dig", "prei\u00b7sen", "k\u00f6nn\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der das, aus aufgezognem Duft,", "tokens": ["Der", "das", ",", "aus", "auf\u00b7ge\u00b7zog\u00b7nem", "Duft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PDS", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Formierte Wasser-Meer der Luft,", "tokens": ["For\u00b7mier\u00b7te", "Was\u00b7ser\u00b7Meer", "der", "Luft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ohn\u2019 Grund und Boden, ohne Strand,", "tokens": ["Ohn'", "Grund", "und", "Bo\u00b7den", ",", "oh\u00b7ne", "Strand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "KOUI", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ohn\u2019 unsre Fluhten fesselnd Sand,", "tokens": ["Ohn'", "uns\u00b7re", "Fluh\u00b7ten", "fes\u00b7selnd", "Sand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Doch dergestalt wei\u00df einzuschr\u00e4nken,", "tokens": ["Doch", "der\u00b7ge\u00b7stalt", "wei\u00df", "ein\u00b7zu\u00b7schr\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Es dergestalt vermag zu lenken,", "tokens": ["Es", "der\u00b7ge\u00b7stalt", "ver\u00b7mag", "zu", "len\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Da\u00df, ob es gleich rings um uns flie\u00dft,", "tokens": ["Da\u00df", ",", "ob", "es", "gleich", "rings", "um", "uns", "flie\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PPER", "ADV", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Doch so gemessen sich ergie\u00dft,", "tokens": ["Doch", "so", "ge\u00b7mes\u00b7sen", "sich", "er\u00b7gie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Da\u00df es uns nicht erstickt, erdr\u00fccket,", "tokens": ["Da\u00df", "es", "uns", "nicht", "er\u00b7stickt", ",", "er\u00b7dr\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Nein, tr\u00e4nket, und die Erd\u2019 erquicket.", "tokens": ["Nein", ",", "tr\u00e4n\u00b7ket", ",", "und", "die", "Erd'", "er\u00b7quic\u00b7ket", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "$,", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Der aber auch zuweilen zeigt,", "tokens": ["Der", "a\u00b7ber", "auch", "zu\u00b7wei\u00b7len", "zeigt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Wenn sich ein solcher Fall er\u00e4ugt,", "tokens": ["Wenn", "sich", "ein", "sol\u00b7cher", "Fall", "er\u00b7\u00e4ugt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Da\u00df Er auch die so strenge Fluhten", "tokens": ["Da\u00df", "Er", "auch", "die", "so", "stren\u00b7ge", "Fluh\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Zuweilen braucht als Seine Ruhten.", "tokens": ["Zu\u00b7wei\u00b7len", "braucht", "als", "Sei\u00b7ne", "Ruh\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOKOM", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Uns kann denn eine Wasser-Fluht,", "tokens": ["Uns", "kann", "denn", "ei\u00b7ne", "Was\u00b7ser\u00b7Fluht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auch selber, wenn sie Schaden thut,", "tokens": ["Auch", "sel\u00b7ber", ",", "wenn", "sie", "Scha\u00b7den", "thut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Wellen aus den Schranken steigen,", "tokens": ["Und", "Wel\u00b7len", "aus", "den", "Schran\u00b7ken", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Sch\u00f6pfers Macht und Weisheit zeigen,", "tokens": ["Des", "Sch\u00f6p\u00b7fers", "Macht", "und", "Weis\u00b7heit", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der sie so richtig sonst verschr\u00e4nkt,", "tokens": ["Der", "sie", "so", "rich\u00b7tig", "sonst", "ver\u00b7schr\u00e4nkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Daher man denn mit Recht gedenkt:", "tokens": ["Da\u00b7her", "man", "denn", "mit", "Recht", "ge\u00b7denkt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIS", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Er schilt das Meer, so fliehts von dannen,", "tokens": ["Er", "schilt", "das", "Meer", ",", "so", "fliehts", "von", "dan\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df seine graue Tiefe braust,", "tokens": ["Da\u00df", "sei\u00b7ne", "grau\u00b7e", "Tie\u00b7fe", "braust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er fa\u00dft das Wasser mit der Faust,", "tokens": ["Er", "fa\u00dft", "das", "Was\u00b7ser", "mit", "der", "Faust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er mi\u00dft den Himmel mit der Spannen.", "tokens": ["Er", "mi\u00dft", "den", "Him\u00b7mel", "mit", "der", "Span\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Ja will man, wie von Seinem Wesen", "tokens": ["Ja", "will", "man", ",", "wie", "von", "Sei\u00b7nem", "We\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "VMFIN", "PIS", "$,", "PWAV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So herrlich Davids Harfe klingt,", "tokens": ["So", "herr\u00b7lich", "Da\u00b7vids", "Har\u00b7fe", "klingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Andacht und Bewundrung, lesen,", "tokens": ["Mit", "An\u00b7dacht", "und", "Be\u00b7wund\u00b7rung", ",", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der lese, wie er folgends singt: ", "tokens": ["Der", "le\u00b7se", ",", "wie", "er", "fol\u00b7gends", "singt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Es erbebete die Erde, und es ward ihr Bau bewegt,", "tokens": ["Es", "er\u00b7be\u00b7be\u00b7te", "die", "Er\u00b7de", ",", "und", "es", "ward", "ihr", "Bau", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KON", "PPER", "VAFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Ja die Grundfest\u2019 hoher Berge wurden wankend und ge-", "tokens": ["Ja", "die", "Grund\u00b7fest'", "ho\u00b7her", "Ber\u00b7ge", "wur\u00b7den", "wan\u00b7kend", "und", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "ART", "NN", "ADJA", "NN", "VAFIN", "ADJD", "