{"textgrid.poem.50838": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Welk lag meines Herzens Garten,", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Welk lag meines Herzens Garten,", "tokens": ["Welk", "lag", "mei\u00b7nes", "Her\u00b7zens", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und sein Springquell war versiegt,", "tokens": ["Und", "sein", "Spring\u00b7quell", "war", "ver\u00b7siegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und das Liedervolk in Zweigen", "tokens": ["Und", "das", "Lie\u00b7der\u00b7volk", "in", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sa\u00df in dumpfen Schlaf gewiegt.", "tokens": ["Sa\u00df", "in", "dum\u00b7pfen", "Schlaf", "ge\u00b7wiegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Starr und klanglos schien mir alles", "tokens": ["Starr", "und", "klang\u00b7los", "schien", "mir", "al\u00b7les"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "ADJD", "VVFIN", "PPER", "PIS"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der frische Duft entflohn!", "tokens": ["Und", "der", "fri\u00b7sche", "Duft", "ent\u00b7flohn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Selbst die fremden Lieblingsweisen", "tokens": ["Selbst", "die", "frem\u00b7den", "Lieb\u00b7lings\u00b7wei\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hatten f\u00fcr mich keinen Ton.", "tokens": ["Hat\u00b7ten", "f\u00fcr", "mich", "kei\u00b7nen", "Ton", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie es oftmals geht im Leben,", "tokens": ["Wie", "es", "oft\u00b7mals", "geht", "im", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das so seltsam webt und flicht:", "tokens": ["Das", "so", "selt\u00b7sam", "webt", "und", "flicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJD", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "L\u00e4ngst schon kannt ich deinen Namen,", "tokens": ["L\u00e4ngst", "schon", "kannt", "ich", "dei\u00b7nen", "Na\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber deine Lieder nicht.", "tokens": ["A\u00b7ber", "dei\u00b7ne", "Lie\u00b7der", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und nun las ich sie; auf einmal", "tokens": ["Und", "nun", "las", "ich", "sie", ";", "auf", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPER", "$.", "APPR", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In so \u00f6der Winterzeit", "tokens": ["In", "so", "\u00f6\u00b7der", "Win\u00b7ter\u00b7zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ging mir auf ein neuer, reicher", "tokens": ["Ging", "mir", "auf", "ein", "neu\u00b7er", ",", "rei\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "$,", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lenz in seiner Herrlichkeit!", "tokens": ["Lenz", "in", "sei\u00b7ner", "Herr\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und in deinen Geistesbl\u00fcten", "tokens": ["Und", "in", "dei\u00b7nen", "Geis\u00b7tes\u00b7bl\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Warst du wie ein Nekromant,", "tokens": ["Warst", "du", "wie", "ein", "Ne\u00b7kro\u00b7mant", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der f\u00fcr meinen eignen Zauber", "tokens": ["Der", "f\u00fcr", "mei\u00b7nen", "eig\u00b7nen", "Zau\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wieder mir das Schlagwort fand.", "tokens": ["Wie\u00b7der", "mir", "das", "Schlag\u00b7wort", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Rasch entfesselt sprang der Bronnen!", "tokens": ["Rasch", "ent\u00b7fes\u00b7selt", "sprang", "der", "Bron\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alle Lauben voller Sang!", "tokens": ["Al\u00b7le", "Lau\u00b7ben", "vol\u00b7ler", "Sang", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und in den geheimsten G\u00e4ngen", "tokens": ["Und", "in", "den", "ge\u00b7heims\u00b7ten", "G\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "War es wieder Duft und Klang.", "tokens": ["War", "es", "wie\u00b7der", "Duft", "und", "Klang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Damals w\u00fcnscht ich, da\u00df ich m\u00f6chte", "tokens": ["Da\u00b7mals", "w\u00fcnscht", "ich", ",", "da\u00df", "ich", "m\u00f6ch\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VMFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ein begabter S\u00e4nger sein,", "tokens": ["Ein", "be\u00b7gab\u00b7ter", "S\u00e4n\u00b7ger", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Um dir recht ein weich und lindernd,", "tokens": ["Um", "dir", "recht", "ein", "weich", "und", "lin\u00b7dernd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "ADV", "ART", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ein vergeltend Lied zu weihn!", "tokens": ["Ein", "ver\u00b7gel\u00b7tend", "Lied", "zu", "weihn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}