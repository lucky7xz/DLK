{"textgrid.poem.26055": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Das ewig ungeduldige Herz ist l\u00e4ngst vor jeder Bl\u00fcte wach", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch ist kein Blatt am Baum,", "tokens": ["Noch", "ist", "kein", "Blatt", "am", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Noch keine wei\u00dfe Bl\u00fcte hingestellt,", "tokens": ["Noch", "kei\u00b7ne", "wei\u00b7\u00dfe", "Bl\u00fc\u00b7te", "hin\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kein Halm sein Spiel im Wind noch hat.", "tokens": ["Kein", "Halm", "sein", "Spiel", "im", "Wind", "noch", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPOSAT", "NN", "APPRART", "NN", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gelb, wie ein irdener Krug, liegt jeder Acker in dem Raum.", "tokens": ["Gelb", ",", "wie", "ein", "ir\u00b7de\u00b7ner", "Krug", ",", "liegt", "je\u00b7der", "A\u00b7cker", "in", "dem", "Raum", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "ADJA", "NN", "$,", "VVFIN", "PIAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+--+-+-+-+-+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Die Lerche aber steigt und f\u00e4llt,", "tokens": ["Die", "Ler\u00b7che", "a\u00b7ber", "steigt", "und", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein kleiner Fink im Schlehdorn geigt,", "tokens": ["Ein", "klei\u00b7ner", "Fink", "im", "Schleh\u00b7dorn", "geigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und eine Amsel in dem finstern kahlen Baum", "tokens": ["Und", "ei\u00b7ne", "Am\u00b7sel", "in", "dem", "fins\u00b7tern", "kah\u00b7len", "Baum"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Aufschluchzend Zwiesprach mit der Leere h\u00e4lt.", "tokens": ["Auf\u00b7schluch\u00b7zend", "Zwie\u00b7sprach", "mit", "der", "Lee\u00b7re", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das ewig ungeduldige Herz ist l\u00e4ngst vor jeder Bl\u00fcte wach,", "tokens": ["Das", "e\u00b7wig", "un\u00b7ge\u00b7dul\u00b7di\u00b7ge", "Herz", "ist", "l\u00e4ngst", "vor", "je\u00b7der", "Bl\u00fc\u00b7te", "wach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "ADJA", "NN", "VAFIN", "ADV", "APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.3": {"line.1": {"text": "Erz\u00e4hlt und ruft den Abendnebeln nach,", "tokens": ["Er\u00b7z\u00e4hlt", "und", "ruft", "den", "A\u00b7bend\u00b7ne\u00b7beln", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und seine Sehnsucht laut der Liebe Nest aus nichts aufbaut.", "tokens": ["Und", "sei\u00b7ne", "Sehn\u00b7sucht", "laut", "der", "Lie\u00b7be", "Nest", "aus", "nichts", "auf\u00b7baut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "ART", "NN", "NN", "APPR", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}