{"textgrid.poem.26635": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nun umwallen die Stadt schon dicht sich senkende Nebel,", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun umwallen die Stadt schon dicht sich senkende Nebel,", "tokens": ["Nun", "um\u00b7wal\u00b7len", "die", "Stadt", "schon", "dicht", "sich", "sen\u00b7ken\u00b7de", "Ne\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADJD", "PRF", "ADJA", "NN", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und aus dem d\u00fcsteren Grau rieselt der Regen herab.", "tokens": ["Und", "aus", "dem", "d\u00fcs\u00b7te\u00b7ren", "Grau", "rie\u00b7selt", "der", "Re\u00b7gen", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Kothig die Stra\u00dfen und triefend die D\u00e4cher; verdrossen und fr\u00f6stelnd,", "tokens": ["Ko\u00b7thig", "die", "Stra\u00b7\u00dfen", "und", "trie\u00b7fend", "die", "D\u00e4\u00b7cher", ";", "ver\u00b7dros\u00b7sen", "und", "fr\u00f6s\u00b7telnd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "KON", "ADJD", "ART", "NN", "$.", "VVPP", "KON", "ADJD", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Unter dem sch\u00fctzenden Schirm, hasten die Menschen dahin.", "tokens": ["Un\u00b7ter", "dem", "sch\u00fct\u00b7zen\u00b7den", "Schirm", ",", "has\u00b7ten", "die", "Men\u00b7schen", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "VVFIN", "ART", "NN", "PAV", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Aber die Blumen, die drau\u00dfen verwelkt auf unwirthlichen Fluren,", "tokens": ["A\u00b7ber", "die", "Blu\u00b7men", ",", "die", "drau\u00b7\u00dfen", "ver\u00b7welkt", "auf", "un\u00b7wirth\u00b7li\u00b7chen", "Flu\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "ADV", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Hier jetzt bl\u00fchen sie auf, zahllos zu Kr\u00e4nzen gereiht.", "tokens": ["Hier", "jetzt", "bl\u00fc\u00b7hen", "sie", "auf", ",", "zahl\u00b7los", "zu", "Kr\u00e4n\u00b7zen", "ge\u00b7reiht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PTKVZ", "$,", "ADJD", "APPR", "NN", "VVPP", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Wehmuth duftet und haucht ringsum aus Zierden f\u00fcr Gr\u00e4ber;", "tokens": ["Weh\u00b7muth", "duf\u00b7tet", "und", "haucht", "ring\u00b7sum", "aus", "Zier\u00b7den", "f\u00fcr", "Gr\u00e4\u00b7ber", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "VVFIN", "ADV", "APPR", "NN", "APPR", "NN", "$."], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Spenden der Liebe empf\u00e4ngt, was schon vermodert zu Staub.", "tokens": ["Spen\u00b7den", "der", "Lie\u00b7be", "emp\u00b7f\u00e4ngt", ",", "was", "schon", "ver\u00b7mo\u00b7dert", "zu", "Staub", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$,", "PRELS", "ADV", "VVPP", "APPR", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.9": {"text": "Ich auch pilg're hinaus auf den einsam gelegenen Friedhof,", "tokens": ["Ich", "auch", "pil\u00b7g'\u00b7re", "hin\u00b7aus", "auf", "den", "ein\u00b7sam", "ge\u00b7le\u00b7ge\u00b7nen", "Fried\u00b7hof", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "NE", "APZR", "APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+--+", "measure": "iambic.octa.plus.octa.plus.chol"}, "line.10": {"text": "Der seit langem bereits S\u00e4rgen sich nicht mehr erschlie\u00dft.", "tokens": ["Der", "seit", "lan\u00b7gem", "be\u00b7reits", "S\u00e4r\u00b7gen", "sich", "nicht", "mehr", "er\u00b7schlie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "ADV", "VVFIN", "PRF", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.11": {"text": "Theuerstes ruht mir dort! Doch nicht bei vertrautesten Gr\u00e4bern", "tokens": ["Theu\u00b7ers\u00b7tes", "ruht", "mir", "dort", "!", "Doch", "nicht", "bei", "ver\u00b7trau\u00b7tes\u00b7ten", "Gr\u00e4\u00b7bern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ADV", "$.", "KON", "PTKNEG", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Blos, in Trauer versenkt, weil' ich, gefeuchtet das Aug':", "tokens": ["Blos", ",", "in", "Trau\u00b7er", "ver\u00b7senkt", ",", "weil'", "ich", ",", "ge\u00b7feuch\u00b7tet", "das", "Aug'", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "NN", "VVPP", "$,", "KOUS", "PPER", "$,", "VVPP", "ART", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.13": {"text": "Nein, an Cypressen vorbei, durchwandl' ich die Reihen der H\u00fcgel,", "tokens": ["Nein", ",", "an", "Cyp\u00b7res\u00b7sen", "vor\u00b7bei", ",", "durch\u00b7wandl'", "ich", "die", "Rei\u00b7hen", "der", "H\u00fc\u00b7gel", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "APPR", "NE", "PTKVZ", "$,", "VMFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.14": {"text": "Welche gedenkende Pflicht immer noch bl\u00fchend erh\u00e4lt;", "tokens": ["Wel\u00b7che", "ge\u00b7den\u00b7ken\u00b7de", "Pflicht", "im\u00b7mer", "noch", "bl\u00fc\u00b7hend", "er\u00b7h\u00e4lt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "ADJA", "NN", "ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.15": {"text": "Lese die Kunde des Tods auf ragenden Steinen und Kreuzen \u2013", "tokens": ["Le\u00b7se", "die", "Kun\u00b7de", "des", "Tods", "auf", "ra\u00b7gen\u00b7den", "Stei\u00b7nen", "und", "Kreu\u00b7zen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "APPR", "ADJA", "NN", "KON", "NN", "$("], "meter": "+--+---+-+-+--+-", "measure": "dactylic.di.plus"}, "line.16": {"text": "Weiter und weiter zur\u00fcck leitet verwitternde Schrift;", "tokens": ["Wei\u00b7ter", "und", "wei\u00b7ter", "zu\u00b7r\u00fcck", "lei\u00b7tet", "ver\u00b7wit\u00b7tern\u00b7de", "Schrift", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "PTKVZ", "VVFIN", "ADJA", "NN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.17": {"text": "Leitet zur\u00fcck in's verfloss'ne Jahrhundert \u2013 zu br\u00fcchigen M\u00e4lern", "tokens": ["Lei\u00b7tet", "zu\u00b7r\u00fcck", "in's", "ver\u00b7floss'\u00b7ne", "Jahr\u00b7hun\u00b7dert", "\u2013", "zu", "br\u00fc\u00b7chi\u00b7gen", "M\u00e4\u00b7lern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "APPRART", "ADJA", "NN", "$(", "APPR", "ADJA", "NN"], "meter": "+--+--+--+--+-+-+", "measure": "dactylic.tetra.plus"}, "line.18": {"text": "Solcher, die man hier einst stolz l\u00e4ngs der Mauer begrub.", "tokens": ["Sol\u00b7cher", ",", "die", "man", "hier", "einst", "stolz", "l\u00e4ngs", "der", "Mau\u00b7er", "be\u00b7grub", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PIS", "ADV", "ADV", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-----+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.19": {"text": "W\u00fcrdigste M\u00e4nner und Frau'n. Und doch, wer nennt sie noch heute?", "tokens": ["W\u00fcr\u00b7digs\u00b7te", "M\u00e4n\u00b7ner", "und", "Frau'", "n.", "Und", "doch", ",", "wer", "nennt", "sie", "noch", "heu\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "abbreviation", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NE", "NE", "KON", "ADV", "$,", "PWS", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "---+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Wer gedenkt noch der Zeit, da sie gelebt und gewirkt?", "tokens": ["Wer", "ge\u00b7denkt", "noch", "der", "Zeit", ",", "da", "sie", "ge\u00b7lebt", "und", "ge\u00b7wirkt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ART", "NN", "$,", "KOUS", "PPER", "VVPP", "KON", "VVPP", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.21": {"text": "Bis auf die Namen vergessen fast alle die \u00e4lt'ren Geschlechter \u2013", "tokens": ["Bis", "auf", "die", "Na\u00b7men", "ver\u00b7ges\u00b7sen", "fast", "al\u00b7le", "die", "\u00e4lt'\u00b7ren", "Ge\u00b7schlech\u00b7ter", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "VVPP", "ADV", "PIS", "ART", "ADJA", "NN", "$("], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "Hin und wieder nur liegt schweigend ein Kranz auf der Gruft.", "tokens": ["Hin", "und", "wie\u00b7der", "nur", "liegt", "schwei\u00b7gend", "ein", "Kranz", "auf", "der", "Gruft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "ADV", "VVFIN", "ADJD", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.23": {"text": "Aber dem Enkel geziemt's, da\u00df er die weihende Thr\u00e4ne", "tokens": ["A\u00b7ber", "dem", "En\u00b7kel", "ge\u00b7ziemt's", ",", "da\u00df", "er", "die", "wei\u00b7hen\u00b7de", "Thr\u00e4\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NE", "$,", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "+--+--+---+--+-", "measure": "dactylic.tri.plus"}, "line.24": {"text": "Mit and\u00e4chtigem Sinn diesen Entschlafenen zollt.", "tokens": ["Mit", "an\u00b7d\u00e4ch\u00b7ti\u00b7gem", "Sinn", "die\u00b7sen", "Ent\u00b7schla\u00b7fe\u00b7nen", "zollt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PDAT", "NN", "VVFIN", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}}}}}