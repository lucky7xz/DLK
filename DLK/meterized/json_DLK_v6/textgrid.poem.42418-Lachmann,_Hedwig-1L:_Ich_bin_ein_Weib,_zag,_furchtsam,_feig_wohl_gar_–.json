{"textgrid.poem.42418": {"metadata": {"author": {"name": "Lachmann, Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich bin ein Weib, zag, furchtsam, feig wohl gar \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin ein Weib, zag, furchtsam, feig wohl gar \u2013", "tokens": ["Ich", "bin", "ein", "Weib", ",", "zag", ",", "furcht\u00b7sam", ",", "feig", "wohl", "gar", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "VVFIN", "$,", "ADJD", "$,", "ADJD", "ADV", "ADV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Geschreckt von dem Gew\u00fchl auf lautem Markt;", "tokens": ["Ge\u00b7schreckt", "von", "dem", "Ge\u00b7w\u00fchl", "auf", "lau\u00b7tem", "Markt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kleinlaut vor j\u00e4hem M\u00e4nnerzwist und bar", "tokens": ["Klein\u00b7laut", "vor", "j\u00e4\u00b7hem", "M\u00e4n\u00b7ner\u00b7zwist", "und", "bar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Kampflust, die am Widerstand erstarkt.", "tokens": ["Der", "Kampf\u00b7lust", ",", "die", "am", "Wi\u00b7der\u00b7stand", "er\u00b7starkt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Blut macht mich schaudern. Schwach und hilflos bin", "tokens": ["Blut", "macht", "mich", "schau\u00b7dern", ".", "Schwach", "und", "hil\u00b7flos", "bin"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "VVINF", "$.", "NN", "KON", "ADJD", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich vor der Wunde, die im Fleische klafft,", "tokens": ["Ich", "vor", "der", "Wun\u00b7de", ",", "die", "im", "Flei\u00b7sche", "klafft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und fremd und feindlich wendet sich mein Sinn", "tokens": ["Und", "fremd", "und", "feind\u00b7lich", "wen\u00b7det", "sich", "mein", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "ADJD", "VVFIN", "PRF", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Von Waffentaten, noch so heldenhaft.", "tokens": ["Von", "Waf\u00b7fen\u00b7ta\u00b7ten", ",", "noch", "so", "hel\u00b7den\u00b7haft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Weich schuf mich die Natur. In Tr\u00e4nen bricht", "tokens": ["Weich", "schuf", "mich", "die", "Na\u00b7tur", ".", "In", "Tr\u00e4\u00b7nen", "bricht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "$.", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mein Unmut sich wohl leicht, nach Frauenart,", "tokens": ["Mein", "Un\u00b7mut", "sich", "wohl", "leicht", ",", "nach", "Frau\u00b7en\u00b7art", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "ADV", "ADJD", "$,", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und traumhaft legt sich eine Zuversicht", "tokens": ["Und", "traum\u00b7haft", "legt", "sich", "ei\u00b7ne", "Zu\u00b7ver\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mir oft verh\u00fcllend um die Gegenwart.", "tokens": ["Mir", "oft", "ver\u00b7h\u00fcl\u00b7lend", "um", "die", "Ge\u00b7gen\u00b7wart", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch lebt in mir ein Etwas, eine Kraft,", "tokens": ["Doch", "lebt", "in", "mir", "ein", "Et\u00b7was", ",", "ei\u00b7ne", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ART", "ADV", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mir selber kaum bewusst und unbew\u00e4hrt,", "tokens": ["Mir", "sel\u00b7ber", "kaum", "be\u00b7wusst", "und", "un\u00b7be\u00b7w\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die gegen herrische Gewalt sich strafft", "tokens": ["Die", "ge\u00b7gen", "her\u00b7ri\u00b7sche", "Ge\u00b7walt", "sich", "strafft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und eine Glut in ihrem Kerne n\u00e4hrt.", "tokens": ["Und", "ei\u00b7ne", "Glut", "in", "ih\u00b7rem", "Ker\u00b7ne", "n\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ich weiss: wenn einst ein k\u00fchneres Geschlecht,", "tokens": ["Ich", "weiss", ":", "wenn", "einst", "ein", "k\u00fch\u00b7ne\u00b7res", "Ge\u00b7schlecht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "KOUS", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von Machtbegehr und Ruhmsucht nicht verf\u00fchrt,", "tokens": ["Von", "Macht\u00b7be\u00b7gehr", "und", "Ruhm\u00b7sucht", "nicht", "ver\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Allein der Stimme seines Bluts gerecht,", "tokens": ["Al\u00b7lein", "der", "Stim\u00b7me", "sei\u00b7nes", "Bluts", "ge\u00b7recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Freiheit forderte, die ihm geb\u00fchrt \u2013", "tokens": ["Die", "Frei\u00b7heit", "for\u00b7der\u00b7te", ",", "die", "ihm", "ge\u00b7b\u00fchrt", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "PPER", "VVPP", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Dies Herz, das jetzt noch zittert vor dem Strahl,", "tokens": ["Dies", "Herz", ",", "das", "jetzt", "noch", "zit\u00b7tert", "vor", "dem", "Strahl", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "$,", "PRELS", "ADV", "ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es hielte stand, so fest und ungebeugt,", "tokens": ["Es", "hiel\u00b7te", "stand", ",", "so", "fest", "und", "un\u00b7ge\u00b7beugt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie, trotz der \u00dcbermacht von Erz und Stahl,", "tokens": ["Wie", ",", "trotz", "der", "\u00dc\u00b7ber\u00b7macht", "von", "Erz", "und", "Stahl", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "APPR", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein Mannesherz f\u00fcr reine Wahrheit zeugt.", "tokens": ["Ein", "Man\u00b7nes\u00b7herz", "f\u00fcr", "rei\u00b7ne", "Wahr\u00b7heit", "zeugt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}