{"textgrid.poem.26500": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bei hei\u00dfen Sonnenbr\u00e4nden,", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bei hei\u00dfen Sonnenbr\u00e4nden,", "tokens": ["Bei", "hei\u00b7\u00dfen", "Son\u00b7nen\u00b7br\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du Beere, duftig, roth,", "tokens": ["Du", "Bee\u00b7re", ",", "duf\u00b7tig", ",", "roth", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "NN", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit nimmerm\u00fcden H\u00e4nden", "tokens": ["Mit", "nim\u00b7mer\u00b7m\u00fc\u00b7den", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Pfl\u00fcckt dich das Kind der Noth.", "tokens": ["Pfl\u00fcckt", "dich", "das", "Kind", "der", "Noth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es sieht die F\u00fclle prangen", "tokens": ["Es", "sieht", "die", "F\u00fcl\u00b7le", "pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und unterdr\u00fcckt dabei", "tokens": ["Und", "un\u00b7ter\u00b7dr\u00fcckt", "da\u00b7bei"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "PAV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das eigene Verlangen,", "tokens": ["Das", "ei\u00b7ge\u00b7ne", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.4": {"text": "Wie m\u00e4chtig es auch sei.", "tokens": ["Wie", "m\u00e4ch\u00b7tig", "es", "auch", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ADV", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Geh\u00e4uften Topf und Teller", "tokens": ["Ge\u00b7h\u00e4uf\u00b7ten", "Topf", "und", "Tel\u00b7ler"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Tr\u00e4gt es zum H\u00e4ndler dann;", "tokens": ["Tr\u00e4gt", "es", "zum", "H\u00e4nd\u00b7ler", "dann", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Der geizt noch mit dem Heller \u2013", "tokens": ["Der", "geizt", "noch", "mit", "dem", "Hel\u00b7ler", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er ist ein kluger Mann.", "tokens": ["Er", "ist", "ein", "klu\u00b7ger", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Doch nicht bei seines Gleichen", "tokens": ["Doch", "nicht", "bei", "sei\u00b7nes", "Glei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vollendet sich der Kreis:", "tokens": ["Voll\u00b7en\u00b7det", "sich", "der", "Kreis", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Erst auf dem Tisch des Reichen,", "tokens": ["Erst", "auf", "dem", "Tisch", "des", "Rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der zu bezahlen wei\u00df.", "tokens": ["Der", "zu", "be\u00b7zah\u00b7len", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "So wird zur Menschenhabe", "tokens": ["So", "wird", "zur", "Men\u00b7schen\u00b7ha\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und dient dem Wucher nur", "tokens": ["Und", "dient", "dem", "Wu\u00b7cher", "nur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Selbst deine frei'ste Gabe,", "tokens": ["Selbst", "dei\u00b7ne", "frei'\u00b7ste", "Ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O liebende Natur!", "tokens": ["O", "lie\u00b7ben\u00b7de", "Na\u00b7tur", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+---+", "measure": "dactylic.init"}}}}}