{"dta.poem.915": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "In DEN NACHMITTAG GEFL\u00dcSTERT", "genre": "Lyrik", "period": "N.A.", "pub_year": "1913", "urn": "urn:nbn:de:kobv:b4-30357-9", "language": ["de:0.99"], "booktitle": "Trakl, Georg: Gedichte. Leipzig, 1913."}, "poem": {"stanza.1": {"line.1": {"text": "Sonne, herbstlich d\u00fcnn und zag,", "tokens": ["Son\u00b7ne", ",", "herbst\u00b7lich", "d\u00fcnn", "und", "zag", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und das Obst f\u00e4llt von den B\u00e4umen", "tokens": ["Und", "das", "Obst", "f\u00e4llt", "von", "den", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Stille wohnt in blauen R\u00e4umen.", "tokens": ["Stil\u00b7le", "wohnt", "in", "blau\u00b7en", "R\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einen langen Nachmittag.", "tokens": ["Ei\u00b7nen", "lan\u00b7gen", "Nach\u00b7mit\u00b7tag", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sterbekl\u00e4nge von Metall;", "tokens": ["Ster\u00b7be\u00b7kl\u00e4n\u00b7ge", "von", "Me\u00b7tall", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ein wei\u00dfes Tier bricht nieder.", "tokens": ["Und", "ein", "wei\u00b7\u00dfes", "Tier", "bricht", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Brauner M\u00e4dchen rauhe Lieder", "tokens": ["Brau\u00b7ner", "M\u00e4d\u00b7chen", "rau\u00b7he", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sind verweht im Bl\u00e4tterfall.", "tokens": ["Sind", "ver\u00b7weht", "im", "Bl\u00e4t\u00b7ter\u00b7fall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Stirne Gottes Farben tr\u00e4umt,", "tokens": ["Stir\u00b7ne", "Got\u00b7tes", "Far\u00b7ben", "tr\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sp\u00fcrt des Wahnsinns sanfte Fl\u00fcgel.", "tokens": ["Sp\u00fcrt", "des", "Wahn\u00b7sinns", "sanf\u00b7te", "Fl\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schatten drehen sich am H\u00fcgel", "tokens": ["Schat\u00b7ten", "dre\u00b7hen", "sich", "am", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PRF", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von Verwesung schwarz ums\u00e4umt.", "tokens": ["Von", "Ver\u00b7we\u00b7sung", "schwarz", "um\u00b7s\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "D\u00e4mmerung voll Ruh und Wein;", "tokens": ["D\u00e4m\u00b7me\u00b7rung", "voll", "Ruh", "und", "Wein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Traurige Guitarren rinnen.", "tokens": ["Trau\u00b7ri\u00b7ge", "Gui\u00b7tar\u00b7ren", "rin\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und zur milden Lampe drinnen", "tokens": ["Und", "zur", "mil\u00b7den", "Lam\u00b7pe", "drin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kehrst du wie im Traume ein.", "tokens": ["Kehrst", "du", "wie", "im", "Trau\u00b7me", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}