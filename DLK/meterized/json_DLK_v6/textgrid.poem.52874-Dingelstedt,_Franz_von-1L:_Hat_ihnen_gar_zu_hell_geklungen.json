{"textgrid.poem.52874": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hat ihnen gar zu hell geklungen", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hat ihnen gar zu hell geklungen", "tokens": ["Hat", "ih\u00b7nen", "gar", "zu", "hell", "ge\u00b7klun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "PTKA", "ADJD", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Ton von meinem alten Horn,", "tokens": ["Der", "Ton", "von", "mei\u00b7nem", "al\u00b7ten", "Horn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hab' ihnen gar zu grell gesungen,", "tokens": ["Hab'", "ih\u00b7nen", "gar", "zu", "grell", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PTKA", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Herrn, sie schliefen just nach vorn.", "tokens": ["Den", "Herrn", ",", "sie", "schlie\u00b7fen", "just", "nach", "vorn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "ADV", "APPR", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Erwachten immer unbequemlich,", "tokens": ["Er\u00b7wach\u00b7ten", "im\u00b7mer", "un\u00b7be\u00b7quem\u00b7lich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und tr\u00e4umten sie auch noch so tief,", "tokens": ["Und", "tr\u00e4um\u00b7ten", "sie", "auch", "noch", "so", "tief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sobald ich stattlich und vernehmlich", "tokens": ["So\u00b7bald", "ich", "statt\u00b7lich", "und", "ver\u00b7nehm\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vor ihrem Haus mein Spr\u00fcchlein rief.", "tokens": ["Vor", "ih\u00b7rem", "Haus", "mein", "Spr\u00fcch\u00b7lein", "rief", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nun haben sie mir's weggenommen", "tokens": ["Nun", "ha\u00b7ben", "sie", "mir's", "weg\u00b7ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "NE", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein gutes, altes, liebes Horn,", "tokens": ["Mein", "gu\u00b7tes", ",", "al\u00b7tes", ",", "lie\u00b7bes", "Horn", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Pfeiflein hab' ich drein bekommen", "tokens": ["Ein", "Pfei\u00b7flein", "hab'", "ich", "drein", "be\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von Gott's- und Magistrates-Zorn;", "tokens": ["Von", "Gott'\u00b7s", "und", "Ma\u00b7gis\u00b7tra\u00b7tes\u00b7Zorn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Ein Pfeiflein, wie f\u00fcr Diebsgesindel", "tokens": ["Ein", "Pfei\u00b7flein", ",", "wie", "f\u00fcr", "Diebs\u00b7ge\u00b7sin\u00b7del"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und f\u00fcr der Haderlumpen Schwarm,", "tokens": ["Und", "f\u00fcr", "der", "Ha\u00b7der\u00b7lum\u00b7pen", "Schwarm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Kinder spielen in der Windel,", "tokens": ["Die", "Kin\u00b7der", "spie\u00b7len", "in", "der", "Win\u00b7del", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit solchen Dingern, Gott erbarm!", "tokens": ["Mit", "sol\u00b7chen", "Din\u00b7gern", ",", "Gott", "er\u00b7barm", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Sie meinten ba\u00df f\u00fcr mich zu sorgen", "tokens": ["Sie", "mein\u00b7ten", "ba\u00df", "f\u00fcr", "mich", "zu", "sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und dachten, mir w\u00e4r's schon genehm,", "tokens": ["Und", "dach\u00b7ten", ",", "mir", "w\u00e4r's", "schon", "ge\u00b7nehm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Da\u00df ich nicht jeden lieben Morgen", "tokens": ["Da\u00df", "ich", "nicht", "je\u00b7den", "lie\u00b7ben", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie atemlos nach Hause k\u00e4m'.", "tokens": ["Wie", "a\u00b7tem\u00b7los", "nach", "Hau\u00b7se", "k\u00e4m'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Prosit, Ihr hohen Herrn, ich merke,", "tokens": ["Pro\u00b7sit", ",", "Ihr", "ho\u00b7hen", "Herrn", ",", "ich", "mer\u00b7ke", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "ADJA", "NN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo hier begraben liegt der Hund:", "tokens": ["Wo", "hier", "be\u00b7gra\u00b7ben", "liegt", "der", "Hund", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVPP", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht meiner guten Lungen St\u00e4rke,", "tokens": ["Nicht", "mei\u00b7ner", "gu\u00b7ten", "Lun\u00b7gen", "St\u00e4r\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Euer schlechter Schlaf allein ist Grund.", "tokens": ["Eu\u00b7er", "schlech\u00b7ter", "Schlaf", "al\u00b7lein", "ist", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "VAFIN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Doch Euch mag's zum Exempel dienen,", "tokens": ["Doch", "Euch", "mag's", "zum", "Ex\u00b7em\u00b7pel", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ihr jungen H\u00f6rner fern und nah,", "tokens": ["Ihr", "jun\u00b7gen", "H\u00f6r\u00b7ner", "fern", "und", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verfistelt Euch in Piccolinen,", "tokens": ["Ver\u00b7fis\u00b7telt", "Euch", "in", "Pic\u00b7co\u00b7li\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Geschieht Euch sonst, wie mir geschah!", "tokens": ["Ge\u00b7schieht", "Euch", "sonst", ",", "wie", "mir", "ge\u00b7schah", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Gottlob, da\u00df ich so abgekommen,", "tokens": ["Gott\u00b7lob", ",", "da\u00df", "ich", "so", "ab\u00b7ge\u00b7kom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Herrn sind sonsten nicht so faul,", "tokens": ["Die", "Herrn", "sind", "sons\u00b7ten", "nicht", "so", "faul", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem Heinz dem ward sein Horn genommen", "tokens": ["Dem", "Heinz", "dem", "ward", "sein", "Horn", "ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "VAFIN", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und schmissen ihn dazu aufs Maul!", "tokens": ["Und", "schmis\u00b7sen", "ihn", "da\u00b7zu", "aufs", "Maul", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PAV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}