{"dta.poem.18891": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Uebergang von Tropen  \n  zu Gleichnissen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.85", "af:0.14"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Seyen sie stets wie Sp\u00e4ne gespalten, wie Lappen", "tokens": ["Se\u00b7yen", "sie", "stets", "wie", "Sp\u00e4\u00b7ne", "ge\u00b7spal\u00b7ten", ",", "wie", "Lap\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "KOKOM", "NN", "VVPP", "$,", "PWAV", "NN"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "zerrissen!", "tokens": ["zer\u00b7ris\u00b7sen", "!"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Wie die N\u00e4gel geklopft! und wie die Pf\u00e4hle ge-", "tokens": ["Wie", "die", "N\u00e4\u00b7gel", "ge\u00b7klopft", "!", "und", "wie", "die", "Pf\u00e4h\u00b7le", "ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVPP", "$.", "KON", "PWAV", "ART", "NN", "TRUNC"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "steckt!", "tokens": ["steckt", "!"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}}}}}