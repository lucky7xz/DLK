{"textgrid.poem.46720": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[unruhig ist's in der Natur]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unruhig ist's in der Natur,", "tokens": ["Un\u00b7ru\u00b7hig", "ist's", "in", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Als wie in meinem Herzen;", "tokens": ["Als", "wie", "in", "mei\u00b7nem", "Her\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch drau\u00dfen ist's der Fr\u00fchling nur,", "tokens": ["Doch", "drau\u00b7\u00dfen", "ist's", "der", "Fr\u00fch\u00b7ling", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In mir sind's andre Schmerzen.", "tokens": ["In", "mir", "sin\u00b7d's", "and\u00b7re", "Schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Fr\u00fchling ist wohl auch ein Schmerz,", "tokens": ["Der", "Fr\u00fch\u00b7ling", "ist", "wohl", "auch", "ein", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der drau\u00dfen wird geboren,", "tokens": ["Der", "drau\u00b7\u00dfen", "wird", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich aber hab', o armes Herz,", "tokens": ["Ich", "a\u00b7ber", "hab'", ",", "o", "ar\u00b7mes", "Herz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "$,", "FM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die ich gebar, verloren.", "tokens": ["Die", "ich", "ge\u00b7bar", ",", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "$,", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}