{"textgrid.poem.44626": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Der alberne Stingel", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein kurzer derber Stingel hing", "tokens": ["Ein", "kur\u00b7zer", "der\u00b7ber", "Stin\u00b7gel", "hing"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "an einem hohen Baume", "tokens": ["an", "ei\u00b7nem", "ho\u00b7hen", "Bau\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "und an ihm eine Pflaume,", "tokens": ["und", "an", "ihm", "ei\u00b7ne", "Pflau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "und was nur da vor\u00fcberging,", "tokens": ["und", "was", "nur", "da", "vor\u00b7\u00fc\u00b7ber\u00b7ging", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "der Waldbewohner ganzer Chor,", "tokens": ["der", "Wald\u00b7be\u00b7woh\u00b7ner", "gan\u00b7zer", "Chor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "sah sehnsuchtsvoll zu ihm empor.", "tokens": ["sah", "sehn\u00b7suchts\u00b7voll", "zu", "ihm", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da sprach zu sich der eitle Tor,", "tokens": ["Da", "sprach", "zu", "sich", "der", "eit\u00b7le", "Tor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "ich bin wohl eine s\u00fc\u00dfe Frucht,", "tokens": ["ich", "bin", "wohl", "ei\u00b7ne", "s\u00fc\u00b7\u00dfe", "Frucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "weil alles nur mich sieht und sucht.", "tokens": ["weil", "al\u00b7les", "nur", "mich", "sieht", "und", "sucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ein Fuchs h\u00f6rt dies und spricht,", "tokens": ["Ein", "Fuchs", "h\u00f6rt", "dies", "und", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "PDS", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Komm doch aus deinem Traume,", "tokens": ["Komm", "doch", "aus", "dei\u00b7nem", "Trau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Wir suchen dich nicht, kleiner Wicht,", "tokens": ["Wir", "su\u00b7chen", "dich", "nicht", ",", "klei\u00b7ner", "Wicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "wir suchen nur die Pflaume.", "tokens": ["wir", "su\u00b7chen", "nur", "die", "Pflau\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}