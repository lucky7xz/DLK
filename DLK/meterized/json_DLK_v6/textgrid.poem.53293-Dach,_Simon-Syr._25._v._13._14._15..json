{"textgrid.poem.53293": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Syr. 25. v. 13. 14. 15.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Wie gro\u00df ist doch der Mann,", "tokens": ["O", "Wie", "gro\u00df", "ist", "doch", "der", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAV", "ADJD", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der durch hoher Wei\u00dfheit Gaben", "tokens": ["Der", "durch", "ho\u00b7her", "Wei\u00df\u00b7heit", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Alles das ergr\u00fcnden kan,", "tokens": ["Al\u00b7les", "das", "er\u00b7gr\u00fcn\u00b7den", "kan", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "PDS", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Was See, Erd vnd Himmel haben,", "tokens": ["Was", "See", ",", "Erd", "vnd", "Him\u00b7mel", "ha\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "$,", "NN", "KON", "NN", "VAFIN", "$,"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Der in alle F\u00e4ll' vnd Sachen", "tokens": ["Der", "in", "al\u00b7le", "F\u00e4ll'", "vnd", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PIAT", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kl\u00fcglich sich zu schicken wei\u00df,", "tokens": ["Kl\u00fcg\u00b7lich", "sich", "zu", "schi\u00b7cken", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Kriegt in Trawren oder Lachen", "tokens": ["Kriegt", "in", "Traw\u00b7ren", "o\u00b7der", "La\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Der geehrten Tugend Preis!", "tokens": ["Der", "ge\u00b7ehr\u00b7ten", "Tu\u00b7gend", "Preis", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aber welcher seinen Gott", "tokens": ["A\u00b7ber", "wel\u00b7cher", "sei\u00b7nen", "Gott"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAT", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fcrchtet, hat nicht seines gleichen,", "tokens": ["F\u00fcrch\u00b7tet", ",", "hat", "nicht", "sei\u00b7nes", "glei\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VAFIN", "PTKNEG", "PPOSAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jederman wird hie zu Spott", "tokens": ["Je\u00b7der\u00b7man", "wird", "hie", "zu", "Spott"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "ADV", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Vnd mu\u00df Hoheit halben weichen,", "tokens": ["Vnd", "mu\u00df", "Ho\u00b7heit", "hal\u00b7ben", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "NN", "ADJA", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn die Furcht des Herren stehet", "tokens": ["Denn", "die", "Furcht", "des", "Her\u00b7ren", "ste\u00b7het"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Vber alle Ding' emp\u00f6rt,", "tokens": ["Vber", "al\u00b7le", "Ding'", "em\u00b7p\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wer Sie hat, wird auch erh\u00f6het", "tokens": ["Wer", "Sie", "hat", ",", "wird", "auch", "er\u00b7h\u00f6\u00b7het"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "PPER", "VAFIN", "$,", "VAFIN", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Vnd zugleich mit Ihr geehrt.", "tokens": ["Vnd", "zu\u00b7gleich", "mit", "Ihr", "ge\u00b7ehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "O Wie wol ist dem zu mut,", "tokens": ["O", "Wie", "wol", "ist", "dem", "zu", "mut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAV", "ADV", "VAFIN", "ART", "APPR", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Der, bi\u00df da\u00df er mu\u00df erkalten,", "tokens": ["Der", ",", "bi\u00df", "da\u00df", "er", "mu\u00df", "er\u00b7kal\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KON", "KOUS", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dieses rechte Himmel-Gut", "tokens": ["Die\u00b7ses", "rech\u00b7te", "Him\u00b7mel\u00b7Gut"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Feste kan im Hertzen halten!", "tokens": ["Fes\u00b7te", "kan", "im", "Hert\u00b7zen", "hal\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Weint, jhr K\u00f6nige der Erden,", "tokens": ["Weint", ",", "jhr", "K\u00f6\u00b7ni\u00b7ge", "der", "Er\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Vnd was sonst hie schwimmt empor,", "tokens": ["Vnd", "was", "sonst", "hie", "schwimmt", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ihr m\u00fcst hie zu schanden werden,", "tokens": ["Ihr", "m\u00fcst", "hie", "zu", "schan\u00b7den", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PTKZU", "VVINF", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wer Gott f\u00fcrchtet, geht euch vor!", "tokens": ["Wer", "Gott", "f\u00fcrch\u00b7tet", ",", "geht", "euch", "vor", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}