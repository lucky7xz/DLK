{"dta.poem.16112": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "166.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bleib in der Mittelh\u00f6h mit deinen W\u00fcnschen stehn,", "tokens": ["Bleib", "in", "der", "Mit\u00b7tel\u00b7h\u00f6h", "mit", "dei\u00b7nen", "W\u00fcn\u00b7schen", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und la\u00df zu hoch hinaus die Hoffnungen nicht gehn.", "tokens": ["Und", "la\u00df", "zu", "hoch", "hin\u00b7aus", "die", "Hoff\u00b7nun\u00b7gen", "nicht", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PTKA", "ADJD", "ADV", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Gar sch\u00f6n ists wenn du mehr erlangst als du gehofft;", "tokens": ["Gar", "sch\u00f6n", "ists", "wenn", "du", "mehr", "er\u00b7langst", "als", "du", "ge\u00b7hofft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "KOUS", "PPER", "ADV", "VVFIN", "KOUS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Unangenehm betraf das Gegentheil dich oft.", "tokens": ["Un\u00b7an\u00b7ge\u00b7nehm", "be\u00b7traf", "das", "Ge\u00b7gen\u00b7theil", "dich", "oft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexan