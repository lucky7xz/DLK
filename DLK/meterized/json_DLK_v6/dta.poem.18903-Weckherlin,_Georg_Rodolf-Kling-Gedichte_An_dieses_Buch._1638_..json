{"dta.poem.18903": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Kling-Gedichte/  \n An dieses Buch.  1638 .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.99"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Wjllkom verlangtes Buch/ das vns wirt wider-", "tokens": ["Wjll\u00b7kom", "ver\u00b7lang\u00b7tes", "Buch", "/", "das", "vns", "wirt", "wi\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$(", "PRELS", "PPER", "VAFIN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "bringen", "tokens": ["brin\u00b7gen"], "token_info": ["word"], "pos": ["VVINF"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Was lang verloren war/ was jederman veracht/", "tokens": ["Was", "lang", "ver\u00b7lo\u00b7ren", "war", "/", "was", "je\u00b7der\u00b7man", "ver\u00b7acht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVPP", "VAFIN", "$(", "PWS", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Seid man so manche sprach ins Teutschland hat", "tokens": ["Seid", "man", "so", "man\u00b7che", "sprach", "ins", "Teutschland", "hat"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAIMP", "PIS", "ADV", "PIS", "VVFIN", "APPRART", "NN", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "gebracht;", "tokens": ["ge\u00b7bracht", ";"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Nu must du dich mit macht durch Teutsch-", "tokens": ["Nu", "must", "du", "dich", "mit", "macht", "durch", "Teut\u00b7sch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "APPR", "VVFIN", "APPR", "TRUNC"], "meter": "----+--++", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Welsch-Franckreich zwingen.", "tokens": ["Wel\u00b7schFran\u00b7ck\u00b7reich", "zwin\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Der Hofman wirt auf dich/ als einen Ketzer/ dringe\u0304/", "tokens": ["Der", "Hof\u00b7man", "wirt", "auf", "dich", "/", "als", "ei\u00b7nen", "Ket\u00b7zer", "/", "dring\u0113", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPER", "$(", "KOUS", "ART", "NN", "$(", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der Newe Cantzelist ist auff die schmach bedacht/", "tokens": ["Der", "Ne\u00b7we", "Cant\u00b7ze\u00b7list", "ist", "auff", "die", "schmach", "be\u00b7dacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "ART", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Dieweil jhm wirt ein ", "tokens": ["Die\u00b7weil", "jhm", "wirt", "ein"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VAFIN", "ART"], "meter": "--+-+", "measure": "anapaest.init"}, "line.11": {"text": "Dan man bey dir nicht h\u00f6rt die frembde W\u00f6rter", "tokens": ["Dan", "man", "bey", "dir", "nicht", "h\u00f6rt", "die", "fremb\u00b7de", "W\u00f6r\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "APPR", "PPER", "PTKNEG", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "klingen.", "tokens": ["klin\u00b7gen", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Willkom gew\u00fcnschtes Buch/ la\u00df dich nu fr\u00f6lich", "tokens": ["Will\u00b7kom", "ge\u00b7w\u00fcnschtes", "Buch", "/", "la\u00df", "dich", "nu", "fr\u00f6\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$(", "VVIMP", "PPER", "ADV", "ADJD"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "blicken/", "tokens": ["bli\u00b7cken", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "Dan deinen aufgang kan kein Neider je ersticken/", "tokens": ["Dan", "dei\u00b7nen", "auf\u00b7gang", "kan", "kein", "Nei\u00b7der", "je", "er\u00b7sti\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VMFIN", "PIAT", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Dein Maister sch\u00fctzet dich bey Hof vnd Cantzeley:", "tokens": ["Dein", "Mais\u00b7ter", "sch\u00fct\u00b7zet", "dich", "bey", "Hof", "vnd", "Cant\u00b7ze\u00b7ley", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPR", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Vnd du/ auch selber du; du machest dich schmachfrey.", "tokens": ["Vnd", "du", "/", "auch", "sel\u00b7ber", "du", ";", "du", "ma\u00b7chest", "dich", "schmach\u00b7frey", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$(", "ADV", "ADV", "PPER", "$.", "PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Wie kans auch anderst sein? dich hat derselb ge-", "tokens": ["Wie", "kans", "auch", "an\u00b7derst", "sein", "?", "dich", "hat", "der\u00b7selb", "ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "ADV", "ADV", "VAINF", "$.", "PPER", "VAFIN", "PDAT", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "macht", "tokens": ["macht"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.20": {"text": "Der erstlich vnsre sprach widrumb in gang gebracht.", "tokens": ["Der", "erst\u00b7lich", "vns\u00b7re", "sprach", "wid\u00b7rumb", "in", "gang", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}