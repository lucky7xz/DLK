{"textgrid.poem.52289": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "10. Perd\u00fch", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ick wahnte fr\u00fcher dicht bi Kalen", "tokens": ["Ick", "wahn\u00b7te", "fr\u00fc\u00b7her", "dicht", "bi", "Ka\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Un makt enmal en gaud Gesch\u00e4ft,", "tokens": ["Un", "makt", "en\u00b7mal", "en", "gaud", "Ge\u00b7sch\u00e4ft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ick hadd up't g\u00fcstrow'sch Mark 'ne Partie Fahlen", "tokens": ["Ick", "hadd", "up't", "g\u00fcstrow'sch", "Mark", "'ne", "Par\u00b7tie", "Fah\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "ADJD", "NN", "ART", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "F\u00f6r einen sch\u00f6nen Pris verk\u00f6fft.", "tokens": ["F\u00f6r", "ei\u00b7nen", "sch\u00f6\u00b7nen", "Pris", "ver\u00b7k\u00f6fft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dat Geld l\u00e4d ick in minen Kuffert 'rin,", "tokens": ["Dat", "Geld", "l\u00e4d", "ick", "in", "mi\u00b7nen", "Kuf\u00b7fert", "'rin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "De hinnen up den Wagen st\u00fcnn,", "tokens": ["De", "hin\u00b7nen", "up", "den", "Wa\u00b7gen", "st\u00fcnn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Un f\u00fchrte nu mit einen gauden Fr\u00fcnd,", "tokens": ["Un", "f\u00fchr\u00b7te", "nu", "mit", "ei\u00b7nen", "gau\u00b7den", "Fr\u00fcnd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "De man jo \u00fcmmer up en Johrmark find't,", "tokens": ["De", "man", "jo", "\u00fcm\u00b7mer", "up", "en", "Johr\u00b7mark", "find't", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Ganz wollgemaud taur\u00fcgg nah Hus;", "tokens": ["Ganz", "woll\u00b7ge\u00b7maud", "tau\u00b7r\u00fcgg", "nah", "Hus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ADJD", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Un v\u00f6r uns satt min Kutscher Jehann Dus.", "tokens": ["Un", "v\u00f6r", "uns", "satt", "min", "Kut\u00b7scher", "Je\u00b7hann", "Dus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPER", "APPR", "PPOSAT", "NN", "NE", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Wi redten nu von dit un dat", "tokens": ["Wi", "red\u00b7ten", "nu", "von", "dit", "un", "dat"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ADV", "APPR", "FM", "FM", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Un ok von einen Kopmann in de Stadt,", "tokens": ["Un", "ok", "von", "ei\u00b7nen", "Kop\u00b7mann", "in", "de", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "ART", "NN", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Von den'n de Wohrheit sick nich let verhehlen,", "tokens": ["Von", "den'n", "de", "Wohr\u00b7heit", "sick", "nich", "let", "ver\u00b7heh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Dat hei nu ok Pankrott wull spelen.", "tokens": ["Dat", "hei", "nu", "ok", "Pan\u00b7krott", "wull", "spe\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Wi deden beid den Kopmann kenn'n.", "tokens": ["Wi", "de\u00b7den", "beid", "den", "Kop\u00b7mann", "kenn'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.16": {"text": "\u00bbje\u00ab, seggt min Fr\u00fcnd, \u00bbde is nu ok tau En'n.\u00ab", "tokens": ["\u00bb", "je", "\u00ab", ",", "seggt", "min", "Fr\u00fcnd", ",", "\u00bb", "de", "is", "nu", "ok", "tau", "En'", "n.", "\u00ab"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "abbreviation", "punct"], "pos": ["$(", "ADV", "$(", "$,", "VVFIN", "PPOSAT", "NN", "$,", "$(", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.17": {"text": "\u00bbja\u00ab, segg ick, \u00bbde is rein ", "tokens": ["\u00bb", "ja", "\u00ab", ",", "segg", "ick", ",", "\u00bb", "de", "is", "rein"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["$(", "PTKANT", "$(", "$,", "VVFIN", "PPER", "$,", "$(", "FM", "FM", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.18": {"text": "Wi redten nu von't G\u00e4uderpachten,", "tokens": ["Wi", "red\u00b7ten", "nu", "von't", "G\u00e4u\u00b7der\u00b7pach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Wi w\u00fc\u00dften nich, woher wi't nehmen,", "tokens": ["Wi", "w\u00fc\u00df\u00b7ten", "nich", ",", "wo\u00b7her", "wi't", "neh\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PTKNEG", "$,", "PWAV", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Un wenn wi dat so recht bedachten,", "tokens": ["Un", "wenn", "wi", "dat", "so", "recht", "be\u00b7dach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "NE", "ART", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.21": {"text": "So m\u00fc\u00dft de Kammer sick doch sch\u00e4men.", "tokens": ["So", "m\u00fc\u00dft", "de", "Kam\u00b7mer", "sick", "doch", "sch\u00e4\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NE", "NN", "VVIMP", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Bet wi des Abends gegen achten", "tokens": ["Bet", "wi", "des", "A\u00b7bends", "ge\u00b7gen", "ach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "APPR", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Gesund un woll tau Hus ankemen.", "tokens": ["Ge\u00b7sund", "un", "woll", "tau", "Hus", "an\u00b7ke\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.24": {"text": "Min Fr\u00fcnd, de was ganz trurig word'n.", "tokens": ["Min", "Fr\u00fcnd", ",", "de", "was", "ganz", "tru\u00b7rig", "word'", "n."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["PPOSAT", "NN", "$,", "NE", "PWS", "ADV", "ADJD", "VAFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Wo s\u00fcll dat warden h\u00fct un morg'n!", "tokens": ["Wo", "s\u00fcll", "dat", "war\u00b7den", "h\u00fct", "un", "mor\u00b7g'n", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "NE", "VAFIN", "ADJD", "FM", "FM", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.26": {"text": "Doch ick was lustig un fidel,", "tokens": ["Doch", "ick", "was", "lus\u00b7tig", "un", "fi\u00b7del", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PIS", "ADJD", "FM", "FM", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Wil mi in minen Sinn infel,", "tokens": ["Wil", "mi", "in", "mi\u00b7nen", "Sinn", "in\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NE", "APPR", "ADJA", "NN", "NE", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.28": {"text": "Dat ick noch hadd wat acht're Hand:", "tokens": ["Dat", "ick", "noch", "hadd", "wat", "acht'\u00b7re", "Hand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Fiwhunnert Daler preu\u00df'schen K'rant,", "tokens": ["Fiw\u00b7hun\u00b7nert", "Da\u00b7ler", "preu\u00df'\u00b7schen", "K'\u00b7rant", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}, "line.30": {"text": "De ick hadd kregen f\u00f6r de Fahlen.", "tokens": ["De", "ick", "hadd", "kre\u00b7gen", "f\u00f6r", "de", "Fah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Ick let 'ne Buddel Win 'rup halen.", "tokens": ["Ick", "let", "'ne", "Bud\u00b7del", "Win", "'rup", "ha\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NE", "NE", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "\u00bbih, Brauder\u00ab, segg ick, \u00bbnich verzagt!", "tokens": ["\u00bb", "ih", ",", "Brau\u00b7der", "\u00ab", ",", "segg", "ick", ",", "\u00bb", "nich", "ver\u00b7zagt", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "PPER", "$,", "NN", "$(", "$,", "VVFIN", "PPER", "$,", "$(", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Hir drink mal ens! De Win is echt,", "tokens": ["Hir", "drink", "mal", "ens", "!", "De", "Win", "is", "echt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "ADV", "$.", "FM", "FM", "FM", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.34": {"text": "Man \u00fcmmer wedder frisch gewagt!", "tokens": ["Man", "\u00fcm\u00b7mer", "wed\u00b7der", "frisch", "ge\u00b7wagt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Dat treckt sick allens wedder t'recht.\u00ab", "tokens": ["Dat", "treckt", "sick", "al\u00b7lens", "wed\u00b7der", "t'\u00b7recht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.36": {"text": "Un as ick so sin Sorg' verdeil", "tokens": ["Un", "as", "ick", "so", "sin", "Sor\u00b7g'", "ver\u00b7deil"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.37": {"text": "Un sei mit Win em 'runner sp\u00e4ul,", "tokens": ["Un", "sei", "mit", "Win", "em", "'run\u00b7ner", "sp\u00e4ul", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "APPR", "NN", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Dunn k\u00fcmmt min Kutscher 'rin un fr\u00f6ggt:", "tokens": ["Dunn", "k\u00fcmmt", "min", "Kut\u00b7scher", "'rin", "un", "fr\u00f6ggt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "NE", "FM", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.39": {"text": "\u00bboh, nich v\u00f6r \u00e4wel! Herr, oh, segg'n Sei mi:", "tokens": ["\u00bb", "oh", ",", "nich", "v\u00f6r", "\u00e4\u00b7wel", "!", "Herr", ",", "oh", ",", "seg\u00b7g'n", "Sei", "mi", ":"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "$,", "PTKNEG", "NE", "NE", "$.", "NN", "$,", "FM", "$,", "FM", "FM", "FM", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Wat heit denn eigentlich 'perd\u00fch'?\u00ab", "tokens": ["Wat", "heit", "denn", "ei\u00b7gent\u00b7lich", "'", "per\u00b7d\u00fch", "'", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct", "punct"], "pos": ["NE", "NE", "KON", "ADV", "$(", "VVIMP", "$(", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "\u00bbperd\u00fch? Perd\u00fch? Wat s\u00fcll dat heiten?", "tokens": ["\u00bb", "per\u00b7d\u00fch", "?", "Per\u00b7d\u00fch", "?", "Wat", "s\u00fcll", "dat", "hei\u00b7ten", "?"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$.", "NE", "$.", "NN", "NE", "ART", "ADJA", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.42": {"text": "Perd\u00fch? Wat willst du dorvon weiten?\u00ab", "tokens": ["Per\u00b7d\u00fch", "?", "Wat", "willst", "du", "dor\u00b7von", "wei\u00b7ten", "?", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$.", "NN", "VMFIN", "PPER", "PAV", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "De Kirl, de steiht un steiht un lurt.", "tokens": ["De", "Kirl", ",", "de", "steiht", "un", "steiht", "un", "lurt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "FM", "FM", "FM", "FM", "FM", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.44": {"text": "\u00bbih, segg'n S' doch, Herr, wat heit dat Wurd?\u00ab", "tokens": ["\u00bb", "ih", ",", "seg\u00b7g'n", "S'", "doch", ",", "Herr", ",", "wat", "heit", "dat", "Wurd", "?", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "$,", "VVFIN", "PPER", "ADV", "$,", "NN", "$,", "PRELS", "NE", "ART", "NN", "$.", "$("], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.45": {"text": "\u00bbperd\u00fch, dat heit so vel as: is verluren.\u00ab", "tokens": ["\u00bb", "per\u00b7d\u00fch", ",", "dat", "heit", "so", "vel", "as", ":", "is", "ver\u00b7lu\u00b7ren", ".", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "FM", "VVINF", "$.", "$("], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.46": {"text": "\u00bbna\u00ab, seggt Jehann un kratzt sick achtr'e Uhren,", "tokens": ["\u00bb", "na", "\u00ab", ",", "seggt", "Je\u00b7hann", "un", "kratzt", "sick", "a\u00b7chtr'e", "Uh\u00b7ren", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$(", "$,", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.47": {"text": "\u00bbdat heww'ck mi dacht! Herr, s\u00fch mal, s\u00fch!", "tokens": ["\u00bb", "dat", "hew\u00b7w'ck", "mi", "dacht", "!", "Herr", ",", "s\u00fch", "mal", ",", "s\u00fch", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["$(", "ART", "NN", "NE", "VVFIN", "$.", "NN", "$,", "ADJD", "ADV", "$,", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.48": {"text": "Denn is uns' Kuffert ok perd\u00fch.\u00ab", "tokens": ["Denn", "is", "un\u00b7s'", "Kuf\u00b7fert", "ok", "per\u00b7d\u00fch", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "FM", "FM", "FM", "FM", "FM", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}