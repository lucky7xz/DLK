{"textgrid.poem.46673": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[es ist zu schwer, was ich erlitt]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist zu schwer, was ich erlitt,", "tokens": ["Es", "ist", "zu", "schwer", ",", "was", "ich", "er\u00b7litt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es ist zu stark, was mich bestritt;", "tokens": ["Es", "ist", "zu", "stark", ",", "was", "mich", "be\u00b7stritt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich dacht', ich wollt' es besiegen,", "tokens": ["Ich", "dacht'", ",", "ich", "wollt'", "es", "be\u00b7sie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Aber ich mu\u00df erliegen.", "tokens": ["A\u00b7ber", "ich", "mu\u00df", "er\u00b7lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Es ist zu stark, was ich erlitt,", "tokens": ["Es", "ist", "zu", "stark", ",", "was", "ich", "er\u00b7litt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es nimmt mich mit;", "tokens": ["Es", "nimmt", "mich", "mit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ich will ihm widerstehen,", "tokens": ["Ich", "will", "ihm", "wi\u00b7der\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Aber ich mu\u00df mit gehen.", "tokens": ["A\u00b7ber", "ich", "mu\u00df", "mit", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "APPR", "VVINF", "$."], "meter": "+-++-+-", "measure": "unknown.measure.tetra"}}, "stanza.3": {"line.1": {"text": "Ich wu\u00dft' es lange zuvor genug:", "tokens": ["Ich", "wu\u00dft'", "es", "lan\u00b7ge", "zu\u00b7vor", "ge\u00b7nug", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich trag' es nicht! und ich ertrug", "tokens": ["Ich", "trag'", "es", "nicht", "!", "und", "ich", "er\u00b7trug"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$.", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es doch so lang', aber nun nicht mehr,", "tokens": ["Es", "doch", "so", "lang'", ",", "a\u00b7ber", "nun", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADV", "$,", "ADV", "ADV", "PTKNEG", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nun ist es mir zu schwer.", "tokens": ["Nun", "ist", "es", "mir", "zu", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich sagt' es meinem M\u00e4dchen ins Ohr,", "tokens": ["Ich", "sagt'", "es", "mei\u00b7nem", "M\u00e4d\u00b7chen", "ins", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Als ich sie verlor:", "tokens": ["Als", "ich", "sie", "ver\u00b7lor", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wenn du nicht bleiben willst bei mir,", "tokens": ["Wenn", "du", "nicht", "blei\u00b7ben", "willst", "bei", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So mu\u00df ich gehn mit dir.", "tokens": ["So", "mu\u00df", "ich", "gehn", "mit", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sie ist nicht geblieben, und hat mit fort", "tokens": ["Sie", "ist", "nicht", "ge\u00b7blie\u00b7ben", ",", "und", "hat", "mit", "fort"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "VVPP", "$,", "KON", "VAFIN", "APPR", "PTKVZ"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Genommen mein Wort;", "tokens": ["Ge\u00b7nom\u00b7men", "mein", "Wort", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ich f\u00fchle von Todesgewalten", "tokens": ["Ich", "f\u00fch\u00b7le", "von", "To\u00b7des\u00b7ge\u00b7wal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Bei meinem Wort mich gehalten.", "tokens": ["Bei", "mei\u00b7nem", "Wort", "mich", "ge\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "\u00bbund wisse, da\u00df du Unrecht thust,", "tokens": ["\u00bb", "und", "wis\u00b7se", ",", "da\u00df", "du", "Un\u00b7recht", "thust", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VVFIN", "$,", "KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn du nachh\u00e4ngst der Schmerzenslust.\u00ab", "tokens": ["Wenn", "du", "nach\u00b7h\u00e4ngst", "der", "Schmer\u00b7zens\u00b7lust", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich h\u00e4nge nicht nach dem Schmerz,", "tokens": ["Ich", "h\u00e4n\u00b7ge", "nicht", "nach", "dem", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Aber es bricht mein Herz.", "tokens": ["A\u00b7ber", "es", "bricht", "mein", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}