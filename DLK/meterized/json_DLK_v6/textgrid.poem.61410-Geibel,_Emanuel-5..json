{"textgrid.poem.61410": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sei nur rein wie der Schwan, und es sprossen von selber die Fl\u00fcgel", "tokens": ["Sei", "nur", "rein", "wie", "der", "Schwan", ",", "und", "es", "spros\u00b7sen", "von", "sel\u00b7ber", "die", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADJD", "KOKOM", "ART", "NN", "$,", "KON", "PPER", "VVFIN", "APPR", "ADV", "ART", "NN"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Dir zu begeistertem Schwung hoch an den Schultern empor,", "tokens": ["Dir", "zu", "be\u00b7geis\u00b7ter\u00b7tem", "Schwung", "hoch", "an", "den", "Schul\u00b7tern", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "---+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und du erkennest die Welt und dich selbst und den waltenden Vater,", "tokens": ["Und", "du", "er\u00b7ken\u00b7nest", "die", "Welt", "und", "dich", "selbst", "und", "den", "wal\u00b7ten\u00b7den", "Va\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "KON", "PPER", "ADV", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+---+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Himmel und Erde beherrscht klar der erleuchtete Blick.", "tokens": ["Him\u00b7mel", "und", "Er\u00b7de", "be\u00b7herrscht", "klar", "der", "er\u00b7leuch\u00b7te\u00b7te", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.5": {"text": "Aber befleckst du mit Staube die g\u00f6ttlich entsprungene Seele,", "tokens": ["A\u00b7ber", "be\u00b7fleckst", "du", "mit", "Stau\u00b7be", "die", "g\u00f6tt\u00b7lich", "ent\u00b7sprun\u00b7ge\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Zieht dich ein ewig Gesetz wieder zum Staube zur\u00fcck.", "tokens": ["Zieht", "dich", "ein", "e\u00b7wig", "Ge\u00b7setz", "wie\u00b7der", "zum", "Stau\u00b7be", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJD", "NN", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Einzelnes magst auch dann du vernehmen. Die himmlische Gabe", "tokens": ["Ein\u00b7zel\u00b7nes", "magst", "auch", "dann", "du", "ver\u00b7neh\u00b7men", ".", "Die", "himm\u00b7li\u00b7sche", "Ga\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VMFIN", "ADV", "ADV", "PPER", "VVINF", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Wirket entweiht selbst fort; aber der Genius schweigt.", "tokens": ["Wir\u00b7ket", "ent\u00b7weiht", "selbst", "fort", ";", "a\u00b7ber", "der", "Ge\u00b7nius", "schweigt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "ADV", "PTKVZ", "$.", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Wie sich der Mond nur voll im lautersten Strome bespiegelt,", "tokens": ["Wie", "sich", "der", "Mond", "nur", "voll", "im", "lau\u00b7ters\u00b7ten", "Stro\u00b7me", "be\u00b7spie\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "ADV", "ADJD", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Ruht still schaffend der Gott einzig im reinsten Gem\u00fct.", "tokens": ["Ruht", "still", "schaf\u00b7fend", "der", "Gott", "ein\u00b7zig", "im", "reins\u00b7ten", "Ge\u00b7m\u00fct", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADJD", "ART", "NN", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}}}}}