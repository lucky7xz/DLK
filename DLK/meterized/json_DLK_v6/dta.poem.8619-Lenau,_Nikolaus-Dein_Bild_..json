{"dta.poem.8619": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Dein Bild .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.71", "af:0.28"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Die Sonne sinkt, die Berge gl\u00fch'n,", "tokens": ["Die", "Son\u00b7ne", "sinkt", ",", "die", "Ber\u00b7ge", "gl\u00fch'n", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und aus des Abends Rosen", "tokens": ["Und", "aus", "des", "A\u00b7bends", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Seh' ich so sch\u00f6n dein Bild mir bl\u00fch'n,", "tokens": ["Seh'", "ich", "so", "sch\u00f6n", "dein", "Bild", "mir", "bl\u00fch'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So fern dem Hoffnunglosen.", "tokens": ["So", "fern", "dem", "Hoff\u00b7nun\u00b7glo\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Strahlt Hesperus dann hell und mild", "tokens": ["Strahlt", "Hes\u00b7pe\u00b7rus", "dann", "hell", "und", "mild"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NE", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am blauen Himmelsbogen,", "tokens": ["Am", "blau\u00b7en", "Him\u00b7mels\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So hat mit ihm dein s\u00fc\u00dfes Bild", "tokens": ["So", "hat", "mit", "ihm", "dein", "s\u00fc\u00b7\u00dfes", "Bild"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Sternenflur bezogen.", "tokens": ["Die", "Ster\u00b7nen\u00b7flur", "be\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Im mondbegl\u00e4nzten Laube spielt", "tokens": ["Im", "mond\u00b7be\u00b7gl\u00e4nz\u00b7ten", "Lau\u00b7be", "spielt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Abendwinde S\u00e4useln;", "tokens": ["Der", "Ab\u00b7end\u00b7win\u00b7de", "S\u00e4u\u00b7seln", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie freudig um dein zitternd Bild", "tokens": ["Wie", "freu\u00b7dig", "um", "dein", "zit\u00b7ternd", "Bild"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "APPR", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Baches Wellen kr\u00e4useln! \u2014", "tokens": ["Des", "Ba\u00b7ches", "Wel\u00b7len", "kr\u00e4u\u00b7seln", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Es braust der Wald, am Himmel zieh'n", "tokens": ["Es", "braust", "der", "Wald", ",", "am", "Him\u00b7mel", "zieh'n"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Sturmes Donnerfl\u00fcge,", "tokens": ["Des", "Stur\u00b7mes", "Don\u00b7ner\u00b7fl\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da mal' ich in die Wetter hin,", "tokens": ["Da", "mal'", "ich", "in", "die", "Wet\u00b7ter", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O M\u00e4dchen, deine Z\u00fcge.", "tokens": ["O", "M\u00e4d\u00b7chen", ",", "dei\u00b7ne", "Z\u00fc\u00b7ge", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ich seh' die Blitze trunkenhaft", "tokens": ["Ich", "seh'", "die", "Blit\u00b7ze", "trun\u00b7ken\u00b7haft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Um deine Z\u00fcge schwanken,", "tokens": ["Um", "dei\u00b7ne", "Z\u00fc\u00b7ge", "schwan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie meiner tiefen Leidenschaft", "tokens": ["Wie", "mei\u00b7ner", "tie\u00b7fen", "Lei\u00b7den\u00b7schaft"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aufflammende Gedanken.", "tokens": ["Auf\u00b7flam\u00b7men\u00b7de", "Ge\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+---+-", "measure": "dactylic.init"}}, "stanza.6": {"line.1": {"text": "Vom Felsen st\u00fcrzt die Gemse dort,", "tokens": ["Vom", "Fel\u00b7sen", "st\u00fcrzt", "die", "Gem\u00b7se", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Enteilet mit den Winden,", "tokens": ["Ent\u00b7ei\u00b7let", "mit", "den", "Win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So sprang von mir die Freude fort,", "tokens": ["So", "sprang", "von", "mir", "die", "Freu\u00b7de", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ist nicht mehr zu finden.", "tokens": ["Und", "ist", "nicht", "mehr", "zu", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Da bin ich, wei\u00df nicht selber wie,", "tokens": ["Da", "bin", "ich", ",", "wei\u00df", "nicht", "sel\u00b7ber", "wie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "VVFIN", "PTKNEG", "ADV", "KOKOM", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An einen Abgrund kommen,", "tokens": ["An", "ei\u00b7nen", "Ab\u00b7grund", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der noch das Kind der Sonne nie", "tokens": ["Der", "noch", "das", "Kind", "der", "Son\u00b7ne", "nie"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "NN", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In seinen Schoos genommen.", "tokens": ["In", "sei\u00b7nen", "Schoos", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ich aber seh' aus seiner Nacht", "tokens": ["Ich", "a\u00b7ber", "seh'", "aus", "sei\u00b7ner", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Bild so hold mir blinken,", "tokens": ["Dein", "Bild", "so", "hold", "mir", "blin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie mir dein Antlitz nie gelacht; \u2014", "tokens": ["Wie", "mir", "dein", "Ant\u00b7litz", "nie", "ge\u00b7lacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "ADV", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Will's mich hinunter winken? \u2014", "tokens": ["Will's", "mich", "hin\u00b7un\u00b7ter", "win\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "PPER", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}