{"textgrid.poem.66442": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Weiber", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie wandelbar sind nicht", "tokens": ["Wie", "wan\u00b7del\u00b7bar", "sind", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "PTKNEG"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die Weiber dieser Erden!", "tokens": ["Die", "Wei\u00b7ber", "die\u00b7ser", "Er\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie k\u00f6nnen Furien,", "tokens": ["Sie", "k\u00f6n\u00b7nen", "Fu\u00b7ri\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sie k\u00f6nnen Engel werden.", "tokens": ["Sie", "k\u00f6n\u00b7nen", "En\u00b7gel", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}