{"dta.poem.9334": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "Xi .  \n Sexain/ Sechszeilich gesetze/  \n so sich von Weiblichen anf\u00e4ht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wje sparsam werden doch/ wie witzig unsre Leuthe/", "tokens": ["Wie", "spar\u00b7sam", "wer\u00b7den", "doch", "/", "wie", "wit\u00b7zig", "uns\u00b7re", "Leu\u00b7the", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VAFIN", "ADV", "$(", "PWAV", "ADJD", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "durch diesen krieg gemacht/ der Mangel stetig denckt", "tokens": ["durch", "die\u00b7sen", "krieg", "ge\u00b7macht", "/", "der", "Man\u00b7gel", "ste\u00b7tig", "denckt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVPP", "$(", "ART", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "auff lauter sparsamkeit; da\u00df sich der Priester kr\u00e4nckt", "tokens": ["auff", "lau\u00b7ter", "spar\u00b7sam\u00b7keit", ";", "da\u00df", "sich", "der", "Pries\u00b7ter", "kr\u00e4nckt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$.", "KOUS", "PRF", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "und gar verarmen mu\u00df: mann gehet auf die freyte", "tokens": ["und", "gar", "ver\u00b7ar\u00b7men", "mu\u00df", ":", "mann", "ge\u00b7het", "auf", "die", "frey\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVINF", "VMFIN", "$.", "NN", "VVFIN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "so lange bi\u00df zugleich im wochen ligt die Braut/", "tokens": ["so", "lan\u00b7ge", "bi\u00df", "zu\u00b7gleich", "im", "wo\u00b7chen", "ligt", "die", "Braut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ADV", "APPRART", "ADJA", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "also da\u00df auf einmahl der Priester taufft vnd traut.", "tokens": ["al\u00b7so", "da\u00df", "auf", "ein\u00b7mahl", "der", "Pries\u00b7ter", "taufft", "vnd", "traut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "APPR", "ADV", "ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}}}}}