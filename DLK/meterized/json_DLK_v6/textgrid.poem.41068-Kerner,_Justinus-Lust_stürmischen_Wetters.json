{"textgrid.poem.41068": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Lust st\u00fcrmischen Wetters", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbha! wie's jetzt st\u00fcrmet und schneit!", "tokens": ["\u00bb", "ha", "!", "wie's", "jetzt", "st\u00fcr\u00b7met", "und", "schneit", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$.", "VVFIN", "ADV", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Das ist ein Graus!\u00ab", "tokens": ["Das", "ist", "ein", "Graus", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Rufet dort einer zum Fenster heraus.", "tokens": ["Ru\u00b7fet", "dort", "ei\u00b7ner", "zum", "Fens\u00b7ter", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "APPRART", "NN", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Kein Graus! nein! nein!", "tokens": ["Kein", "Graus", "!", "nein", "!", "nein", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "$.", "PTKANT", "$.", "PTKANT", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Das ist mir Sonnenschein!", "tokens": ["Das", "ist", "mir", "Son\u00b7nen\u00b7schein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Denn nun bleibt jeder zu Haus,", "tokens": ["Denn", "nun", "bleibt", "je\u00b7der", "zu", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "APPR", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Und ich allein.", "tokens": ["Und", "ich", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}